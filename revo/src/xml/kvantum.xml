<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvantum.xml,v 1.13 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kvantum</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kvantum.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvantum.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Elementa unuo de fizika grando:
      <ekz>
        <tld/>o de energio, de movkvanta
        momanto, de elektro.
      </ekz>
    </dif>
  <trd lng="pl">kwant</trd>
  <trd lng="pt">quantum</trd>
  </snc>
  <snc mrk="kvantum.0o.korpusklo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Elementa korpusklo kun nula a&ubreve; entjera spino;
      <ref tip="sin" cel="boson.0o">bosono</ref>.
    </dif>
    <ref tip="sub" cel="foton.0o">fotono</ref>
    <refgrp tip="vid">
      <ref cel="atom.0o">atomo</ref>,
      <ref cel="elektr14.0o">elektrono</ref>
    </refgrp>
  <trdgrp lng="pt">
    <trd>quantum</trd>,
    <trd>b&oacute;son</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trd lng="ca">qu&agrave;ntum</trd>
  <trd lng="cs">kvantum</trd>
  <trd lng="de">Quant</trd>
  <trd lng="en">quantum</trd>
  <trd lng="fa">&f_k;&a_w;&a_A;&a_n;&a_t;&a_w;&a_m;</trd>
  <trd lng="fr">quantum</trd>
  <trd lng="hu">kvantum</trd>
  <trd lng="nl">kwantum</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">kvantum</trd>
</drv>

<drv mrk="kvantum.0mehxaniko">
  <kap><tld/>me&hcirc;aniko</kap>
  <snc mrk="kvantum.0mehxaniko.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Me&hcirc;aniko, en kiu fizikaj grandoj estas reprezentataj per
      matricoj a&ubreve; operatoroj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_ja;
  &c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">mec&agrave;nica qu&agrave;ntica</trd>
  <trd lng="de">Quantenmechanik</trd>
  <trd lng="en">quantum mechanics</trd>
  <trd lng="fa">&a_m;&f_k;&a_A;&a_n;&f_y;&f_k; &f_k;&a_w;&a_A;&a_n;&a_t;&a_w;&a_m;&f_y;</trd>
  <trd lng="fr">m&eacute;canique quantique</trd>
  <trd lng="hu">kvantummechanika</trd>
  <trd lng="nl">kwantummechanica</trd>
  <trd lng="pl">mechanika kwantowa</trd>
  <trd lng="pt">mec&acirc;nica qu&acirc;ntica</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_n;&c_t;&c_o;&c_v;&c_a;&c_ja;
  &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
