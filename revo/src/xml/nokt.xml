<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: nokt.xml,v 1.50 2015/12/25 06:10:14 revo Exp $">
    <kap><ofc>*</ofc>
      <rad>nokt</rad>/o
    </kap>
      
    <drv mrk="nokt.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="nokt.0o.KAL">
        <uzo tip="fak">KAL</uzo>
        <dif>
          Tiu parto de la <ref tip="malprt" cel="diurn.0o">diurno</ref>,
          dum kiu la suno estas sub horizonto kaj ne lumas al ni:
          <ekz>
            en la <tld/>o ni vidas la palan lunon kaj la belajn stelojn
            <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
          </ekz><ekz>
            la <tld/>o falas <klr>(komenci&gcirc;as)</klr>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="nokt.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Mallumo materia a&ubreve; spirita:
          <ekz>
            la <tld/>o de la tombo;
          </ekz><ekz>
            blindeco estas senfina <tld/>o.
          </ekz>
        </dif>
        <trd lng="ca">fosca</trd>
      </snc>
  <trd lng="be">&c_n;&c_o;&c_ch;</trd>
  <trd lng="bg">&c_n;&c_o;&c_shch;</trd>
  <trdgrp lng="br">
        <trd>noz</trd>,
        <trd>nozvezh</trd>
      </trdgrp>
  <trd lng="ca">nit</trd>
  <trd lng="cs">noc</trd>
  <trd lng="de">Nacht</trd>
  <trdgrp lng="el">
        <trd>&ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
        <trd>&ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
      </trdgrp>
  <trd lng="en">night</trd>
  <trd lng="es">noche</trd>
  <trd lng="fr">nuit</trd>
  <trdgrp lng="hu">
        <trd>&eacute;j</trd>,
        <trd>&eacute;jjel</trd>,
        <trd>&eacute;jszaka</trd>
      </trdgrp>
  <trd lng="id">malam</trd>
  <trd lng="ku">&scedil;ev</trd>
  <trd lng="nl">nacht</trd>
  <trd lng="pl">noc</trd>
  <trd lng="pt">noite</trd>
  <trd lng="ro">noapte</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;</trd>
  <trd lng="sk">noc</trd>
  <trd lng="sv">natt</trd>
  <trd lng="tr">gece</trd>
  <trdgrp lng="zh">
        <trd>&#22812;<klr> [y&egrave;]</klr></trd>,
        <trd>&#23477;<klr> [xi&#257;o]</klr></trd>
      </trdgrp>
</drv>

    <drv mrk="nokt.0e">
      <kap><tld/>e</kap>
      <snc>
        <dif>
          Dum <tld/>o:
          <ekz>
            anta&ubreve; ili tage kaj <tld/>e brulas seninterrompe dekoj kaj
            centoj
            da multekostaj lampionoj
            <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj
&ccirc;uk&ccirc;oj,
                (du skizoj), unua
                skizo, Filip Onkudimov, 1</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_u;&c_n;&c_a;&c_ch;&c_y;</trd>,
        <trd>&c_n;&c_o;&c_ch;&c_ch;&c_u;</trd>
      </trdgrp>
  <trd lng="br">diouzh an <ind>noz</ind></trd>
  <trdgrp lng="ca">
        <trd>de <ind>nit</ind></trd>,
        <trd>de <ind>nit</ind>s</trd>
      </trdgrp>
  <trd lng="cs">v noci <klr>(adv.)</klr></trd>
  <trd lng="de">nachts</trd>
  <trdgrp lng="el">
        <trd>&eta; &ny;&ypsilon_ton;&kappa;&tau;&alfa;</trd>,
        <trd>&eta; &ny;&ypsilon_ton;&chi;&tau;&alfa;</trd>
      </trdgrp>
  <trd lng="en">night-time</trd>
  <trdgrp lng="fr">
        <trd>de <ind>nuit</ind></trd>,
        <trd>nuitamment</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>&eacute;jjel</trd>,
        <trd>&eacute;jszaka</trd>
      </trdgrp>
  <trd lng="nl">'s <ind>nachts</ind></trd>
  <trd lng="pl">noc&aogonek;</trd>
  <trdgrp lng="pt">
        <trd>de <ind>noite</ind></trd>,
        <trd>&agrave; <ind>noite</ind></trd>
     </trdgrp>
  <trd lng="ro">noaptea</trd>
  <trd lng="ru">&c_n;&c_o;&c_ch;&c_mol;&c_ju;</trd>
  <trd lng="sk">v noci</trd>
  <trd lng="sv">p&aring; <ind>natten</ind></trd>
  <trd lng="tr">geceleyin</trd>
</drv>

    <drv mrk="nokt.0i">
      <kap><tld/>i</kap>
      <snc mrk="nokt.0i.mallumi">
        <gra><vspec>sensubjekta</vspec></gra>
        <dif>
          Esti tempo de <tld/>o.
        </dif>
        <trd lng="br">beza&ntilde; <ind>noz</ind></trd>
        <trd lng="ca">ser de <ind>nit</ind></trd>
        <trd lng="en">it's <ind>night</ind> out</trd>
        <trd lng="de">Nacht sein</trd>
        <trd lng="es">ser de <ind>noche</ind></trd>
        <trd lng="fr">faire <ind>nuit</ind></trd>
        <trdgrp lng="hu">
          <trd>&eacute;jjel van</trd>,
          <trd>&eacute;jszaka van</trd>
        </trdgrp>
        <trd lng="nl">nacht zijn</trd>
        <trdgrp lng="pt">
           <trd>ser <ind>noite</ind></trd>,
           <trd>ser de <ind>noite</ind></trd>
        </trdgrp>
      </snc>
      <snc mrk="nokt.0i.tranokti">
        <uzo tip="stl">EVI</uzo>
        <ref tip="sin" cel="nokt.tra0i">tra<tld/>i</ref>
        <ekz>
          en la vojo fari&gcirc;is nokto, kaj ili restis sur la kampo
<tld/>i
          <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
        </ekz>
        <trd lng="nl">overnachten<klr tip="ind"> (ontr.)</klr></trd>
      </snc>
  <trdgrp lng="cs">
    <trd>nocovat</trd>,
    <trd>p&rcaron;enocovat</trd>
  </trdgrp>
  <trd lng="de">&uuml;bernachten</trd>
  <trd lng="pt">passar a <ind>noite</ind></trd>
  <trd lng="sk">prenocova&tcaron;</trd>
</drv>
    <drv mrk="nokt.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <gra><vspec>sensubjekta</vspec></gra>
      <snc>
        <dif>
          Ekesti <tld/>o:
          <ekz>
            <tld/>i&gcirc;is. &Scirc;i ekvidis malgrandan dometon kaj eniris
por
            ripozi
            <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
          </ekz><ekz>
            &ccirc;e la <tld/>i&gcirc;o.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">nozi&ntilde;</trd>
      <trdgrp lng="ca">
        <trd>fer-se de <ind>nit</ind></trd>,
        <trd>negrejar</trd>
      </trdgrp>
      <trd lng="de">Nacht werden</trd>
      <trd
lng="el">&ny;&ypsilon;&chi;&tau;&omega_ton;&ny;&epsilon;&jota;</trd>
      <trd lng="en">night is falling</trd>
      <trd lng="es">anochecer</trd>
      <trd lng="fr">la <ind>nuit</ind> tombe</trd>
      <trdgrp lng="hu">
        <trd>lesz&aacute;ll az <ind>&eacute;j</ind></trd>,
        <trd>&eacute;jszaka lesz</trd>
      </trdgrp>
      <trd lng="nl">nacht worden</trd>
      <trd lng="pl">&sacute;ciemnia&cacute; si&eogonek;</trd>
      <trd lng="pt">anoitecer</trd>
      <trd lng="kek">aak&#789;ab&#789;irk</trd>
    </drv>
    <drv mrk="nokt.tra0i">
      <kap>tra<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Pasigi la <tld/>on, lo&gcirc;i dum <tld/>o:
          <ekz>
            tra<tld/>i en hotelo, sub tendo;
          </ekz><ekz>
            ili man&gcirc;is kaj trinkis kaj tra<tld/>is tie
            <fnt><bib>NT</bib>, <lok>&Jug; 19:4</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">nozvezhia&ntilde;</trd>
  <trdgrp lng="ca">
        <trd>fer <ind>nit</ind></trd>,
        <trd>pernoctar</trd>
      </trdgrp>
  <trd lng="cs">p&rcaron;espat</trd>
  <trdgrp lng="de">
        <trd>&uuml;bernachten</trd>
        <trd>n&auml;chtigenen</trd>
      </trdgrp>
  <trd
lng="el">&delta;&jota;&alfa;&ny;&ypsilon;&kappa;&tau;&epsilon;&rho;&epsilon;
        &ypsilon_ton;&omega;</trd>
  <trd lng="en">spend the <ind>night</ind></trd>
  <trdgrp lng="es">
        <trd>pernoctar</trd>,
        <trd>trasnochar</trd>
      </trdgrp>
  <trd lng="fr"><ind>passer</ind> la <ind>nuit</ind></trd>
  <trdgrp lng="hu">
        <trd>megsz&aacute;ll &eacute;jszak&aacute;ra</trd>,
        <trd>&eacute;jszak&aacute;zik</trd>,
        <trd>megh&aacute;l</trd>
      </trdgrp>
  <trdgrp lng="id">
        <trd>[<ind>inap</ind>] menginap</trd>,
        <trd>[<ind>malam</ind>] bermalam</trd>
      </trdgrp>
  <trd lng="nl">overnachten</trd>
  <trd lng="pl">przenocowa&cacute;</trd>
  <trd lng="pt">pernoitar</trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">prespa&tcaron;</trd>
  <trd lng="tr">geceyi ge&ccedil;irmek</trd>
</drv>
    <drv mrk="nokt.tag0o">
      <kap>tag<tld/>o</kap>
      <snc mrk="nokt.tag0o.KAL">
        <uzo tip="fak">KAL</uzo>
        <ref tip="dif" cel="diurn.0o.KAL">diurno</ref>
        <ekz>
          &ccirc;i tie dum tag<tld/>o tondras la muziko
          <fnt><bib>Metrop</bib></fnt>.
        </ekz>
      </snc>
  <trd lng="be">&c_s;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trd lng="br">deiz<klr tip="ind"> (24 eur)</klr></trd>
  <trdgrp lng="ca">
        <trd>dia solar</trd>,
        <trd>dia sencer</trd>
      </trdgrp>
  <trd lng="cs">den = 24 hod.</trd>
  <trd lng="de">Tag<klr tip="ind"> (Zeitraum von 24
      Stunden)</klr></trd>
  <trd lng="en"><klr>(24-hour) </klr>day</trd>
  <trdgrp lng="fr">
    <trd>jour<klr tip="ind"> (24 heures)</klr></trd>,
    <trd>nycth&eacute;m&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
        <trd>nap&eacute;j</trd>,
        <trd>&eacute;jjel &eacute;s nappal</trd>
      </trdgrp>
  <trd lng="nl">etmaal</trd>
  <trd lng="pl">doba</trd>
  <trdgrp lng="pt">
    <trd>dia e <ind>noite</ind> <klr tip="amb">(24 horas)</klr></trd>,
    <trd>noite e <ind>dia</ind> <klr tip="amb">(24 horas)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
        <trd>douazeci si patru de ore</trd>,
        <trd>zi <klr>(24 ore)</klr></trd>
      </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_t;&c_k;&c_i;</trd>
  <trd lng="sk">de&ncaron; = 24 hod&iacute;n</trd>
  <trd lng="sv">dygn</trd>
  <trd lng="tr">g&uuml;n<klr>(24 saat)</klr></trd>
</drv>
  </art>
