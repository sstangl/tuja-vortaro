<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekci.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sekci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekci.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekci.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Aparta parto de institucio, societo, ordinare kun propra tasko
      a&ubreve; loko:
      <ekz>
        ili decidis proponi al &ccirc;iuj amikoj de interpopola justeco 
        unui&gcirc;i en apartan grupon, kiu inter la amikoj de internacia 
        lingvo prezentus apartan <tld/>on
        <fnt>
          <bib>Homaranismo</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i necesigas la kunlaboron de &ccirc;iuj <tld/>oj en la kompanio en 
        unu grandega projekto
        <fnt>
          <aut>M. Vanden Bempt</aut>,
          <vrk>Al nova komuna epoko</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 3a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fak.0o">fako</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>oszt&aacute;ly</trd>,
      <trd>r&eacute;szleg</trd>,
      <trd>tagozat</trd>
    </trdgrp>
  </snc>
  <snc mrk="sekci.0o.ajxa">
    <dif>
      Parto de sinetenda tuta&jcirc;o, ekzemple de strato, verko:
      <ekz>
        la&ubreve;longe de kanal<tld/>o &ccirc;. sesoble pli longa ol la profundo
        de la
        akvo<fnt><aut>OH</aut>, <vrk>&Ccirc;u akvo fluas supren ie ajn en la
        mondo?</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a, p.
        22a</lok></fnt>;
      </ekz>
      <ekz>
        tion mi klarigos en la sekva <tld/>o
        <fnt><aut>Klivo</aut>, <vrk>Kio
        estas Javo?</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 5a,
        p.
        13a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>szakasz</trd>,
      <trd>r&eacute;sz</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odbor</trd>,
    <trd>oddělení</trd>,
    <trd>sekce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abteilung</trd>,
    <trd>Sektion</trd>
  </trdgrp>
  <trd lng="en">section</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_kh;&a_sh;</trd>,
    <trd>&a_q;&a_s;&a_m;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>section<klr> (partie)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>settore<klr> (parte, gruppo)</klr></trd>,
      <trd>sezione<klr> (parte, gruppo)</klr></trd>
    </trdgrp>
  <trd lng="nl">afdeling</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_t;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odbor</trd>,
    <trd>oddelenie</trd>
  </trdgrp>
  <trd lng="sv">sektion</trd>
</drv>

<drv mrk="sekci.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="sekci.0estro.POL">
    <dif>
      &Ccirc;efulo de <tld/>o:
      <ekz>
        <tld/>esto en asekura entrepreno
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>oborový šéf</trd>,
    <trd>sekční šéf</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_y_hamza;&f_y;&a_s; &a_b;&a_kh;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>chef de groupe</trd>,
    <trd>chef de section</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>caposezione</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odborový šéf</trd>,
    <trd>sekčný šéf</trd>
  </trdgrp>
</drv>
</art>
