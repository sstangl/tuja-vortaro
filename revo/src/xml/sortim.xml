<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sortim.xml,v 1.17 2015/08/28 03:10:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sortiment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sortim.0o">
  <kap><tld/>o</kap>
  <snc mrk="sortim.0o.EKON">
    <dif>
      Aro da diversaj samfakaj elmontrotaj komerca&jcirc;oj;
      <ref tip="vid" cel="specim.0aro">specimenaro</ref>:
          <ekz>
            Normale UEA povas porti al UK maksimume kvaronon de la
            tuta <tld/>o de sia libroservo, sed en Roterdamo &ccirc;io
            estis a&ccirc;etebla
            <fnt>
              <vrk>Libroservo de UEA abunde vendis en 1998</vrk>.
              <bib>LOdE</bib>, <lok>N-ro 54 (1999:4)</lok>
            </fnt>.
          </ekz>
    </dif>
    <trd lng="en">selection</trd>
    <trdgrp lng="pt">
      <trd>sortimento</trd>,
      <trd>estoque</trd>
    </trdgrp>
  </snc>
  <snc mrk="sortim.0o.KOMUNE">
    <dif>
      Aro da diversaj samspecaj objektoj:
          <ekz>
            La kelnerino alportis <tld/>on da froma&gcirc;oj
            <fnt>
              <aut>Marjorie BOULTON</aut>:
              <vrk>Ebrivirgeco</vrk>.
              <lok>La nica literatura revuo, 3:3</lok>.
            </fnt>.
          </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ensemb.0o.KOMUNE">ensemblo</ref>,
      <ref cel="garnit.0o.KOMUNE">garnituro</ref>,
      <ref cel="komple2.0o.KOMUNE">kompleto</ref>,
      <ref cel="servic.0o.KUI">servico</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>sortimento</trd>,
      <trd>estoque</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_r;&c_t;&c_y;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_a;&c_s;&c_o;&c_r;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>sortiment</trd>,
    <trd>v&yacute;b&ecaron;r zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Sortiment</trd>
  <trdgrp lng="en">
    <trd>assortment</trd>,
    <trd>collection</trd>
  </trdgrp>
  <trd lng="fr">assortiment</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;laszt&eacute;k</trd>,
    <trd>szortiment</trd>
  </trdgrp>
  <trd lng="nl">assortiment</trd>
  <trdgrp lng="pt">
    <trd>sortimento</trd>,
    <trd>estoque</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_s;&c_o;&c_r;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sortiment</trd>,
    <trd>v&yacute;ber tovaru</trd>
  </trdgrp>
  <trd lng="sv">sortiment</trd>
</drv>

<drv mrk="sortim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ta&ubreve;ge elekti kaj kunmeti komerca&jcirc;ojn inter si diferencajn,
      sed &ccirc;iujn de sama speco.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>t&rcaron;&iacute;dit</trd>,
    <trd>z&aacute;sobovat zbo&zcaron;&iacute;m</trd>
  </trdgrp>
  <trd lng="en">stock</trd>
  <trd lng="fr">r&eacute;assortir</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszev&aacute;logat</trd>,
    <trd>&ouml;ssze&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">assorteren</trd>
  <trd lng="pt">fazer <ind>sortimento</ind> de</trd>
  <trdgrp lng="sk">
    <trd>triedi&tcaron;</trd>,
    <trd>z&aacute;sobova&tcaron; tovarom</trd>
  </trdgrp>
</drv>
</art>
