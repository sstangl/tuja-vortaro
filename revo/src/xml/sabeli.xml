<kap>
  <rad>sabelik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sabeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Speco de krispa brasiko, savojbrasiko.
    </dif>
  </snc>
  <trd lng="ca"><ind>col</ind> verda</trd>
  <trd lng="cs">kapusta kade&rcaron;av&aacute;</trd>
  <trd lng="de">Wirsing</trd>
  <trdgrp lng="en">
    <trd>Savoy cabbage</trd>,
    <trd>kale</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chou fris&eacute;</trd>,
    <trd>chou de Milan</trd>
  </trdgrp>
  <trd lng="hu">kelk&aacute;poszta</trd>
  <trd lng="nl">savooienkool</trd>
  <trdgrp lng="pt">
   <trd>couve-de-sav&oacute;ia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  <trd lng="sk">ku&ccaron;erav&aacute; kapusta</trd>
</drv>
</art>
