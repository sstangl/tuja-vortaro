<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: patri.xml,v 1.14 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>patri</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>

<drv mrk="patri.0o">
  <kap><tld/>o</kap>
  <snc mrk="patri.0o.MOR">
    <ref tip="dif" cel="patr.0ujo">patrujo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_j;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_t;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mammvro</trd>
  <trdgrp lng="cs">
    <trd>domovina</trd>,
    <trd>otčina</trd>,
    <trd>vlast</trd>
  </trdgrp>
  <trd lng="de">Vaterland</trd>
  <trdgrp lng="en">
    <trd>motherland</trd>,
    <trd>fatherland</trd>
  </trdgrp>
  <trd lng="es">patria</trd>
  <trd lng="fr">patrie</trd>
  <trd lng="hu">haza</trd>
  <trd lng="it">patria</trd>
  <trd lng="nl">vaderland</trd>
  <trd lng="no">fedreland</trd>
  <trd lng="pl">ojczyzna</trd>
  <trd lng="pt">p&aacute;tria</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>domovina</trd>,
    <trd>otčina</trd>,
    <trd>vlasť</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>vatan</trd>,
    <trd>yurt</trd>
  </trdgrp>
</drv>

<drv mrk="patri.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="patri.0ismo.MOR">
    <ref tip="dif" cel="patrio.0ismo.MOR">patriotismo</ref>
    <ekz>
      por tiaj homoj la dialektoj estas pereigindaj malamikoj de la 
      <tld/>ismo
      <fnt>
      <aut>Blanka Meduzo</aut>,
      <vrk><url
      ref="http://d.hatena.ne.jp/shiro-kurage/mobile?date=20100618">
      Hatena de Esperanto</url></vrk>,
      <lok>2010-06-18</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Patriotismus</trd>,
    <trd>Vaterlandsliebe</trd>
  </trdgrp>
  <trd lng="en">patriotism</trd>
  <trd lng="fr">patriotisme</trd>
  <trdgrp lng="hu">
    <trd>hazafis&aacute;g</trd>,
    <trd>hazaszeretet</trd>,
    <trd>patriotizmus</trd>
  </trdgrp>
  <trd lng="it">patriottismo</trd>
  <trdgrp lng="nl">
    <trd>patriotisme</trd>,
    <trd>patriottisme</trd>,
    <trd>vaderlandsliefde</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
