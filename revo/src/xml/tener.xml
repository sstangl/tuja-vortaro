<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tener.xml,v 1.13 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>8</ofc><rad>tener</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Elmontranta aman, mildan atenton, karesemon:
      <ekz>
        el &scirc;ia danka submeti&gcirc;o kaj lia protektemo i&gcirc;is
        iom post iom <tld/>a rilato
        <fnt>
        <aut>Renervo Da&scirc;o</aut>,
        <vrk>Ne-atenditaj sekvoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 8a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolcx.0a.milda">dol&ccirc;a <sncref/></ref>,
      <ref cel="delika.0a">delikata</ref>,
      <ref cel="mild.0a">milda</ref>,
      <ref cel="subtil.0a">subtila</ref>,
      <ref cel="sent.0ema">sentema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tendre</trd>,
    <trd>dol&ccedil;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>láskyplný</trd>,
    <trd>něžný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tender</trd>,
    <trd>affectionate</trd>,
    <trd>fond</trd>,
    <trd>loving</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuoso(a)</trd>,
    <trd>tierno(a)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueux</trd>,
    <trd>tendre<klr> (affectueux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyeng&eacute;d</trd>,
    <trd>l&aacute;gy</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;y</trd>,
    <trd>delikatny</trd>,
    <trd>&lstroke;agodny</trd>,
    <trd>tkliwy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>terno</trd>,
    <trd>afetuoso</trd>,
    <trd>carinhoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>láskyplný</trd>,
    <trd>nežný</trd>
  </trdgrp>
</drv>

<drv mrk="tener.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kun ama mildo, prizorgeme:
      <ekz>
        Feniksa <klr>...</klr> <tld/>e lin demandis pri la farto
        <fnt>
        <bib>RugxDom</bib>,
        <lok>&ccirc;apitro 69a, volumo 2a, p. 468a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tendrament</trd>,
    <trd>dol&ccedil;ament</trd>
  </trdgrp>
  <trd lng="cs">teneramente</trd>
  <trdgrp lng="de">
    <trd>z&auml;rtlich</trd>,
    <trd>sanft</trd>,
    <trd>liebevoll</trd>,
    <trd>weichherzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tenderly</trd>,
    <trd>affectionately</trd>,
    <trd>fondly</trd>,
    <trd>lovingly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>afectuosamente</trd>,
    <trd>tiernamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affectueusement</trd>,
    <trd>tendrement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyeng&eacute;den</trd>,
    <trd>l&aacute;gyan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czule</trd>,
    <trd>delikatnie</trd>,
    <trd>&lstroke;agodnie</trd>,
    <trd>tkliwie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_s; <ind>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_mol;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="sk">nežne</trd>
</drv>
</art>
