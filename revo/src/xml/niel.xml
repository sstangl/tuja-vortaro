<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: niel.xml,v 1.15 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>niel</rad>/o
</kap>

<drv mrk="niel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Gravurita desegno, kies entajlojn oni plenigas per nigra emajlo.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_n;&c_mol; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;., &c_n;&c_a;
  &c_s;&c_r;&c_e;&c_b;&c_r;&c_y;)</klr></trd>
  <trd lng="cs">černý smalt</trd>
  <trd lng="de">Niello</trd>
  <trd lng="en">niello</trd>
  <trd lng="hu">niello</trd>
  <trd lng="nl">ni&euml;llo</trd>
  <trdgrp lng="pt">
    <trd>nigelo</trd>,
    <trd>nielo</trd>,
    <trd>niela</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;&c_n;&c_y;&c_h;
  &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_ja;&c_h;)</klr></trd>
  <trd lng="sk">čierny smalt</trd>
</drv>

<drv mrk="niel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="cs">zdobit černým smaltem</trd>
  <trd lng="de">niellieren</trd>
  <trd lng="hu">niell&oacute;t k&eacute;sz&iacute;t</trd>
  <trd lng="nl">ni&euml;lleren</trd>
  <trd lng="ru">&c_o;&c_t;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_ch;&c_je;&c_r;&c_n;&c_mol;&c_ju;</ind></trd>
  <trd lng="sk">zdobiť čiernym smaltom</trd>
</drv>

<drv mrk="niel.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;.,
  &c_s;&c_r;&c_e;&c_b;&c_r;&c_a;)</klr></trd>
  <trd lng="cs">nielo</trd>
  <trd lng="de">Niellieren</trd>
  <trd lng="hu">niell&oacute;z&aacute;s</trd>
  <trd lng="nl">ni&euml;lleren<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">nielo</trd>
</drv>

<drv mrk="niel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto en <tld/>laboroj.
    </dif>
  </snc>
  <trd lng="de">Niellierer</trd>
  <trdgrp lng="hu">
    <trd>niell&oacute;&ouml;tv&ouml;s</trd>,
    <trd>niellista</trd>
  </trdgrp>
  <trd lng="nl">ni&euml;llist</trd>
</drv>
</art>
