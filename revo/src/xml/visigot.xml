<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: visigot.xml,v 1.8 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>visigot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="visigot.0o">
  <kap><tld/>o</kap>
  <snc mrk="visigot.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de tiu parto de la <ref tip="malprt"
      cel="got.0o.ETN">gota</ref>
      popolo, kiu post diversaj lokoj de sudokcidenta E&ubreve;ropo
      regis Hispanion de la 5a &gcirc;is la komenco de la 8a jarcento:
      <ekz>
        li <klr>...</klr> venkobatis Alarikon la 2an, re&gcirc;on de la
        <tld/>oj
        <fnt><bib>Viki</bib>, <lok>&leftquot;historio de Francio&rightquot;,
        2005-09-28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_je;&c_s;&c_t;&c_g;&c_o;&c_t;</trd>
  <trd lng="ca">visigot</trd>
  <trd lng="cs">Vizogot</trd>
  <trd lng="de">Westgote</trd>
  <trdgrp lng="fr">
    <trd>Visigoth<klr tip="amb"> (subst.)</klr></trd>,
    <trd>Wisigoth<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vizig&oacute;t</trd>,
    <trd>nyugati g&oacute;t</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>visigodo</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_i;&c_g;&c_o;&c_t;</trd>
  <trd lng="sk">Vizig√≥t</trd>
</drv>

<drv mrk="visigot.0a">
  <kap><tld/>a</kap>
  <snc mrk="visigot.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_je;&c_s;&c_t;&c_g;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">visig&ograve;tic</trd>
  <trd lng="de">westgotisch</trd>
  <trdgrp lng="fr">
    <trd>visigoth<klr tip="amb"> (adj.)</klr></trd>,
    <trd>wisigoth<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">vizig&oacute;t</trd>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_i;&c_g;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

