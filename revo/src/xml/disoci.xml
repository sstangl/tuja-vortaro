<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: disoci.xml,v 1.13 2015/07/04 08:37:32 revo Exp $">
  <kap>
    <rad>disoci</rad>/i
  </kap>
  <drv mrk="disoci.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
    <uzo tip="fak">KEM</uzo>
      <dif>
        Parte a&ubreve; progrese malkomponi:
        <ekz>la liberigitaj substancoj nomi&gcirc;as <tld/>atoj.</ekz>
      </dif>
    </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_a;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
      <trd>&c_r;&c_a;&c_z;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_ja; &c_d;&c_o; &c_d;&c_i;&c_s;&c_o;&c_c;&c_i;&c_a;&c_z;&c_i;&c_ja;</trd>
    </trdgrp>
  <trd lng="ca">dissociar</trd>
  <trdgrp lng="cs">
    <trd>disociovat</trd>,
    <trd>rozlučovat</trd>,
    <trd>rozštěpit</trd>
  </trdgrp>
  <trd lng="de">dissoziieren</trd>
  <trd lng="en">dissociate</trd>
  <trd lng="fr">dissocier</trd>
  <trd lng="nl">scheiden</trd>
  <trdgrp lng="pl">
      <trd>dysocjowa&cacute;</trd>,
      <trd>rozk&lstroke;ada&cacute;<klr> (chem.)</klr></trd>
    </trdgrp>
  <trdgrp lng="pt">
      <trd>dissociar</trd>,
      <trd>desagregar</trd>
    </trdgrp>
  <trd
    lng="ru">&c_d;&c_i;&c_s;&c_s;&c_o;&c_c;&c_i;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozlúčiť</trd>,
    <trd>rozštiepiť</trd>
  </trdgrp>
</drv>
</art>
