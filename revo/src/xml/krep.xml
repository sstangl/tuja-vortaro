<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krep.xml,v 1.15 2011/12/08 11:10:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krep</rad>/o
</kap>
<drv mrk="krep.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="krep.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref> el delikata 
      lano, diafana, kun ondoforma supra&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_p;</trd>
  <trdgrp lng="de">
     <trd>Krepp</trd>,
     <trd>Flor</trd>
  </trdgrp>
  <trd lng="ca">crep&eacute;</trd>
  <trd lng="en">crepe</trd>
  <trd lng="es">crep&eacute;</trd>
  <trd lng="fa">&f_k;&a_r;&f_p;</trd>
  <trdgrp lng="fr">
    <trd>cr&ecirc;pe<klr> (textile)</klr></trd>,
    <trd>cr&ecirc;pon</trd>
  </trdgrp>
  <trd lng="nl">cr&ecirc;pe<klr tip="ind"> (textiel)</klr></trd>
  <trd lng="pt">crepe <klr tip="amb">(t&ecirc;xt.)</klr></trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_p;</trd>
</drv>
</art>
