<vortaro>
  <art mrk="$Id: vermif.xml,v 1.14 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <rad>vermifug</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>
    <drv mrk="vermif.0o">
      <kap><tld/>o</kap>
      <snc mrk="vermif.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="tanacet.0o">tanaceto</ref>
        <ekz>
          &Scirc;i volis sidi&gcirc;i sur la tombo de malri&ccirc;ulo, kie floris
          la maldol&ccirc;a <tld/>o
          <fnt><bib>Fab2</bib>, <lok>Ru&gcirc;aj &scirc;uoj</lok></fnt>.
        </ekz>
      </snc>
      <snc mrk="vermif.0o.MED">
        <dif>
          Kontra&ubreve;verma kuracilo.
        </dif>
        <trd lng="be">&c_g;&c_l;&c_ib;&c_s;&c_t;&c_a;&c_g;&c_o;&c_n;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
        <trdgrp lng="ca">
          <trd>vermicida</trd>,
          <trd>verm&iacute;fug</trd>
        </trdgrp>
        <trd lng="de">Wurmmittel</trd>
        <trd lng="el">&alfa;&ny;&theta;&epsilon;&lambda;&my;&jota;&ny;&theta;&jota;&kappa;&omikron_ton;</trd>
        <trd lng="fr">vermifuge</trd>
        <trd lng="hu">f&eacute;reghajt&oacute;</trd>
        <trd lng="nl">wormkruid</trd>
        <trd lng="pt">verm&iacute;fugo</trd>
        <trd lng="ru">&c_g;&c_l;&c_i;&c_s;&c_t;&c_o;&c_g;&c_o;&c_n;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
        <trd lng="sk">hl&iacute;stopudn&yacute; prostriedok</trd>
      </snc>
      <trd lng="cs">prostředek proti červům</trd>
</drv>
  </art>
