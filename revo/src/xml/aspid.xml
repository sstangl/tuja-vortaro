<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aspid.xml,v 1.17 2015/09/17 12:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>aspid</rad>/o
</kap>
<drv mrk="aspid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="vipur.0o">vipuro</ref>
      (<trd lng="la">Vipera aspis</trd>), venena serpenteto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_d; <klr>(&c_z;&c_mol;&c_m;&c_ja;&c_ja;)</klr></trd>
  <trd lng="br">naer-speg</trd>
  <trd lng="ca">escur&ccedil;&oacute; pirinenc</trd>
  <trdgrp lng="cs">
    <trd>brejlovec</trd>,
    <trd>had</trd>,
    <trd>zmije</trd>
  </trdgrp>
  <trd lng="de">Aspisviper</trd>
  <trd lng="en">asp</trd>
  <trd lng="fr">aspic (vip&egrave;re)</trd>
  <trdgrp lng="hu">
    <trd>&aacute;spisvipera</trd>,
    <trd>&aacute;spisk&iacute;gy&oacute;</trd>
  </trdgrp>
  <trd lng="it">aspide</trd>
  <trdgrp lng="pt">
    <trd>naja</trd>,
    <trd>&aacute;spide</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_p;&c_i;&c_d; <klr>(&c_z;&c_m;&c_je;&c_ja;)</klr></trd>
  <trd lng="sk">druh vretenice</trd>
</drv>
</art>
