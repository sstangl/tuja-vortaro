<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: kenopo1.xml,v 1.8 2013/08/25 16:10:40 revo Exp $">
<kap><rad>kenopodiac</rad>/oj</kap>
<drv mrk="kenopo1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Chenopodiaceae</trd>).
    </dif>
    <refgrp tip="sub">
      <ref cel="bet.0o">beto</ref>,
      <ref cel="spinac.0o">spinaco</ref>
    </refgrp>
  </snc>
  <trd lng="en"><klr>the </klr>goosefoot<klr> family</klr></trd>
  <trd lng="de">G&auml;nsefu&szlig;gew&auml;chse</trd>
  <trd lng="fr">ch&eacute;nopodiac&eacute;es</trd>
  <trd lng="hu">libatopf&eacute;l&eacute;k</trd>
  <trd lng="nl">ganzevoetfamilie</trd>
  <trd lng="pl">komosowate</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  <trd lng="sv">m&aring;llv&auml;xter</trd>
</drv>
</art>

