<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id: respub.xml,v 1.21 2015/07/04 08:37:42 revo Exp $">
     <kap>
       <ofc>1</ofc>
       <rad>respublik</rad>/o
     </kap>
     <drv mrk="respub.0o">
       <kap><tld/>o</kap>
       <snc mrk="respub.0o.regximo">
         <uzo tip="fak">JUR</uzo>
         <dif>
           Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>
           en kiu <ref cel="suvere.0o">suvereno</ref> estas
           (konsiderinda parto de) la popolo kaj en kiu regas elektitaj
           reprezentantoj de la popolo.
         </dif>
         <refgrp tip="vid">
           <ref cel="demokr1.0o">demokratio</ref>.
         </refgrp>
       </snc>
       <snc mrk="respub.0o.sxtato">
         <uzo tip="fak">POL</uzo>
         <dif>
           &Scirc;tato kun <tld/>a re&gcirc;imo:
           <ekz>
             la kvina franca <tld/>o.
           </ekz>
         </dif>
       </snc>
  <trd lng="be">&c_r;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">rep&uacute;blica</trd>
  <trd lng="cs">republika</trd>
  <trd lng="de">Republik</trd>
  <trd lng="en">republic</trd>
  <trd lng="es">rep&uacute;blica</trd>
  <trd lng="fr">r&eacute;publique</trd>
  <trd lng="hu">k&ouml;zt&aacute;rsas&aacute;g</trd>
  <trd lng="nl">republiek</trd>
  <trd lng="pl">republika</trd>
  <trd lng="pt">rep&uacute;blica</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">republika</trd>
</drv>
     <drv mrk="respub.0ano">
       <kap><tld/>ano</kap>
       <snc mrk="respub.0o.sxtatano">
         <uzo tip="fak">JUR</uzo>
         <dif>
           Civitano de <tld/>o.
         </dif>
         <trd lng="be">&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_n; &c_r;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_ib;</trd>
         <trd lng="en">republican</trd>
         <trd lng="fr">membre d'une <ind>r&eacute;publique</ind></trd>
         <trd lng="hu">k&ouml;zt&aacute;rsas&aacute;gbeli</trd>
       </snc>
       <snc mrk="respub.0o.partiano">
         <uzo tip="fak">POL</uzo>
         <dif>
           Membro de la <tld lit="R"/>a partio de Usono.
         </dif>
         <trd lng="be">&c_r;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
         <trd lng="en">Republican</trd>
         <trd lng="hu">republik&aacute;nus</trd>
       <trd lng="nl">republikein</trd>
       <trd lng="fr">r&eacute;publicain</trd>
       <trd lng="pl">republikanin</trd>
       <trd lng="sk">republik&aacute;n</trd>
      </snc>
  <trd lng="ca">republic&agrave;</trd>
  <trd lng="cs">republikán</trd>
  <trd lng="de">Republikaner</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
</drv>
     <drv mrk="respub.0isto">
       <kap><tld/>isto</kap>
       <snc>
         <uzo tip="fak">POL</uzo>
         <dif>
           Adepto de <tld/>ismo.
         </dif>
         <ref tip="ant" cel="monarh.0isto">monarkisto</ref>
       </snc>
  <trd lng="be">&c_r;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="cs">republikán</trd>
  <trd lng="fr">r&eacute;publicain</trd>
  <trdgrp lng="hu">
         <trd>republik&aacute;nus</trd>,
         <trd>k&ouml;zt&aacute;rsas&aacute;gi</trd>
       </trdgrp>
  <trd lng="nl">republikein</trd>
  <trd lng="pl">republikanin</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">republik&aacute;n</trd>
</drv>
   </art>
