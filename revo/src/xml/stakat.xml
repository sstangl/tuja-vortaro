<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stakat.xml,v 1.7 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stakat</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="stakat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vo&ccirc;a a&ubreve; muzikila sonigo,
      kiu disigas la sinsekvajn tonojn per abruptaj ekoj
      a&ubreve; e&ccirc; pa&ubreve;zetoj.
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="legat1.0o">legato</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_t;&c_a;&c_k;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szaggatott el&odblac;ad&aacute;sm&oacute;d</trd>,
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>staccato</trd>
  </trdgrp>
  <trd lng="pl">staccato</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_k;&c_a;&c_t;&c_o;</trd>
</drv>

<drv mrk="stakat.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Mallige, disigante la sinsekvajn tonojn.
    </dif>
  </snc>
  <trd lng="cs">staccato</trd>
  <trdgrp lng="fr">
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szaggatottan</trd>,
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>staccato</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_k;&c_a;&c_t;&c_o;</trd>
  <trd lng="sk">staccato</trd>
</drv>
</art>

