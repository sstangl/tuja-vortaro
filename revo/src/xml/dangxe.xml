<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dangxe.xml,v 1.30 2013/07/06 20:10:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dan&gcirc;er</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dangxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dangxe.0o.KOMUNE">
    <dif>
      Ebleco de malbono, de malfeli&ccirc;o:
      <ekz>
        veni, fali en <tld/>on;
      </ekz>
      <ekz>
        meti en <tld/>on de morto;
      </ekz>
      <ekz>
        eviti <tld/>on;
      </ekz>
      <ekz>
        <tld/>o pendanta super Egipto
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 15</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o minacas iun<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiam la estonteco de nia afero estos absolute ekster
        <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="risk">risko</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="har">hareto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;</trd>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">da&ntilde;jer</trd>
  <trdgrp lng="ca">
    <trd>perill</trd>,
    <trd>risc</trd>
  </trdgrp>
  <trd lng="de">Gefahr</trd>
  <trd lng="en">danger</trd>
  <trd lng="es">peligro</trd>
  <trd lng="fa">&a_kh;&a_T;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>danger</trd>,
    <trd>p&eacute;ril</trd>
  </trdgrp>
  <trd lng="hu">vesz&eacute;ly</trd>
  <trd lng="nl">gevaar</trd>
  <trdgrp lng="pl">
    <trd>niebezpiecze&nacute;stwo</trd>,
    <trd>zagro&zdot;enie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>perigo</trd>,
    <trd>risco</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">nebezpe&ccaron;enstvo</trd>
  <trd lng="sv">fara</trd>
  <trd lng="tr">tehlike</trd>
</drv>

<drv mrk="dangxe.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc mrk="dangxe.0a.KOMUNE">
    <dif>
      Enhavanta, prezentanta <tld/>on:
      <ekz>
        <tld/>a vojo, situacio, malsano, homo, najbaro;
      </ekz>
      <ekz>
        estinta amiko estas plej <tld/>a malamiko
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_s;&c_je;&c_n;</trd>
  <trd lng="br">da&ntilde;jerus</trd>
  <trd lng="ca">perill&oacute;s</trd>
  <trd lng="cs">nebezpe&ccaron;n&yacute;</trd>
  <trd lng="de">gef&auml;hrlich</trd>
  <trdgrp lng="en">
    <trd>dangerous</trd>,
    <trd>hazardous</trd>
  </trdgrp>
  <trd lng="es">peligroso</trd>
  <trd lng="fa">&a_kh;&a_T;&a_r;&a_n;&a_A;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>dangereux</trd>,
    <trd>nocif</trd>
  </trdgrp>
  <trd lng="hu">vesz&eacute;lyes</trd>
  <trd lng="nl">gevaarlijk</trd>
  <trdgrp lng="pl">
    <trd>niebezpieczny</trd>,
    <trd>zagra&zdot;aj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nebezpe&ccaron;n&yacute;</trd>
  <trd lng="sv">farlig</trd>
  <trd lng="tr">tehlikeli</trd>
</drv>

<drv mrk="dangxe.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dangxe.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        por virta orelo ne <tld/>as vorto malbela
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">beza&ntilde; <ind>da&ntilde;jerus</ind></trd>
  <trd lng="ca">ser <ind>perill</ind>&oacute;s</trd>
  <trd lng="de">gef&auml;hrlich sein</trd>
  <trd lng="fa">&a_kh;&a_T;&a_r;&a_n;&a_A;&f_k; &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>dangereux</ind></trd>,
    <trd>pr&eacute;senter un <ind>danger</ind></trd>
  </trdgrp>
  <trd lng="hu">vesz&eacute;lyes</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>niebezpiecznym</ind></trd>,
    <trd>zagra&zdot;a&cacute;</trd>,
    <trd>by&cacute; <ind>zagro&zdot;eniem</ind></trd>
  </trdgrp>
  <trd lng="nl">gevaarlijk zijn</trd>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol;
  <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="sk">hrozi&tcaron;</trd>
</drv>

<drv mrk="dangxe.ekster0a">
  <kap>ekster<tld/>a</kap>
  <snc mrk="dangxe.ekster0a.KOMUNE">
    <dif>
      Ne estanta en <tld/>o;
      <ref tip="sin" cel="sekur.0a">sekura</ref>:
      <ekz>
        la tendaro opiniis sin tute ekster<tld/>a
        <fnt><bib>MT</bib>, <lok>&Jug; 8:11</lok></fnt>;
      </ekz>
      <ekz>
        Mi donos al ili la eblon dormi ekster<tld/>e
        <fnt><bib>MT</bib>, <lok>&Hos; 2:18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;-&c_z;&c_a;
  <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;&c_j;</ind></trd>
  <trdgrp lng="br">
    <trd>barrek<klr tip="ind"> (er-maez a za&ntilde;jer)</klr></trd>,
    <trd>er-maez a za&ntilde;jer</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>segur</trd>,
    <trd>a estalvi</trd>,
    <trd>fora de perill</trd>
  </trdgrp>
  <trd lng="de">au&szlig;er <ind>Gefahr</ind></trd>
  <trdgrp lng="en">
    <trd>out of <ind>danger</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_r; &a_A;&a_z; <ind>&a_kh;&a_T;&a_r;</ind></trd>,
    <trd>&a_A;&a_m;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&ucirc;r<klr tip="ind"> (hors de danger)</klr></trd>,
    <trd>hors de <ind>danger</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>biztons&aacute;gos</trd>,
    <trd>vesz&eacute;lyen k&iacute;v&uuml;li</trd>
  </trdgrp>
  <trd lng="nl">buiten <ind>gevaar</ind></trd>
  <trdgrp lng="pl">
    <trd>z dala od <ind>niebezpiecze&nacute;stwa</ind></trd>,
    <trd>z dala od <ind>zagro&zdot;enia</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;
    <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>,
    <trd>&c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_v; <ind>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="dangxe.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="dangxe.en0igi.KOMUNE">
    <dif>
      Meti en <tld/>on:
      <ekz>
        kial ni anka&ubreve; en<tld/>i&gcirc;as &ccirc;iun horon?
        <fnt><bib>NT</bib>, <lok>&Ko1; 15:30</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxance.0i">&scirc;anceli</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;
  <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_y;</ind></trd>
  <trdgrp lng="br">
    <trd>arvari&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>lakaat en <ind>da&ntilde;jer</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>posar en <ind>perill</ind></trd>,
    <trd>arriscar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gef&auml;hrden</trd>
    <trd>in <ind>Gefahr bringen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>endanger</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_h; <ind>&a_kh;&a_T;&a_r;</ind> &a_A;&a_n;&a_d;&a_A;&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>mettre en <ind>danger</ind></trd>,
    <trd>exposer<klr tip="ind"> (&agrave; un danger)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vesz&eacute;lybe sodor</trd>,
    <trd>vesz&eacute;lyeztet</trd>
  </trdgrp>
  <trd lng="nl">in <ind>gevaar</ind> brengen</trd>
  <trd
  lng="ru">&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;
  <ind>&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  <trd lng="sk">ohrozi&tcaron;</trd>
</drv>

<drv mrk="dangxe.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="dangxe.sen0a.KOMUNE">
    <dif>
      Ne prezentanta <tld/>on; sen <tld/>o:
      <ekz>
        sen<tld/>a malsano <klr>(sen risko por la vivo)</klr>;
      </ekz>
      <ekz>
        vivo modera estas vivo sen<tld/>a
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pri tio pli sen<tld/>e estis silenti
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 14</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_s;&c_sh;&c_k;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>inofensiu</trd>,
      <trd>innocu</trd>
    </trdgrp>
    <trd lng="de">ungef&auml;hrlich</trd>
    <trdgrp lng="en">
      <trd>nondangerous</trd>,
      <trd>nonhazardous</trd>
    </trdgrp>
    <trd lng="fa">&a_b;&f_y;&zwnj;&a_kh;&a_T;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>b&eacute;nin</trd>,
      <trd>anodin</trd>,
      <trd>sans <ind>danger</ind></trd>,
      <trd>inoffensif</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vesz&eacute;lytelen</trd>,
      <trd>&aacute;rtalmatlan</trd>,
    </trdgrp>
    <trdgrp lng="nl">
      <trd>onschuldig</trd>,
      <trd>ongevaarlijk</trd>,
      <trd>onschadelijk</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bezpieczny</trd>,
      <trd>nieszkodliwy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dangxe.sen0a.ekserdangxera">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="dangxe.ekster0a">Ekster<tld/>a</ref>;
      <ref tip="dif" cel="sekur.0a">sekura</ref>:
      <ekz>
        vi sentis vin sen<tld/>a en via malboneco
        <fnt><bib>MT</bib>, <lok>&Jes; 47:10</lok></fnt>;
      </ekz>
      <ekz>
        Miajn decidojn observu [...], kaj tiam vi lo&gcirc;os en la
        lando sen<tld/>e
        <fnt><bib>MT</bib>, <lok>&Lev; 25:18</lok></fnt>;
      </ekz>
      <ekz>
        ne &gcirc;enas min al vi skribi ripete, kaj por vi tio estas
        sen<tld/>iga
        <fnt><bib>NT</bib>, <lok>&Fil; 3:1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ja;&c_k;&c_ib;
    &c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a; &c_w;
    <ind>&c_n;&c_je;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_c;&c_y;</ind></trd>
    <trdgrp lng="ca">
      <trd>segur</trd>,
      <trd>a estalvi</trd>
    </trdgrp>
    <trd lng="fa">&a_d;&a_w;&a_r; &a_A;&a_z; <ind>&a_kh;&a_T;&a_r;</ind></trd>
    <trd lng="fr">s&ucirc;r<klr tip="ind"> (hors de danger)</klr></trd>
    <trd lng="nl">buiten <ind>gevaar</ind><klr tip="ind">
    (arch.)</klr></trd>
    <trd lng="ru">&c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_v; <ind>&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </snc>
  <trd lng="br">diza&ntilde;jer</trd>
</drv>

<drv mrk="dangxe.sen0eco">
  <kap>sen<tld/>eco</kap>
  <snc mrk="dangxe.sen0eco.KOMUNE">
    <dif>                  
      Foresto de &ccirc;ia <tld/>o,
      <ref tip="vid" cel="sekur.0eco.KOMUNE">sekureco</ref>:
      <ekz>
        le&gcirc;o garantios al Esperanto <klr>...</klr>
        plenan sen<tld/>econ kontra&ubreve; &ccirc;iuj personaj kapricoj
        <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        esplori la sen<tld/>econ de kuracilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>innocuit&eacute;</trd>,
    <trd>s&ucirc;ret&eacute;<klr tip="ind">
    (innocuit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onschadelijkheid</trd>,
    <trd>ongevaarlijkheid</trd>,
    <trd>onschuldigheid</trd>
  </trdgrp>
</drv>
</art>
