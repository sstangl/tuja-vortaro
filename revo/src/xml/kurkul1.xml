<vortaro>
<art mrk="$Id: kurkul1.xml,v 1.8 2013/08/31 15:10:46 revo Exp $">
<kap>
  <rad>kurkulied</rad>/oj
</kap>
<drv mrk="kurkul1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Curculionidae</trd>), kun rostre longigita kapo.
    </dif>
  </snc>
  <trd lng="ca">curculi&ograve;nids</trd>
  <trd lng="de">R&uuml;sselk&auml;fer</trd>
  <trd lng="hu">orm&aacute;nyos <ind>bogarak</ind></trd>
  <trdgrp lng="nl">
    <trd>snuittorren<klr tip="ind"> (fam.)</klr></trd>,
    <trd>snuitkevers<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_l;&c_o;&c_n;&c_i;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
