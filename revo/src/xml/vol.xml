<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vol.xml,v 1.40 2016/01/04 08:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vol</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vol.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vol.0i.deziri">
    <dif>
      Havi deziron, intencon, decidon, senti bezonon pri io a&ubreve;
      iu:
      <ekz>
        <tld/>i a&ubreve; ne<tld/>i neniu malpermesas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mi havas &ccirc;iam la kapablon <tld/>i, sed ne &ccirc;iam la
        forton realigi;
      </ekz>
      <ekz>
        <ind>kiu <tld/>as, tiu povas</ind><fnt>B</fnt>;
        <trd lng="hu">aki <ind>akar</ind>, az tud</trd>
      </ekz>
      <ekz>
        viro, kiu ne scias <tld/>i;
      </ekz>
      <ekz>
        <tld/>u, kion vi <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>kion oni <tld/>as, tion oni povas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">mindent lehet, csak <ind>akarni</ind> kell</trd>
        <trd lng="ru">&c_h;&c_o;&c_t;&c_je;&c_t;&c_mol;
        &c_z;&c_n;&c_a;&c_ch;&c_i;&c_t; &c_m;&c_o;&c_ch;&c_mol;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <tld/>anta kruro ne laci&gcirc;as de kuro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Kontraste al
      <ref tip="sin" cel="dezir.0i.porSiMem">deziri<sncref/></ref>,
      <tld/>i ofte implicas intencon fari ion por realigi la deziratan.
    </rim>
  </snc>
  <snc mrk="vol.0i.strebi">
    <dif>
      <ref tip="sin" cel="streb.0i">Strebante</ref> al io,
      koncentri sian atenton je ago, por ke &gcirc;i
      efektivi&gcirc;u; intenci, decidi fari ion:
      <ekz>
        mi <tld/>as diri al vi la veron
        <fnt><bib>F</bib>, <lok>&FE; 24</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke vi tien iru
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        maldiligentulo &scirc;ovas sian manon en la po&scirc;on, kaj ne
        <tld/>as venigi &gcirc;in al sia bu&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dio <tld/>u, ke...;
      </ekz>
      <ekz>
        li ne <tld/>is e&ccirc; respondi al mi;
      </ekz>
      <ekz>
        &ccirc;u vi volas, ke mi venigu lin?
      </ekz>
      <ekz>
        &gcirc;i ne akceptas, se vi <tld/>as e&ccirc; multe
        donaci<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kial vi <tld/>as ser&ccirc;i al vi plezuron &ccirc;e fremda
        virino?<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiam mi <tld/>as esprimi direkton, mi...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trovinte neniun, kiu <tld/>us <klr>(bon<tld/>us)</klr> &scirc;in
        akcepti
        <fnt><bib>F</bib>, <lok>&FE; 23</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.0i">celi</ref>,
      <ref cel="decid.0i">decidi</ref>,
      <ref cel="intenc.0i">intenci</ref>
    </refgrp>
  </snc>
  <snc mrk="vol.0i.ien">
    <dif>
      <tld lit="V"/>i atingi lokon, esti ie:
      <ekz>
        mi <tld/>as en militon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is en kale&scirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>ne <tld/>as kokin' al festeno, sed oni &gcirc;in trenas
        perforte</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a kecske sem menne a v&aacute;s&aacute;rra, de
        &uuml;tik a
        h&aacute;tulj&aacute;t</trd>
      </ekz>
      <ekz>
        la hundeto levi&gcirc;as kaj <tld/>as anka&ubreve; sur la
        balancilon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="V"/>i akcepti, havi:
      <ekz>
        <ind><tld/>us kato fi&scirc;ojn, sed la akvon &gcirc;i
        timas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">aki f&eacute;l a v&iacute;zt&odblac;l, nem eszik
        halat</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">cht&iacute;t</trd>
  <trd lng="de">wollen</trd>
  <trd lng="el">&theta;&epsilon_ton;&lambda;&omega;</trd>
  <trdgrp lng="en">
    <trd>want</trd>
    <trd>will</trd>
  </trdgrp>
  <trd lng="es">querer</trd>
  <trd lng="fr">vouloir</trd>
  <trd lng="he">&lamed;&resh;&tsadi;&vav;&tav;</trd>
  <trd lng="hu">akar</trd>
  <trdgrp lng="id">
    <trd>ingin</trd>,
    <trd>mau</trd>
  </trdgrp>
  <trd lng="nl">willen</trd>
  <trd lng="pt">querer</trd>
  <trd lng="ru">&c_h;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">chcie&tcaron;</trd>
  <trd lng="sv">vilja<klr> (verb)</klr></trd>
  <trd lng="th">&#3605;&#3657;&#3629;&#3591;&#3585;&#3634;&#3619;</trd>
  <trd lng="tp">wile</trd>
  <trd lng="vo">vil&ouml;n</trd>
</drv>

<drv mrk="vol.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a ago, movo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsci.0a">konscia</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ja;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>willentlich</trd>,
    <trd>absichtlich</trd>
  </trdgrp>
  <trd
  lng="el">&epsilon;&theta;&epsilon;&lambda;&omikron;&ny;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">volontaire</trd>
  <trd lng="he">&resh;&tsadi;&vav;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;nd&eacute;kos</trd>,
    <trd>akaratlagos</trd>,
    <trd>akarati</trd>
  </trdgrp>
  <trd lng="nl">gewild</trd>
  <trd lng="pt">volunt&aacute;rio</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="vol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kapablo <tld/>i:
      <ekz>
        &ccirc;u la homo posedas liberan <tld/>on?
      </ekz>
      <ekz>
        <ind>kie estas <tld/>o, tie estas vojo</ind>;
        <trd lng="hu">mindent lehet, csak <ind>akarni</ind> kell</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>de la <tld/>o la ordono pli efikas ol bastono</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="ru">&c_o;&c_h;&c_o;&c_t;&c_a; &c_p;&c_u;&c_shch;&c_je;
        &c_n;&c_je;&c_v;&c_o;&c_l;&c_i;</trd>
      </ekz>
      <ekz>
        <tld/>o kaj deziro le&gcirc;ojn ne konas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        energia, malforta <tld/>o;
      </ekz>
      <ekz>
        mi demandis min, kia stre&ccirc;o de la <tld/>o levis
        lin<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_l;&c_ja;</trd>
    <trd lng="de">Wille</trd>
    <trd lng="es">voluntad</trd>
    <trd lng="he">&kaf;&het; &resh;&tsadi;&vav;&fnun;</trd>
    <trd lng="ru">&c_v;&c_o;&c_l;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Tio, kion oni precize volas:
      <ekz>
        <ind>kiom da koroj, tiom da <tld/>oj</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ah&aacute;ny sz&iacute;v, annyi
        sz&aacute;nd&eacute;k</trd>
      </ekz>
      <ekz>
        konformi&gcirc;i al ies <tld/>o;
      </ekz>
      <ekz>
        tia estas mia <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o de la &ccirc;ielo plenumi&gcirc;u!
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Wunsch</trd>
    <trd lng="el">&theta;&epsilon_ton;&lambda;&eta;&sigma;&eta;</trd>
    <trd lng="he">&resh;&tsadi;&vav;&fnun;</trd>
    <trd lng="hu">akarat</trd>
    <trd lng="nl">wil</trd>
    <trd lng="pt">vontade</trd>
    <trd lng="ru">&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;&aacute;n&iacute;</trd>,
    <trd>v&uring;le</trd>
  </trdgrp>
  <trd lng="fr">volont&eacute;</trd>
  <trdgrp lng="sk">
    <trd>prianie</trd>,
    <trd>v&ocirc;&lcaron;a</trd>
  </trdgrp>
  <trd lng="sv">vilja<klr> (subst.)</klr></trd>
  <trdgrp lng="th">
    <trd>&#3588;&#3623;&#3634;&#3617;&#3605;&#3657;&#3629;&#3591;&#3585;&#3634;&#3619;</trd>,
    <trd>&#3626;&#3636;&#3656;&#3591;&#3607;&#3637;&#3656;&#3605;&#3657;&#3629;&#3591;&#3585;&#3634;&#3619;</trd>
  </trdgrp>
  <trd lng="tp">wile</trd>
</drv>

<drv mrk="vol.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="vol.0ema.PSI">
    <dif>
      Kiu prefere mem deziras, mem decidas, la&ubreve; propra <tld/>o;
      <ref tip="vid" cel="rezolut.0a">rezoluta</ref>:
      <ekz>
        vi elektis virinon tre <tld/>eman
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la
        Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ced.mal0ema.KOMUNE">malcedema</ref>
    </refgrp>
  </snc>
  <trd lng="cs">ochotn&yacute;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;cid&eacute;<klr> (caract&egrave;re)</klr></trd>,
    <trd>volontaire<klr> (caract&egrave;re)</klr></trd>
  </trdgrp><trd lng="fr"></trd>
  <trd lng="sk">ochotn&yacute;</trd>
</drv>

<drv mrk="vol.0ene0e">
  <kap><tld/>e ne<tld/>e</kap>
  <snc>
    <dif>
      Devigite; &ccirc;u oni <tld/>as a&ubreve; ne:
      <ekz>
        <tld/>e ne<tld/>e, li devas cedi<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_l;&c_ja;&c_j;-&c_n;&c_ja;&c_v;&c_o;&c_l;&c_ja;&c_j;</trd>,
      <trd><ind>&c_h;&c_o;&c_ch;&c_a;&c_sh;</ind> &c_n;&c_ja;
      &c_h;&c_o;&c_ch;&c_a;&c_sh;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>wohl oder &uuml;bel</trd>,
      <trd>ob man will oder nicht</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>velis nolis</trd>,
      <trd>quieras o no quieras</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>akarva-akaratlanul</trd>,
      <trd>ha <ind>akarja</ind>, ha nem</trd>
    </trdgrp>
    <trd lng="id">mau tak mau</trd>
    <trd lng="nl">willens of onwillens</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_l;&c_je;&c_j;-&c_n;&c_je;&c_v;&c_o;&c_l;&c_je;&c_j;</trd>,
      <trd>&c_h;&c_o;&c_ch;&c_je;&c_sh;&c_mol; &c_n;&c_je;
      &c_h;&c_o;&c_ch;&c_je;&c_sh;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="pt">
    <trd>queira ou n&atilde;o</trd>,
    <trd>querendo ou n&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bon gr&eacute; mal gr&eacute;</trd>,
    <trd>qu'il le veuille ou non</trd>
  </trdgrp>
  <trd lng="sv">vare sig man <ind>vill</ind> eller inte</trd>
</drv>

<drv mrk="vol.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="vol.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Filozofia skolo kiu prezentas la <tld/>on kiel la
      &ccirc;efprincipon de la estado (a&ubreve; komprenado):
      <ekz>
        Kartezio mem dependigis &ccirc;ion de la <tld/>o &#8208;
        la&ubreve; tiu
        <tld/>ismo, de dia <tld/>o dependas la logikaj principoj, kaj
        de dia <tld/>o dependas la eterneco de la animo, kiu do ne
        estus per si mem senmorta
        <fnt>
        <lok><url
        ref="http://www.cfh.ufsc.br/~simpozio/Esperanto/COACTIO.html"
        >Concursus Dei</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ismo kontraste al la raciismo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="vol.0ismo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Skolo kiu rigardas la <tld/>on kiel la &ccirc;effaktoron de la
      mensa vivo.
    </dif>
  </snc>
  <snc mrk="vol.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Agado, precipe agado politika, kiu ne sufi&ccirc;e atentas la
      reala&jcirc;ojn kaj sin direktas per arbitraj decidoj de la
      agantoj (de la politika estraro).
      <ekz>
        <tld/>ismo kontraste al la raciismo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">voluntarism</trd>
  <trd lng="fr">volontarisme</trd>
  <trd lng="hu">voluntarizmus</trd>
  <trd
  lng="ru">&c_v;&c_o;&c_l;&c_ju;&c_n;&c_t;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="vol.bon0i">
  <kap>bon<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti tiel bona, ke oni <tld/>as; afable konsenti:
      <ekz>
        <ind>bon<tld/>u</ind> sidi &ccirc;i tie! (=sidu &ccirc;i tie, mi
        petas!).
        <trd lng="be">&c_k;&c_a;&c_l;&c_ib;
        &c_l;&c_a;&c_s;&c_k;&c_a;</trd>
          <trdgrp lng="hu">
            <trd>l&eacute;gy sz&iacute;ves</trd>,
            <trd>legyen sz&iacute;ves</trd>,
            <trd>k&eacute;rem</trd>,
            <trd>tess&eacute;k</trd>
          </trdgrp>
        <trd
        lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_l;&c_u;&c_j;&c_s;&c_t;&c_a;</trd>
      </ekz>
    </dif>
    <rim>
    Kelkaj uzas anstata&ubreve;e la formon <ekz>bon<tld/>e ...-u</ekz>,
    ekzemple
    <ekz>bon<tld/>e sidu &ccirc;i tie!</ekz> Tio estas sufi&ccirc;e
    logika
    alternativo,
    sed &gcirc;i ne estas kutima, nek tradicia. Nepre ne uzu duoblan
    u-formon:
    ne diru *<ekz>bon<tld/>u sidu &ccirc;i
    tie!</ekz>*<fnt><bib>PMEG</bib></fnt>.
    </rim>
    <trd lng="fr">bien vouloir</trd>
    <trdgrp lng="be">
      <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_a;
      &c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_l;&c_a;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;&c_m;</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_o;
      &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_b;&c_y;&c_t;&c_mol; &c_t;&c_a;&c_k;
      <ind>&c_l;&c_ju;&c_b;&c_je;&c_z;&c_n;&c_y;&c_m;</ind></trd>,
      <trd>&c_s;&c_o;&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <tld lit="V"/>i fari bonon al iu:
      <ekz>
        lia bon<tld/>o donis al mi pardonon;
      </ekz>
      <ekz>
        malbon<tld/>a kritikisto, recenzisto.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_l;&c_a;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_m;&c_je;&c_c;&c_mol;
      <ind>&c_l;&c_a;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_z;
      <ind>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>
    </trdgrp>
    <trd
    lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
    <trd lng="fr">avoir bon <ind>c&oelig;ur</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>b&yacute;t laskav</trd>,
    <trd>r&aacute;&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>die G&uuml;te haben</trd>,
    <trd>so gut sein</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&vav;&ayin;&yod;&lamed;
  &bet;&tet;&vav;&bet;&vav;</trd>
  <trd lng="hu">sz&iacute;veskedik</trd>
  <trd lng="id">[<ind>sila</ind>] mempersilakan</trd>
  <trd lng="nl">welwillend zijn</trd>
  <trd lng="sk">r&aacute;&ccaron;i&tcaron;</trd>
  <trdgrp lng="th">
    <trd>&#3585;&#3619;&#3640;&#3603;&#3634;</trd>,
    <trd>&#3650;&#3611;&#3619;&#3604;</trd>,
    <trd>&#3652;&#3604;&#3657;&#3650;&#3611;&#3619;&#3604;</trd>
  </trdgrp>
</drv>

<drv mrk="vol.kontraux0a">
  <kap>kontra&ubreve;<tld/>a<fnt>Z</fnt>,
  <var><kap>malgra&ubreve;<tld/>a</kap></var></kap>
  <snc mrk="vol.kontraux0a.KOMUNE">
    <dif>
      Okazonta malgra&ubreve; ies <tld/>o; trudita:
      <ekz>
        edzi&gcirc;o kontra&ubreve;<tld/>a;
      </ekz>
      <ekz>
        &scirc;i kapjesis al la &ccirc;efde&jcirc;oristo kaj
        malgra&ubreve;<tld/>e koncentri&gcirc;is sur liaj
        senkarnaj <klr>...</klr> lipoj
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_m;&c_u;&c_sh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_m;&c_u;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">against one's will</trd>
  <trdgrp lng="fr">
    <trd>forc&eacute;<klr> (impos&eacute;)</klr></trd>,
    <trd>impos&eacute;</trd>
  </trdgrp>
  <trd
  lng="he">&bet;&lamed;&tav;&yod;-&resh;&tsadi;&vav;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>nem <ind>k&iacute;v&aacute;nt</ind></trd>,
    <trd>akarat&aacute;val ellent&eacute;tes</trd>
  </trdgrp>
  <trd lng="nl">onvrijwillig</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_n;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_u;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vol.laux0e">
  <kap>la&ubreve;<tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; sia <tld/>o a&ubreve; gusto.
    </dif>
  </snc>
  <trd lng="en">at will</trd>
  <trdgrp lng="fr">
    <trd>&agrave; son gr&eacute;</trd>,
    <trd>ad libitum</trd>
  </trdgrp>
  <trd lng="he">&kaf;&resh;&tsadi;&vav;&nun;&vav;</trd>
  <trdgrp lng="hu">
    <trd>akarata szerint</trd>,
    <trd>tetsz&eacute;se szerint</trd>,
    <trd>szabadon</trd>
  </trdgrp>
  <trd lng="nl">naar <ind>believen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;
    <ind>&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_ju;</ind></trd>,
    <trd>&c_p;&c_o;
    &c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_m;&c_u;
    <ind>&c_u;&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_n;&c_i;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="th">&#3605;&#3634;&#3617;&#3607;&#3637;&#3656;&#3605;&#3657;&#3629;&#3591;&#3585;&#3634;&#3619;</trd>
</drv>

<drv mrk="vol.libera0o">
  <kap>libera <tld/>o</kap>
  <snc mrk="vol.libera0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Kapablo elekti inter pluraj eblaj agoj, konscie kaj inteligente 
      taksante, kio estas preferinda:
      <ekz>
        por reteni la sklavojn en la sklava stato, necesas forpreni de 
        ili la liberan <tld/>on kaj la eblon de kleri&gcirc;o
        <fnt>
          <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
          <vrk>Paradoksisto</vrk>,
          <bib>LOdE</bib>,
          <lok>2001:10 (84)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">libre arbitre</trd>
</drv>

<drv mrk="vol.liber0ismo">
  <kap>liber<tld/>ismo</kap>
  <snc mrk="vol.liber0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino, kiu agnoskas <ref tip="vid" 
      cel="vol.libera0o.FIL">liberan <tld/>on</ref> en homoj:
      <ekz>
        Aliaj temoj estas la debato inter liber<tld/>ismo kaj 
        determinismo, kaj la naturo de la realo
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;The
        Matrix&rightquot;, 
        2007-01-30</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="determ.0ismo.FIL">determinismo <sncref/></ref>
  </refgrp>
</drv>

<drv mrk="vol.liber0a">
  <kap>liber<tld/>a, <var><kap>mem<tld/>a</kap></var>,
  <var><kap>propra<tld/>a</kap></var></kap>
  <snc>
    <dif>
      Okazanta la&ubreve; ies <tld/>o, sen ekstera trudo a&ubreve;
      premo:
      <ekz>
        liber<tld/>a decido;
      </ekz>
      <ekz>
        mem<tld/>a ekzilo;
      </ekz>
      <ekz>
        mem<tld/>e sin enskribi en la armeon<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi mem<tld/>e pasigis parton de mia vivo en grandaj suferoj kaj
        oferoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni propra<tld/>e donis al la mondo tiun rajton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>jugo propra<tld/>a ne estas malmola</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">&ouml;nk&eacute;nt v&aacute;llalt iga nem
        t&ouml;ri a nyakat</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_b;&c_r;&c_a;&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_h;&c_v;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dobrovoln&yacute;</trd>,
    <trd>libovoln&yacute;</trd>
  </trdgrp>
  <trd lng="de">freiwillig</trd>
  <trdgrp lng="el">
    <trd>&alfa;&ypsilon;&theta;&omikron_ton;&rho;&my;&eta;&tau;&omikron;&sigma_fina;</trd>,
    <trd>&epsilon;&theta;&epsilon;&lambda;&omikron;&ny;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">of one's own will</trd>
  <trdgrp lng="fr">
    <trd>libre<klr> (librement d&eacute;cid&eacute;)</klr></trd>,
    <trd>volontaire<klr> (librement d&eacute;cid&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&pe;&shin;&yod;</trd>,
    <trd>&mem;&resh;&tsadi;&vav;&fnun;</trd>
  </trdgrp>
  <trd lng="hu">&ouml;nk&eacute;ntes</trd>
  <trd lng="nl">vrijwillig</trd>
  <trdgrp lng="pt">
    <trd>espont&acirc;neo</trd>,
    <trd>volunt&aacute;rio</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dobrovo&lcaron;n&yacute;</trd>,
    <trd>&lcaron;ubovo&lcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="sv">frivillig</trd>
</drv>

<drv mrk="vol.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="vid" cel="rifuz.0i">Rifuzi</ref>, ne <tld/>i:
      <ekz>
        ne mal<tld/>u puni knabon
        <fnt><bib>MT</bib>, <lok>&Sen; 23:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; <ind>&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol;</ind></trd>,
    <trd>&c_n;&c_je; <ind>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">necht&iacute;t n&#65533;&#8250;co</trd>
  <trd lng="fr">refuser</trd>
  <trdgrp lng="hu">
    <trd>elutas&iacute;t</trd>,
    <trd>vonakodik</trd>
  </trdgrp>
  <trd lng="nl">weigeren</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je; <ind>&c_h;&c_o;&c_t;&c_je;&c_t;&c_mol;</ind></trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">nechcie&tcaron; nie&ccaron;o</trd>
</drv>

<drv mrk="vol.ne0a">
  <kap>ne<tld/>a</kap>
  <snc>
    <dif>
      Sen partopreno de volo:
      <ekz>
        ili estis ne<tld/>a elfluo el &scirc;irita, senespera koro
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 22a, La fino de
        la
        intrigoj</lok></fnt>.
      </ekz>
      <ekz>
        nun mi havas ne<tld/>an ripozon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mimovoln&yacute;</trd>,
    <trd>necht&ecaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">unwilling</trd>
  <trd lng="fr">involontaire</trd>
  <trd lng="hu">akaratlan</trd>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>involunt&aacute;rny</trd>,
    <trd>nepodliehaj&uacute;ci v&ocirc;li</trd>
  </trdgrp>
</drv>

<drv mrk="vol.preter0a">
  <kap>preter<tld/>a</kap>
  <snc mrk="vol.preter0a.PSI">
    <dif>
      Farata sen klara volo a&ubreve; decido, <ref tip="vid"
      cel="sponta1.0a">spontanea</ref>:
      <ekz>
        la larmoj preter<tld/>e fluis
        <fnt><bib>Fab3</bib>, <lok>sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        se iu pentristo &gcirc;in vidis li preter<tld/>e
        kaptis la penikon por &gcirc;in pentri<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mimovoln&yacute;</trd>,
    <trd>ne&uacute;mysln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inconscient<klr> (spontan&eacute;)</klr></trd>,
    <trd>spontan&eacute;<klr> (geste, r&eacute;action...)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;nk&eacute;ntelen</trd>,
    <trd>spont&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_m;&c_o;
    <ind>&c_v;&c_o;&c_l;&c_i;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mimovo&lcaron;n&yacute;</trd>,
    <trd>nedobrovo&lcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="vol.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="vol.sen0a.PSI">
    <dif>
      Ne kapabla <tld/>i.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxaf.0o">&scirc;afo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>bezd&ecaron;&ccaron;n&yacute;</trd>,
    <trd>necht&ecaron;n&yacute;</trd>,
    <trd>ne&uacute;mysln&yacute;</trd>
  </trdgrp>
  <trd lng="de">willenlos</trd>
  <trdgrp lng="fr">
    <trd>aboulique</trd>,
    <trd>sans volont&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>akarat n&eacute;lk&uuml;li</trd>,
    <trd>nem <ind>sz&aacute;nd&eacute;kos</ind></trd>
  </trdgrp>
  <trd lng="nl">willoos</trd>
  <trd
  lng="ru">&c_b;&c_je;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">bez v&ocirc;le</trd>
</drv>
