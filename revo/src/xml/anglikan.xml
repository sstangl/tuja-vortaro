<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anglikan.xml,v 1.3 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anglikan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anglikan.0o">
  <kap><tld/>o</kap>
  <snc mrk="anglikan.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de <ref tip="vid" cel="anglikan.0ismo.KRI"><tld/>ismo</ref>:
      <ekz>
        li estas <tld/>o kaj &gcirc;is nun li ne enpa&scirc;is katolikan pre&gcirc;ejon
        <fnt>
          <aut>F. Hru&scaron;at</aut>,
          <vrk>Dumtempe mi ne Mortos</vrk>,
          <lok><url ref="http://www.ikue.org/cz/arkivo/a-42-2-01.htm#indekso">
          Dio Benu, 2001:2 (42)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">anglikán</trd>
  <trd lng="fr">anglican<klr> (subst.)</klr></trd>
  <trd lng="sk">anglikán</trd>
</drv>

<drv mrk="anglikan.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="anglikan.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Doktrino kaj praktiko de
      <ref tip="vid" cel="anglikan.0o.KRI"><tld/>oj</ref>;
      bran&ccirc;o de <ref tip="malprt" cel="krist.0anismo.KRI">kristanismo</ref>,
      disi&gcirc;inta de katolikismo en la 16a jarcento kaj 
      agnoskanta kiel a&ubreve;toritaton la re&gcirc;on a&ubreve; re&gcirc;inon de Anglujo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>anglikanizmus</trd>,
    <trd>anglikánství</trd>
  </trdgrp>
  <trd lng="fr">anglicanisme</trd>
  <trd lng="sk">anglikanizmus</trd>
</drv>
</art>

