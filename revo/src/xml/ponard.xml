<kap>
  <ofc>*</ofc>
  <rad>ponard</rad>/o
</kap>
<drv mrk="ponard.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Pinta amba&ubreve;tran&ccirc;a mallonga pikarmilo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">dýka</trd>
  <trd lng="de">Dolch</trd>
  <trd lng="en">dagger</trd>
  <trd lng="es">daga</trd>
  <trd lng="fa">&a_kh;&a_n;&a_j;&a_r;</trd>
  <trd lng="fr">poignard</trd>
  <trd lng="hu">t&odblac;r</trd>
  <trd lng="nl">dolk</trd>
  <trd lng="pt">punhal</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_zh;&c_a;&c_l;</trd>
  <trd lng="sk">dýka</trd>
  <trd lng="sv">dolk</trd>
</drv>

<drv mrk="ponard.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Piki per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol; <ind>&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;&c_a;&c_m;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_m;&c_u;&c_sh;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_r;&c_malmol;&c_g;&c_v;&c_a;&c_m; &c_s; <ind>&c_k;&c_a;&c_m;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">bodnout dýkou</trd>
  <trd lng="de">erdolchen</trd>
  <trd lng="en">stab</trd>
  <trd lng="es">apu&ntilde;alar</trd>
  <trd lng="fa">&a_kh;&a_n;&a_j;&a_r; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">poignarder</trd>
  <trd lng="hu">t&odblac;rrel lesz&uacute;r</trd>
  <trd lng="nl">met een <ind>dolk</ind> steken</trd>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol; <ind>&c_k;&c_i;&c_n;&c_zh;&c_a;&c_l;&c_o;&c_m;</ind></trd>
  <trd lng="sk">bodnúť dýkou</trd>
</drv>
</art>
