<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxicximek.xml,v 1.5 2009/04/29 16:30:52 revo Exp $">
<kap>
  <rad>&ccirc;i&ccirc;imek</rad>/o
</kap>
<drv mrk="cxicximek.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxicximek.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref> de iu el la
      nomadaj popoloj, kiuj vivis norde de <ref tip="vid"
      cel="aztek.0o.ETN">azteka</ref> Mezameriko:
      <ekz>
        <tld/>oj el Ksolotlo alvenis al Apano, en nuna Meksikio
        <fnt><bib>Viki</bib>, <lok>&leftquot;1224&rightquot;, 2005-07-25
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chichimeke</trd>
  <trd lng="fr">Chichim&egrave;que<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="it">cicimeco<klr tip="amb"> (sost.)</klr></trd>
  <trdgrp lng="pl">
    <trd>Chichimek</trd>,
    <trd>Cziczimek</trd>
  </trdgrp>
</drv>

<drv mrk="cxicximek.0a">
  <kap><tld/>a</kap>
  <snc mrk="cxicximek.0a.ETN">
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">chichim&egrave;que<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="it">cicimeco<klr tip="amb"> (agg.)</klr></trd>
  <trdgrp lng="pl">
    <trd>chichimecki</trd>,
    <trd>cziczimecki</trd>
  </trdgrp>
</drv>
</art>

