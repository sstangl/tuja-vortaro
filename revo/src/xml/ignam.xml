    <trd>jama</trd>,
    <trd>ziemniak japo&nacute;ski</trd>,
    <trd>bulwa pochrzynu</trd>
  </trdgrp>
  <trd lng="pt">inhame</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_s;</trd>,
    <trd>&c_i;&c_n;&c_mol;&c_ja;&c_m;</trd>
  </trdgrp>
  <trd lng="sv">jamsrot</trd>
  <trd lng="uk">&c_ja;&c_m;&c_s;</trd>
</drv>
