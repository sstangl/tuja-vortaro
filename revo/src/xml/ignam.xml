  <trd lng="be">&c_ja;&c_m;&c_s;</trd>
  <trd lng="de">Jamswurzel</trd>
  <trd lng="en">yam</trd>
  <trd lng="fr">igname</trd>
  <trdgrp lng="hu">
    <trd>jamgy&ouml;k&eacute;r</trd>,
    <trd>jamgum&oacute;</trd>
  </trdgrp>
  <trd lng="nl">broodvrucht</trd>
