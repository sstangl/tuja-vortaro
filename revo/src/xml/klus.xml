  <trd lng="cs">kluz</trd>
  <trd lng="de">Kl&uuml;se</trd>
  <trdgrp lng="en">
    <trd>hawse hole</trd>,
    <trd>hawse pipe</trd>
  </trdgrp>
  <trd lng="fr">&eacute;cubier</trd>
  <trd lng="nl">kluisgat</trd>
  <trd lng="pl">kluza</trd>
  <trd lng="ru">&c_k;&c_l;&c_ju;&c_z;</trd>
  <trd lng="sk">provn&yacute; otvor <klr>(pre kotevn&uacute; re&tcaron;az)</klr></trd>
</drv>
</art>
