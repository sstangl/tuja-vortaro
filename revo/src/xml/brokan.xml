<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brokan.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>brokant</rad>/i
</kap>
<drv mrk="brokan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      A&ccirc;eti kaj revendi jam uzitajn a&jcirc;ojn.
    </dif>
    <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;
      <ind>&c_p;&c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;&c_m;&c_ib;</ind>
      &c_r;&c_e;&c_ch;&c_a;&c_m;&c_ib;
    </trd>
    <trdgrp lng="de">
      <trd>mit Altware <ind>handeln</ind></trd>,
      <trd>mit Gebrauchtware <ind>handeln</ind></trd>,
      <trd>tr&ouml;deln</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>deal in second-hand goods</trd>,
      <trd>buy and sell used products</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_k;&a_A;&a_l;&a_A;&a_h;&a_A;&f_y;
      &a_d;&a_s;&a_t;&zwnj;&a_d;&a_w;&a_m;
      <ind>&a_f;&a_r;&a_w;&a_kh;&a_t;&a_n;</ind></trd>
      <trd>&a_s;&a_m;&a_s;&a_A;&a_r;&f_y;
      &f_k;&a_r;&a_d;&a_n;</trd>
      <trd>&f_k;&a_h;&a_n;&a_h;&zwnj;&a_f;&a_r;&a_w;&a_sh;&f_y;
      &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">brocanter</trd>
    <trdgrp lng="hu">
      <trd>haszn&aacute;lt cikkel <ind>kereskedik</ind></trd>,
      <trd>haszn&aacute;lt cikket <ind>&aacute;rul</ind></trd>
    </trdgrp>
    <trd lng="nl"><ind>tweedehands goed</ind>eren verkopen</trd>
    <trd lng="pl">handlowa&cacute; <ind>starzyzn&aogonek;</ind></trd>
    <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;
    &c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_m;&c_i;
    &c_v;&c_je;&c_shch;&c_a;&c_m;&c_i;</trd>
  </snc>
  <trd lng="cs">obchodovat se starými věcmi</trd>
  <trdgrp lng="sk">
    <trd>kšeftovať</trd>,
    <trd>obchodovať so starinou</trd>
  </trdgrp>
</drv>

<drv mrk="brokan.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Jam uzita objekto, kiun oni vendas denove, &ccirc;ar &gcirc;i
      ankora&ubreve; funkcias a&ubreve; i&gcirc;is rara&jcirc;o
      interesa por kolektantoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="antikv.0ajxo">antikva&jcirc;o</ref>,
    <ref cel="kolekt.0i">kolekti</ref>.
  </refgrp>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_a;&c_ja; &c_r;&c_e;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Altware</trd>,
    <trd>Gebrauchtware</trd>,
    <trd>Tr&ouml;del</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>second-hand good</trd>,
    <trd>used product</trd>
  </trdgrp>
  <trd lng="nl"><ind>tweedehands goed</ind></trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_l;&a_A;&f_y;
    &a_d;&a_s;&a_t;&zwnj;&a_d;&a_w;&a_m;</trd>
    <trd>&a_j;&a_n;&a_s;
    &a_m;&a_s;&a_t;&a_ayn;&a_m;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>antiquit&eacute;</trd>,
    <trd>article d'<ind>occasion</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>haszn&aacute;lt cikk</trd>,
    <trd>zsib&aacute;ru</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>starzyzna</trd>,
    <trd>rzecz z drugiej r&eogonek;ki</trd>
  </trdgrp>
  <trd
  lng="ru">&c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_a;&c_ja;
  &c_v;&c_je;&c_shch;&c_mol;</trd>
</drv>

<drv mrk="brokan.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>, kiu
      <tld/>adas.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>překupník</trd>,
    <trd>vetešník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Antiquar</trd>,
    <trd>Altwarenh&auml;ndler</trd>,
    <trd>Gebrauchth&auml;ndler</trd>,
    <trd>Tr&ouml;dler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dealer in second-hand goods</trd>,
    <trd>buyer and seller of used products</trd>,
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_w;&a_sh;&a_n;&a_d;&a_h;
    <klr tip="amb">(&f_k;&a_A;&a_l;&a_A;&a_h;&a_A;&f_y;
    &a_d;&a_s;&a_t;&zwnj;&a_d;&a_w;&a_m;)</klr></trd>
    <trd>&a_s;&a_m;&a_s;&a_A;&a_r;</trd>
    <trd>&f_k;&a_h;&a_n;&a_h;&zwnj;&a_f;&a_r;&a_w;&a_sh;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>antiquaire</trd>,
    <trd>brocanteur</trd>,
    <trd>fripier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zsib&aacute;rus</trd>,
    <trd>&oacute;csk&aacute;s</trd>,
    <trd>haszn&aacute;ltcikk-keresked&odblac;</trd>
  </trdgrp>
  <trd lng="nl">verkoper van <ind>tweedehands goed</ind>eren</trd>
  <trd lng="pl">handlarz <ind>starzyzn&aogonek;</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_mol;&c_jo;&c_v;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_i;&c_k;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kšeftár</trd>,
    <trd>starinár</trd>,
    <trd>špekulant</trd>
  </trdgrp>
</drv>
</art>
