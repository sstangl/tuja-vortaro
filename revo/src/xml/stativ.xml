<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stativ.xml,v 1.17 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stativ</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stativ.0o">
  <kap><tld/>o</kap>
  <snc mrk="stativ.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ordinara tripieda
      <ref tip="super" cel="baz.0o.postamento">bazo</ref>,
      uzata por subteni aparaton a&ubreve; instrumenton:
      <ekz>
        fotografaparatoj bezonas <tld/>ojn;
      </ekz>
      <ekz>
        kunmetebla <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="postam.0o">postamento</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_a;&c_t;&c_y;&c_w;</trd>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>podstavec</trd>,
    <trd>stativ</trd>,
    <trd>stojan</trd>,
    <trd>věšák</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stativ</trd>,
    <trd>St&auml;nder</trd>,    
    <trd>Dreibein</trd>,
    <trd>Tripode</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_h;&zwnj;&f_p;&a_A;&f_y;&a_h;</trd>
  <trd lng="fr">tr&eacute;pied</trd>
  <trdgrp lng="hu">
    <trd>m&udblac;szer&aacute;llv&aacute;ny</trd>,
    <trd>h&aacute;roml&aacute;b</trd>
  </trdgrp>
  <trd lng="nl">statief</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>podstavec</trd>,
    <trd>statív</trd>,
    <trd>stojan</trd>
  </trdgrp>
</drv>
</art>
