<art mrk="$Id: korego.xml,v 1.14 2014/06/04 10:10:18 revo Exp $">
<kap>
<rad>koregon</rad>/o
</kap>
<drv mrk="korego.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Man&gcirc;ebla salmoforma <ref tip="lst" cel="fisx.0o"
      lst="voko:fi&scirc;oj">fi&scirc;o</ref>, vivanta
      en profundaj lagoj sub 50&nbsp;m sed anka&ubreve; Norda kaj Balta Maroj; 
       
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="korego1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Coregonus</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Renke</trd>,
    <trd>Mar&auml;ne</trd>,
    <trd>Felchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>whitefish</trd>,
    <trd>cisco</trd>
  </trdgrp>
  <trd lng="fr">cor&eacute;gone</trd>
  <trd lng="hu">mar&eacute;na</trd>
  <trd lng="nl">houting</trd>
  <trd lng="pt">esp&eacute;cie de <ind>salm&atilde;o</ind></trd>
  <trd lng="ru">&c_s;&c_i;&c_g;</trd>
</drv>

</art>
