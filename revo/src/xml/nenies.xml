<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nenies.xml,v 1.15 2015/07/04 08:37:40 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>nenies</rad>
</kap>
<drv mrk="nenies.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
       Nevariebla pron. signifanta &leftquot;de neniu&rightquot;:
       <ekz>
         Esperanto estas <tld/> propra&jcirc;o;
       </ekz>
       <ekz>
         <tld/> opinion mi akceptas blinde<fnt>Z</fnt>;
       </ekz>
       <ekz>
         dependi de <tld/> kaprico.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_n;&c_ib;&c_ch;&c_y;&c_j;</trd>
  <trd lng="cs">ničí</trd>
  <trd lng="de">niemandes</trd>
  <trd lng="es">de <ind>nadie</ind></trd>
  <trd lng="fr">de <ind>personne</ind></trd>
  <trdgrp lng="hu">
     <trd>senki ...-je/-ja/-e/-a</trd>,
     <trd>senki&eacute;</trd>
   </trdgrp>
  <trd lng="nl">niemands</trd>
  <trd lng="pt">de <klr tip="amb">ningu&eacute;m</klr></trd>
  <trd lng="ru">&c_n;&c_i;&c_ch;&c_je;&c_j;</trd>
  <trd lng="sk">nikoho</trd>
  <trd lng="sv">ingens</trd>
</drv>
</art>
