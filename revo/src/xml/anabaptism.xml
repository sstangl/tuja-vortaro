<kap>
  <rad>anabaptism</rad>/o
</kap>
<drv mrk="anabaptism.0o">
  <kap><tld/>o</kap>
  <snc mrk="anabaptism.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Doktrino ordinare
      <ref tip="malprt" cel="protes1.0ismo.KRI">protestanta</ref>,
      kiu rigardas valida <ref tip="vid" cel="bapt.0o">bapton</ref> nur
      de plenkreskuloj:
      <ekz>
        en 1525 <klr>...</klr> <tld/>o unue aperis en Zuriko, 
        Svislando, kaj poste disvasti&gcirc;is &gcirc;is Nederlando
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Anabaptismo">
          Anabaptismo</url>, 2010-03-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">anabaptisme</trd>
  <trd lng="be">&c_a;&c_n;&c_a;&c_b;&c_a;&c_p;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_b;&c_a;&c_p;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="cs">anabaptizmus</trd>
  <trd lng="de">T&auml;ufer</trd>
  <trd lng="en">anabaptism</trd>
  <trd lng="fr">anabaptisme</trd>
  <trd lng="hr">anabaptizam</trd>
  <trd lng="nl">anabaptisme</trd>
  <trd lng="sk">anabaptizmus</trd>
</drv>
