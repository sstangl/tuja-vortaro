<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sublim2.xml,v 1.17 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sublimat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sublim2.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sublim2.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Substanco vaporigita kaj poste
      rekte ricevita en solida stato,
      per <ref tip="vid" cel="sublim1.0o">sublimo</ref>.
    </dif>
    <trd lng="be">&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_t;</trd>
    <trd lng="ca">sublimat</trd>
    <trd lng="de">Sublimat</trd>
    <trd lng="en">sublimate</trd>
    <trd lng="fa">&a_m;&a_A;&a_d;&a_h;&a_hamza_sure; <ind>&a_t;&a_S;&a_ayn;&f_y;&a_d;</ind> &a_sh;&a_d;&a_h;</trd>
    <trd lng="fr">sublimat</trd>
    <trdgrp lng="hu">
      <trd>szublim&aacute;tum</trd>,
      <trd>sz&aacute;llad&eacute;k</trd>
    </trdgrp>
    <trd lng="nl">sublimaat</trd>
    <trd lng="pl">sublimat</trd>
    <trd lng="ru">&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  </snc>
  <snc mrk="sublim2.0o.hgcl2">
    <dif>
      Hidrarga diklorido;
      <frm>HgCl<sub>2</sub></frm>:
      <ekz>
        preninte botelon da <tld/>o, paketon da banda&gcirc;o
        el sia selosako li komencis ekzameni la rompitan
        kruron
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_l;&c_je;&c_m;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>sublimat corrosiu</trd>,
      <trd>clorur de mercuri (II)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>mercuric chloride</trd>,
      <trd>corrosive sublimate</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&a_w;&a_b;&a_l;&a_m;&a_h;</trd>,
      <trd>&a_s;&a_w;&a_b;&a_l;&f_y;&a_m;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">chlorure de mercure</trd>
    <trd lng="hu">szublim&aacute;t</trd>
    <trdgrp lng="pl">
      <trd>sublimat</trd>,
      <trd>chlorek rt&eogonek;ci</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_u;&c_l;&c_je;&c_m;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>pevná sraženina par</trd>,
    <trd>sublimát</trd>
  </trdgrp>
  <trd lng="sk">sublimát</trd>
</drv>
</art>
