<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: celulo.xml,v 1.17 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>celuloid</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="celulo.0o">
  <kap><tld/>o</kap>
  <snc mrk="celulo.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Facile flami&gcirc;a kompona&jcirc;o el <ref tip="super"
      cel="kamfor.0o">kamforo</ref>
      kaj nitrocelulozo, el kiu oni fabrikas diversajn objektojn:
      <ekz>
        la sekvantan sabaton Jos&eacute; Arcadio Buend&iacute;a surmetis
        la 
        nigra-drapan veston, la <tld/>an kolumon kaj la &scirc;am-ledajn
        
        botojn
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_e;&c_l;&c_ju;&c_l;&c_jo;&c_ib;&c_d;</trd>
    <trd lng="ca">cel&middot;luloide</trd>
    <trd lng="de">Zelluloid</trd>
    <trd
    lng="el">&sigma;&epsilon;&lambda;&omikron;&ypsilon;&lambda;&omikron_ton;&jota;&ny;&tau;</trd>
    <trd lng="es">celuloide</trd>
    <trd lng="fr">cellulo&iuml;d</trd>
    <trd lng="hu">celluloid</trd>
    <trd lng="it">celluloide</trd>
    <trd lng="pl">celuloid</trd>
    <trd lng="pt">celul&oacute;ide</trd>
    <trd lng="ru">&c_c;&c_je;&c_l;&c_l;&c_u;&c_l;&c_o;&c_i;&c_d;</trd>
    <trd lng="sk">celuloid</trd>
  </snc>
  <trd lng="cs">celuloid</trd>
</drv>
</art>
