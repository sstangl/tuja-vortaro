<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hierau.xml,v 1.32 2015/06/26 09:10:16 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>hiera&ubreve;</rad>
</kap>
<drv mrk="hierau.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>adverbo</vspec></gra>
   <snc mrk="hierau.0.KOMUNE">
     <dif>
       En la &jcirc;us anta&ubreve;a tago, unu tagon
       anta&ubreve; la nuna:
       <ekz>
         <ind><tld/> nokte</ind>
         <trd lng="zh">昨夜 [<ind>zuó yè</ind>]</trd>
         .
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_u;&c_ch;&c_o;&c_r;&c_a;</trd>
   <trd lng="bg">&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="br">dec'h</trd>
   <trd lng="ca">ahir</trd>
   <trd lng="cs">v&ccaron;era</trd>
   <trd lng="de">gestern</trd>
   <trdgrp lng="el">
     <trd>&chi;&theta;&epsilon;&sigma_fina;</trd>,
     <trd>&chi;&tau;&epsilon;&sigma_fina;</trd>
   </trdgrp>
   <trd lng="en">yesterday</trd>
   <trd lng="es">ayer</trd>
   <trd lng="fr">hier</trd>
   <trd lng="he">&alef;&tav;&mem;&vav;&lamed;</trd>
   <trd lng="fi">eilen</trd>
   <trd lng="hu">tegnap</trd>
   <trd lng="id">kemarin</trd>
   <trd lng="it">ieri</trd>
   <trd lng="ja">&#26152;&#26085; [<ind>&#12365;&#12398;&#12358;</ind>]</trd>
   <trd lng="nl">gisteren</trd>
   <trd lng="pl">wczoraj</trd>
   <trd lng="pt">ontem</trd>
   <trd lng="ro">ieri</trd>
   <trd lng="ru">&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="sv">i <ind>g&aring;r</ind></trd>
   <trd lng="tr">d&uuml;n</trd>
   <trd lng="vo">yesdel</trd>
   <trdgrp lng="zh">
     <trd>昨日 [<ind>zuó rì</ind>]</trd>,
     <trd>昨天 [<ind>zuó tiān</ind>]</trd>
   </trdgrp>
</drv>

<drv mrk="hierau.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="hierau.0a.KOMUNE">
     <dif>
       Okazinta <tld/>:
       <ekz>
         lia <tld/>a parolo estis tre bela
         <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
       </ekz>
       <ekz>
         ne beda&ubreve;ru <tld/>an <klr>(tagon)</klr>, ne atendu
         morga&ubreve;an, ne forlasu hodia&ubreve;an
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fi">eilinen</trd>
   <trdgrp lng="fr">
     <trd>d'<ind>hier</ind></trd>,
     <trd>pass&eacute;<klr> (d'hier)</klr></trd>
   </trdgrp>
   <trd lng="it">di <ind>ieri</ind></trd>
   <trd lng="pl">wczorajszy</trd>
</drv>

<drv mrk="hierau.0o">
   <kap><tld/>o</kap>
   <snc mrk="hierau.0o.KOMUNE">
     <ref tip="dif" cel="tag.antaux0o">anta&ubreve;tago, anta&ubreve;a
     tago</ref>
   </snc>
   <trd lng="bg">&c_v;&c_ch;&c_je;&c_r;&c_a;&c_sh;&c_n;&c_i;&c_ja;&c_t; 
&c_d;&c_je;&c_n;</trd>
   <trd lng="br">derc'hent</trd>
   <trdgrp lng="fi">
     <trd>eilinen <klr tip="amb"> (subst.)</klr></trd>,
     <trd>eilisp&auml;iv&auml;</trd>,
     <trd>edellinen p&auml;iv&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>veille<klr tip="ind"> (jour pr&eacute;c&eacute;dent)</klr>
     </trd>,
     <trd>jour pr&eacute;c&eacute;dent</trd>
   </trdgrp>
   <trd lng="it">ieri<klr> (sost.)</klr></trd>
   <trdgrp lng="pl">
     <trd>wczoraj</trd>,
     <trd>dzie&nacute; <ind>wczorajszy</ind></trd>
   </trdgrp>
   <trd lng="pt">o dia de <ind>ontem</ind></trd>
</drv>

<drv mrk="hierau.antaux0">
   <kap>anta&ubreve;<tld/></kap>
   <snc mrk="hierau.antaux0.KOMUNE">
     <dif>
       Du tagojn anta&ubreve; la nuna.
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_z;&c_a;&c_w;&c_ch;&c_o;&c_r;&c_a;</trd>
   <trd lng="bg">&c_z;&c_a;&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="br">derc'hent-dec'h</trd>
   <trd lng="de">vorgestern</trd>
   <trdgrp lng="el">
     <trd>&pi;&rho;&omikron;&chi;&theta;&epsilon_ton;&sigma_fina;</trd>,
     <trd>&pi;&rho;&omikron;&chi;&tau;&epsilon_ton;&sigma_fina;</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>anteayer</trd>,
     <trd>antes de ayer</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>toissa p&auml;iv&auml;n&auml;</trd>
   </trdgrp>
   <trd lng="fr">avant-hier</trd>
   <trd lng="he">&shin;&lamed;&shin;&vav;&fmem;</trd>
   <trd lng="hu">tegnapel&odblac;tt</trd>
   <trdgrp lng="it">
     <trd>avantieri</trd>,
     <trd>l'altro <ind>ieri</ind></trd>
   </trdgrp>
   <trd lng="ja">&#19968;&#26152;&#26085;
[<ind>&#12362;&#12392;&#12392;&#12356;</ind>]</trd>
   <trd lng="nl">eergisteren</trd>
   <trd lng="pl">przedwczoraj</trd>
   <trd lng="pt">anteontem</trd>
   <trd lng="ro">alalt&abreve;ieri</trd>
   <trd lng="ru">&c_p;&c_o;&c_z;&c_a;&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="sv">i <ind>f&ouml;rrg&aring;r</ind></trd>
   <trd lng="tr">evvelki g&uuml;n</trd>
</drv>
</art>
