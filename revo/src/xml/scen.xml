<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: scen.xml,v 1.25 2015/07/29 22:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>scen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="scen.0o">
  <kap><tld/>o<fnt><bib>F</bib>, <lok>Ekzercaro, &para; 4</lok></fnt></kap>
  <snc mrk="scen.0o.KOMUNE">
    <dif>
      Emociiga a&ubreve; interesa vidata okazo:
      <ekz>
        instrua, neforgesebla, na&ubreve;za <tld/>o;
      </ekz>
      <ekz>
        la sola efektive erotika <tld/>o de la libro
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk>Hamburgero ne tre erotika</vrk>,
          <bib>Monato</bib>,
          <lok>1996:7, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="scen.0o.TEA">
      <uzo tip="fak">TEA</uzo>
      <dif>
      Senrompa agado de samaj rolantoj,
      subparto de teatra <ref tip="malprt" cel="akt.0o.TEA">akto</ref>:
      <ekz>
        muta <klr>(senparola)</klr> <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <klr>[...]</klr> re&gcirc;isoro sukcese kreas suspenson ekde
        la unua <tld/>o
        <fnt>
          <aut>M. Ecott</aut>:
          <vrk>Inkubo en kubo</vrk>,
          <bib>Monato</bib>,
          <lok>2000:3, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    </subsnc>
    <subsnc mrk="scen.0o.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Figurado de homoj en tipa situacio:
        <ekz>
          vi tie trovas vere altvaloran vazaron kun la &scirc;atataj
          &ccirc;aso<tld/>oj, kiajn oni vidas sur malnovaj gobelenoj
          a&ubreve; sur anglaj gravura&jcirc;oj
          <fnt>
            <aut>-</aut>,
            <vrk><url
            ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5802/arto.html">
            Arto sur la tablo</url></vrk>,
            <lok>Norda Prismo, 1958:2</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">Szene</trd>
    <trd lng="pl">scena</trd>
  </snc>
  <snc mrk="scen.0o.scenejo">
    <ref tip="dif" cel="scen.0ejo"><tld/>ejo</ref>
    <ekz>
      oni a&ubreve;das pafojn post la <tld/>o<fnt>Z</fnt>;
    </ekz>
    <ekz>
      &gcirc;i neniam estis donata sur la <tld/>o<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      mia persono tute foriras de la <tld/>o<fnt>Z</fnt>
      <klr>(rezignas je publika vidateco)</klr>.
    </ekz>
    <trd lng="nl">toneel</trd>
    <trd lng="pl">scena</trd>
  </snc>
  <trd lng="be">&c_s;&c_c;&c_e;&c_n;&c_a;</trd>
  <trd lng="bg">&c_s;&c_c;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>sc&eacute;na</trd>,
    <trd>v&yacute;jev</trd>,
    <trd>v&yacute;stup</trd>
  </trdgrp>
  <trd lng="en">scene</trd>
  <trd lng="fr">sc&egrave;ne</trd>
  <trd lng="he">&samekh;&tsadi;&yod;&nun;&he;</trd>
  <trdgrp lng="hu">
    <trd>jelenet</trd>,
    <trd>sz&iacute;n</trd>
  </trdgrp>
  <trd lng="it">scena</trd>
  <trd lng="nl">sc&egrave;ne</trd>
  <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>sc&eacute;na</trd>,
    <trd>v&yacute;jav</trd>,
    <trd>v&yacute;stup</trd>
  </trdgrp>
</drv>

<drv mrk="scen.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="scen.0aro.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Skizo de teatra&jcirc;o a&ubreve; kina&jcirc;o, prezentanta la
      rolulojn, lokojn, situaciojn, &ccirc;efajn scenojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        advokato <klr>[...]</klr> deklaris, ke la saman <tld/>aron uzis
        la polico anta&ubreve; du jaroj
        <fnt>
          <aut>G. Van Damme</aut>:
          <vrk>Kiu faris kion?</vrk>,
          <bib>Monato</bib>,
          <lok>1996:11, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sc&eacute;n&aacute;&rcaron;</trd>
  <trdgrp lng="en">
    <trd>scenario</trd>,
    <trd>screenplay</trd>
  </trdgrp>
  <trd lng="fr">sc&eacute;nario</trd>
  <trdgrp lng="he">
    <trd>&tav;&samekh;&resh;&yod;&tet;</trd>,
    <trd>&tav;&resh;&het;&yod;&shin;</trd>
  </trdgrp>
  <trd lng="hu">forgat&oacute;k&ouml;nyv</trd>
  <trdgrp lng="it">
    <trd>scenario<klr> (sequenza di scene)</klr></trd>,
    <trd>sceneggiatura</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>draaiboek</trd>,
    <trd>scenario</trd>
  </trdgrp>
  <trd lng="pl">scenariusz</trd>
  <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">scen&aacute;r</trd>
</drv>

<drv mrk="scen.0aristo">
  <kap><tld/>aristo</kap>
  <snc mrk="scen.0aristo.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Verkisto de <tld/>aroj:
      <ekz>
        &ndash; La generaloj &ndash; rekte enhaki&gcirc;is en la
        disputon la <tld/>aristo Glu&hcirc;arjov
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 49a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sc&eacute;n&aacute;rista</trd>
  <trdgrp lng="en">
    <trd>scenarist</trd>,
    <trd>scriptwriter</trd>
  </trdgrp>
  <trd lng="fr">sc&eacute;nariste</trd>
  <trd lng="he">&tav;&samekh;&resh;&yod;&tet;&alef;&yod;</trd>
  <trd lng="hu">forgat&oacute;k&ouml;nyv&iacute;r&oacute;</trd>
  <trdgrp lng="it">
    <trd>scenarista</trd>, 
    <trd>sceneggiatore</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>scenarioschrijver</trd>,
    <trd>scenarist</trd>
  </trdgrp>
  <trd lng="pl">scenarzysta</trd>
  <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_a;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">scen&aacute;rista</trd>
</drv>

<drv mrk="scen.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="scen.0ejo.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Loko, kie rolistoj ludas;
      <ref tip="sin" cel="scen.0o.scenejo">sceno<sncref/></ref>:
      <ekz>
        Semplejarov propraokule vidis la magiiston sur la <tld/>ejo
        <fnt>
          <bib>MkM</bib>,
          <lok>&ccirc;apitro 27a, p. 288-289a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu okazo &scirc;ovis la demandon pri Esperanto en la
        anta&ubreve;an <tld/>ejon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_c;&c_e;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_c;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;ji&scaron;t&ecaron;</trd>,
    <trd>jevi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">B&uuml;hne</trd>
  <trdgrp lng="en">
    <trd>scene</trd>,
    <trd>stage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plateau<klr> (cin&eacute;ma, t&eacute;l&eacute;v.)</klr></trd>,
    <trd>sc&egrave;ne<klr> (th&eacute;&acirc;tre)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&zayin;&yod;&resh;&he;</trd>,
    <trd>&mem;&qof;&vav;&fmem; <ind>&he;&tav;&resh;&het;&shin;&vav;&tav;</ind></trd>,
    <trd>&bet;&mem;&he;</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;npad</trd>
  <trdgrp lng="it">
    <trd>scena<klr> (palcoscenico)</klr></trd>,
    <trd>palcoscenico</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sc&egrave;ne</trd>,
    <trd>toneel</trd>,
    <trd>b&uuml;hne</trd>
  </trdgrp>
  <trd lng="pl">scena</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_c;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">javisko</trd>
</drv>

<drv mrk="scen.en0igi">
  <kap>en<tld/>igi,
  <var><kap>sur<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="scen.en0igi.TEA">
    <dif>
      &Ccirc;ion aran&gcirc;i por ludado de teatra&jcirc;o a&ubreve;
      kina&jcirc;o &gcirc;is efektivigo:
      <ekz>
        a&ubreve;straliaj helha&ubreve;tuloj en<tld/>igis Freeman,
        aborigen-devenan,
        kvaza&ubreve; ili kritikus <klr>[...]</klr> la pasintecon de sia
        lando
        <fnt>
          <aut>Igarasi T.</aut>,
          <vrk>Tor&ccirc;o de hipokriteco</vrk>,
          <bib>Monato</bib>,
          <lok>2000:12, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la baleto <ctl>La konsekro de la printempo</ctl>
        <klr>[...]</klr> sur<tld/>igita en Parizo en 1913
        <fnt>
          <aut>N. Arolovi&ccirc;</aut>;
          <vrk>La homo de la kosma konscio</vrk>,
          <bib>Monato</bib>,
          <lok>2002:7, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>inscenovat</trd>,
    <trd>re&zcaron;&iacute;rovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>auff&uuml;hren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stage</trd>,
    <trd>direct</trd>,
    <trd>present</trd>
  </trdgrp>
  <trd lng="fr">mettre en sc&egrave;ne</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&ayin;&lamed;&vav;&tav; &ayin;&lamed; &he;<ind>&bet;&mem;&he;</ind></trd>,
    <trd>&lamed;<ind>&bet;&yod;&yod;&fmem;</ind></trd>,
    <trd>&lamed;<ind>&he;&tsadi;&yod;&gimel;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nre visz</trd>,
    <trd>sz&iacute;npadra &aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>allestire<klr> (tea.)</klr></trd>,
    <trd>inscenare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>regisseren</trd>,
    <trd>ensceneren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wystawia&cacute;<klr> na scenie</klr></trd>,
    <trd>inscenizowa&cacute;</trd>,
    <trd>re&zdot;yserowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_c;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_zh;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_p;&c_mol;&c_je;&c_s;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>inscenova&tcaron;</trd>,
    <trd>uvies&tcaron; na sc&eacute;nu</trd>
  </trdgrp>
</drv>

<drv mrk="scen.en0igisto">
  <kap>en<tld/>igisto,
  <var><kap>sur<tld/>igisto</kap></var></kap>
  <snc mrk="scen.en0igisto.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Kunordigisto de drama prezento, ekzercanta kaj
      gvidanta rolistojn kaj ceterajn agantojn:
      <ekz>
        teatra&jcirc;o <klr>[...]</klr> devas elmeti&gcirc;i al
        kompetentuloj &ndash; aktoroj, re&gcirc;isoroj, sur<tld/>igistoj
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Materialismo kaj &jcirc;aluzo</vrk>,
          <bib>Monato</bib>,
          <lok>1998:4, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">re&zcaron;is&eacute;r</trd>
  <trd lng="de">Regisseur</trd>
  <trd lng="en">director</trd>
  <trd lng="fr">metteur en sc&egrave;ne</trd>
  <trd lng="he">&bet;&mem;&alef;&yod;</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;nh&aacute;zi <ind>rendez&odblac;</ind></trd>,
    <trd>sz&iacute;npadra &aacute;ll&iacute;t&oacute;</trd>
  </trdgrp>
  <trd lng="nl">regisseur</trd>
  <trd lng="pl">re&zdot;yser</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_zh;&c_i;&c_s;&c_s;&c_jo;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">re&zcaron;is&eacute;r</trd>
</drv>

<drv mrk="scen.en0igo">
  <kap>en<tld/>igo,
  <var><kap>sur<tld/>igo</kap></var></kap>
  <snc mrk="scen.en0igo.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Aran&gcirc;o kaj efektiva ludado de teatra&jcirc;o a&ubreve; kina&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="scenografi.0o.TEA">scenografio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Auff&uuml;hrung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>staging</trd>,
    <trd>presentation</trd>
  </trdgrp>
  <trd lng="fr">mise en sc&egrave;ne</trd>
  <trdgrp lng="he">
    <trd>&he;&tsadi;&gimel;&he;</trd>,
    <trd>&bet;&yod;&mem;&vav;&yod;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd><klr tip="amb">(sz&iacute;nh&aacute;zi)</klr> rendez&eacute;s</trd>,
    <trd>sz&iacute;nrevitel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>allestimento<klr> (tea.)</klr></trd>,
    <trd>messinscena</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mise-en-sc&egrave;ne</trd>,
    <trd>toneelschikking</trd>,
    <trd>enscenering</trd>,
    <trd>spelleiding</trd>,
    <trd>regie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wystawienie<klr> na scenie</klr></trd>,
    <trd>odegranie<klr> na scenie</klr></trd>,
    <trd>inscenizacja</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>
</art>
