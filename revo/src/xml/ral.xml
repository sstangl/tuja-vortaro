<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ral.xml,v 1.17 2015/09/21 09:10:30 revo Exp $">
<kap>
  <rad>ral</rad>/o
</kap>

<drv mrk="ral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="raled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Rallus</trd>).
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ch&rcaron;&aacute;stal vodni</trd>,
    <trd>ch&rcaron;&aacute;stal</trd>
  </trdgrp>
  <trd lng="de">Wasserralle</trd>
  <trd lng="en">water-rail</trd>
  <trd lng="fr">r&acirc;le</trd>
  <trd lng="hu">guvat</trd>
  <trd lng="nl">ral</trd>
  <trd lng="pl">wodnik</trd>
  <trd lng="pt">ralo</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_o;&c_k;
    <klr tip="amb">(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">chria&scaron;te&lcaron;</trd>
</drv>

<drv mrk="ral.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <ref tip="dif" cel="ral.0o">Ralo</ref>
    <trd lng="fr">r&acirc;le d'eau</trd>
    <trd lng="de">Wasserralle</trd>
    <trd lng="en">water <ind>rail</ind></trd>
    <trd lng="hu">guvat</trd>
    <trd lng="la">Rallus aquaticus</trd>
    <trd lng="nl">waterral</trd>
    <trd lng="pl">wodnik</trd>
    <trd lng="pt">frango d'&acirc;gua</trd>
    <trd lng="sv">vattenrall</trd>
  </snc>
  <trd lng="cs">ch&rcaron;&aacute;stal vodn&iacute;</trd>
  <trd lng="sk">chria&scaron;te&lcaron; vodn&yacute;</trd>
</drv>

<drv mrk="ral.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Ralliformes</trd>).
    </dif>
  </snc>
  <trd lng="en">Ralliformes</trd>
  <trd lng="de">Rallenv&ouml;gel</trd>
  <trd lng="fr">gruiformes</trd>
  <trd lng="hu">guvatalak&uacute;ak</trd>
  <trd lng="pl">chru&sacute;ciele</trd>
</drv>
</art>
