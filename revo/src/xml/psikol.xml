<kap>
  <ofc>2</ofc>
  <rad>psikolog</rad>/o
</kap>
<drv mrk="psikol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <ref tip="dif" cel="psikol1.0isto">psikologiisto</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      la a&ubreve;toro bone sukcesas fari tion, &ccirc;ar li posedas la talenton de
      subtila <tld/>o, kiu atente esploras siajn pensojn kaj
      sentojn<fnt><aut>Julian Modest</aut>: <vrk>Sincere kaj honeste pri
      si mem (Gerrit Berveling: La unuaj 25 jaroj de mia memoro)</vrk>,
      <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 22a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">psycholog</trd>
  <trd lng="de">Psychologe</trd>
  <trd lng="en">psychologist</trd>
  <trdgrp lng="fr">
    <trd>psychologue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pszichol&oacute;gus</trd>,
    <trd>l&eacute;lekb&uacute;v&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">psycholoog</trd>
  <trd lng="no">psykolog</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">psychol√≥g</trd>
</drv>
</art>
