<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vasal.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vasal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vasal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vasal.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <ref tip="dif" cel="feuxd.0ulo.HIS">fe&ubreve;dulo</ref>
      <ekz>
        nun mi lo&gcirc;as en arbaro, /
        &scirc;telas, rabas pro neceso, /
        kaj dividas &ccirc;ion inter /
        mi kaj miaj dek <tld/>oj
        <fnt>
          <aut>trad. P.-J. Doumen</aut>,
          <vrk>Karlo kaj Elegasto</vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_s;&c_a;&c_l;</trd>
  <trd lng="br">gwaz<klr tip="amb"> (dindan un aotrou)</klr></trd>
  <trd lng="ca">vassall</trd>
  <trdgrp lng="cs">
    <trd>leník</trd>,
    <trd>man</trd>,
    <trd>vazal</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vasall</trd>,
    <trd>Lehnsmann</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron;&tau;&epsilon;&lambda;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">vassal</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_A;&a_s;&a_A;&a_l;</trd>,
    <trd>&a_kh;&a_r;&a_A;&a_j;&f_g;&a_z;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">vassal</trd>
  <trdgrp lng="hu">
    <trd>h&udblac;b&eacute;res</trd>,
    <trd>vazallus</trd>
  </trdgrp>
  <trd lng="nl">vazal</trd>
  <trd lng="pt">vassalo</trd>
  <trd lng="ru">&c_v;&c_a;&c_s;&c_s;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>vazal</trd>,
    <trd>závislá osoba</trd>
  </trdgrp>
</drv>
</art>
