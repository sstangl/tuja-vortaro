<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apoge.xml,v 1.23 2015/07/04 08:37:30 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>apoge</rad>/o
</kap>
<drv mrk="apoge.0o">
   <kap><tld/>o</kap>
   <snc mrk="apoge.0o.AST">
     <uzo tip="fak">AST</uzo>
     <dif>
       Tiu punkto de la orbito de
       ter-&ccirc;irkka&ubreve;anta objekto, &ccirc;e kiu 
       tiu estas plej malproksima de la tero.
     </dif>
     <ref tip="ant" cel="perige.0o">perigeo</ref>
     <ref tip="vid" cel="zenit.0o">zenito</ref>
     <trdgrp lng="hu">
       <trd>f&ouml;ldt&aacute;vol</trd>,
       <trd>apogeum</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>apogeum</trd>,
       <trd>kulminacja</trd>
     </trdgrp>
   </snc>
   <snc mrk="apoge.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <ref tip="dif" cel="kulmin.0o.FIG">kulmino<sncref/></ref>
     <ref tip="sin" cel="zenit.0o.FIG">apogeo<sncref/></ref>
       <ekz>
         esti &ccirc;e la <tld/>o
         de famo, de sukceso.
       </ekz>
     <trdgrp lng="de">
       <trd>erdfernster Punkt</trd>,
       <trd>Erdferne</trd>,
       <trd>Apog&auml;um</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>apogee</trd>,
       <trd>zenith</trd>,
       <trd>pinnacle</trd>,
       <trd>acme</trd>,
       <trd>apex</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>H&ouml;hepunkt</trd>,
       <trd>Gipfel</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>cs&uacute;cs</trd>,
       <trd>cs&uacute;cspont</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>apogeum</trd>,
       <trd>kulminacja</trd>,
       <trd>punkt <ind>kulminacji</ind></trd>,
       <trd>punkt <ind>kulminacyjny</ind></trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_a;&c_p;&c_o;&c_g;&c_je;&c_j;</trd>,
       <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_t;</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_g;&c_je;&c_j;</trd>
  <trd lng="ca">apogeu</trd>
  <trdgrp lng="cs">
    <trd>vrchol <klr>(křivky)</klr></trd>,
    <trd>vrcholení</trd>,
    <trd>vyvrcholení</trd>
  </trdgrp>
  <trd lng="es">apog&eacute;o</trd>
  <trd lng="fr">apog&eacute;e</trd>
  <trd lng="pt">apogeu</trd>
  <trdgrp lng="sk">
    <trd>apogeum</trd>,
    <trd>odzemie</trd>
  </trdgrp>
</drv>
</art>
