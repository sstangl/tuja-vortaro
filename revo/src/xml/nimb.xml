<vortaro>
<art mrk="$Id: nimb.xml,v 1.17 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>nimb</rad>/o
</kap>
<drv mrk="nimb.0o">
  <kap><tld/>o</kap>
  <snc>
      <ref tip="dif" cel="auxreo.0o">A&ubreve;reolo</ref>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        doni al la afero <tld/>on <klr>(presti&gcirc;on)</klr> de oficialeco.
      </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_m;&c_b;</trd>,
    <trd>&c_a;&c_r;&c_e;&c_o;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>nimbe</trd>,
    <trd>aur&egrave;ola</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aureola</trd>,
    <trd>svatozář</trd>
  </trdgrp>
  <trd lng="de">Nimbus</trd>
  <trdgrp lng="en">
    <trd>halo</trd>,
    <trd>nimbus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nimbe</trd>,
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re, prestige)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nimbusz</trd>,
    <trd>dicsf&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nimbus</trd>,
    <trd>aureool</trd>,
    <trd>stralenkrans</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aur&eacute;ola</trd>,
    <trd>aura</trd>,
    <trd>nimbo</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_m;&c_b;</trd>,
    <trd>&c_o;&c_r;&c_je;&c_o;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aureola</trd>,
    <trd>nimbus</trd>,
    <trd>svätožiara</trd>
  </trdgrp>
</drv>
</art>
