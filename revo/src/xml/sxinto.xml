<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxinto.xml,v 1.15 2013/09/22 19:11:13 revo Exp $">
<kap>
  <rad>&scirc;intoism</rad>/o
</kap>
<drv mrk="sxinto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Indi&gcirc;ene japana <ref tip="lst" cel="religi.0o.REL"
      lst="voko:religioj">religio</ref>
      plurdia, kultanta natura&jcirc;ojn, spiritojn, heroojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="sxinto1.0o">&scirc;intoo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_t;&c_a;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Schintoismus</trd>
  <trd lng="en">Shintoism</trd>
  <trdgrp lng="fr">
    <trd>shint&ocirc;</trd>,
    <trd>shinto&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">sintoizmus</trd>
  <trdgrp lng="it">
    <trd>scintoismo</trd>,
    <trd>shintoismo</trd>, 
    <trd>shinto</trd>
  </trdgrp>
  <trd lng="nl">sjinto&iuml;sme</trd>
  <trdgrp lng="pl">
    <trd>szintoism</trd>,
    <trd>sintoizm</trd>,
    <trd>shintoizm</trd>
  </trdgrp>
  <trd lng="pt">xintoismo</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_t;&c_o;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
