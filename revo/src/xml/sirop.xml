<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sirop.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sirop</rad>/o
</kap>
<drv mrk="sirop.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Densa kuirita solva&jcirc;o de sukero ofte aromigita per fruktosuko.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_r;&c_o;&c_p;</trd>
  <trd lng="ca">xarop</trd>
  <trdgrp lng="cs">
    <trd>sirop</trd>,
    <trd>sirup</trd>
  </trdgrp>
  <trd lng="de">Sirup</trd>
  <trd lng="en">syrup</trd>
  <trdgrp lng="es">
    <trd>sirope</trd>,
    <trd>jarabe</trd>
  </trdgrp>
  <trd lng="hu">szirup</trd>
  <trdgrp lng="nl">
    <trd>stroop</trd>,
    <trd>siroop</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_p;</trd>
  <trd lng="sk">sirup</trd>
</drv>
</art>
