<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adekva.xml,v 1.24 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>adekvat</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="adekva.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIL</uzo>
  <snc mrk="adekva.0a.FIL">
    <dif>
      Klare kaj tute ampleksanta &ccirc;iujn karakterojn
      de la objekto.
    </dif>
    <adm>
      La redakto de la difino devenas de PIV2, sed mi opinias gxin
      malklara. Mi scivolas, cxu entute ekzistas tia filozofia senco.
      <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="adekva.0a.KOMUNE">
    <refgrp tip="dif">
      <ref cel="konfor.cel0a">celkonforma</ref>,
      <ref cel="konfor.0a.celkonforma">konforma <sncref/></ref>
    </refgrp>
    <ekz>
      parto de la nuntempe laboranta lektoraro ne havas <tld/>ajn
      kvalifikojn
      <fnt><aut>Stanis&lstroke;aw &Sacute;migielski</aut>: <vrk>Instruisto
      de la angla ser&ccirc;ata</vrk>, <bib>Monato</bib><lok>jaro 2001a,
      numero 2a, p. 19a</lok></fnt>;
    </ekz>
    <ekz>
      tia antologio devas <klr>[...]</klr> enteni kiom eble plej
      adekvatajn
      tradukojn de elektitaj pecoj
      <fnt><aut>Nikolao Gudskov</aut>:
      <vrk>Provo ektu&scirc;i serban poezion (Srdj Arandjelovi&cacute;, Antologio
      de
      la serba poezio (1200-2000))</vrk>, <bib>Monato</bib><lok>jaro
      2002a, numero 2a, p. 32a</lok></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="konven.0a">konvena</ref>,
      <ref cel="tauxg.0a">ta&ubreve;ga</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_e;&c_k;&c_v;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_d;&c_je;&c_k;&c_v;&c_a;&c_t;&c_je;&c_n;</trd>
  <trd lng="ca">adequat</trd>
  <trdgrp lng="cs">
    <trd>adekvátní</trd>,
    <trd>odpovídající</trd>,
    <trd>patřičný</trd>,
    <trd>přiměřený</trd>,
    <trd>vhodný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ad&auml;quat</trd>,
    <trd>entsprechend</trd>,
    <trd>angemessen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>adequate</trd>,
    <trd>appropriate</trd>,
    <trd>sufficient</trd>
  </trdgrp>
  <trd lng="es">adecuado</trd>
  <trdgrp lng="fr">
    <trd>ad&eacute;quat</trd>,
    <trd>adapt&eacute;</trd>,
    <trd>appropri&eacute;</trd>,
    <trd>juste<klr tip="ind"> (appropri&eacute;)</klr></trd>,
    <trd>propre<klr tip="ind"> (appropri&eacute;)</klr></trd>,
    <trd>opportun<klr tip="ind"> (appropri&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="it">adeguato</trd>
  <trdgrp lng="nl">
    <trd>geschikt</trd>,
    <trd>adequaat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>adekwatny</trd>,
    <trd>odpowiedni</trd>
 </trdgrp>
  <trdgrp lng="pt">
    <trd>adequado</trd>,
    <trd>apropriado</trd>
  </trdgrp>
  <trd lng="ro">adecvat</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_je;&c_k;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_t;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>primeran&yacute;</trd>,
    <trd>zodpovedajúci</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>adekvat</trd>,
    <trd>tillfredst&auml;llande</trd>,
    <trd>passande</trd>,
    <trd>l&auml;mplig</trd>
  </trdgrp>
</drv>
</art>
