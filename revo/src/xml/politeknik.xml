<vortaro>
<art mrk="$Id: politeknik.xml,v 1.9 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>politeknik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="politeknik.0o">
  <kap><tld/>o</kap>
  <snc mrk="politeknik.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Supera <ref tip="super" cel="lern.0ejo.loko">lernejo</ref>, en kiu
      oni instruas precipe te&hcirc;nikajn fakojn:
      <ekz>
        &ndash; kiun superan lernejon vi estas fininta?
        &ndash; La petrogradan <tld lit="P"/>an Instituton
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_ib;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;</trd>
  <trd lng="cs">polytechnika</trd>
  <trd lng="fa">&f_p;&a_l;&f_y;&zwnj;&a_t;&f_k;&a_n;&f_y;&f_k;</trd>
  <trd lng="fr">polytechnique<klr> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>polytechnicum</trd>,
    <trd>polytechnische school</trd>
  </trdgrp>
  <trd lng="pl">politechnika</trd>
  <trd lng="pt">polit&eacute;cnica</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_t;&c_je;&c_h;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_i;&c_n;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;</trd>
  <trd lng="sk">polytechnika</trd>
</drv>
</art>

