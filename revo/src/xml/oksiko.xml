<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oksiko.xml,v 1.18 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>oksikok</rad>/o
</kap>
<drv mrk="oksiko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="oksiko.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto kreskanta en mar&ccirc;oj de malvarmaj landoj,
      kies ru&gcirc;aj beroj estas uzataj por sukoj kaj konfita&jcirc;oj,
      el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (specioj <trd lng="la">Vaccinium macrocarpon</trd> kaj <trd
      lng="la">Vaccinium oxycoccos</trd> iafoje rigardataj kiel
      subgenro <trd lng="la">Oxycoccus</trd>):
      <ekz>
        bongusta <tld/>a likvoro
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 19a, p. 111a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_y;</trd>
  <trd lng="cs">klikva bahenní</trd>
  <trd lng="de">Moosbeere</trd>
  <trd lng="en">cranberry</trd>
  <trdgrp lng="fr">
    <trd>caneberge</trd>,
    <trd>canneberge</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zeg&aacute;fonya</trd>
  <trd lng="nl">veenbes</trd>
  <trd lng="pl">&zdot;urawina</trd>
  <trd lng="pt">oxicoco</trd>
  <trd lng="ru">&c_k;&c_l;&c_ju;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">kľukva močiarna</trd>
</drv>
</art>
