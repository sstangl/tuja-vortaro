<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: asign.xml,v 1.20 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>asign</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="asign.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Destini, difini, fiksi:
      <ekz>
        <tld/>i al iu lokon, rolon, rendevuon.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>zuweisen</trd>,
       <trd>zuteilen</trd>
    </trdgrp>
      <trdgrp lng="hu">
         <trd>kijel&ouml;l<klr tip="ind"> (pl. feladatot)</klr></trd>,
         <trd>kioszt<klr tip="ind"> (pl. szerepet)</klr></trd>
      </trdgrp>
     <trdgrp lng="it">
       <trd>assegnare</trd>,
       <trd>attribuire</trd>,
       <trd>fissare</trd>
    </trdgrp>
    <trdgrp lng="pl">
    <trd>asygnowa&cacute;</trd>,
    <trd>przeznacza&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">ur&ccaron;i&tcaron;</trd>
  </snc>
  <snc mrk="asign.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Destini kaj disponigi sumon por difinita pago:
      <ekz>
        la registaro <tld/>is
        grandan sumon por la batalo kontra&ubreve; la tuberkulozo;
      </ekz>
      <ekz>
        <tld/>i al iu salajron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="don.trans0i">transdoni</ref>,
      <ref cel="gxir.0i">&gcirc;iri</ref>.
    </refgrp>
    <trd lng="hu">kiutal</trd>
    <trdgrp lng="it">
       <trd>assegnare</trd>,
       <trd>stanziare</trd>,
       <trd>allocare</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>zuweisen</trd>,
       <trd>zuteilen</trd>
    </trdgrp>
    <trdgrp lng="pl">
    <trd>przeznacza&cacute;</trd>,
    <trd>przekazywa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_s;&c_s;&c_i;&c_g;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="sk">pouk&aacute;za&tcaron;</trd>
  </snc>
  <snc mrk="asign.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiale alvoki anta&ubreve; tribunalon:
      <ekz>
        la <tld/>ito ne aperis anta&ubreve; la kortumo.
      </ekz>
    </dif>
    <trdgrp lng="it">
       <trd>compulsare</trd>,
       <trd>convenire<klr> (dir.)</klr></trd>,
       <trd>citare<klr> (dir.)</klr></trd>
    </trdgrp>
    <trd lng="pl">wzywa&cacute;</trd>
    <trd lng="ru">&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>asignovat</trd>,
    <trd>obeslat</trd>,
    <trd>soudně předvolat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>vor Gericht <ind>laden</ind></trd>,
      <trd>vor Gericht <ind>zitieren</ind></trd>
  </trdgrp>
  <trd lng="en">assign</trd>
  <trd lng="es">asignar</trd>
  <trd lng="fr">assigner</trd>
  <trd lng="hu">beid&eacute;z</trd>
  <trdgrp lng="sk">
    <trd>poukázať, prideliť</trd>,
    <trd>prideliť</trd>
  </trdgrp>
</drv>

<drv mrk="asign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i; dokumento signifanta tiun agon.
    </dif>
    <refgrp tip="vid">
      <ref cel="mandat.0o">mandato</ref>,
      <ref cel="pag">pagordono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>asignace</trd>,
    <trd>asignát</trd>,
    <trd>peněžní poukázka</trd>
  </trdgrp>
  <trd lng="en">assignment</trd>
  <trd lng="es">asignaci&oacute;n</trd>
  <trd lng="fr">assignation</trd>
  <trdgrp lng="hu">
      <trd>id&eacute;z&eacute;s</trd>,
      <trd>kiutal&aacute;s</trd>,
      <trd>kioszt&aacute;s<klr tip="amb"> (pl. szerep&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>asygnacja</trd>,
    <trd>przekaz</trd>,
    <trd>wezwanie</trd>
  </trdgrp>
  <trd lng="sk">poukaz <klr>(doklad)</klr></trd>
</drv>
</art>
