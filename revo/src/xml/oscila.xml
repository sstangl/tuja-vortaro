<kap><rad>oscilator</rad>/o</kap>
<drv mrk="oscila.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato por produkti elektromagnetajn oscilojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_c;&c_y;&c_l;&c_ja;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_o;&c_s;&c_c;&c_i;&c_l;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="ca">oscil&middot;lador</trd>
  <trd lng="cs">oscilátor</trd>
  <trdgrp lng="de">
    <trd>Oszillator</trd>,
    <trd>Schwingungserzeuger</trd>
  </trdgrp>
  <trd lng="en">oscillator</trd>
  <trd lng="fr">oscillateur</trd>
  <trdgrp lng="hu">
    <trd>oszcill&aacute;tor</trd>,
    <trd>rezg&eacute;skelt&odblac;</trd>
  </trdgrp>
  <trd lng="nl">oscillator</trd>
  <trd lng="pl">oscylator</trd>
  <trd lng="pt">oscilador <klr>(&ocirc;)(subst.)</klr></trd>
  <trd lng="ru">&c_o;&c_s;&c_c;&c_i;&c_l;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">oscilátor</trd>
</drv>
</art>
