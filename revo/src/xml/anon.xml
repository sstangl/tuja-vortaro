<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anon.xml,v 1.8 2013/10/29 08:10:14 revo Exp $">
<kap>
  <rad>anon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anon.0o">
  <kap><tld/>o</kap>
  <snc mrk="anon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de kelkaj specioj de <tld/>arbo, mezgranda, kun verda &scirc;elo kaj
      blanka
       karno.
    </dif>
    <ref tip="sub" cel="pom.suker0o">sukerpomo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Cherimoya</trd>,
    <trd>S&uuml;&szlig;sack</trd>
    <trd>Sauersack</trd>
  </trdgrp>
  <trd lng="en">cherimoya</trd>
  <trd lng="fr">anone</trd>
  <trdgrp lng="es">
    <trd>anona</trd>,
    <trd>chirimoya</trd>
  </trdgrp>
  <trd lng="hu">ann&oacute;na</trd>
  <trd lng="nl">cherimoya<klr tip="ind"> (vrucht)</klr></trd>
</drv>

<drv mrk="anon.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="anon.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
       Monta <ref tip="lst" cel="arb.0o"
       lst="voko:magnolioparencoj">arbo</ref>
       kreskanta en tropikaj kaj subtropikaj regionoj, 
       kies kelkaj specioj estas kultivataj pro la fruktoj, ekz.
       <trd lng="la">Annona cherimola</trd>, <trd lng="la">Annona
       squamosa</trd> kaj
       <trd lng="la">Annona muricata</trd>;
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">genro</ref>
        el familio <ref tip="malprt" cel="anonac.0oj">anonacoj</ref>
      (<trd lng="la">Annona</trd>):
      <ekz>
        <ctl>mi fartas bone, Ariero</ctl>, diras Tomegino, kiu sidas
        sur la <tld/>arbo
        <fnt>
        <aut>A. Lef&oacute;nst Sardi&ntilde;as, trad. J. de Jes&uacute;s Campos
        Pacheco</aut>,
        <vrk><url
        ref="http://membres.lycos.fr/vilhelmo/Kuba_infanliteraturo.html">
        Kuba infanliteraturo</url>, En la montaro</vrk>,
        <lok>1999-2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Cherimoyabaum</trd>,
    <trd lng="la">Zimtapfelbaum</trd>,
    <trd lng="la">Stachelannone</trd>
  </trdgrp>
  <trd lng="fr">anonier</trd>
  <trd lng="es">chirimoyo</trd>
  <trdgrp lng="hu">
    <trd>cserimoj&oacute;fa</trd>,
    <trd>kr&eacute;malmafa</trd>
  </trdgrp>
  <trd lng="nl">cherimoya<klr tip="ind"> (boom)</klr></trd>
</drv>
</art>

