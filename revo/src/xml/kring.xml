<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kring.xml,v 1.16 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kring</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kring.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda ringoforma seka sukerita kuko:
      <ekz>
        krustetaj <tld/>oj, <tld/>etoj en formoj de &ccirc;iuj dombestoj, 
        katoj, hundoj, bovoj, &ccirc;evaloj, azenoj, &scirc;afoj kaj e&ccirc; kunikloj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_a;&c_k;</trd>
  <trd lng="cs">slaný preclík</trd>
  <trd lng="de">Kringel</trd>
  <trdgrp lng="es">
    <trd>rosquilla</trd>,
    <trd>rosquillo</trd>,
    <trd>rosco <klr>(solamente dulce y peque&ntilde;o)</klr></trd>,
    <trd>rosca <klr>(solamente dulce y peque&ntilde;a)</klr></trd>
  </trdgrp>
  <trd lng="fr">craquelin</trd>
  <trd lng="hu">perec (&eacute;des)</trd>
  <trdgrp lng="nl">
    <trd>kringel</trd>,
    <trd>kransje</trd>
  </trdgrp>
  <trd lng="pl">obwarzanek</trd>
  <trdgrp lng="pt">
    <trd>rosquinha</trd>,
    <trd>rosquilha</trd>,
    <trd>sequilho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_n;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">suché pečivo</trd>
</drv>
</art>
