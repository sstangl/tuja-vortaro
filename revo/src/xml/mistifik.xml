<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mistifik.xml,v 1.11 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>8</ofc><rad>mistifik</rad>/i<fnt>Z</fnt>
</kap>
<drv mrk="mistifik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxerc.0i">&Scirc;erci</ref> per malgrava trompo,
      trouzante ies kredemon.
    </dif>
    <refgrp tip="sin">
      <ref cel="mistif.0i">mistifi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>mystifikovat</trd>,
    <trd>obloudit</trd>,
    <trd>oklamat</trd>,
    <trd>pobláznit</trd>,
    <trd>podvádět</trd>,
    <trd>zmámit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>foppen</trd>,
    <trd>narren</trd>,
    <trd>jemanden hinters <ind>Licht</ind> f&uuml;hren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fool</trd>,
    <trd>hoax</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire une <ind>blague</ind></trd>,
    <trd>mystifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;szed</trd>,
    <trd>becsap</trd>,
    <trd>f&eacute;lrevezet</trd>,
    <trd>beugrat</trd>,
    <trd>k&ouml;d&ouml;s&iacute;t</trd>
    <trd>misztifik&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">foppen</trd>
  <trdgrp lng="pt">
   <trd>mistificar</trd>,
   <trd>ludibriar</trd>,
   <trd>burlar</trd>   
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>mystifikovať</trd>,
    <trd>oklamať</trd>,
    <trd>poblázniť</trd>,
    <trd>podvádzať</trd>,
    <trd>zmámiť</trd>
  </trdgrp>
</drv>

<drv mrk="mistifik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta celitan trompi&gcirc;on a&ubreve; konfuzon:
      <ekz>
        mi estas normala simpla homo, sed la sorto kondukis min en &ccirc;i
        tiun
        labirinton de ege utilaj kaj foje <tld/>aj
        fenomenoj<fnt><aut>Will
        Green</aut>: <vrk>Telefona labirinto</vrk>,
        <bib>Monato</bib><lok>jaro 1994a, numero 1a, p.
        13a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klamný</trd>,
    <trd>nepochopitelný</trd>
  </trdgrp>
  <trd lng="en">tricky</trd>
  <trdgrp lng="fr">
    <trd>blagueur<klr> (adj.)</klr></trd>,
    <trd>mystificateur<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klamný</trd>,
    <trd>nepochopiteľný</trd>
  </trdgrp>
</drv>

<drv mrk="mistifik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;erca trompo:
      <ekz>
        li sukcesis travidi la multajn <tld/>ojn, kiuj kamufle
        &ccirc;irka&ubreve;vualas
        nian realan movadan historion<fnt><aut>Pejno Simono</aut>:
        <vrk>Faligas la fundamentojn de esperantismo (Ziko Marcus
        Sikosek,
        Esperanto sen mitoj)</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero 6a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>klamání</trd>,
    <trd>mystifikace</trd>,
    <trd>podvádění</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hoax</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blague<klr> (tromperie)</klr></trd>,
    <trd>canular</trd>,
    <trd>mystification</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>klamanie</trd>,
    <trd>podvádzanie</trd>
  </trdgrp>
</drv>
</art>

