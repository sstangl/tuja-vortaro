<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bronki.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <ofc>2</ofc>
    <rad>bronkit</rad>/o
  </kap>
  <drv mrk="bronki.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
      Bronka <ref tip="lst" cel="inflam.0o"
      lst="voko:malsanoj">inflamo</ref>.
      </dif>
      <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_h;&c_ib;&c_t;</trd>
      <trd lng="en">bronchitis</trd>
      <trd lng="es">bronquitis</trd>
      <trd lng="fa">&a_b;&a_r;&a_n;&a_sh;&f_y;&a_t;</trd>
      <trd lng="fr">bronchite</trd>
      <trdgrp lng="hu">
        <trd>houml;rghurut</trd>,
        <trd>bronchitis</trd>
      </trdgrp>
      <trdgrp lng="id">
        <trd>bronkitis</trd>,
        <trd><ind>radang</ind> cabang tenggorok</trd>
      </trdgrp>
      <trd lng="it">bronchite</trd>
      <trd lng="nl">bronchitis</trd>
      <trdgrp lng="pl">
        <trd>bronchit</trd>,
        <trd>zapalenie <ind>oskrzeli</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_h;&c_i;&c_t;</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>bronchitida</trd>,
    <trd>zánět průdušek</trd>
  </trdgrp>
  <trd lng="sk">zápal priedušiek</trd>
</drv>
</art>
