<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kriptomeri.xml,v 1.8 2013/10/26 16:10:29 revo Exp $">
<kap>
  <rad>kriptomeri</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kriptomeri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kriptomeri.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Pinglofolia arbo el <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref> kun
      dika, ru&gcirc;e bruna &scirc;elo,malgrandaj
      globaj strobiloj, nature kreskantaj en arbaroj de &Ccirc;inujo,
      Koreujo, Japanujo; <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="cipres1.0oj">cipresacoj</ref> (<trd
      lng="la">Cryptomeria</trd>):
      <ekz>
        inter <tld/>oj sagas nigraj kaj longaj vostoj de pigoj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;de konfuceanoj mi
        lernis
        etiketon por man&gcirc;i&rightquot;, p. 217a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sicheltanne</trd>,
    <trd>Japanische Zeder</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_d;&a_r; &f_zh;&a_A;&f_p;&a_n;&f_y;</trd>,
    <trd>&f_k;&a_r;&f_y;&f_p;&a_t;&a_w;&a_m;&a_r;&f_y;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&egrave;dre du Japon</trd>,
    <trd>cryptom&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jap&aacute;nciprus</trd>,
    <trd>szugifeny&odblac;</trd>
  </trdgrp>
  <trd lng="nl">Japanse <ind>ceder</ind></trd>
  <trdgrp lng="pt">
    <trd>criptom&eacute;ria</trd>,
    <trd>cedro-japon&ecirc;s</trd>
  </trdgrp>
</drv>
</art>

