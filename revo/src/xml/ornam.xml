<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ornam.xml,v 1.21 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>ornam</rad>/i
</kap>
<drv mrk="ornam.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ornam.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Aldoni al tuto plibeligajn detalojn:
      <ekz>
        <tld/>i tablon per
        floroj, <tld/>i &ccirc;ambron per pentra&jcirc;oj;
      </ekz>
      <ekz>
        e&ccirc; &scirc;tipo estos bela, se vi &gcirc;in <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        flag<tld/>i domon, &scirc;ipon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekora.0i">dekoracii</ref>
    </refgrp>
  </snc>
  <snc mrk="ornam.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Servi por <tld/>i <sncref ref="ornam.0i.iu"/>:
      <ekz>
        kiun vesto <tld/>as, tiun homoj ekamas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>být zdobný</trd>,
    <trd>vyzdobit</trd>,
    <trd>zdobit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schm&uuml;cken</trd>,
    <trd>verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&sigma;&kappa;&omikron;&my;&omega_ton;</trd>
  <trd lng="en">adorn</trd>
  <trd lng="es">adornar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;corer</trd>,
    <trd>enjoliver</trd>,
    <trd>orner</trd>,
    <trd>ornementer</trd>,
    <trd>parer</trd>
  </trdgrp>
  <trd lng="hu">d&iacute;sz&iacute;t</trd>
  <trd lng="nl">versieren</trd>
  <trdgrp lng="pt">
    <trd>ornamentar</trd>,
    <trd>decorar</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zdobiť</trd>
  <trdgrp lng="sv">
    <trd>pryda</trd>,
    <trd>smycka</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornam.0o.ado">
    <ref tip="dif" cel="ornam.0ado">ornamado</ref>
    <trdgrp lng="be">
      <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schm&uuml;cken</trd>,
      <trd>Verzieren</trd>
    </trdgrp>
  </snc>
  <snc mrk="ornam.0o.ajxo">
    <ref tip="dif" cel="ornam.0ajxo">ornama&jcirc;o</ref>
    <trd lng="be">&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>d&iacute;sz</trd>,
      <trd>d&iacute;sz&iacute;tm&eacute;ny</trd>,
      <trd>ornamentum</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schmuck</trd>,
      <trd>Verzierung</trd>,
      <trd>Zierrat</trd>,
      <trd>Ornament</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ornament</trd>,
    <trd>ozdoba</trd>,
    <trd>zdobení</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ozdoba</trd>,
    <trd>výzdoba</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Provizado de loko a&ubreve; objekto per <tld/>a&jcirc;oj.
    </dif>
    <trd lng="hu">d&iacute;sz&iacute;tm&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">zdobení</trd>
  <trdgrp lng="de">
    <trd>Schm&uuml;cken</trd>,
    <trd>Verzieren</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa;&kappa;&omikron_ton;&sigma;&my;&eta;&sigma;&eta;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coration<klr> (action)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ornamenta&ccedil;&atilde;o</trd>,
    <trd>decora&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;<klr> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">zdobenie</trd>
</drv>

<drv mrk="ornam.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Detala plibeliga&jcirc;o:
      <ekz>
        arkitekturaj <tld/>oj;
      </ekz>
      <ekz>
        tiu monumento estas la plej bela <tld/>o de la urbo;
      </ekz>
      <ekz>
        jen renkonte al li iras virino en <tld/>o de
        publikulino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o por maljunuloj estas grizeco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekora.0o">dekoracio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schmuck</trd>,
    <trd>Verzierung</trd>,
    <trd>Zierrat</trd>,
    <trd>Ornament</trd>
  </trdgrp>
  <trd lng="el">&delta;&jota;&alfa_ton;&sigma;&kappa;&omikron;&my;&omikron;</trd>
  <trd lng="es">adorno</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;coration<klr> (ornement)</klr></trd>,
    <trd>ornement</trd>,
    <trd>parure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>d&iacute;sz</trd>,
    <trd>d&iacute;sz&iacute;tm&eacute;ny</trd>,
    <trd>ornamentum</trd>,
  </trdgrp>
  <trd lng="nl">versiering</trd>
  <trdgrp lng="pt">
    <trd>ornamento</trd>,
    <trd>enfeite</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ornam.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu fabrikas a&ubreve; metas <tld/>a&jcirc;ojn
      por plibeligi ekzemple hejmon.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">dekoratér</trd>
  <trdgrp lng="fr">
    <trd>architecte d'int&eacute;rieur</trd>,
    <trd>d&eacute;corateur</trd>,
    <trd>ornemaniste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>d&iacute;sz&iacute;t&odblac;m&udblac;v&eacute;sz</trd>,
    <trd>dekorat&odblac;r</trd>,
    <trd>ornamentalista</trd>
  </trdgrp>
  <trd lng="nl">decorateur</trd>
  <trd lng="pt">ornamentista</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>okrášľovací technik</trd>,
    <trd>ozdobovací umelec</trd>
  </trdgrp>
</drv>

<drv mrk="ornam.melodi0o">
  <kap>melodi<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Kromtonoj, per kiuj muzikanto ofte improvize ornamas melodion,
      tamen ne rompante ties ritmon.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ornement musical</trd>
  </trdgrp>
  <trd lng="hu">zenei d&iacute;sz&iacute;tm&eacute;ny</trd>
  <trdgrp lng="pt">
    <trd>ornato</trd>,
    <trd>ornamento</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
