<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kocxer.xml,v 1.13 2011/12/03 18:10:23 revo Exp $">
<kap>
  <rad>ko&ccirc;er</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kocxer.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vetur.0igisto">Veturigisto</ref> de 
kale&scirc;o:
      <ekz>
        la <tld/>o de Nikolao Petrovi&ccirc; elkondukis la &ccirc;evalojn
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 2a, p. 
10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_ib;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cotxer</trd>,
    <trd>majoral</trd>
  </trdgrp>
  <trd lng="de">Kutscher</trd>
  <trd lng="en">coachman</trd>
  <trdgrp lng="fr">
    <trd>cocher</trd>
  </trdgrp>
  <trd lng="nl">koetsier</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kocxer.K0o">
  <kap><tld lit="K"/>o</kap>
  <mlg>Aur</mlg>
  <snc>
    <dif>
      Nord-hemisfera stelfiguro (<trd lng="la">Auriga</trd>).
    </dif>
  </snc>
  <trd lng="ca">el Cotxer</trd>
  <trd lng="de">Fuhrmann</trd>
  <trd lng="en">Auriga</trd>
  <trd lng="es">Cochero</trd>
  <trd lng="nl">Voerman<klr tip="ind"> (sterrenbeeld)</klr></trd>
</drv>

</art>
