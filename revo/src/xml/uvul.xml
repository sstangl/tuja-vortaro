<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: uvul.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <rad>uvul</rad>/o
    </kap>

    <drv mrk="uvul.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ANA</uzo>
      <snc>
        <dif>
          Karna apendico, pendanta &ccirc;e la mezo de la malanta&ubreve;a rando de
          la palata <ref cel="vel.0o.ANA">velo</ref>.
        </dif>
      </snc>
  <trdgrp lng="cs">
    <trd>uvula</trd>,
    <trd>čip</trd>,
    <trd>čípek</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Z&auml;pfchen</trd>,
        <trd>Uvula [science]</trd></trdgrp>
  <trd lng="es">&uacute;vula</trd>
  <trd lng="fi">kitakieleke</trd>
  <trd lng="fr">luette</trd>
  <trdgrp lng="hu">
        <trd>nyelvcsap</trd>,
        <trd>uvula</trd>
      </trdgrp>
  <trd lng="it">ugola</trd>
  <trd lng="nl">huig</trd>
  <trd lng="pl">j&eogonek;zyczek podniebienny</trd>
  <trd lng="pt">&uacute;vula</trd>
  <trdgrp lng="ru">
        <trd>&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k;</trd>,
        <trd>&c_u;&c_v;&c_u;&c_l;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>uvula</trd>,
    <trd>čapík</trd>
  </trdgrp>
</drv>

    <drv mrk="uvul.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <uzo tip="fak">FON</uzo>
            <tld/>a ro
            <klr>(= ro produktata de la vibrado de la <tld/>o)</klr>
            <ref tip="vid" cel="kartav.0i">kartava</ref>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_ja;&c_z;&c_y;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
        <trd>&c_u;&c_v;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

  </art>
