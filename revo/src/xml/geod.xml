<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geod.xml,v 1.12 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>geod</rad>/o
</kap>
<drv mrk="geod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala, kavforma maso, sfereca, interne kovrita de kristaloj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="gang.0o">gango</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">geoda</trd>
  <trdgrp lng="cs">
    <trd>druza</trd>,
    <trd>geoda</trd>,
    <trd>orlovec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Geode</trd>,
     <trd>Druse</trd></trdgrp>
  <trd lng="fr">g&eacute;ode</trd>
  <trdgrp lng="hu">
    <trd>geoda</trd>,
    <trd>druza</trd>
  </trdgrp>
  <trd lng="nl">geode</trd>
  <trd lng="pt">geode</trd>
  <trd lng="ru">&c_zh;&c_je;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">geoda</trd>
</drv>
</art>
