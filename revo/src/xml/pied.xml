<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pied.xml,v 1.52 2015/07/09 06:10:14 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>pied</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pied.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="pied.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Artikita parto &ccirc;e ekstremo de la homa a&ubreve; besta
       <ref tip="malprt" cel="krur.0o.gambo">kruro</ref>,
       servanta por stari kaj pa&scirc;i:
       <ekz>
         mi sidas sur se&gcirc;o kaj tenas
         la <tld/>ojn sur benketo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         &scirc;i &jcirc;etis
         sin al la <tld/>oj de sia edzo<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>bati, <tld/>frapi, <tld/>premi
         (<ref tip="sin" cel="tret.0i">treti</ref>);
       </ekz>
       <ekz>
         <tld/>a <klr>(kie ku&scirc;as la
         <tld/>oj)</klr> parto de lito<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kurba<tld/>a ulo<fnt>Z</fnt>.
       </ekz>
       <ekz>
         li ne volis krei la&ubreve; sia persona pla&ccirc;o la <tld/>an
         lingvon <ind>de l' kapo &gcirc;is la <tld/>oj</ind><fnt>Z</fnt>.
         <trd lng="ca">de cap a peus</trd>
         <trd lng="es">de los pies a la cabeza</trd>
       </ekz>
     </dif>
     <trd lng="bg">&c_k;&c_r;&c_a;&c_k;</trd>
     <trd lng="ca">peu</trd>
     <trd lng="de">Fu&szlig;</trd>
     <trd lng="es">pie</trd>
     <trd lng="ku">nig</trd>
     <trd lng="nl">voet</trd>
     <trd lng="pl">stopa</trd>
     <trd lng="pt">p&eacute;</trd>
     <trdgrp lng="ru">
       <trd>&c_s;&c_t;&c_u;&c_p;&c_n;&c_ja;</trd>,
       <trd>&c_n;&c_o;&c_g;&c_a;</trd>
     </trdgrp>
     <trd lng="sv">fot</trd>
     <trd lng="tr">ayak</trd>
   </snc>
   <snc mrk="pied.0o.mebla">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Parto sur kiu meblo staras:
       <ekz>
         tablo kun kvar <tld/>oj;
       </ekz>
       <ekz>
         la gefian&ccirc;oj sidis, &ccirc;iu apogita al <tld/>o de tablo
         <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
       </ekz>
     </dif>
     <rim>
       Kvankam Zamenhofa, tiu uzo
       &scirc;ajnas malkonvena al iuj, kiuj
       preferas por tiu senco la vorton
       <ref tip="sin" cel="krur.0o.mebla">kruro</ref>
       (krom se temas pri speciale fleksita kaj
       skulptita parto de mebla kruro). Komparu anka&ubreve; kun
       <ctl>brakse&gcirc;o</ctl>, kiu havas brak-, ne man-apogilojn.
     </rim>
     <trd lng="ca">petge</trd>
     <trd lng="es">pie</trd>
     <trd lng="nl">poot</trd>
     <trd lng="ru">&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
     <trd lng="pl">noga</trd>
     <trd lng="pt">p&eacute;</trd>
     <trd lng="sv">ben</trd>
     <trd lng="tr">ayak</trd>
   </snc>
   <snc mrk="pied.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malsupra parto de io:
       <ekz>
         <tld/>o de roko, de pa&gcirc;o;
       </ekz>
       <ekz>
         <tld/>noto;
       </ekz>
       <ekz>
         anta&ubreve; &ccirc;io ni devas penadi, ke nia afero
         stari&gcirc;u sur fortaj <tld/>oj <klr>(bazo)</klr> en la
         mondo<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>base</trd>,
       <trd>cossol</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>pie</trd>,
       <trd>pata</trd>,
       <trd>base</trd>
     </trdgrp>
     <trd lng="nl">voet</trd>
     <trd lng="pl">stopa</trd>
     <trdgrp lng="pt">
       <trd>sop&eacute;</trd>,
       <trd>rodap&eacute;</trd>,
       <trd>base</trd>
     </trdgrp>
     <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; 
<klr>(&c_g;&c_o;&c_r;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
     <trd lng="sv">fot</trd>
     <trd lng="tr">ayak</trd>
   </snc>
   <snc mrk="pied.0o.POE">
     <uzo tip="fak">POE</uzo>
     <dif>
       Parto de <ref tip="malprt" cel="vers.0o">verso</ref>, konsistanta
       el
       karakteriza kombina&jcirc;o de <ref cel="silab.0o">silaboj</ref> kun
       difinita longeco a&ubreve; akcentiteco:
       <ekz>
         en Esperanta metriko la &ccirc;efaj <tld/>oj estas du- a&ubreve; 
tri-silabaj
         kaj entenas nur unu akcentitan silabon.
       </ekz>
     </dif>
     <refgrp tip="sub">
       Dusilabaj piedoj:
       <ref cel="jamb.0o">jambo</ref>,
       <ref cel="pirik.0o">piriko</ref>,
       <ref cel="sponde.0o">spondeo</ref>,
       <ref cel="troke.0o">trokeo</ref>;
       trisilabaj piedoj:
       <ref cel="amfibr.0o">amfibra&hcirc;o</ref>,
       <ref cel="anapes.0o">anapesto</ref>,
       <ref cel="daktil1.0o.POE">daktilo</ref>,
       <ref cel="tribra.0o">tribrako</ref>;
       kvarsilaba piedo:
       <ref cel="jamb.hxor0o">&hcirc;orjambo</ref>.
     </refgrp>
     <trd lng="ca">peu <klr>(po&egrave;tica)</klr></trd>
     <trd lng="es">pie <klr>(poes&iacute;a)</klr></trd>
     <trd lng="nl">versvoet</trd>
     <trd lng="pt">p&eacute;</trd>
     <trd lng="ru">&c_s;&c_t;&c_o;&c_p;&c_a;</trd>
   </snc>
   <snc mrk="pied.0o.MAT">
     <uzo tip="fak">MAT</uzo>
     <uzo tip="stl">ARK</uzo>
     <fnt><bib>MatTerm</bib><lok>p. 26</lok></fnt>
     <uzo tip="klr">(fakula &jcirc;argono)</uzo>
     <dif>
       <klr>(de <ref cel="ort.0anto">ortanto</ref>, sur ajna al &gcirc;i orta
       <ref cel="figur.geometria0o">geometria figuro</ref>)</klr>
       La punkto, en kiu ili sin orte sekcas:
       <ekz>
         la <tld/>o de <ref cel="ort.mez0anto">mezortanto</ref> de
         streko
         estas ties <ref cel="mez.0o.punkto">mezo</ref>.
       </ekz>
     </dif>
     <trd lng="ca">peu d'una perpendicular</trd>
     <trd lng="de">Lotfu&szlig;punkt</trd>
     <trd lng="en">foot<klr tip="ind"> (of a perpendicular)</klr></trd>
     <trd lng="es">pie<klr tip="ind"> (de una perpendicular)</klr></trd>
     <trd lng="fr">pied<klr tip="ind"> (d'une
     perpendiculaire)</klr></trd>
     <trd lng="pl">spodek<klr tip="ind"> (prostopad&lstroke;ej)</klr></trd>
     <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;<klr 
tip="ind"> 
(&c_p;&c_je;&c_r;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_a;)</klr></trd>
   </snc>
  <trd lng="br">troad</trd>
  <trdgrp lng="cs">
    <trd>konec</trd>,
    <trd>noha <klr tip="ind">(od kotn&iacute;ku
   dol&uring;)</klr></trd>,
    <trd>stopa</trd>
  </trdgrp>
  <trd lng="de">Fu&szlig;</trd>
  <trd lng="en">foot</trd>
  <trd lng="es">pie<klr> (anatom&iacute;a, parte baja de algo)</klr></trd>
  <trd lng="fr">pied</trd>
  <trd lng="hu">l&aacute;b</trd>
  <trd lng="it">piede</trd>
  <trd lng="pl">stopa</trd>
  <trdgrp lng="sk">
    <trd>koniec</trd>,
    <trd>stopa</trd>
  </trdgrp>
  <trd lng="vo">fut</trd>
</drv>

<drv mrk="pied.0e">
   <kap><tld/>e</kap>
   <snc mrk="pied.0e.KOMUNE">
     <dif>
       Per la <tld/>oj:
       <ekz>
         iri <tld/>e;
       </ekz>
       <ekz>
         malfeli&ccirc;o venas rajde, foriras <tld/>e
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trd lng="br">war droad</trd>
     <trdgrp lng="ca">
       <trd>a peu</trd>,
       <trd>caminant</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>zu <ind>Fu&szlig;</ind></trd>,
       <trd>by foot</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>a pie</trd>,
       <trd>caminando</trd>
     </trdgrp>
     <trd lng="fr">&agrave; <ind>pied</ind></trd>
     <trd lng="hu">gyalog</trd>
     <trd lng="it">a <ind>piedi</ind></trd>
     <trd lng="nl">te <ind>voet</ind></trd>
     <trdgrp lng="oc">
       <trd>a <ind>p&egrave;</ind></trd>,
       <trd>en <ind>caminant</ind></trd>
     </trdgrp>
     <trd lng="pl">pieszo</trd>
     <trd lng="ru">&c_p;&c_je;&c_sh;&c_k;&c_o;&c_m;</trd>
     <trd lng="sv">till <ind>fots</ind></trd>
   </snc>
  <trd lng="cs">p&ecaron;&scaron;ky</trd>
  <trdgrp lng="sk">
    <trd>pe&scaron;i</trd>,
    <trd>pe&scaron;ky</trd>,
    <trd>pe&scaron;o</trd>
  </trdgrp>
</drv>

<drv mrk="pied.nud0e">
   <kap>nud<tld/>e</kap>
   <snc mrk="pied.nud0e.KOMUNE">
     <dif>
       Per la piedoj nudaj.
       <ekz>
         Mi plandas plu nud<tld/>e sur pado sinuanta preter mar&ccirc;oj kaj
         montetoj. 
         <fnt><aut>S. Johansson</aut>,
       <vrk><url 
ref="http://esperanto.net/literaturo/tekstoj/johansson/nokt.html"> Nokta
halto</url></vrk></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>descal&ccedil;</trd>,
       <trd>amb els peus nus</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>descalzo</trd>,
       <trd>con los pies desnudos</trd>
     </trdgrp>
     <trd lng="fr">&agrave; pieds nus</trd>
   </snc>
  <trd lng="cs">bos</trd>
  <trdgrp lng="sk">
    <trd>bosky</trd>,
    <trd>naboso</trd>
  </trdgrp>
</drv>


<drv mrk="pied.0e_de">
   <kap><tld/>e de</kap>
   <snc mrk="pied.0e_de.iu">
     <dif>
       &Ccirc;e la <tld/>oj de iu.
     </dif>
     <trd lng="ca">als <ind>peus</ind> de</trd>
     <trd lng="de">zu <ind>F&uuml;&szlig;en</ind> <klr tip="ind">(von)</klr></trd>
     <trd lng="es">a los <ind>pies</ind> de</trd>
     <trd lng="fr">aux <ind>pieds</ind> de</trd>
     <trd lng="nl">aan de <ind>voeten</ind></trd>
     <trd lng="pl">u st&oacute;p</trd>
     <trd lng="sv">vid <ind>f&ouml;tterna</ind></trd>
     <trd lng="ru">&c_v; <ind>&c_n;&c_o;&c_g;&c_a;&c_h;</ind> &c_u;<klr> 
(&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
   </snc>
   <snc mrk="pied.0e_de.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malsupre de io:
       <ekz>
         <tld/>e de la pa&gcirc;o trovi&gcirc;as noto.
       </ekz>
     </dif>
     <trd lng="br">ouzh <ind>troad</ind></trd>
     <trd lng="ca">al <ind>peu</ind> de</trd>
     <trd lng="de">am <ind>Fu&szlig;</ind> <klr tip="ind">(von)</klr></trd>
     <trd lng="es">al <ind>pie</ind> de</trd>
     <trd lng="fr">au <ind>pied</ind> de</trd>
     <trd lng="nl">aan de <ind>voet</ind></trd>
     <trd lng="ru">&c_u; 
<ind>&c_p;&c_o;&c_d;&c_n;&c_o;&c_zh;&c_i;&c_ja;</ind></trd>
   </snc>
   <trdgrp lng="en">
     <trd>at the foot of</trd>,
     <trd>below</trd>
   </trdgrp>
   <trd lng="it">ai <ind>piedi</ind> di</trd>
</drv>

<drv mrk="pied.0ingo">
   <kap><tld/>ingo</kap>
   <snc mrk="pied.0ingo.RAJ">
     <dif>
       Speco de ringo, kiu pendas de la selo kaj en kiun oni metas
       ekstrema&jcirc;ojn de la <tld/>oj por sur&ccirc;evali&gcirc;i kaj rajdi.
     </dif>
   </snc>
  <trd lng="br">stleug</trd>
  <trd lng="ca">estrep</trd>
  <trd lng="cs">t&rcaron;men sedla</trd>
  <trd lng="de">Steigb&uuml;gel</trd>
  <trdgrp lng="en">
     <trd>stirrup <klr>(horse)</klr></trd>,
     <trd>toe-clip <klr>(bicycle)</klr></trd>
   </trdgrp>
  <trd lng="es">estribo<klr> (de la silla de montar)</klr></trd>
  <trd lng="fr">&eacute;trier</trd>
  <trd lng="hu">kengyel</trd>
  <trdgrp lng="it">
     <trd>staffa<klr> (equitazione)</klr></trd>,
     <trd>puntapiede<klr> (bicicletta)</klr></trd>
   </trdgrp>
  <trd lng="nl">stijgbeugel</trd>
  <trd lng="pl">strzemi&eogonek;</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_m;&c_ja;</trd>
  <trd lng="sk">strme&ncaron;</trd>
  <trd lng="sv">stigbygel</trd>
</drv>

<drv mrk="pied.du0ulo">
   <kap>du<tld/>ulo</kap>
   <snc mrk="pied.du0ulo.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="super" cel="animal.0o">Animalo</ref>, iranta sur du
       <tld/>oj.
     </dif>
   </snc>
   <trd lng="br">daoudroadeg</trd>
   <trd lng="ca">b&iacute;pede</trd>
   <trd lng="en">biped</trd>
   <trd lng="es">b&iacute;pedo</trd>
   <trd lng="fr">bip&egrave;de<klr tip="ind"> (subst.)</klr></trd>
   <trd lng="it">bipede</trd>
   <trd lng="ru">&c_d;&c_v;&c_u;&c_n;&c_o;&c_g;&c_o;&c_je; 
&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="pied.kvar0ulo">
   <kap>kvar<tld/>ulo</kap>
   <snc mrk="pied.kvar0ulo.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="super" cel="animal.0o">Animalo</ref>, iranta sur kvar 
<tld/>oj.
     </dif>
   </snc>
   <trd lng="br">pevarzroadeg</trd>
   <trd lng="ca">quadr&uacute;pede</trd>
   <trd lng="en">quadruped</trd>
   <trd lng="es">cuadr&uacute;pedo</trd>
   <trd lng="fr">quadrup&egrave;de</trd>
   <trd lng="it">quadrupede</trd>
   <trd lng="pl">czworon&oacute;g</trd>
   <trd lng="ru">&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_o;&c_n;&c_o;&c_g;&c_o;&c_je; 
&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="pied.tri0o">
   <kap>tri<tld/>o</kap>
   <snc mrk="pied.tri0o.KOMUNE">
     <dif>
       Tablo a&ubreve; se&gcirc;o kun tri <tld/>oj.
     </dif>
     <rim>
       <ctl>TriKRURA se&gcirc;o</ctl> estus pli trafa kaj klara.
     </rim>
     <adm>
       &Ccirc;i rimarko estas &gcirc;usta, sed eble ne tre utila. &Gcirc;i
       povus apliki&gcirc;i al &ccirc;iuj deriva&jcirc;oj de la tipo "n-xo":
       "triangula figuro" estas pli klara ol "triangulo",
       "&ccirc;irka&ubreve;mana ornama&jcirc;o" pli ol "&ccirc;irka&ubreve;mano", kaj multaj
       similaj. <aut>MB</aut>
     </adm>
     <adm>
       La rimarko celas ne tiom la strukturon de la kunmetajxo, kiom
       la misan radikon: koncerna segxo povas tute ne havi piedojn
       (iel apartajn, ekz-e pli dikajn, skulptitajn malsupran
       ekstremajxojn), sed gxi nepre havas "krurojn" (aux
       "gambojn").  Angle mi nomus ilin "legs"
       (a tripod is a three-LEGGED object), ne "feet".
       <aut>Sergio</aut>
     </adm>
   </snc>
  <trd lng="ca">tr&iacute;pode</trd>
  <trd lng="cs">trojno&zcaron;ka</trd>
  <trd lng="de">Dreibein</trd>
  <trd lng="en">tripod</trd>
  <trd lng="es">tr&iacute;pode</trd>
  <trd lng="fr">tr&eacute;pied</trd>
  <trd lng="it">treppiede</trd>
  <trd lng="pl">tr&oacute;jn&oacute;g</trd>
  <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_je;&c_n;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>,
     <trd>&c_t;&c_r;&c_je;&c_n;&c_o;&c_g;&c_a;</trd>
   </trdgrp>
  <trd lng="sk">trojno&zcaron;ka</trd>
  <trd lng="sv">trefot</trd>
</drv>

<drv mrk="pied.kapo0e">
   <kap>kapo<tld/>e</kap>
   <snc mrk="pied.kapo0e.KOMUNE">
     <dif>
       Starante duope tiel, ke &ccirc;iu havas la kapon en la loko, en kiu la
       alia havas la <tld/>ojn.
     </dif>
   </snc>
  <trdgrp lng="en">
     <trd>end-to-end</trd>,
     <trd>head-to-toe</trd>
   </trdgrp>
  <trd lng="fr">t&ecirc;te-b&ecirc;che</trd>
  <trd lng="hu">fejt&odblac;l-l&aacute;bt&oacute;l</trd>
  <trd lng="nl">kop-aan-staart</trd>
  <trd lng="pl">pozycja 69</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;&c_o;&c_m;</trd>
  <trd lng="sk">hore nohami</trd>
</drv>

<drv mrk="pied.kapo0uloj">
   <kap>kapo<tld/>uloj</kap>
   <uzo tip="stl">KOMUNE</uzo>
   <snc mrk="pied.kapo0uloj.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <ref tip="dif" cel="cefalo.0oj">cefalopodoj</ref>
   </snc>
  <trd lng="br">penntroadeged</trd>
  <trd lng="ca">cefal&ograve;podes</trd>
  <trd lng="cs">hlavono&zcaron;ci</trd>
  <trd lng="en">cephalopods</trd>
  <trd lng="es">cefal&oacute;podos</trd>
  <trd lng="fr">c&eacute;phalopodes</trd>
  <trd lng="hu">l&aacute;basfej&udblac;ek</trd>
  <trd lng="it">cefalopodi</trd>
  <trd lng="pl">g&lstroke;owonogi</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  <trd lng="sk">hlavono&zcaron;ce</trd>
</drv>

<drv mrk="pied.mult0uloj">
   <kap>mult<tld/>uloj</kap>
   <uzo tip="stl">KOMUNE</uzo>
   <snc mrk="pied.mult0uloj.ZOO">
     <uzo tip="fak">ZOO</uzo>
