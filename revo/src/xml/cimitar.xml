<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cimitar.xml,v 1.8 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>cimitar</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="cimitar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sabr.0o">Sabro</ref> kun tre kurba klingo,
      kiajn oni uzadis precipe en arabaj landoj:
      <ekz>
        la renegato ensaltis la unua, eksvingis <tld/>on kaj
        kriis<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 41a, p.
        312a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_ja;&c_t;&c_a;&c_g;&c_a;&c_n;</trd>
  <trd lng="ca">simitarra</trd>
  <trdgrp lng="cs">
    <trd>handžár</trd>,
    <trd>jatagan</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Scimitar</trd>,
    <trd>Krumms&auml;bel</trd>
  </trdgrp>
  <trd lng="en">scimitar</trd>
  <trd lng="es">cimitarra</trd>
  <trd lng="fr">cimeterre</trd>
  <trd lng="it">scimitarra</trd>
  <trdgrp lng="nl">
    <trd>kromzwaard</trd>,
    <trd>scimitar</trd>
  </trdgrp>
  <trd lng="pl">bu&lstroke;at</trd>
  <trdgrp lng="sk">
    <trd>arabský meč</trd>,
    <trd>handžár</trd>
  </trdgrp>
</drv>
</art>

