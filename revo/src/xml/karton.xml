<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karton.xml,v 1.18 2013/08/19 17:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karton</rad>/o
</kap>
<drv mrk="karton.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Pli a&ubreve; malpli dika folio, farita el paperpasto pistita, gluita kaj
      sekigita sub premilo:
      <ekz>
        glui fotografa&jcirc;on sur <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>karton</trd>,
      <trd>tektura</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Simila pasto, uzata por fabriki aliajn objektojn:
      <ekz>
        <tld/>a pupo, masko.
      </ekz>
    </dif>
    <trd lng="pl">papier-m&acirc;ch&eacute;</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_t;&c_o;&c_n;</trd>
  <trd lng="br">karton</trd>
  <trdgrp lng="de">
     <trd>Karton</trd>,
     <trd>Pappe</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>cardboard</trd>,
     <trd>pasteboard</trd>
  </trdgrp>
  <trd lng="es">cart&oacute;n</trd>
  <trd lng="fr">carton<klr tip="ind"> (mati&egrave;re)</klr></trd>
  <trd lng="hu">karton <klr tip="ind">(pap&iacute;r)</klr></trd>
  <trdgrp lng="ca">
    <trd>cart&oacute;</trd>,
    <trd>cartr&oacute;</trd>
  </trdgrp>
  <trd lng="nl">karton</trd>
  <trd lng="oc">carton</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
