<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monal.xml,v 1.7 2013/09/01 15:46:17 revo Exp $">
<kap>
  <rad>monal</rad>/o
</kap>
<drv mrk="monal.0o">
  <kap><tld/>o</kap>
  <snc mrk="monal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda birdo kun brilaj koloroj vivanta en altaj montaroj de
      Pakistano &gcirc;is &Ccirc;inujo, <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="fazane.0oj.ZOO">fazanedoj</ref> (<trd
      lng="la">Lophophorus</trd>) kun tri specioj: 
      <ekz>
        himalaja <tld/>o, verdvosta <tld/>o, blankmakula <tld/>o;
      </ekz>
      <ekz>
        la <ind>himalaja monalo</ind> <trd lng="la">Lophophorus
        impejanus</trd> <klr>...</klr> vivas en
        suda kaj sudorienta Tibeto de &Ccirc;inio kaj anka&ubreve; en la
regiono de
        orienta Afganio &gcirc;is Bhutano
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url
        ref="http://es.chinabroadcast.cn/1/2005/06/06/1@25404.htm">Himalaja
        monalo</url></vrk>,
        <lok>2005-06-06</lok></fnt>;
      </ekz>
      <ekz>
        la <ind>blankmakula monalo</ind> <trd lng="la">Lophophorus
        sclateri</trd>
         longas 70 cm kaj pezas 2,5 kg
        <fnt>
        <bib>Viki</bib>,
        <vrk>artikolo &leftquot;Blankmakula monalo&rightquot;,
2005-08-10</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Glanzfasan</trd>,
    <trd>Monal</trd>
  </trdgrp>
  <trd lng="en">monal</trd>
  <trd lng="fr">lophophore</trd>
  <trd lng="pt">fais&atilde;o-resplandescente</trd>
  <trd lng="sv">monal</trd>
</drv>
</art>
