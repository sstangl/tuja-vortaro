<kap>
  <ofc>2</ofc>
  <rad>angelik</rad>/o
</kap>
<drv mrk="angeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Angelica</trd>), kun aroma odoro:
      <ekz>
        li do preskribis kelkajn drogojn, kiaj ginsengo, <tld/>o,
        astragalo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, p. 567a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_g;&c_ib;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>andÄ›lika</trd>,
    <trd>prha</trd>
  </trdgrp>
  <trd lng="de">Engelwurz</trd>
  <trd lng="en">angelica</trd>
  <trd lng="fr">ang&eacute;lique</trd>
  <trdgrp lng="hu">
    <trd>angyalgy&ouml;k&eacute;r</trd>,
    <trd>angy&eacute;lika</trd>,
    <trd>angyalf&udblac;</trd>
  </trdgrp>
  <trd lng="nl">engelwortel</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_d;&c_ja;&c_g;&c_i;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">angelika</trd>
  <trd lng="sv">kvanne</trd>
</drv>
</art>
