<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klorof.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klorofil</rad>/o
</kap>
<drv mrk="klorof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Verda substanco de la &ccirc;eloj de l' folioj kaj herboj,
      asimilanta karbonon: 
      <ekz>la lumo estas necesa por produkti la <tld/>on.</ekz>
    </dif>
    <ref tip="malprt" cel="kloroplast.0o">chloroplast</ref>
    <trd lng="be">&c_h;&c_l;&c_ja;&c_r;&c_a;&c_f;&c_ib;&c_l;</trd>
    <trd lng="bg">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_i;&c_l;</trd>
    <trd lng="ca">clorofil&middot;la </trd>
    <trd lng="de">Chlorophyll</trd>
    <trd lng="en">chlorophyll</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_l;&a_r;&a_w;&a_f;&f_y;&a_l;</trd>,
      <trd>&a_s;&a_b;&a_z;&f_y;&a_n;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">chlorophylle</trd>
    <trdgrp lng="hu">
      <trd>klorofill</trd>,
      <trd>lev&eacute;lz&ouml;ld</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bladgroen</trd>,
      <trd>chlorofyl</trd>
    </trdgrp>
    <trd lng="pl">chlorofil</trd>
    <trd lng="ru">&c_h;&c_l;&c_o;&c_r;&c_o;&c_f;&c_i;&c_l;&c_l;</trd>
  </snc>
  <trd lng="cs">chlorofyl</trd>
  <trdgrp lng="sk">
    <trd>chlorofyl</trd>,
    <trd>listová zeleň</trd>
  </trdgrp>
</drv>
</art>
