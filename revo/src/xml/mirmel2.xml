<art mrk="$Id: mirmel2.xml,v 1.6 2013/08/31 16:10:22 revo Exp $">
<kap>
  <rad>mirmeleoned</rad>/oj
</kap>
<drv mrk="mirmel2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="flug.ret0iluloj">retflugiluloj</ref>
      (<trd lng="la">Myrmeleonidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_r;&c_a;&c_sh;&c_y;&c_n;&c_y;&c_ja; <ind>&c_l;&c_mol;&c_v;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">mirmele&ograve;nids</trd>
  <trd lng="de">Ameisenjungfern<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">hangyales&odblac;f&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_i;&c_n;&c_y;&c_je; &c_l;&c_mol;&c_v;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

