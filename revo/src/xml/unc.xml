<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: unc.xml,v 1.24 2015/11/20 17:10:41 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>unc</rad>/o
</kap>
<drv mrk="unc.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="unc.0o.MEZ">
     <dif>
       Malgranda <ref tip="super" cel="unu.mezur0o.SCI">mezurunuo</ref>
       de pezo varia la&ubreve; la landoj, valoranta proksimume 20
a&ubreve; 30
       gramojn:
       <ekz>
         dek <tld/>oj da oro
         <fnt>
           <aut>D. K. Jordan</aut>,
           <vrk>La Avara Muelisto</vrk>,
           <lok><url ref="http://esperanto.ca/kea/j-enkonduko.html">La
Fabeloj de Jordan</url>, [vidita en 2006-05-24]</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_u;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_u;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="br">o&ntilde;s</trd>
  <trd lng="ca">un&ccedil;a</trd>
  <trd lng="cs">unce</trd>
  <trd lng="de">Unze</trd>
  <trd lng="en">ounce</trd>
  <trd lng="es">onza</trd>
  <trd lng="fi">unssi</trd>
  <trd lng="fr">once</trd>
  <trd lng="he">&alef;&vav;&nun;&qof;&yod;&he;</trd>
  <trd lng="hu">uncia</trd>
  <trd lng="it">oncia</trd>
  <trd lng="nl">ons<klr tip="ind"> (gewicht)</klr></trd>
  <trd lng="pl">uncja</trd>
  <trd lng="pt">on&ccedil;a</trd>
  <trd lng="ru">&c_u;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">unca <klr>(v&aacute;ha)</klr></trd>
</drv>
</art>
