<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katakl.xml,v 1.14 2015/09/23 06:10:20 revo Exp $">
<kap>
  <rad>kataklism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katakl.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="katakl.0o.SCI">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">MET</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="katast.natur0o.SCI">Naturkatastrofo</ref>:
      <ekz>
        Japanio sekuras kontra&ubreve; krimoj, ne kontra&ubreve; <tld/>oj
        <fnt><aut>Hori Y.</aut>, <vrk>Kiam verda insulo nigri&gcirc;as</vrk>,
        <bib>Monato</bib>, <lok>2000:12, p. 16a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>la lasta granda <tld/>o historia, la
        disfalo de la sovetia bloko<fnt>
        <aut>H. Baksi&cacute;</aut>, <vrk>La
        fino de la mondo komenci&gcirc;as en Sarajevo</vrk>,
        <bib>Monato</bib>, <lok>1993:10, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_k;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">kataklyzma</trd>
  <trd lng="de">Kataklysmus</trd>
  <trdgrp lng="en">
    <trd>catastrophe</trd>,
    <trd>cataclysm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calamit&eacute;</trd>,
    <trd>catastrophe naturelle</trd>,
    <trd>cataclysme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>term&eacute;szeti <ind>katasztr&oacute;fa</ind></trd>,
    <trd>kataklizma</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>natuurramp</trd>,
    <trd>cataclysme</trd>
  </trdgrp>
  <trd lng="pl">kataklizm</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_k;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">kataklizma</trd>
</drv>
</art>
