<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sulf.xml,v 1.13 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>sulf</rad>/o
</kap>
<drv mrk="sulf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Mallongigita nomo de la sulfuro, kiun oni uzas en la nomoj de
&hcirc;emiaj
      kombinoj:
      <ekz>
        <tld/>ato;
      </ekz>
      <ekz>
        <tld/>ata, <tld/>ita anhidrido.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_l;&c_mol;&c_f;-</trd>
  </trdgrp>
  <trd lng="cs">síra</trd>
  <trd lng="de">Schwefel</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;n-</trd>,
    <trd>szulf-</trd>
  </trdgrp>
  <trd lng="it">solf-<klr> (prefix)</klr></trd>
  <trd lng="nl">zwavel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_l;&c_mol;&c_f;-</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>S</trd>,
    <trd>síra</trd>
  </trdgrp>
  <trd lng="sv">svavel</trd>
</drv>
</art>
