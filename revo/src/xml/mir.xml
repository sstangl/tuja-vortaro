<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mir.xml,v 1.23 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mir</rad>/i
</kap>
<drv mrk="mir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Senti impreson de surprizo pro io stranga, neatendita, nekutima,
      eksterordinara:
      <ekz>
        <tld/>i pri fremdaj kutimoj, pri akrobata&jcirc;o;
      </ekz>
      <ekz>
        <tld/>i ion;
      </ekz>
      <ekz>
        mi <tld/>as, ke li ankora&ubreve; ne respondis;
      </ekz>
      <ekz>
        mi <tld/>is scii&gcirc;ante, ke...
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir.0i">admiri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">divit se</trd>
  <trdgrp lng="de">
    <trd>sich <ind>wundern</ind></trd>,
    <trd>staunen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>marvel</trd>,
    <trd>be amazed</trd>,
    <trd>be astonished</trd>
  </trdgrp>
  <trd lng="es">maravillarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;merveiller</ind></trd>,
    <trd>&ecirc;tre <ind>surpris</ind></trd>
  </trdgrp>
  <trd lng="hu">csod&aacute;lkozik</trd>
  <trd lng="nl">zich <ind>verwonderen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diviť sa</trd>,
    <trd>čudovať sa</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impreso sentita de tiu, kiu <tld/>as:
      <ekz>
        mutigita de <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>div</trd>,
    <trd>mirabilium</trd>,
    <trd>podivnost</trd>
  </trdgrp>
  <trd lng="de">Verwunderung</trd>
  <trdgrp lng="en">
    <trd>wonder</trd>,
    <trd>amazement</trd>,
    <trd>astonishment</trd>
  </trdgrp>
  <trd lng="es">maravilla</trd>
  <trdgrp lng="fr">
    <trd>&eacute;merveillement</trd>,
    <trd>surprise</trd>
  </trdgrp>
  <trd lng="hu">csod&aacute;lkoz&aacute;s</trd>
  <trd lng="nl">verwondering</trd>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>div</trd>,
    <trd>podivuhodnosť</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Forte <tld/>i, senti da&ubreve;ran surprizi&gcirc;on:
      <ekz>
        &ccirc;iuj, kiuj a&ubreve;dis lin, <tld/>egis pro lia kompreno kaj liaj
        respondoj
        <fnt><bib>NT</bib>, <lok>&Luk; 2:47</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.mirigi">frapi <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>velmi se divit</trd>,
    <trd>žasnout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be stupefied</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>&eacute;tonner</ind></trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>veľmi obdivovať niečo</trd>,
    <trd>vytreštiť oči</trd>,
    <trd>žasnúť nad niečím</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Tre forta <tld/>o:
      <ekz>
        &ccirc;io tio &ccirc;i por ok dolaroj semajne! Mi esprimis mian <tld/>egon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la
        metiisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>údiv</trd>,
    <trd>úžas</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stupefaction</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tonnement</trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">úžas</trd>
</drv>

<drv mrk="mir.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on al iu:
      <ekz>
        &ccirc;iuj estis senfine <tld/>igitaj, la instruitulo e&ccirc; eksaltis
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>budit podiv</trd>,
    <trd>udivit</trd>,
    <trd>udivovat</trd>
  </trdgrp>
  <trd lng="de">verwundern</trd>
  <trdgrp lng="en">
    <trd>amaze</trd>,
    <trd>astonish</trd>
  </trdgrp>
  <trd lng="fr">&eacute;merveiller</trd>
  <trdgrp lng="hu">
    <trd>csod&aacute;lkoz&aacute;sra k&eacute;sztet</trd>,
    <trd>&aacute;mulatba ejt</trd>
  </trdgrp>
  <trd lng="nl">verwonderen</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prekvapiť</trd>,
    <trd>udiviť</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Meritanta <tld/>on:
      <ekz>
        pluraj amatoraj lingvistoj posedas en siaj tirkestoj
        <tld/>indajn
        projektojn
        <fnt><bib>Lanti</bib>, <lok>nia pozicio</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>inda&jcirc;oj de la scienco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mirakl">mirakla</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>obdivuhodný</trd>,
    <trd>podivuhodný</trd>,
    <trd>úžasný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wundersam</trd>,
    <trd>wunderbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wonderful</trd>,
    <trd>amazing</trd>,
    <trd>astonishing</trd>
  </trdgrp>
  <trd lng="fr">merveilleux</trd>
  <trd lng="hu">csod&aacute;latos</trd>
  <trd lng="nl">wonderlijk</trd>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">úžasný</trd>
</drv>
</art>
