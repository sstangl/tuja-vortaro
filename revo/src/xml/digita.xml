<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: digita.xml,v 1.21 2015/07/04 08:37:32 revo Exp $">
<kap>
<rad>digital</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="digita.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="skrofu1.0oj">skrofulariacoj</ref>
      (<trd lng="la">Digitalis</trd>).
      Tre venena planto, kies floroj similas formon de gantofingro.
      La ekstraktitaj substancoj estas uzataj en medicino por influi
      la korbatojn.
    </dif>
    <ref tip="vid" cel="digita1.0o">digitalino</ref>
  </snc>
  <bld lok="../bld/digita.jpg">
    <ind>ru&gcirc;a <tld/>o</ind>
    <trd lng="la">Digitalis purpurea</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l; &a_A;&a_n;&f_g;&a_sh;&a_t;&a_A;&a_n;&a_h;</trd>,
      <trd>&a_d;&f_y;&f_zh;&f_y;&a_t;&a_A;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>piros <ind>gy&udblac;sz&udblac;vir&aacute;g</ind></trd>,
      <trd>pettyegetett
      <ind>gy&udblac;sz&udblac;vir&aacute;g</ind></trd>
  </trdgrp>
  </bld>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_je;&c_r;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_y;&c_g;&c_ib;&c_t;&c_a;&c_l;&c_ib;&c_s;</trd>
  </trdgrp>
  <trd lng="de">Fingerhut</trd>
  <trd lng="en">foxglove</trd>
  <trd lng="fa">&f_g;&a_l; &a_A;&a_n;&f_g;&a_sh;&a_t;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">digitale</trd>
  <trd lng="hu">gy&udblac;sz&udblac;vir&aacute;g</trd>
  <trd lng="pl">naparstnica</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_je;&c_r;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_i;&c_g;&c_i;&c_t;&c_a;&c_l;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">náprstník</trd>
  <trdgrp lng="cs">
    <trd>digitális</trd>,
    <trd>náprstník</trd>
  </trdgrp>
</drv>
</art>
