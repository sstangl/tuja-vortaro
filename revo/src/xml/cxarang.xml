<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxarang.xml,v 1.3 2013/09/08 18:11:18 revo Exp $">
<kap>
  <rad>&ccirc;arang</rad>/o
</kap>
<drv mrk="cxarang.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxarang.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sudamerika
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      derivita de akut-sonaj gitaroj, kies sonkesto origine konsistis
      el kiraso de dazipo:
      <ekz>
        <tld/>o, jam uzata en la 18-a jarcento en la boliviaj Andoj 
        <klr>...</klr>, estas malgranda, iom simila al hispana liuto,
        sed kun malpli da kordoj
        <fnt>
          <aut>A. Orellana Rojas</aut>,
          <vrk><url ref="http://www.epa.jp/NV/2005/jun2005.htm">
          Ameriko kaj &gcirc;ia muziko</url></vrk>,
          <lok>Nova Vojo, 2005-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o longas &ccirc;. 66 cm
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/%C4%88arango">
          &ccirc;arango</url>, 2011-01-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Charango</trd>
  <trd lng="fr">charango</trd>
</drv>
</art>

