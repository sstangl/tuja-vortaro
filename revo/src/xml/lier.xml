<vortaro>
<art mrk="$Id: lier.xml,v 1.6 2016/04/22 17:10:19 revo Exp $">
<kap>
<rad>Lier</rad>/o
</kap>

<drv mrk="lier.L0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la belga provinco 
      <ref tip="malprt" cel="antverp.A0o.prov">Antverpeno</ref>, en
      norda 
      Belgujo (4&deg;04&minute;E, 51&deg;07&minute;N).
    </dif>
  </snc>
  <trd lng="de">Lier<klr> (Belgien)</klr></trd>
  <trd lng="fr">Lierre</trd>
  <trd lng="hu">Lier</trd>
  <trd lng="nl">Lier</trd>
  <trd lng="pt">Lierre</trd>
</drv>
</art>

