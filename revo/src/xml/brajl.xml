<vortaro>
<art mrk="$Id: brajl.xml,v 1.15 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <rad>brajl</rad>/o
  </kap>
  <drv mrk="brajl.0o">
    <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">LIN</uzo>
      <dif>
        Speciala skribo por blinduloj, elpensita de Braille (Braj).  
      </dif>
    </snc>
  <trdgrp lng="be">
      <trd>&c_sh;&c_r;&c_y;&c_f;&c_t;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>,
      <trd>&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>,
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>
    </trdgrp>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_j;&c_l;&c_o;&c_v;&c_a; &c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
      <trd>braille</trd>,
      <trd>sistema braille</trd>
    </trdgrp>
  <trd lng="cs">Braillovo slepecké písmo</trd>
  <trd lng="de">Braille-Schrift</trd>
  <trd lng="en">braille</trd>
  <trd lng="fr">braille</trd>
  <trdgrp lng="hu">
      <trd>Braille-&iacute;r&aacute;s</trd>,
      <trd>vak&iacute;r&aacute;s</trd>
    </trdgrp>
  <trdgrp lng="nl">
      <trd>braille</trd>,
      <trd>blindenschrift</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>braill</trd>,
      <trd lng="pl">pismo Braill'a</trd>,
      <trd>pismo wypuk&lstroke;e</trd>,
      <trd>pismo dla niewidomych</trd>
    </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o; &c_d;&c_l;&c_ja;
    &c_s;&c_l;&c_je;&c_p;&c_y;&c_h; &c_p;&c_o;
    &c_m;&c_je;&c_t;&c_o;&c_d;&c_u;
    &c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>Braillovo p&iacute;smo</trd>,
    <trd>slepecké písmo Brailovo</trd>
  </trdgrp>
</drv>
</art>
