<kap>
  <rad>Renat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="renat.0o">
  <kap><tld/>o</kap>
  <snc mrk="renat.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o Korseti, nuna prezidanto de UEA (Universala
        Esperanto-Asocio) babilos kun la esperantistoj en la babilejo de
        &Gcirc;angalo
        <fnt>
        <aut>Esperanto-Brasil</aut>,
        <vrk>Rendevuo kun Renato Korseti vive en &Gcirc;angalo-Babilejo</vrk>,
        <lok>soc.culture.esperanto, 2003-09-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Ren&eacute;</trd>
  <trd lng="it">Renato</trd>
  <trd lng="pt">Renato</trd>
  <trdgrp lng="ru">
    <trd>&c_R;&c_je;&c_n;&c_a;&c_t;&c_o;</trd>,
    <trd>&c_R;&c_je;&c_n;&c_je;</trd>
  </trdgrp>
</drv>
</art>

