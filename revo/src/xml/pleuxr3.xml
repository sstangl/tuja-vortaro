<kap>
  <rad>ple&ubreve;ronekted</rad>/oj
</kap>
<drv mrk="pleuxr3.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="pleuxr2.0oformaj">ple&ubreve;ronektoformaj
      fi&scirc;oj</ref> (<trd lng="la">Pleuronectidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_o;&c_v;&c_y;&c_ja;<klr tip="amb">
  (&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">pleuron&egrave;ctids</trd>
  <trd lng="de">Schollen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">f&eacute;lszeg&uacute;sz&oacute;k</trd>
  <trd lng="nl">schollen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">pleuronect&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_b;&c_a;&c_l;&c_o;&c_v;&c_y;&c_je;<klr tip="amb">
  (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

