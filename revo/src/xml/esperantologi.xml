<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: esperantologi.xml,v 1.8 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>esperantologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="esperantologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="esperantologi.0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Studo pri Esperanto kaj la esperanta movado, parto de <ref
      tip="malprt"
      cel="lingv.plan0oscienco.LIN">planlingvoscienco</ref>:
      <ekz>
        <ctl><tld lit="E"/>o</ctl>
        <klr>(gazeto eldonita ekde 1949)</klr>;
      </ekz>
      <ekz>
        &ccirc;iujare CED organizas la <tld lit="E"/>an Konferencon en
        la 
        Universalaj Kongresoj
        <fnt>
          <aut>M. Fettes</aut>,
          <vrk><url ref="http://esperantic.org/ced/">
          Centro por Esploro kaj Dokumentado pri la Monda
          Lingvo-Problemo</url></vrk>,
          <lok>2005-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">esperantologia</trd>
  <trd lng="cs">esperantologie</trd>
  <trd lng="de">Esperantologie</trd>
  <trd lng="fr">esp&eacute;rantologie</trd>
  <trd lng="it">esperantologia</trd>
  <trd lng="pl">esperantologia</trd>
  <trd lng="pt">esperantologia</trd>
  <trd lng="sk">esperantológia</trd>
</drv>

<drv mrk="esperantologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="esperantologi.0isto.ESP">
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">esperant&ograve;leg</trd>
  <trd lng="cs">esperantolog</trd>
  <trd lng="de">Esperantologe</trd>
  <trd lng="fr">esp&eacute;rantologiste</trd>
  <trd lng="pl">esperantologista</trd>
  <trd lng="pt">esperant&oacute;logo</trd>
  <trd lng="sk">esperantológ</trd>
</drv>
</art>

