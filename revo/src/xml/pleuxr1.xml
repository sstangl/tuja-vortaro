<art mrk="$Id: pleuxr1.xml,v 1.13 2014/11/19 07:10:38 revo Exp $">
<kap>
  <rad>ple&ubreve;rit</rad>/o
</kap>
<drv mrk="pleuxr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref tip="vid" cel="pleuxr.0o">ple&ubreve;ro</ref>:
      <ekz>
        akuta, senakva a&ubreve; seka, pusa <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_w;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Pleuritis</trd>,
    <trd>Rippenfellentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="fa">&a_dh;&a_A;&a_t;&zwnj;&a_A;&a_l;&a_j;&a_n;&a_b;</trd>
  <trd lng="fr">pleur&eacute;sie</trd>
  <trdgrp lng="hu">
    <trd>mellh&aacute;rtyagyullad&aacute;s</trd>,
    <trd>pleuritis</trd>
  </trdgrp>
  <trd lng="id">pleuritis</trd>
  <trdgrp lng="nl">
    <trd>pleuritis</trd>,
    <trd>borstvliesontsteking</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pleurisia</trd>,
    <trd>pleurite</trd>,
    <trd>pleuris</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_v;&c_r;&c_i;&c_t;</trd>
</drv>
</art>
