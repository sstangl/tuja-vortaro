<vortaro>
<art mrk="$Id: piz.xml,v 1.23 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>piz</rad>/o
</kap>
<drv mrk="piz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piz.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
       <ref tip="lst" cel="legom.0o.KUI" 
       lst="voko:faboparencoj">Legomplanto</ref> 
       kun globformaj verdaj semgrajnoj;
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">genro</ref> el familio <ref
       tip="malprt" cel="fabac.0oj">fabacoj</ref>
       (<trd lng="la">Pisum</trd>).
    </dif>
    <trd lng="be">&c_g;&c_a;&c_r;&c_o;&c_h;</trd>
    <trd lng="ca">pesolera</trd>
    <trd lng="hu">bors&oacute;</trd>
    <trdgrp lng="id">
      <trd>ercis</trd>,
      <trd>polong</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_h;</trd>
  </snc>
  <snc mrk="piz.0o.grajno">
    <dif>
      <tld lit="P"/>osemo, la grajno mem de <tld/>o:
      <ekz>
        mi sidis en la sunlumo por sen&scirc;eligi <tld/>ojn
        <fnt><aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>, 
        <lok>FEL, 1999</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_r;&c_o;&c_sh;&c_y;&c_n;&c_a;</trd>
    <trd lng="ca">p&egrave;sol</trd>
    <trd lng="hu">bors&oacute;szem</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_sh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_h;</trd>
  <trd lng="cs">hr√°ch</trd>
  <trd lng="de">Erbse</trd>
  <trd lng="en">green <ind>pea</ind></trd>
  <trd lng="fr">petit <ind>pois</ind></trd>
  <trd lng="nl">erwt</trd>
  <trd lng="pl">groch</trd>
  <trd lng="pt">ervilha</trd>
  <trd lng="sk">hrach <klr>(rastlina)</klr></trd>
</drv>
</art>
