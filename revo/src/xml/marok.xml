<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marok.xml,v 1.14 2014/06/04 16:10:46 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Marok</rad>/o
</kap>

<drv mrk="marok.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="marok.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> en
      la 
      nordokcidenta Afriko kun &ccirc;efurbo Rabato.
      Teritorio de <em>Regno Maroko</em>.
      ISO-3166-1 kodo: MA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      Okcidenta Saharo (asertas &gcirc;in kiel propra&jcirc;o),
      <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteraneo</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>:
      <ekz>
        la onklino el <tld/>o
        <fnt><vrk><url>http://kantaro.ikso.net/onklino_el_maroko</url></vrk></fnt>
      </ekz>
    </dif>
    <trdgrp lng="ar">
      <trd>&a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;</trd>
      <trd>&a_A;&a_l;&a_m;&a_m;&a_l;&a_k;&a_t_marbuta; &a_A;&a_l;&a_m;&a_gh;&a_r;&a_b;&a_y;&a_t_marbuta;&lrm;</trd>
    </trdgrp>
    <trd lng="be">&c_M;&c_a;&c_r;&c_o;&c_k;&c_a;</trd>
    <trd lng="bg">&c_M;&c_a;&c_r;&c_o;&c_k;&c_o;</trd>
    <trdgrp lng="ca">
      <trd>el <ind>Marroc</ind></trd>,
      <trd>Regne del <ind>Marroc</ind></trd>
    </trdgrp>
    <trd lng="de">Marokko</trd>
    <trd lng="en">Morocco</trd>
    <trd lng="fr">Maroc</trd>
    <trd lng="fa">&a_m;&a_r;&a_A;&f_k;&a_sh;</trd>
    <trd lng="id">Maroko</trd>
    <trd lng="it">Marocco</trd>
    <trd lng="nl">Marokko</trd>
    <trdgrp lng="pl">
      <trd>Maroko</trd>,
      <trd>Kr&oacute;lestwo <ind>Maroka</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

