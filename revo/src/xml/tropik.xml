<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tropik.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tropik</rad>/o
</kap>
<drv mrk="tropik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOD</uzo>
    <dif>
      &Ccirc;iu el la du paraleloj, kiuj estas je la distanco de
      23&#730; 27' 27" de
      la ekvatoro.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_o;&c_p;&c_ib;&c_k;</trd>
  <trd lng="cs">obratník</trd>
  <trd lng="de">Wendekreis</trd>
  <trd lng="en">tropic</trd>
  <trd lng="es">tr&oacute;pico</trd>
  <trd lng="fr">tropique</trd>
  <trdgrp lng="hu">
    <trd>tr&oacute;pus</trd>,
    <trd>t&eacute;r&iacute;t&odblac;</trd>
  </trdgrp>
  <trd lng="nl">keerkring</trd>
  <trd lng="pl">zwrotnik</trd>
  <trd lng="pt">tr&oacute;pico</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_p;&c_i;&c_k;</trd>
  <trd lng="sk">obratník</trd>
</drv>

<drv mrk="tropik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la inter<tld/>a regiono:
      <ekz>
        <tld/>a varmego, klimato, vegeta&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>obratníkový</trd>,
    <trd>tropický</trd>
  </trdgrp>
  <trd lng="de">tropisch</trd>
  <trd lng="en">tropical</trd>
  <trd lng="es">tropical</trd>
  <trd lng="fr">tropical</trd>
  <trd lng="hu">tr&oacute;pusi</trd>
  <trd lng="nl">tropisch</trd>
  <trdgrp lng="pl">
    <trd>mi&eogonek;dzyzwrotnikowy</trd>,
    <trd>tropikalny</trd>
  </trdgrp>
  <trd lng="pt">tropical</trd>
  <trd
  lng="ru">&c_t;&c_r;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">tropický</trd>
</drv>
</art>
