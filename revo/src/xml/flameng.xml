<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flameng.xml,v 1.6 2015/06/29 16:29:38 revo Exp $">
<kap>
  <rad>flameng</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="flameng.0o">
  <kap><tld/>o, <var><kap>roza <tld/>o</kap></var></kap>
  <snc mrk="flameng.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="feniko.0o.ZOO">fenikoptero</ref>
      <ekz>
        vi malkovros la kastelojn de la katara lando, sed anka&ubreve; 
        <klr>...</klr> la rozajn <tld/>ojn, kiuj vivas en la lagunoj 
        de nia marbordo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://mescladis.free.fr/katara-lando.htm">
          Aude - la lando de la kataroj</url></vrk>,
          <lok>[vidita en 2007-03]</lok>
        </fnt>;
      </ekz>
      <ekz>
        plenkreskaj <tld/>oj havas rozkolorajn a&ubreve; ru&gcirc;ajn
        plumojn
        <fnt><bib>Viki</bib>, <lok><url
        ref="http://eo.wikipedia.org/wiki/Flamengo">artikolo
        &leftquot;flamengo&rightquot;</url>, 
        2008-01-12</lok></fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a;</trd>
  <trd lng="bg">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="cs">plame&ncaron;&aacute;k</trd>
  <trd lng="de">Flamingo</trd>
  <trd lng="en"><klr>(greater) </klr>flamingo</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_m;&f_y;&a_n;&f_g;&a_w;</trd>
  <trd lng="fr">flamant</trd>
  <trd lng="hu">flaming&oacute;</trd>
  <trd lng="nl">flamingo</trd>
  <trdgrp lng="pl">
    <trd>czerwonak</trd>,
    <trd>flaming<klr> (r&oacute;&zdot;owy)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>flamingo</trd>,
    <trd>flamengo</trd>,
    <trd>fenic&oacute;ptero</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="sk">plameniak</trd>
  <trd lng="sv">flamingo</trd>
</drv>
</art>

