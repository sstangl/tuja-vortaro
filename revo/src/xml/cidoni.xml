<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cidoni.xml,v 1.26 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>cidoni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cidoni.0o">
  <kap><tld/>o</kap>
  <snc mrk="cidoni.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo, malmola, uzata ekzemple por konfita&jcirc;oj.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <trd lng="be">&c_a;&c_j;&c_v;&c_a; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_d;&c_ju;&c_l;&c_ja;</trd>
  <trd lng="br">aval-stoub</trd>
  <trdgrp lng="cs">
    <trd>cydónie <klr>(plod)</klr></trd>,
    <trd>kdoule</trd>
  </trdgrp>
  <trd lng="de">Quitte<klr tip="ind"> (Frucht)</klr></trd>
  <trd lng="el">&kappa;&ypsilon;&delta;&omega_ton;&ny;&jota;</trd>
  <trd lng="en">quince</trd>
  <trd lng="fr">coing</trd>
  <trd lng="hu">birsalma</trd>
  <trdgrp lng="it">
    <trd>mela <klr>cotogna</klr></trd>
    <trd>pera <klr>cotogna</klr></trd>
  </trdgrp>
  <trd lng="nl">kweepeer</trd>
  <trd lng="pl">pigwa<klr> (owoc)</klr></trd>
  <trd lng="pt">marmelo</trd>
  <trd lng="ru">&c_a;&c_j;&c_v;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">dula <klr>(plod)</klr></trd>
</drv>

<drv mrk="cidoni.0arbo">
  <kap><tld/>arbo,
  <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="cidoni.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo kun pir- a&ubreve; pomsimilaj flavaj lanug&scirc;elaj fruktoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Cydonia</trd>):
      <ekz>
        la pirujo estas multe pli facile greftebla
        sur <tld/>ujo <klr>...</klr>, ol sur pomujo
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_j;&c_v;&c_a;<klr tip="amb"> (&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_d;&c_ju;&c_l;&c_ja;</trd>
  <trd lng="br">avalenn-stoub</trd>
  <trdgrp lng="cs">
    <trd>cydónie</trd>,
    <trd>kdouloň obecná</trd>
  </trdgrp>
  <trd lng="de">Quitte<klr tip="ind"> (Baum)</klr></trd>
  <trd lng="el">&kappa;&ypsilon;&delta;&omega;&ny;&jota;&alfa_ton;</trd>
  <trd lng="en">quince</trd>
  <trd lng="fr">cognassier</trd>
  <trd lng="hu">birsalmafa</trd>
  <trd lng="it">cotogno</trd>
  <trd lng="nl">kweepeer</trd>
  <trd lng="pl">pigwa<klr> (drzewo)</klr></trd>
  <trd lng="pt">marmeleiro</trd>
  <trd lng="ru">&c_a;&c_j;&c_v;&c_a; <klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">dula <klr>(strom)</klr></trd>
</drv>
</art>
