<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: fosfit.xml,v 1.8 2007/07/01 16:30:38 revo Exp $">
    <kap>
      <rad>fosfit</rad>/o
    </kap>
    <drv mrk="fosfit.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="sal.0o.KEM">Salo</ref> de
          <tld/>a acido.
        </dif>
        <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_ib;&c_t;</trd>
        <trd lng="de">Phosphit</trd>
        <trd lng="fr">phosphite</trd>
        <trd lng="hu">foszfit</trd>
        <trd lng="nl">fosfiet</trd>
        <trd lng="pt">fosfito</trd>
        <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_i;&c_t;</trd>
      </snc>
    </drv>
    
    <drv mrk="fosfit.0aacido">
      <kap><tld/>a acido</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="acid.0o">Acido</ref> el
          <ref cel="fosfor.0o">fosforo</ref>:
          <frm>H<sub>3</sub>PO<sub>3</sub></frm>.
        </dif>
      </snc>
      <trd lng="be">&c_a;&c_r;&c_t;&c_a;&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
      <trd lng="de">Phosphons&auml;ure</trd>
      <trd lng="fr">acide phosphoreux</trd>
      <trdgrp lng="hu">
        <trd>foszforsav</trd>,
        <trd>ortofoszforsav</trd>
      </trdgrp>
      <trd lng="nl">fosforig zuur</trd>
      <trd lng="pt">&aacute;cido fosforoso</trd>
      <trd lng="ru">&c_o;&c_r;&c_t;&c_o;<ind>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</ind></trd>
    </drv>
  </art>

