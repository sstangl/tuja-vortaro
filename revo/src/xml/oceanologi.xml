<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oceanologi.xml,v 1.8 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>oceanologi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="oceanologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="oceanologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Marscienco studanta la maran medion, &gcirc;iajn fizikajn, &hcirc;emiajn kaj
      vivajn aspektojn:
      <ekz>
        la <tld/>a instituto en San Diego
        <fnt>
        <aut>D. Bindmann</aut>,
        <vrk>Re: biologia klasifiko</vrk>,
        <lok>soc.culture.esperanto, 1999-11-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">oceánologie</trd>
  <trd lng="de">Ozeanografie</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">oc&eacute;anologie</trd>
  <trd lng="nl">oceanologie</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">oceánológia</trd>
</drv>

<drv mrk="oceanologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="oceanologi.0isto.SCI">
    <dif>
      Marsciencisto, fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Ozeanograf</trd>
  <trd lng="fa">&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">oc&eacute;anologiste</trd>
  <trd lng="nl">oceanoloog</trd>
  <trd lng="pt">oceanologia</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>

