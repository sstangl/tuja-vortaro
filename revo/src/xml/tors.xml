<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tors.xml,v 1.19 2015/07/04 08:37:45 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>tors</rad>/o
</kap>
<drv mrk="tors.0o">
   <kap><tld/>o</kap>
   <snc>
     <uzo tip="fak">ANA</uzo>
     <dif>
       Supera parto de la homa korpo sen la kapo kaj sen la brakoj,
       konsiderata aparte de la cetera korpo;
       <ref tip="sin" cel="trunk.0o.ANA">trunko
       <sncref ref="trunk.0o.ANA"/></ref>.
     </dif>
     <trdgrp lng="en">
       <trd>torso</trd>,
       <trd>trunk</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>t&ouml;rzs</trd>,
       <trd>fels&odblac; <ind>test</ind></trd>
     </trdgrp>
     <trd lng="pl">tors</trd>
     <trdgrp lng="pt">
       <trd>torso</trd>,
       <trd>tronco</trd>
     </trdgrp>
   </snc>
   <snc>
     <uzo tip="fak">BELA</uzo>
     <dif>
       Skulpta&jcirc;o prezentanta nur tiun parton.
     </dif>
     <refgrp tip="sin">
       <ref cel="bust.0o">busto</ref>.
     </refgrp>
     <trd lng="en">torso</trd>
     <trd lng="hu">torz&oacute;</trd>
    </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_s;</trd>
  <trd lng="ca">tors</trd>
  <trdgrp lng="cs">
    <trd>torzo</trd>,
    <trd>trup sochy</trd>
  </trdgrp>
  <trd lng="de">Torso</trd>
  <trd lng="fr">torse</trd>
  <trdgrp lng="it">
     <trd>torso</trd>,
     <trd>busto</trd>
   </trdgrp>
  <trd lng="nl">romp</trd>
  <trd lng="pl">popiersie</trd>
  <trdgrp lng="pt">
    <trd>torso</trd>,
    <trd>tronco</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_s;</trd>
  <trd lng="sk">poprsie s ramenami</trd>
</drv>
</art>
