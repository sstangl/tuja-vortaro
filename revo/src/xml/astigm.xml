<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: astigm.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap><rad>astigmat</rad>/a</kap>

<drv mrk="astigm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>econ a&ubreve; <tld/>ismon.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">astigmatický</trd>
  <trd lng="de">astigmatisch</trd>
  <trd lng="en">astigmatic</trd>
  <trd lng="es">astigm&aacute;tico</trd>
  <trdgrp lng="fr">
    <trd>astigmatique</trd>,
    <trd>astigmate</trd>
  </trdgrp>
  <trd lng="hu">asztigmatikus</trd>
  <trd lng="it">astigmatico</trd>
  <trd lng="nl">astigmatisch</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">astigmatický</trd>
</drv>

<drv mrk="astigm.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Aberacio de optika sistemo, en kiu la bilda fokuso estas
      anstata&ubreve;ata
      de du fokusaj strekoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">astigmatičnost</trd>
  <trd lng="de">Astigmatismus</trd>
  <trd lng="en">astigmatism</trd>
  <trd lng="es">astigmatismo</trd>
  <trd lng="fr">astigmatisme</trd>
  <trd lng="hu">asztigmatizmus</trd>
  <trd lng="it">astigmatismo</trd>
  <trd lng="nl">astigmatisme</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">astigmatickosť</trd>
</drv>

<drv mrk="astigm.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vida malneteco pro malegaleco de okullensa kurbi&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">astigmatizmus</trd>
  <trd lng="de">Astigmatismus</trd>
  <trd lng="en">astigmatism</trd>
  <trd lng="es">astigmatismo</trd>
  <trd lng="fr">astigmatisme</trd>
  <trd lng="hu">asztigmatizmus</trd>
  <trd lng="it">astigmatismo</trd>
  <trd lng="nl">astigmatisme</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">astigmatizmus</trd>
</drv>
</art>
