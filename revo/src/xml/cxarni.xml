<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxarni.xml,v 1.16 2015/10/06 00:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;arnir</rad>/o
</kap>
<drv mrk="cxarni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxarni.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kuna&jcirc;o de du pecoj moveblaj &ccirc;irka&ubreve; unu komuna akso:
      <ekz>
        la Sor&ccirc;isto levis tiujn lignajn pordojn de la <tld/>oj
        <fnt><bib>DOz</bib>, <lok>&leftquot;la kavo de la 
draketoj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="artik.0o">artiko</ref>,
      <ref cel="pord.0hoko">pordhoko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_r;&c_n;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_t;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>xarnera</trd>,
    <trd>articulaci&oacute;</trd>
  </trdgrp>
  <trd lng="de">Scharnier</trd>
  <trd lng="en">hinge</trd>
  <trd lng="fa">&a_l;&a_w;&a_l;&a_A;</trd>
  <trd lng="fr">charni&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>csukl&oacute;p&aacute;nt</trd>,
    <trd>zsan&eacute;r</trd>
  </trdgrp>
  <trd lng="it">cerniera<klr> (snodo)</klr></trd>
  <trd lng="nl">scharnier</trd>
  <trdgrp lng="pl">
    <trd>przegub</trd>,
    <trd>zawiasa<klr> {f.}</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>charneira</trd>,
    <trd>dobradi&ccedil;a</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_n;&c_i;&c_r;</trd>
</drv>
</art>
