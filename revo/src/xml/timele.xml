<kap>
  <rad>timeleac</rad>/oj
</kap>
<drv mrk="timele.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Thymelaeaceae</trd>).
    </dif>
  </snc>
  <trd lng="ca">timele&agrave;cies</trd>
  <trd lng="de">Seidelbastgew&auml;chse</trd>
  <trd lng="hu">boroszl&aacute;nf&eacute;l&eacute;k</trd>
  <trd lng="nl">peperboompjesfamilie</trd>
  <trdgrp lng="pt">
    <trd>timele&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_g;&c_o;&c_d;&c_k;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_v;&c_o;&c_l;&c_ch;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
