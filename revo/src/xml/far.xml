  <kap>neni<tld/>o, <var><kap>nenio<tld/>o</kap></var></kap>
  <snc mrk="far.neni0o.KOMUNE">
    <dif>
      Senageco;
      <ref tip="sin" cel="farnient.0o.KOMUNE">farniento</ref>:
      <ekz>
        plej ver&scirc;ajne li dedi&ccirc;as siajn tagojn al nenio<tld/>o, kiel la 
        aliaj dibo&ccirc;uloj
        <fnt><bib>KielAkvo</bib>, <lok>3:1</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sid.forno0a.hejmama">fornosida</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>nečinnost</trd>,
    <trd>nicnedělání</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inaction</trd>,
    <trd>inactivit&eacute;</trd>,
    <trd>oisivet&eacute;</trd>
  </trdgrp>
  <trd lng="sk">nečinnosť</trd>
</drv>

<drv mrk="far.sen0a">
  <kap>sen<tld/>a<fnt>Z</fnt></kap>
  <snc mrk="far.sen0a.KOMUNE">
    <dif>
      Neni<tld/>anta:
      <ekz>
        Dion fidu, sed sen<tld/>e ne sidu<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        resti <ind>sen<tld/>e</ind><fnt>Z</fnt>.
        <trdgrp lng="ru">
          <trd>&c_b;&c_je;&c_z; &c_d;&c_je;&c_l;&c_a;</trd>,
          <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>líný</trd>,
    <trd>povalečný</trd>
  </trdgrp>
  <trd lng="de">unt&auml;tig</trd>
  <trdgrp lng="fr">
    <trd>inactif</trd>,
    <trd>oisif</trd>
  </trdgrp>
  <trd lng="hu">t&eacute;tlen</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_z;&c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">lenivý</trd>
</drv>
