<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: far.xml,v 1.40 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>far</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="far.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="far.0i.estigi">
    <dif>
      Estigi, <ref tip="vid" cel="kre.0i.KOMUNE">krei</ref>.
    </dif>
    <subsnc mrk="far.0i.elMaterio">
      <dif>
        Estigi objekton, eligante &gcirc;in el materio:
        <ekz>
          la ligna&jcirc;isto <tld/>as tablojn
          <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>i ma&scirc;inon, paperon;
        </ekz>
        <ekz>
          bieron, statuon,
          &scirc;losilon, domon, muron, vojon.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="fabrik.0i">fabriki</ref>,
        <ref cel="pret.0igi">pretigi</ref>,
        <ref cel="produk.0i">produkti</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="far.0i.verkon">
      <dif>
        Estigi verkon eligante &gcirc;in el spirito; doni formon:
        <ekz>
          <tld/>i verkon, romanon, versojn, tragedion, tradukon
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="kompil.0i">kompili</ref>,
        <ref cel="kompon.0i">komponi</ref>,
        <ref cel="verk.0i">verki</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="far.0i.elNenio">
      <uzo tip="fak">BIB</uzo>
      <dif>
        Estigi ion, eligante &gcirc;in el nenio:
        <ekz>
          kaj Dio <tld/>is la du grandajn luma&jcirc;ojn kaj la stelojn
          <fnt><bib>MT</bib>, <lok>&Gen; 1:16</lok></fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="kre.0i.REL">krei<sncref/></ref>
    </subsnc>
  </snc>
  <snc mrk="far.0i.EnStaton">
    <dif>
      <ref tip="vid" cel="ig.0i">Igi</ref>,
      doni ian econ al iu a&ubreve; io,
      pasigi de iu stato en alian:
      <ekz>
        oni <tld/>is lin generalo;
      </ekz>
      <ekz>
        tio <tld/>is lin &gcirc;oja;
      </ekz>
      <ekz>
        <tld/>i ilin mokata&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Jer; 25:18</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>os el viaj urboj dezerton
        <fnt><bib>MT</bib>, <lok>&Lev; 26:31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i al si oportune<fnt>Z</fnt>
        <klr>(komforte kaj senceremonie sin aran&gcirc;i)</klr>;
      </ekz>
      <ekz>
        el la neceseco oni devas <tld/>i virton
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="far.0i.iel_agi">
    <dif>
      <ref tip="vid" cel="ag.0i">Agi</ref>.
    </dif>
    <subsnc mrk="far.0i.plenumi">
      <dif>
        Plenumi agon; estigi efikon; okazigi rezulton:
        <ekz>
          li tuj <tld/>is, kion mi volis
          <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
        </ekz>
        <ekz>
          mia vizito faris al li plezuron
          <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>i siajn
          klasikajn studojn, klasojn, sian taskon, sian devon;
        </ekz>
        <ekz>
          <tld/>i kurson, paroladon, &scirc;telon<fnt>Z</fnt>,
          rabon<fnt>Z</fnt>, ven&gcirc;on<fnt>Z</fnt>,
          krimon, atakon, meson, miraklon;
        </ekz>
        <ekz>
          <tld/>i sukcesan kuracadon, profitan aferon;
        </ekz>
        <ekz>
          <tld/>i komercon, metion;
        </ekz>
        <ekz>
          <tld/>i feston<fnt>Z</fnt>, Paskon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          havi multon <tld/>otan <klr>(por <tld/>i)</klr>;
        </ekz>
        <ekz>
          <ind>dirite, <tld/>ite</ind>
          <trd lng="fr">aussit&ocirc;t dit, aussit&ocirc;t fait</trd>
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          kontra&ubreve; <tld/>o <tld/>ita ne helpas
          medito<fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <tld/>i ordon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          tio nenion <tld/>as<fnt>Z</fnt>
          <klr>(estas indiferenta)</klr>;
        </ekz>
        <ekz>
          <tld/>i makulon<fnt>Z</fnt>,
        </ekz>
        <ekz>
          rondon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>i <klr>(formi)</klr> golfeton;
        </ekz>
        <ekz>
          <tld/>i vundon al iu, difekton al io;
        </ekz>
        <ekz>
          <tld/>u al li nenion malbonan
          <fnt><bib>MT</bib>, <lok>&Jer; 39:112</lok></fnt>
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <tld/>i al si la vojon
          (<ref tip="vid" cel="bat.tra0i">trabati</ref>);
        </ekz>
        <ekz>
          <tld/>i doloron, timon<fnt>Z</fnt>, embarason,
          favora&jcirc;on, fidela&jcirc;on<fnt>Z</fnt>,
          perforta&jcirc;on<fnt>Z</fnt>, maljusta&jcirc;on, petola&jcirc;on;
        </ekz>
        <ekz>
          <tld/>i honoron, komplezon, surprizon;
        </ekz>
        <ekz>
          <tld/>i eraron, diferencon, distingon inter;
        </ekz>
        <ekz>
          ka&scirc;u malbenon kaj <tld/>u bonan mienon
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <tld/>i <klr>(prezenti)</klr> demandon;
        </ekz>
        <ekz>
          <tld/>i <klr>(alpreni)</klr> decidon;
        </ekz>
        <ekz>
          <tld/>i <klr>(konduki)</klr> militon;
        </ekz>
        <ekz>
          <tld/>i <klr>(eltiri)</klr> konkludon;
        </ekz>
        <ekz>
          <tld/>i <klr>(plenumi)</klr> servon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>i <klr>(interkonsenti)</klr> kontrakton, pacon;
        </ekz>
        <ekz>
          <tld/>i <klr>(ekbruligi)</klr> fajron, lumon;
        </ekz>
        <ekz>
          kvin kaj sep <tld/>as dek du
          <klr>(kvin kaj sep rezultigas dek du)</klr>
          <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
        </ekz>
        <ekz>
          du botoj <tld/>as paron
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          sesdek minutoj <tld/>as unu horon
          <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="efekti.0igi">efektivigi</ref>,
        <ref cel="efik.0i">efiki</ref>,
        <ref cel="form.0i">formi</ref>,
        <ref cel="kauxz.0i">ka&ubreve;zi</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="far.0i.agiIamaniere">
      <dif>
        Agi iamaniere:
        <ekz>
          <tld/>u la&ubreve; via pla&ccirc;o;
        </ekz>
        <ekz>
          <tld/>u al ili <klr>(agu pri ili)</klr> kiel vi volas;
        </ekz>
        <ekz>
          mi ne sciis kion <tld/>i pri li, pri &gcirc;i
          <klr>(kiamaniere agi)</klr>;
        </ekz>
        <ekz>
          mi ne <tld/>is la&ubreve; mia koro
          <fnt><bib>MT</bib>, <lok>&Nom; 16:28</lok></fnt>
          (<ref tip="vid" cel="kondut.0i">konduti</ref>);
        </ekz>
        <ekz>
          pento<tld/>i
          <fnt><bib>MT</bib>, <lok>&Dan; 10:12</lok></fnt>
          <klr>(penti)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="far.0i.agi">
      <gra><vspec>senkomplemente</vspec></gra>
      <dif>
        Agi, ne resti senfara:
        <ekz>
          kiu multe parolas, ne multe <tld/>as
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <ind>kritiki estas facile, <tld/>i malfacile</ind>
          <trd lng="fr">la <ind>critique</ind> est facile, l'art est
          difficile</trd>
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          ne parolu, sed <tld/>u;
        </ekz>
        <ekz>
          li <tld/>as nenion;
        </ekz>
        <ekz>
          nenion<tld/>anto;
        </ekz>
        <ekz>
          neni<tld/>anto.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;lat</trd>,
    <trd>dělat</trd>,
    <trd>nadělat</trd>,
    <trd>provádět</trd>,
    <trd>provést</trd>,
    <trd>páchat</trd>,
    <trd>spáchat</trd>,
    <trd>tropit</trd>,
    <trd>tvořit</trd>,
    <trd>udělat</trd>,
    <trd>vykonat</trd>,
    <trd>vykonat</trd>,
    <trd>vykonávat</trd>,
    <trd>vytvořit</trd>,
    <trd>zastávat</trd>,
    <trd>zhotovit</trd>,
    <trd>činit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>machen</trd>,
    <trd>erschaffen</trd>,
    <trd>anfertigen</trd>,
    <trd>herstellen</trd>,
    <trd>tun</trd>,
    <trd>vollbringen</trd>
  </trdgrp>
  <trd lng="en">make</trd>
  <trd lng="es">hacer</trd>
  <trdgrp lng="fr">
    <trd>faire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csin&aacute;l</trd>,
    <trd>tesz</trd>,
    <trd>cselekszik</trd>,
    <trd>l&eacute;trehoz</trd>,
    <trd>alkot</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>buat</ind>] membuat</trd>,
    <trd>[<ind>laku</ind>] melakukan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doen</trd>,
    <trd>maken</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fazer</trd>,
    <trd>cometer</trd>,
    <trd>confeccionar</trd>,
    <trd>executar</trd>,
    <trd>formar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konať</trd>,
    <trd>páchať</trd>,
    <trd>robiť</trd>,
    <trd>urobiť</trd>
  </trdgrp>
  <trd lng="vo">mek&ouml;n</trd>
</drv>

<drv mrk="far.0">
  <kap><tld/></kap>
  <uzo tip="stl">NEO</uzo>
  <gra><vspec>prepozicio</vspec></gra>
  <snc mrk="far.0.KOMUNE">
  <uzo tip="stl">EVI</uzo>
    <refgrp tip="dif">
      <ref cel="de.0.aganto">de<sncref/></ref>
    </refgrp>
    <refgrp tip="sin">
      <ref cel="far.0ede"><tld/>e de</ref>,
      <ref cel="flank.0ede.far">flanke de<sncref/></ref>
    </refgrp>
    <ekz>
      rezisti al la malestimo <tld/> la homoj
      <fnt><bib>Revadoj</bib>, <lok>Dua promenado</lok></fnt>;
    </ekz>
    <ekz>
      granda urbo preska&ubreve; tute forbalaita <tld/> titana kataklismo
      <fnt>
      <bib>Voja&gcirc;o</bib>,
      <aut>M. Nervi</aut>,
      <vrk>Tertremo</vrk>,
      <lok>leciono 24a</lok>
      </fnt>;
    </ekz>
    <ekz>
      sen la forta tenado <tld/> mia patrino, &scirc;i estus 
      falinta teren
      <fnt>
        <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
        <vrk>Montara Vila&gcirc;o</vrk>,
        <lok>1984</lok>
      </fnt>.
    </ekz>
    <rim mrk="far.0.RIM">
      En la Universala Vortaro la radiko <em><tld/>'</em> fini&gcirc;as per
      streketo.
      Tio signifas, ke &gcirc;i nepre postulas fina&jcirc;on a&ubreve;, rolante kiel
      substantivo, apostrofon: &gcirc;i do ne povas esti uzata kiel nuda
      radiko,
      &ccirc;ar reguloj, kiuj povus pravigi la forfalon de la fina&jcirc;o, ne
      ekzistas.
    </rim>
    <rim mrk="far.0.RIM2">
      La sama rimarko tamen validas pri ekzemple <em>paf'</em>.
      Se la fina apostrofo signifas, ke &gcirc;i postulas fina&jcirc;on,
      tiam la sonimito <em>paf</em>, kiu ne rolas substantive
      sed adverbe, estas same kontra&ubreve;fundamenta
      kiel la prepozicio <em>far</em>.
      <aut>&jcirc;v</aut>
    </rim>    
  </snc>
  <trdgrp lng="fr">
    <trd>de<klr> (compl&eacute;ment d'agent)</klr></trd>,
    <trd>par<klr> (compl&eacute;ment d'agent)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="far.0o">
  <kap><tld/>o</kap>
  <snc mrk="far.0o.KOMUNE">
    <dif>
      Tio, kion oni <tld/>as:
      <ekz>
        ju&gcirc;u la&ubreve; <tld/>oj, ne la&ubreve; paroloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        inter diro kaj <tld/>o estas maro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pli da bruo ol da <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        lupo &scirc;an&gcirc;as la harojn <klr>(mienon)</klr> sed ne la
        <tld/>ojn
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>skutek</trd>,
    <trd>výkon</trd>,
    <trd>úkon</trd>,
    <trd>čin</trd>
  </trdgrp>
  <trd lng="de">Tat</trd>
  <trdgrp lng="fr">
    <trd>action</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tett</trd>,
    <trd>cselekedet</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>daad</trd>,
    <trd>handeling</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>skutok</trd>,
    <trd>výkon</trd>,
    <trd>čin</trd>
  </trdgrp>
</drv>

<drv mrk="far.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="far.0ado.KOMUNE">
    <dif>
      Da&ubreve;ra produktado:
      <ekz>
        <tld/>ado de mebloj;
      </ekz>
      <ekz>
        vin<tld/>ado, vort<tld/>ado, vers<tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dělání</trd>,
    <trd>konání</trd>
  </trdgrp>
  <trd lng="de">Herstellung</trd>
  <trdgrp lng="fr">
    <trd>fabrication</trd>,
    <trd>production</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csin&aacute;l&aacute;s</trd>,
    <trd>cselekv&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>doen</ind></trd>,
    <trd>het <ind>maken</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konanie</trd>,
    <trd>robenie</trd>
  </trdgrp>
</drv>

<drv mrk="far.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt>,
  <var><kap><tld/>ita&jcirc;o<fnt>Z</fnt></kap></var></kap>
  <snc>
    <dif>
      Tio, kio estas <tld/>ita de iu; produkto de la laboro;
      ellabora&jcirc;o; verko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Werk</trd>,
    <trd>Ergebnis</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;sultat<klr> (d'une fabrication)</klr></trd>,
    <trd>produit</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;</trd>,
    <trd>alkot&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">werk</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_d;<klr tip="amb"> (&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="far.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="far.0ebla.KOMUNE">
    <dif>
      Povanta esti <tld/>ata, efektivigebla, realigebla.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_v;&c_ib;&c_m;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_j;&c_s;&c_mol;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">machbar</trd>
  <trdgrp lng="fr">
    <trd>faisable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megcsin&aacute;lhat&oacute;</trd>,
    <trd>megtehet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">doenbaar</trd>
  <trd lng="ru">&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_m;&c_y;&c_j;</trd>
</drv>

<drv mrk="far.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <refgrp tip="dif">
      <ref cel="de.0.aganto">de<sncref/></ref> 
    </refgrp>
    <refgrp tip="sin">
