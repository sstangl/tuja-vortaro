<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxed.xml,v 1.9 2012/10/23 00:13:55 revo Exp $">
<kap>
  <rad>&scirc;ed</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxed.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxed.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Scirc;irmejo por brutoj a&ubreve; iloj, kun tegmento kaj subtenaj 
      fostoj, ordinare ligna kaj sen vandoj:
      <ekz>
        la hejtligna <tld/>o de la vilao
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a, p. 64a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="remiz.0o">remizo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Verschlag</trd>,
    <trd>Unterstand</trd>,
    <trd>Schuppen</trd>
  </trdgrp>
  <trd lng="fr">cabane<klr> (agr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pajta</trd>,
    <trd>sz&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>capannone</trd>,
    <trd>capanno</trd>
  </trdgrp>
  <trd lng="pt">abrigo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="sxed.abel0o">
  <kap>abel<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="sxed.abel0o.ABE">
    <dif>
      &Scirc;irmejo por <ref tip="prt" cel="abel.0ujo">abelujoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bienenstand</trd>,
    <trd>Bienenhaus</trd>
  </trdgrp>
  <trd lng="fr">rucher<klr> (batiment)</klr></trd>
  <trd lng="hu">m&eacute;hes</trd>
  <trd lng="it">apiario</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ch;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxed.garbo0o">
  <kap>garbo<tld/>o, <var><kap>garb<tld/>o</kap></var></kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="sxed.garbo0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Kampara <ref tip="super" cel="garb.0ejo">garbejo</ref> por fre&scirc;e
      rikoltita greno.
    </dif>
  </snc>
  <trd lng="de">Banse</trd>
  <trd lng="fr">grange</trd>
  <trdgrp lng="hu">
    <trd>cs&udblac;r</trd>,
    <trd>mag&aacute;r</trd>
  </trdgrp>
  <trd lng="it">pagliaio<klr> (capanno)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_y;&c_t;&c_o;&c_je; &c_g;&c_u;&c_m;&c_n;&c_o;</trd>,
    <trd>&c_r;&c_i;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_r;&c_n;&c_o;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
</drv>
</art>

