<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stup.xml,v 1.14 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stup</rad>/o
</kap>
<drv mrk="stup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Implikaj krispaj fadenoj restantaj el &scirc;pinita kanabo a&ubreve; lino kaj
      uzataj por &scirc;topi:
      <ekz>
        embarasata kiel kokido en <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_l;&c_mol;&c_l;&c_je;</trd>, 
    <trd>&c_k;&c_u;&c_d;&c_z;&c_je;&c_l;&c_ja;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cucek</trd>,
    <trd>koudel</trd>
  </trdgrp>
  <trd lng="de">Werg</trd>
  <trdgrp lng="en">
    <trd>tow<klr tip="amb"> (fiber)</klr></trd>,
    <trd>oakum</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estopa</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csep&udblac;</trd>, 
    <trd>k&oacute;c</trd> 
  </trdgrp>
  <trd lng="nl">werk<klr tip="ind"> (uitgeplozen touw)</klr></trd>
  <trd lng="pl">paku&lstroke;y<klr> {pl.}</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kúdeľ <klr>(výčesky)</klr></trd>
</drv>
</art>
