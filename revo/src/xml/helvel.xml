<vortaro>
<art mrk="$Id: helvel.xml,v 1.17 2013/10/27 18:10:13 revo Exp $">
<kap>
  <rad>helvel</rad>/o
</kap>
<drv mrk="helvel.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
     <ref tip="lst" cel="fung.0o.fruktokorpo"
     lst="voko:fungoj">Fungo</ref> 
     man&gcirc;ebla nur post kuirado kaj forver&scirc;o de la kuirakvo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
       el familio <ref tip="malprt"
      cel="helvela.0oj"><tld/>acoj</ref>
      (<trd lng="la">Helvella</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_ch;&c_o;&c_k;</trd>
  <trd lng="de">Lorchel</trd>
  <trd lng="fr">helvelle</trd>
  <trd lng="hu">papsapkagomba</trd>
  <trd lng="it">elvella</trd>
  <trd lng="nl">kluifjeszwam</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
