<vortaro>
<art mrk="$Id: portep.xml,v 1.10 2006/06/27 16:31:33 revo Exp $">
<kap>
  <rad>portepe</rad>/o <fnt><bib>PV</bib></fnt> <fnt>Z</fnt>
</kap>
<drv mrk="portep.0o">
  <kap><tld/>o</kap>
  <snc mrk="portep.0o.">
    <dif>
      <ref tip="super" cel="balte.0o">Balteo</ref>, rimeno por subteni 
      glavon:
      <ekz>
        li ekfajfis malla&ubreve;te ian gajan melodion, ludante per la manoj 
        je la kvastetoj de la <tld/>o
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_u;&c_p;&c_e;&c_ja;</trd>
  <trd lng="de">Portepee</trd>
  <trd lng="fr">porte-&eacute;p&eacute;e</trd>
  <trd lng="hu">kardsz&iacute;j</trd>
  <trd lng="nl">koppelriem</trd>
  <trdgrp lng="pt">
    <trd>cinto</trd>,
    <trd>cintur&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_u;&c_p;&c_je;&c_ja;</trd>
</drv>
</art>
