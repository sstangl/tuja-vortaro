<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: judais.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>judaism</rad>/o
</kap>
<drv mrk="judais.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="jud.0ismo">judismo</ref>
  </snc>
  <trd lng="be">&c_ju;&c_d;&c_a;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">judaisme</trd>
  <trdgrp lng="cs">
    <trd>judaizmus</trd>,
    <trd>mosaizmus</trd>,
    <trd>semitizmus</trd>,
    <trd>židovské náboženství</trd>,
    <trd>židovství</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Judaismus</trd>,
    <trd>Judentum</trd>
  </trdgrp>
  <trd lng="el">&jota;&omikron;&ypsilon;&delta;&alfa;&jota_trema;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">juda&iacute;smo</trd>
  <trd lng="fr">juda&iuml;sme</trd>
  <trdgrp lng="it">
    <trd>ebraismo</trd>,
    <trd>giudaismo</trd>
  </trdgrp>
  <trd lng="nl">jodendom</trd>
  <trd lng="pt">juda&iacute;smo</trd>
  <trd lng="ru">&c_i;&c_u;&c_d;&c_a;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">náboženstvo a náb. osveta židov</trd>
  <trd lng="sv">judendom</trd>
</drv>
</art>
