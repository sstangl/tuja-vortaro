<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hist.xml,v 1.23 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hist</rad>/o
</kap>
<drv mrk="hist.0o">
  <kap><tld/>o</kap>
  <tezrad fak="ANA"/>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="hist.0o.ANA">
    <dif>
      Sistemo da samfunkciaj kaj samdevenaj
      <ref tip="dif" cel="cxel.0o.BIO">&ccirc;eloj</ref>, kune kun la
      inter&ccirc;elaj substancoj kaj la strukturoj produktitaj de la
      &ccirc;eloj:
      <ekz>
        <ind>mola <tld/>o</ind>;
        <trd lng="ca">teixit tou</trd>
        <trd lng="de">Weichgewebe</trd>
        <trd lng="en">soft tissue</trd>
        <trd lng="es">tejido blando</trd>
        <trd lng="hu">l&aacute;gysz&ouml;vet</trd>
        <trd lng="it">tessuto<klr> molle</klr></trd>
        <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; 
&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind>kuniga <tld/>o</ind>;
        <trd lng="ca">teixit connectiu</trd>
        <trd lng="de">Bindegewebe</trd>
        <trd lng="en">connective tissue</trd>
        <trd lng="es">tejido conectivo</trd>
        <trd lng="hu">k&ouml;t&odblac;sz&ouml;vet</trd>
        <trd lng="it">tessuto<klr> connettivo</klr></trd>
        <trd lng="ru">
&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; 
&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind>osto<tld/>o</ind>.
        <trd lng="ca">teixit ossi</trd>
        <trd lng="de">St&uuml;tzgewebe</trd>
        <trd lng="en">bone tissue</trd>
        <trd lng="hu">csontsz&ouml;vet</trd>
        <trd lng="es">tejido &oacute;seo</trd>
        <trd lng="it">tessuto<klr> osseo</klr></trd>
        <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; 
&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">teixit</trd>
  <trdgrp lng="cs">
    <trd>buněčné pletivo</trd>,
    <trd>tkanivo</trd>,
    <trd>tkáň</trd>
  </trdgrp>
  <trd lng="de">(organisches) <ind>Gewebe</ind></trd>
  <trd lng="el">&jota;&sigma;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">tissue</trd>
  <trd lng="es">tejido (org&aacute;nico)</trd>
  <trd lng="fr">tissu<klr tip="ind"> (cellulaire)</klr></trd>
  <trd lng="hu">sz&ouml;vet<klr tip="ind"> (szerves)</klr></trd>
  <trd lng="it">tessuto<klr> (anat.)</klr></trd>
  <trd lng="nl">weefsel<klr tip="ind"> (organisch)</klr></trd>
  <trd lng="pl">tkanka</trd>
  <trd lng="pt">tecido<klr tip="ind"> (celular)</klr></trd>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol;<klr tip="ind"> 
(&c_b;&c_i;&c_o;&c_l;.)</klr></trd>
  <trd lng="sk">tkanivo</trd>
</drv>
</art>
