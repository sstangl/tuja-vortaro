<vortaro>
<art mrk="$Id: rimes.xml,v 1.8 2006/02/21 17:32:42 revo Exp $">
<kap>
  <rad>rimes</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="rimes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Speco de kambio, kiun &scirc;uldanto sendas al sia kreditoro por tiel pagi
      la pli malfrue pagotan &scirc;uldon.
    </dif>
    <refgrp tip="sin">
      <ref cel="kambi.0o">kambio</ref>,
      <ref cel="trat.0o">trato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Rimesse</trd>,
    <trd>Besitzwechsel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;ldv&eacute;ny</trd>,
    <trd>rimessza</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>overmaking</trd>,
    <trd>remise</trd>
  </trdgrp>
    <trd lng="pt">remessa</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d; &c_d;&c_je;&c_n;&c_je;&c_g;</trd>
  </trdgrp>
</drv>
</art>
