<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koncer1.xml,v 1.27 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>koncert</rad>/o
</kap>
<drv mrk="koncer1.0o">
  <kap><tld/>o</kap>
  <snc mrk="koncer1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika kunveno, dum kiu oni ludas plurajn kanta&jcirc;ojn
      a&ubreve; instrumentajn kompona&jcirc;ojn:
      <ekz>
        ludi, doni <tld/>on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; la senprudentaj birdoj muzikas sian &ccirc;ielan
        <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="koncxert.0o">kon&ccirc;erto</ref>.
    </rim>
    <trd lng="bg">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
    <trd lng="de">Konzert</trd>
    <trd lng="es">concierto</trd>
    <trd lng="nl">concert</trd>
    <trd lng="pt">concerto <klr tip="amb">(audi&ccedil;&atilde;o)</klr></trd>
    <trd lng="vo">konzed</trd>
  </snc>
  <snc mrk="koncer1.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akordo de opinioj a&ubreve; ideoj:
      <ekz>
        oni a&ubreve;das nur <tld/>on de la&ubreve;doj pri li;
      </ekz>
      <ekz>
        la E&ubreve;ropa <tld/>o havas kiel bazon la akordi&gcirc;on
        de Germanujo kaj Francujo;
      </ekz>
      <ekz>
        a&ubreve;digi sian vo&ccirc;on en la E&ubreve;ropa <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="akord.0o.samopinieco">akordo <sncref/></ref>,
    <ref cel="harmon1.0o.paco">harmonio <sncref/></ref>,
    <ref cel="konsen.inter0o">interkonsento</ref>
    </refgrp>
    <trd lng="bg">&c_s;&c_malmol;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Einverst&auml;ndnis</trd>,
      <trd>Einklang</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accord</trd>,
      <trd>agreement</trd>,
      <trd>harmony</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>samenspel</trd>,
      <trd>harmonie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</trd>
  <trd lng="ca">concert</trd>
  <trd lng="cs">koncert</trd>
  <trd lng="en">concert</trd>
  <trdgrp lng="fr">
    <trd>concert<klr tip="ind"> (spectacle de musique)</klr></trd>,
    <trd>r&eacute;cital</trd>,
    <trd>ch&oelig;ur<klr tip="ind"> (fig.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangverseny</trd>,
    <trd>koncert</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
  <trd lng="sk">koncert</trd>
</drv>

<drv mrk="koncer1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0i">Muziki</ref> prezentante <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</ind></trd>
  <trd lng="bg">&c_i;&c_z;&c_n;&c_je;&c_s;&c_je; <ind>&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</ind></trd>
  <trd lng="ca">fer un concert</trd>
  <trd lng="cs">koncertovat</trd>
  <trdgrp lng="de">
    <trd>konzertieren</trd>,
    <trd>ein <ind>Konzert geben</ind></trd>,
    <trd>in einem <ind>Konzert mitspielen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>give a concert</trd>,
    <trd>play a concert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>donner un <ind>concert</ind></trd>,
    <trd>concerter<klr tip="ind"> (donner un concert)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangversenyez</trd>,
    <trd>koncertezik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>concert</ind> geven</trd>,
    <trd>concerteren</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_mol;
<ind>&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</ind></trd>
  <trd lng="sk">koncertovať</trd>
</drv>

<drv mrk="koncer1.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Loko por <tld/>oj, ordinare <ref tip="super"
      cel="konstr.0ajxo">konstrua&jcirc;o</ref> kun akustike
ta&ubreve;ga salono a&ubreve;
      amfiteatro.
    </dif>
    <ref tip="vid" cel="kaf.koncert0ejo"><tld/>kafejo</ref>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;&c_n;&c_a; &c_z;&c_a;&c_l;&c_a;</trd>
  <trd lng="ca">sala de concerts</trd>
  <trdgrp lng="cs">
    <trd>koncertní místnost</trd>,
    <trd>koncertní sál</trd>
  </trdgrp>
  <trd lng="en">concert hall</trd>
  <trd lng="fr">salle de <ind>concert</ind></trd>
  <trd lng="hu">hangversenyterem</trd>
  <trdgrp lng="nl">
    <trd>concertgebouw</trd>,
    <trd>concertzaal</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;
&c_z;&c_a;&c_l;</trd>
  <trd lng="sk">koncertná sála</trd>
</drv>
</art>
