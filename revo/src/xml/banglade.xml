<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: banglade.xml,v 1.8 2014/06/03 10:10:42 revo Exp $">
<kap>
  <rad>banglade&scirc;</rad>/o
</kap>

<drv mrk="banglade.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="banglade.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda Azio kun &ccirc;efurbo Dako.
      Teritorio de Popola Respubliko Banglade&scirc;o.
      ISO-3166 kodo: BD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>,
      <ref tip="vid" cel="birm.B0o.GEOG">Birmo</ref>
      kaj <ref tip="vid" cel="hind.H0aOceano">Hinda Oceano</ref> 
      (Bengala Golfo).
    </dif>
    </snc>
    <trd lng="be">&c_B;&c_a;&c_n;&c_g;&c_l;&c_ja;&c_d;&c_e;&c_sh;</trd>
    <trdgrp lng="ca">
      <trd>Bangla Desh</trd>,
      <trd>Rep&uacute;blica Popular de Bangla Desh</trd>
    </trdgrp>
    <trd lng="fr">Bangladesh</trd>
    <trd lng="id">Bangladesh</trd>
    <trd lng="nl">Bangladesh</trd>
    <trdgrp lng="pl">
      <trd>Bangladesz</trd>,
      <trd>Ludowa Republika <ind>Bangladeszu</ind></trd>
    </trdgrp>
    <trd lng="pt">Bangladesh</trd>
</drv>

</art>

