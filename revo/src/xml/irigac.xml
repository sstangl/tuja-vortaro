<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: irigac.xml,v 1.13 2015/10/14 06:10:15 revo Exp $">
<kap>
  <rad>irigac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="irigac.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0o.AGR">
  <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="irigaci.0o.AGR">irigacio</ref>
  </snc>
  <trd lng="fr">irrigation</trd>
  <trdgrp lng="pl">
    <trd>irygacja</trd>,
    <trd>nawodnienie</trd>
  </trdgrp>
  <trd lng="pt">irriga&ccedil;&atilde;o</trd>
</drv>

<drv mrk="irigac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="irigac.0i.AGR">
    <ref tip="dif" cel="irigaci.0i.AGR">irigacii</ref>
  </snc>
  <trd lng="en">to make irriguous</trd>
  <trd lng="fr">irriguer</trd>
  <trdgrp lng="pl">
    <trd>irygowa&cacute;</trd>,
    <trd>nawadnia&cacute;</trd>
  </trdgrp>
</drv>
</art>
