<kap>
<ofc>*</ofc>
<rad>jasmen</rad>/o
</kap>
<drv mrk="jasmen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Jasminum</trd>),
      arbusto kun nepare plumaj folioj kaj funelformaj, blankaj
      a&ubreve; flavaj, bonodoraj floroj.
    </dif>
  </snc>
  <bld lok="../bld/jasmen.jpg">
    floroj de <tld/>o
  </bld>
  <trd lng="be">&c_ja;&c_z;&c_mol;&c_m;&c_ib;&c_n;</trd>
  <trd lng="br">jesmi</trd>
  <trd lng="de">Jasmin</trd>
  <trd lng="el">&gamma;&jota;&alfa;&sigma;&epsilon;&my;&jota_ton;</trd>
  <trd lng="en">jasmine</trd>
  <trd lng="fa">&f_y;&a_A;&a_s;&a_m;&a_n;</trd>
  <trd lng="fr">jasmin</trd>
  <trd lng="hu">j&aacute;zmin</trd>
  <trd lng="it">gelsomino</trd>
  <trd lng="ru">&c_zh;&c_a;&c_s;&c_m;&c_i;&c_n;</trd>
  <trd lng="nl">jasmijn</trd>
  <trd lng="pt">jasmin</trd>
  <trd lng="sk">jazm&iacute;n</trd>
  <trd lng="sv">jasmin</trd>
  <trd lng="tr">yasemin</trd>
  <trd lng="cs">jasm√≠n</trd>
</drv>
</art>
