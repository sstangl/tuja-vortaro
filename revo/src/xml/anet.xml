<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anet.xml,v 1.11 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anet.0o">
  <kap><tld/>o</kap>
  <snc mrk="anet.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Jara herba vegeta&jcirc;o el mezvarmaj kaj varmaj landoj,
      kies folipintoj, floroj kaj semoj estas uzataj
      kiel <ref tip="lst" cel="spic.0o.KUI"
      lst="voko:spicoj">spico</ref> en kuirarto,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">anethum graveolens</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_p;&c_malmol;&c_r;</trd>
  <trd lng="ca">anet</trd>
  <trd lng="cs">kopr</trd>
  <trd lng="de">Dill</trd>
  <trd lng="en">dill</trd>
  <trd lng="fr">aneth</trd>
  <trd lng="hu">kapor</trd>
  <trd lng="nl">dille</trd>
  <trdgrp lng="pl">
    <trd>koper</trd>,
    <trd>koperek</trd>
  </trdgrp>
  <trd lng="pt">aneto</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_o;&c_p;</trd>
  <trd lng="sk">k√¥por</trd>
</drv>
</art>

