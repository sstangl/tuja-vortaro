<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bov.xml,v 1.62 2015/12/20 14:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bov</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bov.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bov.0o.ZOO">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda kvarpieda, korna besto, el kiu kelkaj specioj estas
      bredataj pro viando kaj lakto, 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Bos</trd>).
    </dif>
  </snc>
  <bld lok="../bld/bov.jpg">
    e&ubreve;ropa hejmbovo
  </bld>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trdgrp lng="br">
    <trd>bevin<klr tip="ind"> (loen-saout)</klr></trd>,
    <trd>loen-saout</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vaca</trd>,
    <trd>bou</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skot</trd>,
    <trd>b&yacute;k</trd>,
    <trd>v&uring;l</trd>
  </trdgrp>
  <trd lng="de">Rind</trd>
  <trd lng="en">cow</trd>
  <trd lng="es">res</trd>
  <trd lng="fa">&f_g;&a_A;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>b&oelig;uf<klr tip="ind"> (esp&egrave;ce)</klr></trd>,
    <trd>boeuf</trd>
  </trdgrp>
  <trd lng="he">&bet;&qof;&resh;</trd>
  <trd lng="hu">szarvasmarha</trd>
  <trd lng="id">sapi</trd>
  <trdgrp lng="it">
     <trd>bovino</trd>,
     <trd>manzo</trd>,
     <trd>bue</trd>
   </trdgrp>
  <trd lng="nl">rund</trd>
  <trd lng="pl">w&oacute;&lstroke;</trd>
  <trd lng="pt">boi</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">hov&auml;do</trd>
  <trd lng="tr">bo&gbreve;a</trd>
  <trd lng="sk">hov&auml;do</trd>
  <trdgrp lng="cs">
    <trd>hlup&aacute;k</trd>,
    <trd>vulg. v&uring;l</trd>
  </trdgrp>
</drv>

<drv mrk="bov.B0o">
  <kap><tld lit="B"/>o</kap>
  <mlg>Tau</mlg>
  <mlg>&#9801;</mlg>
  <snc mrk="bov.B0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro"
      lst="voko:zodiakaj_signoj" val="2">stelfiguro</ref> kaj
      signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref> (<trd lng="la">Taurus</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_y;&c_k;</trd>,
    <trd>&c_C;&c_ja;&c_l;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="br">Tarv<klr tip="ind"> (steredeg)</klr></trd>
  <trd lng="ca">el Taure</trd>
  <trdgrp lng="cs">
    <trd>hlup&aacute;k</trd>,
    <trd>skot</trd>,
    <trd>vulg. v&uring;l</trd>
  </trdgrp>
  <trd lng="de">Stier</trd>
  <trd lng="en">Taurus</trd>
  <trd lng="es">Tauro</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_j; <ind>&a_th;&a_w;&a_r;</ind></trd>,
    <trd>&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y; <ind>&a_th;&a_w;&a_r;</ind></trd>
  </trdgrp>
  <trd lng="fr">Taureau<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &shin;&vav;&resh;</trd>
  <trd lng="hu">Bika<klr> (csillagk&eacute;p)</klr></trd>
  <trdgrp lng="id">
    <trd>Saur</trd>,
    <trd>Taurus</trd>,
    <trd>Wrisaba</trd>
  </trdgrp>
  <trd lng="it">Toro<klr tip="ind"> (costellazione)</klr></trd>
  <trd lng="nl">stier</trd>
  <trd lng="no">tyr</trd>
  <trd lng="pl">byk</trd>
  <trd lng="pt">Touro</trd>
  <trd lng="ru">&c_T;&c_je;&c_l;&c_je;&c_c;</trd>
  <trdgrp lng="sk">
    <trd>B&yacute;k</trd>,
    <trd>hov&auml;do</trd>
  </trdgrp>
  <trd lng="tr">bo&gbreve;a</trd>
</drv>

<drv mrk="bov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bov.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_l;&c_a;&c_v;&c_ib;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>kig <ind>bevin</ind></trd>,
    <trd>kig <ind>saout</ind></trd>
  </trdgrp>
  <trd lng="ca">carn de bou</trd>
  <trd lng="de">Rindfleisch</trd>
  <trd lng="en">beef</trd>
  <trd lng="es">carne de <ind>res</ind></trd>
  <trd lng="fa">&f_g;&a_w;&a_sh;&a_t; &f_g;&a_A;&a_w;</trd>
  <trd lng="fr"><klr tip="ind">(viande de) </klr>b&oelig;uf</trd>
  <trd lng="he">&bet;&shin;&resh; &bet;&qof;&resh;</trd>
  <trd lng="hu">marhah&uacute;s</trd>
  <trdgrp lng="it">
     <trd><ind>carne</ind> bovina</trd>,
     <trd><ind>carne</ind> di manzo</trd>
  </trdgrp>
  <trd lng="nl">rundsvlees</trd>
  <trd lng="no">oksekj&oslash;tt</trd>
  <trd lng="pl">wo&lstroke;owina</trd>
  <trd lng="ru">&c_g;&c_o;&c_v;&c_ja;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">hov&auml;dzie</trd>
  <trd lng="tr">s&inodot;&gbreve;&inodot;r eti</trd>
</drv>

<drv mrk="bov.0idajxo">
  <kap><tld/>ida&jcirc;o</kap>
  <snc mrk="bov.0idajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>ido.
    </dif>
  </snc>
  <trd lng="ca">carn de vedella</trd>
  <trd lng="en">veal</trd>
  <trd lng="it"><ind>carne</ind> di vitello</trd>
  <trd lng="fa">&f_g;&a_w;&a_sh;&a_t; &f_g;&a_w;&a_s;&a_A;&a_l;&a_h;</trd>
  <trd lng="nl">kalfsvlees</trd>
  <trd lng="pl">ciel&eogonek;cina</trd>
</drv>

<drv mrk="bov.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="bov.0ido.AGR">
    <dif>
      Ido de <tld/>o:
      <ekz>
        foriris <tld/>ido revenis <tld/>o
        <klr>(pli stulti&gcirc;is)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_l;&c_jo;</trd>,
    <trd>&c_c;&c_ja;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="br">leue</trd>
  <trdgrp lng="ca">
    <trd>vedell</trd>,
    <trd>vedella</trd>
  </trdgrp>
  <trd lng="cs">tele</trd>
  <trd lng="de">Kalb</trd>
  <trd lng="en">calf</trd>
  <trd lng="es">becerro</trd>
  <trd lng="fa">&f_g;&a_w;&a_s;&a_A;&a_l;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nisse</trd>,
    <trd>veau</trd>
  </trdgrp>
  <trd lng="he">&ayin;&gimel;&lamed;</trd>
  <trd lng="hu">borj&uacute;</trd>
  <trd lng="it">vitello</trd>
  <trd lng="nl">kalf</trd>
  <trd lng="no">kalv</trd>
  <trdgrp lng="pl">
    <trd>ciel&eogonek;</trd>,
    <trd>cielak</trd>
  </trdgrp>
  <trd lng="pt">bezerro</trd>
  <trd lng="sk">te&lcaron;a</trd>
  <trd lng="tr">buza&gbreve;&inodot;</trd>
</drv>

<drv mrk="bov.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="bov.0ino.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Inseksa <tld/>o:
      <ekz>
        e&ccirc; plej nigra <tld/>ino donas lakton nur blankan
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_v;&c_a;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_v;&c_a;</trd>
  <trd lng="br">buoc'h</trd>
  <trd lng="ca">vaca</trd>
  <trd lng="cs">kr&aacute;va</trd>
  <trd lng="de">Kuh</trd>
  <trd lng="en">cow</trd>
  <trd lng="es">vaca</trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&a_h;&zwnj;&f_g;&a_A;&a_w;</trd>
  <trd lng="fr">vache</trd>
  <trd lng="he">&pe;&resh;&he;</trd>
  <trd lng="hu">teh&eacute;n</trd>
  <trdgrp lng="it"><trd>mucca</trd>, <trd>vacca</trd></trdgrp>
  <trd lng="nl">koe</trd>
  <trd lng="no">ku</trd>
  <trd lng="pl">krowa</trd>
  <trd lng="pt">vaca</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_v;&c_a;</trd>
  <trd lng="sk">krava</trd>
  <trd lng="tr">inek</trd>
</drv>

<drv mrk="bov.B0isto">
  <kap><tld lit="B"/>isto
    <fnt><vrk><url ref="http://at.komputilo.org">Astronomia
  Terminaro</url></vrk></fnt>
  </kap>
  <mlg>Boo</mlg>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">Stelfiguro</ref> de la norda
      duonsfero (<trd
      lng="la">Bootes</trd>).
    </dif>
  </snc>
  <trd lng="ca">el Bover</trd>
  <trd lng="en">Bo&ouml;tes</trd>
  <trd lng="fr">Bouvier<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="it">Boote</trd>
</drv>

<drv mrk="bov.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="bov.vir0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        prenu vir<tld/>on kaj vir&scirc;afon por pacofero
        <fnt><bib>MT</bib>, <lok>&Lev; 9:4</lok></fnt>;
      </ekz>
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="br">ejen</trd>
        <trd lng="ca">bou</trd>
        <trd lng="fr">b&oelig;uf<klr tip="ind"> (castr&eacute;)</klr>
        </trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_k;</trd>
  <trd lng="br">tarv</trd>
  <trd lng="ca">bou <klr>(brau)</klr></trd>
  <trd lng="cs">b&yacute;k</trd>
  <trd lng="de">Stier</trd>
  <trd lng="en">bull</trd>
  <trd lng="es">toro</trd>
  <trd lng="fa">&f_g;&a_A;&a_w; &a_n;&a_r;</trd>
  <trd lng="fr">taureau</trd>
  <trdgrp lng="he">
    <trd>&shin;&vav;&resh;</trd>,
    <trd>&pe;&resh;</trd>
  </trdgrp>
  <trd lng="hu">bika</trd>
  <trd lng="it">toro</trd>
  <trd lng="nl">stier</trd>
  <trd lng="no">okse</trd>
  <trdgrp lng="pl">
    <trd>byk</trd>,
    <trd>buhaj</trd>
  </trdgrp>
  <trd lng="pt">touro</trd>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
  <trd lng="sk">b&yacute;k</trd>
</drv>

<drv mrk="bov.grunt0o">
  <kap>grunt<tld/>o</kap>
  <snc mrk="bov.grunt0o.ZOO">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="bov.0o.ZOO">bovo</ref>
      kun kovra hararo tre longa, kiu vivas en himalajaj 
      landoj (<trd lng="la">Bos grunniens</trd>):
      <ekz>
        la hejma grunt<tld/>o diferencas de la sova&gcirc;a per siaj
        malpli grandaj korpo, hufoj kaj kornoj
        <fnt>
          <aut>-</aut>,
          <vrk>
          <url ref="http://esperanto.cri.cn/1/2005/06/23/1@26120.htm">
          Gruntbovo</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2005-06-23</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="poefag.0o.ZOO">poefago</ref>
    <trd lng="ca">iac</trd>
    <trd lng="en">yak</trd>
    <trd lng="fr">yak</trd>
    <trd lng="id">yak</trd>
    <trdgrp lng="it">
      <trd>yak</trd>,
      <trd><ind>bue</ind> tibetano</trd>
    </trdgrp>
    <trd lng="nl">jak</trd>
    <trd lng="pl">jak</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>jak chrochtav&yacute;</trd>,
    <trd>jak divok&yacute;</trd>
  </trdgrp>
  <trd lng="sk">jak dom&aacute;ci</trd>
</drv>
</art>
