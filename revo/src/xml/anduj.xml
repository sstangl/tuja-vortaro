<art mrk="$Id: anduj.xml,v 1.6 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anduj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="anduj.0o">
  <kap><tld/>o</kap>
  <snc mrk="anduj.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <ref tip="dif" cel="kolbas.trip0o.KUI">tripkolbaso</ref>
      <ekz>
        kiam la hejmanoj finman&gcirc;is, ili &jcirc;etis <tld/>-resta&jcirc;ojn al la 
        kato sur la fornon
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,<vrk><url 
        ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve; 
        fabeloj</url></vrk>,
        <lok>Fabelo hunda, [2002?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        famaj <klr>...</klr> <tld/>oj de Gu&eacute;m&eacute;n&eacute;
        <fnt>
          <aut>B. Bailly</aut>,
          <vrk><url ref="">Renkonte al la bazaroj de Bretonio</url></vrk>,
          <lok>[vidita je 2006-05-11]</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="cs">taliánek</trd>
  <trd lng="de">Andouille</trd>
  <trd lng="fr">andouille<klr tip="ind"> (mets)</klr></trd>
  <trd lng="pt">chouri&ccedil;o</trd>
  <trd lng="sk">tlačenka</trd>
</drv>
</art>

