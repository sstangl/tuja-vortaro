<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trihxi.xml,v 1.12 2015/11/11 17:10:16 revo Exp $">
<kap>
  <rad>tri&hcirc;in</rad>/o
</kap>
<drv mrk="trihxi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro de parazitaj vermoj, kiu vivas en la muskoloj de porkoj
      kaj kiu ka&ubreve;zas tre gravan malsanon &ccirc;e homoj, kiuj &gcirc;in man&gcirc;as.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_h;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_t;&c_r;&c_i;&c_h;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">triquina</trd>
  <trd lng="de">Trichine</trd>
  <trd lng="en">trichina</trd>
  <trdgrp lng="fr">
    <trd>trichinose</trd>,
    <trd>trichinellose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>trichina</trd>
    <trd>bors&oacute;f&eacute;reg</trd>,
    <trd>bors&oacute;ka</trd></trdgrp>
  <trd lng="nl">trichine</trd>
  <trdgrp lng="pt">
    <trd>triquina</trd>,
    <trd>triquinela</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_h;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
