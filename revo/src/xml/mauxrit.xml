<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mauxrit.xml,v 1.9 2014/06/10 04:10:43 revo Exp $">
<kap>
  <rad>ma&ubreve;ritani</rad>/o
</kap>

<drv mrk="mauxrit.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="mauxrit.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordokcidenta Afriko kun &ccirc;efurbo Nuak&scirc;oto.
      Teritorio de <em>Islama Respubliko Ma&ubreve;ritanio</em>.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref>,
      Okcidenta Saharo,
      <ref tip="vid" cel="senegal.S0o.GEOG">Senegalo</ref>,
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="ar">&a_m;&a_w;&a_r;&a_y;&a_t;&a_A;&a_n;&a_y;&a_A;</trd>
    <trd lng="bg">&c_M;&c_a;&c_v;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Maurit&agrave;nia</trd>,
      <trd>Rep&uacute;blica Isl&agrave;mica de <ind>Maurit&agrave;nia</ind></trd>
    </trdgrp>
    <trd lng="de">Mauretanien</trd>
    <trd lng="fa">&a_m;&a_w;&a_r;&f_y;&a_t;&a_A;&a_n;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>Mauritanie</trd>,
      <trd>R&eacute;publique islamique de Mauritanie</trd>
    </trdgrp>
    <trd lng="hu">Maurit&aacute;nia</trd>
    <trd lng="id">Mauritania</trd>
    <trdgrp lng="pl">
      <trd>Mauretania</trd>,
      <trd>Islamska Republika Mauretra&nacute;ska</trd>
    </trdgrp>
    <trd lng="pt">Ma&ubreve;ritanio</trd>
  </snc>
</drv>

</art>

