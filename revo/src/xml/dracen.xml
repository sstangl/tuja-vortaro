<kap>
  <rad>dracen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dracen.0o">
  <kap><tld/>o</kap>
  <snc mrk="dracen.0o.BOT">
    <ref tip="dif" cel="arb.drak0o.BOT">drakarbo</ref>
    <ekz>
      prezento de la <tld/>o <klr>...</klr> trovi&gcirc;as en
      gravura&jcirc;o de la
      konata germana pentristo kaj gravuristo Martin Schonghauer
      (1453&ndash;1491)
      <fnt><aut>Juan R&eacute;gulo P&eacute;rez</aut>, <vrk>dracenoj
      &ccirc;e pentristoj de la
      15a jarcento</vrk>, <lok>norda prismo, 1957, numero 3a, p.
      123-128a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="cs">dracéna</trd>
  <trd lng="de">Drachenbaum</trd>
  <trd lng="pl">dracena</trd>
  <trd lng="pt">dracena</trd>
  <trd lng="sk">dracéna</trd>
</drv>
</art>

