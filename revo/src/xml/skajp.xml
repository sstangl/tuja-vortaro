    <rad>Skajp</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="skajp.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">TEL</uzo>    
    <dif>
      <ref tip="dif" cel="mesagx.tuj0ilo.KOMP">Tujmesa&gcirc;ilo</ref> kaj
      <ref tip="dif" cel="ret.inter0o.la.KOMP">interreta</ref>
