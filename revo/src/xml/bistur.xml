<art mrk="$Id: bistur.xml,v 1.20 2015/10/02 18:10:14 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>bisturi</rad>/o
</kap>
<drv mrk="bistur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Sekcilo, &hcirc;irurgia tran&ccirc;ilo.  
    </dif>
    <ref tip="vid" cel="skalpe.0o">skalpelo</ref>
  </snc>
  <trd lng="lat">scalprum</trd>
  <trd lng="bg">&c_l;&c_a;&c_n;&c_c;&c_je;&c_t;</trd>
  <trd lng="ca">bistur&iacute;</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;itvice</trd>,
    <trd>chirurgick&yacute; no&zcaron;&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lanzette</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>bistoury</trd><trd>lancet</trd></trdgrp>
  <trd lng="es">bistur&iacute;</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_y;&a_gh; &a_j;&a_r;&a_A;&a_H;&f_y;</trd>
    <trd>&a_n;&f_y;&a_sh;&a_t;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">bistouri</trd>
  <trd lng="hu">bistouri</trd>
  <trd lng="it">bisturi</trd>
  <trd lng="la">scalprum</trd>
  <trdgrp lng="nl">
    <trd>scalpel</trd>,
    <trd>lancet</trd>,
    <trd>bistouri</trd>,
    <trd>operatiemes</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bistur</trd>,
    <trd>lancet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_s;&c_t;&c_u;&c_r;&c_i;</trd>,
    <trd>&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    &c_n;&c_o;&c_zh;</trd>
  </trdgrp>
  <trd lng="sk">chirurgick&yacute; no&zcaron;&iacute;k</trd>
</drv>
</art>
