<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konven1.xml,v 1.20 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konvenci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konven1.0o">
  <kap><tld/>o</kap>
  <snc mrk="konven1.0o.gxenerale">
    <dif>
      Tio, kio estas starigita per esprimita a&ubreve; neesprimita
      <ref tip="vid" cel="konsen.inter0o">interkonsento</ref> inter la 
      homoj; interakceptita fikcio:
      <ekz>
        sociaj <tld/>oj
        (<ref tip="vid" cel="dec">deco</ref>);
      </ekz>
      <ekz>
        teatraj <tld/>oj;
      </ekz>
      <ekz>
        ili baras al si la sanan naturon per sensencaj
        <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a valoro de la papermono;
      </ekz>
      <ekz>
        <tld/>a alfabeto, &jcirc;argono
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondic1.0a">kondi&ccirc;a</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Konvention</trd>,
      <trd>&Uuml;bereinkunft</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>convention</trd>,
      <trd>general usage</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>koncenci&oacute;</trd>,
      <trd>bevett szok&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_u;&c_s;&c_l;&c_o;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o; &c_v;&c_o;
      &c_m;&c_n;.&c_ch;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="konven1.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <ref tip="dif" cel="trakta.0o.JUR">traktato <sncref/></ref>
      <ekz>
        <tld/>o gardanta la a&ubreve;torajn rajtojn;
      </ekz>
      <ekz>
        la po&scirc;ta <tld/>o de Bern;
      </ekz>
      <ekz>
        <tld/>a kla&ubreve;zo.
      </ekz>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="ca">convenci&oacute;</trd>
    <trd lng="de">Abkommen</trd>
    <trdgrp lng="en">
      <trd>convention</trd>,
      <trd>agreement</trd>
    </trdgrp>
    <trd lng="es">convenci&oacute;n</trd>
    <trd lng="hu">&aacute;llamk&ouml;zi <ind>egyezm&eacute;ny</ind></trd>
    <trd lng="pt">conven&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_v;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konven1.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Nacia eksterordinara asembleo:
      <ekz>
        la Franca <tld/>o (1792&ndash;1795);
      </ekz>
      <ekz>
        <tld/>ano.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Konvent</trd>,
      <trd>konstituierende Versammlung</trd>
    </trdgrp>
    <trd lng="en">convention</trd>
    <trdgrp lng="hu">
      <trd>konvent</trd>,
      <trd>alkotm&aacute;nyoz&oacute; nemzetgy&udblac;l&eacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_v;&c_je;&c_n;&c_t;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dohoda</trd>,
    <trd>konvence</trd>,
    <trd>konvenience</trd>,
    <trd>obyčej</trd>,
    <trd>smlouva</trd>,
    <trd>zvyklost</trd>,
    <trd>úmluva <klr>(mezinárodní)</klr></trd>
  </trdgrp>
  <trd lng="fr">convention</trd>
  <trdgrp lng="sk">
    <trd>dohoda</trd>,
    <trd>konvencia</trd>,
    <trd>zmluva</trd>
  </trdgrp>
</drv>

<drv mrk="konven1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al a&ubreve; rezultanta el <tld/>o.
    </dif>
  <trd lng="en">conventional</trd>
  <trd lng="es">convencional</trd>
  <trd lng="fr">conventionnel</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>běžný</trd>,
    <trd>konvencionální</trd>,
    <trd>konvenienční</trd>,
    <trd>konvenční</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bežný</trd>,
    <trd>konvenčný</trd>
  </trdgrp>
</drv>
</art>
