  <trd lng="en">mistletoe</trd>
  <trd lng="es">mu&eacute;rdago</trd>
  <trd lng="fa">&a_d;&a_A;&a_r;&a_w;&a_A;&a_sh;</trd>
  <trd lng="hu">fagy&ouml;ngy</trd>
  <trd lng="nl">maretak</trd>
  <trdgrp lng="pt">
    <trd>visco</trd>,
    <trd>visgo</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_je;&c_l;&c_a;</trd>
  <trd lng="sk">imelo</trd>
</drv>
