<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: recita.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>recitativ</rad>/o
</kap>
<drv mrk="recita.0o">
  <kap><tld/>o</kap>
  <snc mrk="recita.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Deklamata duonkanto sen fiksa takto, sekvanta nur la naturan
      ritmon de la parola frazo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ch;&c_y;&c_t;&c_a;&c_t;&c_y;&c_w;</trd>
  <trd lng="bg">&c_r;&c_je;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>mluvený zpěv</trd>,
    <trd>recitativ</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rezitativ</trd>,
    <trd>Sprechgesang</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;citatif</trd>
  <trdgrp lng="hu">
    <trd>&eacute;nekbesz&eacute;d</trd>,
    <trd>recitativo</trd>
  </trdgrp>
  <trd lng="it">recitativo<klr> (mus.)</klr></trd>
  <trd lng="nl">recitatief</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
  <trd lng="sk">recitatív</trd>
</drv>
</art>
