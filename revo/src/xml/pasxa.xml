<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pasxa.xml,v 1.11 2011/12/10 23:10:09 revo Exp $">
<kap>
  <rad>pa&scirc;a</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pasxa.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasxa.0o.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="provin.0estro.POL">Provincestro</ref>
      en malnova Turkujo:
      <ekz>
        mi delonge rimarkas, &mdash; vipe kontra&ubreve;diris &scirc;i, &mdash; ke vi kondutas 
        kiel azia <tld/>o, ne havante e&ccirc; guton da respekto al virino
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_a; <klr tip="amb">(&c_t;&c_u;&c_r;&c_e;&c_c;&c_k;&c_ib;
  &c_ch;&c_y;&c_n;&c_o;&c_w;&c_n;&c_ib;&c_k;)</klr></trd>
  <trdgrp lng="ca">
    <trd>paix&agrave;</trd>,
    <trd>baix&agrave;</trd>
  </trdgrp>
  <trd lng="de">Pascha</trd>
  <trd lng="en">pasha</trd>
  <trd lng="fa">&f_p;&a_A;&a_sh;&a_A;</trd>
  <trd lng="fr">pacha</trd>
  <trdgrp lng="hu">
    <trd>basa</trd>,
    <trd>pasa</trd>
  </trdgrp>
  <trd lng="nl">pasja</trd>
  <trd lng="pt">pax&aacute;</trd>
  <trd lng="ru">&c_p;&c_a;&c_sh;&c_a;</trd>
</drv>
</art>
