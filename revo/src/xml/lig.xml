      <ekz>
        li parolas sen senco kaj sen inter<tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd
    lng="be">&c_u;&c_z;&c_a;&c_je;&c_m;&c_a;&c_s;&c_u;&c_v;&c_ja;&c_z;&c_mol;</trd>
