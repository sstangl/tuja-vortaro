  <rad>demograf</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="demograf.0o">
  <kap><tld/>o</kap>
  <snc mrk="demograf.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <ref tip="dif" cel="demografi.0isto.SCI">demografiisto</ref>
    <ekz>
      la neregula lo&gcirc;antara distribuo <klr>...</klr> estis
      sufi&ccirc;e 
      studata de historiistoj, geografoj, <tld/>oj, ekonomiistoj kaj 
      sociologoj
      <fnt>
        <aut>S. Chaves</aut>,
        <vrk><url
        ref="http://www.kke.org.br/esperanto/prelegoj/graveco_de_sanrio_por_la_brazila_esperanto_movado.pdf">Graveco
        de Sanrio</url></vrk>,          
        <lok>2005-07</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="cs">demograf</trd>
  <trd lng="de">Demograph</trd>
  <trd lng="en">demographer</trd>
  <trd lng="fr">d&eacute;mographe</trd>
  <trd lng="hu">demogr&aacute;fus</trd>
  <trd lng="nl">demograaf</trd>
  <trd lng="pl">demograf</trd>
  <trd lng="pt">dem&oacute;grafo</trd>
  <trd lng="sk">demograf</trd>
</drv>
</art>

