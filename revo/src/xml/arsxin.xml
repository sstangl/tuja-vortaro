<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arsxin.xml,v 1.16 2014/01/24 16:10:25 revo Exp $">
<kap>
  <rad>ar&scirc;in</rad>/o
</kap>
<drv mrk="arsxin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malnova orienta <ref tip="lst"
      cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref> de longo:
      <ekz>
       rusa <tld/>o egalas al 0,7112 m,
       bulgara, estona, litova, rumana, serba kaj historia turka
       <tld/>oj longis inter 0,68 kaj 0,69 m,
       moderna turka <tld/>o je 1 m;
      </ekz>
      <ekz>
        li donis al vi por uniformo du <tld/>ojn da drapo
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 4a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="verst.0o">versto</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_sh;&c_y;&c_n;</trd>
  <trd lng="bg">&c_a;&c_r;&c_sh;&c_i;&c_n;</trd>
  <trd lng="de">Arschin</trd>
  <trd lng="en">archine</trd>
  <trd lng="es">arsh&iacute;n</trd>
  <trd lng="fr">archine</trd>
  <trd lng="hu">arsin</trd>
  <trd lng="pl">arszyn</trd>
  <trd lng="ru">&c_a;&c_r;&c_sh;&c_i;&c_n;</trd>
  <trdgrp lng="tr">
    <trd>ar&scedil;&inodot;n</trd>,
    <trd>endaze</trd>
  </trdgrp>
</drv>
</art>
