<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kavalk.xml,v 1.13 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kavalkad</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kavalk.0o">
  <kap><tld/>o</kap>
  <snc mrk="kavalk.0o.RAJ">
    <dif>
      Pompa ceremonia a&ubreve; plezura promenado de rajdantaro:
      <ekz>
        montri&gcirc;is survoje brua kaj brila <tld/>o
        <fnt><bib>Mary</bib></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>jezdecká promenáda</trd>,
    <trd>kavalkáda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kavalkade</trd>,
    <trd>Reiteraufzug</trd>
  </trdgrp>
  <trd lng="en">cavalcade</trd>
  <trd lng="fr">cavalcade<klr tip="ind"> (promenade, c&eacute;r&eacute;monie)</klr></trd>
  <trdgrp lng="hu">
    <trd>lovasfelvonul&aacute;s</trd>,
    <trd>sz&iacute;nes forgatag</trd>,
    <trd>kavalk&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cavalcade</trd>,
    <trd>ruiterstoet</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">jazdecká promenáda</trd>
</drv>
</art>
