<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: batat.xml,v 1.17 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>batat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="batat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="batat.0o.BOT">
    <dif>
      Specio el familio
      <ref tip="super" cel="konvol1.0oj">konvolvulacoj</ref>
      (<trd lng="la">Ipomoea batatas</trd>), kun grandaj tuberoj.
    </dif>
    <trd lng="de">Knollenwinde</trd>
    <trd lng="hu">bat&aacute;ta</trd>
  </snc>
  <snc mrk="batat.0o.tubero">
    <dif>
      La man&gcirc;ebla dol&ccirc;a tubero de
      <ref tip="malprt" cel="batat.0o.BOT"><tld/>o</ref>.
    </dif>
    <trdgrp lng="ca">
      <trd>batata</trd>,
      <trd>moniato</trd>
    </trdgrp>
    <trd lng="de">Batate</trd>
    <trd lng="es">batata</trd>
    <trd lng="fr">patate douce</trd>
    <trd lng="hu">&eacute;desburgonya</trd>
   
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;&c_ja;
    &c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>batata</trd>,
    <trd>povijnice jedlá</trd>,
    <trd>sladký brambor</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sweet <ind>potato</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bataat</trd>,
    <trd>zoete <ind>aardappel</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>batat</trd>,
      <trd>s&lstroke;odki ziemniak</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>batata</trd>,
    <trd>batata-doce</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;
    &c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">topinambur</trd>
</drv>
</art>
