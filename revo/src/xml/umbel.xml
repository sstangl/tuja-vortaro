<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: umbel.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
<rad>umbel</rad>/o
</kap>
<drv mrk="umbel.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="inflor.0o">Infloresko</ref>, &ccirc;e kiu
       la floroj trovi&gcirc;as sur tigetoj, elkreskantaj el la sama
       punkto &ccirc;iudirekten, simile al vergoj de ombrelo:
       <ekz>
         <tld/>o de primolo, apiacoj;
       </ekz>
       <ekz>
         <ind>kunmetita a&ubreve; duobla <tld/>o</ind>
         <klr>(el unu punkto elkreskas <tld/>etoj kiel &ccirc;e
         multaj apiacoj)</klr>.
         <trd lng="de">zusammengesetzte <ind>Dolde</ind></trd>
         <trd lng="hu">&ouml;sszetett <ind>erny&odblac;</ind></trd>
       </ekz>
     </dif>
   </snc>
  <trd lng="bg">&c_s;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="ca">umbel&middot;la</trd>
  <trdgrp lng="cs">
    <trd>květenství</trd>,
    <trd>okolík</trd>,
    <trd>umbela</trd>
  </trdgrp>
  <trd lng="de">Dolde</trd>
  <trd lng="en">umbel</trd>
  <trd lng="es">umbela</trd>
  <trd lng="fi">sarja<klr tip="amb"> (kukinto)</klr></trd>
  <trd lng="fr">ombelle</trd>
  <trd lng="hu">erny&odblac;<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
  <trd lng="it">ombrella<klr> (bot.)</klr></trd>
  <trd lng="la">umbella</trd>
  <trdgrp lng="nl"><trd>scherm <klr tip="ind">(bot.)</klr></trd>,
       <trd>bloemscherm</trd></trdgrp>
  <trd lng="pt">umbela</trd>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;
   <klr tip="ind">(&c_b;&c_o;&c_t;.)</klr></trd>
  <trd lng="sk">okolík</trd>
</drv>
</art>
