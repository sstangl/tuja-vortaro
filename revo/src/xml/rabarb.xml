<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rabarb.xml,v 1.19 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>rabarb</rad>/o
</kap>
<drv mrk="rabarb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Rheum</trd>),
      kun tre ampleksaj folioj, kies trunkoj
      estas uzataj por fari kompoton a&ubreve; konfita&jcirc;on kaj kies
      pulvorigitaj radikoj servas kiel laksigilo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>rebarbora</trd>,
    <trd>reve≈à</trd>
  </trdgrp>
  <trd lng="de">Rhabarber</trd>
  <trd lng="en">rhubarb</trd>
  <trd lng="fr">rhubarbe</trd>
  <trd lng="hu">rebarbara</trd>
  <trd lng="nl">rabarber</trd>
  <trd lng="pl">rabarbar</trd>
  <trdgrp lng="pt">
   <trd>ruibarbo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">rebarbora</trd>
  <trd lng="sv">rabarber</trd>
</drv>
</art>
