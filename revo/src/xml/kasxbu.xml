<vortaro>
<art mrk="$Id: kasxbu.xml,v 1.5 2013/10/03 08:10:28 revo Exp $">
<kap>
  <rad>ka&scirc;bu</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kasxbu.0o">
  <kap><tld/>o</kap>
  <snc mrk="kasxbu.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Babilonia <ref tip="lst" cel="unu.mezur0o"
      lst="voko:da&ubreve;roj">mezurunuo</ref> de
      tempo, sesono de hela tago a&ubreve; de nokto:
      <ekz>
        aro da gvardianoj foriris anta&ubreve; unu <tld/>o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 21a, Zalmuna batalas
        kontra&ubreve;
        la &ccirc;efpastroj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">kasbu <klr tip="amb">(2 &oacute;ra)</klr></trd>
    <trd lng="nl">kasboe<klr tip="ind"> (Babylonische
    tijdseenheid)</klr></trd>
  </snc>
</drv>
</art>

