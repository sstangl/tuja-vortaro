<vortaro>
<art mrk="$Id: sauxc.xml,v 1.20 2013/09/29 19:10:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sa&ubreve;c</rad>/o
</kap>
<uzo tip="fak">KUI</uzo>
<drv mrk="sauxc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sauxc.0o.KUI">
    <dif>
      Flueca spica&jcirc;o aldonata al man&gcirc;a&jcirc;o:
      <ekz>
        kaj e&ccirc; nenia <tld/>o a&ubreve; kuka&jcirc;o! Senta&ubreve;guloj
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 6a</lok></fnt>!
      </ekz>
      <ekz>
        menta <tld/>o
        <fnt>
        <aut>OH</aut>,
        <vrk>Molekula arkeologio por frandemuloj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 3a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_o;&c_w;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_o;&c_s;</trd>
  <trd lng="ca">salsa</trd>
  <trd lng="de">So&szlig;e</trd>
  <trd lng="en">sauce</trd>
  <trd lng="es">salsa</trd>
  <trd lng="fa">&a_s;&a_damma;&a_s;</trd>
  <trd lng="fr">sauce</trd>
  <trd lng="he">&resh;&vav;&tet;&bet;</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;sz</trd>,
    <trd>m&aacute;rt&aacute;s</trd>
  </trdgrp>
  <trd lng="it">salsa</trd>
  <trd lng="nl">saus</trd>
  <trd lng="pl">sos</trd>
  <trd lng="ro">sos</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="tp">telo</trd>
</drv>
</art>
