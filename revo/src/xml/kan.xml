<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kan.xml,v 1.28 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="kan.0o.BOT">
    <dif>
      Alta <ref tip="lst" cel="herb.0o"
      lst="voko:pooparencoj">herbo</ref>,
      kies trunketoj i&gcirc;as per seki&gcirc;ado fortikaj,
      ordinare el familio <ref tip="super" cel="poac.0oj">poacoj</ref>, 
      kiel ekzemple 
      <ref tip="sub" cel="fragmi.0o">fragmito</ref>:
      <ekz>
        <klr>[&scirc;i metis la infanon]</klr> inter la <tld/>ojn
        sur la bordo de la Rivero
        <fnt><bib>MT</bib>, <lok>&Eli; 2:3</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_y;&c_s;&c_mol;&c_n;&c_jo;&c_g;</trd>     
    <trd lng="br">korzenn<klr tip="ind"> (plant)</klr></trd>
    <trd lng="de">Schilfrohr</trd>
    <trdgrp lng="en">
      <trd>cane</trd>,
      <trd>reed</trd>
    </trdgrp>
    <trd lng="es">ca&ntilde;a</trd>
    <trdgrp lng="fr">
      <trd>roseau</trd>,
      <trd>canne<klr tip="ind"> (bot.)</klr></trd>
    </trdgrp>
    <trd lng="hu">n&aacute;d</trd>
    <trd lng="nl">riet</trd>
    <trd lng="pl">trzcina</trd>
  <trdgrp lng="pt">
    <trd>cana-de-vassoura</trd>,
    <trd>junco</trd>
  </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="kan.0o.ilo">
    <dif>
      Trunketo de <tld/>o <sncref ref="kan.0o.BOT"/>, uzata kiel
      materialo pro fari meblojn, a&ubreve; kiel iloj, ekzemple bastono
      por sin apogi, a&ubreve; por frapi:
      <ekz>
        &scirc;i prenis por li keston el <tld/>oj kaj
        &ccirc;irka&ubreve;&scirc;miris &gcirc;in per asfalto kaj
        pe&ccirc;o
        <fnt><bib>MT</bib>, <lok>&Eli; 2:3</lok></fnt>;
      </ekz>
      <ekz>
        vi fidas la apogon de Egiptujo, tiu <tld/>o rompita, kiu, se iu
        sin apogas sur &gcirc;i, eniras en lian manon kaj trapikas
        &gcirc;in
        <fnt><bib>MT</bib>, <lok>&Re2; 18:21</lok></fnt>;
      </ekz>
      <ekz>
        sin apogi sur rompita <tld/>o
        <klr>(iluzie kalkuli pri neefektiva helpo, vane fidi senpovan
        helponton)</klr>;
      </ekz>
      <ekz>
        ne malvolu puni knabon: se vi batos lin per <tld/>o, li ne
        mortos
        <fnt><bib>MT</bib>, <lok>&Sen; 23:13</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj instruo donas sa&gcirc;on
        <fnt><bib>MT</bib>, <lok>&Sen; 29:15</lok></fnt>;
      </ekz>
      <ekz>
        tia okazo mia &scirc;ultro defalu de la dorso, kaj mia brako
        rompi&gcirc;u de <tld/>oi <klr>(de bato per <tld/>o)</klr>
        <fnt><bib>MT</bib>, <lok>&Ijo; 31:22</lok></fnt>.
      </ekz>
    </dif>                                               
    <trdgrp lng="br">         
      <trd>bazh</trd>,
      <trd>gwialenn</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Rattan</trd>,
      <trd>Rohr-</trd>
    </trdgrp>
    <trd lng="fr">canne<klr tip="ind"> (b&acirc;ton)</klr></trd>
    <trd lng="hu">bambuszn&aacute;d</trd>
    <trdgrp lng="nl">
      <trd>rietstengel</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>cana</trd>,
    <trd>cani&ccedil;o</trd>,
    <trd>vara</trd>,
    <trd>bengala</trd>
  </trdgrp>
  </snc>
  <trd lng="ca">canya</trd>
  <trdgrp lng="cs">
    <trd>rákos</trd>,
    <trd>třtina</trd>
  </trdgrp>
  <trd lng="sk">trsť obyčajná</trd>
</drv>

<drv mrk="kan.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="kan.suker0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      alta herbo, kies suko donas sukeron,
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Saccharum</trd>).
    </dif>
  </snc>
  <trd lng="be">
  &c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_y;
  &c_t;&c_r;&c_y;&c_s;&c_mol;&c_n;&c_jo;&c_g;
  </trd>
  <trd lng="br">korzenn-sukr</trd>
  <trd lng="ca"><ind>canya</ind> de sucre</trd>
  <trd lng="cs">cukrová třtina</trd>
  <trd lng="de">Zuckerrohr</trd>
  <trd lng="en">sugar <ind>cane</ind></trd>
  <trd lng="es">ca&ntilde;a de az&uacute;car</trd>
  <trd lng="fr">canne &agrave; sucre</trd>
  <trd lng="hu">cukorn&aacute;d</trd>
  <trd lng="nl">suikerriet</trd>
  <trd lng="pl">cukier <ind>trzcinowy</ind></trd>
  <trd lng="pt">cana-de-a&ccedil;&uacute;car</trd>
  <trd lng="ru">&c_s;&c_a;&c_h;&c_a;&c_r;&c_n;&c_y;&c_j;
  &c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">cukrová trstina</trd>
</drv>
</art>
