<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diabol.xml,v 1.15 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>diabol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diabol.0o">
  <kap><tld/>o</kap>
  <snc mrk="diabol.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Jcirc;ongla <ref tip="lst" cel="lud.0ilo"
      lst="voko:ludiloj">ludilo</ref> konsistanta el bobeno, kiun oni
      movas, &jcirc;etas supren
      kaj rekaptas per &scirc;nuro havanta tenilon &ccirc;e &ccirc;iu
      ekstremo:
      <ekz>
        la plej granda <tld/>o, kiun ni ludis
        diametras 1,5 metrojn
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://www.youtube.com/user/cikonio#p/a/u/2/ZKeyJU3xOXM">
          Diaboloj en &Ccirc;inio</url></vrk>,
          <lok>2011-01-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_b;&c_o;&c_l;&c_o;</trd>
  <trd lng="ca">di&agrave;bolo</trd>
  <trd lng="cs">diabolo</trd>
  <trd lng="de">Diabolo</trd>
  <trd lng="en">diabolo</trd>
  <trd lng="es">di&aacute;bolo</trd>
  <trd lng="fr">diabolo<klr tip="ind"> (jouet)</klr></trd>
  <trd lng="hu">diabol&oacute;</trd>
  <trd lng="pl">diabolo</trd>
  <trdgrp lng="pt">
    <trd>diabol&ocirc;</trd>,
    <trd>di&aacute;bolo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_b;&c_o;&c_l;&c_o;</trd>
  <trd lng="sk">diabolo</trd>
</drv>
</art>
