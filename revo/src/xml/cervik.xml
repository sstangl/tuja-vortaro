<kap>
<rad>cervik</rad>/o
</kap>
<drv mrk="cervik.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<snc>
<dif>
Pli mallar&gcirc;a parto de organo:  
<ekz>
  denta, utera <tld/>o;
</ekz>
<ekz>
  <ind>uter<tld/>a</ind> tumoro.
  <trd lng="fr">
    cervical<klr tip="ind"> (relatif au col de l'ut&eacute;rus)</klr></trd>
  <trd lng="hu">m&eacute;hnyakdaganat</trd>
</ekz>
</dif>
</snc>
  <trd lng="bg">&c_sh;&c_i;&c_j;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
  <trd>coll</trd>,
  <trd>c&egrave;rvix</trd>
</trdgrp>
  <trd lng="cs">krček</trd>
  <trd lng="de">Hals</trd>
  <trd lng="en">cervix</trd>
  <trdgrp lng="fr">
  <trd>col<klr tip="ind"> (d'un organe)</klr></trd>,
  <trd>collet<klr tip="ind"> (d'une dent)</klr></trd></trdgrp>
  <trd lng="hu">nyak<klr tip="ind"> (szerv&eacute;)</klr></trd>
  <trd lng="it">cervice</trd>
  <trd lng="la">Cervix</trd>
  <trdgrp lng="nl">
  <trd>cervix</trd>,
  <trd>hals<klr tip="ind"> (als in baarmoederhals)</klr></trd>
</trdgrp>
  <trd lng="pl">szyjka <klr>(organu)</klr></trd>
  <trd lng="ru">&c_sh;&c_je;&c_j;&c_k;&c_a; <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>krk</trd>,
    <trd>kŕčik</trd>,
    <trd>kŕčok</trd>
  </trdgrp>
</drv>
</art>
