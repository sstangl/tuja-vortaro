<vortaro>
<art mrk="$Id: herald.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap><rad>heraldik</rad>/o <fnt>Z</fnt></kap>
<drv mrk="herald.0o">
  <kap><tld/>o</kap>
  <snc mrk="herald.0o.SCI">
    <uzo tip="fak">HER</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <ref cel="blazon.0o">blazonoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_d;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>erbovnictví</trd>,
    <trd>heraldika</trd>,
    <trd>znakoznalství</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heraldik</trd>,
    <trd>Wappenkunde</trd>
  </trdgrp>
  <trd lng="el">&omikron;&jota;&kappa;&omikron;&sigma;&eta;&my;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="es">her&aacute;ldica</trd>
  <trd lng="fr">h&eacute;raldique</trd>
  <trdgrp lng="hu">
    <trd>c&iacute;mertan</trd>,
    <trd>heraldika</trd>
  </trdgrp>
  <trd lng="it">araldica</trd>
  <trdgrp lng="nl">
    <trd>heraldiek</trd>,
    <trd>wapenkunde</trd>
  </trdgrp>
  <trd lng="pl">heraldyka</trd>
  <trd lng="pt">her&aacute;ldica</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_d;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">náuka o erboch</trd>
</drv>
</art>
