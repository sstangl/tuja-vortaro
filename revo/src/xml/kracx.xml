<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kracx.xml,v 1.19 2016/02/09 21:10:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kra&ccirc;</rad>/i
</kap>
<drv mrk="kracx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kracx.0i.KOMUNE">
    <dif>
      El&jcirc;eti sekrecia&jcirc;on a&ubreve; iun likvon el la bu&scirc;o per rapida, vola
      elspiro:
      <ekz>
        ne <tld/>u en puton, &ccirc;ar vi trinki
        bezonos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i sur iun<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sur ion
        <klr>(signe de malestimo)</klr>;
      </ekz>
      <ekz>
        <tld/>i sangon<fnt>B</fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sput.0i">sputi</ref>
    </refgrp>
  </snc>
  <snc mrk="kracx.0i.FIGURE">
  <uzo tip="stl">FIG</uzo>
    <dif>
      El&jcirc;eti:
      <ekz>
        la naturo el<tld/>u min kiel ekstermindan beston, se mi rompos
        tiun &ccirc;i &jcirc;uron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iudiman&ccirc;e la urbo kvaza&ubreve; el<tld/>as sian tutan lo&gcirc;antaron
        en la
        &ccirc;irka&ubreve;a&jcirc;on;
      </ekz>
      <ekz>
        kiu supren <tld/>on &jcirc;etas, sian barbon al &gcirc;i
        submetas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">escopir</trd>
  <trdgrp lng="de">
    <trd>spucken</trd>,
    <trd>speien</trd>,
    <trd>ausspucken</trd>,
    <trd>ausspeien</trd>
  </trdgrp>
  <trd lng="en">spit</trd>
  <trd lng="es">escupir</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_f; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_f; &a_A;&a_n;&a_d;&a_A;&a_H;&a_t;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">cracher</trd>
  <trd lng="hu">k&ouml;p</trd>
  <trd lng="nl">spugen</trd>
  <trdgrp lng="pt">
    <trd>escarrar</trd>,
    <trd>cuspir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kracx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kracx.0ajxo.FIZ">
  <uzo tip="fak">FIZ</uzo>
    <dif>
      Muko, salivo, kiun iu el<tld/>is:
      <ekz>
        li <tld/>is sur la teron, kaj faris el la <tld/>a&jcirc;o koton
        <fnt><bib>NT</bib>, <lok>&Joh; 9:6</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_v;&c_o;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>escopinada</trd>,
    <trd>salivada</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escupidura</trd>,
    <trd>escupitajo<klr> (coloquial)</klr></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_f;</trd>,
    <trd>&a_A_madda;&a_b; &a_d;&a_h;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crachat</trd>,
    <trd>expectoration</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;pet</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_v;&c_o;&c_k;</trd>
</drv>

<drv mrk="kracx.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Poto por ricevi <tld/>a&jcirc;ojn:
      <ekz>
        tri paroj da oraj <tld/>ujoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">escopidora</trd>
  <trd lng="es">escupidera</trd>
  <trd lng="fa">&a_t;&a_f;&zwnj;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">crachoir</trd>
  <trd lng="hu">k&ouml;p&odblac;cs&eacute;sze</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
