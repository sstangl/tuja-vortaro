<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: celesten.xml,v 1.9 2015/10/15 13:10:16 revo Exp $">
<kap>
  <rad>Celesten</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="celesten.0o">
  <kap><tld/>o</kap>
  <snc mrk="celesten.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o <klr>...</klr> brilis en la tuta mondo kiel la unua el
        la episkopoj
        <fnt>
        <aut>P. Ottaviano</aut>,
        <vrk><url
        ref="http://www.murialdo.it/didaskaleion/esper/fund10.htm">
        La fundamentoj de la kristanismo, 10</url></vrk>,
        <lok>2006-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Celest&iacute;</trd>
  <trd lng="de">Coelestin</trd>
  <trdgrp lng="en">
    <trd>Celestin</trd>,
    <trd>Celestine</trd>
  </trdgrp>
  <trd lng="fr">C&eacute;lestin</trd>
  <trd lng="hu">Celesztin</trd>
  <trd lng="it">Celestino</trd>
  <trd lng="pl">Celestyn</trd>
</drv>
</art>

