<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fakir.xml,v 1.20 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fakir</rad>/o
</kap>
<drv mrk="fakir.0o">
  <kap><tld/>o</kap>
  <snc mrk="fakir.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Hinda mahometana piulo celanta perfekti&gcirc;on per korpaj
      rigora&jcirc;oj:
      <ekz>
        venis al la palaco <tld/>o kaj petis almozojn
        <fnt>
          <aut>lakshmiswar Sinha</aut>,
          <vrk><url ref="http://82.182.30.179/inko/111-9.pdf">Tri
          bengalaj fabeloj</url></vrk>,
          <lok>1930</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_f;&c_a;&c_k;&c_i;&c_r;</trd>
  <trd lng="ca">faquir</trd>
  <trdgrp lng="cs">
    <trd>fakír</trd>,
    <trd>sálik</trd>
  </trdgrp>
  <trd lng="de">Fakir</trd>
  <trd lng="en">fakir</trd>
  <trd lng="es">faquir</trd>
  <trd lng="fr">fakir</trd>
  <trd lng="hu">fak&iacute;r</trd>
  <trd lng="nl">fakir</trd>
  <trd lng="pl">fakir</trd>
  <trd lng="pt">faquir</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_i;&c_r;</trd>
  <trd lng="sk">fakír</trd>
  <trd lng="sv">fakir</trd>
  <trd lng="tr">fakir</trd>
</drv>
</art>
