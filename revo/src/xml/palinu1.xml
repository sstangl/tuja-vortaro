<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: palinu1.xml,v 1.9 2013/08/31 16:10:26 revo Exp $">
<kap><rad>palinured</rad>/oj</kap>

<drv mrk="palinu1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref> (<trd lng="la">Palinuridae</trd>),
      sen pin&ccirc;iloj kaj kun dika, malmola karapaco.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_n;&c_g;&c_u;&c_s;&c_t;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Langusten</trd>
  <trd lng="hu">langusztaf&eacute;l&eacute;k</trd>
  <trd lng="nl">langoesten<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">palinur&iacute;deos</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>  
</drv>
</art>

