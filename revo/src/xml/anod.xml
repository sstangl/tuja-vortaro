<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anod.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anod</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Pozitiva
      <ref tip="super" cel="elektr3.0o">elektrodo</ref> de
      <ref tip="malprt" cel="pil.0o">pilo</ref>,
      <ref tip="malprt" cel="valv.0o.ELE">valvo</ref> a&ubreve; alia
      elektra aparato.
    </dif>
    <refgrp tip="ant">
      <ref cel="katod.0o">katodo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_o;&c_d;</trd>
  <trd lng="bg"></trd>
  <trdgrp lng="cs">
    <trd>anoda</trd>,
    <trd>kladná elektroda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pluspol</trd>,
    <trd>Anode</trd>
  </trdgrp>
  <trd lng="en">anode</trd>
  <trd lng="es">&aacute;nodo</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_n;&a_d;</trd>,
    <trd>&a_q;&a_T;&a_b; &a_m;&a_th;&a_b;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">anode</trd>
  <trd lng="hu">an&oacute;d</trd>
  <trd lng="nl">anode</trd>
  <trd lng="pl">anoda</trd>
  <trd lng="ru">&c_a;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>an&oacute;da</trd>,
    <trd>anóda</trd>
  </trdgrp>
  <trd lng="sv">anod</trd>
</drv>
</art>
