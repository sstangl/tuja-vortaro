<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: informatik.xml,v 1.10 2015/10/13 09:10:35 revo Exp $">
<kap>
  <rad>informatik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="informatik.0o">
  <kap><tld/>o</kap>
  <snc mrk="informatik.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="komputik.0o.KOMP">komputiko</ref>
    <ekz>
      kelkfoje mi havas problemojn pensi logike, ver&scirc;ajne &ccirc;ar mi
      studis 
      unu jaron <tld/>on  
      <fnt>
      <aut>E. Gardavsky</aut>,
      <vrk>kiel oni ne skribu amleteron</vrk>,
      <lok><url 
      ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
      1999-11-18</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">inform&agrave;tica</trd>
  <trdgrp lng="cs">
    <trd>informatika</trd>,
    <trd>nauka o informac&iacute;ch</trd>
  </trdgrp>
  <trd lng="de">Informatik</trd>
  <trdgrp lng="en">
    <trd>information technology</trd>,
    <trd>information science</trd>
  </trdgrp>
  <trd lng="fr">informatique<klr> (subst.)</klr></trd>
  <trd lng="nl">informatica</trd>
  <trd lng="pl">informatyka</trd>
  <trd lng="pt">inform&aacute;tica</trd>
  <trd lng="sk">informatika</trd>
</drv>

<drv mrk="informatik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="informatik.0isto.SCI">
    <ref tip="dif" cel="komput.0isto.KOMP">komputisto</ref>
    <ekz>
      kiel <tld/>isto mi multe laboras kun datumbazoj
      <fnt>
        <aut>santvi</aut>,
        <vrk><url 
        ref="http://www.esperanto-es.net/hejs/forumo">[Hejs-forumo]</url></vrk>,
        <lok>2005-12-22</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">inform&agrave;tic</trd>
  <trd lng="de">Informatiker</trd>
  <trdgrp lng="en">
    <trd>information technologist</trd>,
    <trd>information scientist</trd>
  </trdgrp>
  <trd lng="fr">informaticien</trd>
  <trd lng="nl">informaticus</trd>
  <trd lng="pl">informatyk</trd>
</drv>
</art>

