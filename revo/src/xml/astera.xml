<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: astera.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>asterac</rad>/oj
</kap>
<drv mrk="astera.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="astera.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj, kies floroj estas dense kunigitaj kvaza&ubreve;e en kapeto
      (<trd lng="la">Asteraceae</trd>).
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>aster&agrave;cies</trd>,
    <trd>compostes</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hvězdicovité</trd>,
    <trd>složnokvěté <klr>(čeleď)</klr></trd>
  </trdgrp>
  <trd lng="de">Korbbl&uuml;tengew&auml;chse</trd>
  <trd lng="en">asteraceae</trd>
  <trd lng="fr">ast&eacute;rac&eacute;es</trd>
  <trd lng="hu">f&eacute;szkesvir&aacute;gzat&uacute;ak</trd>
  <trd lng="it">asteracee</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hviezdicovité</trd>,
    <trd>zložnokveté</trd>
  </trdgrp>
  <trd lng="sv">korgblommiga v&auml;xter</trd>
</drv>
</art>

