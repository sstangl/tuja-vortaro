<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: merd.xml,v 1.7 2015/07/04 08:37:39 revo Exp $">
<kap>
<rad>merd</rad>/o
</kap>
<drv mrk="merd.0o">
  <kap><tld/>o</kap> 
  <uzo tip="stl">EVI</uzo>
  <snc>
    <ref tip="dif" cel="fek.0ajxo">feka&jcirc;o</ref>
    <dif>
    <ekz>
    estas hunda <tld/>o sur la strato;
    </ekz>
    <ekz>
    tiu libro estas <tld/>o.
    </ekz>
    </dif>
  </snc>
  <trd lng="ca">merda</trd>
  <trdgrp lng="cs">
    <trd>fekálie</trd>,
    <trd>hovno</trd>,
    <trd>lejno</trd>
  </trdgrp>
  <trd lng="fr">merde</trd>
  <trdgrp lng="nl">
    <trd>kak<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>poep<klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fekálie</trd>,
    <trd>hovno</trd>,
    <trd>lajno</trd>
  </trdgrp>
</drv>
</art>

