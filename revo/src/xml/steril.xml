<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: steril.xml,v 1.12 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>steril</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="steril.0a">
  <kap><tld/>a</kap>
  <snc mrk="steril.0a.KOMUNE">
    <dif>
      Senfrukta, nekapabla naski a&ubreve; produkti:
      <ekz>
        <tld/>a lando, bovino, planto, jaro, peno, diskuto.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ja;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>
    <trdgrp lng="fa">
      <trd>&a_n;&a_A;&a_z;&a_A;</trd>,
      <trd>&a_ayn;&a_q;&f_y;&a_m;</trd>,
      <trd>&a_n;&a_A;&a_H;&a_A;&a_S;&a_l;&a_kh;&f_y;&a_z;</trd>,
      <trd>&a_b;&a_A;&f_y;&a_r;</trd>,
      <trd>&a_l;&a_m;&zwnj;&f_y;&a_z;&a_r;&a_ayn;</trd>,
      <trd>&a_b;&f_y;&zwnj;&a_th;&a_m;&a_r;</trd>,
      <trd>&a_b;&f_y;&zwnj;&a_H;&a_A;&a_S;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>medd&odblac;</trd>,
      <trd>magtalan</trd>,
      <trd>term&eacute;ketlen</trd>
    </trdgrp>
    <trd
    lng="ru">&c_b;&c_je;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="steril.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Libera je &ccirc;ia viva &gcirc;ermo, speciale je
      <ref tip="vid" cel="mikrob.0o">mikroboj</ref>,
      <ref tip="vid" cel="bacil.0o">baciloj</ref>
      a&ubreve;
      <ref tip="vid" cel="bakter.0o">bakterioj</ref>:
      <ekz>
        <ind><tld/>a pansa&jcirc;o</ind>.
        <trd lng="hu">steril k&ouml;t&eacute;s</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="fa">
      <trd>&a_D;&a_d;&a_ayn;&a_f;&a_w;&a_n;&f_y; &a_sh;&a_d;&a_h;</trd>,
      <trd>&a_A;&a_s;&a_t;&a_r;&f_y;&a_l;</trd>
    </trdgrp>
    <trd lng="hu">steril</trd>    
    <trd
    lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>jalový</trd>,
    <trd>neplodný</trd>,
    <trd>sterilní</trd>
  </trdgrp>
  <trd lng="de">steril</trd>
  <trd lng="en">sterile</trd>
  <trd lng="fa">&a_s;&a_t;&a_r;&a_w;&a_n;</trd>
  <trd lng="fr">st&eacute;rile</trd>
  <trd lng="nl">steriel</trd>
  <trdgrp lng="sk">
    <trd>neplodný</trd>,
    <trd>sterilný</trd>,
    <trd>zdravotne nezávadný</trd>
  </trdgrp>
</drv>

<drv mrk="steril.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="steril.0igi.BIO">
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi lakton;
      </ekz>
      <ekz>
        <ind><tld/>igilo</ind>.
        <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
        <trd lng="fr">st&eacute;rilisateur</trd>
        <trd lng="hu">steriliz&aacute;tor</trd>
        <trd
        lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>sterilizovat</trd>,
    <trd>způsobit neplodnost</trd>
  </trdgrp>
  <trd lng="de">sterilisieren</trd>
  <trd lng="en">sterilize</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_t;&a_r;&a_w;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_D;&a_d;&a_ayn;&a_f;&a_w;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_r;&f_y;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">st&eacute;riliser</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;r&aacute;tlan&iacute;t</trd>,
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>steriliz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">steriliseren</trd>
  <trd
  lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">sterilizovať</trd>
</drv>
</art>
