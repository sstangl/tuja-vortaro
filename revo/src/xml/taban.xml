<art mrk="$Id: taban.xml,v 1.22 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>taban</rad>/o
</kap>
<drv mrk="taban.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="tabane.0oj"><tld/>edoj</ref>
      (<trd lng="la">Tabanus</trd>), granda mu&scirc;simila insekto, kies
      inoj pikas prefere la brutojn:
      <ekz>
        &gcirc;ena&jcirc;oj en somerdoma feli&ccirc;o estas kuloj, 
mu&scirc;oj, formikoj kaj foje
        <tld/>oj<fnt><aut>Saliko</aut>: <vrk>&Gcirc;ojoj kaj &gcirc;uoj de 
finna
        somero</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 12a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_shch;&c_r;&c_malmol;&c_k;&c_l;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>t&agrave;vec</trd>,
    <trd>tave</trd>
  </trdgrp>
  <trd lng="cs">ov√°d</trd>
  <trd lng="de">Bremse</trd>
  <trdgrp lng="fr">
    <trd>taon</trd>
  </trdgrp>
  <trd lng="hu">b&ouml;g&ouml;ly</trd>
  <trd lng="it">tafano</trd>
  <trd lng="nl">horzel</trd>
  <trd lng="pl">b&aogonek;k <klr>(owad)</klr></trd>
  <trdgrp lng="pt">
   <trd>tav&atilde;o</trd>,
    <trd>mutuca</trd>
    <trd>butuca</trd>,
    <trd>moscardo</trd>,
    <trd>motuca</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">ovad</trd>
  <trd lng="sv">broms</trd>
</drv>
</art>
