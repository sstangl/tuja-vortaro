<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkav.xml,v 1.24 2012/01/01 20:10:20 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konkav</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="konkav.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Prezentanta kavan randon, kiel interno de cirklo a&ubreve; sfero:
      <ekz>
        la <tld/>a supra&jcirc;o de okulvitro por miopulo.
      </ekz>
    </dif>
    <trd lng="en">concave</trd>
    <trd lng="es">c&oacute;ncavo</trd>
    <trd lng="fr">concave</trd>
    <trdgrp lng="nl">
      <trd>concaaf</trd>,
      <trd>hol</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="konkav.0a.subaro">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="figur.geometria0o">geometria figuro</ref>,
        subaro de <ref cel="spac.afina0o">afina spaco</ref>, a&ubreve; de
        <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
        Ne <ref cel="konvek.0a.subaro">konveksa</ref>:
        <ekz>
          <tld/>a <ref cel="angul.0o.figuro">angulo</ref> <klr>(pli
          granda ol <ref cel="strecx.0ita.angulo">stre&ccirc;ita</ref>
          angulo)</klr>,
          <ref cel="later.plur0o">plurlatero</ref>,
          <ref cel="edr.plur0o">pluredro</ref>.
        </ekz>
      </dif>
      <refgrp tip="ant">
        <ref cel="konvek.0a.subaro">konveksa</ref>.
      </refgrp>
      <rim>
        <tld lit="K"/>ajn angulojn oni foje nomas
        <ctl>transobtuzaj</ctl>
        a&ubreve; <ctl>superstre&ccirc;itaj</ctl> (amba&ubreve; en
        <fnt><bib>MatVort</bib></fnt>).
      </rim>
      <trd lng="de">konkav<klr tip="ind"> (-e Untermenge)</klr></trd>
      <trd lng="en">concave<klr tip="ind"> (subset)</klr></trd>
      <trd lng="fr"><klr tip="ind">(sous-ensemble) </klr>concave</trd>
      <trd lng="nl">concaaf</trd>
      <trd lng="pl"><klr tip="ind">(zbi&oacute;r) </klr>wkl&eogonek;s&lstroke;y</trd>
      <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_o;&c_je; <klr
      tip="amb">(&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="konkav.0a.bildigo">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>
        <frm><k>f</k></frm>
        de la aro de <ref cel="reel.0o.MAT">reeloj</ref> al si
        mem)</klr>
        Tia, ke &gcirc;ia <ref cel="egal.kontraux0o">kontra&ubreve;egalo</ref>
        estas <ref cel="konvek.0a.bildigo">konveksa</ref>.
      </dif>
      <refgrp tip="ant">
        <ref cel="konvek.0a.bildigo">konveksa</ref>.
      </refgrp>
      <trd lng="de">konkav<klr tip="ind"> (-e Funktion)</klr></trd>
      <trd lng="en">concave<klr tip="ind"> (function)</klr></trd>
      <trd lng="es">c&oacute;ncavo</trd>
      <trd lng="fr"><klr tip="ind">(fonction) </klr>concave</trd>
      <trd lng="nl">concaaf</trd>
      <trd lng="pl"><klr tip="amb">(funkcja) </klr>wkl&eogonek;s&lstroke;a</trd>
      <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_a;&c_ja;<klr tip="amb">
      (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_v;&c_a;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>,
    <trd>&c_u;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">c&ograve;ncau</trd>
  <trd lng="pt">c&ocirc;ncavo</trd>
  <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
</drv>
</art>
