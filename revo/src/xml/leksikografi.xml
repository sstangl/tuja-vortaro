<kap>
  <rad>leksikografi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leksikografi.0o">
  <kap><tld/>o</kap>
  <snc mrk="leksikografi.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Arto fari <ref tip="vid" cel="vort.0aro">vortarojn</ref>:
      <ekz>
        por moderna kaj altkvalita priskriba <tld/>o necesas uzi
        komputilajn tekstarojn
        <fnt>
          <aut>E. Grimley-Evans</aut>,
          <vrk>Re: Esperanto: A Planned Language, naturally
          evolving...</vrk>,
          <lok>soc.culture.esperanto, 1994-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">lexicografia</trd>
  <trdgrp lng="cs">
    <trd>lexikografie</trd>,
    <trd>slovníkářství</trd>
  </trdgrp>
  <trd lng="es">lexicograf&iacute;a</trd>
  <trd lng="fa">&a_f;&a_r;&a_h;&a_n;&f_g;&zwnj;&a_n;&a_w;&f_y;&a_s;&f_y;</trd>
  <trd lng="fr">lexicographie</trd>
  <trdgrp lng="hu">
    <trd>lexikogr&aacute;fia</trd>,
    <trd>sz&oacute;t&aacute;r&iacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">lexicografie</trd>
  <trd lng="pt">lexicografia</trd>
  <trd lng="sk">slovnikárstvo</trd>
</drv>
</art>

