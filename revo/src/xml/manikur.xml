<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: manikur.xml,v 1.5 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>manikur</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="manikur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="manikur.0i.KOS">
    <dif>
      Manflegi:
      <ekz>
        mi ne kredas, ke tia birdo <tld/>us siajn ungojn sola
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
          <vrk><url ref="">La dekkvar karata a&#468;tomobilo</url></vrk>,
          <lok>2002-03-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino &scirc;in vestas, kombas, nutras, <tld/>as kaj banas
        <fnt>
          <aut>&Scirc;ja&ubreve;&scirc;ja&ubreve;</aut>,
          <vrk><url ref="http://www.ebudhano.cn/cn/blog/">E-budhana
Retejo</url></vrk>,
          <lok>2005-09-27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pedikur.0i.KOS">pedikuri</ref>
    </refgrp>
  </snc>
  <trd lng="ca">fer la manicura</trd>
  <trdgrp lng="cs">
    <trd>manikúrovat</trd>,
    <trd>manikýrovat</trd>,
    <trd>provádět manikúru</trd>
  </trdgrp>
  <trd lng="fr">manucurer</trd>
  <trd lng="nl">manicuren</trd>
  <trd lng="sk">manikúrovať</trd>
</drv>

<drv mrk="manikur.0o">
  <kap><tld/>o</kap>
  <snc mrk="manikur.0o.KOS">
    <dif>
      Manflego.
    </dif>
  </snc>
  <trd lng="ca">manicura</trd>
  <trd lng="cs">manikúra</trd>
  <trd lng="fr">manucure<klr> (activit&eacute;)</klr></trd>
  <trd lng="nl">manicure</trd>
  <trd lng="sk">manikúra</trd>
</drv>

<drv mrk="manikur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="manikur.0isto.KOS">
    <dif>
      Manflegisto:
      <ekz>
        la du viroj estis &leftquot;&Ccirc;efoj de la <tld lit="M"/>istoj en la
Re&gcirc;a
        Palaco&rightquot;
        <fnt>
        <aut>K. Lendon</aut>,
        <vrk>Pri samseksamo en antikva Egiptio</vrk>,
        <lok><url
       
ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
        2001-03-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">manicur</trd>
  <trd lng="fr">manucure<klr> (personne)</klr></trd>
  <trd lng="nl">manicuurder</trd>
</drv>
</art>

