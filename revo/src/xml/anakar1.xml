<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anakar1.xml,v 1.7 2013/08/26 17:10:16 revo Exp $">
<kap>
  <rad>anakardiac</rad>/oj
</kap>
<drv mrk="anakar1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Anacardiaceae</trd>), tropikaj
      lignoplantoj.
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_m;&c_r;&c_a;&c_d;&c_l;&c_i;&c_k;&c_o;&c_v;&c_i;</trd>
  <trd lng="de">Sumachgew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>anacardiaceae</trd>,
    <trd>cashew family</trd>,
    <trd>sumac family</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&ouml;m&ouml;rcef&eacute;l&eacute;k</trd>,
    <trd>kesufaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">pruikenboomfamilie</trd>
  <trd lng="pl">nanerczowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_h;&c_o;&c_v;&c_y;&c_je;</trd>, 
    <trd>&c_a;&c_n;&c_a;&c_k;&c_a;&c_r;&c_d;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

