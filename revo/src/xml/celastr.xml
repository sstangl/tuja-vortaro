<art mrk="$Id: celastr.xml,v 1.13 2013/08/25 13:10:30 revo Exp $">
<kap>
  <rad>celastrac</rad>/oj
</kap>
<drv mrk="celastr.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Celastraceae</trd>).
    </dif>
  </snc>
  <trd lng="ca">celastr&agrave;cies</trd>
  <trd lng="de">Spindelbaumgew&auml;chse</trd>
  <trd lng="fr">c&eacute;lastrac&eacute;es</trd>
  <trd lng="hu">kecsker&aacute;g&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="it">celastracee</trd>
  <trd lng="pl">trzmielinowate</trd>
  <trd lng="pt">celastr&aacute;ceas</trd>
  <trd
  lng="ru">&c_b;&c_je;&c_r;&c_je;&c_s;&c_k;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_je;
  <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
