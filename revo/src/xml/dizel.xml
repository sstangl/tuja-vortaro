<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dizel.xml,v 1.18 2015/09/18 13:10:18 revo Exp $">
<kap>
  <rad>dizel</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="dizel.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="motor.0o.TEK">Motoro</ref> kun interna
      brulado, en kiu la brula&jcirc;o, injektita en forte densigitan
      aeron, spontane ekbrulas.
    </dif>
  </snc>
  <trd lng="en">diesel motor</trd>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;</trd>,
    <trd>
    &c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_y;
    &c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;
    </trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_z;&c_je;&c_l;&c_o;&c_v; &c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;</trd>
  <trd lng="ca">motor di&egrave;sel</trd>
  <trdgrp lng="cs">
    <trd>diesel</trd>,
    <trd>dieselov&yacute; motor</trd>
  </trdgrp>
  <trd lng="de">Dieselmotor</trd>
  <trd lng="el">&Ny;&tau;&jota;&zeta;&epsilon;&lambda;&omikron;&my;&eta;&chi;&alfa;&ny;&eta_ton;</trd>
  <trd lng="es">motor <ind>disel</ind></trd>
  <trd lng="fr">diesel<klr tip="ind"> (moteur)</klr></trd>
  <trd lng="he">&dalet;&yod;&zayin;&lamed;<klr tip="amb">
  (&mem;&nun;&vav;&ayin;)</klr></trd>
  <trd lng="hu">d&iacute;zelmotor</trd>
  <trd lng="it">diesel<klr> (motore)</klr></trd>
  <trd lng="nl">dieselmotor</trd>
  <trdgrp lng="pl">
    <trd>dizel<klr> (pot.)</klr></trd>,
    <trd>silnik Diesla</trd>,
    <trd>silnik wysokopr&eogonek;&zdot;ny</trd>
  </trdgrp>
  <trd lng="pt">motor diesel, motor a diesel</trd>
  <trd lng="ru">&c_d;&c_i;&c_z;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">dieselov&yacute; motor</trd>
</drv>
</art>
