<art mrk="$Id: angile.xml,v 1.6 2013/08/31 14:10:29 revo Exp $">
<kap>
  <rad>angiled</rad>/oj
</kap>
<drv mrk="angile.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="angil.0oformaj">angiloformaj</ref>
      fi&scirc;oj (<trd lng="la">Anguillidae</trd>), kun muka, skvama
      ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="ca">angu&iacute;l&middot;lids</trd>
  <trd lng="de">Aale</trd>
  <trd lng="hu">angolnaf&eacute;l&eacute;k</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_ch;&c_n;&c_y;&c_je; <ind>&c_u;&c_g;&c_r;&c_i;</ind>
<klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_s;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;
<ind>&c_u;&c_g;&c_r;&c_i;</ind> <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

