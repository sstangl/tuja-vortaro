<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekal.xml,v 1.23 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sekal</rad>/o
</kap>

<drv mrk="sekal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sekal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd
      lng="la">Secale</trd>).
    </dif>
    <trd lng="hu">rozs</trd>    
  </snc>
  <snc mrk="sekal.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> el la genro <ref
      tip="super"
      cel="sekal.0o.BOT"><tld/>o</ref>
      (<trd lng="la">Secale cereale</trd>), kulturata kiel <ref
      tip="super" cel="gren.0o.planto">greno</ref> por produkti grizan
      farunon, uzatan precipe por <ref tip="malprt"
      cel="pan.0o.mangxo">pano</ref>:
      <ekz>
        flugis odoro de varma <tld/>a pano
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 1a, p..
        8a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges rozs</trd>,
      <trd>termesztett rozs</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_t;&c_a;</trd>
  <trd lng="bg">&c_r;&c_malmol;&c_zh;</trd>
  <trd lng="ca">s&egrave;gol</trd>
  <trdgrp lng="cs">
    <trd>rež</trd>,
    <trd>žito</trd>
  </trdgrp>
  <trd lng="de">Roggen</trd>
  <trd lng="en">rye</trd>
  <trd lng="es">centeno</trd>
  <trdgrp lng="fr">
    <trd>seigle</trd>
  </trdgrp>
  <trd lng="it">segale</trd>
  <trd lng="nl">rogge</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_mol;</trd>
  <trd lng="sk">raž</trd>
  <trd lng="sv">r&aring;g</trd>
</drv>
</art>
