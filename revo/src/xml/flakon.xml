<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flakon.xml,v 1.15 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>flakon</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="flakon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="botel.0o">Boteleto</ref> por parfumoj,
      medikamentoj, likvoroj, plej ofte fermita per &gcirc;ustege
      adaptita &scirc;topilo el sama materio. 
    </dif>
    <refgrp tip="vid">
      <ref cel="karaf.0o">karafo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_k;&c_o;&c_n;</trd>
  <trd lng="cs">flakon</trd>
  <trdgrp lng="de">
     <trd>Flakon</trd>,
     <trd>Parf&uuml;mfl&auml;schchen</trd></trdgrp>
  <trd lng="en">flacon</trd>
  <trdgrp lng="fr">
    <trd>flacon</trd>,
    <trd>fiole</trd></trdgrp>
  <trdgrp lng="hu">
     <trd>flakon</trd>,
     <trd>&uuml;vegcse</trd>,
     <trd>fiola</trd></trdgrp>
  <trd lng="nl">flacon</trd>
  <trd lng="pl">flakon</trd>
  <trdgrp lng="pt">
    <trd>frasco</trd>,
    <trd>vidro</trd>,
    <trd>garrafinha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_l;&c_a;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_u;&c_z;&c_y;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>flakón</trd>,
    <trd>fľaštička na voňavku</trd>
  </trdgrp>
</drv>
</art>
