<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kapor.xml,v 1.16 2012/09/01 18:10:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapor</rad>/o
</kap>
<drv mrk="kapor.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Capparis</trd>).
      La florbur&gcirc;onojn de iu specio oni konservas en
      vinagro kaj uzas kiel spica&jcirc;on.
    </dif>
    <trd lng="br">gwezenn-gaprez</trd>
    <trd lng="de">Kapernstrauch</trd>
    <trd lng="en">caper bush</trd>
    <trd lng="fr">c&acirc;prier</trd>
    <trd lng="hu">kapri<klr tip="ind"> (cserje)</klr></trd>
    <trd lng="nl">kapperboom</trd>
  </snc>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>a bur&gcirc;ono, konservita en
      vinagro kaj uzata kiel spica&jcirc;o.
    </dif>    
    <trd lng="de">Kaper</trd>
    <trd lng="en">caper</trd>
    <trd lng="fr">c&acirc;pre</trd>
    <trd lng="hu">kapribogy&oacute;</trd>
    <trd lng="nl">kapper<klr tip="ind"> (specerij)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_e;&c_r;&c_s;</trd> 
  <trd lng="br">kaprezenn</trd>
  <trd lng="ca">t&agrave;pera</trd>
  <trd lng="es">alcaparra</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_je;&c_r;&c_s;</trd>
</drv>
</art>
