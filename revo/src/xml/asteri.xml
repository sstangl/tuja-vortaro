<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: asteri.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>asteri</rad>/o
</kap>
<drv mrk="asteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="asteri.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="stel.mar0o.ZOO">marstelo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mořská hvězda</trd>,
    <trd>mořská hvězdice</trd>
  </trdgrp>
  <trd lng="de">Seestern</trd>
  <trd lng="en">starfish</trd>
  <trd lng="es">estrella de mar</trd>
  <trdgrp lng="fr">
    <trd>ast&eacute;rie</trd>,
    <trd>&eacute;toile de mer</trd>
  </trdgrp>
  <trd lng="he">&kaf;&vav;&kaf;&bet;-&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>tengeri csillag</trd>,
    <trd>csillag&aacute;llatka</trd>
  </trdgrp>
  <trd lng="it"><ind>stella</ind> di mare</trd>
  <trd lng="pt">estrela do mar</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">morská hviezdica</trd>
</drv>
</art>
