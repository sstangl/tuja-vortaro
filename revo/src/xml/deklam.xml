<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: deklam.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>deklam</rad>/i
</kap>
<drv mrk="deklam.0i">
  <kap><tld/>i</kap>
  <snc mrk="deklam.0i.POE">
    <uzo tip="fak">POE</uzo>
    <gra><vspec>tr</vspec></gra>
    <dif>
      La&ubreve;te vo&ccirc;legi a&ubreve; reciti, kun artisme
      ta&ubreve;ga tono:
      <ekz>
        <tld/>i versojn;
      </ekz>
      <ekz>
        Nolan siavice transprenis la libron kaj <tld/>is al la ceteraj,
        kaj li tre bone povis <tld/>i
        <fnt>
        <aut>Edward Everett Hale, trad. E. L. Robinson, trad. Don
        Harlow</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html">La
        viro senpatria</url></vrk>,
        <lok>1907 kaj 1994</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>deklamieren</trd>,
      <trd>vortragen</trd>,
      <trd>aufsagen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>deklamowa&cacute;</trd>,
      <trd>recytowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(absolute)</uzo>
    <dif>
      Paroli per pasie emfaza tono:
      <ekz>
        oni multe <tld/>is pri sendependeco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beschw&ouml;ren</trd>,
      <trd>gro&szlig;e Worte machen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>u&zdot;ywa&cacute; wielkich
      <ind>s&lstroke;&oacute;w</ind></trd>,
      <trd>u&zdot;ywa&cacute; okr&aogonek;g&lstroke;ych
      <ind>zda&nacute;</ind></trd>,
      <trd>m&oacute;wi&cacute; okr&aogonek;g&lstroke;ymi
      <ind>zdaniami</ind></trd>
    </trdgrp>
  </snc>
  <trd
  lng="be">&c_d;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">declamar</trd>
  <trdgrp lng="cs">
    <trd>deklamovat</trd>,
    <trd>přednášet</trd>,
    <trd>recitovat</trd>,
    <trd>skandovat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>declaim</trd>,
    <trd>recite</trd>
  </trdgrp>
  <trd lng="es">declamar</trd>
  <trd lng="fr">d&eacute;clamer</trd>
  <trd lng="hu">szaval</trd>
  <trdgrp lng="pt">
    <trd>declamar</trd>,
    <trd>recitar</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_je;&c_k;&c_l;&c_a;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prednášať</trd>,
    <trd>recitovať</trd>
  </trdgrp>
</drv>

<drv mrk="deklam.ekspon0i">
  <kap>ekspon<tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Ccirc;e foiro, bazaro ks, provi allogi la pasantojn, <tld/>ante
      pri la bonaj ecoj de la vendota&jcirc;o.
    </dif>
  </snc>
  <trd lng="ca">fer bombo<klr> (al mercat)</klr></trd>
  <trd lng="de">anpreisen</trd>
  <trdgrp lng="en">
    <trd>hype</trd>,
    <trd>hawk<klr> (wares)</klr></trd>
  </trdgrp>
  <trd lng="fr">bonimenter</trd>
  <trdgrp lng="hu">
    <trd>vev&odblac;t csalogat</trd>,
    <trd>kiki&aacute;lt <klr>(v&aacute;s&aacute;rban)</klr></trd>
  </trdgrp>
  <trd lng="pl">zachwala&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
  &c_p;&c_o;&c_k;&c_u;&c_p;&c_a;&c_t;&c_je;&c_l;&c_ja;</trd>
</drv>
</art>
