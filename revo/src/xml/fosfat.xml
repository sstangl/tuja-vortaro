<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: fosfat.xml,v 1.19 2015/07/04 08:37:34 revo Exp $">
    <kap>
      <rad>fosfat</rad>/o
    </kap>
    <drv mrk="fosfat.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="sal.0o.KEM">Salo</ref> de <tld/>a acido.
           
        </dif>
      </snc>
      <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_t;</trd>
      <trd lng="bg">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
      <trd lng="ca">fosfat</trd>
      <trd lng="de">Phosphat</trd>
      <trd lng="en">phosphate</trd>
      <trd lng="es">fosfato</trd>
      <trd lng="fr">phosphate</trd>
      <trd lng="hu">foszf&aacute;t</trd>
      <trd lng="nl">fosfaat</trd>
      <trd lng="pt">fosfato</trd>
      <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
      <trd lng="sv">fosfat</trd>
    </drv>
    <drv mrk="fosfat.0aacido">
      <kap><tld/>a acido</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="acid.0o">Acido</ref> el
          <ref cel="fosfor.0o">fosforo</ref>, precipe
          <frm>H<sub>3</sub>PO<sub>4</sub></frm>.
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>,
        <trd>&c_a;&c_r;&c_t;&c_a;&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
      </trdgrp>
  <trd lng="bg">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a; &c_k;&c_i;&c_s;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">&agrave;cid fosf&ograve;ric</trd>
  <trd lng="cs">kyselina fosforečná</trd>
  <trd lng="de">Phosphats&auml;ure</trd>
  <trd lng="en">phosphoric acid</trd>
  <trd lng="fr">acide phosphorique</trd>
  <trd lng="hu">foszforsav</trd>
  <trd lng="nl">fosforzuur</trd>
  <trd lng="pt">&aacute;cido fosf&oacute;rico</trd>
  <trdgrp lng="ru">
        <trd>&c_o;&c_r;&c_t;&c_o;&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>,
        <trd>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
      </trdgrp>
  <trd lng="sk">kyselina fosforová</trd>
  <trd lng="sv">fosforsyra</trd>
</drv>
  </art>
