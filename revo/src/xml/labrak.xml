<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: labrak.xml,v 1.9 2013/10/06 18:10:19 revo Exp $">
<kap>
    <rad>labrak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="labrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="labrak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      kun pufaj lipoj kaj dorna dorsa na&gcirc;ilo,
      fama pro voremo kaj pro tio plurlingve
      alinomata <ctl>lupo</ctl>, <ctl>mara lupo</ctl>...
      (<trd lng="la">Dicentrarchus labrax</trd>);
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref>
      el familio <ref tip="super" cel="morone.0oj">moronedoj</ref>
    </dif>
  </snc>
  <trd lng="bg">&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_a; <ind>&c_l;&c_a;&c_v;&c_r;&c_a;&c_k;</ind></trd>
  <trd lng="ca">llobarro</trd>
  <trd lng="de">Europ&auml;ischer Wolfsbarsch</trd>
  <trd lng="en">European seabass</trd>
  <trdgrp lng="es">
    <trd>lubina</trd>, 
    <trd> sabalo</trd>,
    <trd> r&oacute;balo</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bar<klr> (poisson)</klr></trd>, 
    <trd>loup<klr> (poisson)</klr></trd>,
    <trd>perche de mer</trd>
  </trdgrp>
  <trd lng="nl">Europese <ind>zeebaars</ind></trd>
  <trdgrp lng="pl">
    <trd>labraks</trd>,
    <trd>bar</trd>,
    <trd>moren</trd>,
    <trd>oko&nacute; morski</trd>,
    <trd>wilk morski <klr>(b&lstroke;&eogonek;dnie)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_L;&c_a;&c_v;&c_r;&c_a;&c_k;</trd>
</drv>
</art>

