<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mucid.xml,v 1.18 2016/04/17 07:10:15 revo Exp $">
<kap>
  <rad>mucid</rad>/a
  <fnt>Z</fnt>
</kap>

<drv mrk="mucid.0a">
  <kap><tld/>a</kap>
  <snc mrk="mucid.0a.odora">
    <dif>
      Havanta &scirc;iman odoron a&ubreve; guston:
      <ekz>
        la <tld/>a klaso&ccirc;ambro fermita jam de du monatoj
        <fnt>
          <aut>A. Fournier, trad. R. Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 198</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="haladz.0a">haladza</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_t;&c_h;&c_l;&c_y;</trd>,
      <trd>&c_z; &c_p;&c_a;&c_h;&c_a;&c_m; <ind>&c_p;&c_l;&c_je;&c_s;&c_je;&c_n;&c_ib;</ind></trd>
    </trdgrp>
    <trd lng="bg">&c_m;&c_u;&c_h;&c_l;&c_ja;&c_s;&c_a;&c_l;</trd>
    <trdgrp lng="en">
      <trd>mucid</trd>,
      <trd>musty</trd>,
      <trd>moldy</trd>,
      <trd>foul</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>qui sent le <ind>moisi</ind></trd>,
      <trd>qui sent le <ind>renferm&eacute;</ind></trd>,
      <trd>moisi<klr> (odeur)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>dohos</trd>,
      <trd>pen&eacute;szszag&uacute;</trd>,
      <trd>&aacute;porodott</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>muffig</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>muf</trd>,
      <trd>duf</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>bolorento</trd>,
      <trd>mofado</trd>,
      <trd>ran&ccedil;oso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_t;&c_h;&c_l;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_a;&c_h;&c_n;&c_u;&c_shch;&c_i;&c_j; <ind>&c_p;&c_l;&c_je;&c_s;&c_je;&c_n;&c_mol;&c_ju;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="mucid.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malagrable eksmoda, malfre&scirc;a:
      <ekz>
        kelkaj <tld/>aj &scirc;ercoj verkitaj a&ubreve; tradukitaj de tri
        &leftquot;renomaj&rightquot;
        a&ubreve;toroj<fnt><aut>Paul Veenman</aut>: <vrk>Diverskvalita erotiko
        (-, Antologio de amoro)</vrk>, <bib>Monato</bib><lok>jaro 1996a,
        numero 4a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_ja;&c_v;&c_je;&c_l;&c_y; <klr
      tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_a;&c_s;&c_t;&c_a;&c_r;&c_e;&c_l;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_o;&c_s;&c_t;&c_a;&c_r;&c_ja;&c_l;</trd>
    <trdgrp lng="de">
      <trd>altmodisch</trd>,
      <trd>verstaubt</trd>
    </trdgrp>
    <trd lng="en">musty</trd>
    <trdgrp lng="fr">
      <trd>de <ind>mauvais go&ucirc;t</ind></trd>,
      <trd>&eacute;cul&eacute;<klr> (pass&eacute; de mode)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;satag</trd>,
      <trd>idej&eacute;tm&uacute;lt</trd>
    </trdgrp>
    <trd lng="nl">stoffig</trd>
    <trdgrp lng="pt">
      <trd>bolorento</trd>,
      <trd>mofado</trd>,
      <trd>ran&ccedil;oso</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_n;&c_je;&c_v;&c_je;&c_l;&c_y;&c_j;<klr tip="amb">
      (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>,
      <trd>&c_i;&c_z;&c_b;&c_i;&c_t;&c_y;&c_j;<klr tip="amb"> (&c_o; &c_sh;&c_u;&c_t;&c_k;&c_je; &c_i;
      &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_s; <ind>&c_b;&c_o;&c_r;&c_o;&c_d;&c_o;&c_j;</ind><klr tip="amb"> (&c_o; &c_sh;&c_u;&c_t;&c_k;&c_je; &c_i;
      &c_t;.&c_p;.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>zatuchl&yacute;</trd>,
    <trd>ztuchl&yacute;</trd>
  </trdgrp>
  <trd lng="sk">stuchnut&yacute;</trd>
</drv>
</art>
