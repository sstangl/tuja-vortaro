<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geodez.xml,v 1.21 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>geodezi</rad>/o
</kap>
<drv mrk="geodez.0o">
  <kap><tld/>o</kap>
  <snc mrk="geodez.0o.SCI">
    <uzo tip="fak">GEOD</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako"
      lst="voko:sciencoj">Scienco</ref>, per kiu oni
      difinas
      la grandon kaj formon de la tero a&ubreve; de parto de la tero.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_d;&c_e;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="ca">geod&egrave;sia</trd>
  <trdgrp lng="cs">
    <trd>geodezie</trd>,
    <trd>zeměměřičství</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geod&auml;sie</trd>,
    <trd>Vermessungskunde</trd>
  </trdgrp>
  <trd lng="en">geodesy</trd>
  <trd lng="es">geodesia</trd>
  <trd lng="fa">&f_zh;&a_y_hamza;&a_w;&a_d;&a_z;&f_y;</trd>
  <trd lng="fr">g&eacute;od&eacute;sie</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldm&eacute;r&eacute;stan</trd>,
    <trd>geod&eacute;zia</trd>
  </trdgrp>
  <trd lng="id">geodesi</trd>
  <trdgrp lng="nl">
    <trd>geodesie</trd>,
    <trd>landmeetkunde</trd>
  </trdgrp>
  <trd lng="pl">geodezja</trd>
  <trdgrp lng="pt">
    <trd>geod&eacute;sia</trd>,
    <trd>geodesia</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_d;&c_je;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">geodézia</trd>
</drv>
</art>
