<art mrk="$Id: gen.xml,v 1.19 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>9</ofc><rad>gen</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="gen.0o">
  <kap><tld/>o</kap>
  <snc mrk="gen.0o.GNT">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Hereda faktoro, kiu respondecas pri la reprodukto de unu
      elementa karaktero en la rezultanta individuo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_e;&c_n;</trd>
  <trd lng="ca">gen</trd>
  <trdgrp lng="cs">
    <trd>gen</trd>,
    <trd>vloha</trd>
  </trdgrp>
  <trd lng="de">Gen</trd>
  <trd lng="en">gene</trd>
  <trdgrp lng="es">
    <trd>gen</trd>,
    <trd>gene</trd>
  </trdgrp>
  <trd lng="fa">&f_zh;&a_n;</trd>
  <trd lng="fr">g&egrave;ne</trd>
  <trd lng="hu">g&eacute;n</trd>
  <trd lng="it">gene</trd>
  <trd lng="nl">gen</trd>
  <trd lng="pl">gen</trd>
  <trd lng="pt">gene<klr tip="amb"> (biologia)</klr></trd>
  <trd lng="ru">&c_g;&c_je;&c_n;</trd>
  <trd lng="sk">g√©n</trd>
  <trd lng="sv">gen</trd>
  <trd lng="tr">gen</trd>
</drv>

<drv mrk="gen.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="gen.0aro.GNT">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Tuto de la <tld/>oj de vivulo a&ubreve; de specio:
      <ekz>
        &ccirc;inaj sciencistoj kune esploris kaj plenumis la
        <ctl>skizon pri
        la riza <tld/>aro</ctl>
        <fnt>
        <aut>A. Chu</aut>,
        <vrk><url
        ref="http://www.verdapekino.com/raporto/index4.htm">Nova&jcirc;oj
        de
        scienco</url></vrk>,
        <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">genoma</trd>
  <trdgrp lng="de">
    <trd>Genom</trd>,
    <trd>Erbgut</trd>
  </trdgrp>
  <trd lng="en">genome</trd>
  <trd lng="fa">&f_zh;&a_n;&a_w;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nome</trd>,
    <trd>patrimoine g&eacute;n&eacute;tique</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>genoma</trd>,
    <trd>patrimonio genetico</trd>
  </trdgrp>
  <trd lng="pl">genom</trd>
</drv>
</art>
