<vortaro>
<art mrk="$Id: diasta.xml,v 1.13 2015/07/04 08:37:32 revo Exp $">
  <kap>
    <rad>diastaz</rad>/o
  </kap>
  <drv mrk="diasta.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">KEM</uzo>
    <snc>
      <dif>
        Fermento, kiu aliigas amelon en dekstrinon.  
      </dif>
    </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_s;&c_t;&c_a;&c_z;&c_a;</trd>
  <trd lng="ca">diastasa</trd>
  <trdgrp lng="cs">
    <trd>amyláza</trd>,
    <trd>diastáza</trd>
  </trdgrp>
  <trd lng="de">Diastase</trd>
  <trd lng="es">diastasa</trd>
  <trd lng="fr">diastase</trd>
  <trdgrp lng="hu">
      <trd>diaszt&aacute;z</trd>,
      <trd>anil&aacute;z</trd>
    </trdgrp>
  <trd lng="pl">diastaza</trd>
  <trd lng="pt">di&aacute;stase</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_s;&c_t;&c_a;&c_z;&c_a;</trd>
  <trd lng="sk">diastáza</trd>
</drv>
</art>
