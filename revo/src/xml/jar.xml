<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jar.xml,v 1.55 2015/12/31 08:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>jar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="jar.0o.AST">
    <tezrad fak="AST"/>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="dauxr.0o" 
lst="voko:da&ubreve;roj">Da&ubreve;ro</ref> 
      dum kiu la tero faras kompletan rondiron
      &ccirc;irka&ubreve; la suno, proksimume 365 tagoj kaj 1
      kvarono:
      <ekz>
        en la da&ubreve;ro de du <tld/>oj
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        per la sa&gcirc;o aldoni&gcirc;os al vi <tld/>oj da
        vivo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiom da <tld/>oj vi havas?;
      </ekz>
      <ekz>
        kiom<tld/>a vi estas?;
      </ekz>
      <ekz>
        mi estas dek<tld/>a;
      </ekz>
      <ekz>
        griza pro mult<tld/>eco;
      </ekz>
      <ekz>
        cent<tld/>a jubileo;
      </ekz>
      <ekz>
        cent<tld/>ulo.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>rok<klr> s&lstroke;oneczny</klr></trd>,
      <trd>rok<klr> astronomiczny</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="jar.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kalendara <ref tip="super" cel="period.0o">periodo</ref> 
      de proksimuma da&ubreve;ro de unu tera
      &ccirc;irka&ubreve;sunado kun fiksitaj
      komenca kaj fina tagoj:
      <ekz>
        la <tld/>o mil sepcent tridek dua
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">rok<klr> kalendarzowy</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_d;</trd>
  <trd lng="bg">&c_g;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>bloaz</trd>,
    <trd>bloavezh</trd>
  </trdgrp>
  <trd lng="ca">any</trd>
  <trdgrp lng="cs">
    <trd>rok</trd>,
    <trd>rok <klr>(doba)</klr></trd>
  </trdgrp>
  <trd lng="de">Jahr</trd>
  <trdgrp lng="el">
    <trd>&epsilon_ton;&tau;&omikron;&sigma_fina;</trd>,
    <trd>&chi;&rho;&omikron_ton;&ny;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">year</trd>
  <trd lng="es">a&ntilde;o</trd>
  <trd lng="fa">&a_s;&a_A;&a_l;</trd>
  <trd lng="fr">ann&eacute;e</trd>
  <trd lng="hu">&eacute;v</trd>
  <trd lng="id">tahun</trd>
  <trd lng="it">anno</trd>
  <trd lng="nl">jaar</trd>
  <trd lng="no">&aring;r</trd>
  <trd lng="oc">an</trd>
  <trd lng="pt">ano</trd>
  <trd lng="ro">an</trd>
  <trd lng="ru">&c_g;&c_o;&c_d;</trd>
  <trd lng="sk">rok</trd>
  <trd lng="sv">&aring;r</trd>
  <trd lng="vo">yel</trd>
  <trd lng="zh">&#24180; [<ind>ni&aacute;n</ind>]</trd>
</drv>

<drv mrk="jar.cent0o">
  <kap>cent<tld/>o<fnt>Z</fnt></kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="jar.cent0o.KAL">
    <ref tip="dif" cel="cent.jar0o"><tld/>cento</ref>
    <ekz>
      tutan homan vivon, e&ccirc; cent<tld/>ojn da&ubreve;ras la
      fruktoj de laborado de fruktoplantisto
      <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
      bagateloj</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="ca">segle</trd>
  <trd lng="cs">stolet&iacute;</trd>
  <trd lng="de">Jahrhundert</trd>
  <trd lng="hu">&eacute;vsz&aacute;zad</trd>
  <trd lng="id">abad</trd>
  <trd lng="it">secolo</trd>
  <trdgrp lng="pl">
    <trd>stulecie</trd>,
    <trd>wiek</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>secol</trd>,
    <trd>veac</trd>,
  </trdgrp>
  <trd lng="sk">storo&ccaron;ie</trd>
</drv>

<drv mrk="jar.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc mrk="jar.cxiu0a.KAL">
    <dif>
      Revenanta a&ubreve; rekomenci&gcirc;anta &ccirc;iun
      <tld/>on:
      <ekz>
        la &ccirc;iu<tld/>a renkonti&gcirc;o de
        <klr>kamaradoj</klr> el multaj
        diversaj regionoj
        <fnt><bib>Lanti</bib>, <lok>la signifo de niaj
        Kongresoj</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iu<tld/>a vegeta&jcirc;o, ceremonio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_g;&c_a;&c_d;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>bloaziek</trd>,
    <trd>bloavezhiek</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>anyal</trd>,
    <trd>anual</trd>
  </trdgrp>
  <trd lng="de">j&auml;hrlich</trd>
  <trdgrp lng="el">
    <trd>&epsilon;&tau;&eta_ton;&sigma;&jota;&omikron;&sigma_fina;</trd>,
    <trd>&chi;&rho;&omikron;&ny;&jota;&alfa_ton;&tau;&jota;&kappa;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">annual</trd>
  <trd lng="es">anual</trd>
  <trd lng="fr">annuel</trd>
  <trdgrp lng="hu">
    <trd>&eacute;venk&eacute;nti</trd>,
    <trd>&eacute;vel&odblac;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>anual</trd>,
    <trd><ind>tahun</ind>an</trd>
  </trdgrp>
  <trd lng="it">annuale</trd>
  <trd lng="nl">jaarlijks</trd>
  <trd lng="no">&aring;rlig</trd>
  <trd lng="pl">coroczny</trd>
  <trd lng="pt">anual</trd>
  <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ka&zcaron;doro&ccaron;n&yacute;</trd>
  <trd lng="sv">&aring;rlig</trd>
</drv>

<drv mrk="jar.cxiudu0a">
  <kap>&ccirc;iudu<tld/>a</kap>
  <snc mrk="jar.cxiudu0a.KAL">
    <dif>
      Revenanta a&ubreve; rekomenci&gcirc;anta po periodo
      du<tld/>a:
      <ekz>
        tiu asembleo <klr>...</klr> rolas &ccirc;efe por aprobi la
        programon kaj la bu&gcirc;eton de &ccirc;iudu<tld/>a administrado
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Monda
        Organiza&jcirc;o pri Sano&rightquot;, 2008-05-25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">biennal</trd>
  <trd lng="es">bienal</trd>
  <trd lng="fr">bisannuel</trd>
  <trd lng="id">bienial</trd>
  <trd lng="it">biennale</trd>
</drv>

<drv mrk="jar.dekkelk0ulo">
  <kap>dekkelk<tld/>ulo</kap>
  <snc mrk="jar.dekkelk0ulo.KOMUNE">
    <dif>
      <ref tip="vid" cel="jun.0ulo">Junulo</ref> iom pli ol dekjara&gcirc;a, 
      proksimume 13 &gcirc;is 19-jara, 
<ref tip="vid" cel="adolesk.0ulo.KOMUNE">adoleskulo</ref>:
      <ekz>
        kiam li estis dekkelk<tld/>ulo, li konstruis funkciantan 
        telefonan sistemon
        <fnt>
          <aut>J. Casey</aut>,
          <vrk>
<url
ref="http://www.skotlando.org/En%20Esperanto/Skota%20Kulturo/John%20Logie
%20Baird.htm">
          John Logie Baird</url></vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">adolescent</trd>
  <trd lng="en">teenager</trd>
  <trd lng="es">adolescente</trd>
  <trdgrp lng="id">
    <trd>remaja</trd>,
    <trd><ind>umur</ind> belasan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>teenager</trd>,
    <trd>ragazzotto</trd>
  </trdgrp>
</drv>

<drv mrk="jar.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="jar.duon0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Duono da <ref tip="malprt" cel="jar.0o.AST"><tld/>o<sncref/>
      </ref>:
      <ekz>
        e&ccirc; duon<tld/>on mi ne a&ubreve;guras al lia regado
        <fnt><bib>SatirRak</bib>, <lok>Logos</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_g;&c_o;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="jar.duon0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <ref tip="dif" cel="du.jar0ono.KAL"><tld/>duono</ref>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_g;&c_o;&c_d;&c_i;&c_je;
    </trd>
  </snc>
  <trd 
lng="be">&c_p;&c_a;&c_w;&c_g;&c_o;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>
  <trd lng="br">c'hwec'hmiziad</trd>
  <trdgrp lng="ca">
    <trd>semestre</trd>,
    <trd>mig any</trd>
  </trdgrp>
  <trd lng="cs">pololet&iacute;</trd>
  <trd lng="de">Halbjahr</trd>
  <trd lng="es">semestre</trd>
  <trd lng="fr">semestre</trd>
  <trd lng="hu">f&eacute;l&eacute;v</trd>
  <trd lng="id">semester</trd>
  <trd lng="it">semestre</trd>
  <trd lng="nl">semester</trd>
  <trd lng="pl">p&oacute;&lstroke;rocze</trd>
  <trd lng="pt">semestre</trd>
  <trd lng="sk">polrok</trd>
  <trd lng="sv">halv&aring;r</trd>
</drv>

<drv mrk="jar.lum0o">
  <kap>lum<tld/>o</kap>
  <snc mrk="jar.lum0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">mezurunuo</ref> de
      <ref cel="long.0o.FIZ">longo</ref>, uzata por mezuri grandegajn
      interastrajn distancojn, egala al 9,4605278.10<sup>15</sup>
      <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simbolo l.j.:
      <ekz>
        unu lum<tld/>o egalas al la longo trairata de lumo en vakuo
        dum unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_a;&c_v;&c_y; 
<ind>&c_g;&c_o;&c_d;</ind></trd>
  <trd lng="ca">any <ind>llum</ind></trd>
  <trd lng="cs">sv&ecaron;teln&yacute; rok</trd>
  <trd lng="de">Lichtjahr</trd>
  <trd lng="en">light-year</trd>
  <trd lng="es">a&ntilde;o luz</trd>
  <trd lng="fr">ann&eacute;e-lumi&egrave;re</trd>
  <trd lng="hu">f&eacute;ny&eacute;v</trd>
  <trd lng="id"><ind>tahun</ind> cahaya</trd>
  <trd lng="it">anno luce</trd>
  <trd lng="nl">lichtjaar</trd>
  <trd lng="no">lys&aring;r</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_o;&c_v;&c_o;&c_j; &c_g;&c_o;&c_d;
</trd>
  <trd lng="sk">sveteln&yacute; rok</trd>
  <trd lng="sv">ljus&aring;r</trd>
</drv>

<drv mrk="jar.mil0o">
  <kap><ofc>*</ofc>mil<tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="jar.mil0o.KAL">
    <ref tip="dif" cel="mil.jar0o"><tld/>milo</ref>
    <ekz>
      mil <tld/>oj (a&ubreve; milo da <tld/>oj) faras mil<tld/>on
      <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="ca">mil&middot;lenni</trd>
  <trd lng="cs">tis&iacute;cilet&iacute;</trd>
  <trd lng="de">Jahrtausend</trd>
  <trd lng="hu">&eacute;vezred</trd>
  <trd lng="id">milenium</trd>
  <trd lng="it">millennio</trd>
  <trdgrp lng="no">
    <trd>millenium</trd>,
    <trd>&aring;rtusen</trd>
  </trdgrp>
  <trd lng="pl">tysi&aogonek;clecie</trd>
  <trd lng="ro">mileniu</trd>
  <trd lng="sk">tis&iacute;cro&ccaron;ie</trd>
</drv>

<drv mrk="jar.nov0o">
  <kap>nov<tld/>o</kap>
  <snc mrk="jar.nov0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Unua tago de la jaro kaj la rilata
      <ref tip="lst" cel="fest.0o.KAL" lst="voko:festoj">festo</ref>
      en la koncerna kalendaro:
      <ekz>
        Dio donu al ni &ccirc;iuj &gcirc;ojigan nov<tld/>on!
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nov&yacute; rok</trd>
  <trd lng="es">A&ntilde;o nuevo</trd>
  <trd lng="fr">nouvel an</trd>
  <trd lng="sk">Nov&yacute; rok</trd>
</drv>

<drv mrk="jar.prov0o">
  <kap>prov<tld/>o</kap>
  <snc mrk="jar.prov0o.PED">
    <dif>
      <tld lit="J"/>o, dum kiu oni ion provas, ekzemple dum kiu 
      komencanta profesiulo lernas kaj akiras kapablojn en laborejo:
      <ekz>
        &ccirc;i-somere &leftquot;Kvinpetalo&rightquot; i&gcirc;os dudekjara
        kaj fakte estos funkcianta de dek tri jaroj, enkalkulante
        la prov<tld/>on 1984
        <fnt><vrk>Eventoj</vrk><lok>
        <url ref="http://www.eventoj.hu/arkivo/eve-128.htm">
        1997-6 (128)</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">any de <ind>pr&agrave;ctiques</ind></trd>
  <trdgrp lng="es">
    <trd>periodo de <ind>aprendizaje</ind></trd>,
    <trd>periodo de <ind>pruebas</ind></trd>,
    <trd>periodo de <ind>formaci&oacute;n</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>p&eacute;riode d'<ind>apprentissage</ind></trd>,
    <trd>p&eacute;riode de <ind>formation</ind></trd>
  </trdgrp>
  <trd lng="hu">pr&oacute;ba&eacute;v</trd>
  <trd lng="id"><ind>tahun</ind> ajaran</trd>
  <trdgrp lng="it">
    <trd>apprendistato</trd>, 
    <trd>tirocinio</trd>
    <trd>periodo di <ind>formazione</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rok pr&oacute;bny</trd>,
    <trd>roczny sta&zdot;</trd>
  </trdgrp>
  <trd 
lng="ru">&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; 
&c_g;&c_o;&c_d;</trd>
</drv>

<drv mrk="jar.stud0o">
  <kap>stud<tld/>o</kap>
  <snc mrk="jar.stud0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <tld lit="J"/>a periodo, dum kiu funkcias instruejoj, kutime
      de a&ubreve;tuno &gcirc;is somerkomenco:
      <ekz>
        post la tria stud<tld/>o en la universitato
        <klr>...</klr> mi jam
        lacis vidi nur ekvaciojn kaj komputilojn
        <fnt>
        <aut>viric</aut>,
        <vrk><url ref="http://viric.livejournal.com/9142.html">
        [blogo]</url></vrk>,
        <lok>2005-11-01</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>curs <ind>escolar</ind></trd>,
    <trd>any <ind>acad&egrave;mic</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schuljahr</trd>,
    <trd>Studienjahr</trd>,
    <trd>akademisches Jahr</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>curso escolar</trd>,
    <trd>curso acad&eacute;mico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ann&eacute;e scolaire</trd>,
    <trd>ann&eacute;e universitaire</trd>
  </trdgrp>
  <trd lng="id"><ind>tahun</ind> akademik</trd>
  <trdgrp lng="it">
    <trd>anno scolastico</trd>,
    <trd>anno accademico</trd>
  </trdgrp>
  <trd lng="no">skole&aring;r</trd>
  <trdgrp lng="pl">
    <trd>rok szkolny</trd>,
    <trd>rok akademicki</trd>,
    <trd>semestr<klr> (semestr to zazwyczaj po&lstroke;owa roku
    szkolnego)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_je;&c_b;&c_n;&c_y;&c_j; &c_g;&c_o;&c_d;
    </trd>,
    <trd>&c_k;&c_u;&c_r;&c_s;<klr>
    (&c_g;&c_o;&c_d; &c_u;&c_ch;&c_jo;&c_b;&c_y;)</klr>
    </trd>
  </trdgrp>
</drv>

<drv mrk="jar.super0o">
  <kap>super<tld/>o</kap>
  <snc mrk="jar.super0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <tld lit="J"/>o pli longa ol normala por al al&gcirc;ustigi
      la kalendaran jaron al la suna, ekzemple 366-taga <tld/>o
      en la kalendaroj julia kaj gregoria:
      <ekz>
        la 29a tago de Februaro en super<tld/>o
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        fingra kalendaro, originale verkita de J. Grohn, privata
        instruisto.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_v;&c_ib;&c_s;&c_a;&c_k;&c_o;&c_s;&c_n;&c_y; &c_g;&c_o;&c_d;
  </trd>
  <trd lng="br">bloavezh <ind>bizeost</ind></trd>
  <trdgrp lng="ca">
    <trd>any bixest</trd>,
    <trd>any de <ind>trasp&agrave;s</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_i;&c_s;&c_o;&c_k;&c_o;&c_s;&c_n;&c_a; &c_g;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">p&rcaron;estupn&yacute; rok</trd>
  <trd lng="de">Schaltjahr</trd>
  <trd lng="en">leap year</trd>
  <trd lng="es">a&ntilde;o bisiesto</trd>
  <trd lng="fr">ann&eacute;e bissextile</trd>
  <trd lng="hu">sz&ouml;k&odblac;&eacute;v</trd>
