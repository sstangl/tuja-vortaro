<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: panikl.xml,v 1.8 2015/07/04 08:37:41 revo Exp $">
<kap><rad>panikl</rad>/o</kap>
<drv mrk="panikl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Kunmetita kaj tre bran&ccirc;i&gcirc;anta
      <ref tip="super" cel="grapol.0o">grapolo</ref>, kies
      &ccirc;iuj bran&ccirc;oj fini&gcirc;as en floro:
      <ekz>
        <tld/>o de aveno, milio, rizo;
      </ekz>
      <ekz>
        <ind>umbela <tld/>o</ind>
        <klr>(la floroj trovi&gcirc;as en unu ebeno)</klr>.
        <trdgrp lng="de">
          <trd>Doldenrispe</trd>,
          <trd>Ebenstrau&szlig;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>funela <tld/>o</ind>
        <klr>(la flankaj bran&ccirc;oj estas tiom plilongigitaj,
        ke la floroj formas funelon, ekz. &ccirc;e filipendulo)</klr>
        <trdgrp lng="de">
          <trd>Spirre</trd>,
          <trd>Trichterrispe</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_c;&c_jo;&c_l;&c_k;&c_a; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
  <trd lng="cs">vrstva tuku pod pokožkou</trd>
  <trd lng="de">Rispe</trd>
  <trd lng="hu">buga</trd>
  <trd lng="nl">pluim<klr tip="ind"> (v.plant)</klr></trd>
  <trd lng="pt">pan&iacute;cula</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_jo;&c_l;&c_k;&c_a; <klr tip="amb">(&c_u; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>
  <trd lng="sk">vrstva tuku pod pokožkou</trd>
</drv>
</art>
