<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kopul.xml,v 1.4 2015/09/25 22:10:16 revo Exp $">
<kap>
    <rad>kopul</rad>/o <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="kopul.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <ref tip="sin" cel="lig.0verbo">ligverbo</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>kopula</trd>,
    <trd>spona</trd>,
    <trd>sponov&eacute; sloveso</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>copular verb</trd>,
    <trd>copulative</trd>, <trd>copula</trd>
  </trdgrp>
  <trdgrp lng="fr">
  <trd>copule</trd>,
  <trd>verbe d'&eacute;tat</trd>
</trdgrp>
  <trd lng="sk">sponov&eacute; sloveso</trd>
</drv>
</art>

