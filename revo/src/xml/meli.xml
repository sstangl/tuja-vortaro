<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: meli.xml,v 1.7 2013/08/27 15:10:24 revo Exp $">
<kap>
  <rad>meli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="meli.0o">
  <kap><tld/>o</kap>
  <snc mrk="meli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto kreskanta de Hindio tra Sudazio
      &gcirc;is Norda&ubreve;stralio, kun grandaj panikloj de floretoj,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="meliac.0oj">meliacoj</ref>
      (<trd lng="la">Melia</trd>):
      <ekz>
        onidire la frukto de <tld/>o estas venena
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url
        ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
        <lok>2005-12-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zedrachbaum</trd>,
    <trd>Persischer Flieder</trd>,
    <trd>Chinesischer Holunder</trd>,
    <trd>Paternosterbaum</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lilas des indes</trd>,
    <trd>m&eacute;lia</trd>
  </trdgrp>
    <trd lng="pt">m&eacute;lia</trd>
</drv>
</art>

