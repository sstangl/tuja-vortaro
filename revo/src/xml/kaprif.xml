<kap>
  <rad>kaprifoli</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kaprif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="lonice.0o">lonicero</ref>
      (<trd lng="la">Lonicera caprifolium</trd>).
    </dif>
  </snc>
  <trd lng="ca">lligabosc de jard&iacute;</trd>
  <trd lng="cs">zimolez kozí list</trd>
  <trdgrp lng="de">
    <trd>Wohlriechendes Gei&szlig;blatt</trd>,
    <trd>Jel&auml;ngerjelieber</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&f_y;&f_ch; &a_A;&a_m;&f_y;&a_n;&zwnj;&a_A;&a_l;&a_d;&a_w;&a_l;&a_h;</trd>
  <trd lng="fr">ch&egrave;vre-feuille</trd>
  <trd lng="hu">jerik&oacute;i lonc</trd>
  <trd lng="nl">kamperfoelie</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_p;&c_r;&c_i;&c_f;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_i;&c_m;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;
    &c_d;&c_u;&c_sh;&c_i;&c_s;&c_t;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">kozí list</trd>
</drv>
</art>
