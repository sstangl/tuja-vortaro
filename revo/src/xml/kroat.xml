<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kroat.xml,v 1.9 2014/06/14 03:10:23 revo Exp $">
<kap>
  <rad>kroat</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="kroat.K0ujo">
  <kap>
    <tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kroat.K0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Zagrebo.
      Teritorio de Respubliko Kroatujo.
      ISO-3166 kodo: HR.
      &Gcirc;i limas kun
      <ref tip="vid" cel="bosn.B0ujoHercegovino.sxtato">Bosnujo kaj
      Hercegovino</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>,
      <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>,
      <ref tip="vid" cel="sloven.S0ujo.GEOG">Slovenujo</ref>
      kaj Adriatiko.
    </dif>
    <ref tip="sin" cel="kroati.K0o.GEOG">Kroatio</ref>
    </snc>
    <trd lng="be">&c_H;&c_a;&c_r;&c_v;&c_a;&c_t;&c_y;&c_ja;</trd>
    <trd lng="bg">&c_H;&c_malmol;&c_r;&c_v;&c_a;&c_t;&c_i;&c_ja;</trd>
    <trd lng="de">Kroatien</trd>
    <trd lng="en">Croatia</trd>
    <trd lng="fr">Croatie</trd>
    <trd lng="id">Kroasia</trd>
    <trd lng="nl">Kroati&euml;</trd>
    <trdgrp lng="pl">
      <trd>Chorwacja</trd>,
      <trd>Republika <ind>Chorwacji</ind></trd>
    </trdgrp>
    <trd lng="pt">Cro&aacute;cia</trd>
  </drv>

</art>

