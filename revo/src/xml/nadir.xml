<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nadir.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
   <rad>nadir</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="nadir.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">AST</uzo>
   <snc>
     <dif>
         Punkto de la &ccirc;iela sfero, trovi&gcirc;anta sur la
         vertikalo de l' observanto
         kaj direkte malsupre sub liaj piedoj:
       <ekz>
         la punkto diametre kontra&ubreve;a estas
         <ref tip="ant" cel="zenit.0o">zenito</ref>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_z;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_n;&c_a;&c_d;&c_i;&c_r;</trd>
  <trd lng="ca">nadir</trd>
  <trdgrp lng="cs">
    <trd>nadir</trd>,
    <trd>podnožník</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Nadir</trd>,
     <trd>Fu&szlig;punkt</trd>,
     <trd>Nullpunkt</trd>
   </trdgrp>
  <trd lng="el">&ny;&alfa;&delta;&jota_ton;&rho;</trd>
  <trd lng="en">nadir</trd>
  <trdgrp lng="fa">
     <trd>&f_p;&a_A;&a_s;&a_w;</trd>,
     <trd>&a_s;&a_m;&a_t;&zwnj;&a_A;&a_l;&a_q;&a_d;&a_m;</trd>
   </trdgrp>
  <trd lng="fr">nadir</trd>
  <trdgrp lng="hu">
     <trd>nad&iacute;r</trd>,
     <trd>talppont</trd>
   </trdgrp>
  <trd lng="nl">nadir</trd>
  <trd lng="pl">nadir</trd>
  <trd lng="pt">nadir</trd>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_i;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>nadir</trd>,
    <trd>podnožník</trd>
  </trdgrp>
</drv>
</art>
