<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tumult.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tumult</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tumult.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tumult.0o.KOMUNE">
    <dif>
      Konfuza, malorda bruado kaj malkvieti&gcirc;o de homamaso,
      eventuale kun eksciti&gcirc;o kaj perfortoj:
      <ekz>
        infera <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        bolis terura <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        petolema <tld/>o de maskoj;
      </ekz>
      <ekz>
        <tld/>a kunveno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit">agiti&gcirc;o</ref>,
      <ref cel="pusx.inter0igxo.MOV">interpu&scirc;i&gcirc;o</ref>,
      <ref cel="ribel.0o.POL">ribelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>tumult</trd>,
    <trd>avalot</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mela</trd>,
    <trd>nával</trd>,
    <trd>povyk</trd>,
    <trd>rej</trd>,
    <trd>rozruch</trd>,
    <trd>ruch</trd>,
    <trd>shluk</trd>,
    <trd>shon</trd>,
    <trd>srocení</trd>,
    <trd>tlačenice</trd>,
    <trd>tumult</trd>,
    <trd>vřava</trd>,
    <trd>zmatek</trd>
  </trdgrp>
  <trd lng="de">Tumult</trd>
  <trdgrp lng="en">
    <trd>tumult</trd>,
    <trd>riot</trd>,
    <trd>commotion</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;meute</trd>,
    <trd>tumulte</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tolong&aacute;s</trd>,
    <trd>t&uuml;leked&eacute;s</trd>,
    <trd>kavarod&aacute;s</trd>,
    <trd>tumultus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tumult</trd>,
    <trd>opschudding</trd>,
    <trd>herrie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tumult</trd>,
    <trd>harmider</trd>,
    <trd>rwetest</trd>,
    <trd>wrzawa</trd>,
    <trd>zamieszanie</trd>,
    <trd>zgie&lstroke;k</trd>
  </trdgrp>
  <trd lng="pt">tumulto</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_t;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je; <klr
    tip="amb">(&c_v; &c_t;&c_o;&c_l;&c_p;&c_je;)</klr></trd>,
    <trd>&c_t;&c_o;&c_l;&c_ch;&c_je;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hurhaj</trd>,
    <trd>trma-vrma</trd>,
    <trd>zhluk</trd>,
    <trd>zhon</trd>,
    <trd>zmätok</trd>
  </trdgrp>
</drv>

<drv mrk="tumult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tumult.0i.KOMUNE">
    <dif>
      Brue kaj malorde malkvieti&gcirc;i:
      <ekz>
        kial <tld/>as popoloj kaj gentoj pripensas
        vana&jcirc;on<fnt>Z</fnt>?
      </ekz>
      <ekz>
        la popol' <tld/>as en supozoj pri l' morto de
        Polonio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiam <tld/>i&gcirc;is la tuta komunumo kaj ekmurmuris
        kontra&ubreve; Moseo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>avalotar</trd>,
    <trd>revoltar-se</trd>
  </trdgrp>
  <trd lng="cs">hlučet</trd>
  <trd lng="de">toben</trd>
  <trdgrp lng="en">
    <trd>rage</trd>,
    <trd>riot</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agiter</ind></trd>,
    <trd>chahuter</trd>,
    <trd>se <ind>soulever</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tolong</trd>,
    <trd>t&uuml;lekedik</trd>,
    <trd>kavarog</trd>,
    <trd>zavago</trd>
  </trdgrp>
  <trd lng="nl">opschudding veroorzaken</trd>
  <trdgrp lng="pl">
    <trd>robi&cacute; <ind>tumult</ind></trd>,
    <trd>robi&cacute; <ind>harmider</ind></trd>,
    <trd>robi&cacute; <ind>rwetest</ind></trd>,
    <trd>robi&cacute; <ind>wrzaw&eogonek;</ind></trd>,
    <trd>robi&cacute; <ind>zamieszanie</ind></trd>,
    <trd>robi&cacute; <ind>zgie&lstroke;k</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_v; &c_s;&c_m;&c_ja;&c_t;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o; &c_t;&c_o;&c_l;&c_p;&c_je;, &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>,
    <trd>&c_v;&c_z;&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_o; &c_t;&c_o;&c_l;&c_p;&c_je;, &c_n;&c_a;&c_r;&c_o;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>búriť sa</trd>,
    <trd>nepokojne vrieť v dave</trd>,
    <trd>plašiť sa</trd>
  </trdgrp>
</drv>

<drv mrk="tumult.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="tumult.0ema.KOMUNE">
    <dif>
      Kiu volonte <tld/>as, ka&ubreve;zas malkvieton:
      <ekz>
        <tld/>emaj infanoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>agit&eacute;</trd>,
    <trd>chahuteur</trd>
  </trdgrp>
</drv>
</art>
