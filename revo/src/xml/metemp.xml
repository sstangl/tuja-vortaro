<kap>
  <ofc>2</ofc>
  <rad>metempsikoz</rad>/o
</kap>
<drv mrk="metemp.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Doktrino pri la postmorta transmigrado de la animo el iu korpo en
      alian.
    </dif>
    <trd lng="ca">teoria de la transmigraci&oacute; de l'&agrave;nima</trd>
  </snc>
  <snc>
    <dif>
      Tiu transmigrado mem.
    </dif>
    <trdgrp lng="ca">
      <trd>transmigraci&oacute; de l'&agrave;nima</trd>,
      <trd>reencarnaci&oacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">metempsicosi</trd>
  <trd lng="de">Seelenwanderung</trd>
  <trdgrp lng="en">
    <trd>metempsychosis</trd>,
    <trd>reincarnation</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_n;&a_A;&a_s;&a_kh;</trd>
  <trd lng="fr">m&eacute;tempsycose</trd>
  <trdgrp lng="hu">
      <trd>l&eacute;lekv&aacute;ndorl&aacute;s</trd>,
      <trd>metempszich&oacute;zis</trd>
  </trdgrp>
  <trd lng="nl">zielsverhuizing</trd>
  <trd lng="pt">metempsicose</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_je;&c_m;&c_p;&c_s;&c_i;&c_h;&c_o;&c_z;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;
&c_d;&c_u;&c_sh;</trd>
  </trdgrp>
</drv>
</art>
