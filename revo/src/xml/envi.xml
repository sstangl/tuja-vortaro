<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: envi.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>envi</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="envi.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra> 
  <snc mrk="envi.0i.ion">
    <dif>
      Deziri havi tion, kion alia posedas:
      <ekz>
        <tld/>i ion al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kion mi ne scias, tion mi ne
        <tld/>as
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la princoj povus <tld/>i al ili
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 3</lok></fnt>;
      </ekz>
      <ekz>
        elokventeco, kiun multaj <tld/>us al li.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avid.0i">avidi</ref>,
      <ref cel="jxaluz.0i">&jcirc;aluzi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beneiden</trd>,
      <trd>neidisch sein</trd>
    </trdgrp>
    <trd lng="pl">zazdro&sacute;ci&cacute;</trd>
  </snc>
  <snc mrk="envi.0i.iun">
    <dif>
      Senti mal&gcirc;ojon kaj malamon kontra&ubreve; iu, kiu posedas
      ion, kion oni mem malhavas:
      <ekz>
        <tld/>i iun pro io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la ri&ccirc;ulojn;
      </ekz>
      <ekz>
        ne <tld/>u la malvirtulojn
        <fnt><bib>MT</bib>, <lok>&Sen; 24:19</lok></fnt>;
      </ekz>
      <ekz>
        pli bone estas <ind><tld/>igi</ind> ol kompatigi.
        <trd lng="fr">faire <ind>envie</ind></trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz> 
    </dif>
    <trd lng="de">missg&ouml;nnen</trd>
    <trdgrp lng="pl">
      <trd>czu&cacute; <ind>zawi&sacute;&cacute;</ind></trd>,
      <trd>odczuwa&cacute; <ind>zawi&sacute;&cacute;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_z;&c_d;&c_r;&c_o;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
     <trd>tenir enveja</trd>,
     <trd>envejar</trd>
  </trdgrp>
  <trd lng="cs">závidět <klr>(někomu něco)</klr></trd>
  <trd lng="en">envy</trd>
  <trd lng="es">envidiar</trd>
  <trd lng="fr">envier</trd>
  <trd lng="pt">invejar</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">závidieť</trd>
  <trd lng="sv">avundas</trd>
</drv>

<drv mrk="envi.0o">
  <kap><tld/>o</kap>
  <snc mrk="envi.0o.KOMUNE">
    <dif>
      Stato de <tld/>anto:
      <ekz>
        feli&ccirc;o kaj ri&ccirc;o <tld/>on
        elvokas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la reciproka <tld/>o de la popoloj<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_z;&c_d;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">enveja</trd>
  <trd lng="cs">závist</trd>
  <trdgrp lng="de">
     <trd>Neid</trd>,
     <trd>Missgunst</trd>
  </trdgrp>
  <trd lng="es">envidia</trd>
  <trd lng="fr">envie</trd>
  <trd lng="pl">zawi&sacute;&cacute;</trd>
  <trd lng="pt">inveja</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">závisť</trd>
  <trd lng="sv">avundsjuka</trd>
</drv>

<drv mrk="envi.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="envi.0ema.KOMUNE">
    <dif>
      Inklina al <tld/>o. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_z;&c_d;&c_r;&c_o;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_z;&c_d;&c_r;&c_o;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">envej&oacute;s</trd>
  <trd lng="cs">závistivý <klr>(člověk)</klr></trd>
  <trdgrp lng="de">
     <trd>neidisch</trd>,
     <trd>missg&uuml;nstig</trd>
  </trdgrp>
  <trd lng="es">envidioso</trd>
  <trd lng="fr">envieux</trd>
  <trd lng="pl">zawistny</trd>
  <trd lng="pt">invejoso</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nežičlivý</trd>,
    <trd>závistlivý</trd>
  </trdgrp>
  <trd lng="sv">avundsjuk</trd>
</drv>

<drv mrk="envi.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="envi.0inda.KOMUNE">
    <dif>
      Meritanta <tld/>on:
      <ekz>
        <tld/>inda feli&ccirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_z;&c_d;&c_r;&c_o;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">envejable</trd>
  <trd lng="cs">záviděníhodný</trd>
  <trd lng="de">beneidenswert</trd>
  <trd lng="es">envidiable</trd>
  <trd lng="fr">enviable</trd>
  <trd lng="pt">invej&aacute;vel</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">závideniahodný</trd>
  <trd lng="sv">avundsv&auml;rd</trd>
</drv>
</art>
