<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxim.xml,v 1.8 2009/04/24 16:31:23 revo Exp $">
<kap><rad>&hcirc;im</rad>/o</kap>
<drv mrk="hxim.0o">
   <kap><tld/>o</kap>
   <snc>
     <uzo tip="fak">BIO</uzo>
     <uzo tip="fak">MED</uzo>
     <uzo tip="fak">FIZL</uzo>
     <dif>
       Ka&ccirc;aspekta miksa&jcirc;o, formi&gcirc;anta en la
       stomako rezulte de diserigo de la man&gcirc;itaj
       nutra&jcirc;oj per la stomakparietaj muskoloj kaj efiko
       de la stomaksuko.
     </dif>
     <ref tip="sin" cel="cxim.0o">&ccirc;imo</ref>
   </snc>
   <trd lng="be">&c_h;&c_ib;&c_m;&c_u;&c_s;</trd>
   <trd lng="bg">&c_h;&c_i;&c_m;&c_u;&c_s;</trd>
   <trd lng="ca">quim</trd>
   <trdgrp lng="de">
     <trd>Chymus</trd>,
     <trd>Nahrungsbrei</trd>,
     <trd>Speisebrei</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>chyme</trd>,
     <trd>chymus</trd>
   </trdgrp>
   <trd lng="es">quimo</trd>
   <trdgrp lng="fi">
     <trd>ruokasula</trd>
   </trdgrp>
   <trd lng="fr">chyme</trd>
   <trd lng="it">chimo</trd>
   <trdgrp lng="pl">
     <trd>chymus</trd>,
     <trd>mlecz</trd>,
     <trd>tre&sacute;&cacute; &zdot;o&lstroke;&aogonek;dka</trd>,
     <trd>tre&sacute;&cacute; pokarmowa</trd>
   </trdgrp>
   <trd lng="pt">quimo<klr tip="amb"> (fisiologia)</klr></trd>
   <trd lng="ru">&c_h;&c_i;&c_m;&c_u;&c_s;</trd>
</drv>
</art>

