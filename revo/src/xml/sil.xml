<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sil.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sil</rad>/o
</kap>
<drv mrk="sil.0o">
  <kap><tld/>o</kap>
  <snc mrk="sil.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Scirc;aktforma <ref tip="super" cel="stok.0ejo">stokejo</ref> por meti
      en
      &gcirc;in grenon, legomojn kaj tiel plu:
      <ekz>
        prezo por konstruo de deponejoj kaj <tld/>oj por &scirc;tata
        instanco<fnt><aut>Gilbert R. Ledon</aut>: <vrk>Esenco de
        korupto</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
        8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_l;&c_a;&c_s;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_l;&c_a;&c_s;&c_n;&c_a;&c_ja; &c_v;&c_je;&c_zh;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_i;&c_l;&c_o;&c_z;</trd>
  <trd lng="ca">sitja</trd>
  <trdgrp lng="cs">
    <trd>silo</trd>,
    <trd>zásobník</trd>
  </trdgrp>
  <trd lng="de">Silo</trd>
  <trd lng="en">silo</trd>
  <trd lng="es">silo</trd>
  <trd lng="fa">&a_s;&f_y;&a_l;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>silo</trd>
  </trdgrp>
  <trd lng="hu">sil&oacute;</trd>
  <trd lng="nl">silo</trd>
  <trd lng="pl">silos</trd>
  <trd lng="pt">silo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_sh;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>silo</trd>,
    <trd>zásobník</trd>
  </trdgrp>
</drv>

<drv mrk="sil.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Meti en <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ensitjar</trd>
  <trd lng="cs">dát do sila</trd>
  <trd lng="es">ensilar</trd>
  <trdgrp lng="fr">
    <trd>ensiler</trd>
  </trdgrp>
  <trd lng="hu">sil&oacute;z</trd>
  <trd lng="pl">sk&lstroke;adowa&cacute; w <ind>silosie</ind></trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_o;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">silážovať</trd>
</drv>
</art>
