<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: raj.xml,v 1.18 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>raj</rad>/o
</kap>
<drv mrk="raj.0o">
  <kap><tld/>o</kap>
  <snc mrk="raj.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="raj.0oformaj"><tld/>oformaj</ref>
      fi&scirc;oj; plata romboforma kartilaga <ref tip="lst" cel="fisx.0o"
      lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      kun bu&scirc;o kaj nazotruoj sur la malsupra, plej ofte hela flanko 
      kaj kun mallar&gcirc;a vosto.
    </dif>
  </snc>
  <snc mrk="raj.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="rajed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Raja</trd>) 
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_t;</trd>
  <trd lng="cs">rejnok</trd>
  <trd lng="de">Rochen</trd>
  <trd lng="en">ray</trd>
  <trd lng="es">raya</trd>
  <trd lng="fr">raie</trd>
  <trd lng="hu">r&aacute;ja</trd>
  <trd lng="id">pari</trd>
  <trd lng="it">razza</trd>
  <trdgrp lng="nl">
    <trd>rog</trd>,
    <trd>vleet</trd>
  </trdgrp>
  <trd lng="pl">raja</trd>
  <trdgrp lng="pt">
    <trd>raia</trd>,
    <trd>arraia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_t;</trd>
  <trd lng="sk">raja</trd>
</drv>

<drv mrk="raj.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="raj.0oformaj.ordo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
       <ref tip="lst" cel="ord.0o.BIO"
       lst="voko:zoologiaj_ordoj">Ordo</ref> 
       de <ref tip="super" cel="fisx.kartilago0oj">kartilagofi&scirc;oj</ref>
       (<trd lng="la">Rajiformes</trd>).
    </dif>
  </snc>
</drv>

</art>
