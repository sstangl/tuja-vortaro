<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: knel.xml,v 1.8 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>knel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="knel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Buleto a&ubreve; cilindro el fi&scirc;a a&ubreve; vianda ka&ccirc;o koherigita per
      ova&jcirc;o por kuirado:
      <ekz>
        supo kun salikokaj <tld/>oj kaj kokha&ubreve;taj pecoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 348a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_n;&c_e;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_r;&c_y;&c_k;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">quenelle</trd>
  <trdgrp lng="cs">
    <trd>klops</trd>,
    <trd>knedlíček polévkový</trd>
  </trdgrp>
  <trd lng="fr">quenelle</trd>
  <trd lng="hu">h&uacute;sgomb&oacute;c</trd>
  <trd lng="nl">quenelle</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_d;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_n;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">polievková knedľa</trd>
</drv>
</art>

