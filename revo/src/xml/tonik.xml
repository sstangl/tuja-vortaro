<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tonik.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>tonik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tonik.0o">
  <kap><tld/>o</kap>
  <snc mrk="tonik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="medika1.0o">Medikamento</ref> kun vigliga,
      fortiga efiko, kontra&ubreve; korpa malforto:
      <ekz>
        mi donu al &scirc;i <tld/>on por &scirc;in refortigi
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        567a</lok></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_t;&c_o;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_ib;&c_z;&c_u;&c_ju;&c_ch;&c_y;
&c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tonic</trd>,
    <trd>Tonic Water</trd>
  </trdgrp>
  <trd lng="fr">tonique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>er&odblac;s&iacute;t&odblac;(szer)</trd>,
    <trd>toniz&aacute;l&oacute;(szer)</trd>
  </trdgrp>
  <trd lng="it">tonico<klr> (sost.)</klr></trd>
  <trd lng="nl">tonicum</trd>
  <trd lng="pl">tonik</trd>
  <trd lng="pt">t&ocirc;nico</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_n;&c_i;&c_k;</trd>,
 
<trd>&c_t;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_je;&c_je;
&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  </snc>
  <snc mrk="tonik.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      La <ref tip="sin" cel="ton.0o.MUZ_baza">baza tono</ref>
      de <ref tip="super" cel="gam.0o.MUZ">gamo</ref>
    </dif>
    <trd lng="fr">tonique<klr> (subst.)</klr></trd>
    <trd lng="it">tonica<klr> (sost.)</klr></trd>
    <trd lng="nl">stamtoon</trd>
    <trdgrp lng="pl">
      <trd>tonika</trd>,
      <trd>tonacja</trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nica</trd>
  </snc>
  <trd lng="cs">t√≥nika</trd>
  <trd lng="sk">tonikum</trd>
</drv>

<drv mrk="tonik.0a">
  <kap><tld/>a</kap>
  <snc mrk="tonik.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Vigliga, refortiga.
    </dif>
  <trd lng="be">&c_t;&c_a;&c_n;&c_ib;&c_z;&c_u;&c_ju;&c_ch;&c_y;</trd>
  <trd lng="fr">tonique<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>er&odblac;s&iacute;t&odblac;</trd>,
    <trd>toniz&aacute;l&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tonisch</trd>,
    <trd>versterkend</trd>,
    <trd>opwekkend</trd>
  </trdgrp>
  <trd lng="pl">tonizuj&aogonek;cy</trd>
  <trd lng="pt">tonificante</trd>
  <trd lng="ru">
  &c_t;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;
  </trd>
  </snc>
  <snc mrk="tonik.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Referanta al la <ref tip="vid" cel="tonik.0o.MUZ">toniko</ref>.
    </dif>
    <trd lng="nl">betr. hebbend op de <ind>stamtoon</ind></trd>
    <trdgrp lng="pl">
      <trd>toniczny</trd>,
      <trd>w <ind>tonacji</ind></trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nico</trd>
  </snc>
  <trd lng="it">tonico<klr> (agg.)</klr></trd>
  </drv>
</art>
