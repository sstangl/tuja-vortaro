<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arkean.xml,v 1.7 2012/08/10 14:11:39 revo Exp $">
<kap>
  <rad>arkean</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="arkean.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkean.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      La unua, plej praa <ref tip="super" cel="era.0o.GEOL">erao</ref>
      de <ref tip="super" cel="kambri.antaux0o">anta&ubreve;kambrio</ref>,
      iam taksita kiel periodo;
      <tld/>o da&ubreve;ris proksimume anta&ubreve; inter 3000
      kaj 1500 milionoj da jaroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="proterozoik.0o.GEOL">proterozoiko</ref>,
      <ref cel="paleozoik.0o.GEOL">paleozoiko</ref>,
      <ref cel="mezozoik.0o.GEOL">mezozoiko</ref>,
      <ref cel="kenozoik.0o.GEOL">kenozoiko</ref>
    </refgrp>
  </snc>
  <trd lng="ca">arque&agrave;</trd>
  <trd lng="fr">Arch&eacute;en</trd>
  <trd lng="it">archeano</trd>
  <trdgrp lng="pl">
    <trd>era <ind>archaiczna</ind></trd>,
    <trd>archaik</trd>,
    <trd>era <ind>archeozoiczna</ind></trd>
  </trdgrp>
</drv>
</art>
