<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dazipe.xml,v 1.10 2013/08/31 08:10:35 revo Exp $">
<kap>
  <rad>daziped</rad>/oj
</kap>
<drv mrk="dazipe.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="artik.krom0uloj">kromartikuloj</ref>
      (<trd lng="la">Dasypodidae</trd>),
      kies dorso kaj vosto estas kovritaj de skvamoj ofte en formo de
      zonoj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_ja;&c_n;&c_o;&c_s;&c_c;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">G&uuml;rteltiere <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">dasypodid&eacute;s</trd>
  <trd lng="hu">p&aacute;nc&eacute;l&ouml;vesf&eacute;l&eacute;k</trd>
  <trd lng="pl">pancernikowate</trd>
  <trd lng="pt">dasipod&iacute;deos</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_c;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

