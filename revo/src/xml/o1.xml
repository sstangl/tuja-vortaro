<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: o1.xml,v 1.5 2014/04/12 07:10:39 revo Exp $">
<kap>
    <ofc>*</ofc><rad>o</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="o1.0.vokalo">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      Unu el la kvin <ref tip="dif" cel="vokal.0o">vokaloj</ref> de
      Esperanto, la mezvasta malanta&ubreve;a, kaj
      nomo de tiu <ref tip="dif" cel="fonem.0o">fonemo</ref> [<tld/>].
    </dif>
    <trd lng="fr">o<klr> (voyelle)</klr></trd>
  </snc>
  <snc>
     <dif>
      La dekna&ubreve;a <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta
      alfabeto, uzata i.a. kiel:
     </dif>
     <trd lng="fr">o<klr> (lettre)</klr></trd>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
     <dif>
     (maj.) simbolo de oksigeno: <ekz> <tld lit="O"/></ekz>;
     </dif>
     <trd lng="fr">oxyg&egrave;ne</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">TEK</uzo>
     <dif>
     (maj.) signo de formo: <ekz> <tld lit="O"/>-aperturo</ekz>;
     </dif>
     <trd lng="fr">O<klr> (forme de)</klr></trd>
     </subsnc>   
  </snc>
  <snc>
  <ref tip="vid" cel="o.0">-<tld/> (fina&jcirc;o)</ref>
  <trd lng="fr">-o<klr> (terminaison)</klr></trd>
  </snc>
</drv>
</art>

