<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nenie.xml,v 1.21 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nenie</rad>
</kap>
<drv mrk="nenie.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo signifanta: &leftquot;en neniu ajn loko&rightquot;:
      <ekz>
        <tld/> mi povos &gcirc;in trovi;
      </ekz>
      <ekz>
        <tld/> estas al mi pli pla&ccirc;e ol en mia hejmo;
      </ekz>
      <ekz>
        ni <ind><tld/>n</ind> sendis &scirc;in.
        <trd lng="be">&c_n;&c_ib;&c_k;&c_u;&c_d;&c_y;</trd>
        <trd lng="hu">sehov&aacute;</trd>
        <trd lng="ru">&c_n;&c_i;&c_k;&c_u;&c_d;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_d;&c_z;&c_je;</trd>
  <trd lng="ca">enlloc</trd>
  <trd lng="cs">nikde</trd>
  <trdgrp lng="de">
    <trd>nirgends</trd>,
    <trd>nirgendwo</trd>
  </trdgrp>
  <trd lng="el">&pi;&omikron;&ypsilon;&theta;&epsilon;&ny;&alfa_ton;</trd>
  <trdgrp lng="en">
    <trd>nowhere</trd>,
    <trd>in no <ind>case</ind></trd>,
    <trd>not <ind>anywhere</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en ning&uacute;n <ind>sitio</ind></trd>,
    <trd>en ninguna <ind>parte</ind></trd>,
    <trd>en ning&uacute;n <ind>lugar</ind></trd>
  </trdgrp>
  <trd lng="fr">nulle part</trd>
  <trd lng="hu">sehol</trd>
  <trd lng="nl">nergens</trd>
  <trdgrp lng="pt">
    <trd>nenhures</trd>,
    <trd>em <klr tip="amb">lugar</klr> algum</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_g;&c_d;&c_je;</trd>
  <trd lng="sk">nikde</trd>
  <trd lng="sv">ingenstans</trd>
</drv>
</art>
