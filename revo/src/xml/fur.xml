</kap>

<drv mrk="fur.0o">
  <kap><tld/>o</kap>
  <snc mrk="fur.0o.CXAS">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="putor.cxas0o.CXAS">&ccirc;asputoro:</ref>
