<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: batist.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>batist</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="batist.0o">
  <kap><tld/>o</kap>
  <snc mrk="batist.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Plej delikata tolo el <ref tip="super" cel="lin.0o">lino</ref>:
      <ekz>
        vi <klr>...</klr> portos <tld/>ajn
        piedtukojn
        <fnt>
          <aut>trad. skirlet</aut>,
          <vrk><url ref="http://filmoj.net/12-segxoj">
          12 se&gcirc;oj</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">batista</trd>
  <trdgrp lng="cs">
    <trd>batist</trd>,
    <trd>kment <klr>(text.)</klr></trd>
  </trdgrp>
  <trd lng="de">Batist</trd>
  <trd lng="en">batiste</trd>
  <trd lng="es">batista</trd>
  <trd lng="fr">batiste</trd>
  <trd lng="hu">batiszt</trd>
  <trd lng="pl">batyst</trd>
  <trd lng="ru">&c_b;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">batist</trd>
</drv>
</art>
