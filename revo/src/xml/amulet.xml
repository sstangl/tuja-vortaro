<vortaro>
<art mrk="$Id: amulet.xml,v 1.16 2015/11/20 06:10:16 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>amulet</rad>/o
</kap>
<drv mrk="amulet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Objekto, kiu, per supozata magia efiko, &scirc;irmas kontra&ubreve;
      dan&gcirc;ero, malsano, sor&ccirc;o, ktp.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_u;&c_l;&c_je;&c_t;</trd>
  <trd lng="cs">amulet</trd>
  <trdgrp lng="de">
    <trd>Amulett</trd>,
    <trd>Talisman</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amulet</trd>,
    <trd>talisman</trd>,
    <trd>mojo</trd>
  </trdgrp>
  <trd lng="es">amuleto</trd>
  <trd lng="fr">amulette</trd>
  <trd lng="hu">amulett</trd>
  <trdgrp lng="nl">
    <trd>amulet</trd>,
    <trd>talisman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>amulet</trd>,
    <trd>talizman</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_u;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_i;&c_s;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>talizman</trd>,
    <trd>amulet</trd>
  </trdgrp>
</drv>
</art>
