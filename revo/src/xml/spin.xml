<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: spin.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>spin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spin.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tuto de la sinsekvaj <ref tip="prt"
      cel="verteb.0o">vertebroj</ref>
      formantaj la dorsan parton de
      la <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>
      &ccirc;e la vertebruloj = vertebraro, vertebra kolono. 
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>, 
      <trd>&c_h;&c_r;&c_y;&c_b;&c_je;&c_t;</trd>
    </trdgrp>
    <trd lng="de">R&uuml;ckgrat</trd>
    <trdgrp lng="en">
      <trd>spine</trd>,
      <trd>spinal column</trd>,
      <trd>backbone</trd>
    </trdgrp>
    <trd lng="es">columna vertebral</trd>
    <trd lng="fr"></trd>
    <trdgrp lng="fr">
      <trd>colonne vert&eacute;brale</trd>,
      <trd>&eacute;pine dorsale</trd>,
      <trd>rachis</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>colonna vertebrale</trd>,
      <trd>spina dorsale</trd>
    </trdgrp>
    <trd lng="nl">ruggegraat</trd>
    <trd lng="pl">kr&eogonek;gos&lstroke;up</trd>
    <trd lng="pt">espinha dorsal</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="spin.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Precipa akso, plej grava linio de io:
      <ekz>alta <ind>mont<tld/>o</ind> (suprolinio, vertolinio).
      <trdgrp lng="it">
        <trd>cresta<klr> (alp.)</klr></trd>,
        <trd>crinale</trd>
      </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_r;&c_y;&c_b;&c_je;&c_t;</trd>
    <trd lng="es">columna vertebral<klr> (fig.)</klr></trd>
    <trd lng="ru">&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>
  </snc>
  <trd lng="cs">páteř</trd>
  <trd lng="fa">&a_s;&a_t;&a_w;&a_n; &a_f;&a_q;&a_r;&a_A;&a_t;</trd>
  <trdgrp lng="sk">
    <trd>chrbtica</trd>,
    <trd>chrbtová kosť</trd>
  </trdgrp>
</drv>
</art>
