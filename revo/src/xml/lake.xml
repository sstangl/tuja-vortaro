<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lake.xml,v 1.21 2015/09/21 12:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lake</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lake.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lake.0o.servisto">
    <dif>
      Hejma <ref tip="super" cel="serv.0isto">servisto</ref> &ccirc;e 
      ri&ccirc;uloj, ordinare portanta livreon:
      <ekz>
        &scirc;i estis sen kale&scirc;o, sen <tld/>oj, kaj vestita per siaj 
        malbelaj vestoj
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk>Cindrulino</vrk>,
          <lok><url
          ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">Rakontoj
          pri feinoj</url>, Hachette, 1904</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_jo;&c_k;&c_a;&c_j;</trd>
    <trd lng="br">lakez</trd>
    <trdgrp lng="de">
      <trd>Lakai</trd>,
      <trd>Diener in Livree</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>servant</trd>,
      <trd>footman</trd>,
      <trd>flunkey</trd>,
      <trd>lackey</trd>
    </trdgrp>
    <trd lng="pl">lokaj</trd>
  </snc>
  <snc mrk="lake.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Humila&ccirc;a homo sen memestimo.
    </dif>
    <trdgrp lng="en">
      <trd>lackey</trd>,
      <trd>minion</trd>, <trd>flunky</trd>, <trd>flunkey</trd>
    </trdgrp>
    <trdgrp lng="be">
      <trd>&c_l;&c_jo;&c_k;&c_a;&c_j;</trd>,
      <trd>&c_h;&c_a;&c_l;&c_u;&c_j;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;&c_ch;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kriecher</trd>,
      <trd>unterw&uuml;rfiger Mensch</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>lokaj</trd>,
    <trd>pansk&yacute; sluha</trd>,
    <trd>p&rcaron;en. p&rcaron;isluhova&ccaron;</trd>
  </trdgrp>
  <trd lng="es">lacayo</trd>
  <trd lng="fr">laquais</trd>
  <trd lng="hu">lak&aacute;ly</trd>
  <trd lng="nl">lakei</trd>
  <trd lng="pt">lacaio</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_je;&c_j;</trd>
  <trd lng="sk">lokaj; poskok <klr>(pren)</klr></trd>
</drv>
</art>
