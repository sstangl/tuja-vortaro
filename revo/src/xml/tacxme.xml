<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tacxme.xml,v 1.22 2015/04/29 22:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ta&ccirc;ment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tacxme.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tacxme.0o.KOMUNE">
    <dif>
      Hom<ref tip="super" cel="grup.0o.personoj">grupo</ref> komisiita
      pri iu misio a&ubreve; tasko:
    </dif>
    <subsnc mrk="tacxme.0o.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Parto de regimento mar&scirc;anta a&ubreve; operacianta izole:
        <ekz>
          tuta <tld/>o da tamburistoj
          <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de Feli&ccirc;o</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_t;&c_r;&c_a;&c_d;</trd>
      <trd lng="ca">destacament</trd>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>detachment<klr tip="ind"> (military unit)</klr></trd>
      </trdgrp>
      <trd lng="es">destacamento</trd> 
      <trdgrp lng="it">
        <trd>distaccamento<klr> (mil.)</klr></trd>,
        <trd>squadra<klr> (mil.)</klr></trd>,
        <trd>commando</trd>,
        <trd>reparto<klr> (mil.)</klr></trd>,
        <trd>drappello<klr> (mil.)</klr></trd>,
        <trd>manipolo<klr> (mil.)</klr></trd>,
        <trd>compagnia<klr> (mil.)</klr></trd>,
      </trdgrp>
      <trd lng="nl">detachement</trd>
      <trd lng="pl">oddzia&lstroke;</trd>
      <trdgrp lng="pt">
        <trd>destacamento</trd>,
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_r;&c_ja;&c_d;</trd>
      <trd lng="sv">truppavdelning</trd>
    </subsnc>
    <subsnc mrk="tacxme.0o.LAB">
      <dif>
        Aro da homoj farantaj apartan laboron, a&ubreve; periode
        laborantaj dum alia aro ripozas:
        <ekz>
          kun studentaj konstruta&ccirc;mentoj mi estis en Sa&hcirc;alin
          <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 18a, p. 108a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a;
      <klr>(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_y;&c_h;)</klr></trd>
      <trdgrp lng="ca">
        <trd>colla</trd>,
        <trd>torn</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Trupp</trd>,
        <trd>Abteilung</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>team</trd>,
        <trd>shift</trd>,
        <trd>gang</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>nucleo<klr> (squadra)</klr></trd>,
        <trd>squadra</trd>,
        <trd>equipe</trd>,
        <trd>turno<klr> (squadra)</klr></trd>
      </trdgrp>
      <trd lng="nl">ploeg</trd>
      <trdgrp lng="pl">
        <trd>zmiana</trd>,
        <trd>hufiec</trd>,
        <trd>brygada</trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a;
      <klr tip="amb">(&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_h;)</klr></trd>
    </subsnc>
    <subsnc mrk="tacxme.0o.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Kunludantoj en konkurso.
      </dif>
      <refgrp tip="vid">
        <ref cel="parti.0o.JUR">partio <sncref/></ref>,
        <ref cel="team.0o.SPO">teamo</ref>
      </refgrp>
      <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a;
      <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;.)</klr></trd>
      <trd lng="ca">equip</trd>
      <trdgrp lng="de">
        <trd>Mannschaft</trd>,
        <trd>Truppe</trd>,
        <trd>Gruppe</trd>,
        <trd>Elf<klr> [futbalo]</klr></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>team</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>gruppo</trd>,
        <trd>squadra</trd>,
        <trd>team</trd>
      </trdgrp>
      <trd lng="nl">partij</trd>
      <trdgrp lng="pl">
        <trd>dru&zdot;yna</trd>
      </trdgrp>
      <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;
      <klr tip="amb">
      (&c_s;&c_p;&c_o;&c_r;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;)
      </klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;tachement</trd>,
    <trd>&eacute;quipe</trd>,
    <trd>mission<klr tip="amb"> (&eacute;quipe)</klr></trd>
  </trdgrp>
  <trd lng="hu">csapat</trd>
</drv>

<drv mrk="tacxme.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tacxme.0i.KOMUNE">
    <dif>
      Kunigi <tld/>on kaj 
      <ref tip="vid" cel="task.0i.KOMUNE">taski</ref>
      &gcirc;in pri iu faro:
      <ekz>
        li tuj <tld/>u siajn plej ta&ubreve;gajn spertulinojn
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;tacher<klr> (mil.)</klr></trd>
</drv>
</art>
