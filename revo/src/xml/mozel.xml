<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mozel.xml,v 1.10 2013/09/06 19:10:31 revo Exp $">
<kap>
  <rad>Mozel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mozel.M0o">
  <kap><tld/>o</kap>
  <snc mrk="mozel.M0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref> ekfluanta en <ref tip="vid"
      cel="franc.F0ujo">Francujo</ref>
      kaj enfluanta en <ref tip="vid" cel="rejn.R0o">Rejnon</ref> je
      Koblenco en <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>.
    </dif>
    <trd lng="de">Mosel</trd>
    <trd lng="fr">Moselle</trd>
    <trd lng="hu">Mosel</trd>
    <trd lng="lat">Mosella</trd>
    <trd lng="nl">Moezel</trd>
    <trd lng="pl">Mozela</trd>
    <trdgrp lng="pt">
      <trd>Mosela <klr tip="amb">(rio)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_M;&c_o;&c_z;&c_je;&c_l;&c_mol;</trd>
  </snc>
</drv>
</art>
