<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lokal.xml,v 1.8 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lokal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lokal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parto de domo kun apartaj funkcio kaj uzantoj,
      eventuale nur dumtempaj, ekzemple asocia kunvenejo, oficejeto:
      <ekz>
        la <tld/>o, en kiu estas legota mia raporto, estas fama bierejo
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_ja;&c_sh;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">local</trd>
  <trdgrp lng="cs">
    <trd>lokál</trd>,
    <trd>obch. místnost</trd>
  </trdgrp>
  <trd lng="es">local</trd>
  <trd lng="fr">local<klr> (subst.)</klr></trd>
  <trd lng="nl">lokaal<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pt">local</trd>
  <trdgrp lng="sk">
    <trd>lokál</trd>,
    <trd>obchodná miestnosť</trd>
  </trdgrp>
</drv>
</art>

