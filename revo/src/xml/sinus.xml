<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinus.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sinus</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sinus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="sinus.0o.entriangulo">
    <dif>
      <klr>(de akuta <ref cel="angul.0o.MAT">angulo</ref> <frm>&alfa;</frm>
      en orta triangulo)</klr>
      Kvociento de la <ref cel="later.0o.streko">latero</ref>
      <ref cel="kontraux.0a.triangulo">kontra&ubreve;a</ref> al &gcirc;ia
      <ref cel="vertic.0o.angulo">vertico</ref> per
      la <ref cel="hipote1.0o">hipotenuzo</ref>; 
      simb. <frm>sin &alfa;</frm>:
      <ekz>
        la <tld/>o de orto egalas al <frm>1</frm>;
      </ekz>
      <ekz>
        la <tld/>o de obtuza angulo difini&gcirc;as kiel la <tld/>o de
        &gcirc;ia <ref cel="suplem.0o.angulo">suplemento</ref>;
      </ekz>
      <ekz>
        la <tld/>o de konkava angulo difini&gcirc;as kiel la kontra&ubreve;egalo 
        de la <tld/>o de &gcirc;ia <ref cel="komple1.0o.aro">komplemento</ref>
        rilate al la plena angulo.
      </ekz>
    </dif>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm>&alfa;</frm> egalas al <frm>KM/OM</frm>
      -->
      la <tld/>o de angulo &alfa; egalas al KM/OM 
    </bld>
  </snc>
  <snc mrk="sinus.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu 
      &jcirc;etas reelon <frm><k>x</k></frm> al la 
      <tld/>o <sncref ref="sinus.0o.entriangulo"/> de angulo, kies
      mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas de
      <frm><k>x</k></frm> 
      per oblo de <frm>2&pi;</frm>; simb. <frm>sin</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun
        <ref cel="period.0o.funkcio">periodo <sncref/></ref>
        <frm>2&pi;</frm>;
      </ekz>
      <ekz>
        la rilato de <tld/>o al &gcirc;ia argumento strebas al <frm>1</frm>, 
        kiam la argumento strebas al <frm>0</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_u;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>dutina</trd>,
    <trd>záhyb</trd>
  </trdgrp>
  <trd lng="de">Sinus</trd>
  <trd lng="en">sine</trd>
  <trd lng="es">seno<klr tip="amb"> (matem&aacute;ticas)</klr></trd>
  <trd lng="fr">sinus</trd>
  <trd lng="hu">szinusz</trd>
  <trd lng="nl">sinus</trd>
  <trd lng="pl">sinus</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_u;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>dutina</trd>,
    <trd>záhyb</trd>
  </trdgrp>
</drv>

<drv mrk="sinus.are0o">
  <kap>are<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <ref tip="dif" cel="sinus.inversa_hiperbola0o">inversa hiperbola
    <tld/>o.</ref>
    <rim>
      Vd rimarkon sub <ref cel="are.0o.argumento">areo</ref>.
    </rim>
  </snc>
</drv>

<drv mrk="sinus.ark0o">
  <kap>ark<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref> 
      <ref cel="sinus.0o.funkcio"><tld/>o</ref> al la
      intervalo <frm>[-&pi;/2,&pi;/2]</frm>; 
      simb. <frm>arcsin</frm> a&ubreve; <frm>arksin</frm>:
      <ekz>
        ark<tld/>o de <frm>1</frm> egalas al <frm>&pi;/2</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_s;&c_ib;&c_n;&c_u;&c_s;</trd>
  <trd lng="de">Arkussinus</trd>
  <trd lng="en">arc sine</trd>
  <trd lng="fr">arc sinus</trd>
  <trd lng="hu">arkusz szinusz</trd>  
  <trd lng="nl">arcsinus</trd>
  <trd lng="pl">arcus sinus</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_s;&c_i;&c_n;&c_u;&c_s;</trd>
</drv>

<drv mrk="sinus.hiperbola0o">
  <kap>hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm><k>x</k></frm> al 
      <frm>&#189;(<k>e</k><sup><k>x</k></sup>-<k>e</k><sup>-<k>x</k></sup>)</frm>;
      
      simb. <frm>sinh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_s;&c_ib;&c_n;&c_u;&c_s;</ind></trd>
  <trdgrp lng="de">
    <trd>Hyperbelsinus</trd>,
    <trd>hyperbolischer <ind>Sinus</ind></trd>
  </trdgrp>
  <trd lng="en">hyperbolic <ind>sine</ind></trd>
  <trd lng="fr">sinus hyperbolique</trd>
  <trd lng="hu">szinusz hiperbolikusz</trd>  
  <trd lng="pl">sinus hiperboliczny</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_s;&c_i;&c_n;&c_u;&c_s;</ind></trd>
</drv>

<drv mrk="sinus.inversa_hiperbola0o">
  <kap>inversa hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="sinus.hiperbola0o">hiperbola <tld/>o</ref> al la
      intervalo <frm>[0,+&#8734;[</frm>; simb. <frm>arsinh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_s;&c_ib;&c_n;&c_u;&c_s;</ind></trd>
  <trd lng="de">Areasinus</trd>
  <trdgrp lng="en">
    <trd>area-hyperbolic sine</trd>,
    <trd>inverse hyperbolic sine</trd></trdgrp> 
  <trdgrp lng="fr">
    <trd>argument du <ind>sinus</ind> hyperbolique</trd>,
    <trd>sinus hyperbolique inverse</trd></trdgrp>
  <trd lng="hu">arkusz szinusz hiperbolikusz</trd>     
  <trd lng="pl">area sinus hiperboliczny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_a;-&c_s;&c_i;&c_n;&c_u;&c_s; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_s;&c_i;&c_n;&c_u;&c_s;</ind></trd></trdgrp> 
</drv>
</art>
