<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geohxemi.xml,v 1.2 2015/12/31 17:10:16 revo Exp $">
<kap>
  <rad>geo&hcirc;emi</rad>/o
  <fnt><vrk><url
  ref="http://www.majstro.com/vortaroj/esperanto-germana/geo%C4%A5emio">majstro</url></vrk></fnt>
</kap>
<drv mrk="geohxemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="geohxemi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Apliko de <ref tip="super" cel="hxemi.0o.SCI">&hcirc;emio</ref>,
      al mineraloj, &scirc;tonoj, roka&jcirc;oj;
      <ref tip="sin" cel="geokemi.0o">geokemio</ref>.
    </dif>
  </snc>
  <trd lng="en">geochemistry</trd>
  <trd lng="fr">g&eacute;ochimie</trd>
</drv>

<drv mrk="geohxemi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="geohxemi.0isto.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="geohxemi.0o.GEOL"><tld/>o</ref>;
      <ref tip="sin" cel="geokemi.0isto">geokemiisto</ref>:
      <ekz>
        Harmon Craig <klr>...</klr> estis usona <tld/>isto
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Harmon_Craig">
          Harmon Craig</url>, 2012-08-13</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">geochemist</trd>
  <trd lng="fr">g&eacute;ochimiste</trd>
</drv>
</art>

