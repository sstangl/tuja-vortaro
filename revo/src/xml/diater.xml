  <kap>
    <rad>diaterm</rad>/a
  </kap>
  <drv mrk="diater.0a">
    <kap><tld/>a</kap>
    <uzo tip="fak">FIZ</uzo>
    <snc>
      <dif>
        Translasanta la varmon.
      </dif>
    </snc>
  <trd
    lng="be">&c_d;&c_y;&c_ja;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>diatermální</trd>,
    <trd>průteplivý</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>diatherm</trd>,
      <trd>w&auml;rmedurchl&auml;ssig</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>diathermic</trd>,
      <trd>diathermal</trd>,
      <trd>diathermanous</trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>diaterm&aacute;n</trd>,
      <trd>diatermikus</trd>,
      <trd>h&odblac;&aacute;tereszt&odblac;</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>termoprzewodni</trd>
    </trdgrp>
  <trd lng="pt">diat&eacute;rmico</trd>
  <trd lng="sk">diatermálny</trd>
</drv>
</art>
