<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mugx.xml,v 1.14 2012/02/04 01:10:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mu&gcirc;</rad>/i
</kap>

<drv mrk="mugx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mugx.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel bovo.
    </dif>
    <trd lng="be">&c_m;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="en">
      <trd>moo</trd>,
      <trd>low</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>beugler</trd>,
      <trd>meugler</trd>
    </trdgrp>
    <trd lng="nl">loeien</trd>
    <trd lng="ru">&c_m;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="mugx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> a&ubreve;digante fortan,
      obtuzan, longe tiratan sonon:
      <ekz>
        <tld/>ado de &scirc;iphupo;
      </ekz>
      <ekz>
        <tld/>anta popolamaso;
      </ekz>
      <ekz>
        la rivero <tld/>is, &scirc;veligita de la printempaj pluvoj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la nigra
        virino</lok></fnt>;
      </ekz>
      <ekz>
        ventego <klr>...</klr> <tld/>is terure
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_w;&c_c;&c_ib;</trd>
    <trdgrp lng="en">
      <trd>roar</trd>,
      <trd>bellow</trd>
    </trdgrp>
    <trd lng="es">mugir</trd>
    <trd lng="nl">brullen</trd>
    <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">br&uuml;llen</trd>
  <trd lng="fr">mugir</trd>
  <trdgrp lng="pt">
    <trd>mugir</trd>,
    <trd>bramir</trd>,
    <trd>bramar</trd>,
    <trd>rugir</trd>
  </trdgrp>
</drv>
</art>
