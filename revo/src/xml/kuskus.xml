<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kuskus.xml,v 1.4 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kuskus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kuskus.0o">
  <kap><tld/>o</kap>
  <snc mrk="kuskus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Populara man&gcirc;a&jcirc;o el Nordafriko
      el vaporumita a&ubreve; boligita grio;
      &gcirc;in ofte akompanas fabaco,
      boligitaj legomoj, eventuale viando:
      <ekz>
        tunizia <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://sezonoj.ru/2014/11/svisujo/">
          Vivanta lingvo de vivanta komunumo</url></vrk>,
          <lok>2014-11-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>arabské knedlíčky</trd>,
    <trd>karbanátky</trd>,
    <trd>kus-kus</trd>,
    <trd>kuskus</trd>
  </trdgrp>
  <trd lng="fr">couscous</trd>
  <trd lng="nl">couscous</trd>
  <trd lng="no">couscous</trd>
  <trdgrp lng="sk">
    <trd>kus-kus</trd>,
    <trd>kuskus</trd>
  </trdgrp>
</drv>
</art>

