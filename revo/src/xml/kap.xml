    <trd>barani <ind>&lstroke;eb</ind></trd>
  </trdgrp>
  <trd lng="pt">cabe&ccedil;a-dura</trd>
</drv>

<drv mrk="kap.trans0igxo">
  <kap>trans<tld/>i&gcirc;o</kap>
  <snc mrk="kap.trans0igxo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="kaprio.0o.SPO">Kapriolo</ref> en kiu oni
      transpu&scirc;as la korpon super la mallevita kapo:
      <ekz>
	afi&scirc;oj kun trans<tld/>i&gcirc;antaj kla&ubreve;noj,
	dancistinoj <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="saltomortal.0o.SPO">saltomortalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>tombarella</trd>,
    <trd>capitomba</trd>,
    <trd>salt mortal</trd>
  </trdgrp>
  <trd lng="en">somersault</trd>
  <trdgrp lng="fr">
    <trd>coquillon</trd>,
    <trd>roulade<klr tip="amb"> (gym.)</klr></trd>
  </trdgrp>
  <trd lng="hu">bukfencezik</trd>
  <trdgrp lng="pl">
    <trd>przewr&oacute;t</trd>,
    <trd>fiko&lstroke;ek</trd>,
    <trd>salto</trd>
  </trdgrp>
  <trd lng="nl">kopstand</trd>
  <trd lng="pt">cambalhota</trd>
</drv>

<drv mrk="kap.vent0a">
  <kap>vent<tld/>a</kap>
  <snc mrk="kap.vent0a.FIG">
    <dif>
      Tute stulta, mense malkapabla:
      <ekz>
        li edzi&gcirc;is al bela kaj vent<tld/>a sinjorino
        <fnt>
          <aut>M. de Assis, trad. P. Viana</aut>
          <vrk>La divenistino kaj aliaj rakontoj</vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>baj&agrave;</trd>,
    <trd>cap de pardals</trd>
  </trdgrp>
  <trd lng="cs">lehkomysln&yacute;</trd>
  <trd lng="en">airhead</trd>
  <trdgrp lng="hu">
    <trd>hebehurgya</trd>,
    <trd>kelek&oacute;tya</trd>
  </trdgrp>
  <trd lng="nl">lichtzinnig</trd>
  <trdgrp lng="pl">
    <trd>z pust&aogonek; <ind>g&lstroke;ow&aogonek;</ind></trd>,
    <trd>z sianem w <ind>gowie</ind><klr> zamiast m&oacute;zgu</klr>
    </trd>
  </trdgrp>
  <trd lng="pt">cabe&ccedil;a-de-vento<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s; <ind>&c_v;&c_je;&c_t;&c_r;&c_o;&c_m;</ind> &c_v; &c_g;&c_o;&c_l;&c_o;&c_v;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;ahkomyse&lcaron;n&yacute;</trd>
</drv>

<drv mrk="kap.de0ogxiskalkano">
  <kap>de <tld/>o &gcirc;is kalkano,
  <var><kap>de <tld/>o &gcirc;is kalkanoj</kap></var></kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="kap.de0ogxiskalkano.KOMUNE">
    <dif>
      Tute, la&ubreve; la tuta korpo:
      <ekz>
        ili &ccirc;irka&ubreve;iradis &scirc;in kaj observadis &scirc;in de <tld/>o &gcirc;is kalkano
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/I.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">de la t&ecirc;te aux pieds</trd>
</drv>
