<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karot.xml,v 1.24 2014/06/10 15:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karot</rad>/o
</kap>

<drv mrk="karot.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="karot.0o.KUI">
    <dif>
      La man&gcirc;ebla radiko de la planto <tld/>o.
    </dif>
  </snc>
  <snc mrk="karot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Daucus</trd>), ofte kun unuopa nigra floro meze de
      la
      floraro. La oran&gcirc;kolora radiko de la kulturataj varia&jcirc;oj
      estas man&gcirc;ata kiel legomo.
    </dif>
    <bld lok="../bld/karot.jpg">
      floraro kun meza nigra floro
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_r;&c_k;&c_v;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_k;&c_o;&c_v;</trd>
  <trd lng="br">karotezenn</trd>
  <trdgrp lng="ca">
    <trd>pastanaga</trd>,
    <trd>carlota</trd>,
    <trd>safan&ograve;ria</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>M&ouml;hre</trd>,
     <trd>Mohrr&uuml;be</trd>,
     <trd>Gelbe R&uuml;be</trd>,
     <trd>Karotte</trd>
  </trdgrp>
  <trd lng="en">carrot</trd>
  <trd lng="es">zanahoria</trd>
  <trd lng="fr">carotte</trd>
  <trd lng="hu">s&aacute;rgar&eacute;pa</trd>
  <trd lng="id">wortel</trd>
  <trd lng="nl">wortel</trd>
  <trdgrp lng="pl">
    <trd>marchewka</trd>,
    <trd>marchew</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_k;&c_o;&c_v;&c_mol;</trd>
  <trd lng="tr">havu&ccedil;</trd>
</drv>
</art>
