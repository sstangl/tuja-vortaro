<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ikeban.xml,v 1.6 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>ikeban</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="ikeban.0o">
  <kap><tld/>o</kap>
  <snc mrk="ikeban.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Japandevena 
      <ref tip="lst" cel="art.bel0o.BELA" lst="voko:artoj">belarto</ref>
      pri ornama aran&gcirc;o de <ref tip="vid" cel="buked.0o">bukedoj</ref>
      el bran&ccirc;oj, folioj, floroj:
      <ekz>
        la <tld/>o-stiloj, tradiciaj stiloj estas <ctl>rikka,
        seika</ctl>;
        <ctl>nageire</ctl> a&ubreve; <ctl>cha-bana</ctl> estas pli novtipaj
        stiloj
        <fnt>
          <aut>Esperanto-Kultivejo Sue</aut>,
          <vrk><url
          ref="http://esperanto-hs.in.coocan.jp/Florarangho/Florbazo.html">
          Floraran&gcirc;o</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Floristik</trd>
  </snc>
  <snc mrk="ikeban.0o.ikebanajxo">
    <dif>
      <tld lit="I"/>a&jcirc;o, arta <ref tip="vid"
      cel="buked.0o">bukedo</ref>:
      <ekz>
        Nora <klr>...</klr> &scirc;atas fari <tld/>on
        <fnt>
        <aut>-</aut>,
        <vrk><url ref="http://jeachr.ostudio.org/?q=node/78">
        roloj</url></vrk>,
        <lok>2007-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gesteck</trd>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_je;&c_b;&c_a;&c_n;&c_a;</trd>
  <trd lng="cs">ikebana</trd>
  <trd lng="de">Ikebana</trd>
  <trd lng="en">ikebana</trd>
  <trd lng="fr">ikebana</trd>
  <trd lng="id">ikebana</trd>
  <trd lng="sk">ikebana</trd>
</drv>
</art>

