    <trd>alabastr</trd>,
    <trd>stavební sádra</trd>
  </trdgrp>
  <trd lng="de">Alabaster</trd>
  <trd lng="en">alabaster</trd>
  <trd lng="es">alabastro</trd>
  <trd lng="fr">alb&acirc;tre</trd>
  <trd lng="hu">alab&aacute;strom</trd>
  <trd lng="nl">albast</trd>
  <trd lng="pl">alabaster</trd>
  <trd lng="pt">alabastro</trd>
  <trd lng="ru">&c_a;&c_l;&c_je;&c_b;&c_a;&c_s;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>alabaster</trd>,
    <trd>úbeľ</trd>
  </trdgrp>
</drv>
</art>
