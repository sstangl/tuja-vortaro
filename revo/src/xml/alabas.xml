<kap>
  <ofc>1</ofc>
  <rad>alabastr</rad>/o
</kap>
<drv mrk="alabas.0o">
  <kap><tld/>o</kap>
  <snc mrk="alabas.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Speco de mola &scirc;tono diafaneta, similanta marmoron.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ja;&c_b;&c_a;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">alabastre</trd>
