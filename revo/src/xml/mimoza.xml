<vortaro>
<art mrk="$Id: mimoza.xml,v 1.8 2013/08/25 16:10:53 revo Exp $">
<kap>
  <rad>mimozac</rad>/oj
</kap>
<drv mrk="mimoza.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Mimosaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_m;&c_o;&c_z;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;&c_w;)</klr></trd>
  <trd lng="ca">mimos&agrave;cies</trd>
  <trd lng="de">Mimosengew&auml;chse</trd>
  <trd lng="hu">mim&oacute;zaf&eacute;l&eacute;k</trd>
  <trd lng="nl">mimosafamilie</trd>
  <trd lng="pt">mimos&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_i;&c_m;&c_o;&c_z;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
