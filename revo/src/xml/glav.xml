<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glav.xml,v 1.26 2016/02/28 18:10:22 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>glav</rad>/o
</kap>
<drv mrk="glav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glav.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="arm.0ilo">armilo</ref> konsistanta
      el longa klingo kaj mallonga tenilo:
      <ekz>
        &ccirc;iuj, kiuj <tld/>on prenas, per <tld/>o pereos
        <fnt><bib>NT</bib>, <lok>&Mat; 26:52</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ponard.0o">ponardo</ref>,
      <ref cel="bajone.0o">bajoneto</ref>
    </refgrp>
   <trd lng="it">spada</trd>
  </snc>
  <snc mrk="glav.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Punpovo:
       <ekz>
         la <tld/>o de la Le&gcirc;o.
       </ekz>
     </dif>
    <trd lng="it">braccio<klr> (figurato)</klr></trd>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_ch;</trd>
  <trd lng="bg">&c_m;&c_je;&c_ch;</trd>
  <trd lng="ca">espasa</trd>
  <trd lng="cs">me&ccaron;</trd>
  <trd lng="de">Schwert</trd>
  <trd lng="en">sword</trd>
  <trd lng="es">espada</trd>
  <trdgrp lng="fr">
    <trd>glaive</trd>,
    <trd>&eacute;p&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">kard</trd>
  <trd lng="it">braccio<klr> (figurato)</klr></trd>
  <trd lng="nl">zwaard</trd>
  <trdgrp lng="pt">
    <trd>gl&aacute;dio</trd>,
    <trd>espada</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_ch;</trd>
  <trd lng="sk">me&ccaron;</trd>
  <trd lng="sv">sv&auml;rd</trd>
</drv>

<drv mrk="glav.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
    <dif>
      &Scirc;irma ingo, en kiu oni enigas la klingon de <tld/>o:
      <ekz>
        <tld/>o <klr>...</klr> pendis en la <tld/>ingo &ccirc;e lia lumbo
        kaj facile povis eliri kaj eniri
        <fnt><bib>MT</bib>, <lok>&Sa2; 20:8</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_zh;&c_n;&c_y; 
<klr tip="amb">(&c_m;&c_ja;&c_ch;&c_a;)</klr></trd>
  <trd lng="bg">&c_n;&c_o;&c_zh;&c_n;&c_i;&c_c;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">beina</trd>
  <trd lng="cs">pochva na me&ccaron;</trd>
  <trd lng="de">Scheide</trd>
  <trd lng="en">scabbard</trd>
  <trd lng="fr">fourreau<klr tip="ind"> (d'une &eacute;p&eacute;e)</klr></trd>
  <trd lng="hu">kardh&uuml;vely</trd>
  <trd lng="it">fodero</trd>
  <trd lng="nl">schede<klr tip="ind"> (v.zwaard)</klr></trd>
  <trd lng="pt">bainha</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;&c_n;&c_y; <klr>(&c_m;&c_je;&c_ch;&c_a;)
</klr></trd>
  <trd lng="sk">po&scaron;va me&ccaron;a</trd>
  <trd lng="sv"><klr>(sv&auml;rds)</klr>skida</trd>
</drv>

<drv mrk="glav.0bati0otrancxi0opiki">
  <kap><tld/>bati, <var><kap><tld/>otran&ccirc;i</kap></var>,
  <var><kap><tld/>opiki</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Frapi, tran&ccirc;i, piki per <tld/>o.
    </dif>
  </snc>
  <adm>
    Temas pri 3 diverssencaj verboj. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol; 
<klr tip="amb">(&c_m;&c_ja;&c_ch;&c_o;&c_m;)</klr></trd>
  <trdgrp lng="ca">
    <trd>copejar, ferir amb l'espasa</trd>,
    <trd>fer una estocada</trd>
  </trdgrp>
  <trd lng="fr">estoquer</trd>
  <trdgrp lng="pt">
    <trd>espadear</trd>,
    <trd>ferir com a espada</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; 
<klr tip="amb">(&c_m;&c_je;&c_ch;&c_o;&c_m;)</klr></trd>
<trd lng="it">colpire<klr> (con la spada)</klr></trd>
  <trd lng="sk">pichn&uacute;&tcaron; me&ccaron;om</trd>
  <trdgrp lng="cs">
    <trd>bodat me&ccaron;em</trd>,
    <trd>p&iacute;chat me&ccaron;em</trd>,
    <trd>p&iacute;chnout me&ccaron;em</trd>
  </trdgrp>
</drv>
</art>
