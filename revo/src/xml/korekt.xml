<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korekt.xml,v 1.26 2015/07/04 08:37:38 revo Exp $">
<kap><ofc>*</ofc>
<rad>korekt</rad>/i</kap>
<drv mrk="korekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Re&gcirc;ustigi la erarojn a&ubreve; rebonigi la difektojn de iu a&ubreve;
      io:
      <ekz>
        sin <tld/>i;
      </ekz>
      <ekz>
        <tld/>i taskon de lernanto, presprova&jcirc;on, la prononcon de
        fremdulo, kalkulon, fotografian kopion;
      </ekz>
      <ekz>
        pli facile estas malbonon eviti ol
        <tld/>i<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plia esploro <tld/>is mian opinion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bon.re0igi">rebonigi</ref>,
      <ref cel="tusx.re0i">retu&scirc;i</ref>,
      <ref cel="amend">amendamenti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">corregir</trd>
  <trdgrp lng="cs">
    <trd>korigovat</trd>,
    <trd>napravovat</trd>,
    <trd>opravit</trd>,
    <trd>opravovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>berichtigen</trd>,
    <trd>verbessern</trd>,
    <trd>korrigieren</trd>
  </trdgrp>
  <trd lng="en">correct</trd>
  <trd lng="es">corregir</trd>
  <trdgrp lng="fr">
    <trd>corriger</trd>,
    <trd>rectifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kijav&iacute;t</trd>,
    <trd>helyesb&iacute;t</trd>
    <trd>korrig&aacute;l</trd>
  </trdgrp><trdgrp lng="hu">
    <trd>kijav&iacute;t</trd>,
    <trd>helyesb&iacute;t</trd>,
    <trd>korrig&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbeteren</trd>,
    <trd>corrigeren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>corrigir</trd>,
    <trd>retificar</trd>,    
    <trd>emendar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
   
<trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korigovať</trd>,
    <trd>opraviť</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, rimarko de <tld/>anto:
      <ekz>
        <tld/>oj en presprova&jcirc;o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_r;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>correcci&oacute;</trd>,
    <trd>rectificaci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korekce</trd>,
    <trd>korektura</trd>,
    <trd>oprava</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Berichtigung</trd>,
    <trd>Verbesserung</trd>,
    <trd>Korrektur</trd>
  </trdgrp>
  <trd lng="es">correcci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>correction</trd>,
    <trd>rectification</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kijav&iacute;t&aacute;s</trd>,
    <trd>helyesb&iacute;t&eacute;s</trd>,
    <trd>korrekci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbetering</trd>,
    <trd>correctie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korektúra</trd>,
    <trd>oprava</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.0a">
  <kap><tld/>a</kap>
  <snc mrk="korekt.0a.gxustiga">
    <dif>
      Rilata al <tld/>(ad)o:
      <ekz>
        por<tld/>a tabelo;
      </ekz>
      <ekz>
        <tld/>aj reguloj de presejo
        <fnt><bib>PIV3</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a domo (establejo, kie oni provas <tld/>i deliktulojn)
        <fnt><bib>PIV3</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>correctiu</trd>,
      <trd>correccional</trd>
    </trdgrp>
    <trd lng="en">corrective</trd>
    <trdgrp lng="fr">
      <trd>correctionnel</trd>,
      <trd>correctif</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>corrigerend</trd>,
    <trd>correctioneel</trd>
  </trdgrp>
  </snc>
  <snc mrk="korekt.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Enhavanta plu neniun eraron a&ubreve; difekton:
      <ekz>
        <tld/>a eldono, teksto, desegno;
      </ekz>
      <ekz>
        la <tld/>eco de la lingvo devus esti la unua zorgo de &ccirc;iu
        verkisto;
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>corregit</trd>,
      <trd>revisat</trd>,      
      <trd>lliure d'errates</trd>
    </trdgrp>
    <trdgrp lng="de">
    <trd>richtig</trd>,
    <trd>fehlerfrei</trd>,
    <trd>korrekt</trd>
  </trdgrp>
  <trd lng="es">correcto</trd>
  <trd lng="nl">correct<klr tip="ind"> (ontr.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezvadný</trd>,
    <trd>korekční</trd>,
    <trd>nápravný</trd>,
    <trd>opravný</trd>,
    <trd>správný</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helyes</trd>,
    <trd>korrekt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezchybný</trd>,
    <trd>korektný</trd>,
    <trd>správny</trd>
  </trdgrp>
</drv>

<drv mrk="korekt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Enhavanta erarojn
    </dif>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="makul.0i">makuli</ref>,
      <ref cel="bel">malbeligi</ref>,
      <ref cel="bon.mal0igi">malbonigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>erroni</trd>,
    <trd>incorrecte</trd>
  </trdgrp>
  <trd lng="cs">nesprávný</trd>
  <trdgrp lng="de">
    <trd>falsch</trd>,
    <trd>verkehrt</trd>,
    <trd>fehlerhaft</trd>,
    <trd>inkorrekt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>err&oacute;neo</trd>,
    <trd>equivocado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>incorrect</trd>,
    <trd>erron&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helytelen</trd>,
    <trd>hib&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fout<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>niet <ind>gecorrigeerd</ind><klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_sh;&c_i;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">nesprávny</trd>
</drv>

<drv mrk="korekt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i komposterarojn en presejo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">corrector</trd>
  <trdgrp lng="cs">
    <trd>korektor</trd>,
    <trd>oprávce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Korrektor</trd>
  </trdgrp>
  <trd lng="es">corrector</trd>
  <trd lng="fr">correcteur</trd>
  <trd lng="hu">korrektor</trd>
  <trd lng="nl">corrector</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">korektor</trd>
</drv>

<drv mrk="korekt.pun0i">
  <kap>pun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi plibonigi ies konduton per puno; edukpuni:
      <ekz>
        pun<tld/>i malican infanon;
      </ekz>
      <ekz>
        <ind>pun<tld/>ejo</ind>;
        <trd lng="ca">reformatori</trd>
        <trd lng="hu">jav&iacute;t&oacute;int&eacute;zet</trd>
        <trd lng="nl">strafinstelling</trd>
      </ekz>
      <ekz>
        <ind>pun<tld/>a tribunalo</ind>.
        <trd lng="ca">Tribunal Correccional</trd>
        <trd lng="de">Strafgericht</trd>
        <trd lng="fr">tribunal correctionnel</trd>
        <trd lng="nl">correctionele <ind>rechtbank</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">punir</trd>
  <trd lng="fr">corriger (en punissant)</trd>
  <trd lng="hu">b&uuml;ntetve <ind>jav&iacute;t</ind></trd>
  <trd lng="nl">bestraffen</trd>
</drv>

<drv mrk="korekt.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="K"/>i skribante &gcirc;ustan formon sur ne&gcirc;ustan:
      <ekz>
        estas malpermesite sur<tld/>i oficialajn dokumentojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">esmenar</trd>
  <trd lng="cs">provést opravu</trd>
  <trdgrp lng="de">
    <trd>ausbessern</trd>,
    <trd>&uuml;berschreiben</trd>
  </trdgrp>
  <trd lng="fr">surcharger (un document)</trd>
  <trd lng="hu">fel&uuml;l&iacute;r</trd>
  <trd lng="nl">overschr&iacute;jven</trd>
  <trd lng="sk">prepísať nesprávny text správnym</trd>
</drv>
</art>
