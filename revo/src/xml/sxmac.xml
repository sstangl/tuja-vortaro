<vortaro>
<art mrk="$Id: sxmac.xml,v 1.13 2015/08/12 12:10:16 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>&scirc;mac</rad>/i
</kap>
<drv mrk="sxmac.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
  <snc>
  <dif>
Kisi a&ubreve; man&gcirc;i kun bruo: <ekz>li time kra&ccirc;otusis k <tld/>is
per la lipoj<fnt>K</fnt>;
    </ekz> <ekz>la <tld/>ado de 5 bu&scirc;oj, kiuj man&gcirc;is kun granda
apetito<fnt>B</fnt>;
    </ekz> (f) <ekz>la koto <tld/>is (klakis kiel <tld/>o) sub la
    piedoj.
    </ekz>
  </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_m;&c_o;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_ch;&c_m;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_ch;&c_v;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schmatzen</trd>
  <trd lng="en">smack</trd>
  <trd lng="es">besar o comer con ruido</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_l;&f_ch;&zwnj;&a_m;&a_l;&a_w;&f_ch; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_A;&f_ch;&zwnj;&a_w;&a_m;&a_w;&f_ch; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&aacute;mcsog</trd>, 
    <trd>cuppant</trd>, 
    <trd>cuppog</trd>
  </trdgrp>
  <trd lng="it">schioccare<klr> (con bocca o labbra)</klr></trd>
  <trd lng="nl">smakken</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_v;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_m;&c_o;&c_k;&c_a;&c_t;&c_mol; 
<klr>(&c_g;&c_u;&c_b;&c_a;&c_m;&c_i;)</klr></trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_a;&c_t;&c_mol; <klr>(&c_o; 
&c_g;&c_r;&c_ja;&c_z;&c_i;)</klr></trd>
  </trdgrp>
</drv>
</art>
