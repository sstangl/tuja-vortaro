<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: juli.xml,v 1.31 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>juli</rad>/o
</kap>

<drv mrk="juli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="juli.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sepa <ref tip="lst" cel="monat.0o.KAL"
      lst="voko:monatoj" val="7">monato</ref>  de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> en la gregoria kalendaro.
    </dif>
    <rim>
      Vidu la rimarkon &ccirc;e 
<ref tip="vid" cel="januar.0o.RIM">januaro</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_l;&c_i;</trd>
  <trd lng="br">Gouere</trd>
  <trd lng="cs">&ccaron;ervenec</trd>
  <trd lng="de">Juli</trd>
  <trd lng="el">
&Jota;&omikron;&ypsilon_ton;&lambda;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">July</trd>
  <trd lng="es">julio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&f_y;&a_h;</trd>
  <trd lng="fr">juillet</trd>
  <trd lng="he">&yod;&vav;&lamed;&yod;</trd>
  <trd lng="hu">j&uacute;lius</trd>
  <trd lng="id">Juli</trd>
  <trd lng="it">luglio</trd>
  <trd lng="ja">&#19971;&#26376; [<ind>&#12375;&#12385;&#12364;&#12388;
</ind>]</trd>
  <trd lng="nl">juli</trd>
  <trd lng="pl">lipiec</trd>
  <trd lng="pt">julho</trd>
  <trd lng="ro">iulie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_l;&c_mol;</trd>
  <trd lng="sk">júl</trd>
  <trd lng="sv">juli</trd>
  <trd lng="tr">temmuz</trd>
  <trd lng="vo">yulul</trd>
</drv>

<drv mrk="juli.J0o">
  <kap><tld lit="J"/>o</kap>
  <snc mrk="juli.J0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        ha, kara <tld lit="J"/>o, se mia artikolo <klr>...</klr> estos
        presita, tiam mi por la honorario tuj a&ccirc;etos al mi 
plu&scirc;an
        matenveston
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">červenec</trd>
  <trd lng="de">Julius</trd>
  <trd lng="fa">&f_zh;&a_w;&a_l;&f_y;&a_w;&a_s;</trd>
  <trd lng="fr">Jules</trd>
  <trd lng="it">Giulio</trd>
  <trd lng="ja">&#12518;&#12522;&#12454;&#12473; 
[<ind>&#12422;&#12426;&#12358;&#12377;</ind>]</trd>
  <trd lng="pl">Julian</trd>
  <trd lng="sk">júl</trd>
  <trd lng="sv">Julius</trd>
</drv>
</art>
