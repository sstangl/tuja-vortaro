<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amfibi.xml,v 1.24 2015/10/27 11:10:25 revo Exp $">
<kap>
  <ofc>*</ofc><rad>amfibi</rad>/oj
</kap>
<drv mrk="amfibi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <tld/>oj.
    </dif>
  </snc>
  <trd lng="cs">oboj&zcaron;iveln&iacute;k</trd>
  <trd lng="fr">amphibien</trd>
  <trd lng="pl">p&lstroke;az</trd>
  <trdgrp lng="sk">
    <trd>amf&iacute;bia</trd>,
    <trd>oboj&zcaron;iveln&iacute;k</trd>
  </trdgrp>
</drv>
<drv mrk="amfibi.0oj">
  <kap><tld/>oj</kap>
   <snc>
   <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">Klaso</ref> de <ref tip="malprt"
      cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Amphibia</trd>), nudha&ubreve;taj, kies larvoj
      vivas en dol&ccirc;akvo spirante per brankoj kaj kies plenkreskuloj
      vivas surtere spirante per pulmoj.
    </dif>
    <refgrp tip="prt">
      <ref cel="vost.sen0uloj">senvostuloj</ref>,
      <ref cel="urodel.0oj">urodeloj</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_f;&c_ib;&c_b;&c_ib;&c_ib;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="br">amfibienned</trd>
  <trd lng="de">Lurche</trd>
  <trd lng="en">amphibians</trd>
  <trd lng="fr">amphibiens<klr> (classe des)</klr></trd>
  <trd lng="he">&dalet;&vav;-&het;&yod;&yod;&fmem;</trd>
  <trd lng="hu">k&eacute;t&eacute;lt&udblac;ek</trd>
  <trd lng="nl">amfibie&euml;n</trd>
  <trd lng="pl">p&lstroke;azy</trd>
  <trd lng="pt">anf&iacute;bio</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_a;&c_m;&c_f;&c_i;&c_b;&c_i;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="sk">oboj&zcaron;iveln&iacute;ky</trd>
  <trd lng="tp">akesi</trd>
</drv>
</art>
