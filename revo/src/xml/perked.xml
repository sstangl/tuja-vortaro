<art mrk="$Id: perked.xml,v 1.9 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>perked</rad>/oj
</kap>
<drv mrk="perked.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="perk.0oformaj">perkoformaj</ref>
      fi&scirc;oj (<trd lng="la">Percidae</trd>).
    </dif>
  </snc>
  <trd lng="ca">p&egrave;rcids</trd>
  <trd lng="cs">okounovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Barsche</trd>
  <trd lng="hu">s&uuml;g&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">echte baarzen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">perc&iacute;deos</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_n;&c_je;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">ostriežovité</trd>
</drv>
</art>

