<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sest.xml,v 1.8 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sest</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="sest.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="sest.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el ses sinsekvaj tonoj de diatona gamo.
    </dif>
  </snc>
  <snc mrk="sest.0o.de.toniko">
    <dif>
      Sesa tono de gamo.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="sekst.0o">seksto</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;&c_t;&c_a;</trd>
  <trd lng="ca">sexta<klr> (m&uacute;s.)</klr></trd>
  <trd lng="de">Sexte</trd>
  <trdgrp lng="fr">
    <trd>sixte<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">szext</trd>
  <trd lng="it">sesta<klr> (mus.)</klr></trd>
  <trd lng="nl">sext</trd>
  <trd lng="pl">seksta</trd>
  <trd lng="ro">sext&abreve;</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_s;&c_t;&c_a;</trd>
  <trd lng="sk">sexta <klr>(interval)</klr></trd>
  <trd lng="cs">sexta <klr>(hud. interval)</klr></trd>
</drv>
</art>

