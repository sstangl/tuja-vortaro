<vortaro>
<art mrk="$Id: eranti.xml,v 1.13 2013/08/30 14:10:53 revo Exp $">
<kap>
<rad>erantid</rad>/o
</kap>
<drv mrk="eranti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Eranthis</trd>), tre frue en vintro
      floranta, kun flavaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/erantid.jpg">
    <ind>sude&ubreve;ropa <tld/>o</ind>
    <trd lng="la">Eranthis hyemalis</trd>
    <trd lng="hu">csillagos <ind>t&eacute;ltemet&odblac;</ind></trd>
  </bld>
  <trd lng="de">Winterling</trd>
  <trd lng="fr">&eacute;ranthis</trd>
  <trdgrp lng="pt">
    <trd>eranto</trd>,
    <trd>erantis</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;ltemet&odblac;</trd>,
    <trd>t&eacute;like</trd></trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
