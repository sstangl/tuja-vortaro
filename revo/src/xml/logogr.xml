<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: logogr.xml,v 1.14 2013/01/28 15:10:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>logogrif</rad>/o
</kap>
<drv mrk="logogr.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Enigmo, en kiu intermiksitaj literoj a&ubreve; silaboj de iu vorto formas
      aliajn vortojn, kiujn oni devas diveni.
    </dif>
    <ref tip="vid" cel="anagra.0o">anagramo</ref>
    <trdgrp lng="hu">
      <trd>sz&oacute;rejtv&eacute;ny</trd>,
      <trd>bet&udblac;rejtv&eacute;ny</trd>,
      <trd>logogrif</trd>
    </trdgrp>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nekomprenebla frazo.
    </dif>
    <trd lng="hu">tal&aacute;nyos mondat</trd>    
  </snc>
  <trd lng="ca">logogrif</trd>
  <trdgrp lng="de">
     <trd>Wortr&auml;tsel</trd>,
     <trd>Buchstabenr&auml;tsel</trd>,
     <trd>Logogriph</trd>
  </trdgrp>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_a;&c_g;&c_r;&c_y;&c_f;</trd>
  <trd lng="en">logogriph</trd>
  <trd lng="es">logogrifo</trd>
  <trd lng="nl">letterraadsel</trd>
  <trd lng="pt">logogrifo</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_o;&c_g;&c_r;&c_i;&c_f; <klr>(&c_r;&c_o;&c_d; &c_z;&c_a;&c_g;&c_a;&c_d;&c_k;&c_i;)</klr></trd>
</drv>
</art>
