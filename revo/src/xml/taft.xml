<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: taft.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>taft</rad>/o
</kap>
<drv mrk="taft.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre maldika brila silka a&ubreve; &hcirc;emia <ref tip="super"
      cel="sxtof.0o.TEKS">&scirc;tofo</ref>:
      <ekz>
        Majstro Pedro havis <klr>...</klr> preska&ubreve; la duonon de
        la vango kovrita per peco el verda <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 25a, p.
        537a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_f;&c_t;&c_a;</trd>
  <trd lng="ca">tafet&agrave;</trd>
  <trd lng="cs">taft</trd>
  <trd lng="de">Taft</trd>
  <trd lng="en">taffeta</trd>
  <trd lng="fr">taffetas</trd>
  <trdgrp lng="hu">
    <trd>taft</trd>,
    <trd>tafota</trd>
  </trdgrp>
  <trd lng="nl">taffetas</trd>
  <trdgrp lng="pl">
    <trd>tafta</trd>,
    <trd>kitajka</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>tafet&aacute;</trd>,
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_f;&c_t;&c_a;</trd>
  <trd lng="sk">taft</trd>
  <trd lng="sv">taft</trd>
</drv>
</art>
