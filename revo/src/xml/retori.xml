  <trd lng="be">&c_r;&c_y;&c_t;&c_o;&c_r;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>rétorika</trd>,
    <trd>řečnictví</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rhetorik</trd>,
    <trd>Redekunst</trd>
