<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kavern.xml,v 1.18 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kavern</rad>/o
</kap>
<drv mrk="kavern.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kavern.0o.KOMUNE">
    <dif>
      Natura profunda subtera kavo:
      <ekz>
        la anta&ubreve;historioj homoj lo&gcirc;is en <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grot.0o">groto</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_ja;&c_ch;&c_o;&c_r;&c_a;</trd>  
    <trdgrp lng="br">
      <trd>kev<klr tip="ind"> (mougev)</klr></trd>,
      <trd>mougev</trd>
    </trdgrp>
    <trd lng="de">H&ouml;hle</trd>
    <trdgrp lng="en">
      <trd>cavern</trd>,
      <trd>cave</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caverne<klr tip="ind"> (grotte)</klr></trd>,
      <trd>grotte<klr tip="ind"> (naturelle)</klr></trd>
    </trdgrp>
    <trd lng="hu">barlang</trd>
    <trd lng="id">gua</trd>
    <trdgrp lng="nl">
      <trd>hol <klr tip="ind">(z.nw.)</klr></trd>,
      <trd>spelonk</trd>
    </trdgrp>
    <trd lng="pl">jaskinia</trd>
    <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="kavern.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsaneca kavo en organo:
      <ekz>
        <tld/>oj de la pulmoj &ccirc;e tuberkulozo;
      </ekz>
      <ekz>
        <tld/>o en arbo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>,
      <trd>&c_k;&c_a;&c_v;&c_e;&c_r;&c_n;&c_a;</trd>
    </trdgrp>             
    <trd lng="br">kleuzadenn<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
    <trd lng="de">Kaverne</trd>
    <trd lng="en">cavern</trd>
    <trd lng="hu">kaverna</trd>
    <trd lng="fr">caverne<klr tip="ind"> (m&eacute;d.)</klr></trd>
    <trd lng="nl">holte</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_d;&c_u;&c_p;&c_l;&c_o;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>dutina</trd>,
    <trd>jeskyně</trd>,
    <trd>kaverna</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dutina</trd>,
    <trd>jaskyňa</trd>,
    <trd>kaverna</trd>
  </trdgrp>
</drv>

<drv mrk="kavern.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Prezentanta <tld/>on:
      <ekz>
        <tld/>a arbo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &scirc;tonego, dento, pulmo.
      </ekz>
    </dif>
    <trd lng="hu">barlangi</trd>
    <trd lng="pl">jaskiniowy</trd>
  </snc>
  <snc>
    <dif>
      Obtuze sonora kiel <tld/>o:
      <ekz>
        <tld/>a blovo, stertoro, vo&ccirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tomb">tomba</ref>,
      <ref cel="cxerk.0a">&ccirc;erka</ref>.
    </refgrp>
    <trd lng="hu">&ouml;bl&ouml;s</trd>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="br">kleuz</trd>
  <trdgrp lng="cs">
    <trd>dutý</trd>,
    <trd>kavernový</trd>
  </trdgrp>
  <trd lng="en">cavernous</trd>
  <trdgrp lng="fr">
    <trd>creux<klr tip="ind"> (pr&eacute;sentant des cavernes)</klr></trd>,
    <trd>pr&eacute;sentant des <ind>cavernes</ind></trd>,
    <trd>de <ind>caverne</ind></trd>,
    <trd>caverneux</trd>
  </trdgrp>
  <trd lng="nl">hol</trd>
  <trd lng="ru">&c_p;&c_je;&c_shch;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dutý</trd>,
    <trd>kavernový</trd>
  </trdgrp>
</drv>
</art>
