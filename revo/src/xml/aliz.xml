<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aliz.xml,v 1.15 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>aliz</rad>/o
</kap>
<drv mrk="aliz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Frukto de <tld/>arbo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>břekyně</trd>,
    <trd>plod jeřábu muku</trd>
  </trdgrp>
  <trd lng="de">Mehlbeere</trd>
  <trd lng="fr">alise</trd>
  <trd lng="nl">meelbes</trd>
  <trd lng="pl">jarz&eogonek;bina</trd>
  <trdgrp lng="sk">
    <trd>brekyňa</trd>,
    <trd>mukyňa</trd>
  </trdgrp>
</drv>

<drv mrk="aliz.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="sorp.0o">sorpo</ref>
      (<trd lng="la">Sorbus aria</trd>), kun oran&gcirc;koloraj
      &gcirc;is skarlatru&gcirc;aj, interne flavaj &scirc;ajnfruktoj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>břekyně</trd>,
    <trd>jeřáb muk</trd>
  </trdgrp>
  <trd lng="de">Mehlbeerbaum</trd>
  <trd lng="en">common whitebeam</trd>
  <trd lng="fr">alisier</trd>
  <trdgrp lng="hu">
    <trd>lisztes <ind>berkenye</ind></trd>, 
    <trd>s&uuml;v&ouml;lt&eacute;ny</trd>
  </trdgrp>
  <trd lng="pl">jarz&eogonek;bina</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_i;&c_n;&c_a; &c_a;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>brekyňa</trd>,
    <trd>mukyňa</trd>
  </trdgrp>
  <trd lng="sv">vitoxel</trd>
</drv>
</art>
