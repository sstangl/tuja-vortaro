<vortaro>
<art mrk="$Id: sudan.xml,v 1.14 2014/06/04 16:10:42 revo Exp $">
<kap>
  <rad>Sudan</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="sudan.S0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="sudan.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Kartumo.
      Teritorio de Respubliko Sudano.
      ISO-3166 kodo: SD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="afrik.CentrA0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="egipt.E0ujo.GEO">Egiptujo</ref>,
      <ref tip="vid" cel="eritre.E0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="etiop.E0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      <ref tip="vid" cel="libi.L0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="ugand.U0o.GEOG">Ugando</ref>
      kaj <ref tip="vid" cel="mar.RugxaM0o">Ru&gcirc;a <tld
      lit="M"/>o</ref>.
    </dif>
    </snc>
    <trd lng="ar">&a_A;&a_l;&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
    <trd lng="be">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
    <trd lng="bg">&c_S;&c_u;&c_d;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>el Sudan</trd>,
      <trd>Rep&uacute;blica del Sudan</trd>
    </trdgrp>
    <trd lng="de">Sudan</trd>
    <trd lng="en">Sudan</trd>
    <trd lng="fa">&a_s;&a_w;&a_d;&a_A;&a_n;</trd>
    <trd lng="fr">Soudan</trd>
    <trd lng="id">Sudan</trd>
    <trd lng="it">Sudan</trd>
    <trd lng="nl">Soedan</trd>
    <trd lng="no">Sudan</trd>
    <trdgrp lng="pl">
      <trd>Sudan</trd>,
      <trd>Republika <ind>Sudanu</ind></trd>
    </trdgrp>
    <trd lng="pt">Sud&atilde;o</trd>
  </drv>
</art>
