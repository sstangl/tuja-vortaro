<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinkron.xml,v 1.4 2015/11/04 15:10:20 revo Exp $">
<kap>
  <rad>sinkron</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sinkron.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sinkron.0a.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="temp.sam0a.KOMUNE">Samtempa</ref>,
      <ref tip="dif" cel="takt.sam0a.KOMUNE">samtakta</ref>;
      konsideranta aferojn en difinita momento, sen tempa evoluo;
      kontraste al <ref tip="ant" cel="diakron.0a.SCI">diakrona</ref>:
      <ekz>
        etaj galaksioj, kiuj movi&gcirc;as interne de la Andromeda,
        <klr>...</klr> rotacias la&ubreve; <tld/>a rapideco
        <fnt>
          <aut>P. S. Viana</aut>,
          <vrk><url
          ref="http://retradio.webnode.at/news/dek-kvin-jara-sciencisto/">
          Dek-kvin-jara sciencisto</url></vrk>,
          <lok>esperanta retradio, 2013-01-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">synchronic</trd>
  <trd lng="fr">synchronique</trd>
  <trdgrp lng="pl">
   <trd>jednoczesny</trd>,
   <trd>synchroniczny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>synchronick&yacute;</trd>,
    <trd>s&uacute;&ccaron;asn&yacute;</trd>
  </trdgrp>
</drv>
</art>

