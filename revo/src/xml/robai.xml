<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: robai.xml,v 1.6 2015/08/21 06:10:15 revo Exp $">
<kap>
  <rad>robai</rad>/o <fnt><bib>PGl</bib></fnt>
</kap>

<drv mrk="robai.0o">
  <kap><tld/>o</kap>
  <snc mrk="robai.0o.POE">
    <uzo tip="stl">POE</uzo>
    <dif>
      Persdevena kvarversa&jcirc;o kutime pri mistika, sa&gcirc;eca a&ubreve; sprita
      temo, kun rimskemo &leftquot;abaa&rightquot;:
      <ekz>
        sian famon tra la mondo Kajjam pleje &scirc;uldas al siaj poemetoj
        verkitaj en formo de <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Omar Kajjam&rightquot;,
        2005-11-28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="robai.0o.RIM">
    Anka&ubreve; registrita kiel &leftquot;robajo&rightquot; en pluraj vortaroj.
  </rim>
  <trd lng="de">Rubai</trd>
  <trd lng="en">rubaiyat</trd>
  <trdgrp lng="fr">
    <trd>ruba&iuml;yat</trd>,
    <trd>rouba&iuml;yat</trd>,
    <trd>robayat</trd>
  </trdgrp>
  <trd lng="hu">rob&aacute;i</trd>
  <trd lng="nl">rubaiyat</trd>
  <trd lng="pt">rubaiyat</trd>
</drv>
</art>

