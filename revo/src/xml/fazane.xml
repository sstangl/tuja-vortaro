  <trd lng="fr">phasianid&eacute;s</trd>
  <trd lng="hu">f&aacute;c&aacute;nf&eacute;l&eacute;k</trd>
  <trd lng="oc">Fasianids</trd>
  <trd lng="pl">kurowate</trd>
  <trd lng="pt">fasian&iacute;deos</trd>
  <trd lng="ru">&c_f;&c_a;&c_z;&c_a;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
