  <trd lng="be">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;
  &c_n;&c_a; &c_z;&c_ja;&c_m;&c_l;&c_ju;</trd>
  <trd lng="cs">uzemn&ecaron;n&iacute;</trd>
  <trd lng="he">&he;&alef;&resh;&qof;&he;</trd>
  <trd lng="hu">f&ouml;ldr&ouml;vidz&aacute;rlat</trd>
  <trd lng="nl">aarding</trd>
  <trdgrp lng="pl">
    <trd>przebicie do <ind>ziemi</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_i;&c_je;
  &c_n;&c_a; &c_z;&c_je;&c_m;&c_l;&c_ju;</trd>
  <trdgrp lng="sk">
    <trd>zemn&yacute; kontakt</trd>,
    <trd>zvod</trd>
  </trdgrp>
</drv>

<drv mrk="ter.0kulturo">
  <kap><tld/>kulturo<fnt>Z</fnt></kap>
  <snc mrk="ter.0kulturo.AGR">
    <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="agriku.0o">agrikulturo</ref>
    <ekz>
      unuj el ili sin okupis je <tld/>kulturo, aliaj je
      fi&scirc;kaptado a&ubreve;
      marveturado
      <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="cs">zem&ecaron;d&ecaron;lstv&iacute;</trd>
  <trd lng="es">agricultura</trd>
  <trd lng="fr">agriculture</trd>
  <trd lng="he">&het;&qof;&lamed;&alef;&vav;&tav;</trd>
  <trd lng="hu">f&ouml;ldm&odblac;vel&eacute;s</trd>
  <trd lng="nl">landbouw</trd>
  <trdgrp lng="pl">
    <trd>uprawa <ind>ziemi</ind></trd>,
    <trd>agrokultura</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_o;&c_je;
  &c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">po&lcaron;nohospod&aacute;rstvo</trd>
</drv>

<drv mrk="ter.0laboristo">
  <kap><tld/>laboristo<fnt>Z</fnt></kap>
