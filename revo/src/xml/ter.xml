  <trdgrp lng="hu">
    <trd>b&eacute;res</trd>,
    <trd>mez&odblac;gazdas&aacute;gi <ind>munk&aacute;s</ind></trd>,
    <trd>mezei <ind>munk&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="nl">landbouwer</trd>
  <trd lng="pl">robotnik do prac <ind>ziemnych</ind></trd>
  <trd
