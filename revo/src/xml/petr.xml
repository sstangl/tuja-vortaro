<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: petr.xml,v 1.4 2015/02/22 21:10:15 revo Exp $">
<kap>
  <ofc>*</ofc><rad>Petr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="petr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="petr.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        sinjoro <tld/>o kaj lia edzino tre amas miajn infanojn
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        kaj mi diras al vi, ke vi estas <tld/>o
        <fnt><bib>&Mat;</bib>, <lok>16:18</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Pere</trd>
    <trd lng="de">Peter</trd>
    <trd lng="en">Peter</trd>
    <trd lng="fr">Pierre<klr> (pr&eacute;nom)</klr></trd>
    <trd lng="pl">Piotr</trd>
  </snc>
</drv>
</art>

