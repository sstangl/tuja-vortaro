<kap>
  <rad>volfram</rad>/o
</kap>
<drv mrk="volfram.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">W</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="74">elemento</ref> kun
      atomnumero 74, atompezo 183,85; 
      <ref tip="super" cel="metal.0o">metalo</ref> malmola, griza, uzata
      por
      hardi &scirc;talon, kun tre alta boltemperaturo kaj pro tio uzata por
      fabriki
      filamentojn de inkandeskaj kaj elektronaj lampoj; &gcirc;ia eksa nomo
      estas
      <refgrp tip="sin">
        <ref cel="tungst.0o">tungsteno</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_f;&c_r;&c_a;&c_m;</trd>
  <trd lng="bg">&c_v;&c_o;&c_l;&c_f;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">tungsten</trd>
  <trdgrp lng="ca">
      <trd>wolframi</trd>,
      <trd>tungst&egrave;</trd>,
      <trd>w&oacute;lfram</trd>
  </trdgrp>
  <trd lng="cs">wolfram</trd>
  <trd lng="de">Wolfram</trd>
  <trd lng="el">&tau;&omikron;&ypsilon;&gamma;&gamma;&omikron_ton;&sigma;&tau;&eta;&ny;&omikron;</trd>
  <trdgrp lng="en">
      <trd>tungsten</trd>,
      <trd>wolfram</trd>
   </trdgrp>
  <trdgrp lng="es">
      <trd>wolframio</trd>,
      <trd>tungsteno</trd>,
      <trd>volframio</trd>,
      <trd>w&oacute;lfram</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>tungst&egrave;ne</trd>,
     <trd>wolfram</trd>
  </trdgrp>
  <trd lng="hu">volfr&aacute;m</trd>
  <trd lng="id">wolfram</trd>
  <trd lng="it">tungsteno</trd>
  <trd lng="la">Wolfram</trd>
  <trd lng="nl">wolfraam</trd>
  <trd lng="pl">wolfram</trd>
  <trd lng="pt">tungst&ecirc;nio</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_mol;&c_f;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>W</trd>,
    <trd>volfr√°m</trd>
  </trdgrp>
  <trd lng="sv">volfram</trd>
</drv>
</art>
