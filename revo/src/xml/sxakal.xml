<vortaro>
<art mrk="$Id: sxakal.xml,v 1.20 2012/08/14 16:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;akal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxakal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxakal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specioj de sova&gcirc;a <ref tip="super" cel="hund.0o">hundo</ref>
      (<trd lng="la">Canis aureus</trd> k.a.), proksimume vulpograndaj:
      <ekz>
        fore, pro malsato kaj timo plorema <tld/>o boja&ccirc;as
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_k;&c_a;&c_l;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_k;&c_a;&c_l;</trd>
  <trd lng="ca">xacal</trd>
  <trd lng="de">Schakal</trd>
  <trd lng="el">&tau;&sigma;&alfa;&kappa;&alfa_ton;&lambda;&jota;</trd>
  <trd lng="en">jackal</trd>
  <trd lng="es">chacal</trd>
  <trd lng="fr">chacal</trd>
  <trd lng="he">&tav;&fnun;</trd>
  <trd lng="hu">sak&aacute;l</trd>
  <trd lng="it">sciacallo</trd>
  <trd lng="nl">jakhals</trd>
  <trd lng="pt">chacal</trd>
  <trd lng="ru">&c_sh;&c_a;&c_k;&c_a;&c_l;</trd>
  <trd lng="sv">schakal</trd>
</drv>
</art>
