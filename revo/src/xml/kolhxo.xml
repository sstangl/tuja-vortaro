<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolhxo.xml,v 1.11 2010/12/12 17:30:20 revo Exp $">
<kap>
  <rad>kol&hcirc;oz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolhxo.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolhxo.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rusa kamparana mastruma kolektivo a&ubreve;
      <ref tip="super" cel="kooper1.0o">kooperativo</ref>,
      nomata per mallongigo de rusaj vortoj "&c_k;&c_o;&c_l;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je; 
      &c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;", t.e. "kolektiva mastrumado":
      <ekz>
        la knabo denuncis sian patron, kiam tiu
        &scirc;telis sakon da greno de la <tld/>o
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  
  <trd lng="be">&c_k;&c_a;&c_l;&c_g;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Kolchos</trd>,
    <trd>Kolchose</trd>
  </trdgrp>
  <trd lng="fr">kolkhoze</trd>
  <trd lng="nl">kolchoz</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_h;&c_o;&c_z;</trd>
</drv>
</art>
