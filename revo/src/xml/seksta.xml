<art mrk="$Id: seksta.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sekstant</rad>/o
</kap>
<drv mrk="seksta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Astronomia instrumento, per kiu oni observas angulon inter du
      punktoj, kaj plej ofte la alton de astro super la horizonto de la
      maro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="ca">sextant</trd>
  <trd lng="cs">sextant</trd>
  <trd lng="de">Sextant</trd>
  <trd lng="en">sextant</trd>
  <trd lng="es">sextante</trd>
  <trd lng="fr">sextant</trd>
  <trd lng="hu">szext&aacute;ns</trd>
  <trd lng="it">sestante</trd>
  <trd lng="nl">sextant</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_s;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">sextant</trd>
</drv>
</art>
