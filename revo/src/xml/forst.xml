<art mrk="$Id: forst.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>forst</rad>/o
</kap>
<drv mrk="forst.0o">
  <kap><tld/>o</kap>
  <snc mrk="forst.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Metode flegata arbaro.  
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_s; <klr
  tip="amb">(&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_u;&c_je;&c_m;&c_y;)</klr></trd>
  <trd lng="ca">explotaci&oacute; forestal</trd>
  <trdgrp lng="cs">
    <trd>hvozd</trd>,
    <trd>pěstěný les</trd>
  </trdgrp>
  <trd lng="de">Forst</trd>
  <trd lng="en">cultivated forest</trd>
  <trd lng="es">explotaci&oacute;n forestal</trd>
  <trd lng="fr">exploitation foresti&egrave;re</trd>
  <trd lng="pt">floresta plantada</trd>
  <trd lng="ru">&c_l;&c_je;&c_s; <klr>(&c_e;&c_k;&c_s;&c_p;&c_l;&c_u;&c_a;&c_t;&c_i;&c_r;&c_u;&c_je;&c_m;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">les <klr>(pestovaný)</klr></trd>
</drv>
</art>
