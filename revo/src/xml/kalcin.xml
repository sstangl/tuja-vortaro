<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kalcin.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
   <rad>kalcin</rad>/i
</kap>
<drv mrk="kalcin.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Cindrigi per forta varmigo nefandeblajn substancojn:
       <ekz><tld/>ita magnezo.</ekz>
     </dif>
   </snc>
  <trd lng="ca">calcinar</trd>
  <trdgrp lng="cs">
    <trd>kalcinovat</trd>,
    <trd>pražit</trd>,
    <trd>vyžíhat</trd>,
    <trd>žíhat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>kalzinieren</trd>,
      <trd>zu <ind>Kalk</ind> brennen</trd>,
      <trd>veraschen</trd>
   </trdgrp>
  <trd lng="en">calcine</trd>
  <trd lng="es">calcinar</trd>
  <trd lng="fa">&a_t;&f_k;&a_l;&f_y;&a_s; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fi">
     <trd>kalsinoida</trd>,
     <trd>hehkuttaa <klr tip="amb"> (tekn.)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
      <trd>calciner</trd>,
      <trd>r&eacute;duire en cendres</trd>
   </trdgrp>
  <trd lng="hu">kalcin&aacute;l</trd>
  <trdgrp lng="nl">
     <trd>calcineren</trd>,
     <trd>uitgloeien</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>wypra&zdot;a&cacute;</trd>,
     <trd>wypala&cacute;</trd>
   </trdgrp>
  <trd lng="pt">calcinar</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_k;&c_a;&c_l;&c_mol;&c_c;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">kalcinovať</trd>
</drv>
</art>
