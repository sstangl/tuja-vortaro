<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: trohxe.xml,v 1.3 2012/08/14 14:10:11 revo Exp $">
    <kap><rad>tro&hcirc;e</rad>/o <fnt><bib>BoER</bib></fnt></kap>

    <drv mrk="trohxe.0o">
      <kap><tld/>o</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<uzo tip="fak">POE</uzo>
        <ref tip="dif" cel="troke.0o">Trokeo</ref>
        <ekz>
          La <tld/>ojn &ccirc;i tie dufoje interrompas daktilo
          <fnt>
            <aut>K. Kalocsay</aut>: <vrk>Letero al G. Waringhien</vrk>.
            <lok><url
ref="http://www.vanoostendorp.nl/interlinguistiek/kalocsay.kaj.waringhien/xml/270924kk.xml"
                >Budapest, la 24a de sept. 1927</url></lok>
          </fnt>.
        </ekz>
      </snc>
      <trd lng="ca">troqueu</trd>
      <trdgrp lng="en">
        <trd>choree</trd>, <trd>trochee</trd>
      </trdgrp>
  <trdgrp lng="pt">
    <trd>troqueu</trd>,
    <trd>coreu</trd>
  </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_h;&c_o;&c_r;&c_je;&c_j;</trd>,
        <trd>&c_t;&c_r;&c_o;&c_h;&c_je;&c_j;</trd>
      </trdgrp>
    </drv>

  </art>

