<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fenome.xml,v 1.23 2015/09/21 05:10:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fenomen</rad>/o
</kap>

<drv mrk="fenome.0o">
  <kap><tld/>o</kap>
  <snc mrk="fenome.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Perceptebla per la sentumoj apero; natura okazo:
      <ekz>
        astronomiaj, fizikaj, spiritaj <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ph&auml;nomen</trd>,
      <trd>Erfahrungstatsache</trd>
    </trdgrp>
    <trd lng="fa">&f_p;&a_d;&f_y;&a_d;&a_h;</trd>
    <trd lng="fr">ph&eacute;nom&egrave;ne</trd>
    <trdgrp lng="hu">
      <trd>jelens&eacute;g</trd>,
      <trd>t&uuml;nem&eacute;ny</trd>
    </trdgrp>
  </snc>
  <snc mrk="fenome.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malofta, eksterordinara, stranga afero:
      <ekz>
        renkonti vin estas vera <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="event.0o">evento</ref>,
      <ref cel="monstr.0o.FIG">monstro <sncref/></ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Ph&auml;nomen</trd>,
      <trd>seltenes <ind>Ereignis</ind></trd>,
      <trd>seltsame <ind>Sache</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ph&eacute;nom&egrave;ne<klr>
      (&eacute;v&egrave;nement)</klr></trd>,
      <trd>merveille<klr> (&eacute;v&egrave;nement)</klr></trd>,
      <trd>prodige<klr> (&eacute;v&egrave;nement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ritka <ind>dolog</ind></trd>,
      <trd>rendk&iacute;v&uuml;li <ind>eset</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_n;&c_o;&c_m;&c_e;&c_n;</trd>,
    <trd>&c_z;&c_mol;&c_ja;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efekt</trd>,
    <trd>fenom&eacute;n</trd>,
    <trd>jev</trd>
  </trdgrp>
  <trd lng="de">Ph&auml;nomen</trd>
  <trd lng="es">fen&oacute;meno</trd>
  <trd lng="nl">fenomeen</trd>
  <trd lng="pl">fenomen</trd>
  <trd lng="pt">fen&ocirc;meno</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jav</trd>,
    <trd>zjav</trd>,
    <trd>&uacute;kaz</trd>
  </trdgrp>
  <trd lng="tr">fenomen</trd>
</drv>

<drv mrk="fenome.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malofta, stranga, eksterordinara:
      <ekz>
        <tld/>a aventuro, floro, estulo.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_f;&c_e;&c_n;&c_a;&c_m;&c_e;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_i;&c_z;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fenomen&aacute;ln&iacute;</trd>,
    <trd>fenom&eacute;nov&yacute;</trd>,
    <trd>nev&scaron;edn&iacute;</trd>,
    <trd>t&yacute;kaj&iacute;c&iacute; se fenom&eacute;nu</trd>
  </trdgrp>
  <trd lng="es">fenomenal</trd>
  <trd
  lng="fa">&a_kh;&a_A;&a_r;&a_q;&zwnj;&a_A;&a_l;&a_ayn;&a_A;&a_d;&a_h;</trd>
  <trd lng="fr">ph&eacute;nom&eacute;nal</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nem&eacute;nyes</trd>,
    <trd>nem mindennapi</trd>
  </trdgrp>
  <trd lng="nl">fenomenaal</trd>
  <trd lng="pl">fenomenalny</trd>
  <trd
  lng="ru">&c_f;&c_je;&c_n;&c_o;&c_m;&c_je;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>fenomen&aacute;lny</trd>,
    <trd>neoby&ccaron;ajn&yacute;</trd>,
    <trd>nev&scaron;edn&yacute;</trd>,
    <trd>vynikaj&uacute;ci</trd>
  </trdgrp>
  <trd lng="tr">ola&gbreve;an&uuml;st&uuml;</trd>
</drv>
</art>
