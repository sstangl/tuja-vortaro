<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: prus.xml,v 1.17 2015/07/04 08:37:42 revo Exp $">
    <kap><rad>prus</rad>/o</kap>

    <drv mrk="prus.0o">
      <kap><tld/>o</kap>
      <snc mrk="prus.0o.balto">
        <uzo tip="fak">HIS</uzo>
        <dif>
          Ano de <ref tip="super" cel="balt.0o">balta</ref> etno,
          kiu en la 1a jarmilo p.K. prilo&gcirc;is la teritorion de la
          suda bordo de la Balta Maro:
          <ekz>
            okcidentaj baltoj estis <tld/>oj
            <fnt>
              <aut>P. &Ccaron;eliauskas</aut>:
              <vrk>Litovio dum jarcentoj</vrk>.
              <lok>Kaunas, 2002. P. 13.</lok>
            </fnt>;
          </ekz><ekz>
            anta&ubreve; la 13a jarcento sur la kur&scirc;a terlango vivis
            <tld/>oj, semboj, norde samogitoj, kur&scirc;oj
            <fnt>
              <aut>Laimius STRAZNICKAS</aut>:
              <vrk>Terlango &#8208; natura valoro</vrk>.
              <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            la kruckavaliroj konkeris la <tld/>ojn kaj eklo&gcirc;is
            &ccirc;i tie meze de la 13a jarcento
            <fnt>
              <aut>Halina Gorecka</aut>: <vrk>Redakcie</vrk>.
              <bib>LOdE</bib>, <lok>2001, n-ro 8&ndash;9</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_p;&c_r;&c_u;&c_s;</trd>
        <trd lng="pl">Prus</trd>
        <trd lng="pt">prussiano</trd>
        <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;</trd>
      </snc>
      <snc mrk="prus.0o.prusujano">
        <uzo tip="fak">HIS</uzo>
        <dif>
          = <ref tip="sin" cel="prus.0ujano"><tld/>ujano</ref>:
          <ekz>
            oni devas distingi inter la malnovaj <tld/>oj
            (a&ubreve;to&hcirc;tonaj lo&gcirc;antoj) kaj novaj <tld/>oj (lo&gcirc;antoj
            de <tld lit="P"/>ujo, <tld/>ujanoj)
            <fnt>
              <aut>Halina Gorecka</aut>:
              <vrk>Pa&gcirc;oj el la historio de la Sukcena Lando</vrk>.
              <bib>LOdE</bib>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">Prussian</trd>
      <trd lng="fr">Prussien<klr> (subst.)</klr></trd>
      <trd lng="hu">porosz</trd>
      <trd lng="nl">Pruis</trd>
    </drv>

    <drv mrk="prus.0a">
      <kap><tld/>a</kap>
      <snc mrk="prus.0a.balta">
        <dif>
          Rilata al la <ref cel="prus.0o.balto">prusoj</ref>:
          <ekz>
            <uzo tip="fak">LIN</uzo>
            la <ind><tld/>a lingvo</ind>;
            <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_k;&c_a;&c_ja;
              &c_m;&c_o;&c_v;&c_a;</trd>
            <trd lng="hu">&oacute;porosz nyelv</trd>
            <trd lng="nl">Pruisisch<klr tip="ind"> (taal)</klr></trd>
            <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;
              &c_ja;&c_z;&c_y;&c_k;</trd>
          </ekz><ekz>
            tiutempe <klr>(en la 14a jc)</klr> <tld lit="P"/>ujo
            havis 85 urbojn <klr>...</klr>, 1400 germanajn vila&gcirc;ojn
            kaj plurajn vila&gcirc;ojn <tld/>ajn kaj polajn
            <fnt>
              <aut>Halina Gorecka</aut>:
              <vrk>Pa&gcirc;oj el la historio de la Sukcena Lando</vrk>.
              <bib>LOdE</bib>.
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <snc mrk="prus.0a.prusuja">
        <dif>
          <tld lit="P"/>uja.
        </dif>
        <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr> (&c_p;&c_r;&c_u;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
      </snc>
      <snc mrk="prus.0a.prusujana">
        <dif>
          <tld lit="P"/>ujana.
        </dif>
        <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_k;&c_ib;</trd>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_c;&c_k;&c_i;&c_j;</trd>
          <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
        </trdgrp>
      </snc>
  <trd lng="cs">pruský</trd>
  <trd lng="de">preu&szlig;isch</trd>
  <trd lng="en">Prussian</trd>
  <trd lng="fr">prussien<klr> (adj.)</klr></trd>
  <trd lng="hu">porosz</trd>
  <trd lng="nl">Pruisisch</trd>
  <trd lng="sk">pruský</trd>
</drv>

    <drv mrk="prus.P0ujo">
      <kap><tld lit="P"/>ujo</kap>
      <uzo tip="fak">HIS</uzo>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="prus.P0ujo.balta">
        <dif>
          La lando de la
          <ref cel="prus.0o.balto"><tld/>oj<sncref/></ref>.
        </dif>
      </snc>
      <snc mrk="prus.P0ujo.germana">
        <dif>
          La germana(j) lando(j) esti&gcirc;inta(j) sur la teritorio de
          <ref cel="prus.0o.balto"><tld lit="P"/>ujo<sncref/></ref>.
        </dif>
      </snc>
      <trd lng="be">&c_P;&c_r;&c_u;&c_s;&c_ib;&c_ja;</trd>
      <trd lng="bg">&c_P;&c_r;&c_u;&c_s;&c_i;&c_ja;</trd>
      <trd lng="de">Preu&szlig;en</trd>
      <trd lng="en">Prussia</trd>
      <trd lng="fr">Prusse</trd>
      <trd lng="hu">Poroszorsz&aacute;g</trd>
      <trd lng="nl">Pruisen</trd>
      <trd lng="ru">&c_P;&c_r;&c_u;&c_s;&c_s;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="prus.0uja">
      <kap><tld/>uja</kap>
      <uzo tip="fak">HIS</uzo>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Rilata al <ref cel="prus.P0ujo"><tld lit="P"/>ujo</ref>:
          <ekz>
            greno estis la &ccirc;efa <tld/>uja eksporto
            <fnt>
              <aut>Halina Gorecka</aut>:
              <vrk>Pa&gcirc;oj el la historio de la Sukcena Lando</vrk>.
              <bib>LOdE</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">Prussian</trd>
      <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr> (&c_p;&c_r;&c_u;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
    </drv>

    <drv mrk="prus.0ujano">
      <kap><tld/>ujano</kap>
      <snc>
        <dif>
          Lo&gcirc;anto de <tld lit="P"/>ujo.
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_a;&c_k;</trd>
      <trd lng="de">Preu&szlig;e</trd>
      <trd lng="en">Prussian</trd>
      <trd lng="fr">Prussien<klr> (subst.)</klr></trd>
      <trd lng="hu">porosz</trd>
      <trd lng="pl">Prusak</trd>
      <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_k;</trd>
    </drv>
  </art>
