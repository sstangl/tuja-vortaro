<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: disident.xml,v 1.7 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>disident</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="disident.0o">
  <kap><tld/>o</kap>
  <snc mrk="disident.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Enlanda kontra&ubreve;anto de registaro, elmetata al malpermesoj
      a&ubreve;
      persekutoj:
      <ekz>
        &scirc;i rakontis pri &leftquot;<tld/>oj&rightquot;,
        persekutataj en Leningrado kaj
        ne nur tie
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 6a, p.
        35a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="opozic.0o.POL">opozicio</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_d;&c_i;&c_s;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>
  <trd lng="ca">dissident</trd>
  <trdgrp lng="cs">
    <trd>disident</trd>,
    <trd>jinověrec</trd>,
    <trd>odpadlík</trd>
  </trdgrp>
  <trd lng="da">systemkritiker</trd>
  <trd lng="de">Dissident</trd>
  <trd lng="en">dissident</trd>
  <trd lng="fi">toisinajattelija</trd>
  <trd lng="fr">dissident<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hr">disident</trd>
  <trd lng="mk">&c_d;&c_i;&c_s;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>
  <trd lng="nl">dissident</trd>
  <trdgrp lng="pl">
    <trd>dysydent</trd>,
    <trd>opozycjonista</trd>
  </trdgrp>
  <trd lng="pt">dissidente</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_s;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>inoverec</trd>,
    <trd>odpadlík</trd>
  </trdgrp>
  <trd lng="sr">&c_d;&c_i;&c_s;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>
  <trd lng="uk">&c_d;&c_i;&c_s;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>
</drv>
</art>

