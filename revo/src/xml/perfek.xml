<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: perfek.xml,v 1.24 2015/10/08 14:10:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>perfekt</rad>/a
</kap>
<drv mrk="perfek.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta &ccirc;iujn kvalitojn kaj neniun mankon:
      <ekz>
        ni konceptas Dion nepre <tld/>a.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Havanta &ccirc;iujn kvalitojn necesajn en sia speco:
      <ekz>
        <tld/>a oratoro, verko
        <klr>(ironie)</klr>;
      </ekz>
      <ekz>
        <tld/>a kanajlo<fnt>Z</fnt>
        <klr>(<ref tip="vid" cel="protok1.0o">protokanajlo</ref>)</klr>,
      </ekz>
      <ekz>
        malspritulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e ludi pianon;
      </ekz>
      <ekz>
        ni admiris longe la <tld/>econ de la pentra&jcirc;o;
      </ekz>
      <ekz>
        mi neniel konsideras tiun teorion kiel <tld/>an kaj
        nekritikeblan.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="model.0a">modela</ref>,
      <ref cel="mank.sen0a">senmanka</ref>,
      <ref cel="difekt.sen0a">sendifekta</ref>,
      <ref cel="pek.sen0a">senpeka</ref>,
      <ref cel="makul.sen0a">senmakula</ref>,
      <ref cel="erar">senerara</ref>.
    </refgrp>
  </snc>
  <snc mrk="perfek.0a.nombro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="entjer.natura0o">natura entjero</ref>)</klr>
      Tia, ke &gcirc;i egalas al la sumo de
      &ccirc;iuj &gcirc;iaj <ref cel="divizo.0o">divizoroj</ref>
      (krom &gcirc;i):
      <ekz>
        <frm>28 = 2<sup>2</sup>&#215;7 = 1+2+4+7+14</frm>.
      </ekz>
    </dif>                                                   
    <trd lng="br"><klr tip="ind">(niver) </klr>parfet</trd>    
    <trd lng="de">vollkommen<klr tip="ind"> (-e Zahl)</klr></trd>
    <trd lng="en">perfect<klr tip="ind"> (number)</klr></trd>
    <trd lng="fr"><klr tip="ind">(nombre) </klr>parfait</trd>
    <trd lng="pl"><klr tip="amb">(liczba) </klr>doskona&lstroke;a</trd>
    <trd 
lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_je;<klr 
tip="amb"> (&c_ch;&c_i;&c_s;&c_l;&c_o;)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_k;&c_a;&c_n;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_k;&c_o;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_z;&c_a;&c_g;&c_a;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">peurvat</trd>
  <trdgrp lng="cs">
    <trd>dokonal&yacute;</trd>,
    <trd>mistrn&yacute;</trd>,
    <trd>perfektn&iacute;</trd>
  </trdgrp>
  <trd lng="de">perfekt</trd>
  <trd lng="en">perfect</trd>
  <trd lng="es">perfecto</trd>
  <trd lng="fr">parfait</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;k&eacute;letes</trd>,
    <trd>hi&aacute;nytalan</trd>,
    <trd>perfekt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>volmaakt</trd>,
    <trd>perfect</trd>
  </trdgrp>
  <trd lng="pl">doskona&lstroke;y</trd>
  <trd lng="pt">perfeito</trd>
  <trd lng="ro">perfect</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_u;&c_p;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dokonal&yacute;</trd>
</drv>

<drv mrk="perfek.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Peni por igi <tld/>a:
      <ekz>
        <tld/>igi lernanton per ekzercado;
      </ekz>
      <ekz>
        li multe <tld/>i&gcirc;is en kalkulado;
      </ekz>
      <ekz>
        ri&ccirc;i&gcirc;ado kaj <tld/>i&gcirc;ado de la lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        homo estas erarema sed <tld/>igebla.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_u;&c_d;&c_a;&c_s;&c_k;&c_a;&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">peurwellaat</trd>
  <trd lng="cs">zdokonalit</trd>
  <trd lng="de">perfektionieren</trd>
  <trd lng="en">to perfect</trd>
  <trd lng="fr">perfectionner</trd>
  <trd lng="hu">t&ouml;k&eacute;letes&iacute;t</trd>
  <trd lng="nl">vervolmaken</trd>
  <trd lng="pl">udoskonala&cacute;</trd>
  <trdgrp lng="ru">
    
<trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    
<trd>&c_u;&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cibri&tcaron;</trd>,
    <trd>zdokona&lcaron;ova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="perfek.0o">
  <kap><tld/>o<fnt>Z</fnt></kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Verba tempo uzata en kelkaj lingvoj por montri rezultaton de ago
      tute plenumita, ne precizigante kiam &gcirc;i plenumi&gcirc;is
    </dif>
    <refgrp tip="sin">
      <ref cel="imperf.0o">imperfekto</ref>,
      <ref cel="preter1.0o">preterito</ref>.
    </refgrp>
  </snc>
  <trd lng="br">amzer amdremenet kevrennek</trd>
  <trdgrp lng="cs">
    <trd>minul&yacute; &ccaron;as</trd>,
    <trd>perfektum</trd>
  </trdgrp>
  <trd lng="de">Perfekt</trd>
  <trd lng="en">perfect</trd>
  <trd lng="fr">parfait<klr tip="ind"> (gram.)</klr></trd>
  <trdgrp lng="hu">
    <trd>befejezett m&uacute;lt <ind>id&odblac;</ind></trd>,
    <trd>perfektum</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>perfectum</trd>,
    <trd>voltooid tegenwoordige <ind>tijd</ind></trd>
  </trdgrp>
  <trd lng="pl">czas przesz&lstroke;y dokonany</trd>
  <trd lng="ro">perfect</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je; 
&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_je; 
&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_f;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">minul&yacute; &ccaron;as</trd>
</drv>
</art>
