<kap>
  <ofc>8</ofc>
  <rad>obstakl</rad>/o
</kap>
<drv mrk="obstak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malhelpo, baro:
      <ekz>
        neniu el la doloraj punktoj de &ccirc;iuj partneroj i&gcirc;os nesuperebla
        <tld/>o<fnt><aut>Marc Vanden Bempt</aut>: <vrk>KB+ABB+CERA egalas
        KBC</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 11a, p.
        19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>
  <trd lng="ca">obstacle</trd>
  <trdgrp lng="cs">
    <trd>překážka</trd>,
    <trd>úskalí</trd>
  </trdgrp>
  <trd lng="de">Hindernis</trd>
  <trd lng="en">obstacle</trd>
  <trd lng="es">obst&aacute;culo</trd>
  <trdgrp lng="fr">
    <trd>emp&ecirc;chement</trd>,
    <trd>obstacle</trd>
  </trdgrp>
  <trd lng="hu">akad&aacute;ly</trd>
  <trdgrp lng="nl">
    <trd>obstakel</trd>,
    <trd>hinderpaal</trd>
  </trdgrp>
  <trd lng="pt">obst&aacute;culo</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_p;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">prekážka</trd>
  <trd lng="sv">hinder</trd>
</drv>
</art>
