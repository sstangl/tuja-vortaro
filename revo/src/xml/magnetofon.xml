<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: magnetofon.xml,v 1.11 2013/07/28 19:10:10 revo Exp $">
<kap>
  <ofc>9</ofc><rad>magnetofon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="magnetofon.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="magnetofon.0o.SON">
    <dif>
      <ref tip="super" cel="regist.son0ilo.SON">Sonregistrilo</ref>
      konsistanta en
      <ref tip="super" cel="bend.sur0igilo">surbendigilo</ref>,
      ordinare uzebla ne nur por registrado sed anka&ubreve; por a&ubreve;digo:
      <ekz>
        inter la novaj varoj <klr>...</klr> trovi&gcirc;is la unuaj <tld/>oj
        <fnt><bib>BonaLingvo</bib>, <lok>p. 51a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_a;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>enregistrador magn&egrave;tic</trd>,
    <trd>magnet&ograve;fon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tape recorder</trd>,
    <trd>tape player</trd>,
    <trd>tape deck</trd>,
    <trd>stereo</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>grabadora<klr> (magnet&oacute;fono)</klr></trd>,
    <trd>magnet&oacute;fono<klr> (en desuso)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_D;&a_b;&a_T; &a_S;&a_w;&a_t;<klr tip="amb"> (&a_d;&a_s;&a_t;&f_g;&a_A;&a_h;)</klr></trd>
  <trd lng="fr">magn&eacute;tophone</trd>
  <trd lng="hu">magnetofon</trd>
  <trdgrp lng="pt">
    <trd>gravador<klr> (subst., eletr&ocirc;n.)</klr></trd>,
    <trd>magnetofone</trd>
  </trdgrp>
  <trd lng="nl">bandopnemer</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_o;&c_f;&c_o;&c_n;</trd>
</drv>
</art>

