<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: felise.xml,v 1.15 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>felised</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="felise.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="felise.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Felidae</trd>):
      <ekz>
        la unuaj <tld/>oj aperis sur la Tero anta&ubreve;
        proksimume 35 milionoj da jaroj
        <fnt>
          <aut>F. S. Wechsler</aut>,
          <vrk><url
          ref="http://www.kke.org.br/_media/eo/palestras/filogenio_katoj.pdf">
          Filogenio de Katoj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_e;&c_ch;&c_y;&c_ja;<klr tip="amb"> (&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_o;&c_t;&c_k;&c_i;<klr tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="cs">kočkovití</trd>
  <trd lng="de">Katzen</trd>
  <trd lng="es">f&eacute;lidos</trd>
  <trd lng="fr">f&eacute;lid&eacute;s</trd>
  <trd lng="hu">macskaf&eacute;l&eacute;k</trd>
  <trd lng="nl">katachtigen</trd>
  <trd lng="pt">fel&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_a;&c_ch;&c_mol;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">mačkovité šelmy</trd>
</drv>
</art>
