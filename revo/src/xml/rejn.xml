<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rejn.xml,v 1.14 2013/10/02 08:10:14 revo Exp $">
<kap>
  <rad>Rejn</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="rejn.R0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref> elfluanta el
      <ref tip="malprt" cel="svis.S0ujo">Svisujo</ref>, tra
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> kaj <ref
      tip="malprt" cel="franc.F0ujo">Francujo</ref> al
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>,
      kie &gcirc;i en&jcirc;etas en la Nordan Maron:
      <ekz>
        tiu, kiu veturis sur <tld/>o de Bazelo al Kolonjo, devis
        fine de la 16a jarcento doganpagi je 31 lokoj.
        <fnt><aut>Bo SANDELIN</aut>:
        <vrk><url ref="&Monato;/007278.html">Historio de la ekonomika
        pensado</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 02/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_e;&c_j;&c_n;</trd>
  <trd lng="br">Rhin</trd>
  <trd lng="bg">&c_R;&c_je;&c_j;&c_n;</trd>
  <trd lng="ca">Rin</trd>
  <trd lng="cs">R&yacute;n</trd>
  <trd lng="en">Rhine</trd>
  <trd lng="es">Rin</trd>
  <trd lng="de">Rhein</trd>
  <trd lng="fa">&a_r;&a_A;&f_y;&a_sukun;&a_n;</trd>
  <trd lng="fr">Rhin</trd>
  <trd lng="gd">An <ind>R&eacute;in</ind></trd>
  <trd lng="is">R&iacute;n</trd>
  <trd lng="it">Reno</trd>
  <trd lng="hu">Rajna</trd>
  <trd lng="lat">Rhenus</trd>
  <trd lng="nl">Rijn</trd>
  <trd lng="no">Rhinen</trd>
  <trd lng="pt">Reno</trd>
  <trd lng="pl">Ren</trd>
  <trd lng="ru">&c_R;&c_je;&c_j;&c_n;</trd>
  <trd lng="sl">Ren</trd>
  <trd lng="sv">Rhen</trd>
</drv>
</art>
