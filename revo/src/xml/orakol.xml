<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: orakol.xml,v 1.16 2015/07/04 08:37:40 revo Exp $">
    <kap>
      <ofc>*</ofc> <rad>orakol</rad>/o
    </kap>

    <drv mrk="orakol.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="orakol.0o.ulo">
	<dif>
	  A&ubreve;toritatulo respondanta, nome de dia&jcirc;o, je la demandoj
	  de tiuj kiuj venas &gcirc;in konsulti:
	  <ekz>
	    Marta rigardis la parolantan virinon kiel <tld/>on
	    <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>: <vrk>Marta.</vrk></fnt>;
	  </ekz><ekz>
	    tute ne necesas esti <tld/>o por deklari, ke &gcirc;i neniam
	    atingos la 17 eldonojn de la Fundamenta Krestomatio
	    <fnt>
	      <aut>A. Kor&jcirc;enkov</aut>: <vrk>Ne &ccirc;io nova estas
		bona</vrk>. <lok><url ref="http://www.esperanto.org/Ondo/R-nek.htm"
		  >LOdE, 1992. n-ro 2 (3)</url></lok>
	    </fnt>;
	  </ekz><ekz>
	    <uzo tip="fak">SCI</uzo>
	    <uzo tip="stl">FIG</uzo>
	    <tld/>o foje respondas kaj foje ne, kaj ne eblas influi tion
	    <fnt><vrk>Orakolaj teknikoj</vrk><lok><url
		ref="http://www.ais-sanmarino.org/kursoj/kultprog"
		  >AIS San-Marino</url></lok></fnt>.
	  </ekz>
	</dif>
	<trd lng="ru">&c_o;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>
      </snc>
      <snc mrk="orakol.0o.respondo">
	<dif>
	  <tld lit="O"/>a&jcirc;o, respondo de <tld/>o<sncref ref="orakol.0o.ulo"/>:
	  <ekz>
	    la <tld/>oj de Jesaja;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    la guvernisto Panglos estis la hejma <tld/>diranto.
	  </ekz>
	</dif>
        <trd lng="en">revelation</trd>
	<trdgrp lng="ru">
	  <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
	  <trd>&c_p;&c_r;&c_o;&c_r;&c_i;&c_c;&c_a;&c_n;&c_i;&c_je;</trd>
	</trdgrp>
     </snc>
      <snc mrk="orakol.0o.ejo">
	<dif>
	  <tld lit="O"/>ejo, sanktejo kien oni venas por konsulti
	  <tld/>on<sncref ref="orakol.0o.ulo"/>:
	  <ekz>
	    la <tld/>o de Apolono en Delfo.
	  </ekz>
	</dif>
	<trd lng="ru">&c_o;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>
      </snc>
  <trdgrp lng="be">
	<trd>&c_a;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>,
	<trd>&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>orákulum</trd>,
    <trd>proroctví</trd>,
    <trd>věštba</trd>
  </trdgrp>
  <trd lng="de">Orakel</trd>
  <trd lng="el">&chi;&rho;&eta;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">oracle</trd>
  <trdgrp lng="hu">
	<trd>j&oacute;slat</trd>,
	<trd>or&aacute;kulum</trd>
      </trdgrp>
  <trd lng="nl">orakel</trd>
  <trd lng="pt">or&aacute;culo</trd>
  <trdgrp lng="sk">
    <trd>orákulum</trd>,
    <trd>predpoveď</trd>,
    <trd>proroctvo</trd>,
    <trd>veštba</trd>
  </trdgrp>
</drv>

    <drv mrk="orakol.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  Eldiri <tld/>on.
	</dif>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>prorokovat</trd>,
    <trd>věštit</trd>
  </trdgrp>
  <trd lng="de">orakeln</trd>
  <trdgrp lng="hu">
	<trd>j&oacute;sol</trd>,
	<trd>j&ouml;vend&odblac;t mond</trd>
      </trdgrp>
  <trd lng="nl">orakelen</trd>
  <trd lng="pt">oracular</trd>
  <trdgrp lng="sk">
    <trd>predpovedať</trd>,
    <trd>prorokovať</trd>,
    <trd>veštiť</trd>
  </trdgrp>
</drv>
  </art>
