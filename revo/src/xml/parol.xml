    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">dislavaret</trd>
  <trd lng="de">widersprechen</trd>
  <trd lng="en">speak against</trd>
  <trdgrp lng="fr">
    <trd>contredire</trd>,
    <trd>s'<ind>opposer</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellene <ind>sz&oacute;l</ind></trd>,
    <trd>ellenez</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>contraddire</trd>,
    <trd>obiettare</trd>
  </trdgrp>
  <trd lng="id">[<ind>tentang</ind>] menentang</trd>
  <trd lng="nl">tegenspreken</trd>
  <trdgrp lng="pl">
    <trd>sprzeciwia&cacute; si&eogonek;</trd>,
    <trd>og&lstroke;asza&cacute; <ind>sprzeciw</ind></trd>,
    <trd>ripostowa&cacute;</trd>,
    <trd>wyg&lstroke;asza&cacute; <ind>ripost&eogonek;</ind></trd>,
    <trd>przedstawia&cacute; <ind>kontrargumenty</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="parol.kun0anto">
  <kap>kun<tld/>anto</kap>
  <snc mrk="parol.kun0anto.KOMUNE">
    <dif>
      Persono, kun kiu oni inter&scirc;an&gcirc;as dirojn:
      <ekz>
        eble io simila ekzistas anka&ubreve; en la gepatra lingvo de
        mia kun<tld/>anto
        <fnt><bib>BonaLingvo</bib>, <lok>p. 60a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_a;&c_z;&c_m;&c_o;&c_w;&c_c;&c_a;</trd>
  <trd lng="br">kendivizer</trd>
  <trdgrp lng="en">
    <trd>interlocutor</trd>,
    <trd>conversation partner</trd>
  </trdgrp>
  <trd lng="fr">interlocuteur</trd>
  <trd lng="hu">besz&eacute;lget&odblac;t&aacute;rs</trd>
  <trd lng="it">interlocutore</trd>
  <trd lng="nl">gesprekspartner</trd>
  <trd lng="pl">rozm&oacute;wca</trd>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_je;&c_s;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="parol.lauxd0o">
  <kap>la&ubreve;d<tld/>o,
  <var><kap>la&ubreve;d<tld/>ado</kap></var></kap>
  <snc mrk="parol.lauxd0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Solena parola a&ubreve; skriba la&ubreve;do;
      <ref tip="sin" cel="lauxdaci.0o.BELE">la&ubreve;dacio</ref>,
      <ref tip="dif" cel="panegi.0o.BELE">panegiro</ref>:
      <ekz>
        la filmo montras la la&ubreve;d<tld/>on de prezidanto 
        Probal Dasgupta al la jubileulo
        <fnt>
          <aut>UEA</aut>,
          <vrk>Novaj filmoj en la reta kinejo</vrk>,
          <lok><url
          ref="http://www.uea.org/dokumentoj/komunikoj/gk.php?no=361">
          Gazetaraj Komunikoj, 2010-02-01</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        amaskomunikiloj oma&gcirc;is per raportoj kaj 
        la&ubreve;d<tld/>adoj la 75-an naski&gcirc;datrevenon de Ornette 
        Coleman
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Ornette_Coleman">
          Ornette Coleman</url>, 
          2014-02-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;loge</trd>,
    <trd>pan&eacute;gyrique</trd>
  </trdgrp>
</drv>

<drv mrk="parol.por0anto">
  <kap>por<tld/>anto<fnt><bib>PV</bib><lok>advokato</lok></fnt></kap>
  <snc mrk="parol.por0anto.advokato">
    <dif>
      Persono, kiu defendas la interesojn de iu
      a&ubreve; de ia afero anta&ubreve; iu;
      <ref tip="sin"
      cel="advoka.0o.porparolanto">advokato<sncref/></ref>.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>alvokad<klr tip="ind"> (dre skeud.)</klr></trd>,
    <trd>breutaer<klr tip="ind"> (dre skeud.)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>advocate</trd>,
    <trd>spokesperson</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avocat<klr tip="ind"> (intercesseur)</klr></trd>,
    <trd>d&eacute;fenseur<klr tip="ind"> (intercesseur)</klr></trd>,
    <trd>intercesseur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;gyv&eacute;d</trd>,
    <trd>sz&oacute;sz&oacute;l&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>acara</ind>] pengacara</trd>,
    <trd>advokat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rappresentante<klr> (portavoce)</klr></trd>,
    <trd>avvocato<klr> (rappresentante leg.)</klr></trd>,
    <trd>portavoce</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>adwokat</trd>,
    <trd>obro&nacute;ca</trd>,
    <trd>rzecznik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_v;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_h;&c_o;&c_d;&c_a;&c_t;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="kek">aatinahom</trd>
</drv>

<drv mrk="parol.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="parol.post0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Akcesora posta parto de iu <tld/>ado a&ubreve;
      <ref tip="malprt" cel="verk.0o.propra">verko</ref>, destinita
      por
      &gcirc;in konkludi, a&ubreve; aldoni rilatantajn informojn.
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
  <trdgrp lng="br">
    <trd>goude-skrid</trd>,
    <trd>gourfennskrid</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nachwort</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>afterword</trd>,
    <trd>epilogue</trd>,
    <trd>conclusion</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>postface</trd>,
    <trd>&eacute;pilogue<klr tip="ind"> (postface)</klr></trd>
  </trdgrp>
  <trd lng="hu">ut&oacute;sz&oacute;</trd>
  <trd lng="id">epilog</trd>
  <trdgrp lng="it">
    <trd>postfazione</trd>,
    <trd>epilogo</trd>,
    <trd>conclusione<klr> (capitolo)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nawoord</trd>,
    <trd>epiloog</trd>,
    <trd>narede</trd>,
    <trd>nabericht</trd>
  </trdgrp>
  <trd lng="pl">pos&lstroke;owie</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>konklúzia</trd>,
    <trd>uzáver</trd>,
    <trd>záver</trd>
  </trdgrp>
</drv>

<drv mrk="parol.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="parol.pri0i.KOMUNE">
    <dif>
      Esprimi sian penson, sian opinion pri io:
      <ekz>
        mi ilin ne pri<tld/>os &ccirc;i tie
        <fnt><bib>BonaLingvo</bib>, <lok>p. 76a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">komz diwar-benn</trd>
  <trd lng="cs">projednat</trd>
  <trdgrp lng="de">
    <trd>besprechen</trd>,
    <trd>&uuml;ber etwas <ind>sprechen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>talk about</trd>,
    <trd>discuss</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commenter</trd>,
    <trd>discuter<klr> (qqch)</klr></trd>
  </trdgrp>
  <trd lng="hu">megbesz&eacute;l</trd>
  <trd lng="id">[<ind>diskusi</ind>] berdiskusi</trd>
  <trdgrp lng="it">
    <trd>commentare</trd>,
    <trd><ind>discutere</ind> di</trd>,
    <trd><ind>dire</ind> su</trd>
  </trdgrp>
  <trd lng="nl">bespreken</trd>
  <trdgrp lng="pl">
    <trd>omawia&cacute;</trd>,
    <trd>obgadywa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol; &c_o;</trd>
  </trdgrp>
  <trd lng="sk">pohovoriť si o niečom</trd>
</drv>

<drv mrk="parol.pro0anto">
  <kap>pro<tld/>anto</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="parol.pro0anto.porparolanto">
    <ref tip="dif" cel="parol.por0anto">por<tld/>anto</ref>
    <ekz>
      se li havas por si an&gcirc;elon pro<tld/>anton, kvankam unu
      el mil, kiu elmontrus pri la homo lian pravecon, tiam Li
      indulgas lin
      <fnt><bib>MT</bib><lok>&Ijo; 33:23</lok></fnt>.
    </ekz>
    <rim>
      Malgra&ubreve; la biblia ekzemplo &ccirc;i tiu vorto
      &scirc;ajnas evitinda pro la malfacilo interpreti &gcirc;in
      surbaze de la normala senco de prepozicio <ctl>pro</ctl>.
    </rim>
    <trdgrp lng="it">
      <trd>rappresentante<klr> (portavoce)</klr></trd>,
      <trd>avvocato<klr> (rappresentante leg.)</klr></trd>,
      <trd>portavoce</trd>
    </trdgrp>
    <trd lng="nl">voorspreker</trd>
    <trdgrp lng="pl">
      <trd>adwokat</trd>,
      <trd>obro&nacute;ca</trd>,
      <trd>rzecznik</trd>
    </trdgrp>
  </snc>
  <trd lng="kek">aatinahom</trd>
  <trdgrp lng="cs">
    <trd>přímluvce</trd>,
    <trd>zastánce</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>príhovorca</trd>,
    <trd>zástanca</trd>
  </trdgrp>
</drv>

<drv mrk="parol.sat0i">
  <kap>sat<tld/>i</kap>
  <snc mrk="parol.sat0i.KOMUNE">
    <dif>
      <tld lit="P"/>i sufi&ccirc;e, &gcirc;is la stato en kiu oni ne
      plu deziras <tld/>i:
      <ekz>
        mi ja &ccirc;iam silentas, &ccirc;iam mi havas serenan
        viza&gcirc;on, permesu do al mi sat<tld/>i hodia&ubreve;...
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 7a.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>dire tout ce qu'on a sur le c&oelig;ur</trd>,
    <trd>parler tout son soul</trd>
  </trdgrp>
  <trd lng="hu">kibesz&eacute;li mag&aacute;t</trd>
  <trd lng="it">sfogarsi<klr> (a parole)</klr></trd>
  <trd lng="nl">uitspreken<klr tip="ind"> (tot het einde
  spreken)</klr></trd>
  <trdgrp lng="pl">
    <trd>nagada&cacute; si&eogonek;<klr> (do syta)</klr></trd>,
    <trd>wygada&cacute; si&eogonek;<klr> (do syta)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
