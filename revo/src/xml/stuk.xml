<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stuk.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stuk</rad>/i
</kap>
<drv mrk="stuk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;mirpentri muron per miksa&jcirc;o de kalko kaj sablo.
    </dif>
    <trd lng="hu">vakol</trd>    
  </snc>
  <snc>
    <dif>
      Kovri ion per tavolo el speco de mortero similanta marmoron.
    </dif>
    <trd lng="hu">stukk&oacute;z</trd>    
  </snc>
  <trd lng="be">&c_t;&c_y;&c_n;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">estucar</trd>
  <trdgrp lng="cs">
    <trd>omítat</trd>,
    <trd>štukovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stuckieren</trd>,
    <trd>mit <ind>Stuck</ind> versehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stucco</trd>,
    <trd>plaster</trd>
  </trdgrp>
  <trd lng="es">estucar</trd>
  <trd lng="fr">pl&acirc;trer</trd>
  <trd lng="nl">pleisteren</trd>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">1.omietať  2. pokryť umelým kameňom</trd>
</drv>

<drv mrk="stuk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Substanco servanta por <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="gips.0o">gipso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_n;&c_k;</trd>, 
    <trd>&c_t;&c_y;&c_n;&c_k;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">estuc</trd>
  <trdgrp lng="cs">
    <trd>přen. omítka</trd>,
    <trd>štuk</trd>,
    <trd>štuka</trd>
  </trdgrp>
  <trd lng="de">Stuck</trd>
  <trdgrp lng="en">
    <trd>stucco</trd>,
    <trd>plaster</trd>
  </trdgrp>
  <trd lng="es">estuco</trd>
  <trdgrp lng="fr">
    <trd>pl&acirc;tre</trd>,
    <trd>stuc</trd>,
    <trd>stucco</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vakolat</trd>,
    <trd>gipszvakolat</trd>,
    <trd>stukk&oacute;</trd>,
    <trd>stukat&uacute;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pleisterkalk</trd>,
    <trd>gips</trd>,
    <trd>plafondmortel</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_k;&c_a;&c_t;&c_u;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>omietka</trd>,
    <trd>štuka</trd>
  </trdgrp>
</drv>
</art>
