    Ofte skribata per grandaj literoj: <em>ZEO</em>, <em>ZEOj</em>.
    </rim>
    </snc>
  <trdgrp lng="ca">
      <trd>monument a l'Esperanto</trd>,
      <trd>monument a en Zamenhof</trd>
    </trdgrp>
