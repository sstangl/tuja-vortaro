<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxaligram.xml,v 1.5 2012/08/14 16:10:21 revo Exp $">
<kap>
  <rad>&scirc;aligram</rad>/o
</kap>
<drv mrk="sxaligram.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxaligram.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <dif>
      &Scirc;tono kun amonita fosilio, trovebla en kelkaj lokoj de Himalajo,
      kultata kiel formo de la dio <ref tip="vid" 
      cel="visxnu.0o.HIN">Vi&scirc;nuo</ref>:
      <ekz>
        &Scirc;akuntala volis trovi <tld/>on
        <fnt>
        <aut>R. Manandhar</aut>,
        <vrk><url ref="http://razeno.blog.com/">Mia voja&gcirc;o al
        Muktinath</url></vrk>,
        <lok>2006-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">f&ograve;ssil d'ammonit</trd>
  <trd lng="fr">shaligram</trd>
  <trd lng="hu">saligram</trd>
  <trdgrp lng="nl">
    <trd>saligram<klr tip="ind"> (ammoniet)</klr></trd>,
    <trd>shaligram<klr tip="ind"> (ammoniet)</klr></trd>
  </trdgrp>
</drv>
</art>

