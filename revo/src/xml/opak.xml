<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: opak.xml,v 1.5 2015/11/02 21:10:22 revo Exp $">
<kap>
  <rad>opak</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="opak.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="opak.0a.KOMUNE">
    <ref tip="dif" cel="diafan.mal0a.FOT">maldiafana</ref>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trdgrp lng="sk">
    <trd>matn&yacute;</trd>,
    <trd>neprieh&lcaron;adn&yacute;</trd>,
    <trd>zakalen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>matn&yacute;</trd>,
    <trd>nepr&uring;hledn&yacute;</trd>,
    <trd>opakn&iacute;</trd>,
    <trd>zakalen&yacute;</trd>
  </trdgrp>
  <trd lng="en">opaque</trd>
</drv>

<drv mrk="opak.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="opak.0igxi.KOMUNE">
    <dif>
      I&gcirc;i maldiafana, netravidebla, malluma:
      <ekz>
        &scirc;i kutimis stari apudfenestre &gcirc;is <tld/>i&gcirc;is 
        ekstere
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
<trdgrp lng="en">
  <trd>to dim <klr>(the day dimmed)</klr></trd>,
  <trd>to develop opacity</trd>
</trdgrp>
</drv>
</art>

