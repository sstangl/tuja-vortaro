<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boac.xml,v 1.22 2013/09/20 17:10:09 revo Exp $">
<kap>
<ofc>9</ofc>
<rad>boac</rad>/o
</kap>
<drv mrk="boac.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="cerved.0oj">cervedoj</ref>
      (<trd lng="la">Rangifer</trd>), vivanta en nordaj regionoj.
      Amba&ubreve; seksoj portas kornojn:
      <ekz>
        la <tld/>o estas la sola hejmbesta specio el cervedoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="cerv.norda0o">norda cervo</ref>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_n;&c_mol; <klr
  tip="amb">(&c_p;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="bg">&c_s;&c_je;&c_v;&c_je;&c_r;&c_je;&c_n; <ind>&c_je;&c_l;&c_je;&c_n;</ind></trd>
  <trd lng="de">Rentier</trd>
  <trdgrp lng="en">
    <trd>reindeer</trd>,
    <trd>caribou</trd>
  </trdgrp>
  <trd lng="nl">rendier</trd>
  <trd lng="fr">renne</trd>
  <trd lng="he">&alef;&yod;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>tar&aacute;ndszarvas</trd>,
    <trd>r&eacute;nszarvas</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>renifer</trd>,
    <trd>ren</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_mol;
  <klr>(&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sv">ren</trd>
</drv>
</art>
