<vortaro>
<art mrk="$Id: granat1.xml,v 1.11 2013/08/25 16:10:32 revo Exp $">
<kap>
  <rad>granatujac</rad>/oj
</kap>
<drv mrk="granat1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Punicaceae</trd>),
      kreskantaj en mediteraneaj kaj aziaj regionoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">punic&agrave;cies</trd>
  <trd lng="de">Granatapfelgew&auml;chse</trd>
  <trd lng="fr">punicac&eacute;es</trd>
  <trd lng="hu">gr&aacute;n&aacute;talmaf&eacute;l&eacute;k</trd>
  <trd lng="nl">granaatboomfamilie</trd>
  <trd lng="pl">granatowiec</trd>
  <trd lng="pt">punic&aacute;ceas</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
