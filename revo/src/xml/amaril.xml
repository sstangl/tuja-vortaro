<vortaro>
<art mrk="$Id: amaril.xml,v 1.18 2013/08/26 08:10:20 revo Exp $">
<kap>
  <rad>amarilid</rad>/o
</kap>
<drv mrk="amaril.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="amaril1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Amaryllis</trd>), ornama bulba planto, kun grandaj,
      bonodoraj floroj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;&c_y;&c_l;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_i;&c_s;</trd>
  <trd lng="ca">amaril&middot;lis</trd>
  <trd lng="de">Amaryllis</trd>
  <trd lng="en">amaryllis</trd>
  <trd lng="fr">amaryllis</trd>
  <trdgrp lng="hu">
    <trd>amarillisz</trd>,
    <trd>h&ouml;lgyliliom</trd></trdgrp>
  <trd lng="nl">amaryllis</trd>
  <trd lng="ru">&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_l;&c_i;&c_s;</trd>
</drv>
</art>
