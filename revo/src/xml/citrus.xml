<vortaro>
<art mrk="$Id: citrus.xml,v 1.17 2015/07/04 08:37:32 revo Exp $">
<kap>
<rad>citrus</rad>/o
</kap>
<drv mrk="citrus.0o">
  <kap><tld/>o</kap>
  <snc mrk="citrus.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="rutac.0oj">rutacoj</ref>
      (<trd lng="la">Citrus</trd>).
    </dif>
    <refgrp tip="sub">
      <ref cel="citron.0ujo">citronujo</ref>,
      <ref cel="orangx.0arbo.BOT">oran&gcirc;ujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_t;&c_r;&c_u;&c_s;</trd>
  <trd lng="ca">c&iacute;tric</trd>
  <trd lng="cs">citrus</trd>
  <trd lng="de">Zitrusfrucht</trd>
  <trd lng="fa">&a_m;&a_r;&f_k;&a_b;&a_A;&a_t;</trd>
  <trd lng="fr">citrus</trd>
  <trd lng="hu">citrus</trd>
  <trdgrp lng="it">
  <trd>citrus</trd>, 
  <trd>citro</trd>
  </trdgrp>
  <trd lng="pl">cytrus</trd>
  <trd lng="ru">&c_c;&c_i;&c_t;&c_r;&c_u;&c_s;&c_o;&c_v;&c_y;&c_je;</trd>
  <trd lng="sk">citrus</trd>
  <trd lng="sv">citrus</trd>
</drv>
</art>
