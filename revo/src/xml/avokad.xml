<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: avokad.xml,v 1.10 2016/04/24 12:10:19 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>avokad</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="avokad.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="avokad.0o.KUI">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <!-- Frukto de <tld/>arbo, iom granda (100 &gcirc;is 1000 gramojn
      peza),
      kun verda, mola karno kaj malmola kerno: -->
      Frukto de <ref cel="avokad.0ujo"><tld/>ujo</ref>, kun verda a&ubreve;
      bruneta &scirc;elo, tre nutra, butereca karno, kaj unu sola tre granda
      malmola kerno:
      <ekz>
        la&ubreve; botanika vidpunkto <tld/>o estas
        <ref tip="super" cel="ber.0o.BOT">bero</ref>
        <fnt>
        <bib>Viki</bib>,
        <lok>avokado, 2005-03-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_k;&c_a;&c_d;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;</trd>
  <trd lng="de">Avokado</trd>
  <trd lng="en">avocado</trd>
  <trd lng="es">aguacate</trd>
  <trd lng="fa">&a_A_madda;&a_w;&a_w;&f_k;&a_A;&a_d;&a_w;</trd>
  <trd lng="fr">avocat<klr tip="amb"> (fruit)</klr></trd>
  <trd lng="hu">avok&aacute;d&oacute;</trd>
  <trd lng="it">avocado</trd>
</drv>

<drv mrk="avokad.0ujo">
  <kap><ofc>9</ofc><tld/>ujo, 
  <var><kap><ofc>9</ofc><tld/>arbo</kap></var></kap>
  <snc mrk="avokad.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el Centrameriko, kultivata en tropikaj landoj pro siaj
      fruktoj,
      el familio <ref tip="super" cel="lauxrac.0oj">la&ubreve;racoj</ref>
      (<trd lng="la">Persea americana</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_k;&c_a;&c_d;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="bg">&c_a;&c_v;&c_o;&c_k;&c_a;&c_d;&c_o;</trd>
  <trd lng="de">Avokadobaum</trd>
  <trd lng="en">avocado</trd>
  <trd lng="es">aguacate</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_A_madda;&a_w;&a_w;&f_k;&a_A;&a_d;&a_w;</ind></trd>
  <trd lng="fr">avocatier</trd>
  <trd lng="hu">avok&aacute;d&oacute;fa</trd>
  <trd lng="it">avocado</trd>
</drv>
</art>
