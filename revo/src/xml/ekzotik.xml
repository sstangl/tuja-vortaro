<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekzotik.xml,v 1.11 2012/01/02 10:10:29 revo Exp $">
<kap>
  <rad>ekzotik</rad>/a
</kap>
<drv mrk="ekzotik.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="ekzot.0a">ekzota</ref>
    <ekz>
      por multaj homoj lo&gcirc;antaj sude de Svedio, la nomoj P&auml;r
      Lagerkvist,
      Eyvind Johnson kaj Harry Martinson ne nur estas <tld/>aj, sed tute
      nekonataj<fnt><aut>Franz-Georg R&ouml;ssler</aut>: <vrk>Kion
      aludas la
      flava koloro? (Hjalmar S&ouml;derberg, La kiso kaj dek tri aliaj
      noveloj)</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
      23a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">ex&ograve;tic</trd>
  <trdgrp lng="de">
    <trd>exotisch</trd>,
    <trd>fremdartig</trd>,
    <trd>fremdl&auml;ndisch</trd>
  </trdgrp>
  <trd lng="en">exotic</trd>
  <trd lng="es">ex&oacute;tico</trd>
  <trd lng="fr">exotique</trd>
  <trdgrp lng="hu">
    <trd>egzotikus</trd>,
    <trd>k&uuml;l&ouml;nleges</trd>,
    <trd>t&aacute;volki</trd>
  </trdgrp>
   <trdgrp lng="nl">
    <trd>exotisch</trd>,
    <trd>uitheems</trd>
  </trdgrp>
  <trd lng="pl">egzotyczny</trd>
  <trdgrp lng="pt">
    <trd>ex&oacute;tico</trd>,
    <trd>estrangeiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_z;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_i;&c_k;&c_o;&c_v;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ekzotik.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="ekzot.0o">ekzoto</ref>
    <ekz>
      Vilnius devus revigligi sian a&ubreve;tentikan historian
      hereda&jcirc;on kaj
      estigi litovan &leftquot;<tld/>on&rightquot;<fnt><aut>Last</aut>:
      <vrk>Vilnius ser&ccirc;as
      &leftquot;ekzotikon&rightquot;</vrk>, <bib>Monato</bib><lok>jaro
      2002a, numero 2a, p.
      20a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_o;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>peculiaritat</trd>,
    <trd>exotisme</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Exotik</trd>,
    <trd>Fremdartigkeit</trd>
  </trdgrp>
  <trd lng="en">exoticness</trd>
  <trdgrp lng="fr">
    <trd>exotisme<klr> (caract&egrave;re exotique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egzotikuss&aacute;g</trd>,
    <trd>k&uuml;l&ouml;nlegess&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">exotisme</trd>
  <trd lng="pl">egzotyka</trd>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_o;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="ekzotik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <ref tip="dif" cel="ekzot.0ajxo">ekzota&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;
  &c_r;&c_e;&c_ch;</trd>
  <trdgrp lng="ca">
    <trd>raresa</trd>,
    <trd>cosa singular</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Exotisches</trd>,
    <trd>Fremdartiges</trd>
  </trdgrp>
  <trd lng="en">exotic</trd>
  <trdgrp lng="fr">
    <trd>objet <ind>exotique</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egzotikum</trd>,
    <trd>k&uuml;l&ouml;nleges dolog</trd>
  </trdgrp>
   <trdgrp lng="nl">
    <trd>iets <ind>exotisch</ind></trd>,
    <trd>iets <ind>uitheems</ind></trd>
  </trdgrp>
  <trd lng="pl">egzotyk</trd>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_o;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
</art>

