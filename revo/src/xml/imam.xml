<art mrk="$Id: imam.xml,v 1.14 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>imam</rad>/o
</kap>
<drv mrk="imam.0o">
  <kap><tld/>o</kap>
  <snc mrk="imam.0o.pregxestro">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Teologo kaj kultoficanto de <ref tip="vid"
      cel="moske.0o">moskeo</ref>.
    </dif>
  </snc>
  <snc mrk="imam.0o.kalifo">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Religia kaj politika suvereno de islamanoj, titolo de la
      <ref tip="vid" cel="kalif.0o">kalifoj</ref>:
      <ekz>
        aperis la aktoroj, kiuj prezentas scenojn pri la morto de
        <tld/>o
        Husejn
        <fnt>
        <aut>Mahbube Mirkadiri</aut>,
        <vrk>Donaco el voja&gcirc;o</vrk>,
        <bib>LOdE</bib>,
        <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_a;&c_m;</trd>
  <trd lng="ca">imam</trd>
  <trd lng="cs">imam</trd>
  <trd lng="de">Imam</trd>
  <trd lng="en">imam</trd>
  <trd lng="es">im&aacute;n <klr>(islam)</klr></trd>
  <trdgrp lng="fr">
    <trd>imam</trd>,
    <trd>iman</trd>
  </trdgrp>
  <trd lng="hu">im&aacute;m</trd>
  <trd lng="nl">imam</trd>
  <trd lng="pl">imam</trd>
  <trdgrp lng="pt">
    <trd>im&atilde;<klr tip="amb"> (islamismo)</klr></trd>,
    <trd>imamo<klr tip="amb"> (islamismo)</klr></trd>,
    <trd>imame<klr tip="amb"> (islamismo)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_a;&c_m;</trd>
  <trd lng="sk">im√°m</trd>
</drv>
