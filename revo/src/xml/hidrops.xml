<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hidrops.xml,v 1.3 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hidrops</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hidrops.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="hidrops.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="edem.0o.MED">edemo</ref>
      <ekz>
        <klr>[la kadavro]</klr> estis &scirc;velinta, pli ol &ccirc;e 
        mortinto pro <tld/>o
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>edém</trd>,
    <trd>hydrops</trd>,
    <trd>otok</trd>,
    <trd>vodnatelnost</trd>
  </trdgrp>
  <trd lng="fr">hydropisie</trd>
  <trdgrp lng="sk">
    <trd>výkrojok</trd>,
    <trd>zárez</trd>
  </trdgrp>
</drv>
</art>

