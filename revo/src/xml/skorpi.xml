<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skorpi.xml,v 1.28 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skorpi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skorpi.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="skorpi.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj havantaj pin&ccirc;ilojn kaj
      sessegmentan abdomenon fini&gcirc;antan je venena pikilo;
      ordo el la filumo
      <ref tip="super" cel="artik.0uloj.ZOO">artikuloj</ref>
      (<trd lng="la">Scorpiones</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_p;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="ca">escorpins</trd>
  <trd lng="de">Skorpione</trd>
  <trd lng="en">scorpion</trd>
  <trd lng="es">escorpi&oacute;n</trd>
  <trd lng="fr">scorpions</trd>
  <trd lng="hu">skorpi&oacute;k<klr tip="ind"> (rend></klr></trd>
  <trd lng="id">kalajengking</trd>
  <trd lng="nl">schorpioenen</trd>
  <trd lng="pl">skorpiony</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_p;&c_i;&c_o;&c_n;</trd>
</drv>

<drv mrk="skorpi.S0o">
  <kap><tld lit="S"/>o</kap>
  <mlg>Sco</mlg>
  <mlg>&#9807;</mlg>
  <snc mrk="skorpi.S0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj"
      val="8">Stelfiguro</ref> kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Scorpio</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_k;&c_a;&c_r;&c_p;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="ca">(l') Escorpi&oacute;</trd>
  <trd lng="cs">štír</trd>
  <trd lng="de">Skorpion</trd>
  <trd lng="en">Scorpio</trd>
  <trd lng="es">Escorpio</trd>
  <trd lng="fr">Scorpion<klr> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &ayin;&qof;&resh;&bet;</trd>
  <trd lng="hu">Skorpi&oacute;</trd>
  <trdgrp lng="id">
    <trd>Akrab</trd>,
    <trd>Kala</trd>,
    <trd>Skorpio</trd>
  </trdgrp>
  <trd lng="nl">schorpioen</trd>
  <trd lng="pl">skorpion</trd>
  <trd lng="ru">&c_S;&c_k;&c_o;&c_r;&c_p;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">škorpión</trd>
</drv>
</art>
