<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxajot.xml,v 1.6 2013/08/27 07:10:35 revo Exp $">
<kap>
  <rad>&ccirc;ajot</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="cxajot.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxajot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Liano el varmaj landoj kultivata pro la frukto,      
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio 
<ref tip="super" cel="kukurb1.0oj.BOT">kukurbacoj</ref>
      (<trd lng="la">Sechium edule</trd>).
    </dif>
   <trdgrp lng="it">
    <trd>chayote</trd>,
    <trd>sechio</trd>,
    <trd>sechium edule</trd>
  </trdgrp>
  <adm>&ccirc;u ne devus esti &ccirc;ajotujo?<aut>wd</aut></adm>
  </snc>
  <snc mrk="cxajot.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>planto, pugnogranda, helflava a&ubreve; helverda,
      kun la&ubreve;longaj malrektaj sulkoj kaj unusola kerno, kuirebla 
      kiel <ref tip="super" cel="legom.0o.KUI">legomo</ref>:
      <ekz>
        <tld/>o <klr>...</klr> estas piroforma kun faldetoj
        <fnt><bib>Viki</bib>, 
        <lok>artikolo &leftquot;&ccirc;ajoto&rightquot;, 
        2007-09-21</lok></fnt>;
      </ekz>
      <ekz>
        ili rikoltis <klr>...</klr> <tld/>on <klr>...</klr> kaj papajon
        <fnt>
          <aut>J. L. Mu&ntilde;oz</aut>,
          <vrk>
          <url ref="http://www.geocities.com/skarabo_2000/mayas.htm">
          La Majaoj</url></vrk>,
          <lok>[vidita en 2008-01]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
      <trd>chayote</trd>,
      <trd>sechio</trd>,
      <trd>sechium edule</trd>,
      <trd><ind>patata</ind> spinosa<klr> (pop.)</klr></trd>, 
      <trd><ind>melanzana</ind> spinosa<klr> (pop.)</klr></trd>, 
      <trd><ind>zucchina</ind> spinosa<klr> (pop.)</klr></trd>, 
      <trd><ind>melanzana</ind> americana<klr> (pop.)</klr></trd>, 
      <trd><ind>lingua</ind> di lupo<klr> (pop.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="de">Chayote</trd>
  <trdgrp lng="es">
    <trd>chayota</trd>,
    <trd>chayote</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chayote</trd>,
    <trd>chocho</trd>,
    <trd>christophine</trd>,
    <trd>merliton</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chayote</trd>,
    <trd>chouchou<klr> (l&eacute;gume)</klr></trd>,
    <trd>christophine<klr> (l&eacute;gume)</klr></trd>
  </trdgrp>
  <trd lng="pl">kolczoch<klr> jadalny</klr></trd>
  <trd lng="pt">chuchu</trd>
</drv>
</art>

