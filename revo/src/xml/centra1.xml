<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: centra1.xml,v 1.20 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>central</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="centra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="centra1.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Komutejo, nodo en komunikada reto telefona a&ubreve; telegrafa.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_z;&c_je;&c_l;
      &c_s;&c_u;&c_v;&c_ja;&c_z;&c_ib;</trd>,
      <trd>&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Zentrale</trd>
    <trdgrp lng="en">
      <trd>central</trd>,
      <trd>telephone exchange</trd>
    </trdgrp>
    <trd lng="fr">centrale</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;zpont<klr tip="ind">
      (centr&aacute;l&eacute;)</klr></trd>;
      <trd>centr&aacute;l&eacute;</trd>
    </trdgrp>
    <trd lng="pl">centrala<klr> telefoniczna</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_z;&c_je;&c_l; &c_s;&c_v;&c_ja;&c_z;&c_i;</trd>;
      <trd>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="centra1.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Fabriko produktanta elektran a&ubreve; termikan energion:
      <ekz>
        <ind>varmo<tld/>o</ind>;
        <trd
        lng="be">&c_c;&c_je;&c_p;&c_l;&c_a;&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
        <trd lng="ca"><ind>central</ind> t&egrave;rmica</trd>
        <trd lng="pl">ciep&lstroke;ownia</trd>
        <trd
        lng="ru">&c_t;&c_je;&c_p;&c_l;&c_o;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind>elektro<tld/>o</ind>
        <klr>(<ref tip="sub" cel="elektr.0ejo">elektrejo</ref>)</klr>
        <trd lng="ca"><ind>central</ind> el&egrave;ctrica</trd>
        <trd lng="pl">elektrownia</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
    <trd lng="de">Kraftwerk</trd>
    <trdgrp lng="en">
      <trd>power plant</trd>,
      <trd>power station</trd>,
      <trd>generating station</trd>
    </trdgrp>
    <trd lng="fr">centrale</trd>
    <trd lng="hu">er&odblac;m&udblac;</trd>
    <trd lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
  </snc>
  <snc mrk="centra1.0o.oficejo">
    <dif>
      Centra oficejo:
      <ekz>
        <ind>sindikata <tld/>o</ind>.
        <trd lng="ca"><ind>central</ind> sindical</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_t;&c_e;&c_r;&c_a;
      <klr tip="amb">(&c_f;&c_ib;&c_r;&c_m;&c_y;,
      &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ib;)</klr></trd>,
      <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y;
      &c_o;&c_f;&c_ib;&c_s;</trd>
    </trdgrp>
    <trd lng="de">Zentrale</trd>
    <trdgrp lng="en">
      <trd>central office</trd>
    </trdgrp>
    <trd lng="fr">centrale</trd>
    <trd lng="hu">k&ouml;zpont<klr tip="ind">
    (sz&eacute;khely)</klr></trd>
    <trdgrp lng="pl">
      <trd>centrala</trd>,
      <trd>kwatera g&lstroke;&oacute;wna</trd>
    </trdgrp>
    <trd
    lng="ru">&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;
    <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
    <adm>
      PIV havas alian difinon, kaj la tuta senco &scirc;ajnas arkaika
      a&ubreve; nacilingva.
      <aut>Sergio</aut>
    </adm>
  </snc>
  <trd lng="ca">central</trd>
  <trdgrp lng="cs">
    <trd>centrála</trd>,
    <trd>výrobna energie</trd>,
    <trd>ústředna</trd>
  </trdgrp>
  <trd lng="es">central</trd>
  <trd lng="it">centrale</trd>
  <trd lng="nl">centrale</trd>
  <trd lng="pt">central</trd>
  <trdgrp lng="sk">
    <trd>centr&aacute;la</trd>,
    <trd>ústredňa</trd>
  </trdgrp>
</drv>
</art>
