<vortaro>
<art mrk="$Id: bifste.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>bifstek</rad>/o
</kap>
<uzo tip="fak">KUI</uzo>

<drv mrk="bifste.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Maldika rostita tran&ccirc;a&jcirc;o da bova&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_f;&c_sh;&c_t;&c_e;&c_k;&c_s;</trd>
  <trd lng="ca">bistec</trd>
  <trd lng="cs">biftek</trd>
  <trdgrp lng="de">
    <trd>Beefsteak</trd>
  </trdgrp>
  <trd lng="en">steak</trd>
  <trd lng="es">bistec</trd>
  <trdgrp lng="fr">
    <trd>bifteck</trd>,
    <trd>beefsteak</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&vav;&mem;&tsadi;&he;</trd>,
    <trd>&samekh;&tet;&yod;&yod;&qof;</trd>
  </trdgrp>
  <trd lng="hu">bifsztek</trd>
  <trd lng="nl">biefstuk</trd>
  <trd lng="pl">befsztyk</trd>
  <trd lng="pt">bife</trd>
  <trd lng="ru">&c_b;&c_i;&c_f;&c_sh;&c_t;&c_je;&c_k;&c_s;</trd>
  <trd lng="sk">biftek</trd>
</drv>
</art>
