        <trd>klausztrof&oacute;bia</trd>
      </trdgrp>
      <trd lng="id">klaustrofobia</trd>
      <trd lng="nl">claustrofobie</trd>
      <trd
lng="ru">&c_k;&c_l;&c_a;&c_u;&c_s;&c_t;&c_r;&c_o;&c_f;&c_o;&c_b;&c_i;&c_ja;</trd>
      <trd lng="sk">klaustrof√≥bia</trd>
