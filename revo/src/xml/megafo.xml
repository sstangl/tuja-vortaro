<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: megafo.xml,v 1.15 2012/08/19 18:10:34 revo Exp $">
<kap>
  <rad>megafon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="megafo.0o">
  <kap><tld/>o</kap>
  <snc mrk="megafo.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Forta <ref tip="super" cel="lauxt.0ilo.MUZ">la&ubreve;tparolilo</ref>
      por disa&ubreve;digo en granda ejo a&ubreve; sub libera &ccirc;ielo:
      <ekz>
        mi eka&ubreve;das <tld/>an vo&ccirc;on
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mikrof1.0o">mikrofono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_g;&c_a;&c_f;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">meg&agrave;fon</trd>
  <trd lng="de">Megaphon</trd>
  <trd lng="en">megaphone</trd>
  <trd lng="es">meg&aacute;fono</trd>
  <trd lng="fr">m&eacute;gaphone</trd>
  <trdgrp lng="hu">
    <trd>megafon</trd>,
    <trd>hangosbesz&eacute;l&odblac;</trd>
  </trdgrp>
  <trd lng="nl">megafoon</trd>
  <trdgrp lng="pl">
    <trd>megafon</trd>,
    <trd>szczekaczka<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>megafone</trd>,
    <trd>alto-falante</trd>,
    <trd>porta-voz <klr tip="amb">(megafone)</klr></trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_g;&c_a;&c_f;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">megaf&oacute;n</trd>
</drv>
</art>
