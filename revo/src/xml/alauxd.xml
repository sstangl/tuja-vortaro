<vortaro>
<art mrk="$Id: alauxd.xml,v 1.21 2013/09/01 13:10:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ala&ubreve;d</rad>/o
</kap>
<drv mrk="alauxd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="alauxd.0o.familio">
    <dif>
      Birdo el la familio <tld/>edoj.
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_v;&c_a;&c_r;&c_a;&c_n;&c_a;&c_k;</trd>
    <trd lng="bg">&c_ch;&c_u;&c_ch;&c_u;&c_l;&c_i;&c_g;&c_a;</trd>
    <trd lng="br">alc'hweder</trd>
    <trd lng="cs">sk&rcaron;ivan</trd>
    <trd lng="de">Lerche</trd>
    <trd lng="en">lark</trd>
    <trd lng="fr">alouette</trd>
    <trd lng="he">&ayin;&pe;&resh;&vav;&nun;&yod;</trd>
    <trd lng="hu">pacsirta</trd>
    <trd lng="nl">leeuwerik</trd>
    <trdgrp lng="pt">
      <trd>cotovia</trd>,
      <trd>calhandra</trd>,
      <trd>laverca</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
    <trd lng="sk">&scaron;kovr&aacute;nok</trd>
  </snc>
  <snc mrk="alauxd.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="alauxd1.0oj">ala&ubreve;dedoj</ref>
      (<trd lng="la">Alauda</trd>):
      <ekz>
        de &ccirc;ie flugis la seninterrompaj triloj de la <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p. 15a</lok></fnt>
      </ekz>
    </dif>
    <trd lng="br">alc'hweder-sant-P&ecirc;r</trd>
    <trd lng="cs">sk&rcaron;ivan poln&iacute;</trd>
    <trd lng="de">Feldlerche</trd>
    <trd lng="en">skylark</trd>
    <trd lng="fr">alouette des champs</trd>
    <trd lng="he">&ayin;&pe;&resh;&vav;&nun;&yod; &he;&shin;&dalet;&he;</trd>
    <trd lng="hu">mezei pacsirta</trd>
    <trd lng="pl">skowronek</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j;
    &c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
  </snc>
  <trd lng="ca">alosa</trd>
</drv>
</art>
