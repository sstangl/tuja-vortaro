<art mrk="$Id: gerani1.xml,v 1.11 2015/10/30 11:10:18 revo Exp $">
<kap>
  <rad>geraniac</rad>/oj
</kap>
<drv mrk="gerani1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Geraniaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_n;&c_je;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Storchschnabelgew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Geraniaceae</trd>,
    <trd>geranium family</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;raniac&eacute;es</trd>
  <trd lng="hu">g&oacute;lyaorrf&eacute;l&eacute;k</trd>
  <trd lng="nl">ooievaarsbekfamilie</trd>
  <trd lng="pl">bodziszkowate</trd>
  <trd lng="pt">gerani&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_a;&c_n;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_a;&c_n;&c_je;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
