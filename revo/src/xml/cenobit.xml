<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: cenobit.xml,v 1.8 2015/07/04 08:37:32 revo Exp $">
    <kap>
      <rad>cenobit</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="cenobit.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
    	<dif>
    	  <ref tip="super" cel="monahx.0o">Mona&hcirc;o</ref> vivanta en
          komunumo, en komuna lo&gcirc;ejo:
    	  <ekz>
    	    rezidejo de <tld/>inoj, kun abundo da &ccirc;ambroj ekstreme puraj
    	    <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 279a</lok></fnt>.
    	  </ekz>
    	</dif>
    	<refgrp tip="vid">
    	  <ref cel="ermit.0o.REL">ermito <sncref/></ref>
    	</refgrp>
      </snc>
  <trd lng="ca">cenobita</trd>
  <trdgrp lng="cs">
    <trd>cenobita</trd>,
    <trd>poustevník</trd>
  </trdgrp>
  <trdgrp lng="de">
         <trd>Klosterm&ouml;nch</trd>,
         <trd>Z&ouml;nobit</trd>
      </trdgrp>
  <trdgrp lng="fr">
    	<trd>c&eacute;nobite</trd>
      </trdgrp>
  <trdgrp lng="hu">
         <trd>szerzetes</trd>,
         <trd>c&ouml;nobita</trd>
      </trdgrp>
  <trd lng="it">cenobita</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_v;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>cenobita</trd>,
    <trd>pustovník</trd>
  </trdgrp>
</drv>
  </art>

