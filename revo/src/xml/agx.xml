<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: agx.xml,v 1.47 2016/01/21 07:10:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>a&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="agx.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="agx.0o.KOMUNE">
     <dif>
       Kiomo de la tempo pasinta depost la naski&gcirc;o a&ubreve;
       esti&gcirc;o:
       <ekz>
	 kia estas via <tld/>o?<fnt>Z</fnt>
       </ekz>
       <ekz>
	 kioman <tld/>on vi havas?
       </ekz>
       <ekz>
	 kian <tld/>on mi havas
	 <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>?
       </ekz>
       <ekz>
	 en la <tld/>o de ok tagoj
	 <fnt><bib>MT</bib>, <lok>&Gen; 17:12</lok></fnt>;
       </ekz>
       <ekz>
	 en la <tld/>o de dudek jaroj
	 <fnt><bib>MT</bib>, <lok>&Nom; 1:20</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="th">&#3629;&#3634;&#3618;&#3640;</trd>
   </snc>
   <trd lng="be">&c_u;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>
   <trd lng="br">oad</trd>
   <trd lng="cs">st&aacute;&rcaron;&iacute;</trd>
   <trd lng="de">Alter</trd>
   <trd lng="en">age</trd>
   <trd lng="es">edad</trd>
   <trd lng="fr">&acirc;ge</trd>
   <trdgrp lng="id">
     <trd>umur</trd>,
     <trd>usia</trd>
   </trdgrp>
   <trd lng="it">et&agrave;</trd>
   <trd lng="nl">leeftijd</trd>
   <trd lng="pl">wiek</trd>
   <trd lng="pt">idade</trd>
   <trd lng="ro">v&acirc;rst&abreve;</trd>
   <trd lng="ru">&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</trd>
   <trd lng="sk">vek</trd>
   <trd lng="sv">&aring;lder</trd>
   <trd lng="tr">ya&scedil;</trd>
</drv>

<drv mrk="agx.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="agx.0i.KOMUNE">
     <dif>
       Esti atinginta la <tld/>on de:
       <ekz>
         kiom li <tld/>as? li estas dekkvinjara;
       </ekz>
       <ekz>
         tiu &ccirc;i tombo <tld/>as almena&ubreve; jarcenton
         <fnt><bib>Kiso</bib>, <lok>tombeja arabesko</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">beza&ntilde; <ind>oadet</ind> a</trd>
   <trd lng="de">alt sein</trd>
   <trd lng="en">be the age of</trd>
   <trd lng="es">cumplir a&ntilde;os</trd>
   <trd lng="fr">&ecirc;tre <ind>&acirc;g&eacute;</ind> 
<klr>(de)</klr></trd>
   <trdgrp lng="id">
     <trd>[<ind>umur</ind>] berumur</trd>,
     <trd>[<ind>usia</ind>] berusia</trd>
   </trdgrp>
   <trd lng="pl"><klr tip="amb">(mie&cacute;)</klr> <ind>lat</ind></trd>
   <trd lng="pt">ter a <ind>idade</ind> <klr>(de)</klr></trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol;
<ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>

</drv>

<drv mrk="agx.0a">
   <kap><tld/>a<fnt>Z</fnt></kap>
   <snc mrk="agx.0a.KOMUNE">
     <dif>
       Havanta ian <tld/>on:
       <ekz>
	 mi estas pli <tld/>a ol vi;
       </ekz>
       <ekz>
	 esti 5 jarojn <tld/>a;
       </ekz>
       <ekz>
	 <tld/>ulo de unu monato &gcirc;is kvin jaroj
	 <fnt><bib>MT</bib>, <lok>&Lev; 27:6</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">oadet</trd>
   <trd lng="de">alt</trd>
   <trd lng="en">aged</trd>
   <trd lng="fr">&acirc;g&eacute;</trd>
   <trdgrp lng="id">
     <trd>tua</trd>,
     <trd>[<ind>umur</ind>] berumur</trd>
   </trdgrp>
   <trd lng="nl">oud</trd>
   <trd lng="pl"><klr tip="amb">(maj&aogonek;cy)</klr>
   <ind>lat</ind></trd>
   <trd lng="pt">idoso</trd>
   <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j;
<ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;</ind></trd>
<trd lng="tr">ya&scedil;l&inodot;</trd>
</drv>

<drv mrk="agx.grand0a">
   <kap>grand<tld/>a</kap>
   <snc mrk="agx.grand0a.KOMUNE">
     <dif>
       Jam tre <ref tip="super" cel="jun.mal0a">maljuna</ref>:
       <ekz>
	 kiel vi ne hontas, grand<tld/>a viro, tiel brui pro kelkaj
	 mankantaj butonoj
	 <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>!
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y;
<klr>(&c_n;&c_a;&c_z;.)</klr></trd>
   <trdgrp lng="br">
     <trd>kozh-tre</trd>,
     <trd>er maez a <ind>oad</ind></trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>aged</trd>,
     <trd>of great <ind>age</ind></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>de grand <ind>&acirc;ge</ind></trd>,
     <trd>tr&egrave;s <ind>ag&eacute;</ind></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>tua</trd>,
     <trd>[<ind>umur</ind>] berumur</trd>
   </trdgrp>
   <trd lng="nl">oud</trd>
   <trd lng="pl">wiekowy</trd>
   <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
</drv>

<drv mrk="agx.grand0ulo">
   <kap>grand<tld/>ulo</kap>
   <snc mrk="agx.grand0ulo.KOMUNE">
     <dif>
       Ulo tre maljuna:
       <ekz>
	 ilia respektego anta&ubreve; la grand<tld/>ulo
	 <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a,
Babel</lok></fnt>
	 .
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>elder</trd>,
     <trd>person of great age</trd>,
     <trd>doyen</trd>
   </trdgrp>
   <trd lng="fr">personne &acirc;g&eacute;e</trd>
   <trd lng="id"><ind>orang</ind> tua (bangka)</trd>
   <trd lng="pl">cz&lstroke;owiek w podesz&lstroke;ym
   <ind>wieku</ind></trd>
   <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_i;&c_k;</trd>
   <trd lng="tr">ya&scedil;l&inodot; ki&scedil;i</trd>
</drv>

<drv mrk="agx.infan0o">
   <kap>infan<tld/>o</kap>
   <snc mrk="agx.infan0o.KOMUNE">
     <dif>
       Unua vivotempo inter nasko kaj jun<tld/>o:
       <ekz>
         li komencis blindi&gcirc;i jam en sia infan<tld/>o
         <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj
         &ccirc;uk&ccirc;oj</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="br">
     <trd>bugaleaj</trd>,
     <trd>bugeliezh</trd>
   </trdgrp>
   <trd lng="de">Kindheit</trd>
   <trd lng="en">infancy <klr tip="amb">(age)</klr></trd>
   <trd lng="fr">enfance<klr tip="amb"> (&acirc;ge)</klr></trd>
   <trdgrp lng="id">
     <trd><ind>masa</ind> kanak-kanak</trd>,
     <trd><ind>masa</ind> kecil</trd>
   </trdgrp>
   <trd lng="pl">dzieci&nacute;stwo</trd>
   <trd lng="ro">copil&abreve;rie</trd>
   <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
   <trd lng="sv">barndom</trd>
</drv>

<drv mrk="agx.jun0o">
   <kap>jun<tld/>o</kap>
   <snc mrk="agx.jun0o.KOMUNE">
     <dif>
       Vivotempo, dum kiu oni estas juna:
       <ekz>
	 mi memoras tagojn de mia jun<tld/>o
	 <fnt><bib>Metrop</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">yaouankiz</trd>
   <trd lng="de">Jugend</trd>
   <trd lng="en">youth <klr tip="amb">(age)</klr></trd>
   <trd lng="fr">jeunesse<klr tip="amb"> (&acirc;ge)</klr></trd>
   <trd lng="id"><ind>masa</ind> muda</trd>
   <trd lng="pl">m&lstroke;odo&sacute;&cacute;</trd>
   <trd lng="ro">tinere&tcedil;e</trd>
   <trd lng="ru">&c_ju;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="agx.mez0a">
   <kap>mez<tld/>a</kap>
   <snc mrk="agx.mez0a.KOMUNE">
     <dif>
       Proksimume en la mezo de normale longa vivo,
       nek tre juna, nek tre maljuna:
       <ekz>
	 mez<tld/>a nerazita viro en kalvi&gcirc;inta leda jako
	 <fnt><bib>Metrop</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">etre daou <ind>oad</ind></trd>
   <trd lng="en">middle-aged</trd>
   <trd lng="fr">d'<ind>&acirc;ge</ind> moyen</trd>
   <trdgrp lng="id">
     <trd>se<ind>paruh</ind> baya</trd>,
     <trd>se<ind>tengah</ind> baya</trd>
   </trdgrp>
   <trd lng="pl">w &sacute;rednim <ind>wieku</ind></trd>
   <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_g;&c_o;
<ind>&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;</ind></trd>
   <trd lng="sv">medel&aring;lders</trd>
   <trd lng="tr">orta ya&scedil;</trd>
</drv>

<drv mrk="agx.mez0ulo">
   <kap>mez<tld/>ulo</kap>
   <snc mrk="agx.mez0ulo.KOMUNE">
     <dif>
       Ulo de meza <tld/>o, nek tre juna, nek tre maljuna:
       <ekz>
	 la viro, mez<tld/>ulo, estis Hagaj, iama sklavo
	 <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="en">middle-aged person</trd>
   <trd lng="fr">homme d'&acirc;ge moyen</trd>
   <trdgrp lng="id">
     <trd><ind>orang</ind> separuh baya</trd>,
     <trd><ind>orang</ind> setengah baya</trd>
   </trdgrp>
   <trd lng="pl">cz&lstroke;owiek w &sacute;rednim
   <ind>wieku</ind></trd>
   <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;
&c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_g;&c_o;
&c_v;&c_o;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;</trd>
   <trd lng="sv">medel&aring;lders person</trd>
</drv>

<drv mrk="agx.mond0o">
   <kap>mond<tld/>o</kap>
   <snc mrk="agx.mond0o.KAL">
     <uzo tip="fak">KAL</uzo>
     <dif>
       Longa <ref tip="super" cel="epok.0o">epoko</ref>, parto de la
       homa a&ubreve; universa historio:
       <ekz>
	 tiel estos &ccirc;e la fino de la mond<tld/>o
	 <fnt><bib>NT</bib>, <lok>&Mat; 13:40</lok></fnt>;
       </ekz>
       <ekz>
	 la sa&gcirc;eco ka&scirc;ita, kiun Dio anta&ubreve;destinis
anta&ubreve; la mond<tld/>oj por
	 nia glorado
	 <fnt><bib>NT</bib>, <lok>&Ko1; 2:7</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">oadvezh</trd>
   <trdgrp lng="en">
     <trd>epoch</trd>,
     <trd>period <klr tip="amb">(geological)</klr></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>p&eacute;riode<klr tip="amb"> (historique)</klr></trd>,
     <trd>si&egrave;cles<klr tip="amb"> (longue
     &eacute;poque)</klr></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>masa</trd>,
     <trd>zaman</trd>
   </trdgrp>
   <trd lng="pl">epoka</trd>
   <trd lng="ro">er&abreve;</trd>
   <trd lng="ru">&c_e;&c_p;&c_o;&c_h;&c_a;</trd>
</drv>

<drv mrk="agx.neplen0a">
   <kap>neplen<tld/>a</kap>
   <snc mrk="agx.neplen0a.POL">
     <dif>
       Ankora&ubreve; ne atinginta la <tld/>on de plenaj sociaj rajtoj:
       <ekz>
	 Ijob volonte konsentis kontroladi la administradon de la bienoj de
	 Omar dum la neplen<tld/>o de ties filino
	 <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a, paca
Betra&hcirc;</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="br">dindan an <ind>oad</ind></trd>
   <trd lng="br">dindan an <ind>oad</ind></trd>
   <trdgrp lng="en">
     <trd>minor <klr tip="amb">(legally)</klr></trd>,
     <trd>underage</trd>
   </trdgrp>
   <trd lng="fr">mineur<klr tip="amb"> (civiquement)</klr></trd>
   <trd lng="id"><ind>di</ind> bawah umur</trd>
   <trdgrp lng="pl">
     <trd>nieletni</trd>,
     <trd>niepe&lstroke;noletni</trd>
   </trdgrp>
   <trd lng="ro">minor</trd>
   <trd
lng="ru">&c_n;&c_je;&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="agx.plej0ulo">
   <kap>plej<tld/>ulo</kap>
   <snc mrk="agx.plej0ulo.POL">
     <ref tip="dif" cel="agx.pli0ulo.POL">pli<tld/>ulo</ref>
     <ekz>
       &ccirc;e &ccirc;iu fenestro staris tri skribistoj kaj unu urba
plej<tld/>ulo
       <fnt><bib>Fab3</bib>, 
<lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok></fnt>.
     </ekz>
   </snc>
   <trd lng="de">&Auml;ltester</trd>
   <trdgrp lng="en">
     <trd>elder</trd>,
     <trd>doyen</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>a&icirc;n&eacute;<klr tip="amb"> (subst.)</klr></trd>,
     <trd>ancien<klr tip="amb"> (subst.)</klr></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>senior</trd>,
     <trd>[<ind>sepuh</ind>] sesepuh</trd>,
     <trd>[<ind>tua</ind>]tetua</trd>
   </trdgrp>
   <trd lng="pl">senior</trd>
   <trd
   lng="ru">&c_s;&c_t;&c_a;&c_r;&c_je;&c_j;&c_sh;&c_i;&c_n;&c_a;</trd>
   <trd lng="sv">&auml;ldste</trd>
</drv>

<drv mrk="agx.plen0a">
   <kap>plen<tld/>a</kap>
   <snc mrk="agx.plen0a.POL">
     <dif>
       Havanta la <tld/>on, kiu donas &ccirc;iujn civitanajn rajtojn:
       <ekz>
         &ccirc;iuj plen<tld/>aj lo&gcirc;antoj de amba&ubreve; seksoj
         havas la rajton de vo&ccirc;donado
         <fnt><bib>FK</bib>, <lok>El la vivo kaj sciencoj</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_r;&c_o;&c_s;&c_l;&c_y;</trd>,
     <trd>&c_p;&c_a;&c_w;&c_n;&c_a;&c_l;&c_je;&c_t;&c_n;&c_ib;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>
   </trdgrp>
   <trd lng="br">deuet (d'e oad gour)</trd>
   <trd lng="de">vollj&auml;hrig</trd>
   <trd lng="en">over the age of majority</trd>
   <trd lng="es">mayor de edad</trd>
   <trd lng="fr">majeur<klr tip="amb"> (civiquement)</klr></trd>
   <trd lng="nl">meerderjarig</trd>
   <trd lng="pl">pe&lstroke;noletni</trd>
   <trdgrp lng="pt">
     <trd>adulto</trd>,
     <trd>maior de idade</trd>
   </trdgrp>
   <trd lng="ro">major</trd>
   <trdgrp lng="ru">
     <trd>&c_v;&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>
   </trdgrp>
   <trd lng="sk">plnolet&yacute;</trd>
   <trd lng="tp">suli</trd>
   <trd lng="tr">ergin</trd>
</drv>

<drv mrk="agx.pli0igxi">
  <kap>pli<tld/>i&gcirc;i</kap>
  <snc mrk="agx.pli0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli <tld/>a,
      <ref tip="vid" cel="jun.mal0igxi">maljuni&gcirc;i</ref>:
      <ekz>
        li &scirc;ajne neniel &scirc;an&gcirc;i&gcirc;is a&ubreve;
        pli<tld/>i&gcirc;is
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">prendre de l'<ind>&acirc;ge</ind></trd>
  <trd lng="id">[<ind>tua</ind>] menua</trd>
</drv>

<drv mrk="agx.pli0ulo">
   <kap>pli<tld/>ulo</kap>
   <snc mrk="agx.pli0ulo.POL">
     <dif>
       Iu el la pli <tld/>aj, respektataj homoj en grupo a&ubreve;
       socio:
       <ekz>
	 kiam la &ccirc;efpastroj kaj pli<tld/>uloj akuzis lin, li respondis
	 nenion
	 <fnt><bib>NT</bib>, <lok>&Mat; 27:12</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>elder</trd>,
     <trd>doyen</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>a&icirc;n&eacute;<klr tip="amb"> (subst.)</klr></trd>,
     <trd>ancien<klr tip="amb"> (subst.)</klr></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>senior</trd>,
     <trd>[<ind>sepuh</ind>] sesepuh</trd>,
     <trd>[<ind>tua</ind>]tetua</trd>
   </trdgrp>
   <trd lng="de">&Auml;lterer</trd>
   <trd lng="pl">starzec</trd>
   <trd
   lng="ru">&c_s;&c_t;&c_a;&c_r;&c_je;&c_j;&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="agx.profund0a">
   <kap>profund<tld/>a</kap>
   <snc mrk="agx.profund0a.KOMUNE">
     <ref tip="dif" cel="agx.grand0a">grand<tld/>a</ref>
     <ekz>
       Abraham <klr>...</klr> mortis en bona maljuneco, profund<tld/>a
       kaj sata de vivo
       <fnt><bib>NT</bib>, <lok>&Gen; 25:8</lok></fnt>.
     </ekz>
   </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y;
<klr>(&c_n;&c_a;&c_z;.)</klr></trd>
