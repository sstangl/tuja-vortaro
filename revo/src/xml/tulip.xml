<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: tulip.xml,v 1.22 2015/07/04 08:37:45 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>tulip</rad>/o
</kap>
<drv mrk="tulip.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="liliac.0oj">liliacoj</ref>
       (<trd lng="la">Tulipa</trd>), ornama vegeta&jcirc;o kun belaj
       diverskoloraj
       floroj.
     </dif>
   </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_mol;&c_p;&c_a;&c_n;</trd>
  <trd lng="bg">&c_l;&c_a;&c_l;&c_je;</trd>
  <trdgrp lng="ca">
     <trd>tulipa</trd>,
     <trd>g&egrave;nere Tulipa</trd>
   </trdgrp>
  <trd lng="cs">tulipán</trd>
  <trd lng="de">Tulpe</trd>
  <trd lng="en">tulip</trd>
  <trd lng="es">tulip&aacute;n</trd>
  <trd lng="fa">&a_l;&a_A;&a_l;&a_h;</trd>
  <trd lng="fr">tulipe</trd>
  <trd lng="hu">tulip&aacute;n</trd>
  <trd lng="nl">tulp</trd>
  <trd lng="pl">tulipan</trd>
  <trd lng="pt">tulipa</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_mol;&c_p;&c_a;&c_n;</trd>
  <trd lng="sk">tulipán</trd>
  <trd lng="sv">tulpan</trd>
</drv>
</art>
