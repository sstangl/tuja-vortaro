<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: norveg.xml,v 1.12 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>norveg</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="norveg.0o">
  <kap><tld/>o</kap>
  <snc mrk="norveg.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="norveg.N0ujo.GEOG"><tld lit="N"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="norveg.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="cs">Nor</trd>
  <trd lng="en">Norwegian</trd>
  <trd lng="fr">Norv&eacute;gien<klr> (subst.)</klr></trd>
  <trd lng="nl">Noor</trd>
  <trd lng="no">nordmann</trd>
  <trd lng="sk">Nór</trd>
</drv>

<drv mrk="norveg.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="cs">norský</trd>
  <trd lng="en">Norwegian</trd>
  <trd lng="fr">norv&eacute;gien<klr> (adj.)</klr></trd>
  <trd lng="nl">Noors</trd>
  <trd lng="no">norsk</trd>
  <trd lng="sk">nórsky</trd>
</drv>


<drv mrk="norveg.N0ujo">
  <kap>
    <tld lit="N"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="norveg.N0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la norda 
      E&ubreve;ropo sur la Skandinava duoninsulo kun &ccirc;efurbo Oslo;
      teritorio de Regno Norvegujo;
      &gcirc;i limas kun
      <ref tip="vid" cel="finn.F0lando">Finnlando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>,
      <ref tip="vid" cel="sved.S0ujo">Svedujo</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>
      (ISO-3166 kodo: NO).
    </dif>
    <ref tip="sin" cel="norvegi.N0o">Norvegio</ref>
    <trd lng="en">Norway</trd>
    <trd lng="fa">&a_n;&a_r;&a_w;&f_zh;</trd>
    <trd lng="fr">Norv&egrave;ge</trd>
    <trd lng="id">Norwegia</trd>
    <trd lng="nl">Noorwegen</trd>
    <trd lng="no">Norge</trd>
    <trdgrp lng="pl">
      <trd>Norwegia</trd>,
      <trd>Kr&oacute;lestwo <ind>Norwegii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

