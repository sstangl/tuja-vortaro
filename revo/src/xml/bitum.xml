<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bitum.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>bitum</rad>/o
<ofc>1</ofc>
</kap>

<drv mrk="bitum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nigrebruna terpe&ccirc;o. 
    </dif>
    <adm>
      La referencoj inter bitumo, terpe&ccirc;o kaj asfalto estas
      cirklaj
      kaj la difinoj de asfalto kaj bitumo aspektas al mi
      mal&gcirc;ustaj de 
      faka vidpunkto.<aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="gudr.0o">gudro</ref>,
      <ref cel="pecx.0o">pe&ccirc;o</ref>,
      <ref cel="pecx.ter0o">terpe&ccirc;o</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_ib;&c_t;&c_u;&c_m;</trd>
    <trd lng="bg">&c_b;&c_i;&c_t;&c_u;&c_m;</trd>
    <trd lng="de">Bitumen</trd>
    <trd lng="en">bitumen</trd>
    <trd lng="fr">bitume</trd>
    <trd lng="hu">bitumen</trd>
    <trd lng="nl">bitumen</trd>
    <trd lng="pl">bitum</trd>
    <trd lng="ru">&c_b;&c_i;&c_t;&c_u;&c_m;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bitumen</trd>,
    <trd>zemní vosk</trd>,
    <trd>živice</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bitúmen</trd>,
    <trd>živica</trd>
  </trdgrp>
</drv>
<drv mrk="bitum.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kovri a&ubreve; &scirc;miri per <tld/>o.  
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;
  <ind>&c_b;&c_ib;&c_t;&c_u;&c_m;&c_a;&c_m;</ind></trd>
  <trdgrp lng="fr">
    <trd>bitumer</trd>,
    <trd>bituminer</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>bitumenez</trd>,
    <trd>aszfaltoz</trd>
  </trdgrp>
  <trd lng="nl">bitumineren</trd>
  <trdgrp lng="pl">
    <trd>bitumowa&cacute;</trd>,
    <trd>pokrywa&cacute; <ind>bitumem</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;
  &c_b;&c_i;&c_t;&c_u;&c_m;&c_o;&c_m;</trd>
</drv>
</art>
