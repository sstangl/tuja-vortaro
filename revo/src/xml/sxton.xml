    <trd>sialolithiasis</trd>,
    <trd>speekselsteenziekte</trd>
  </trdgrp>
  <trd lng="pl">kamica &sacute;linianek</trd>
  <trd
  lng="ru">&c_s;&c_l;&c_ju;&c_n;&c_n;&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
  &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
</drv>

<drv mrk="sxton.urin0amalsano">
  <kap>urin<tld/>a malsano
  <fnt> <vrk> <url
  ref="http://vortareto.free.fr/dictionnaire/esp-fr/m.htm"> Dictionnaire
  m&eacute;dical esp&eacute;ranto-fran&ccedil;ais</url></vrk>
  </fnt></kap>
  <snc mrk="sxton.urin0amalsano.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
