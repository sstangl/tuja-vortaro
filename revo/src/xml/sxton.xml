  </trdgrp>
  <trd lng="it">urolitiasi</trd>
  <trd lng="nl">urolithiasis</trd>
  <trd lng="pl">kamica nerkowa</trd>
  <trd
  lng="ru">&c_m;&c_o;&c_ch;&c_je;&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
  &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
</drv>
