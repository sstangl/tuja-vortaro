<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxton.xml,v 1.39 2015/12/16 10:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ton</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxton.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malmola pli malpli granda minerala maso:
      <ekz>
        sur la tero ku&scirc;as <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 6</lok></fnt>;
      </ekz>
      <ekz>
        ruli <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Mi faros Jerusalemon amaso da <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Jer; 9:10</lok></fnt>;
      </ekz>
      <ekz>
        alligi <tld/>on al &jcirc;etilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sa&gcirc;o estas pli bona ol multekostaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Sen; 8:11</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj alportas sian <tld/>on <klr>(kunlaboras)</klr>
        por la konstruo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>trafi du celojn per unu <tld/>o</ind>
        <trd lng="es">matar dos p&aacute;jaros de un tiro</trd>
        <trd lng="fr">faire d'une pierre deux coups</trd>
        <klr>(per sama rimedo havi du profitojn)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per unu <tld/>o oni du &jcirc;etojn ne faras
        <klr>(por sukcesi oni ne devas samtempe celi plurajn
        atingota&jcirc;ojn;
        vd <ref tip="vid" cel="lepor.0o">leporo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne mi &jcirc;etis la unuan <tld/>on
        <klr>(ne mi unue atakis, iniciatis la malamikajn
        agojn)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &jcirc;eti <tld/>ojn sur la vojon de iu
        <klr>(intence aran&gcirc;i malfacila&jcirc;ojn al iu)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>o de falpu&scirc;i&gcirc;o</ind>
        <klr>(falilo; ka&ubreve;zo de skandalo)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
        &c_s;&c_p;&c_a;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ja;
        </trd>
        <trd lng="es">escollo</trd>
        <trd lng="hu">botr&aacute;nyk&odblac;</trd>
        <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;
        &c_p;&c_r;&c_je;&c_t;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;
        </trd>
      </ekz>
    </dif>
    <trd lng="en">stone</trd>
    <trd lng="es">piedra</trd>
    <trd lng="fr">pierre</trd>
    <trdgrp lng="it">
      <trd>pietra</trd>,
      <trd>sasso</trd>
    </trdgrp>
    <trd lng="pl">kamie&nacute;</trd>
  </snc>
  <snc mrk="sxton.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Negranda <tld/>eca
      <ref tip="super" cel="konkrement.0o.FIZL">konkremento</ref>
      kiu fojfoje formi&gcirc;as en glandoj a&ubreve; iliaj duktoj:
      <ekz>
        havi <tld/>on en reno, galveziko, salivglando.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxton.gal0amalsano">gal<tld/>a malsano</ref>,
      <ref cel="sxton.saliv0amalsano">saliv<tld/>a malsano</ref>,
      <ref cel="sxton.urin0amalsano">urin<tld/>a malsano</ref>
    </refgrp>
    <trd lng="en">calculus</trd>
    <trd lng="es">c&aacute;lculo<klr> (medicina)</klr></trd>
    <trd lng="fr">calcul<klr> (m&eacute;decine)</klr></trd>
    <trd lng="it">calcolo</trd>
    <trd lng="pl">kamie&nacute;</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Stein</trd>
  <trd lng="es">piedra</trd>
  <trd lng="hu">k&odblac;</trd>
  <trd lng="id">batu</trd>
  <trd lng="nl">steen</trd>
  <trd lng="pt">pedra</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">sten</trd>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="sxton.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxton.0a.MIN">
    <dif>
      El <tld/>o:
      <ekz>
        mi dezirus lo&gcirc;i en granda <tld/>a kastelo
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">stone</trd>
    <trdgrp lng="de">
      <trd>steinern</trd>,
      <trd>aus Stein</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de piedra</trd>,
      <trd>p&eacute;treo</trd>
    </trdgrp>
    <trd lng="pl">kamienny</trd>
  </snc>
  <snc mrk="sxton.0a.FIG_fortika">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortika nevenkebla:
      <ekz>
        ili dormis <tld/>an dormon<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fer.0a.FIG">fera <sncref/></ref>,
      <ref cel="sxtal.0a.FIG">&scirc;tala <sncref/></ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>iron</trd>,
      <trd>cast-iron</trd>
    </trdgrp>
    <trd lng="de">wie ein Stein</trd>
    <trd lng="es">de piedra</trd>
    <trd lng="pl">kamienny</trd>
  </snc>
  <snc mrk="sxton.0a.FIG_senindulga">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne mildigebla, senkompata, senindulga, senama:
      <ekz>
        koro <tld/>a<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="marmor">marmora</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>hard</trd>,
      <trd>stone</trd>
    </trdgrp>
    <trd lng="de">aus Stein</trd>
    <trd lng="es">de piedra</trd>
    <trd lng="pl">kamienny</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="es">
    <trd>l&iacute;tico</trd>,
    <trd>de <ind>piedra</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lithique</trd>,
    <trd>de <ind>pierre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&odblac;-</trd>,
    <trd>k&odblac;kem&eacute;ny</trd>,
    <trd>k&odblac;b&odblac;l val&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stenen</trd>,
    <trd>van <ind>steen</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>litico</trd>,
    <trd>di <ind>pietra</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="sxton.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="sxton.0ego.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Granda nemovebla <tld/>o:
      <ekz>
        tombo <klr>...</klr> elhakita el <tld/>ego
        <fnt><bib>NT</bib>, <lok>&Mar; 15:46</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rok.0o">roko</ref>
    </refgrp>
  </snc>
  <trd lng="en">boulder</trd>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_l;&c_y;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Fels</trd>
  <trd lng="es">roca</trd>
  <trd lng="fr">roc</trd>
  <trd lng="hu">k&odblac;szikla</trd>
  <trd lng="it">masso</trd>
  <trd lng="nl">rotsblok</trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;az</trd>,
    <trd>ska&lstroke;a</trd>,
    <trd>kamol<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="sxton.0eto.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Tre malgranda <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia bu&scirc;o estas plena de <tld/>etoj<fnt>Z</fnt>
        <klr>(li malklare prononcas)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gruz.0o">gruzo</ref>,
      <ref cel="sabl.0o">sablo</ref>
    </refgrp>
  </snc>
  <trd lng="en">pebble</trd>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Steinchen</trd>,
    <trd>Kiesel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>guijarro</trd>,
    <trd>chino</trd>,
    <trd>canto rodado</trd>
  </trdgrp>
  <trd lng="fr">caillou</trd>
  <trd lng="hu">kavics</trd>
  <trd lng="id">kerikil</trd>
  <trdgrp lng="it">
    <trd>pietruzza</trd>,
    <trd>sassolino</trd>
  </trdgrp>
  <trd lng="nl">kiezelsteen</trd>
  <trdgrp lng="pl">
    <trd>kamyk</trd>,
    <trd>kamyczek</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_sh;&c_je;&c_k;</trd>
</drv>

<drv mrk="sxton.0ejo">
  <kap><tld/>ejo, <var><kap><tld/>rompejo</kap></var>,
  <var><kap><tld/>minejo</kap></var></kap>
  <snc>
    <dif>
      Loko kie oni ekspluatas <tld/>ojn:
      <ekz>
        mi memoros viajn vortojn, dum mi rompos rokojn en
        <tld/>minejo
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro
        30a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>stone quarry</trd>,
    <trd>stone mine</trd>
  </trdgrp>
  <trd
  lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
  <trd lng="de">Steinbruch</trd>
  <trd lng="es">cantera de piedra</trd>
  <trd lng="fr">carri&egrave;re de pierre</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;t&ouml;r&odblac;</trd>,
    <trd>k&odblac;fejt&odblac;</trd>,
    <trd>k&odblac;b&aacute;nya</trd>
  </trdgrp>
  <trd lng="it">cava<klr> di pietra</klr></trd>
  <trd lng="nl">steengroeve</trd>
  <trdgrp lng="pl">
    <trd>kamienio&lstroke;om</trd>,
    <trd>wyrobisko <ind>kamieni</ind></trd>
  </trdgrp>
  <trd lng="pt">pedreira</trd>
  <trd
  lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_l;&c_o;&c_m;&c_n;&c_ja;</trd>
  <trd lng="sv">stenbrott</trd>
</drv>

<drv mrk="sxton.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="sxton.0igi.MIT">
  <uzo tip="fak">MIT</uzo>
    <dif>
      Transformi en &scirc;tonon, masive a&ubreve; supra&jcirc;e:
      <ekz>
      la sor&ccirc;istino <klr>[...]</klr> tu&scirc;is per la vergo anka&ubreve; la re&gcirc;on
      kaj &scirc;tonigis lin
      <fnt>
        <aut>Fratoj Grimm, trad. Kabe</aut>,
        <vrk>Elektitaj Fabeloj; Du Fratoj</vrk>
      </fnt>
      </ekz>.
    </dif>
  </snc>
  <snc mrk="sxton.0igi.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Haltigi, senaktivigi:
      <ekz>
        Aleksandro, &scirc;tonigita de mirego
        <fnt>
          <aut>Henri Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>&ccirc;apitro dua</lok>
        </fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="fr">p&eacute;trifier</trd>
</drv>

<drv mrk="sxton.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="sxton.0igxi.MIN">
    <dif>
      I&gcirc;i <tld/>a, simila al <tld/>o.
    </dif>
    <trdgrp lng="en">
      <trd>to become petrified</trd>,
      <trd>to petrify</trd>
    </trdgrp>
    <trd lng="de">versteinern</trd>
  <trd lng="de">erstarren</trd>
  <trd lng="es"><ind>fosilizar</ind>se</trd>
    <trdgrp lng="hu">
      <trd>elk&odblac;vesedik</trd>,
      <trd>k&odblac;v&eacute; v&aacute;lik;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skamienie&cacute;</trd>,
      <trd>obraca&cacute; si&eogonek; w
      <ind>kamie&nacute;</ind></trd>,
      <trd>zmienia&cacute; si&eogonek; w
      <ind>kamie&nacute;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="sxton.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rigidi&gcirc;i, senmovigi, i&gcirc;i neprogrespova:
      <ekz>
        &scirc;ia viza&gcirc;o <klr>...</klr> aspektis kiel
        <tld/>i&gcirc;inta
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi preska&ubreve; <tld/>i&gcirc;is &ccirc;e la ekvido de
        leono
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        vivanta lingvo ne <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        homo kun <tld/>i&gcirc;intaj ideoj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to become petrified</trd>,
      <trd>to turn to stone</trd>, <trd>to become inert</trd>
    </trdgrp>
    <trd lng="es">quedarse de piedra</trd>
    <trd lng="hu">k&odblac;v&eacute; dermed</trd>
    <trd lng="pl">skamienie&cacute;</trd>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  <trd lng="es"><ind>fosilizar</ind>se</trd>
  <trd lng="fr">se <ind>p&eacute;trifier</ind></trd>
  <trd lng="id"><ind>batu</ind> (diam)membatu</trd>
  <trdgrp lng="it">
    <trd>pietrificarsi</trd>,
    <trd>pietrificare<klr> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">verstenen</trd>
  <trd lng="ru">&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">f&ouml;rstenas</trd>
</drv>

<drv mrk="sxton.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxton.0umi.sxtonmortigi">
    <ref tip="dif" cel="mort.sxton0igi.KOMUNE"><tld/>mortigi</ref>
  </snc>
  <trdgrp lng="en">
    <trd>to stone</trd>,
    <trd>to throw stones at</trd>
  </trdgrp>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;&c_m;&c_ib;</ind>
  <klr>(&c_d;&c_a;
  &c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ib;)</klr></trd>
  <trd lng="de">steinigen</trd>
  <trd lng="es">lapidar</trd>
  <trd lng="fr">lapider</trd>
  <trd lng="hu">megk&odblac;vez</trd>
  <trd lng="it">lapidare</trd>
  <trd lng="nl">stenigen</trd>
  <trd lng="pl">kamieniowa&cacute;</trd>
  <trd lng="pt">lapidar<klr tip="ind"> (matar)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
  &c_k;&c_a;&c_m;&c_n;&c_ja;&c_m;&c_i;</trd>
  <trd lng="sv">stena<klr> (till d&ouml;ds)</klr></trd>
</drv>

<drv mrk="sxton.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="sxton.sen0igi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Senigi je <tld/>oj, ekzemple kampon por faciligi kultivon:
      <ekz>
        vinber&gcirc;ardenon havis mia amiko <klr>...</klr> li
        &ccirc;irka&ubreve;fosis &gcirc;in,
        sen<tld/>igis &gcirc;in, kaj plantis en &gcirc;i plej
        bonspecajn vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 5:1-2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">to clear of stones</trd>
  <trdgrp lng="de">
    <trd>entsteinen</trd>,
    <trd>von <ind>Steinen</ind> bereinigen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desempedrar</trd>,
    <trd>despedregar</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pierrer</trd>
  <trd lng="it">dissodare</trd>
  <trd lng="nl">van <ind>steen</ind> ontdoen</trd>
  <trdgrp lng="pl">
    <trd>okdamieniowywa&cacute;</trd>,
    <trd>odgruzowywa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.aer0o">
  <kap>aer<tld/>o</kap>
  <snc mrk="sxton.aer0o.aerolito">
    <uzo tip="fak">MET</uzo>
    <ref tip="dif" cel="aeroli.0o">aerolito</ref>
  </snc>
  <trd lng="br">aerolitenn</trd>
  <trdgrp lng="de">
    <trd>Meteorstein</trd>,
    <trd>Meteor</trd>,
    <trd>Aerolith</trd>,
    <trd>Sternschnuppe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meteor</trd>,
    <trd>aerolite</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>meteorito</trd>,
    <trd>aerolito</trd>
  </trdgrp>
  <trd lng="fr">a&eacute;rolithe</trd>
  <trd lng="it">meteorite</trd>
  <trd lng="nl">a&euml;roliet</trd>
  <trdgrp lng="pl">
    <trd>aerolit</trd>,
    <trd>meteoryt</trd>,
    <trd>meteor</trd>
  </trdgrp>
  <trd lng="pt">aer&oacute;lito</trd>
  <trd lng="sk">meteorit</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_e;&c_r;&c_o;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.filozofia0o">
  <kap>filozofia <tld/>o, <var><kap><tld/>o de
  sa&gcirc;uloj</kap></var></kap>
  <snc mrk="sxton.filozofia0o.MAG">
    <dif>
      Celo de <ref tip="vid" cel="alhxem.0o">al&hcirc;emio</ref>,
      la&ubreve;vorte
      elemento kapabla &scirc;an&gcirc;i ordinarajn metalojn al oro:
      <ekz>
        li elkore pre&gcirc;is <klr>...</klr>, ke la atendata
        miraklo estu
        nek la eltrovo de la filozofia <tld/>o, nek la liberigo de
        la
        metal-viviga blovo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">philosopher's stone</trd>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_z;&c_o;&c_f;&c_s;&c_k;&c_ib;
  <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</ind></trd>
  <trd lng="de">Stein der Weisen</trd>
  <trd lng="es">piedra filosofal</trd>
  <trd lng="fr">pierre philosophale</trd>
  <trd lng="it">pietra filosofale</trd>
  <trd lng="nl"><ind>steen</ind> der wijzen</trd>
  <trd lng="pl">kamie&nacute; filozoficzny</trd>
</drv>

<drv mrk="sxton.juvel0o">
  <kap>juvel<tld/>o</kap>
  <snc mrk="sxton.juvel0o.MIN">
    <tezrad fak="MIN"/>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta <tld/>o uzata por fari juvelojn:
      <ekz>
        mi jam a&ubreve;dis la famon de via sa&gcirc;eco, kaj se
        &gcirc;i estas vere pli
        valora ol oro kaj juvel<tld/>o, ni &gcirc;ojos pri &gcirc;i
        <fnt><bib>SatirRak</bib>, <lok>Pythagoras, 4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gem.0o">gemo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>gemstone</trd>,
    <trd>precious stone</trd>, <trd>semi-precious stone</trd>
  </trdgrp>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;
  <ind>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</ind></trd>
  <trd lng="de">Edelstein</trd>
  <trd lng="es">gema</trd>
  <trd lng="fr">gemme</trd>
  <trd lng="hu">&eacute;kk&odblac;</trd>
  <trdgrp lng="id">
    <trd>berlian</trd>,
    <trd>permata</trd>
  </trdgrp>
  <trd lng="it"><ind>pietra</ind> preziosa</trd>
  <trd lng="nl">edelsteen</trd>
  <trdgrp lng="pl">
    <trd>kamie&nacute; szlachetny</trd>,
    <trd>kamie&nacute; drogocenny</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_o;&c_c;&c_je;&c_n;&c_n;&c_y;&c_j;
  &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">&auml;delsten</trd>
</drv>

<drv mrk="sxton.lazur0o">
  <kap>lazur<tld/>o</kap>
  <snc mrk="sxton.lazur0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Speco de
      <ref tip="super" cel="sxton.juvel0o">juvel&scirc;tono</ref>
      <ref tip="vid" cel="lazur.0o">lazur</ref>kolora:
      <ekz>
        kiel lazur<tld/>o estu mia vivo altvalora en viaj okuloj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a, pluaj
        insidoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">lapis lazuli</trd>
  <trdgrp lng="de">
    <trd>Lapislazuli</trd>,
    <trd>Lasurstein</trd>
  </trdgrp>
  <trd lng="es">lapisl&aacute;zuli</trd>
  <trd lng="fr">lapis lazuli</trd>
  <trd lng="it">lapislazzuli</trd>
  <trdgrp lng="nl">
    <trd>azuursteen</trd>,
    <trd>lapis lazuli</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lazuryt</trd>,
    <trd>lapis-lazuli</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lazurita</trd>,
    <trd>l&aacute;pis-laz&uacute;li</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.mejlo0o">
  <kap>mejlo<tld/>o</kap>
  <snc mrk="sxton.mejlo0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Iu el la star&scirc;tonoj metitaj la&ubreve; mejla a&ubreve;
      alia distanco la&ubreve; landvojo.
    </dif>
    <trd lng="en">milestone</trd>
    <trdgrp lng="de">
      <trd>Meilenstein</trd>,
      <trd>Kilometermarke</trd>
    </trdgrp>
    <trdgrp lng="es">
    <trd>moj&oacute;n<klr tip="amb"> (kilom&eacute;trico)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
    <trd>borne<klr tip="amb"> (kilom&eacute;trique)</klr></trd>,
    <trd>pierre milliaire</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kamie&nacute; milowy</trd>,
      <trd>kamie&nacute; przydro&zdot;ny</trd>,
      <trd>znacznik drogowy</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxton.mejlo0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Grava atingo en ankora&ubreve; ne finita afero:
      <ekz>
        la nova ekumena pre&gcirc;- kaj kantlibro <klr>...</klr>
        estas mejlo<tld/>o survoje al la unueco de la kristanoj
        <fnt>
        <aut>U. Matthias</aut>,
        <vrk>Recenzo pri &leftquot;Adoru&rightquot;</vrk>,
        <lok>soc.culture.esperanto, 2001-07-02</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">milestone</trd>
    <trd lng="de">Meilenstein</trd>
    <trd lng="es">etapa importante</trd>
    <trd lng="fr">&eacute;tape importante</trd>
    <trd lng="pl">kamie&nacute; milowy</trd>
  </snc>
  <trd lng="it"><ind>pietra</ind> miliare</trd>
  <trd lng="nl">mijlpaal</trd>
</drv>

<drv mrk="sxton.rul0o">
  <kap>rul<tld/>o</kap>
  <snc mrk="sxton.rul0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malgranda, glata &scirc;tono, kiu tiel formi&gcirc;is pro
      longatempa ruli&gcirc;o, pelate de rivera a&ubreve; mara akvo:
      <ekz>
        rondeco kaj plateco de rul<tld/>oj indikas &ccirc;efe sedimenton de
        riveraj rubodeltoj en varma kaj sufi&ccirc;e seka klimato
        <fnt>
          <aut>J. M. Mabesoone</aut>,
          <vrk><url
          ref="http://www.repository.naturalis.nl/document/549662">
          Tertiary and Quaternary Sedimentation in a Part of the 
          Duero Basin Palencia (Spain)</url></vrk>,
          <lok>[Leidse Geologische Mededelingen, 1959 (24), p. 
          31a-180a]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>river rock</trd>,
    <trd>beach pebble</trd>, <trd>landscape stone</trd>
  </trdgrp>
  <trd lng="fr">galet<klr> (caillou)</klr></trd>
  <trd lng="pl">otoczak</trd>
</drv>

<drv mrk="sxton.sal0o">
  <kap>sal<tld/>o</kap>
  <snc mrk="sxton.sal0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Roko, el kiu oni ekstraktas salon.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>halite</trd>,
    <trd>rock salt</trd>, <trd>evaporite</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Salzstock</trd>,
    <trd>Steinsalz</trd>,
    <trd>Evaporit</trd>
  </trdgrp>
  <trd lng="es">halita</trd>
  <trd lng="fr">halite</trd>
  <trd lng="hu">k&odblac;s&oacute;</trd>
  <trd lng="nl">zoutsteen</trd>
</drv>

<drv mrk="sxton.tombo0o">
  <kap>tombo<tld/>o, <var><kap>tomb<tld/>o</kap></var></kap>
  <snc mrk="sxton.tombo0o.KOMUNE">
    <dif>
      <tld lit="&Scirc;"/>o, per kiu oni kovras a&ubreve; prisignas
      tombon:
      <ekz>
        tombo<tld/>oj kun krucoj kaj antikvaj &ccirc;izliteroj
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 3a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">tombstone</trd>
  <trd lng="de">Grabstein</trd>
  <trd lng="es">piedra del sepulcro</trd>
  <trd lng="fr">pierre tombale</trd>
  <trd lng="id"><ind>batu</ind> nisan</trd>
  <trdgrp lng="it">
    <trd>lapide</trd>,
    <trd><ind>pietra</ind> tombale</trd>
  </trdgrp>
  <trd lng="nl">grafsteen</trd>
  <trdgrp lng="pl">
    <trd>nagrobek</trd>,
    <trd>p&lstroke;yta nagrobna</trd>
  </trdgrp>
</drv>

<drv mrk="sxton.0hakisto">
  <kap><tld/>hakisto, <var><kap><tld/>ministo</kap></var></kap>
  <snc>
    <dif>
      <ref tip="super" cel="meti.0isto">Metiisto</ref>, kies laboro 
      estas tiri <tld/>ojn el <tld/>ejo:
      <ekz>
        oni dungis <tld/>hakistojn kaj &ccirc;arpentistojn, por
        renovigi
        la domon de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Kr2; 24:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">stonecutter</trd>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_ch;&c_o;&c_s;</trd>
  <trd lng="de">Steinbrecher</trd>
  <trd lng="es">cantero</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;fejt&odblac; (munk&aacute;s)</trd>,
    <trd>k&odblac;t&ouml;r&odblac;</trd>,
    <trd>k&odblac;b&aacute;ny&aacute;sz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cavapietre</trd>,
    <trd>cavatore<klr> (in cave di pietra)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>steenhakker</trd>,
    <trd>steenhouwer</trd>
  </trdgrp>
  <trd lng="pl">kamieniarz</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_o;&c_t;&c_jo;&c_s;</trd>
</drv>

<drv mrk="sxton.0trancxisto">
  <kap><tld/>tran&ccirc;isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="meti.0isto">Metiisto</ref>, kiu 
      formtran&ccirc;as <tld/>ojn por konstruo.
    </dif>
  </snc>
  <trd lng="en">stonemason</trd>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_r;&c_e;&c_z;</trd>
  <trd lng="de">Steinmetz</trd>
  <trd lng="es">picapedrero</trd>
  <trd lng="fr">tailleur de pierre</trd>
  <trd lng="hu">k&odblac;farag&oacute;</trd>
  <trdgrp lng="it">
    <trd>scalpellino</trd>,
    <trd>lapidario</trd>
  </trdgrp>
  <trd lng="nl">steenhouwer</trd>
  <trd lng="pl">kamieniarz</trd>
  <trd lng="sv">stenhuggare</trd>
</drv>

<drv mrk="sxton.gal0amalsano">
  <kap>gal<tld/>a malsano
  <fnt> <vrk> <url
  ref="http://vortareto.free.fr/dictionnaire/esp-fr/m.htm">Dictionnaire
  m&eacute;dical esp&eacute;ranto-fran&ccedil;ais</url></vrk>
  </fnt></kap>
  <snc mrk="sxton.gal0amalsano.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> ka&ubreve;zata de
      <ref cel="sxton.0o.MED"><tld/>o<sncref/></ref> en la
      <ref cel="gal.0veziko">galveziko</ref> kaj (a&ubreve;) ties
      duktoj.
    </dif>
  </snc>
  <trd lng="de">Gallensteinleiden</trd>
  <trd lng="en">cholelithiasis</trd>
  <trd lng="es">colelitiasis</trd>
  <trdgrp lng="fr">
    <trd>chol&eacute;lithiase</trd>,
    <trd>lithiase biliaire</trd>
  </trdgrp>
  <trd lng="id"><ind>batu</ind> empedu</trd>
  <trd lng="it">colelitiasi</trd>
  <trdgrp lng="nl">
    <trd>galsteenziekte</trd>,
    <trd>cholelithiasis</trd>
  </trdgrp>
  <trd lng="pl">kamica &zdot;&oacute;&lstroke;ciowa</trd>
  <trd
  lng="ru">&c_zh;&c_je;&c_l;&c_ch;&c_n;&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
  &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
</drv>

<drv mrk="sxton.saliv0amalsano">
  <kap>saliv<tld/>a malsano
  <fnt> <vrk> <url
  ref="http://vortareto.free.fr/dictionnaire/esp-fr/m.htm"> Dictionnaire
  m&eacute;dical esp&eacute;ranto-fran&ccedil;ais</url></vrk>
  </fnt></kap>
  <snc mrk="sxton.saliv0amalsano.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> ka&ubreve;zata de
      <ref cel="sxton.0o.MED"><tld/>o<sncref/></ref> en salivglando.
