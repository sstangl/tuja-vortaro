<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: peoni.xml,v 1.21 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>peoni</rad>/o
</kap>
<drv mrk="peoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="peonia.0oj"><tld/>acoj</ref>
      (<trd lng="la">Paeonia</trd>), kun belaj grandaj multpetalaj
      floroj.
    </dif>
  </snc>
  <bld lok="../bld/peoni.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="be">&c_p;&c_ib;&c_v;&c_o;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_o;&c_zh;&c_u;&c_r;</trd>
  <trd lng="de">Pfingstrose</trd>
  <trd lng="en">peony</trd>
  <trdgrp lng="hu">
    <trd>bazsar&oacute;zsa</trd>,
    <trd>pe&oacute;nia</trd>
  </trdgrp>
  <trd lng="nl">pioenroos</trd>
  <trd lng="pt">pe&ocirc;nia</trd>
  <trd lng="ru">&c_p;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">pivónia</trd>
  <trd lng="cs">pivoňka</trd>
</drv>
</art>
