<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: spont.xml,v 1.4 2015/07/04 08:37:44 revo Exp $">
    <kap>
      <rad>spont</rad>/a <fnt><bib>PGl</bib></fnt>
    </kap>

    <drv mrk="spont.0a">
      <kap><tld/>a</kap>
      <snc>
        <ref tip="dif" cel="sponta1.0a">spontanea</ref>
        <ekz>
          Son&gcirc;as pure kaj senhonte |
          pri Nialo bronzabrova |
          kiu &scirc;in prila&ubreve;dos <tld/>e, |
          kiam kuros &scirc;i renkonte |
          en la blua robo nova
          <fnt>
            <aut>John FRANCIS</aut>:
            <vrk><url
 ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr44/hebridea.html"
                >Hebridea Romanco</url></vrk>,
            <lok>La nica literatura revuo, n-ro 22 (4:4, 1959)</lok>
          </fnt>.
        </ekz>
      </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>neuvědomělý</trd>,
    <trd>samovolný</trd>,
    <trd>spontánní</trd>,
    <trd>živelný</trd>
  </trdgrp>
  <trd lng="de">spontan</trd>
  <trd lng="fr">spontan&eacute;</trd>
  <trdgrp lng="hu">
        <trd>spont&aacute;n</trd>,
        <trd>&ouml;nk&eacute;nytelen</trd>
      </trdgrp>
  <trd lng="nl">spontaan</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dobrovoľný</trd>,
    <trd>mimovoľný</trd>,
    <trd>samovoľný</trd>,
    <trd>spontánny</trd>
  </trdgrp>
</drv>
  </art>

