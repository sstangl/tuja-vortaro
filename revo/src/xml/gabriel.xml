<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gabriel.xml,v 1.12 2012/01/05 14:10:20 revo Exp $">
<kap>
  <rad>Gabriel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="gabriel.0o">
  <kap><tld/>o</kap>
  <snc mrk="gabriel.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira nomo hebredevena:
      <ekz>
        vi estas bela, kiel la an&gcirc;elo <tld/>o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_G;&c_a;&c_v;&c_r;&c_i;&c_l;</trd>,
    <trd>&c_G;&c_a;&c_b;&c_r;&c_i;&c_je;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Gabriel</trd>,
    <trd>Biel</trd>
  </trdgrp>
  <trd lng="ca">Gabriel</trd>
  <trd lng="de">Gabriel</trd>
  <trd lng="fr">Gabriel</trd>
  <trdgrp lng="hu">
    <trd>G&aacute;bor</trd>,
    <trd>G&aacute;briel</trd>
  </trdgrp>
  <trd lng="it">Gabriele</trd>
  <trd lng="nl">Gabri&euml;l</trd>
  <trd lng="pt">Gabriel</trd>
  <trdgrp lng="ru">
    <trd>&c_G;&c_a;&c_v;&c_r;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_G;&c_a;&c_v;&c_r;&c_i;&c_i;&c_l;</trd>,
    <trd>&c_G;&c_a;&c_b;&c_r;&c_i;&c_e;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">Cebrail</trd>
</drv>
</art>

