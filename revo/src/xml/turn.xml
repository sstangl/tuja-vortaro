<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: turn.xml,v 1.27 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>turn</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="turn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="turn.0i.ruli">
    <dif>
      Movi ion &ccirc;irka&ubreve; &gcirc;ia akso:
      <ekz>
        <tld/>i radon;
      </ekz>
      <ekz>
        <tld/>i &scirc;losilon en la seruro;
      </ekz>
      <ekz>
        &ccirc;e la sono de lia vo&ccirc;o Stella <tld/>is la kapon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
        glacia&jcirc;o,
        (rakonto de L. Dilling, tradukita de Eduard Hall el Joensuu
        (Finnlando))</lok></fnt>;
      </ekz>
      <ekz>
        la kamparano <klr>...</klr> <tld/>is kaj re<tld/>is la
        pa&gcirc;ojn
        <fnt><bib>ElektFab</bib>, <lok>doktoro
        &Ccirc;ioscia</lok></fnt>;
      </ekz>
      <ekz>
        la folioj en la arbaro fari&gcirc;is flavaj <klr>...</klr>, la
        vento
        ilin forportis kaj <tld/>opelis
        <fnt><bib>Fab2</bib>, <lok>malbela anasido</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kirl.0i">kirli</ref>,
      <ref cel="rul.0i.KOMUNE">ruli <sncref/></ref>
    </refgrp>
    <trd lng="de">drehen</trd>
    <trdgrp lng="hu">
      <trd>ford&iacute;t</trd>,
      <trd>forgat</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obraca&cacute;</trd>,
      <trd>kr&eogonek;ci&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>girar</trd>,
      <trd>rodar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_r;&c_t;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">vrida</trd>
  </snc>
  <snc mrk="turn.0i.alidirekti">
    <dif>
      Direkti en iun direkton:
      <ekz>
        <tld/>i posten la &ccirc;evalon<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili <tld/>is al Mi la dorson
        <klr>(signo de malestimo, rifuzo, fu&gcirc;o...)</klr>
        <fnt><bib>MT</bib>, <lok>&Jer; 32:33</lok></fnt>;
      </ekz>
      <ekz>
        re<tld/>u vin de viaj malbonaj vojoj
        <fnt><bib>MT</bib>, <lok>&Re2; 17:13</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la okulojn al la pordo;
      </ekz>
      <ekz>
        <tld/>i sian rigardon, atenton al io;
      </ekz>
      <ekz>
        <tld/>i &ccirc;iujn siajn fortojn, klopodojn al iu celo;
      </ekz>
      <ekz>
        la randoj de la &ccirc;inaj tegmentoj estas <tld/>itaj supren.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="adres.0i">adresi</ref>,
      <ref cel="aplik.0i">apliki</ref>,
      <ref cel="cel.0i">celi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zuwenden</trd>,
      <trd>richten</trd>,
      <trd>lenken</trd>
    </trdgrp>
    <trd lng="en">turn</trd>
    <trdgrp lng="hu">
      <trd>elford&iacute;t</trd>,
      <trd>ir&aacute;ny&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obraca&cacute;</trd>,
      <trd>zakr&eogonek;ca&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">v&auml;nda</trd>
  </snc>
  <snc mrk="turn.0i.sxangxi">
    <dif>
      Meti en novan staton, <ref tip="sin"
      cel="sxangx.0i.anstatauxigi">&scirc;an&gcirc;i</ref>:
      <ekz>
        la edzi&gcirc;o de la princo devis ja alporti al &scirc;i la
        morton kaj <tld/>i
        &scirc;in en &scirc;a&ubreve;mon sur la maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="form.ali0i.KOMUNE">aliformi</ref>,
      <ref cel="metamo1.0i">metamorfozi</ref>
    </refgrp>
    <trd lng="hu">v&aacute;ltoztat <klr
    tip="amb">(vmiv&eacute;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">f&ouml;rvandla</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obr&aacute;tit</trd>,
    <trd>obracet</trd>,
    <trd>stočit</trd>,
    <trd>točit</trd>,
    <trd>vrtět</trd>
  </trdgrp>
  <trd lng="de">verwandeln</trd>
  <trd lng="en">turn</trd>
  <trd lng="es">girar</trd>
  <trd lng="fr">tourner</trd>
  <trd lng="nl">draaien</trd>
  <trdgrp lng="pl">
    <trd>obraca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>virar</trd>,
   <trd>voltar</trd>,
   <trd>volver</trd>,
   <trd>rodar</trd>,
   <trd>voltear</trd>,
   <trd>transformar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozkrútiť</trd>,
    <trd>roztočiť</trd>,
    <trd>zvrtnúť</trd>
  </trdgrp>
</drv>

<drv mrk="turn.0igxisin0i">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc mrk="turn.0igxi.ruligxi">
    <dif>
      Movi&gcirc;i &ccirc;irka&ubreve; sia akso:
      <ekz>
        la rado <tld/>i&gcirc;is rapide;
      </ekz>
      <ekz>
        pordo <tld/>i&gcirc;as sur sia hoko
        <fnt><bib>MT</bib>, <lok>&Sen; 26:14</lok></fnt>;
      </ekz>
      <ekz>
        la tero <tld/>i&gcirc;as &ccirc;irka&ubreve; sia akso kaj
        rondiras &ccirc;irka&ubreve; la
        suno<fnt>B</fnt>;
      </ekz>
      <ekz>
        junaj fra&ubreve;linoj <tld/>i&gcirc;adis en gaja
        danco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i&gcirc;adi kiel serpento</ind>
        <klr>(rapide &ccirc;iuflanken tordi sian korpon)</klr>
        <trd lng="fr">se tordre comme un vers</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la ne&gcirc;o <tld/>i&gcirc;adis
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino, fabelo en
        sep rakontoj,
        dua rakonto, malgranda knabo kaj malgranda knabino</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o sin <tld/>is kaj <tld/>adis anta&ubreve; la
        spegulo
        <fnt><bib>Fab1</bib>, <lok>la novaj vestoj de la
        re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        li sin <tld/>is sur la kalkano
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tord.0i">tordi</ref>
    </refgrp>
    <trd lng="de"><klr>sich </klr>umdrehen</trd>
    <trdgrp lng="hu">
      <trd>forog</trd>,
      <trd>p&ouml;r&ouml;g</trd>,
      <trd>kering</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obraca&cacute; si&eogonek;</trd>,
      <trd>kr&eogonek;ci&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>rotera</trd>,
      <trd>vridas</trd>
    </trdgrp>
  </snc>
  <snc mrk="turn.0igxi.alidirektigxi">
    <dif>
      &Scirc;an&gcirc;i sian direkton:
      <ekz>
        tie la karavano sin <tld/>is al la sudo;
      </ekz>
      <ekz>
        sin <tld/>i kontra&ubreve; la malamikon;
      </ekz>
      <ekz>
        la fortuno povas <klr>...</klr> forlasi nin, e&ccirc; sin
        <tld/>i
        kontra&ubreve; ni
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kien ajn li sin <tld/>os, li sukcesos<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sin <tld/>i al iu kun peto por helpo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por &ccirc;iuj informoj <tld/>u vin al Sinjoro Ajnulo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne <tld/>u vin al idoloj
        <fnt><bib>MT</bib>, <lok>&Lev; 19:4</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia vo&ccirc;o sin <tld/>as al la homoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia atento balda&ubreve; sin <tld/>is al ni;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kontra&ubreve; min mem sin <tld/>is mia ruzo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.de0igxi">deklini&gcirc;i</ref>,
      <ref cel="konver3.0igxi.REL">konverti&gcirc;i</ref>
    </refgrp>
    <trd lng="de">umkehren</trd>
    <trdgrp lng="hu">
      <trd>fordul</trd>,
      <trd>ir&aacute;nyul</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obraca&cacute; si&eogonek;</trd>,
      <trd>zwraca&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>v&auml;ndas</trd>,
      <trd>v&auml;nda<klr> sig</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="turn.0igxi.sxangxigxi">
    <dif>
      I&gcirc;i en novan staton:
      <ekz>
        lia amo <tld/>i&gcirc;is en malamon;
      </ekz>
      <ekz>
        li mortis kaj <tld/>i&gcirc;is en polvon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">v&aacute;ltozik <klr
    tip="amb">(vmiv&eacute;)</klr></trd>
    <trdgrp lng="pl">
      <trd>obraca&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
    <trd lng="sv">f&ouml;rvandlas</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obracet</trd>,
    <trd>stočit</trd>,
    <trd>točit</trd>,
    <trd>vrtět</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>retourner</ind></trd>,
    <trd>se <ind>tourner</ind></trd>,
    <trd>tourner en</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozkrútiť</trd>,
    <trd>roztočiť</trd>,
    <trd>zvrtnúť</trd>
  </trdgrp>
</drv>

<drv mrk="turn.de0ifor0i">
  <kap>de<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;an&gcirc;i la kutiman a&ubreve; normalan direkton de:
      <ekz>
        de<tld/>i la fluon de rivero;
      </ekz>
      <ekz>
        for<tld/>i la kapon;
      </ekz>
      <ekz>
        de<tld/>i iun de la vojo;
      </ekz>
      <ekz>
        timu la Eternulon, kaj de<tld/>u vin de malbono
        <fnt><bib>MT</bib>, <lok>&Sen; 3:7</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admon">deadmoni</ref>,
      <ref cel="klin.de0i">deklini</ref>,
      <ref cel="log">delogi</ref>,
      <ref cel="voj">devojigi</ref>,
      <ref cel="rel">elreligi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>odklonit</trd>,
    <trd>odvracet</trd>,
    <trd>odvrátit</trd>
  </trdgrp>
  <trd lng="de"><klr>sich </klr>abwenden</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner</trd>
  </trdgrp>
  <trd lng="hu">elford&iacute;t</trd>
  <trd lng="nl">ronddraaien</trd>
  <trdgrp lng="pl">
    <trd>odwraca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_o;&c_r;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odbočiť</trd>,
    <trd>odkloniť sa</trd>
  </trdgrp>
  <trd lng="sv">v&auml;nda bort</trd>
</drv>

<drv mrk="turn.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc mrk="turn.el0igxi.FIG">
    <dif>
      Eliri el malfacila situacio per sia lerto.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd><ind>arreglar</ind>-s'ho</trd>,
    <trd><ind>arreglar</ind>-se</trd>
  </trdgrp>
  <trd lng="de"><klr>sich </klr>herauswinden</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;brouiller</ind></trd>,
    <trd>se <ind>tirer d'affaire</ind></trd>
  </trdgrp>
  <trd lng="hu">kiv&aacute;gja mag&aacute;t</trd>
  <trd lng="nl">zich eruit <ind>redden</ind></trd>
  <trd lng="oc">s'<ind>adobar</ind></trd>
  <trdgrp lng="pl">
    <trd>wykr&eogonek;ci&cacute; si&eogonek;</trd>,
    <trd>wykaraska&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>vrida sig ur</trd>,
    <trd>ta sig ur</trd>
  </trdgrp>
</drv>

<drv mrk="turn.el0ajxo">
  <kap>el<tld/>a&jcirc;o</kap>
  <snc mrk="turn.el0ajxo.FIG">
    <dif>
      El<tld/>i&gcirc;a rimedo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>exp&eacute;dient</trd>,
    <trd>syst&egrave;me D</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiseg&iacute;t&odblac; megold&aacute;s</trd>,
    <trd>&aacute;tmeneti megold&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">redmiddel</trd>
  <trdgrp lng="pl">
    <trd>wyj&sacute;cie z sytuacji</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">flyktmedel</trd>
</drv>

<drv mrk="turn.re0e">
  <kap>re<tld/>e</kap>
  <snc mrk="turn.re0e.retro">
    <dif>
      La&ubreve; kontra&ubreve;a direkto; malanta&ubreve;en irante;
      revenante al la deira
      punkto:
      <ekz>
        mi iros re<tld/>e al mia mizero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li prenis la galo&scirc;ojn re<tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ricevas re<tld/>e nian homan formon
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        rigardi re<tld/>en;
      </ekz>
      <ekz>
        flamo movata de vento tien kaj re<tld/>en<fnt>B</fnt>;
      </ekz>
      <ekz>
        meti ion re<tld/>e en &gcirc;ian lokon.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>vissza</trd>,
      <trd>visszafel&eacute;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odwrotnie</trd>,
      <trd>wstecz</trd>,
      <trd>z powrotem</trd>,
      <trd>nazad<klr> (pot. rzadkie)</klr></trd>,
      <trd>w ty&lstroke;</trd>
    </trdgrp>
  </snc>
  <snc mrk="turn.re0e.repage">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kompense:
      <ekz>
        doni <tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>w zamian</trd>,
      <trd>zwrotnie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>nazpátek</trd>,
    <trd>nazpět <klr>(adv.)</klr></trd>,
    <trd>zpátky</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en <ind>retour</ind></trd>
  </trdgrp>
  <trd lng="hu">viszonz&aacute;sul</trd>
  <trd lng="nl">terug</trd>
  <trd lng="ru">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trd lng="sk">späť</trd>
  <trd lng="sv">tillbaka</trd>
</drv>

<drv mrk="turn.re0ite">
  <kap>re<tld/>ite</kap>
  <snc>
    <dif>
      La&ubreve; inversa maniero.
    </dif>
  </snc>
  <trd lng="hu">ford&iacute;tva</trd>
  <trd lng="nl">omgedraaid</trd>
  <trdgrp lng="pl">
    <trd>odwrotnie</trd>,
    <trd>ty&lstroke; na prz&oacute;d<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_o;&c_m;
  &c_n;&c_a;&c_p;&c_je;&c_r;&c_jo;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>naopak</trd>,
    <trd>obrátene</trd>,
    <trd>opačne</trd>
  </trdgrp>
</drv>

<drv mrk="turn.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="turn.akvo0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Loko de akvejo, kie akvo <tld/>i&gcirc;as kun ensu&ccirc;a forto:
      <ekz>
        ili haltigas a&ubreve; turnas la &scirc;ipon, por eviti
        dan&gcirc;erajn akvo<tld/>ojn
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 12a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="vortic.0o">vortico</ref>
    <ref tip="sin" cel="kirl.akvo0o.MAR">akvokirlo</ref>
    <trd lng="fr">tourbillon<klr tip="amb"> (d'eau)</klr></trd>
    <trd lng="pl">wir wodny</trd>
    <trd lng="sv">str&ouml;mvirvel</trd>
  </snc>
</drv>

<drv mrk="turn.kap0o">
  <kap>kap<tld/>o</kap>
  <snc mrk="turn.kap0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sento pri nefirmeco en la spaco, pri manko de sampezo, pro kiu
      &scirc;ajnas turni&gcirc;i la objektoj &ccirc;irka&ubreve;e:
      <ekz>
        havi kap<tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sinjorino Anneto sentis kap<tld/>on de ravo
        <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
        Devjatnin), 4</lok></fnt>;
      </ekz>
      <ekz>
        en kap<tld/>a alto
        <fnt><bib>Fab3</bib>, <lok>sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis belulo kaj <tld/>is la kapojn al la virinoj
        <klr>(malsa&gcirc;igis ilin)</klr>
        <fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">otočení hlavy</trd>
  <trd lng="de">Schwindel</trd>
  <trd lng="en">vertigo</trd>
  <trd lng="fr">vertige</trd>
  <trd lng="hu">sz&eacute;d&uuml;l&eacute;s</trd>
  <trd lng="nl">duizeling</trd>
  <trd lng="pl">zawr&oacute;t g&lstroke;owy</trd>
  <trd
  lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_k;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>točenie hlavy</trd>,
    <trd>závrat</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>svindel</trd>,
    <trd>yrsel</trd>
  </trdgrp>
</drv>

<drv mrk="turn.kap0igxo">
  <kap>kap<tld/>i&gcirc;o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turn.kap0igxo.kapturno">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="turn.kap0o.MED">kap<tld/>o</ref>
    <ekz>
      li ricevis kap<tld/>i&gcirc;on
      <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 2, kio
      okazis al la
      jura konsilisto</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Schwindel</trd>
  <trd lng="en">vertigo</trd>
  <trd lng="fr">vertige</trd>
  <trd lng="hu">sz&eacute;d&uuml;l&eacute;s</trd>
  <trd lng="nl">duizeling</trd>
  <trd lng="pl">zawr&oacute;t g&lstroke;owy</trd>
  <trd
  lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_k;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sv">
    <trd>svindel</trd>,
    <trd>yrsel</trd>
  </trdgrp>
</drv>

<drv mrk="turn.parol0o">
  <kap>parol<tld/>o</kap>
  <snc mrk="turn.parol0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Dirmaniero:
      <ekz>
        fre&scirc;aj, surprizaj parol<tld/>oj
        <fnt><bib>Plumamikoj</bib>, <lok>dramoj en
        esperanto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">slovní obrat</trd>
  <trd lng="de">Redewendung</trd>
  <trdgrp lng="fr">
    <trd>expression</trd>,
    <trd>tournure<klr> (de phrase)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&oacute;fordulat</trd>,
    <trd>mond&aacute;s</trd>,
    <trd>sz&oacute;l&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">zegswijze</trd>
  <trdgrp lng="pl">
    <trd>zwrot<klr> j&eogonek;zykowy</klr></trd>,
    <trd>powiedzenie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;
  &c_r;&c_je;&c_ch;&c_i;</trd>
  <trd lng="sk">slovný zvrat</trd>
</drv>
</art>
