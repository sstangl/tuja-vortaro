<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ole.xml,v 1.39 2015/10/21 16:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ole</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ole.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ole.0o.KOMUNE">
    <dif>
      Grasa flua&jcirc;o el besta (baleno, foko), vegeta&jcirc;a (lino,
      nukso) a&ubreve;
      &scirc;tona (nafto, skisto) origino, uzata por nutrado, medicino,
      lumigado, arto, industrio...:
      <ekz>
        oliva, nuksa <tld/>o;
      </ekz>
      <ekz>
        sanktaj <tld/>oj
        <klr>(uzataj por sakramentoj)</klr>;
      </ekz>
      <ekz>
        glata kiel <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>enver&scirc;i <tld/>on en la fajron</ind>
        <trd lng="fr">verser de l'huile sur le feu</trd>
        <klr>(inciti ardon de disputantoj)</klr>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>havaj, <tld/>donaj vegeta&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <rim mrk="ole.0o.RIM_HOM">
    <ref tip="hom" cel="ole1.0o">Oleo</ref> anka&ubreve; estas
    nomo de arbo-genro.
  </rim>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_je;&c_j;</trd>,
    <trd>&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_je;&c_ch;&c_n;&c_o; <ind>&c_m;&c_a;&c_s;&c_l;&c_o;</ind></trd>
  <trd lng="br">eoul</trd>
  <trd lng="de">&Ouml;l</trd>
  <trd lng="en">oil</trd>
  <trd lng="es">aceite</trd>
  <trd lng="fr">huile</trd>
  <trd lng="hu">olaj</trd>
  <trd lng="id">minyak</trd>
  <trd lng="nl">olie</trd>
  <trd lng="pl">olej</trd>
  <trd lng="pt">&oacute;leo</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_o;<klr>
  (&c_zh;&c_i;&c_d;&c_k;&c_o;&c_je;: &c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;, &c_m;&c_i;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sv">olja</trd>
  <trdgrp lng="sk">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
</drv>

<drv mrk="ole.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Enhavanta <tld/>on:
      <ekz>
        <tld/>a pentrado;
      </ekz>
      <ekz>
        <tld/>aj haroj, ha&ubreve;to.
      </ekz>
    </dif>
    <trd lng="br">eoulek</trd>
    <trd lng="de">&ouml;lhaltig</trd>
    <trdgrp lng="en">
      <trd>oily</trd>,
      <trd>oil</trd>, <trd>greasy</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>olaj-</trd>,
      <trd>olajos</trd>
    </trdgrp>
    <trd lng="nl">olie-</trd>
    <trdgrp lng="pl">
    <trd>olejny</trd>,
    <trd>oleisty</trd>,
    <trd>t&lstroke;usty</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta la naturon de <tld/>o:
      <ekz>
        <tld/>a karaktero<fnt>Z</fnt>
        <klr>(cedema.
        <refgrp tip="vid">
          <ref cel="or.0o">oro</ref>,
          <ref cel="vaks.0o">vakso</ref>
        </refgrp>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">&ouml;lig</trd>
    <trd lng="nl">olieachtig</trd>
    <trd lng="pl">mi&eogonek;kki</trd>
    <trd lng="pt">oleoso</trd>
    <trd
    lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_a;&c_s;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_p;&c_je;&c_t;&c_r;&c_o;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_f;&c_t;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>olejnat&yacute;</trd>,
    <trd>olejov&yacute;</trd>
  </trdgrp>
  <trd lng="fr">huileux</trd>
  <trd lng="id">[<ind>minyak</ind>] berminyak</trd>
  <trdgrp lng="sk">
    <trd>olejnat&yacute;</trd>,
    <trd>olejov&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ole.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ole.0i.TEK">
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        <tld/>i ma&scirc;inon, &scirc;losilon, la hokojn de la
        pordo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ita papero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_je;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_l;&c_je;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_m;&c_a;&c_z;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_m;&c_a;&c_s;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_i;&c_m;&c_p;&c_r;&c_je;&c_g;&c_n;&c_i;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">eoulia&ntilde;</trd>
  <trd lng="cs">naolejovat</trd>
  <trdgrp lng="de">
    <trd>&ouml;len</trd>,
    <trd>ein&ouml;len</trd>
  </trdgrp>
  <trd lng="en">to oil</trd>
  <trd lng="es">aceitar</trd>
  <trd lng="fr">huiler</trd>
  <trdgrp lng="hu">
    <trd>beolajoz</trd>,
    <trd>megolajoz</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>lumas</ind>] melumasi</trd>,
    <trd>[<ind>minyak</ind>] meminyaki</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>smeren</trd>,
    <trd>inoli&euml;n</trd>,
    <trd>oli&euml;n</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>smarowa&cacute; <ind>olejem</ind></trd>,
    <trd>olei&cacute;</trd>
  </trdgrp>
  <trd lng="pt">olear</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
    &c_m;&c_a;&c_s;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_s;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">naolejova&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>sm&ouml;rja</trd>,
    <trd>olja in</trd>
  </trdgrp>
</drv>

<drv mrk="ole.gas0o">
  <kap>gas<tld/>o, <var><kap>dizel<tld/>o</kap></var></kap>
  <snc mrk="ole.gas0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Naftoderiva&jcirc;o ta&ubreve;ga kiel brula&jcirc;o por
      <ref tip="vid" cel="dizel.0o">dizeloj</ref>;
      <ref tip="sin" cel="gasojl.0o.KEM">gasojlo</ref>:
      <ekz>
        evidenti&gcirc;is, ke estas akvo anka&ubreve; en la gas<tld/>o
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomobiloj en &Ccirc;inio &ccirc;iujare konsumas <klr>...</klr>
        20% de dizel<tld/>o de la lando
        <fnt>
          <aut>Zuo Yan</aut>,
          <vrk><url
          ref="http://esperanto.china.org.cn/ekonomio/2007-06/13/content_8380897.htm">
          Uzado de novaj energioj por a&ubreve;tomobiloj en &Ccirc;inio</url></vrk>,
          <lok>2007-06-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">diesel fuel</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_j;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_y;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gazolin</trd>,
    <trd>petrolether</trd>,
    <trd>plynov&yacute; olej</trd>
  </trdgrp>
  <trd lng="de">Diesel</trd>
  <trdgrp lng="fr">
    <trd>diesel</trd>,
    <trd>gazole</trd>
  </trdgrp>
  <trd lng="id">diesel</trd>
  <trdgrp lng="pl">
    <trd>olej nap&eogonek;dowy</trd>,
    <trd>diesel <klr>(pot.)</klr></trd>,
    <trd>ropa <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">&oacute;leo diesel</trd>
  <trd lng="sk">plynov&yacute; olej</trd>
</drv>

<drv mrk="ole.oliv0o">
  <kap>oliv<tld/>o</kap>
  <snc mrk="ole.oliv0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="ole.0o.KOMUNE">Oleo</ref> el  
      <ref tip="vid" cel="oliv.0o">olivoj</ref>, uzata por kuiro kaj
      preparo de man&gcirc;a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_j; &c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;&c_v;&c_y;</trd>
  <trd lng="bg">&c_z;&c_je;&c_h;&c_t;&c_i;&c_n;</trd>
  <trd lng="cs">olivov&yacute; olej</trd>
  <trd lng="de">Oliven&ouml;l</trd>
  <trd lng="en">olive oil</trd>
  <trd lng="fr">huile d'olive</trd>
  <trd lng="id"><ind>minyak</ind> zaitun</trd>
  <trd lng="pl">oliwa z oliwek</trd>
  <trd lng="sk">olivov&yacute; olej</trd>
</drv>

<drv mrk="ole.palm0o">
  <kap>palm<tld/>o</kap>
  <snc mrk="ole.palm0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="ole.0o.KOMUNE">Oleo</ref> el la fruktoj de  
      olepalmo (Elaeis guineensis), uzata por kuiro, en sapo, en 
      me&hcirc;anika&jcirc;oj...:
      <ekz>
        tian damnan laboron mi havas, sinjoro: fari, kvaza&ubreve; mi
        a&ccirc;etas palm<tld/>on, kaj dume ser&ccirc;i novajn trovejojn de
        perlokonkoj
        <fnt><bib>Salamandroj</bib>, <lok>libro 1a, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_l;&c_je;&c_j; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_v;&c_y;</trd>
    <trd lng="bg">&c_p;&c_a;&c_l;&c_m;&c_o;&c_v;&c_o; &c_m;&c_a;&c_s;&c_l;&c_o;</trd>
    <trd lng="de">Palm&ouml;l</trd>
    <trd lng="en">palm oil</trd>
    <trd lng="fr">huile de palme</trd>
    <trd lng="id"><ind>minyak</ind> kelapa (sawit)</trd>
    <trd lng="pl">olej palmowy</trd>
    <trd lng="pt">azeite de dend&ecirc;</trd>  </snc>
  <trd lng="cs">palmov&yacute; olej</trd>
  <trd lng="sk">palmov&yacute; olej</trd>
</drv>

<drv mrk="ole.sankt0i">
  <kap>sankt<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ole.sankt0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Konsekri, &scirc;mirante per <tld/>o sanktigita:
      <ekz>
        ili sankt<tld/>is Davidon re&gcirc;o super Izrael
        <fnt><bib>MT</bib>, <lok>&Sa2; 5:3</lok></fnt>;
      </ekz>
      <ekz>
        la Sankt<tld/>ito
        <klr>(<ref tip="vid" cel="mesi.0o.REL">mesio</ref>)</klr>;
      </ekz>
      <ekz>
        la lasta sankt<tld/>ado
        <klr>(unu el la katolikaj sakramentoj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">to anoint</trd>                  
  <trd lng="br">olevi&ntilde;</trd>
  <trd lng="de">salben</trd>
  <trd lng="es">dar los santos <ind>&oacute;leos</ind></trd>
  <trdgrp lng="fr">
    <trd>consacrer<klr> (par onction)</klr></trd>,
    <trd>oindre</trd>
  </trdgrp>
  <trd lng="hu">felken <klr>(szentelt olajjal</klr></trd>
  <trd lng="nl">zalven</trd>
  <trd lng="pl">namaszcza&cacute;<klr> olejkiem</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_a;&c_t;&c_mol; &c_o;&c_b;&c_r;&c_ja;&c_d;
  <ind>&c_p;&c_o;&c_m;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
</drv>

<drv mrk="ole.sxton0o">
  <kap>&scirc;ton<tld/>o</kap>
  <snc mrk="ole.sxton0o.MIN">
  <uzo tip="fak">MIN</uzo>
  <ref tip="dif" cel="naft.0o">nafto</ref>
  </snc>
  <trd lng="en">crude oil</trd>
  <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_je;&c_t;&c_r;&c_o;&c_l;</trd>,
    <trd>&c_n;&c_je;&c_f;&c_t;</trd>
  </trdgrp>
  <trd lng="br">tireoul</trd>
  <trd lng="de">Erd&ouml;l</trd>
  <trd lng="es">petr&oacute;leo</trd>
  <trd lng="fr">p&eacute;trole<klr> (brut)</klr></trd>
  <trdgrp lng="id">
    <trd><ind>minyak</ind> bumi</trd>,
    <trd>petroleum</trd>
  </trdgrp>
  <trd lng="pl">ropa naftowa</trd>
  <trd lng="pt">&oacute;leo (petr&oacute;leo)</trd>
</drv>
</art>
