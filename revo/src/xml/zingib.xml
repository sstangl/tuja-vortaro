<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: zingib.xml,v 1.29 2015/07/04 08:37:46 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>zingibr</rad>/o
</kap>
<drv mrk="zingib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio 
<ref tip="malprt" cel="zingib1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Zingiber</trd>);
      plurjara <ref tip="lst" cel="herb.0o.BOT" 
      lst="voko:zingibroparencoj">herbo</ref> kun disbran&ccirc;i&gcirc;anta aroma
      rizomo 
      kun tuberoj; kun du vicoj de pli malpli lancetformaj folioj &ccirc;e
      senfloraj 
      longaj tigoj kaj konusformaj floraroj sur mallongaj tigoj a&ubreve; rekte
      sur la 
      rizomo; la tegole aran&gcirc;itaj brakteoj portas po unu triloban
      nesimetrian 
      ordinare blankecan floron;
      la rizomo de la specio <trd lng="la">Zingiber officinale</trd>
       estas uzata kiel <ref tip="lst" cel="spic.0o"
      lst="voko:spicoj">spico</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_b;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_d;&c_zh;&c_i;&c_n;&c_d;&c_zh;&c_i;&c_f;&c_i;&c_l;</trd>
  <trd lng="ca">gingebre</trd>
  <trdgrp lng="cs">
    <trd>zázvor</trd>,
    <trd>zázvorovník</trd>
  </trdgrp>
  <trd lng="de">Ingwer</trd>
  <trd lng="el">
&pi;&jota;&pi;&epsilon;&rho;&omikron_ton;&rho;&rho;&jota;&zeta;&alfa;</trd>
  <trd lng="en">ginger</trd>
  <trd lng="es">jengibre</trd>
  <trd lng="fr">gingembre</trd>
  <trd lng="he">&zayin;&nun;&gimel;&bet;&yod;&lamed;</trd>
  <trd lng="hu">gy&ouml;mb&eacute;r</trd>
  <trd lng="id">jahe</trd>
  <trdgrp lng="it">
    <trd>zenzero</trd>,
    <trd>ginger</trd>
  </trdgrp>
  <trd lng="nl">gember</trd>
  <trd lng="pt">gengibre</trd>
  <trd lng="ru">&c_i;&c_m;&c_b;&c_i;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zázvor</trd>,
    <trd>ďumbier</trd>
  </trdgrp>
  <trd lng="sv">ingef&auml;ra</trd>
</drv>
</art>
