<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pigme.xml,v 1.17 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pigme</rad>/o
</kap>
<drv mrk="pigme.0o">
  <kap><tld/>o</kap>
  <snc mrk="pigme.0o.antropologio">
    <dif>
      Ano de iu el diversaj etstaturaj popoloj vivantaj precipe en
      tropika Afriko:
      <ekz>
        e&ccirc; kompleta vila&gcirc;o kun &ccirc;. 300 konganoj (inter
        kiuj anka&ubreve; <tld/>oj) estis en la ekspozicio
        <fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Anta&ubreve; cent jaroj</vrk>, <bib>Monato</bib>
        <lok>jaro 1998a, numero 5a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>pygm&eacute;e</trd>
    </trdgrp>
    <trd lng="hu">pigmeus</trd>
    <trd lng="pt">pigmeu</trd>
  </snc>
  <snc mrk="pigme.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Ano de mita popolo rimarkinda pro &gcirc;ia malgranda kresko.
    </dif>
    <trdgrp lng="fr">
      <trd>pygm&eacute;e</trd>
    </trdgrp>
    <trd lng="hu">pigmeus</trd>
  </snc>
  <snc mrk="pigme.0o.nano">
    <ref tip="dif" cel="grand.mal0egulo">malgrandegulo</ref>
    <trdgrp lng="fr">
      <trd>nain</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rpe</trd>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_g;&c_m;&c_e;&c_j;</trd>
  <trd lng="bg">&c_p;&c_i;&c_g;&c_m;&c_je;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>pygmej</trd>,
    <trd>pygmeus</trd>,
    <trd>trpaslík</trd>
  </trdgrp>
  <trd lng="de">Pygm&auml;e</trd>
  <trd lng="en">pygmy</trd>
  <trd lng="fa">&f_p;&f_y;&f_g;&a_m;&a_h;</trd>
  <trd lng="nl">pygmee</trd>
  <trd lng="pl">pigmej</trd>
  <trd lng="ru">&c_p;&c_i;&c_g;&c_m;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>piadimužík</trd>,
    <trd>pygmej</trd>,
    <trd>trpaslík</trd>
  </trdgrp>
</drv>
</art>
