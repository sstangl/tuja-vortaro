<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: protek.xml,v 1.24 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>protekt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="protek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="protek.0i.patroni">
    <dif>
      Helpi iun per sia influo kaj a&ubreve;toritato:
      <ekz>
        kiun re&gcirc;o <tld/>as, tiun ministro elektas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ni ne estas tiaj homoj, kiuj <tld/>us sian filinon en
        malbonaj faroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i iun al ofico<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="patron.0i">patroni</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;mogat</trd>,
      <trd>seg&iacute;t</trd>,
      <trd>protezs&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="protek.0i.sxirmi">
    <dif>
      Defendi, gardi kontra&ubreve; minaco:
      <ekz>
        unu malamiko pli difektas ol cent amikoj <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        estu mia genio <tld/>anta kontra&ubreve; tiu &ccirc;i fremdulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i tiajn trezorojn kontra&ubreve; la detruo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxirm.0i.protekti">&scirc;irmi <sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="defend.0i">defendi</ref>,
      <ref cel="flug.0ilo">flugilo</ref>,
      <ref cel="rempar.0o">remparo</ref>,
      <ref cel="sxild.0o">&scirc;ildo</ref>,
      <ref cel="ten">teni sub</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_shch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>chránit</trd>,
    <trd>protěžovat</trd>,
    <trd>zastávat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sch&uuml;tzen</trd>,
    <trd>besch&uuml;tzen</trd>
  </trdgrp>
  <trd lng="en">protect</trd>
  <trd lng="es">proteger</trd>
  <trd lng="fr">prot&eacute;ger</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;d</trd>,
    <trd>&oacute;v</trd>
  </trdgrp>
  <trd lng="nl">beschermen</trd>
  <trdgrp lng="pt">
    <trd>proteger</trd>,
    <trd>patrocinar</trd>,
    <trd>salvaguardar</trd>,
    <trd>apadrinhar</trd>    
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chrániť</trd>,
    <trd>protežovať</trd>,
    <trd>zastať</trd>
  </trdgrp>
</drv>

<drv mrk="protek.0a">
  <kap><tld/>a</kap>
  <snc mrk="protek.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, helpas kontra&ubreve; dan&gcirc;eroj kaj minacoj:
      <ekz>
        <klr>&scirc;i</klr> diris kun <tld/>a tono
        <fnt><bib>Fab2</bib>, <lok>malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        kun afekte <tld/>ema sintenado<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
  <trd>sch&uuml;tzend</trd>,
  <trd>Schutz-</trd>
  </trdgrp>
  <trd lng="es">protector</trd>
  <trd lng="fr">protecteur<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="nl">beschermend</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="protek.0o">
  <kap><tld/>o</kap>
  <snc mrk="protek.0o.KOMUNE">
    <dif>
      Subteno, helpo kontra&ubreve; dan&gcirc;eroj kaj eblaj misoj:
      <ekz>
        Al <tld/>o kaj forto helpas la sorto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la Eternulo, Dio de Izrael, sub kies flugiloj vi ser&ccirc;is
        <tld/>on
        <fnt><bib>MT</bib>, <lok>&Rut; 2:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hájení</trd>,
    <trd>ochrana</trd>
  </trdgrp>
  <trd lng="de">Schutz</trd>
  <trd lng="es">protecci&oacute;n</trd>
  <trd lng="fr">protection</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;mogat&aacute;s</trd>,
    <trd>v&eacute;delem</trd>
  </trdgrp>
  <trd lng="nl">bescherming</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochrana</trd>,
    <trd>protekcia</trd>,
    <trd>záštita</trd>
  </trdgrp>
</drv>

<drv mrk="protek.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="protek.0ado.KOMUNE">
    <dif>
      Da&ubreve;ra <tld/>o, helpado:
      <ekz>
        ne rifuzu al ni vian <tld/>adon
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 7a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>ado al la malfortuloj estas la devo de &ccirc;iu vera homo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hájení</trd>,
    <trd>ochrana</trd>
  </trdgrp>
  <trd lng="de">Schutz</trd>
  <trd lng="es">protecci&oacute;n</trd>
  <trd lng="fr">protection</trd>
  <trd lng="nl">bescherming</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_shch;&c_i;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochrana</trd>,
    <trd>protekcia</trd>,
    <trd>záštita</trd>
  </trdgrp>
</drv>

<drv mrk="protek.alta0ado">
  <kap>alta <tld/>ado</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="protek.alta0ado.POL">
    <dif>
      Aprobo, apogo, subteno, beno kiun a&ubreve;toritata persono,
      funkciulo a&ubreve; instanco donas al aran&gcirc;o:
      <ekz>
        la konferenco malfermi&gcirc;is sub la Alta <tld lit="P"/>ado de
        Unesko;
      </ekz>
      <ekz>
        mi esperas, ke mi esprimos la deziron de &ccirc;iuj
        kongresanoj, se mi proponos, ke ni sendu telegrafe
        nian diversgentan saluton al la regnestro de la lando,
        kies gastoj ni estas, kaj nian dankon al nia alta
        <tld/>anto
        <fnt>
        <aut>&Z;</aut>,
        <vrk>La parolado anta&ubreve; la Sepa Kongreso Esperantista</vrk>,
        <lok>Antverpeno, la 21an de a&ubreve;gusto 1911</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="egid.0o.protekto">egido <sncref/></ref>
  </snc>
  <trd lng="cs">vysoká ochrana</trd>
  <trd lng="de">Schirmherrschaft</trd>
  <trd lng="fr">&eacute;gide</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;dn&ouml;ks&eacute;g</trd>,
    <trd>&eacute;gisz</trd>
  </trdgrp>
  <trd lng="nl">hoge <ind>bescherming</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_e;&c_g;&c_i;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">vysoká ochrana</trd>
</drv>

<drv mrk="protek.alta0anto">
  <kap>alta <tld/>anto</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="protek.alta0anto.POL">
    <dif>
      Persono, organizo, kiu aprobas, apogas, subtenas, benas per sia
      a&ubreve;toritato ian aran&gcirc;on.
      <ekz>
        La Alta <tld lit="P"/>anto de la konferenco estas Unesko;
      </ekz>
          </dif>
     </snc>
  <trd lng="de">Schirmherr</trd>
  <trd lng="nl">beschermheer</trd>
  </drv>

<drv mrk="protek.0anteco">
  <kap><tld/>anteco</kap>
  <snc mrk="protek.0anteco.POL">
    <ref tip="dif" cel="protek.alta0ado.POL">alta <tld/>ado</ref>
    <ekz>
      la re&gcirc;o prenis sur sin la <tld/>antecon super la
      kongreso<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="fr">&eacute;gide</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;dn&ouml;ks&eacute;g</trd>,
    <trd>&eacute;gisz</trd>
  </trdgrp>
  <trd lng="nl">beschermheerschap</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_e;&c_g;&c_i;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="protek.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="protek.0ismo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ekonomia doktrino a&ubreve; politiko, metanta altajn doganojn sur
      importa&jcirc;ojn por <tld/>i la enlandan industrion a&ubreve; komercon:
      <ekz>
        <tld/>ismaj rimedoj
        <fnt>
        <aut>Bo Sandelin</aut>,
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="libera.0ismo.EKON">liberalismo <sncref/></ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_k;&c_c;&c_y;&c_ja;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>ochranářství</trd>,
    <trd>systém ochranných cel</trd>
  </trdgrp>
  <trd lng="de">Protektionismus</trd>
  <trd lng="es">proteccionismo</trd>
  <trd lng="fr">protectionnisme</trd>
  <trd lng="hu">protekcionizmus</trd>
  <trd lng="nl">protectionisme</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>protekcionizmus</trd>,
    <trd>systém ochranných ciel</trd>
  </trdgrp>
</drv>

<drv mrk="protek.medi0o">
  <kap>medi<tld/>o</kap>
  <snc mrk="protek.medi0o.EKOL">
  <ref tip="dif" cel="protek.natur0o.EKOL">natur<tld/>o</ref>
  <uzo tip="fak">EKOL</uzo>
      <ekz>
        &Ccirc;inio mobilizas &ccirc;iujn civitanojn kaj familiojn fari siajn 
        kontribuojn al energi&scirc;paro kaj medi<tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/362/2007/10/12/1@70906.htm">
          &Ccirc;inio atingis komencan sukceson</url></vrk>,
          <lok>2007-10-12</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="de">Umweltschutz</trd>
  <trd lng="es">protecci&oacute;n medioambiental</trd>
  <trd lng="nl">milieubescherming</trd>
</drv>

<drv mrk="protek.natur0o">
  <kap>natur<tld/>o</kap>
  <snc mrk="protek.natur0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Agado por konservi la naturon sendifekta, senpolua:
      <ekz>
        muzeo de natur<tld/>ado
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.liptovtravel.com/esperanto/jaskyne.html">
          Grotoj</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Naturschutz</trd>
  <trd lng="es">protecci&oacute;n de la naturaleza</trd>
  <trd lng="fr">protection de la nature</trd>
  <trd lng="nl">natuurbescherming</trd>
</drv>
</art>
