<kap>
  <ofc>*</ofc>
  <rad>ablativ</rad>/o
</kap>
<drv mrk="ablati.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="kaz.0o.GRA">Kazo</ref> de deklinacio en
      hinde&ubreve;ropaj lingvoj, esprimanta
      propre la deirpunkton:
      <ekz>
     <tld/>o absoluta estas speciala formo, uzata nur en kelkaj
     malmultaj lingvoj: en Esperanto &gcirc;i ne
     ekzistas<fnt><bib>LR</bib></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_a;&c_t;&c_y;&c_w;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_t;&c_y;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>tro-lemel</trd>,
    <trd>ablativ</trd>,
  </trdgrp>
  <trd lng="ca">ablatiu</trd>
  <trd lng="cs">ablativ</trd>
  <trd lng="de">Ablativ</trd>
  <trd lng="en">ablative</trd>
  <trd lng="es">ablativo</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&a_A;&a_l;&a_t; <ind>&a_A;&a_fatha;&a_z;&f_y;</ind></trd>,
    <trd>&a_H;&a_A;&a_l;&a_t; <ind>&a_m;&a_f;&a_ayn;&a_w;&a_l; &a_ayn;&a_n;&a_h;</ind></trd>
  </trdgrp>
  <trd lng="fr">ablatif</trd>
  <trdgrp lng="hu">
    <trd>ablativus</trd>,
    <trd>hat&aacute;roz&oacute;eset</trd>
  </trdgrp>
  <trd lng="it">ablativo</trd>
  <trd lng="nl">ablatief</trd>
  <trd lng="oc">ablatiu</trd>
  <trd lng="pl">narz&eogonek;dnik</trd>
  <trd lng="pt">ablativo</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_b;&c_l;&c_a;&c_t;&c_i;&c_v;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_t;&c_i;&c_v;</trd>,
    <trd>
   &c_o;&c_t;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  </trdgrp>
  <trd lng="sk">ablat√≠v</trd>
  <trd lng="sv">ablativ</trd>
  <trd lng="tr">-den hali</trd>
</drv>
</art>
