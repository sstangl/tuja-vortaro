<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pakt.xml,v 1.8 2015/11/06 17:10:14 revo Exp $">
<kap>
  <rad>pakt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pakt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pakt.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="trakta.0o.JUR">traktato <sncref/></ref>
    <ekz>
      en la interno de E&ubreve;ropa Unio pli kaj pli la&ubreve;tas la
      alvoko por amendi la <tld lit="P"/>on pri Stabileco
      kaj Kreskado
      <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url
        ref="http://esperanto.cri.cn/1/2005/03/08/1@21901.htm">
        Germana kaj franca &scirc;tat&ccirc;efoj...</url></vrk>,
        <lok>2005-03-08</lok>
      </fnt>.
    </ekz>
    <trd lng="pl">pakt</trd>
  </snc>
  <snc mrk="pakt.0o.POL">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="manife.0o.proklamo">manifesto</ref>
    <ekz>
      <tld lit="P"/>o de la Mondcivitanoj
      <fnt>
        <aut>-</aut>,
        <vrk><url ref="http://www.recim.org/cdm/pact-eo.htm">
        Pakto de la Mondcivitanoj</url></vrk>,
        <lok>2007</lok>
      </fnt>
    </ekz>
    <trd lng="pl">pakt</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>mezin&aacute;rodn&iacute; smlouva</trd>,
    <trd>pakt</trd>,
    <trd>z&aacute;vazn&aacute; dohoda</trd>,
    <trd>&uacute;mluva</trd>
  </trdgrp>
  <trd lng="en">pact</trd>
  <trd lng="fr">pacte</trd>
  <trd lng="nl">pakt<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="sk">
    <trd>dohoda</trd>,
    <trd>pakt</trd>
  </trdgrp>
</drv>
</art>

