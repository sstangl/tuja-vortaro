<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: puf.xml,v 1.23 2015/07/04 08:37:42 revo Exp $">
    <kap>
      <ofc>3</ofc>
      <rad>puf</rad>/o
    </kap>

    <drv mrk="puf.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Plenblovita a&ubreve; plen&scirc;topita objekto, kuseno a&ubreve; sako, kaj
          precipe plenblovita faldo de vesto:
          <ekz>
            puntoj por <tld/>o a&ubreve; por papilio?
            <fnt><bib>Marta</bib></fnt>.            
          </ekz>
        </dif>
        <adm>
          La difino estas dubinda. &Gcirc;i malakordas kun la zamenhofa
          ekzemplo kaj kun PIV2.
          <aut>Sergio</aut>
        </adm>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_u;&c_f;</trd>,
        <trd>&c_p;&c_u;&c_f;&c_ib;&c_k;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>nabíraná látka</trd>,
    <trd>poduška</trd>,
    <trd>vycpávka</trd>
  </trdgrp>
  <trd lng="de">Puff</trd>
  <trdgrp lng="en">
        <trd>puff</trd>,
        <trd>stuffing</trd>,
        <trd>pad</trd>,
        <trd>padding</trd>,
        <trd>wad</trd>
      </trdgrp>
  <trd lng="fr">pouf<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
        <trd>buggy</trd>,
        <trd>puff</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>poef</trd>,
        <trd>pof</trd>
      </trdgrp>
  <trd lng="pl">puf</trd>
  <trdgrp lng="pt">
        <trd>pufe</trd>,
        <trd>fofo <klr tip="amb">(de vestido)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_p;&c_u;&c_f;</trd>,
        <trd>&c_p;&c_u;&c_f;&c_i;&c_k;</trd>,
        <trd>&c_b;&c_u;&c_f;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>naberaná látka</trd>,
    <trd>vypchávka</trd>
  </trdgrp>
</drv>

    <drv mrk="puf.0a">
      <kap><tld/>a<fnt>Z</fnt></kap>
      <snc>
        <dif>
          &Scirc;velinta:
          <ekz>
            <tld/>aj vangoj, paka&jcirc;oj;
          </ekz><ekz>
            vaste <tld/>ita kamizolo<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="rembur.0i">remburi</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_u;&c_l;&c_mol;&c_h;&c_n;&c_y;</trd>,
        <trd>&c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_t;&c_y;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>nadýchaný</trd>,
    <trd>nafouklý</trd>,
    <trd>načechraný</trd>
  </trdgrp>
  <trd lng="de">bauschig</trd>
  <trdgrp lng="en">
        <trd>puffed</trd>,
        <trd>puffy</trd>,
        <trd>stuffed</trd>,
        <trd>padded</trd>
      </trdgrp>
  <trd lng="fr">pouf<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
        <trd>buggyos</trd>,
        <trd>puffadt</trd>,
        <trd>puf&oacute;k</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>pofferig</trd>,
        <trd>opgezwollen</trd>
      </trdgrp>
  <trdgrp lng="pl">
        <trd>wzd&eogonek;ty</trd>,
        <trd>wyd&eogonek;ty</trd>,
        <trd>spuchni&eogonek;ty</trd>,
        <trd>wypchany</trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>fofo</trd>,
        <trd>inflado</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_y;&c_j;</trd>,
        <trd>&c_v;&c_z;&c_d;&c_u;&c_t;&c_y;&c_j;</trd>,
        <trd>&c_p;&c_u;&c_h;&c_l;&c_y;&c_j;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>nafúknutý</trd>,
    <trd>napchaný</trd>
  </trdgrp>
</drv>

    <drv mrk="puf.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Plivasti&gcirc;i la&ubreve; &ccirc;iuj dimensioj:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            la konflikto &ccirc;iutage <tld/>i&gcirc;as;
          </ekz><ekz>
            mi preferus, ke velkseki&gcirc;u miaj piedoj, ol tu&scirc;i fremdan
            apartena&jcirc;on, &#8208; ekkriis la <tld/>i&gcirc;inta kato
	    <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24a</lok></fnt>. 
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="sxvel.0i">&scirc;veli</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_p;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_u;&c_s;&c_p;&c_u;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>puff up</trd>,
        <trd>puff out</trd>,
        <trd>swell</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>puffad</trd>,
        <trd>felf&uacute;j&oacute;dik</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>poffen</trd>,
        <trd>opzwellen</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>puchn&aogonek;&cacute;</trd>,
        <trd>spuchn&aogonek;&cacute;</trd>,
        <trd>wyd&aogonek;&cacute; si&eogonek;</trd>,
        <trd>wydyma&cacute; si&eogonek;</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>inchar(-se)</trd>,
        <trd>afofar-se</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_v;&c_z;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
  </art>
