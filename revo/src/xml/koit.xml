<kap>
  <rad>koit</rad>/i
</kap>
<drv mrk="koit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="kun.seks0igxi">sekskuni&gcirc;i</ref>
    <refgrp tip="sin">
      <ref cel="amor.0i">amori</ref>,
      <ref cel="fik.0i">fiki</ref>,
      <ref cel="kopula.0i">kopulacii</ref>
    </refgrp>
  </snc>
  <trd lng="ca">copular</trd>
  <trd lng="cs">souložit</trd>
  <trdgrp lng="de">
    <trd>koitieren</trd>,
    <trd>den Beischlaf vollziehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>copulate</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>accoupler</ind></trd>,
    <trd>faire l'<ind>amour</ind></trd>,
    <trd>baiser<klr tip="ind"> (faire l'amour)</klr></trd>,
    <trd>co&iuml;ter</trd>,
    <trd>copuler</trd>,
    <trd>coucher<klr tip="ind"> (co&iuml;ter)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>co&iuml;teren</trd>,
    <trd>geslachtsgemeenschap hebben</trd>
  </trdgrp>
  <trd
  lng="ru">&c_s;&c_o;&c_v;&c_o;&c_k;&c_u;&c_p;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">s&uacute;lo&zcaron;i&tcaron;</trd>
</drv>

<drv mrk="koit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        natura, kontra&ubreve;natura, <ind>interrompita <tld/>o</ind>.
        <trdgrp lng="en">
          <trd>coitus interruptus</trd>,
          <trd>&leftquot;<ind>pull and pray</ind>&rightquot;</trd>
        </trdgrp>
        <trd lng="fr">co&iuml;t interrompu</trd>
        <trd lng="nl">coitus interruptus</trd>
        <trd lng="sk">preru&scaron;ovan&aacute; <ind>s&uacute;lo&zcaron;</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="amor.0o.seksa">amoro</ref>,
      <ref cel="fik.0o">fiko</ref>,
      <ref cel="kopula.0o">kopulacio</ref>,
      <ref cel="kun.seks0igxo">sekskuni&gcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>coit</trd>,
    <trd>c&ograve;pula</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>koitus</trd>,
    <trd>sexuální akt</trd>,
    <trd>soulož</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Koitus</trd>,
    <trd>Beischlaf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>coitus</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accouplement</trd>,
    <trd>amour<klr tip="ind"> (charnel)</klr></trd>,
    <trd>baise<klr tip="ind"> (faire l'amour)</klr></trd>,
    <trd>co&iuml;t</trd>,
    <trd>copulation</trd>,
    <trd>relation sexuelle</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>co&iuml;tus</trd>,
    <trd>geslachtsgemeenschap</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_o;&c_k;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_je;
    &c_s;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;lo&zcaron;</trd>,
    <trd>súloženie</trd>
  </trdgrp>
</drv>
</art>
