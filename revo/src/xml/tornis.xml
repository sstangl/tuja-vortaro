<vortaro>
<art mrk="$Id: tornis.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tornistr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tornis.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tornis.0o.TUR">
    <ref tip="dif" cel="sak.dorso0o.TUR">dorsosako</ref>
    <ekz>
      la alvenantoj faris fajron en la forno, malfermis siajn
      <tld/>ojn, man&gcirc;is
      <fnt>
        <aut>P. Haanp&auml;&auml;, trad. B. Lehtinen</aut>,
        <vrk>Baraka artisto</vrk>,
        <bib>LOdE</bib>,
        <lok>2002:12 (98)</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_r;&c_u;&c_k;&c_z;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_ch;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">motxilla</trd>
  <trdgrp lng="cs">
    <trd>taška na záda</trd>,
    <trd>torna</trd>,
    <trd>tornistra</trd>
  </trdgrp>
  <trd lng="de">Tornister</trd>
  <trdgrp lng="en">
    <trd>pack</trd>,
    <trd>back pack</trd>,
    <trd>haversack</trd>,
    <trd>knapsack</trd>
  </trdgrp>
  <trd lng="es">mochila</trd>
  <trd lng="fr">sac &agrave; dos</trd>
  <trd lng="hu">h&aacute;tizs&aacute;k</trd>
  <trd lng="nl">rugzak</trd>
  <trdgrp lng="pl">
    <trd>tornister</trd>,
    <trd>plecak</trd>
  </trdgrp>
  <trd lng="pt">mochila</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_r;&c_ju;&c_k;&c_z;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">torba</trd>
</drv>
</art>
