<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanali.xml,v 1.12 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kanaliz</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kanali.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Provizi per <ref cel="kanal.0o.akvovojo">kanaloj <sncref/></ref>
      a&ubreve; <ref cel="kanal.0o.kondukilo"><sncref/></ref>:
      <ekz>
        <tld/>i urbon
        <fnt><bib>VdE</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie mankis plankoj, tegmentoj, elektra lumo, 
        <ind><tld/>o</ind>
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 11</lok></fnt>;
        <trdgrp lng="fr">
          <trd>canalisation</trd>,
          <trd>installation des <ind>canalisation</ind>s</trd>
        </trdgrp>
      </ekz>
      <ekz>
        sur trotuaroj supre aperos kradoj similaj al tiuj, kiuj estas
        aran&gcirc;ataj super la <tld/>aj kavoj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanal.0igi">kanaligi</ref>,
      <ref cel="kanal.en0igi">enkanaligi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">zřídit kanalizaci</trd>
  <trd lng="de">kanalisieren</trd>
  <trdgrp lng="en">
    <trd>canalize</trd>,
    <trd>ditch</trd>
  </trdgrp>
  <trd lng="es">canalizar</trd>
  <trdgrp lng="fr">
    <trd>canaliser<klr tip="ind"> (pourvoir en canaux, en canalisations)</klr></trd>,
    <trd>installer les <ind>canalisation</ind>s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatorn&aacute;z</trd>,
    <trd>kanaliz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">kanaliseren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ju;</trd>
  </trdgrp>
  <trd lng="sk">kanalizovať</trd>
</drv>
</art>
