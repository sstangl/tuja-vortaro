<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: euxsk.xml,v 1.10 2015/01/13 12:10:23 revo Exp $">
<kap><rad>e&ubreve;sk</rad>/o <fnt><bib>PIV2</bib></fnt></kap>
<drv mrk="euxsk.0o">
  <kap><tld/>o</kap>
  <snc>
      <ref tip="dif" cel="vask.0o">vasko</ref>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>&egrave;uscar</trd>,
    <trd>basc<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="en">Basque</trd>
  <trd lng="fr">Basque<klr> (subst.)</klr></trd>
  <trd lng="hu">baszk</trd>
  <trd lng="nl">Bask</trd>
  <trdgrp lng="pl">
    <trd>Bask</trd>,
    <trd>Baskijczyk</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>euscaro</trd>,
    <trd>basco</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_k;</trd>
</drv>

<drv mrk="euxsk.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="vask.0a">vaska</ref>
    <ekz>
      la balotado por la <tld/>a parlamento en januaro
      2001<fnt><aut>Hektor Alos i Font</aut>: <vrk>Nur etaj esperoj pri
      balda&ubreve;a solvo</vrk>, <bib>Monato</bib><lok>jaro 2002a,
      numero 8a,
      p. 11a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>&egrave;uscar</trd>,
    <trd>basc<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="en">Basque</trd>
  <trd lng="fr">basque<klr> (adj.)</klr></trd>
  <trd lng="hu">baszk</trd>
  <trd lng="nl">Baskisch</trd>
  <trd lng="pl">baskijski</trd>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_k;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
