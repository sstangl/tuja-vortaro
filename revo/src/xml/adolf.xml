<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adolf.xml,v 1.15 2015/09/12 20:10:25 revo Exp $">
<kap>
  <rad>Adolf</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="adolf.0o">
  <kap><tld/>o</kap>
  <snc mrk="adolf.0o.NOM">
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        <tld/>o estis metallaboristo
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en"><trd>Adolf</trd><trd>Adolph</trd></trdgrp>
  <trd lng="be">&c_A;&c_d;&c_o;&c_l;&c_mol;&c_f;</trd>
  <trd lng="ca">Adolf</trd>
  <trd lng="cs">Adolf</trd>
  <trd lng="de">Adolf</trd>
  <trd lng="es">Adolfo</trd>
  <trd lng="fr">Adolphe</trd>
  <trd lng="hu">Adolf</trd>
  <trd lng="it">Adolfo</trd>
  <trd lng="nl">Adolf</trd>
  <trd lng="pl">Adolf</trd>
  <trd lng="pt">Adolfo</trd>
  <trd lng="sk">Adolf</trd>
  <trd lng="sv">Adolf</trd>
</drv>
</art>

