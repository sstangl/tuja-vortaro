  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">členovec</trd>
</drv>
