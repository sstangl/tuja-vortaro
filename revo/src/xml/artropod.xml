<kap>
  <rad>artropod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artropod.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="artropod.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
