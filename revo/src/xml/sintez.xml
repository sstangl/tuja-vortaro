<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sintez.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sintez</rad>/o
</kap>

<drv mrk="sintez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="sintez.0o.kombino">
    <dif>
      Kombino a&ubreve; kunigo de eroj al kohera tuto kun propra
      substanco a&ubreve; aspekto.
    </dif>
  </snc>
  <snc mrk="sintez.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Generaligo, sistemigo.
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="analiz.0o">analizo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_y;&c_n;&c_t;&c_e;&c_z;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;</trd>
  <trd lng="de">Synthese</trd>
  <trd lng="en">synthesis</trd>
  <trd lng="es">s&iacute;ntesis</trd>
  <trdgrp lng="fr">
    <trd>synth&egrave;se</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szit&eacute;zis</trd>,
    <trd>&ouml;sszefoglal&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">synthese</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>spájanie</trd>,
    <trd>syntéza</trd>,
    <trd>systematické usporiadanie</trd>,
    <trd>zlučovanie</trd>,
    <trd>zovšeobecnenie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skladba</trd>,
    <trd>slučování</trd>,
    <trd>spojení</trd>,
    <trd>syntéza</trd>
  </trdgrp>
</drv>

<drv mrk="sintez.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="S"/>e fari tuta&jcirc;on el diversaj elementoj:
      <ekz>
        figuro universala, plej renoma en la kataluna arkitektura arto
        pro
        sia kapablo <tld/>i tradiciojn de la lando<fnt><aut>Augusto
        Casquero</aut>: <vrk>2002 - Internacia jaro de Antoni
        Gaud&iacute;</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_t;&c_e;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>slučovat</trd>,
    <trd>syntetizovat</trd>
  </trdgrp>
  <trd lng="de">synthetisieren</trd>
  <trd lng="en">synthesize</trd>
  <trd lng="es">sintetizar</trd>
  <trdgrp lng="fr">
    <trd>synth&eacute;tiser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szintetiz&aacute;l</trd>,
    <trd>&ouml;sszefoglal</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>synthetiseren</trd>,
    <trd>samenvatten</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>spájať</trd>,
    <trd>zlučovať</trd>
  </trdgrp>
</drv>

<drv mrk="sintez.0ilo">
  <kap><tld/>ilo</kap> <fnt><bib>RokPop</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Elektra muzikinstrumento, kiu kreas sonon elektronike per
      sintezado,
      ordinare sed ne nepre sonigata per klavaro:
      <ekz>
        <tld/>ila muziko ripete sed bone farita<fnt><aut>JPVDD</aut>:
        <vrk>&Scirc;paru monon (Vinilkosmokompil)</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_t;&c_e;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>syntetizátor</trd>,
    <trd>syntezátor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Synthesizer</trd>,
    <trd>Keyboard</trd>
  </trdgrp>
  <trd lng="en">synthesizer</trd>
  <trdgrp lng="es">
    <trd>sintetizador</trd>,
    <trd>sintetizadora</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>synth&eacute;</trd>,
    <trd>synth&eacute;tiseur</trd>
  </trdgrp>
  <trd lng="hu">szintetiz&aacute;tor</trd>
  <trd lng="nl">synthesiser</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_t;&c_je;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">syntetizátor</trd>
</drv>
</art>
