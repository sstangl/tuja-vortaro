<vortaro>
<art mrk="$Id: korsaj.xml,v 1.15 2014/06/10 14:10:16 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>korsa&jcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korsaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="korsaj.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de virina vesto, kovranta la buston:
      <ekz>
        dekoltita <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_s;&c_a;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Leibchen</trd>,
    <trd>Mieder</trd>,
    <trd>Korsage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bodice</trd>,
    <trd>body</trd>,
    <trd>corsage</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&f_y;&a_m;&zwnj;&a_t;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_A;&a_l;&a_A;&a_t;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">corsage</trd>
  <trd lng="hu">ruhader&eacute;k</trd>
  <trd lng="id">korsase</trd>
  <trdgrp lng="pt">
    <trd>busti&ecirc;</trd>,
    <trd>top</trd>,
    <trd>corpete <klr tip="amb">(&ecirc;)</klr></trd>,
    <trd>corpinho</trd>,
    <trd>corselete <klr tip="amb">(&ecirc;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_zh;</trd>,
    <trd>&c_l;&c_i;&c_f;</trd>
  </trdgrp>
</drv>
</art>
