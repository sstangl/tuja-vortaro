<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alhxem.xml,v 1.18 2015/08/21 13:10:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>al&hcirc;emi</rad>/o
</kap>
<drv mrk="alhxem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Okulta scienco, kiu per &hcirc;emiaj procedoj kaj magiaj formuloj
      ser&ccirc;is la &scirc;tonon de la sa&gcirc;uloj kaj aliajn
      &hcirc;imerojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Alchemie</trd>,
    <trd>Alchimie</trd>,
    <trd>Goldmacherkunst</trd>
  </trdgrp>
  <trd lng="en">alchemy</trd>
  <trd lng="es">alquimia</trd>
  <trd lng="fr">alchimie</trd>
  <trdgrp lng="nl">
    <trd>alchemie</trd>,
    <trd>alchimie</trd>
  </trdgrp>
  <trd lng="pl">alchemia</trd>
  <trd lng="pt">alquimia</trd>
  <trd lng="ru">&c_a;&c_l;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">alch&yacute;mia</trd>
</drv>

<drv mrk="alhxem.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono kiu praktikas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_h;&c_ib;&c_m;&c_ib;&c_k;</trd>
  <trd lng="de">Alchimist</trd>
  <trd lng="en">alchemist</trd>
  <trd lng="es">alquimista</trd>
  <trd lng="fr">alchimiste</trd>
  <trdgrp lng="nl">
    <trd>alchemist</trd>,
    <trd>alchimist</trd>
  </trdgrp>
  <trd lng="pl">alchemik</trd>
  <trd lng="pt">alquimista</trd>
  <trd lng="ru">&c_a;&c_l;&c_h;&c_i;&c_m;&c_i;&c_k;</trd>
  <trd lng="sk">alchymista</trd>  
</drv>
</art>
