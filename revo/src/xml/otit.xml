<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: otit.xml,v 1.6 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>otit</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="otit.0o">
  <kap><tld/>o</kap>
  <snc mrk="otit.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="inflam.orel0o">orelinflamo</ref>
    <ekz>
      kuracisto preskribis por Roselyne antibiotikon de la tria 
      generacio kontra&ubreve; akuta <tld/>o
      <fnt>
        <aut>P. Rivi&egrave;re, trad. M. Leon</aut>,
        <vrk><url ref="http://eo.mondediplo.com/article830.html">
        La farmaciaj granduloj...</url></vrk>,
        <lok>le Monde diplomatique, 2003-10</lok>
      </fnt>;
    </ekz>
    <ekz>
      mi <klr>...</klr> estas iom surda post grava <tld/>o okazinta 
      anta&ubreve; multaj jaroj
      <fnt>
        <aut>P. Levy</aut>,
        <vrk>Internacia komunikado</vrk>,
        <lok><url
        ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
        soc.culture.esperanto</url>, 2009-06-29</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>otitida</trd>,
    <trd>otitis</trd>,
    <trd>ušní zánět</trd>
  </trdgrp>
  <trd lng="en">otitis</trd>
  <trd lng="fr">otite</trd>
  <trd lng="it">otite</trd>
  <trdgrp lng="nl">
    <trd>otitis</trd>,
    <trd>oorontsteking</trd>
  </trdgrp>
  <trd lng="sk">zápal ucha</trd>
</drv>
</art>

