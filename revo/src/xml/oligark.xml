<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oligark.xml,v 1.7 2015/10/27 10:10:19 revo Exp $">
<kap>
  <rad>oligark</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="oligark.0o">
  <kap><tld/>o</kap>
  <snc mrk="oligark.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano de reganta grupo a&ubreve; familio en
      <ref tip="vid" cel="oligarki.0o.POL">oligarkio</ref>:
      <ekz>
        efektiva potenco estas en la manoj de la prezidento,
        la provincestroj kaj la <tld/>oj
        <fnt>
          <aut>S. Kalb</aut>,
          <vrk><url
          ref="http://www.oocities.org/kalblando/enc/r/ru.htm">
          Rusio</url></vrk>,
          <lok>en: Enciklopedio Kalblanda, 1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_r;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>oligarch</trd>,
    <trd>oligarcha</trd>
  </trdgrp>
  <trd lng="en">oligarch</trd>
  <trd lng="fr">oligarque</trd>
  <trd lng="pl">oligarcha</trd>
  <trd lng="sk">oligarcha</trd>
</drv>
</art>

