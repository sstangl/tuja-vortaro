<kap>
<ofc>2</ofc>
<rad>balzamin</rad>/o
</kap>
<drv mrk="balzam1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="balzam2.0oj"><tld/>acoj</ref>
      (<trd lng="la">Impatiens</trd>).
    </dif>
  </snc>
  <bld lok="../bld/balzamin.jpg">
    <ind>glanda <tld/>o</ind>
    <trd lng="de">dr&uuml;siges Springkraut</trd>
    <trd lng="la">Impatiens glandulifera</trd>
    <trd lng="nl">reuzenbalsemien</trd>
    <trd lng="sv">j&auml;ttebalsamin</trd>
  </bld>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_ib;&c_n;</trd>
  <trd lng="de">Springkraut</trd>
  <trd lng="ca">balsamina</trd>
  <trd lng="en">impatiens</trd>
  <trd lng="fr">balsamine</trd>
  <trdgrp lng="hu">
    <trd>f&aacute;jvir&aacute;g</trd>,
    <trd>neb&aacute;ncsvir&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">springzaad</trd>
  <trdgrp lng="pl">
    <trd>niecierpek</trd>,
    <trd>balsamina</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_t;&c_r;&c_o;&c_g;&c_a;
    &c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;&c_i;&c_n;&c_o;&c_v;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">balsamin</trd>
  <trd lng="sk">netýkavka</trd>
  <trd lng="cs">balzamínka</trd>
</drv>

</art>
