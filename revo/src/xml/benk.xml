<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: benk.xml,v 1.27 2015/12/01 19:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>benk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="benk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="benk.0o.MEBL">
    <dif>
      <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">Meblo</ref>,
      longa tabula
      <ref tip="super" cel="sid.0ilo">sidilo</ref> por pli
      ol unu persono, kun a&ubreve; sen dorsapogilo, en hejmo, a&ubreve;to,
      vagono, kaj similaj:
      <ekz>
        &scirc;i <klr>...</klr> sidigis lin sur la <tld/>on
        <fnt>
        <aut>A. Edelmann, trad. I. Kaminski</aut>,
        <bib>FK</bib>,
        <vrk>La forgesita pipo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la landlimo la senpasportulo devis ku&scirc;i sur la
        malanta&ubreve;a <tld/>o de l' a&ubreve;to
        <fnt>
        <aut>S. Maul</aut>,
        <vrk>Pozitive pri EU</vrk>,
        <bib>Monato</bib>,
        <lok>1997:8, p. 5a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_o;&c_n;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_z;&c_e;&c_d;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_z;&c_e;&c_d;&c_l;&c_ib;&c_ch;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_ja;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr
    tip="amb">(&c_u; &c_v;&c_a;&c_g;&c_o;&c_n;&c_je;,
    &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>skaon</trd>,
    <trd>bank<klr tip="ind"> (skaon)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>banc</trd>,
    <trd>banca</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lavice</trd>,
    <trd>sedadlo <klr>(ve vagon&ecaron; ap.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bank</trd>,
    <trd>Sitzbank</trd>
  </trdgrp>
  <trd lng="en">bench</trd>
  <trdgrp lng="es">
    <trd>banca<klr> (objeto usado para sentarse)</klr></trd>,
    <trd>banco<klr> (objeto usado para sentarse)</klr></trd>,
    <trd>asiento<klr> (...en medios de transpoerte)</klr></trd>,
  </trdgrp>
  <trd lng="fa">&a_n;&f_y;&a_m;&f_k;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>banc</trd>,
    <trd>banquette</trd>
  </trdgrp>
  <trd lng="hu">pad</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;awa</trd>,
    <trd>&lstroke;awka</trd>
  </trdgrp>
  <trd lng="pt">banco</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_m;&c_mol;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&c_je;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_d;&c_je;&c_n;&c_mol;&c_je; <klr>(&c_v;
    &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lavica</trd>,
    <trd>sedadlo</trd>
  </trdgrp>
</drv>

<drv mrk="benk.konduk0o">
  <kap>konduk<tld/>o,
  <var><kap>ko&ccirc;era <tld/>o</kap></var></kap>
  <snc mrk="benk.konduk0o.TRA">
    <dif>
      <tld lit="B"/>o sur kiu sidas kondukanto de veturilo:
      <ekz>
        La ko&ccirc;ero &scirc;an&gcirc;is la &ccirc;evalojn kaj suririnte la konduk<tld/>on,
        demandis: dekstren, &ccirc;u maldekstren? 
        <fnt><bib>PatrojFiloj</bib></fnt>;
      </ekz>
      <ekz>
        sur la ko&ccirc;era <tld/>o sidis knabo grasa kaj ru&gcirc;viza&gcirc;a
        <fnt>
          <aut>C. Dickens, trad. W Auld</aut>,
          <vrk><url
          ref="http://www.esperanto-ondo.ru/Libroj/Pikviko.pdf">
          La Postlasitaj Paperoj de la Klubo Pikvika</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">banc du cocher</trd>
</drv>

<drv mrk="benk.pied0eto">
  <kap>pied<tld/>eto</kap>
  <snc mrk="benk.pied0eto.MEBL">
    <dif>
      Malalta ebena <ref tip="super" cel="mebl.0o">mebleto</ref>, sur
      kiu oni apogas la piedojn, sidante sur alta sidilo:
      <ekz>
        tiele diras la Eternulo: La &ccirc;ielo estas Mia trono, kaj la
        tero estas Mia pied<tld/>eto
        <fnt><bib>MT</bib>, <lok>&Jes; 66:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">skabell<klr tip="ind"> (da lakaat e dreid
  warni)</klr></trd>
  <trd lng="ca">escambell</trd>
  <trdgrp lng="cs">
    <trd>podno&zcaron;ka</trd>,
    <trd>stoli&ccaron;ka pod nohy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fu&szlig;bank</trd>,
    <trd>Fu&szlig;schemel</trd>
  </trdgrp>
  <trd lng="fr">repose-pied</trd>
  <trd lng="pl">podn&oacute;&zdot;ek</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_r;&c_a; &c_d;&c_l;&c_ja;
    &c_n;&c_o;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_n;&c_o;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">podno&zcaron;ka</trd>
</drv>
</art>
