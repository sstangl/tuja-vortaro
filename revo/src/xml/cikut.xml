<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cikut.xml,v 1.21 2013/09/13 16:10:10 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>cikut</rad>/o
</kap>
<drv mrk="cikut.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Cicuta</trd>), tre venena, simila al
      <ref tip="vid" cel="koni.0o">konio</ref>:
      <ekz>
        salatoj el semoj de fungoj, malsekaj bu&scirc;oj de musoj kaj
        cikuto
        <fnt><bib>Fab2</bib>, <lok>Monteto de elfoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">csomorika</trd>
    <trd lng="pl">szalej jadowity</trd>
  </snc>
  <snc>
    <dif>
      Venena suko el tiu vegeta&jcirc;o:
      <ekz>
        la Atenanoj kondamnis Sokraton trinki <tld/>on.
      </ekz>
    </dif>
    <trd lng="hu">b&uuml;r&ouml;k</trd>
    <trd lng="pl">cykuta</trd>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_u;&c_t;&c_a;</trd>
  <trd lng="br">kegid</trd>
  <trd lng="de">Wasserschierling</trd>
  <trd lng="el">&kappa;&omega_ton;&ny;&epsilon;&jota;&omikron;</trd>
  <trd lng="en">water <ind>hemlock</ind></trd>
  <trd lng="es">cicuta</trd>
  <trd lng="fr">cigu&euml;</trd>
  <trd lng="it">cicuta</trd>
  <trd lng="nl">cicuta</trd>
  <trd lng="pt">cicuta</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_u;&c_t;&c_a;</trd>
  <trd lng="sv">spr&auml;ng&ouml;rt</trd>
</drv>
</art>
