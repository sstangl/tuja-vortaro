<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nupt.xml,v 1.14 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>nupt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="nupt.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="nupt.0o.MOR">
    <ref tip="dif" cel="fest.geedzigxo0o.MOR">geedzi&gcirc;ofesto</ref>
    <ekz>
      lia patro jam faris planon pri gefian&ccirc;igo kaj lia onklo
      projekton pri <tld/>o
      <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
      399a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>svatba</trd>,
    <trd>svatební hostina</trd>,
    <trd>sňatek</trd>
  </trdgrp>
  <trd lng="de">Hochzeit</trd>
  <trdgrp lng="en">
    <trd>wedding</trd>,
    <trd>nuptials</trd>
  </trdgrp>
  <trd lng="es">nupcias</trd>
  <trd lng="fr">noce</trd>
  <trd lng="hu">n&aacute;sz</trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">&sacute;lub</trd>
  <trdgrp lng="pt">
    <trd>n&uacute;pcias</trd>,
    <trd>casamento</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sobáš</trd>,
    <trd>svadba</trd>,
    <trd>veselie</trd>
  </trdgrp>
</drv>
</art>
