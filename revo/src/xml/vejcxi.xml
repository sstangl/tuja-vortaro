<vortaro>
<art mrk="$Id: vejcxi.xml,v 1.11 2015/06/27 16:10:18 revo Exp $">
<kap>
  <rad>vej&ccirc;i</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="vejcxi.0o">
  <kap><tld/>o</kap>
  <snc mrk="vejcxi.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <ref tip="dif" cel="go.0o">goo</ref>
  </snc>
  <trd lng="be">&c_g;&c_o;</trd>
  <trdgrp lng="bg">
    <trd>&c_g;&c_o;</trd>,
    <trd>&c_V;&c_je;&c_j;&c_ch;&c_i;</trd>
  </trdgrp>
  <trd lng="ca">go</trd>
  <trd lng="de">Go</trd>
  <trd lng="en">go</trd>
  <trd lng="fr">go</trd>
  <trd lng="hu">g&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#30849;</trd>,
    <trd>go</trd>,
    <trd>igo</trd>
  </trdgrp>
  <trdgrp lng="ko">
    <trd>&#48148;&#46161;</trd>,
    <trd>baduk</trd>
  </trdgrp>
  <trd lng="nl">go</trd>
  <trdgrp lng="pt">
   <trd>go</trd>,
   <trd>jogo de go</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#22285;&#26827; <klr>(trad.)</klr></trd>,
    <trd>围棋 [<ind>wéi qí</ind>] <klr>(simp.)</klr></trd>,
  </trdgrp>
</drv>

<drv mrk="vejcxi.0i">
  <kap><tld/>i</kap>
  <snc mrk="vejcxi.0i.SPO">
    <dif>
      Ludi <tld/>on:
      <ekz>
        kiam la legado lacigis, li <tld/>is a&ubreve; drinkis kun siaj
sekretarioj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
        497a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol; &c_u;
<ind>&c_g;&c_o;</ind></trd>
  <trd lng="ca">jugar al <ind>go</ind></trd>
  <trd lng="de">Go <ind>spielen</ind></trd>
  <trd lng="en">play go</trd>
  <trd lng="fr">jouer au <ind>go</ind></trd>
  <trdgrp lng="hu">
    <trd>g&oacute;zik</trd>,
    <trd>g&oacute;t j&aacute;tszik</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_v;
<ind>&c_g;&c_o;</ind></trd>
</drv>
</art>
