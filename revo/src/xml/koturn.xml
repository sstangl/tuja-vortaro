<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koturn.xml,v 1.14 2013/09/27 16:10:11 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>koturn</rad>/o
</kap>
<drv mrk="koturn.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <snc mrk="koturn.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="fazane.0oj">fazanedoj</ref>
       (<trd lng="la">Coturnix</trd>), flavgriza kampobirdo:
       <ekz>
         dudek bone rostitaj <tld/>oj sur rostita pano
         <fnt><bib>DOz</bib>, <lok>&leftquot;Ja&ccirc;jo la Kale&scirc;o-&Ccirc;evalo&rightquot;</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_jo;&c_l;&c_k;&c_a;</trd>
   <trd lng="bg">&c_p;&c_malmol;&c_d;&c_p;&c_malmol;&c_d;&c_malmol;&c_k;</trd>
   <trd lng="cs">k&rcaron;epelka obecn&aacute;</trd>
   <trd lng="de">Wachtel</trd>
   <trd lng="en">quail</trd>
   <trd lng="fr">caille</trd>
   <trd lng="hu">f&uuml;rj</trd>
   <trd lng="nl">kwartel</trd>
   <trd lng="pl">przepi&oacute;rka</trd>
   <trdgrp lng="pt">
     <trd>codorna</trd>,
     <trd>codorna-caseira</trd>,    
     <trd>codorna-dom&eacute;stica</trd>,    
     <trd>codorniz</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_jo;&c_l;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_je;&c_l;</trd>
   </trdgrp>
</drv>
</art>
