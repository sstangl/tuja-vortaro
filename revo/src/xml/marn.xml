<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marn.xml,v 1.13 2015/09/01 18:10:18 revo Exp $">
<kap>
  <rad>marn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="marn.0o">
  <kap><tld/>o</kap>
  <snc mrk="marn.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Kalkeca tera&jcirc;o miksita kun argilo, uzebla
      kiel <ref tip="super" cel="sterk.0o.AGR">sterko</ref>
      por plibonigi grundon.
      <ekz>
        blankaj kaj grizaj <tld/>oj
        <fnt>
          <aut>J. M. Mabesoone</aut>,
          <vrk><url
          ref="http://www.repository.naturalis.nl/document/549662">
          Tertiary and Quaternary Sedimentation in a Part of the 
          Duero Basin Palencia (Spain)</url></vrk>,
          <lok>[Leidse Geologische Mededelingen, 1959 (24), p. 
          31a-180a]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>
  <trd lng="ca">marga</trd>
  <trd lng="de">Mergel</trd>
  <trd lng="en">marl</trd>
  <trd lng="fa">&a_A_madda;&a_h;&f_k;&a_r;&a_damma;&a_s;</trd>
  <trd lng="fr">marne</trd>
  <trd lng="hu">m&aacute;rga</trd>
  <trd lng="nl">mergel</trd>
  <trd lng="pt">marga</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_h;&c_l;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="marn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="marn.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Plibonigi kampon per la <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_g;&c_n;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_je;&c_m;</ind></trd>
  <trd lng="ca">margar</trd>
  <trd lng="de">mergeln</trd>
  <trd lng="en">marl</trd>
  <trd lng="fr">marner</trd>
  <trd lng="hu">m&aacute;rg&aacute;z</trd>
  <trd lng="nl">mergelen</trd>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol; &c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_je;&c_m;</trd>
</drv>
</art>
