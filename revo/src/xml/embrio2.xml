<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: embrio2.xml,v 1.18 2011/10/08 20:10:13 revo Exp $">
<kap>
  <rad>embriologi</rad>/o <fnt><bib>PV</bib></fnt>
  <fnt>Z</fnt>
</kap>
<drv mrk="embrio2.0o">
  <kap><tld/>o</kap>
  <snc mrk="embrio2.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco, parto de <ref tip="malprt"
      cel="biolog1.0o">biologio</ref>,
      kiu pritraktas la disvolvi&gcirc;on de organismoj ekde ilia
      stato de <ref tip="vid" cel="embri.0o">embrio</ref> &gcirc;is ilia
      naski&gcirc;o:
      <ekz>
        <tld/>o fari&gcirc;as tre interesa, kiam oni konsideras la
        embrion kiel bildon, pli malpli malklaran, de la komuna gepatra
        formo de &ccirc;iu granda klaso de animaloj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_b;&c_r;&c_y;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Embryologie</trd>
  <trd lng="en">embryology</trd>
  <trd lng="es">embriolog&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&f_y;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_j;&a_n;&f_y;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">embryologie</trd>
  <trd lng="hu">embriol&oacute;gia</trd>
  <trd lng="nl">embryologie</trd>
  <trd lng="pl">embriologia</trd>
  <trd lng="pt">embriologia</trd>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="embrio2.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="embrio2.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="embrio1.0o">embriologo</ref>
  </refgrp>
  <trd lng="be">&c_e;&c_m;&c_b;&c_r;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Embryologe</trd>
  <trd lng="en">embryologist</trd>
  <trd lng="es">embri&oacute;logo</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&f_y;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_j;&a_n;&f_y;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">embryologue</trd>
  <trd lng="hu">embriol&oacute;gus</trd>
  <trd lng="nl">embryoloog</trd>
  <trd lng="pl">embriolog</trd>
  <trdgrp lng="pt">
    <trd>embri&oacute;logo</trd>,
    <trd>embriologista</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
