<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: disk.xml,v 1.42 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>disk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="disk.0o">
  <kap><tld/>o</kap>
  <snc mrk="disk.0o.diversa">
    <dif>
      Plata, ronda objekto:
      <ekz>
        <tld/>o de suno, de fervoja signalo;
      </ekz><ekz>
        damluda <ind><tld/>o</ind>.
      </ekz>
    </dif>
    <trd lng="es">pe&oacute;n<klr> (del juego de damas)</klr></trd>
    <trd lng="hu">k&odblac;
    <klr>(d&aacute;maj&aacute;t&eacute;kban)</klr></trd>
    <trd lng="pl">pionek<klr> (w warcabach)</klr></trd>
    <trd lng="pt">disco</trd>
    <trd lng="ru">&c_sh;&c_a;&c_sh;&c_k;&c_a;<klr> (&c_v; &c_sh;&c_a;&c_sh;&c_k;&c_a;&c_h;)</klr></trd>
    <subsnc mrk="disk.0o.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        uzata en &jcirc;etludoj:
        <ekz>
          antikva helena statuo de <tld/>o&jcirc;etanto.
        </ekz>
      </dif>
      <trd lng="de">Diskus</trd>
      <trd lng="hu">diszkosz</trd>
      <trd lng="nl">discus</trd>
      <trd lng="pl">dysk</trd>
      <trd lng="pt">disco</trd>
    </subsnc>
    <subsnc mrk="disk.0o.MUZ.KOMP">
      <uzo tip="fak">MUZ</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        sur kiu estas registritaj iuj datumoj:
        <ekz>
          <ind>son<tld/></ind>o;
          <trd lng="hu">hanglemez</trd>
        </ekz><ekz>
          magneta <tld/>o, <tld/>eto;
        </ekz><ekz>
          lum<tld/>o.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Platte</trd>,
        <trd>Diskette</trd>,
        <trd>CD</trd> ktp.
      </trdgrp>
      <trd lng="hu">lemez</trd>
      <trdgrp lng="nl">
        <trd>schijf</trd>,
        <trd>diskette</trd>,
        <trd>CD</trd>,
        <trd>plaat <klr tip="ind">(grammofoon-)</klr></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>p&lstroke;yta<klr> (gramofonowa, kompaktowa, CD,
        DVD)</klr></trd>
      </trdgrp>
    </subsnc>
    <trd lng="nl">schijf</trd>
  </snc>

  <snc mrk="disk.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="disk.0o.cirklo">
      <fnt><bib>MatVort</bib></fnt>
      <fnt><bib>PIV2</bib>, <lok>disko</lok></fnt>
      <dif>
        Ebena <ref tip="super" cel="surfac.0o.MAT">surfaco</ref>, limata
        de <tld/>o<sncref ref="cirkl.0o.linio"/>:
        <ekz>
          la areo de <tld/>o, havanta la radiuson <frm>R</frm>,
          estas <frm>&pi;R&quadrat;</frm>.
        </ekz>
      </dif>
      <ref tip="sin" cel="cirkl.0o.surfaco">cirklo<sncref/></ref>
      <refgrp tip="prt">
        <ref cel="cirkl.0o.linio">cirklo<sncref/></ref>,
        <ref cel="segmen.0o.cirklo">segmento</ref>,
        <ref cel="sektor.0o.de_cirklo">sektoro</ref>.
      </refgrp>
      <refgrp tip="vid">
        <ref cel="centr.0o.cirklo">centro</ref>,
        <ref cel="cirkon.0o">cirkonferenco</ref>,
        <ref cel="diamet.0o.cirklo">diametro</ref>,
        <ref cel="radius.0o.cirklo">radiuso</ref>,
        <ref cel="sag.0o.MAT">sago</ref>,
        <ref cel="sxnur.0o.cirklo">&scirc;nuro</ref>;
      </refgrp>
      <bld lok="../bld/e_cirklo.svg.gif" alt="300" lrg="300">
        <tld lit="D"/>o kaj &gcirc;iaj partoj
      </bld>
      <trdgrp lng="de">
        <trd>Kreis</trd>,
        <trd>Kreisscheibe</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>circle</trd>,
        <trd>disk</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>cercle</trd>,
        <trd>disque</trd>
      </trdgrp>
      <trd lng="hu">k&ouml;rlemez</trd>
      <trd lng="pl">ko&lstroke;o</trd>
      <trd lng="pt">c&iacute;rculo</trd>
      <trd lng="ru">&c_k;&c_r;&c_u;&c_g;</trd>
      <trd lng="sk">kruh</trd>
    </subsnc>

    <subsnc mrk="disk.0o.en_kompleksa_ebeno">
      <fnt><bib>PIV2</bib>, <lok>konver&gcirc;i</lok></fnt>
      <dif>
        <klr>(en la aro de <ref
        cel="komple.0o.MAT">kompleksoj</ref>)</klr>
      </dif>
      <ref tip="dif" cel="glob.0o.metrikaspaco">globo<sncref/></ref>
      <trdgrp lng="en">
        <trd>disk</trd>,
        <trd>circle</trd>
      </trdgrp>
      <trd lng="de">Kreis</trd>
      <trdgrp lng="fr">
        <trd>disque</trd>,
        <trd>cercle</trd>
      </trdgrp>
      <trd lng="hu">egys&eacute;gk&ouml;rlemez</trd>      
      <trd lng="pl">kula</trd>
      <trd lng="ru">&c_k;&c_r;&c_u;&c_g;</trd>
    </subsnc>
    <adm>
      Kontroli, ke la jamaj tradukoj plu validas.
      <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_g;</trd>,
    <trd>&c_sh;&c_a;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_w;
    &c_sh;&c_a;&c_sh;&c_k;&c_a;&c_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>disk</trd>,
    <trd>kotouč</trd>
  </trdgrp>
  <trd lng="de">Scheibe</trd>
  <trd lng="en">disk</trd>
  <trd lng="es">disco</trd>
  <trd lng="fr">disque</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>disk</trd>,
    <trd>kotúč</trd>
  </trdgrp>
</drv>

<drv mrk="disk.0i">
  <kap><tld/>i<fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="disk.0i.turnidiskon">
    <dif>
      Fingre kunmeti telefonnumeron per cifer<tld/>o:
      <ekz>
        F-ino Ueda tuj diskis la telefonnumeron
        kaj enmanigis la aparaton al D-ro Nakagawa
        <fnt>
          <aut>B. Golden</aut>,
          <vrk>Kosma ruba&jcirc;o</vrk>,
          <lok>Beletra Almanako, 2009-02 (4), p. 11a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">dial<klr tip="amb"> (telephone)</klr></trd>
</drv>

<drv mrk="disk.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="disk.0eto.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Relative malgranda demetebla
      <ref cel="disk.0o.magneta">magneta <tld/>o</ref>
      uzata por datumtransportado a&ubreve; konservado de malofte
      uzataj
      datumaroj:
      <ekz>
        tiel nomatajn <ctl>3&#189;-colajn <tld/>etojn</ctl> la normo
        fakte difinas kiel 9-centimetrajn <tld/>etojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_k;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>disketa</trd>,
    <trd>floppy-disk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Floppy</trd>,
    <trd>Diskette</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>floppy disk</trd>, <trd>diskette</trd>
  </trdgrp>
  <trd lng="es"><ind>disco</ind> flexible</trd>
  <trd lng="fr">disquette</trd>
  <trdgrp lng="hu">
    <trd>hajl&eacute;konylemez</trd>,
    <trd>floppy</trd>
  </trdgrp>
  <trd lng="nl">diskette</trd>
  <trd lng="pl">dyskietka</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_k;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">disketa</trd>
</drv>

<drv mrk="disk.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="disk.0ingo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Fendo en diskturnilo, tra kiu la uzanto instalas
      disketon (a&ubreve;, simile, lumdiskon).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_a;
  &c_d;&c_y;&c_s;&c_k;&c_a;&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="cs">štěrbina v diskové aparatuře pro vkládání disku</trd>
  <trd lng="de">Schlitz (des Laufwerks)</trd>
  <trd lng="en">disk drive</trd>
  <trd lng="fr">lecteur de disque</trd>
  <trd lng="hu">lemezmeghajt&oacute;-ny&iacute;l&aacute;s</trd>
  <trd lng="pl">szczelina nap&eogonek;du<klr> (dyskietek,
  CD)</klr></trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_mol;
  <ind>&c_d;&c_i;&c_s;&c_k;&c_o;&c_v;&c_o;&c_d;&c_a;</ind></trd>
  <trd lng="sk">priestor na vkladanie disku</trd>
</drv>

<drv mrk="disk.bluradia0o">
  <kap>bluradia <tld/>o</kap>
  <snc mrk="disk.bluradia0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="disk.lum0o.KOMP">Lum<tld/>o</ref>
      el tria generacio kun enhaveco de 50 gigabitokoj:
      <ekz>
        Blu-radia <tld/>o havas diametron de 12 centimetroj,
        same kiel DVD kaj KD
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Blu-radia_disko">
          Blu-radia disko</url>, 2010-11-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">disque <ind>blu-ray</ind></trd>
</drv>

<drv mrk="disk.0o.fiksita">
  <kap>fiksita <tld/>o</kap>
  <snc mrk="disk.0o.fiksita.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="disk.0aparato">Diskaparato</ref>
      muntita en komputilon (kontraste al la demeteblaj
      disketoj kaj diskaparatoj).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_n;&c_ch;&c_e;&c_s;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;
    <ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="cs">pevný disk</trd>
  <trd lng="de">Festplatte</trd>
  <trdgrp lng="en">
    <trd>fixed disk</trd>, <trd>hard disk</trd>
  </trdgrp>
  <trd lng="es"><ind>disco</ind> duro</trd>
  <trd lng="fr">disque fixe<klr tip="amb">
  (informatique)</klr></trd>
  <trdgrp lng="hu">
    <trd>merevlemez</trd>,
    <trd>winchester</trd>
  </trdgrp>
  <trd lng="nl">harde <ind>schijf</ind></trd>
  <trd lng="pl">twardy <ind>dysk</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_ch;&c_je;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_y;&c_j;
    &c_d;&c_i;&c_s;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pevn&yacute; <ind>disk</ind></trd>,
    <trd>pevný disk</trd>
  </trdgrp>
</drv>

<drv mrk="disk.kompakta0o">
  <kap>kompakta <tld/>o,
  <var><kap>kompakt<tld/>o</kap></var>,
  <var><kap>k-<tld/>o</kap></var>,
  <var><kap>KD-<tld/>o</kap></var>
  </kap>
  <mlg>KD</mlg>
  <snc mrk="disk.kompakta0o.KOMP">
    <dif>
     <ref tip="super" cel="disk.lum0o.KOMP">Lum<tld/>o</ref>
     el unua generacio kun enhaveco de proksimume 700 megabitokoj:
     <ekz>
       en 1996 aperis nia unua kompakt<tld/>o <ctl>Muzikpluvo</ctl>
       <fnt>
         <aut>Akordo</aut>,
         <vrk><url ref="http://www.tekstoj.nl/akordo/kompaktdiskoj.htm">
         Kompaktaj Diskoj de Akordo</url></vrk>,
         <lok>[2006]</lok>
       </fnt>;
     </ekz>
     <ekz>
       la tuta k-<tld/>o <ctl>En la spegulo</ctl> de la
       bando Persone estas nun senkoste el&scirc;utebla
       <fnt>
         <aut>B. Wennergren</aut>,
         <vrk><url
         ref="http://groups.yahoo.com/group/esperanto-l/message/71047">
         La k-disko...</url></vrk>,
         <lok>sce, 2007-09-25</lok>
       </fnt>;
     </ekz>
     <ekz>
       &scirc;iaj KD-<tld/>oj estis <klr>...</klr> bone a&ccirc;etataj
       <fnt>
         <aut>&Jcirc;omart kaj Nata&scirc;a</aut>,
         <vrk><url ref="http://www.jomart.net/novajxoj.htm">
         Nova&jcirc;oj</url></vrk>,
         <lok>[2007?]</lok>
       </fnt>.
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_k;&c_t;-&c_d;&c_y;&c_s;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>CD</trd>,
    <trd>kompaktní disk</trd>
  </trdgrp>
  <trd lng="de">CD</trd>
  <trd lng="es">disco compacto</trd>
  <trdgrp lng="fr">
    <trd>CD</trd>,
    <trd>disque compact</trd>
  </trdgrp>
  <trd lng="hu">kompaktlemez</trd>
  <trd lng="nl">CD</trd>
  <trdgrp lng="pl">
    <trd>p&lstroke;yta CD</trd>,
    <trd>p&lstroke;yta kompaktowa</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_a;&c_k;&c_t;-&c_d;&c_i;&c_s;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>CD</trd>,
    <trd>kompaktný disk</trd>
  </trdgrp>
</drv>

<drv mrk="disk.lum0o">
  <kap>lum<tld/>o</kap>
  <snc mrk="disk.lum0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="D"/>forma
      <ref tip="super" cel="datum.0portilo">datumportilo</ref>,
      funkcianta per lasera registrado kaj legado de datumoj:
      <ekz>
        eblas registri sur lum<tld/>o &ccirc;ion registrindan
        <fnt>
          <aut>A. Oberndorfer</aut>,
          <vrk>Re: &Ccirc;u UEA forgesos...</vrk>,
          <lok>soc.culture.esperanto, 2003-09-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        en recenzo pri la lum<tld/>o <ctl>PoLimoRFiA
        ArKiteKnia</ctl>
        <klr>...</klr> mi retrovis mian nomon en esperantigo
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk>Psikedela sperto</vrk>,
          <bib>Monato</bib>,
          <lok>2000-03</lok>
        </fnt>;
      </ekz>
      <ekz>
        Belnuba Liu: Gaja lernado de Esperanto, lum<tld/>o por
        Vindozo (Eldonis Xi'an-a Esperanto-Asocio);
      </ekz>
      <ekz>
        la lum<tld/>o bazi&gcirc;as sur la distribua&jcirc;o
        Slackware
        <fnt>
          <lok><url
          ref="http://dmoz.org/World/Esperanto/Komputiloj/Programoj/Linukso/">
          Linukso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas pli a&ubreve; malpli naturfidele a&ubreve;skulti lum<tld/>on
        per du a&ubreve;diletoj
        <fnt>
          <aut>Pejno S.</aut>,
          <vrk><url ref="http://www.esperanto.be/fel/mon/rec/kviv.html">
          Balda&ubreve; &ccirc;i-teatre</url></vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <rim num="1">
      La terminon <ctl>lumdisko</ctl>, imitantan la koncernan
      terminon &ccirc;inan (guang1pan2), proponis Lee Sau Dan.
      Simile, en la Islanda oni nomas &gcirc;in <ctl>geisladiskur</ctl>
      (<ctl>radi-disko</ctl>, t.e. <ctl>laserradia disko</ctl>).
      &Ccirc;i-momente (2005-12-12) Guglo donas 1640 trafojn por &ccirc;i
      tiu termino en la Reto.
    </rim>
    <rim num="2">
      Plej ofte, la kunteksto sufi&ccirc;as por kompreni, &ccirc;u
      <ctl>lumdisko</ctl> estas nurlegebla komputila lumdisko
      a&ubreve; sondisko.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
  <trdgrp lng="cs">
    <trd>CD</trd>,
    <trd>kompaktní disk</trd>
  </trdgrp>
  <trd lng="en">optical disc</trd>
  <trd lng="fr">disque optique</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;zerlemez</trd>,
    <trd>optikai <ind>lemez</ind></trd>
  </trdgrp>
  <trd lng="pl">dysk optyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>CD</trd>,
    <trd>kompaktný disk</trd>
  </trdgrp>
</drv>

<drv mrk="disk.lum0o.ego">
  <kap>lum<tld/>ego,
  <var><kap>DVD-<tld/>o</kap></var>
  </kap>
  <mlg>DVD</mlg>  
  <snc mrk="disk.lum0o.ego.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="disk.lum0o.KOMP">Lum<tld/>o</ref>
      el dua generacio kun enhaveco de 4 &gcirc;is 17 gigabitokoj:
      <ekz>
        se vi havas vian &scirc;atatan filmon en lum<tld/>ego kaj
        volas nepre subtitoli &gcirc;in, anka&ubreve; vi devas
        konverti la filmon al tiu formato
        <fnt>
          <aut>Filmtraduka Teamo Fulmo</aut>,
          <vrk><url
          ref="http://www.esperanto.de/dej/asocio/nr04_04/subtitoloj.php">
          Kiel Esperantigi vian &scirc;atatan filmon?</url></vrk>,
          <lok>2005-09-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesis dividi la filmon je 2 DVD-<tld/>oj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperantoestas.blogspot.com/">
          Esperanto estas...</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">DVD-<ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
  <trdgrp lng="cs">
    <trd>CD</trd>,
    <trd>kompaktní disk</trd>
  </trdgrp>
  <trd lng="de">DVD</trd>
  <trd lng="en">DVD</trd>
  <trd lng="fr">DVD</trd>
  <trdgrp lng="hu">
    <trd>DVD</trd>,
    <trd>videolemez</trd>
  </trdgrp>
  <trd lng="pl">p&lstroke;yta DVD</trd>
  <trdgrp lng="sk">
    <trd>CD</trd>,
    <trd>kompaktný disk</trd>
  </trdgrp>
</drv>

<drv mrk="disk.0o.magneta">
  <kap>magneta <tld/>o</kap>
  <snc mrk="disk.magneta0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="datum.0portilo">Datumportilo</ref>
      en formo de <tld/>o tegita per magnetebla tavolo por
      registrado kaj legado de datumoj:
      <ekz>
        skribi, registri datumojn sur magnetan <tld/>on;
      </ekz>
      <ekz>
        legi, vi&scirc;i datumojn de magneta <tld/>o.
      </ekz>
    </dif>
    <ref tip="sub" cel="disk.0eto">disketo</ref>
    <ref tip="vid" cel="disk.0o.fiksita">fiksita disko</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_n;&c_y; <ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
  <trd lng="en">magnetic disk</trd>
  <trd lng="fr">disque magn&eacute;tique</trd>
  <trd lng="hu">m&aacute;gneslemez</trd>  
  <trd lng="pl">dysk magnetyczny</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_n;&c_y;&c_j; &c_d;&c_i;&c_s;&c_k;</trd>
  <trd lng="sk">magnetick&yacute; <ind>disk</ind></trd>
</drv>

<drv mrk="disk.sfer0o">
  <kap>sfer<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="disk.sfer0o.MAT">
    <ref tip="dif" cel="tavol.glob0o">globtavolo</ref>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_r;&c_a;&c_v;&c_y;
  <ind>&c_p;&c_l;&c_a;&c_s;&c_t;</ind></trd>
  <trd lng="de">Kugelschicht</trd>
  <trd lng="en">spherical <ind>layer</ind></trd>
  <trd lng="fr">segment sph&eacute;rique &agrave; deux bases</trd>
  <trd lng="hu">g&ouml;mbr&eacute;teg</trd>  
  <trd lng="pl">warstwa kuli</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_s;&c_l;&c_o;&c_j;</ind></trd>
</drv>

<drv mrk="disk.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="disk.sub0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Ccirc;iu el la membroj de
      <ref cel="part.dis0igi.KOMP">dispartigita</ref> <tld/>o
      (precipe <ref cel="disk.0o.fiksita">fiksita <tld/>o</ref>),
      &ccirc;iu kun sia aparta
      <ref tip="prt" cel="dosier.0sistemo">dosiersistemo</ref>:
      <ekz>
        tiu programo kreos du dosierojn &ccirc;e
        via Vindoza sub<tld/>o por Linukso
        <fnt>
          <lok>
          <url
          ref="http://www.task.gda.pl/pub/linux/Mandrake/current/i586/doc/install/eo/install.htm">
          Instalada Instrukcio &#8208; Linuks-Mandrejko
          7.2</url></lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l; <klr tip="amb">(&c_d;&c_y;&c_s;&c_k;&c_a;&c_v;&c_y;)</klr></trd>
  <trd lng="de">Partition <klr tip="amb"> (einer Festplatte)</klr></trd>
  <trd lng="en">a <ind>partition</ind> <klr>(of a disk)</klr></trd>
  <trdgrp lng="fr">
    <trd>partition<klr tip="amb"> (de disque informatique)</klr></trd>
  </trdgrp>
  <trd lng="hu">part&iacute;ci&oacute;</trd>
  <trd lng="pl">partycja <ind>dysku</ind></trd>  
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l; <klr>(&c_d;&c_i;&c_s;&c_k;&c_a;)</klr></trd>
</drv>

<drv mrk="disk.0turnilo">
  <kap><tld/>turnilo</kap>
  <snc mrk="disk.0turnilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Organo turnanta disketon kaj stiranta la kapetojn por
      skribi datumojn sur la diskon a&ubreve; legi datumojn de sur
      la disketo.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_k;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="cs">štěrbina v diskové aparatuře pro vkládání disku</trd>
  <trd lng="en">disk unit</trd>
  <trd lng="es">unidad de <ind>disco</ind></trd>
  <trd lng="fr">lecteur</trd>
  <trd lng="hu">lemezmeghajt&oacute;</trd>
  <trd lng="pl">nap&eogonek;d<klr> (dyskietek, CD)</klr></trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_k;&c_o;&c_v;&c_o;&c_d;</trd>
  <trd lng="sk">priestor na vkladanie disku</trd>
</drv>

<drv mrk="disk.0aparato">
  <kap><tld/>aparato</kap>
  <snc mrk="disk.0aparato.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="datum.0portilo">Datumportilo</ref>
      konsistanta el <ref cel="disk.0o.magneta">magneta disko</ref>
      (a&ubreve; pluraj tiaj diskoj samaksaj), la lega-skribaj
      kapetoj
      kaj ceteraj partoj me&hcirc;anikaj kaj elektronikaj,
      kune muntitaj en metala skatolo..
    </dif>
    <rim>
      Normale komputilo havas diskaparaton kiel sian
      <ref tip="sub" cel="disk.0o.fiksita">fiksitan diskon</ref>;
      sed ekzistas anka&ubreve; demeteblaj diskaparatoj.
    </rim>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;
  <ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
  <trd lng="cs">disková aparatura PC</trd>
  <trd lng="en">hard disk</trd>
  <trd lng="es"><ind>disco</ind> duro</trd>
  <trd lng="fr">disque dur</trd>
  <trd lng="hu">lemezegys&eacute;g</trd>
  <trd lng="pl">dysk</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j;
  &c_d;&c_i;&c_s;&c_k;</trd>
  <trd lng="sk">disková jednotka</trd>
</drv>

<drv mrk="disk.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="disk.flug0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Diskoforma ludilo plej ofte farita el plasto.
    </dif>
  </snc>
  <trd lng="en">frisbee</trd>
  <trd lng="es"><ind>disco</ind> volador</trd>
  <trd lng="fr">ultimate</trd>
</drv>
