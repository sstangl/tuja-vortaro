<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fenol.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>fenol</rad>/o
</kap>
<drv mrk="fenol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>6</sub>O a&ubreve; C<sub>6</sub>H<sub>5</sub>(OH).
      Solida korpo, senkolora, de akra, malagrabla odoro,
      kristali&gcirc;anta en belajn pintojn kaj ekstraktata el karba gudro:
      <ekz><tld/>o estas bona desinfektilo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_n;&c_o;&c_l;</trd>
  <trd lng="bg">&c_F;&c_je;&c_n;&c_o;&c_l;</trd>
  <trd lng="ca">fenol</trd>
  <trd lng="cs">kyselina karbolová</trd>
  <trdgrp lng="de">
     <trd>Phenol</trd>,
     <trd>Karbols&auml;ure</trd>
  </trdgrp>
  <trd lng="en">phenol</trd>
  <trd lng="es">fenol</trd>
  <trd lng="fr">ph&eacute;nol</trd>
  <trdgrp lng="hu">
    <trd>fenol</trd>,
    <trd>karbolsav</trd>
  </trdgrp>
  <trd lng="nl">fenol</trd>
  <trd lng="pt">fenol</trd>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_o;&c_l;</trd>
  <trd lng="sk">kyselina karbolová</trd>
</drv>
</art>
