<kap>
  <ofc>1</ofc>
  <rad>kantat</rad>/o
</kap>
<drv mrk="kantat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      kun lirika kanta&jcirc;o por malmultaj solkantistoj
      kaj banda a&ubreve; orkestra akompano.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">cantata</trd>
  <trd lng="cs">kantáta</trd>
  <trd lng="de">Kantate</trd>
  <trd lng="en">cantata</trd>
  <trd lng="es">cantata</trd>
  <trdgrp lng="fr">
    <trd>cantate</trd>
  </trdgrp>
  <trd lng="hu">kant&aacute;ta</trd>
  <trd lng="nl">cantate</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_t;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">kantáta</trd>
</drv>
</art>
