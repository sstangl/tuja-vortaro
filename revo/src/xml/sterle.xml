<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sterle.xml,v 1.13 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sterled</rad>/o
</kap>
<drv mrk="sterle.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="lst" cel="acipenser.0o" 
      lst="voko:fi&scirc;oj">acipensero</ref>
      (<trd lng="la">Acipenser ruthenus</trd>), kun malgranda mallar&gcirc;a
      korpo &gcirc;is maksimume 1&nbsp;m longa,
      kun kapo pli longa kaj maldika ol <ref
      cel="sturg.0o">sturgo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_je;&c_r;&c_l;&c_ja;&c_d;&c_z;&c_mol;</trd>
  <trd lng="cs">jeseter malý</trd>
  <trd lng="de">Sterlet</trd>
  <trd lng="fr">esturgeon sterlet</trd>
  <trd lng="hu">kecsege</trd>
  <trd lng="nl">sterlet</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_l;&c_ja;&c_d;&c_mol;</trd>
  <trd lng="sk">jeseter malý</trd>
</drv>
</art>
