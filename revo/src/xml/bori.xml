<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bori.xml,v 1.17 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bori</rad>/o
</kap>
<drv mrk="bori.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Bh</mlg>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="107">elemento</ref> kun
      atomnumero 107, atompezo 262,1229; nomita la&ubreve; la dana
      fizikisto Nils Bohr; <ref tip="super"
      cel="urani.trans0o">transuranio</ref>, kies
      &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj.
    </dif>
    <rim>
      IUPAC en 1997 jaro findecidis pri la nomo borio, 
      anta&ubreve;e estis uzata provizore la nomo unnilseptio 
      (science Unnilseptium).
    </rim>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="cs">bór</trd>
  <trd lng="de">Bohrium</trd>
  <trd lng="fa">&a_b;&a_w;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">bohrium</trd>
  <trd lng="hu">bohrium</trd>
  <trd lng="id">bohrium</trd>
  <trd lng="la">Bohrium</trd>
  <trd lng="nl">bohrium</trd>
  <trd lng="pl">Bohr</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>b&oacute;rium</trd>,
    <trd>bórium</trd>
  </trdgrp>
  <trd lng="sv">bohrium</trd>
</drv>
</art>
