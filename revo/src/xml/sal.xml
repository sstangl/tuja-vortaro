<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sal.xml,v 1.36 2015/07/04 08:37:43 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>sal</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="sal.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="sal.0o.natriaKlorido">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Ordinara nomo de la natria klorido, uzata
          interalie kiel <ref tip="super" cel="spic.0o">spico</ref>
          por gustigi man&gcirc;a&jcirc;ojn.
        </dif>
      </snc>
      <snc mrk="sal.0o.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Kombina&jcirc;o de acido kun bazo.
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_o;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_o;&c_l;</trd>
  <trd lng="ca">sal</trd>
  <trd lng="cs">kuchyňská sůl</trd>
  <trd lng="de">Salz</trd>
  <trd lng="en">salt</trd>
  <trd lng="es">sal</trd>
  <trd lng="fr">sel</trd>
  <trd lng="he">&mem;&lamed;&het;</trd>
  <trd lng="hu">s&oacute;</trd>
  <trd lng="id">garam</trd>
  <trd lng="it">sale</trd>
  <trd lng="nl">zout<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">s&oacute;l</trd>
  <trd lng="pt">sal</trd>
  <trd lng="ro">sare</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">kuchynská soľ</trd>
  <trd lng="sv">salt</trd>
</drv>

    <drv mrk="sal.0a">
      <kap><tld/>a</kap>
      <snc mrk="sal.0a.KUI">
        <dif>
          <tld lit="S"/><ref tip="lst" cel="gust.0o.eco"
          lst="voko:gustoj">gusta</ref>.
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>
  <trd lng="ca">salat</trd>
  <trdgrp lng="cs">
    <trd>osolený</trd>,
    <trd>slaný</trd>
  </trdgrp>
  <trd lng="de">salzig</trd>
  <trd lng="en">salt</trd>
  <trd lng="es">salado</trd>
  <trd lng="fr">sal&eacute;</trd>
  <trd lng="he">&mem;&lamed;&vav;&het;</trd>
  <trd lng="hu">s&oacute;s</trd>
  <trd lng="id">asin</trd>
  <trd lng="it">salato</trd>
  <trd lng="nl">zout<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">s&lstroke;ony</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>slaný</trd>,
    <trd>soľný</trd>
  </trdgrp>
  <trd lng="sv">salt<klr tip="ind"> (adj.)</klr></trd>
</drv>

    <drv mrk="sal.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="sal.0i.KUI">
        <dif>
          Aldoni <tld/>on:
          <ekz>
            pli bone ne <tld/>i ol <tld/>i tro multe <klr>(ne troigu)
            </klr>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">salar</trd>
  <trd lng="cs">solit</trd>
  <trd lng="de">salzen</trd>
  <trdgrp lng="es">
         <trd>salar</trd>,
         <trd>sazonar</trd>
      </trdgrp>
  <trd lng="fr">saler</trd>
  <trd lng="he">&lamed;&he;<ind>&mem;&lamed;&yod;&het;</ind></trd>
  <trd lng="hu">s&oacute;z</trd>
  <trd lng="id">[<ind>garam</ind>] menggarami</trd>
  <trd lng="it">salare</trd>
  <trd lng="nl">zouten</trd>
  <trd lng="pl">soli&cacute;</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>posoliť</trd>,
    <trd>soliť</trd>
  </trdgrp>
  <trd lng="sv">salta</trd>
</drv>

    <drv mrk="sal.ne0a">
      <kap>ne<tld/>a</kap>
      <snc mrk="sal.ne0a.KUI">
        <dif>
          Kiu ne estas <tld/>a, ne <tld/>gusta:
          <ekz>
            piranjoj vivas en <ind>ne<tld/>a akvo</ind>
            <klr>(vidu <ref tip="vid" cel="akv.mar0o">marakvo</ref>)
            </klr>;
            <trd lng="ca">aigua dol&ccedil;a</trd>
            <trd lng="de">S&uuml;&szlig;wasser</trd>
            <trdgrp lng="en">
              <trd>fresh water</trd>,
              <trd>sweet water</trd>
            </trdgrp>
            <trd lng="id"><ind>air</ind> tawar</trd>
            <trd lng="pl">s&lstroke;odka woda</trd>
            <trd lng="ru">
            &c_p;&c_r;&c_je;&c_s;&c_n;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;
            </trd>
            <trd lng="sv">s&ouml;tvatten</trd>
          </ekz><ekz>
            la jaro 2003a estas deklarita de UNO kiel la
            Jaro de ne<tld/>a akvo
            <fnt><lok>Radio Polonia</lok></fnt>
            <klr>(fakte, &ccirc;i-okaze pli konvenus paroli pri
              <ref cel="akv.trink0o">trinkakvo</ref>)</klr>;
          </ekz><ekz>
            la maljunulo gustumis unu &gcirc;ia&ubreve;zi-on, kies
            ne<tld/>a far&ccirc;o malpla&ccirc;is al li
            <fnt>
              <aut>Chi Zijian</aut>,
              <vrk><url
     ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd99-02-27.html">
                  Avo kaj nepo</url></vrk>
              <lok>1999</lok>
            </fnt>;
          </ekz><ekz>
            se la <tld/>o fari&gcirc;is ne<tld/>a, kiel vi rebonigos
            &gcirc;in?
            <fnt><bib>NT</bib>. <lok>&Mar; 12:17</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">ins&iacute;pid</trd>
      <trdgrp lng="fr">
        <trd>insipide</trd>,
        <trd>sans sel</trd>
      </trdgrp>
      <trd lng="id">hambar</trd>
      <trd lng="it">insipido</trd>
      <trdgrp lng="nl">
        <trd>zoutloos</trd>,
        <trd>zoet<klr tip="ind"> (zoutloos)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_s;&c_o;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sal.mar0o">
      <kap>mar<tld/>o</kap>
      <snc mrk="sal.mar0o.KUI">
        <dif>
          <ref tip="super" cel="sal.0o.natriaKlorido"><tld
          lit="S"/>o</ref>
          ekstraktata el mara akvo per elvaporigo.
        </dif>
      </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_l;&c_mol;</trd>
  <trd lng="ca"><ind>sal</ind> marina</trd>
  <trd lng="cs">mořská sůl</trd>
  <trd lng="de">Meersalz</trd>
  <trd lng="es">sal marina</trd>
  <trd lng="fr">sel de mer</trd>
  <trd lng="he">&mem;&lamed;&het; &yod;&fmem;</trd>
  <trd lng="hu">tengeri <ind>s&oacute;</ind></trd>
  <trd lng="id"><ind>garam</ind> laut</trd>
  <trd lng="it"><ind>sale</ind> marino</trd>
  <trd lng="nl">zeezout</trd>
  <trd lng="pl"><ind>s&oacute;l</ind> morska</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">morská soľ</trd>
  <trd lng="sv">havssalt</trd>
</drv>
    
    <drv mrk="sal.sxton0o">
      <kap>&scirc;ton<tld/>o,
      <var><kap>min<tld/>o</kap></var>,
      <var><kap>rok<tld/>o</kap></var>
      </kap>
      <snc mrk="sal.sxton0o.KUI">
        <dif>
          <ref tip="super" cel="sal.0o.natriaKlorido"><tld
          lit="S"/>o</ref>
          ekstraktata el tero, ofte malpura - kun sablo 
          kaj &scirc;tonpecetoj.
        </dif>
      </snc>
      <trd lng="ca"><ind>sal</ind> gemma</trd>
      <trd lng="fr">sel gemme</trd>
      <trd lng="it">salgemma</trd>
      <trdgrp lng="nl">
        <trd>steenzout</trd>,
        <trd>mijnzout</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd><ind>s&oacute;l</ind> kamienna</trd>,
        <trd><ind>s&oacute;l</ind> kopalna</trd>,
      </trdgrp>
    </drv>
  </art>
