<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: leopar.xml,v 1.20 2014/11/09 10:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>leopard</rad>/o
</kap>
<drv mrk="leopar.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">Specio</ref> de <ref tip="lst" 
cel="panter.0o.genro" lst="voko:rabobestoj">pantero</ref>
      (<trd lng="la">Panther pardus</trd>), kun nigre makulita felo.
    </dif>
    <rim>La <tld/>on oni nomas anka&ubreve;
      <ref tip="sin" cel="panter.0o.leopardo">pantero</ref>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_a;&c_p;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_s;</trd>
  </trdgrp>
  <trd lng="br">loupard</trd>
  <trd lng="ca">lleopard</trd>
  <trd lng="de">Leopard</trd>
  <trd lng="el">&lambda;&epsilon;&omikron;&pi;&alfa_ton;&rho;&delta;&alfa;&lambda;&eta;</trd>
  <trd lng="en">leopard</trd>
  <trd lng="es">leopardo</trd>
  <trd lng="fr">l&eacute;opard</trd>
  <trd lng="hu">leop&aacute;rd</trd>
  <trd lng="id"><ind>macan</ind> tutul</trd>
  <trdgrp lng="nl">
    <trd>luipaard</trd>,
    <trd>panter</trd>
  </trdgrp>
  <trd lng="pt">leopardo</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_o;&c_p;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_s;</trd>
  </trdgrp>
  <trd lng="sv">leopard</trd>
</drv>
</art>
