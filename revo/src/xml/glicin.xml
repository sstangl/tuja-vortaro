    <trd>glicin</trd>,
    <trd>glikokoll</trd>,
    <trd>enyvcukor</trd>
  </trdgrp>
  <trd lng="nl">glycine</trd>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_c;&c_i;&c_n;</trd>
  <trd lng="sk">hlísta</trd>
</drv>
