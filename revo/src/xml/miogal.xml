<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miogal.xml,v 1.17 2015/04/18 05:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>miogal</rad>/o
</kap>
<drv mrk="miogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="miogal.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref cel="mangx.insekto0ulo"
      lst="voko:insektoman&gcirc;uloj">Insektoman&gcirc;ulo</ref>
      kun <ref tip="prt" cel="rostr.0o">rostro</ref> kaj 
      <ref cel="nagx.0i.KOMUNE">na&gcirc;</ref>ha&ubreve;to inter la piedfingroj,
      vivanta en e&ubreve;ropaj kaj aziaj <ref
      cel="river.0o.GEOG">riveroj</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="talped.0oj">talpedoj</ref>
      (<trd lng="la">Desmana</trd>), 
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_h;&c_u;&c_l;&c_ja;</trd>
  <trd lng="ca">almesquera</trd>
  <trdgrp lng="de">
    <trd>Bisamr&uuml;ssler</trd>,
    <trd>Desman</trd>
  </trdgrp>
  <trd lng="en">desman</trd>
  <trd lng="es">desm&aacute;n</trd>
  <trd lng="fr">desman</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;zsmacick&aacute;ny</trd>,
    <trd>dezm&aacute;n</trd>
  </trdgrp>
  <trd lng="nl">muscusspitsmuis</trd>
  <trd lng="ru">&c_v;&c_y;&c_h;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
