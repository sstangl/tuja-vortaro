<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: balkasx.xml,v 1.6 2011/03/27 20:10:09 revo Exp $">
    <kap>
      <rad>Balka&scirc;</rad>/o
    </kap>

    <drv mrk="balkasx.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Neprofunda senelflua lago en sudokrienta parto de Kaza&hcirc;ujo.
          &Gcirc;i estas salakva oriente, kaj nesalakva okcidente.
	</dif>
      </snc>
      <trd lng="be">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
      <trd lng="bg">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
      <trd lng="de">Balkasch-See</trd>
      <trdgrp lng="en">
        <trd>Balkhash</trd>, <trd>Balqash</trd>
      </trdgrp>
      <trd lng="fr">Balkhach</trd>
      <trd lng="hu">Balkas-t&oacute;</trd> 
      <trd lng="ru">&c_B;&c_a;&c_l;&c_h;&c_a;&c_sh;</trd>
    </drv>

  </art>

