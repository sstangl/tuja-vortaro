<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tromb.xml,v 1.19 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tromb</rad>/o
</kap>
<drv mrk="tromb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tromb.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Turni&gcirc;a akvokolono levita el la maro fare de turnovento
      kaj rapide anta&ubreve;eniranta kun terura detrua forto; akva <ref
      tip="vid" cel="ciklon.0o.MET">ciklono</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="vent">kirloventego</ref>,
      <ref cel="uragan.0o.MET">uragano</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_r;&c_ch;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_r;&c_a;&c_s;&c_l;&c_u;&c_p;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>m&agrave;nega</trd>,
    <trd><ind>tromba</ind> marina</trd>
  </trdgrp>
  <trd lng="cs">trombus</trd>
  <trd lng="de">Trombe</trd>
  <trdgrp lng="en">
    <trd>waterspout</trd>,
    <trd>tornado over water</trd>,
    <trd>column of water</trd>
  </trdgrp>
  <trd lng="fr">trombe</trd>
  <trd lng="hu">v&iacute;zt&ouml;lcs&eacute;r</trd>
  <trd lng="nl">waterhoos</trd>
  <trd lng="pl">tr&aogonek;ba wodna</trd>
  <trdgrp lng="pt">
    <trd>tromba d'&aacute;gua</trd>,
    <trd>tromba</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_je;&c_r;&c_ch;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>morský vír</trd>,
    <trd>tromba</trd>
  </trdgrp>
</drv>
</art>
