<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: feston.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>feston</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feston.0o">
  <kap><tld/>o</kap>
  <snc mrk="feston.0o.KOMUNE">
    <dif>
      Ornama borderanta
      <ref tip="vid" cel="girlan.0o.KOMUNE">girlando</ref>
      el kunplektitaj floroj, folioj, bran&ccirc;etoj.
    </dif>
    <trdgrp lng="de">
     <trd>Girlande</trd>,
     <trd>Blumengebinde</trd>,
     <trd>Laubgebinde</trd>,
     <trd>Blumengeh&auml;nge</trd></trdgrp>
     <trdgrp lng="hu">
       <trd>vir&aacute;gf&uuml;z&eacute;r</trd>,
       <trd>lombf&uuml;z&eacute;r</trd>
     </trdgrp>
  </snc>
  <snc mrk="feston.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Ornama&jcirc;o imitanta arkoformajn girlandojn.  
    </dif>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Ornamentgeh&auml;nge</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>f&uuml;z&eacute;rd&iacute;sz</trd>,
      <trd>feszton</trd></trdgrp>
  </snc>
  <snc mrk="feston.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama&jcirc;o de rando de &scirc;tofo formita per regulaj arkoformaj
      dentetoj, ordinare broditaj:
      <ekz>
        elegante <tld/>ita &ccirc;emizo.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Zierb&ouml;gen</trd></trdgrp>
    <trd lng="hu">slingel&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_ib;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>girlanda</trd>,
    <trd>věncoví</trd>
  </trdgrp>
  <trd lng="en">festoon</trd>
  <trd lng="fr">feston</trd>
  <trdgrp lng="pt">
    <trd>fest&atilde;o</trd>,
    <trd>grinalda</trd>,
    <trd>bambolim</trd>,
    <trd>recorte</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_i;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>girlanda</trd>,
    <trd>kvetinový záves</trd>
  </trdgrp>
  <trd lng="sv">festong</trd>
</drv>

<drv mrk="feston.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="feston.0i.TEKS">
    <dif>
      Ornami per <tld/>o,
      randi kiel <tld/>o;
      <ref tip="vid" cel="border.0i">borderi</ref>:
      <ekz>
        dank' al tiu, kies arkon floroj <tld/>as
        <klr>(la dio Amoro)</klr>
        <fnt>
          <aut>Kalidasa, trad. K. Kalocsay</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr66/amaj/hindujo/printempo.html">
          La printempo</url></vrk>,
          <lok>la nica literatura revuo, 1961-07 &gcirc;is 08 
          (36), p. 204a-207a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">věnčit</trd>
  <trd lng="fr">festonner</trd>
  <trd lng="sk">venčiť</trd>
</drv>
</art>
