<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skulptur.xml,v 1.5 2014/06/17 16:10:15 revo Exp $">
<kap>
  <rad>skulptur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skulptur.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="skulptur.0o.skulptarto">
    <ref tip="dif" cel="art.skulpt0o.BELA">skulptarto</ref>
    <ekz>
      la <tld lit="S"/>o, la Pentrarto kaj la Ar&hcirc;itekturo
      <fnt><bib>Fab1</bib>, <lok>kupra porko</lok></fnt>.
    </ekz>
    <trd lng="nl">sculptuur<klr tip="ind"> (de
    beeldhouwkunst)</klr></trd>
  </snc>
  <snc mrk="skulptur.0o.skulptajxo">
    <ref tip="dif" cel="skulpt.0ajxo.BELA">skulpta&jcirc;o</ref>
    <ekz>
      la <tld/>oj en la besto&gcirc;ardeno
      <fnt>
      <vrk><url
      ref="http://www.esperanto.de/org/lv.berlin/informilo/kaljul2001.htm">Berlina
      Kalendaro</url></vrk>,
      <lok>2001</lok>
      </fnt>.
    </ekz>
    <trd lng="nl">sculptuur<klr tip="ind"> (het
    beeldhouwwerk)</klr></trd>
  </snc>
  <trd lng="de">Skulptur</trd>
  <trd lng="fr">sculpture</trd>
  </drv>
</art>
