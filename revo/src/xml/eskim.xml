<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eskim.xml,v 1.13 2015/07/04 08:37:33 revo Exp $">
<kap><rad>eskim</rad>/o <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="eskim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de popolo lo&gcirc;anta en Arkto, precipe en Grenlando, Norda
      Kanado,
      Alasko kaj orienta Siberio:
      <ekz>
        man&gcirc;kutimo alta je graso <klr>...</klr> ligi&gcirc;as kun
        plia
        ofteco de
        arteriosklerozo kaj korinfarkto, kvankam <tld/>oj estas esceptoj
        al tiu regulo
        <fnt><aut>Krys Ungar</aut>: <vrk>Vivu vigle kaj vegetare!</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 7a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="inuit.0o">inuito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;</trd>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;</trd>
  <trd lng="cs">Eskymák</trd>
  <trd lng="de">Eskimo</trd>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trd lng="es">esquimal</trd>
  <trdgrp lng="fr">
    <trd>Esquimau<klr> (subst.)</klr></trd>,
    <trd>Eskimo<klr> (subst.)</klr></trd>,
    <trd>Inuit<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">eszkim&oacute;</trd>
  <trd lng="nl">eskimo</trd>
  <trd lng="pl">Eskimos</trd>
  <trdgrp lng="pt">
    <trd>esquim&oacute;</trd>,
    <trd>esquimau</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;</trd>
  <trd lng="sk">Eskimák</trd>
</drv>

<drv mrk="eskim.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="inuit.0a">inuita</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;&c_k;&c_i;</trd>
  <trd lng="es">esquimal</trd>
  <trd lng="de">Eskimo...</trd>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>esquimau<klr> (adj.)</klr></trd>,
    <trd>eskimo<klr> (adj.)</klr></trd>,
    <trd>inuit<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">eszkim&oacute;</trd>
  <trd lng="nl">eskimo-</trd>
  <trd lng="pl">eskimoski</trd>
  <trd
  lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
