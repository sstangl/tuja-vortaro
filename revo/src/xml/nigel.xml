<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nigel.xml,v 1.20 2015/09/21 09:10:28 revo Exp $">
<kap>
  <rad>nigel</rad>/o
</kap>
<drv mrk="nigel.0o">
  <kap><tld/>o</kap>
  <snc mrk="nigel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Nigella</trd>).
    </dif>
    <trdgrp lng="hu">
      <trd>katicavir&aacute;g</trd>,
      <trd>kandilla</trd>
    </trdgrp>
    <trd lng="pl">czarnuszka</trd>
  </snc>
  <snc mrk="nigel.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> el la
      nigraj semoj
      de unu specio (<trd lng="la">Nigella sativa)</trd>) de
      <ref tip="malprt" cel="nigel.0o.BOT"><tld/>o<sncref/></ref>:
      <ekz>
        &ccirc;ar ne per fera dra&scirc;ilo oni dra&scirc;as <tld/>on
        kaj oni ne rulas radon de &ccirc;aro sur
        <ref tip="vid" cel="kumin.0o.spico">kumino</ref>;
        sed per bastono oni elbatas <tld/>on kaj kuminon
        per vergo
        <fnt><bib>MT</bib>, <lok>&Jes; 28:27</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en"><trd>black cumin</trd>, <trd>kalonji</trd></trdgrp>
    <trd lng="hu">feketek&ouml;m&eacute;ny</trd>
  </snc>
  <trd lng="cs">&ccaron;ernucha</trd>
  <trdgrp lng="de">
    <trd>Schwarzk&uuml;mmel</trd>,
    <trd>Jungfer im Gr&uuml;nen</trd>
  </trdgrp>
  <trd lng="en">nigella</trd>
  <trd lng="fr">nigelle</trd>
  <trd lng="nl">juffertje in 't groen</trd>
  <trd lng="pt">nigela</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>&ccaron;ernu&scaron;ka</trd>,
    <trd>&ccaron;ierna rasca</trd>
  </trdgrp>
</drv>
</art>
