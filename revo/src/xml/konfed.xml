  <kap>
    <rad>konfeder</rad>/i
  </kap>
  <drv mrk="konfed.0i">
    <kap><tld/>i</kap>
    <uzo tip="fak">POL</uzo>
    <snc>
      <ref tip="dif" cel="konfed1.0i">konfederacii</ref>
    </snc>
  <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>,
      <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_a;&c_c;&c_c;&c_a; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
    </trdgrp>
  <trd lng="ca">confederar</trd>
  <trd lng="cs">vytvořit konfederaci</trd>
  <trdgrp lng="fr">
      <trd>conf&eacute;d&eacute;rer</trd>,
      <trd>unir en une <ind>conf&eacute;d&eacute;ration</ind></trd>
    </trdgrp>
  <trd lng="nl">tot een <ind>confederatie</ind> verenigen</trd>
  <trd lng="pt">confederar</trd>
  <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_t;&c_mol; <ind>&c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</ind></trd>,
      <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_v; <ind>&c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</ind></trd>
    </trdgrp>
  <trd lng="sk">vytvoriť konfederáciu</trd>
