<vortaro>
<art mrk="$Id: kapsta.xml,v 1.11 2011/12/07 23:10:11 revo Exp $">
<kap>
  <rad>kapstan</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kapsta.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Vertikala vin&ccirc;o kun horizontalaj stangoj:
      <ekz>
        oni levas la ankron de &scirc;ipo per <tld/>o a&ubreve; vindaso.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="krik.0o">kriko</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="vincx.0o">vin&ccirc;o</ref>,
      <ref cel="vindas.0o">vindaso</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">argue</trd>
  <trd lng="de">Spill</trd>
  <trdgrp lng="en">
    <trd>capstan</trd>,
    <trd>winding gear</trd>
  </trdgrp>
  <trd lng="es">cabrestante</trd>
  <trd lng="fr">cabestan</trd>
  <trdgrp lng="hu">
    <trd>horgonycs&ouml;rl&odblac;</trd>,
    <trd>gugora</trd>
  </trdgrp>
  <trd lng="nl">kaapstander</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_b;&c_je;&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_sh;&c_p;&c_i;&c_l;&c_mol; <klr>(&c_v;&c_i;&c_d; &c_l;&c_je;&c_b;&c_jo;&c_d;&c_k;&c_i;)</klr></trd>
  </trdgrp>
</drv>
</art>
