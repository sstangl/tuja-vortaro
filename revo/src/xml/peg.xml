    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdhvu8.htm">
           Birdonomoj en Esperanto por Hejma Vortaro</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la <ref tip="malprt"
      cel="peged.0oj">pegedoj</ref> (<trd lng="la">Dendrocopos</trd>).
    </dif>
    <trd lng="ca">picot garser</trd>
    <trd lng="de">Buntspecht</trd>
    <trd lng="en">spotted <ind>woodpecker</ind></trd>
    <trd lng="fr">pic<klr> (&eacute;peiche)</klr></trd>
    <trd lng="hu">fakop&aacute;ncs</trd>
    <trd lng="pl">dzi&eogonek;cio&lstroke;</trd>
    <trd lng="pt">pica-pau malhado<klr> (grande)</klr></trd>
    <trd lng="sv">hack<ind>spett</ind></trd>
  </snc>
</drv>

