<kap>
  <rad>agorafobi</rad>/o
</kap>
<drv mrk="agoraf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Mensa <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> karakterizata per timo al
      malplena spaco.
    </dif>
    <refgrp tip="vid">
      <ref cel="klostr1.0o">klostrofobio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_r;&c_a;&c_f;&c_o;&c_b;&c_ib;&c_ja;</trd>
  <trd lng="ca">agoraf&ograve;bia</trd>
  <trd lng="cs">agorafobie</trd>
  <trdgrp lng="de">
     <trd>Platzangst</trd>,
     <trd>Agoraphobie<klr> [fak]</klr></trd>,
     <trd>Angst vor gro&szlig;en Pl&auml;tzen</trd>
  </trdgrp>
  <trd lng="en">agoraphobia</trd>
  <trd lng="es">agorafobia</trd>
  <trd lng="fr">agoraphobie</trd>
  <trd lng="hu">t&eacute;riszony</trd>
  <trd lng="id">agorafobia</trd>
  <trd lng="it">agorafobia</trd>
  <trdgrp lng="nl">
    <trd>pleinvrees</trd>,
    <trd>agorafobie</trd>
  </trdgrp>
  <trd lng="pt">agorafobia</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_r;&c_a;&c_f;&c_o;&c_b;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>agoraf&oacute;bia</trd>,
    <trd>agoraf√≥bia <klr>(lek.)</klr></trd>,
    <trd>strach z priestoru</trd>
  </trdgrp>
</drv>
</art>
