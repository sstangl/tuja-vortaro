<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: andant.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>andant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="andant.0o">
  <kap><tld/>o</kap>
  <snc mrk="andant.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzik-verko a&ubreve; -movimento, kies movo estas meze rapida:
      <ekz>
        &scirc;i pruvas nesufi&ccirc;an guston ludante kun tia emfazo
        tiun tiel simplan <tld/>on
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_e;</trd>
  <trd lng="cs">skladba ve středním tempu</trd>
  <trd lng="en">andante</trd>
  <trd lng="es">andante</trd>
  <trd lng="fr">andante<klr> (subst.)</klr></trd>
  <trd lng="hu">andante</trd>
  <trd lng="it">andante</trd>
  <trd lng="nl">andante</trd>
  <trd lng="ru">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>andante</trd>,
    <trd>pomalá časť skladby</trd>
  </trdgrp>
</drv>

<drv mrk="andant.0e">
  <kap><tld/>e</kap>
  <snc mrk="andant.0e.MUZ">
    <dif>
      Mezrapide.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>andante</trd>,
    <trd>středním tempem</trd>
  </trdgrp>
  <trd lng="en">andante</trd>
  <trd lng="es">andante</trd>
  <trd lng="fr">andante</trd>
  <trd lng="hu">andante</trd>
  <trd lng="it">andante</trd>
  <trd lng="nl">andante</trd>
  <trd lng="ru">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>andante</trd>,
    <trd>pomalá časť skladby</trd>
  </trdgrp>
</drv>
</art>
