<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tamias.xml,v 1.8 2014/11/09 04:10:37 revo Exp $">
    <kap><rad>tamias</rad>/o</kap>

    <drv mrk="tamias.0o">
      <kap><tld/>o <fnt><bib>NePiv</bib></fnt></kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>
          <ref tip="lst" cel="rongx.0ulo"
          lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref>
          vivanta sur arboj, kun granda vosto kaj kvin la&ubreve;longaj 
          <ref tip="prt" cel="stri.0o">strioj</ref>
          sur la dorso;
          <ref tip="lst" cel="genr.0o.BIO"
          lst="voko:zoologiaj_genroj">genro</ref> el la familio
          <ref tip="malprt" cel="sciure.0oj.ZOO">sciuredoj</ref>:
          <ekz>
            la vulpojn mi beda&ubreve;rinde ne povis foti, sed anstata&ubreve;e mi
            sukcesis fari kelkajn fotojn de <tld/>o
            <fnt>
              <aut>Birke kaj Bertilo</aut>:
              <vrk><url
 ref="http://bertilow.com/blogo/2005/08/streifenhoernchen-und-wale-tamiasoj-kaj-balenoj"
                  >Tamiasoj kaj balenoj</url></vrk>,
                  <lok>2005-08-02</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Streifenh&ouml;rnchen</trd>
      <trd lng="en">chipmunk</trd>
      <trd lng="es">ardilla listada</trd>
      <trd lng="fr">tamia</trd>
      <trdgrp lng="id">
        <trd><ind>bajing</ind> tanah</trd>,
        <trd><ind>tupai</ind> tanah</trd>
      </trdgrp>
      <trd lng="la">Tamias</trd>
      <trd lng="ru">&c_b;&c_u;&c_r;&c_u;&c_n;&c_d;&c_u;&c_k;</trd>
    </drv>

  </art>

