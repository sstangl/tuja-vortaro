<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxur.xml,v 1.2 2014/10/11 11:10:33 revo Exp $">
<kap>
  <rad>&ccirc;ur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cxur.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="cxur.0o.FIZL">
    <ref tip="dif" cel="sperm.0o.FIZL">spermo</ref>
      <ekz>
        &scirc;i <klr>...</klr> preska&ubreve; deziris, ke Ricardo
        plenigu &scirc;in per intensa, neeltenebla <tld/>o
        <fnt>
          <aut>M. de Seabra</aut>,
          <vrk>Malamu vin, unu la alian</vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="es">lefa <klr>(vulgar)</klr></trd>
  <trd lng="fr">foutre<klr> (subst., sperme)</klr></trd>
</drv>

<drv mrk="cxur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="cxur.0i.FIZL">
    <ref tip="dif" cel="ejakul.0i.FIZL">ejakuli</ref>
      <ekz>
        knaboj <klr>...</klr> kiuj perkace jam scipovas &gcirc;ui, kvankam 
        <tld/>i ankora&ubreve; ne
        <fnt>
          <aut>&Jcirc;. Tanguy </aut>,
          <vrk><url
          ref="http://gejaj-petolajhoj.blogspot.fr/2012/02/sovigado-el-ingo.html">
          &Scirc;ovi&gcirc;ado el ingo</url></vrk>,
          <lok>2012-02-29</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="es">correrse <klr>(vulg. eyacular)</klr></trd>
  <trd lng="fr">d&eacute;charger<klr> (&eacute;jaculer)</klr></trd>
</drv>
</art>

