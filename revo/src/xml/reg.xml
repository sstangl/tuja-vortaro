<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: reg.xml,v 1.27 2016/03/27 16:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>reg</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="reg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="reg.0i.KOMUNE">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Efektive estri, direkti, obeigi:
      <ekz>
        <tld/>i &ccirc;evalon;
      </ekz>
      <ekz>
        <tld/>i sian eksciton<fnt>B</fnt>;
      </ekz>
      <ekz>
        ne plu povi sin <tld/>i<fnt>B</fnt>;
      </ekz>
      <ekz>
        malfacile <tld/>i la situacion;
      </ekz>
      <ekz>
        super ni <tld/>as ia ne&scirc;an&gcirc;ebla fatalo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mono mondon <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="reg.0i.POL">
      <dif>
        Direkti la aferojn de la &scirc;tato.
      </dif>
      <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
        <klr>(&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;&c_j;)</klr></trd>
      <trd lng="de">regieren</trd>
      <trd lng="hu">uralkodik</trd>
      <trd lng="nl">regeren</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="reg.0i.EKON">
      <dif>
        Direkti kaj estri iun entreprenon:
        <ekz>
          bone <tld/>i sian hejmon;
        </ekz>
        <ekz>
          tri trustoj <tld/>as la diversajn industriojn de la tuta
          lando.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="gvid.0i">gvidi</ref>,
        <ref cel="konduk.0i">konduki</ref>,
        <ref cel="mastr.0i">mastri</ref>
      </refgrp>
      <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
        <klr>(&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_k;&c_a;&c_j;,
          &c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;&c_m;
          &c_ib; &c_d;&c_a; &c_g;.&c_p;.)</klr></trd>
      <trdgrp lng="hu">
        <trd>ir&aacute;ny&iacute;t</trd>,
        <trd>vezet</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>besturen</trd>
      </trdgrp>
      <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="reg.0i.GRA">
      <uzo tip="fak">GRA</uzo>
      <dif>
        Havi kiel dependa&jcirc;on:
        <ekz>
          transitiva verbo <tld/>as objekton.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="reg.0i.FIG_iun">
      <uzo tip="klr">(pri sento)</uzo>
      <dif>
        Ekposedi, plenigi la animon de iu:
        <ekz>
          malespero, granda fiero, furioza kolero ek<tld/>is lian
          koron<fnt>B</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="de">beherrschen</trd>
      <trd lng="hu">ural</trd>
      <trd lng="nl">beheersen</trd>
      <trd lng="ru">&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="dikt.0i">dikti</ref>,
      <ref cel="ir.0igi">irigi</ref>,
      <ref cel="man.0o">mano</ref>,
      <ref cel="prem.0i">premi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">beherrschen</trd>
    <trd lng="en">control</trd>
    <trd lng="hu">ural</trd>
    <trd lng="nl">beheersen</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_l;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_d;
        &c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_je;&c_m;</trd>,
      <trd>&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v;
        &c_r;&c_u;&c_k;&c_a;&c_h;</trd>
    </trdgrp>
  </snc>
  <snc mrk="reg.0i.FIG_estadi">
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom da&ubreve;re kaj vaste estadi, manifesti&gcirc;i, influi, efiki:
      <ekz>
        <tld/>as silento, ordo, bruo, malkvieto, norda vento,
        ventego,
        malvarmo, mallumo, nokto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as paco, plena harmonio, profunda mizero, krizo,
        senlaboreco, sento
        de reciproka fremdeco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as gaja humoro, vigla babilado;
      </ekz>
      <ekz>
        sur la viza&gcirc;o <tld/>is fiera trankvilo<fnt>B</fnt>;
      </ekz>
      <ekz>
        en la urbo <tld/>as vigla trafiko;
      </ekz>
      <ekz>
        maltoleremo kaj trompemo <tld/>as ankora&ubreve; &ccirc;ie en la mondo;
      </ekz>
      <ekz>
        en tiu rilato <tld/>as inter la publiko tute erara opinio;
      </ekz>
      <ekz>
        tiu scienco okupas nun <tld/>antan pozicion.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>honol</trd>,
      <trd>uralkodik</trd>,
      <trd>domin&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">heersen</trd>
    <trd lng="ru">&c_c;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ovl&aacute;dat</trd>,
    <trd>panovat</trd>,
    <trd>vl&aacute;dnout</trd>,
    <trd>&rcaron;&iacute;dit</trd>
  </trdgrp>
  <trd lng="de">herrschen</trd>
  <trdgrp lng="en">
    <trd>govern</trd>,
    <trd>rule</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dominar</trd>,
    <trd>controlar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diriger</trd>,
    <trd>gouverner</trd>,
    <trd>r&eacute;gir</trd>,
    <trd>r&eacute;gner</trd>,
    <trd>r&eacute;guler</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ovl&aacute;da&tcaron;</trd>,
    <trd>panova&tcaron;</trd>,
    <trd>vl&aacute;dnu&tcaron;</trd>
  </trdgrp>
  <trd lng="tp">lawa</trd>
</drv>

<drv mrk="reg.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="reg.0ado.KOMUNE">
    <dif>
      Agado de tiu, kiu <tld/>as:
      <ekz>
        la demokratia <tld/>ado
        <klr>(re&gcirc;imo)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni donu hodia&ubreve; plenan <tld/>adon al tiu silenta, sed solena
        kaj profunda sento<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>vl&aacute;da <klr>(&ccaron;innost)</klr></trd>,
    <trd>vl&aacute;dnut&iacute;</trd>
  </trdgrp>
  <trd lng="de">Herrschaft</trd>
  <trd lng="fr">r&egrave;gne</trd>
  <trdgrp lng="hu">
    <trd>uralkod&aacute;s</trd>,
    <trd>ir&aacute;ny&iacute;t&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">regering<klr tip="ind"> (het regeren)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">vl&aacute;da <klr>(&ccaron;innos&tcaron;)</klr></trd>
</drv>

<drv mrk="reg.0ato">
  <kap><ofc>*</ofc><tld/>ato</kap>
  <snc mrk="reg.0ato.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tiu, kiu estas submetata al la <tld/>ado de iu &scirc;tato:
      <ekz>
        &ccirc;iu regnano sciu, ke li <klr>...</klr> devas obei
        <klr>...</klr>
        fundamentajn le&gcirc;ojn de sia lando, kiuj estas egale devigaj
        por la
        <tld/>antoj kaj <tld/>atoj
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_d;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ovl&aacute;dan&yacute;</trd>,
    <trd>poddan&yacute;</trd>
  </trdgrp>
  <trd lng="de">Untertan</trd>
  <trd lng="es">s&uacute;bdito</trd>
  <trd lng="fr">sujet<klr> (pol., d'un &eacute;tat)</klr></trd>
  <trd lng="hu">alattval&oacute;</trd>
  <trd lng="nl">onderdaan</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ovl&aacute;dan&yacute;</trd>,
    <trd>poddan&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="reg.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="reg.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aran&gcirc;a&jcirc;o, per kiu oni <tld/>as aparaton:
      <ekz>
        mi ser&ccirc;is inter la regiloj, &gcirc;is mi trovis la &scirc;altilon
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ovlada&ccaron;</trd>,
    <trd>regul&aacute;tor</trd>
  </trdgrp>
  <trd lng="fr">commande<klr tip="amb"> (de machine)</klr></trd>
  <trdgrp lng="sk">
    <trd>ovl&aacute;da&ccaron;</trd>,
    <trd>regul&aacute;tor</trd>
  </trdgrp>
</drv>

<drv mrk="reg.0istaro">
  <kap><ofc>*</ofc><tld/>istaro</kap>
  <snc mrk="reg.0istaro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la personoj, kiuj <tld/>as &scirc;taton:
      <ekz>
        la <tld/>istaroj de la plej &ccirc;efaj regnoj
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>...
      </ekz>
    </dif>
    <ref tip="vid" cel="kabine.0o.ministraro">kabineto</ref>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>sbor ministr&uring;</trd>,
    <trd>vl&aacute;da</trd>
  </trdgrp>
  <trd lng="de">Regierung</trd>
  <trd lng="es">gobierno <klr>(grupo de personas)</klr></trd>
  <trdgrp lng="fr">
    <trd>gouvernement</trd>,
    <trd>cabinet<klr tip="ind"> (ensemble des ministres,
        gouvernement)</klr></trd>
  </trdgrp>
  <trd lng="hu">korm&aacute;ny</trd>
  <trd lng="nl">regering</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>vl&aacute;da</trd>,
    <trd>zbor ministrov</trd>
  </trdgrp>
</drv>

<drv mrk="reg.mem0ado">
  <kap>mem<tld/>ado, <var><kap>mem<tld/>eco</kap></var>,
  <var><kap>sin<tld/>ado</kap></var></kap>
  <snc mrk="reg.mem0ado.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      La rajto <tld/>i sin, fari proprajn decidojn kaj
      le&gcirc;ojn pri sia vivo:
      <ekz>
        jen urbo, kie kolekti&gcirc;is anta&ubreve; kvincent jaroj la unua
        parlamento kaj metis fundamenton por tutpopola
        mem<tld/>ado
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Vivo vokas</vrk>,
          <lok>1946</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxton.0eco">a&ubreve;tonomeco</ref>,
      <ref cel="pend.sende0eco.POL">sendependeco</ref>
    </refgrp>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="reg.mem0ado.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      La kapablo funkcii sen ekstera <tld/>ado, a&ubreve;tomate:
      <ekz>
        la nuna esplorado male celas da&ubreve;ran mem<tld/>econ,
        kapablon por la roboto konstrui mem planon
        <fnt>
          <aut>F. Maurelli</aut>,
          <vrk><url
          ref="http://www.uea.org/dokumentoj/IKU/2012/Maurelli.html">
          Subakva robotiko (resumo)</url></vrk>,
          <lok>IKU-2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">autonom&iacute;a</trd> 
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="reg.mem0ado.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Kapablo <tld/>i sin, <tld/>i siajn emociojn, kaj rezulta
      <ref tip="vid" cel="trankv.0o.anima">trankvilo</ref>:
      <ekz>
        kaj ne sole li reakiris la mem<tld/>adon, sed e&ccirc; li
        eksentis sin senfine supera, ol &ccirc;io vivanta
	<fnt><bib>Far3</bib>, <lok>&ccirc;ap. 14a.</lok></fnt>;
      </ekz>
      <ekz>
        mi perdas mian mem<tld/>adon, freneza kolero donas al
        mi la forton malfermi la pordon de l' &scirc;ranko, kaj mi
        batas &scirc;in per la pugnoj, per la piedoj <klr>(...)</klr>
        <fnt>
          <aut>H. Garc&iacute;a</aut>,
          <vrk>Vanesa</vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="es">autocontrol</trd>
    <trd lng="fr">maitrise de soi</trd>
    <trd lng="hu">&ouml;nuralom</trd>
    <trd lng="nl">zelfbeheersing</trd>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
</drv>

<drv mrk="reg.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu, io)</uzo>
    <dif>
      Havi pli da potenco ol:
      <ekz>
        <ind>super<tld/>a pozicio</ind>.
        <trd lng="hu">domin&aacute;ns helyzet</trd>
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Esti pli grava, pli multnombra ol:
      <ekz>
        tiu principo super<tld/>as &ccirc;iujn aliajn<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;ion super<tld/>as tiu grava cirkonstanco, ke...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_e;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;evl&aacute;dat</trd>,
    <trd>uplat&ncaron;ovat nadvl&aacute;du</trd>
  </trdgrp>
  <trd lng="de">vorherrschen</trd>
  <trd lng="en">overrule</trd>
  <trd lng="es">predominar</trd>
  <trdgrp lng="fr">
    <trd>dominer</trd>,
    <trd>maitriser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>domin&aacute;l</trd>,
    <trd>ural</trd>
  </trdgrp>
  <trd lng="nl">overheersen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_v;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ovl&aacute;da&tcaron;</trd>
</drv>
</art>
