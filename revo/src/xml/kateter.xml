<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kateter.xml,v 1.6 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kateter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kateter.0o">
  <kap><tld/>o</kap>
  <snc mrk="kateter.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Maldika <ref tip="super" cel="tub.0o.TEK">tubeto</ref>
      uzata por enfluigo en organon a&ubreve; defluigo de &gcirc;i:
      <ekz>
        la injekto de kontrastiga substanco per la <tld/>o, permesas 
        vidi la urinvezikon
        <fnt>
          <aut>M. Casartelli</aut>,
          <vrk><url ref="http://www.uea.org/vikio/Marcelo_Casartelli">
          Simptomoj de la Urogenitala Sistemo</url></vrk>,
          <lok>2006-07-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">cat&egrave;ter</trd>
  <trdgrp lng="cs">
    <trd>katétr</trd>,
    <trd>sonda</trd>
  </trdgrp>
  <trd lng="da">kateter</trd>
  <trd lng="de">Katheter</trd>
  <trd lng="en">catheter</trd>
  <trdgrp lng="fr">
    <trd>cath&eacute;ter</trd>,
    <trd>sonde m&eacute;dicale</trd>
  </trdgrp>
  <trdgrp lng="hr">
    <trd>kateter</trd>,
    <trd>sonda</trd>
  </trdgrp>
  <trd lng="hu">kat&eacute;ter</trd>
  <trd lng="it">catetere</trd>
  <trdgrp lng="nl">
    <trd>katheter</trd>,
    <trd>sonde<klr tip="ind"> (med.)</klr></trd>
  </trdgrp>
  <trd lng="pl">cewnik</trd>
  <trdgrp lng="sk">
    <trd>katéter</trd>,
    <trd>sonda</trd>
  </trdgrp>
</drv>
</art>

