<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korund.xml,v 1.14 2015/06/23 15:10:19 revo Exp $">
<kap>
  <rad>korund</rad>/o
</kap>
<drv mrk="korund.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta &scirc;tono el pura 
      <ref tip="super" cel="alumin.0o">alumino</ref>
      <frm>Al<sub>2</sub>O<sub>3</sub></frm>, 
      ortorombe <ref tip="vid"
cel="krista.0igxi">kristali&gcirc;anta</ref>, 
      la plej malmola post la 
      <ref tip="vid" cel="diaman.0o.MIN">diamanto</ref>:
      <ekz>
        ekzistas multaj varioj de nepura <tld/>o: 
        <ref tip="sub" cel="safir.0o.MIN">safiro</ref>, 
        <ref tip="sub" cel="smirg.0o.MIN">smirgo</ref>,
        ktp.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_u;&c_n;&c_d;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_u;&c_n;&c_d;</trd>
  <trd lng="de">Korund</trd>
  <trd lng="en">corundum</trd>
  <trd lng="fr">corindon</trd>
  <trd lng="hu">korund</trd>
  <trd lng="nl">korund</trd>
  <trd lng="pl">korund</trd>
  <trdgrp lng="pt">
    <trd>cor&iacute;ndon</trd>,
    <trd>corindo</trd>    
    <trd>corundo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_u;&c_n;&c_d;</trd>
</drv>
</art>
