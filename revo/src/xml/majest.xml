<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: majest.xml,v 1.15 2012/08/19 11:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>majest</rad>/a
</kap>
<drv mrk="majest.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Inspiranta admiran respekton per sia impona aspekto kaj
      dignoplenaj
      manieroj:
      <ekz>
        <tld/>a pa&scirc;ado;
      </ekz>
      <ekz>kapo <tld/>a, sed cerbo modesta
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>a rivero;
      </ekz>
      <ekz>
        <tld/>eco de la Alpaj montoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="pomp.0a">pompa</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="tron.0i">troni</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">meurdezus</trd>
  <trd lng="ca">majest&agrave;tic</trd>
  <trd lng="de">majest&auml;tisch</trd>
  <trd lng="en">majestic</trd>
  <trdgrp lng="hu">
    <trd>fels&eacute;ges</trd>,
    <trd>pomp&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>statig</trd>,
    <trd>majestueus</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>majestoso</trd>,
    <trd>alteroso</trd>,
    <trd>soberbo</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_a;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="majest.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="majest.0o.eco">
    <dif>
      Eco de iu, io majesta.
    </dif>
  </snc>
  <snc mrk="majest.0o.titolo">
    <dif>
      Re&gcirc;a a&ubreve; imperiestra mo&scirc;to.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="br">meurdez</trd>
  <trd lng="ca">majestat</trd>
  <trd lng="de">Majest&auml;t</trd>
  <trd lng="en">majesty</trd>
  <trd lng="hu">fels&eacute;g</trd>
  <trd lng="nl">majesteit</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
