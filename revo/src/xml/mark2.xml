<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mark2.xml,v 1.9 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mark</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mark2.0o">
  <kap><tld/>o</kap>
  <snc mrk="mark2.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iama <ref tip="lst" cel="unu.mon0o.EKON"
      lst="voko:monunuoj">monunuo</ref> de 
      Estonio, Finlando, Germanujo kaj aliaj:
      <ekz>
        en Bosnio kaj Hercegovino la monunuo estas bosna <tld/>o, 
        oficiale nomita <ctl>Konvertibilna marka (KM)</ctl>
        <fnt>
          <aut>-</aut>,
          <vrk>
          <url ref="http://www.esperanto.ba/ijk/KostojSarajevo.htm">
          62-a internacia junulara kongreso</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="mark2.0o.RIM_HOM">
    <ref tip="hom" cel="mark.0o">Marko</ref> fundamente signifas 
    distingan signon sur objekto.  
    <ref tip="hom" cel="mark1.0o">Marko</ref> estas anka&ubreve; voknomo.
  </rim>
  <trd lng="be">&c_m;&c_a;&c_r;&c_k;&c_a; <klr tip="amb">
  (&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; 
  &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)
  </klr></trd>  
  <trd lng="br">mark</trd>
  <trd lng="de">Mark</trd>
  <trd lng="en">mark</trd>
  <trd lng="fa">&a_m;&a_A;&a_r;&f_k;<klr tip="ind"> (&a_w;&a_A;&a_H;&a_d; &f_p;&a_w;&a_l;)</klr></trd>
  <trd lng="fr">mark<klr> (monnaie)</klr></trd>
  <trd lng="hu">m&aacute;rka</trd>
  <trd lng="id">mark</trd>
  <trd lng="nl">mark</trd>
  <trd lng="pl">marka</trd>
  <trd lng="pt">marco</trd>
  <trd lng="vo">mak</trd>
  <trdgrp lng="sk">
    <trd>návesť</trd>,
    <trd>označenie</trd>,
    <trd>štítok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>névus</trd>,
    <trd>otisk</trd>,
    <trd>pohmožděnina</trd>,
    <trd>skvrna</trd>,
    <trd>znaménko</trd>
  </trdgrp>
</drv>
</art>

