<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hagx.xml,v 1.9 2013/05/01 16:10:18 revo Exp $">
<kap>
  <rad>ha&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hagx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hagx.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Pilgrimo al la sankta urbo Meko:
      <ekz>
        <tld/>o estas unu el Kvin Kolonoj de Islamo
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;ha&gcirc;o&rightquot;, 
        2007-01-11</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_h;&c_a;&c_d;&c_zh;</trd>
  <trdgrp lng="ca">
    <trd>hajj</trd>,
    <trd>pelegrinatge a la Meca</trd>
  </trdgrp>
  <trd lng="de">Haddsch</trd>
  <trd lng="en">hajj</trd>
  <trd lng="fr">hajj<klr> (p&eacute;lerinage)</klr></trd>
  <trdgrp lng="hu">
    <trd>h&aacute;dzs</trd>,
    <trd>mekkai <ind>zar&aacute;ndoklat</ind></trd>
   </trdgrp>
   <trd lng="id">haji</trd>
  <trd lng="it">Hajj</trd>
</drv>

<drv mrk="hagx.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="hagx.0ulo.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Honora nomo de tiu, kiu pilgrimis al Meko:
      <ekz>
        &ccirc;inaj <tld/>uloj revenas hejmen el Mekko
        <fnt>
          <aut>&Ccirc;ina Interreta Informa Centro</aut>,
          <vrk><url
          ref="http://esperanto.china.org.cn/world/07-0115-2.htm">
          &Ccirc;inaj ha&gcirc;uloj...</url></vrk>,
          <lok>2007-15-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>hajj</trd>,
    <trd>pelegr&iacute; a la Meca</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Haddschi</trd>,
    <trd>Hadschi</trd>,
    <trd>Hadji</trd>,
    <trd>Hajji</trd>
  </trdgrp>
  <trd lng="en">hajji</trd>
  <trd lng="fr">hajj<klr> (p&egrave;lerin)</klr></trd>
  <trdgrp lng="hu">
    <trd>hadzsi</trd>,
    <trd>mekkai <ind>zar&aacute;ndok</ind></trd>
   </trdgrp>
  <trd lng="it">h&#257;jj&#299;</trd>
  <trdgrp lng="pt">
    <trd>hadji</trd>,
    <trd>h&aacute;ji</trd>,
    <trd>haje</trd>
  </trdgrp>
</drv>
</art>

