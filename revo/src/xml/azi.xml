<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: azi.xml,v 1.38 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>Azi</rad>/o
  <ofc>8</ofc>
</kap>

<drv mrk="azi.A0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La <ref tip="lst" cel="mond.0oparto"
      lst="voko:mondopartoj">mondoparto</ref>
      inter E&ubreve;ropo kaj Nord-Ameriko:
      <ekz>
        &leftquot;nun vi povas jam vidi la Himalajon, &gcirc;i estas la
        plej alta montaro en <tld/>o, <klr>[...]</klr>&rightquot;,
        diris la orimenta vento
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <fnt><bib>Fab1</bib>, <lok></lok></fnt>;
        kaj la rozo ardis pli bele ol anta&ubreve;e, &gcirc;i estis ja
        en sia varma Azio
        <fnt><bib>Fab2</bib>, <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkdeko da parfumoj, koloraj vitroj, mebloj, vazoj,
        teksa&jcirc;oj, veturiloj, &ccirc;io &ccirc;i venas al ni
        el <tld/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 3</lok></fnt>;
      </ekz>
      <ekz>
        A&ubreve;skultu do, <tld/>o, mamnutristino nia, <klr>/</klr>
        Afriko sfinkse muta, tribar' Oceania <klr>...</klr>
        <fnt>
        <aut>N. Hohlov</aut>: <vrk>E&ubreve;ropo</vrk>.</fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="euxraz.E0o">E&ubreve;razio</ref>
  </snc>
  <trd lng="lat">Asia</trd>
  <trd lng="be">&c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">&Agrave;sia</trd>
  <trd lng="cs">Asie</trd>
  <trd lng="de">Asien</trd>
  <trd lng="el">&Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="en">Asia</trd>
  <trd lng="es">Asia</trd>
  <trd lng="fr">Asie</trd>
  <trd lng="hu">&Aacute;zsia</trd>
  <trd lng="it">Asia</trd>
  <trd lng="nl">Azi&euml;</trd>
  <trd lng="pl">Azja</trd>
  <trd lng="pt">&Aacute;sia</trd>
  <trd lng="ru">&c_A;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>&Aacute;zia</trd>,
    <trd>Ázia</trd>
  </trdgrp>
  <trd lng="sv">Asien</trd>
  <trd lng="vo">Silop</trd>
</drv>

<drv mrk="azi.0ano">
  <kap><tld lit="a"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>:
      <ekz>
        en la vicoj oni povis facile distingi la brunajn Egiptanojn,
        nigrajn Negrojn, flavajn <tld/>anojn kaj blankajn
        lo&gcirc;antojn de Libio kaj de la insuloj de Mediteraneo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la palaco staris garde <tld/>anoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_ib;&c_ja;&c_t;</trd>
  <trd lng="bg">&c_a;&c_z;&c_i;&c_a;&c_t;&c_je;&c_c;</trd>
  <trd lng="ca">asi&agrave;tic</trd>
  <trd lng="de">Asiate</trd>
  <trd lng="en">Asian</trd>
  <trd lng="es">asi&aacute;tico</trd>
  <trdgrp lng="fr">
    <trd>asiatique<klr> (nom)</klr></trd>
  </trdgrp>
  <trd lng="hu">&aacute;zsiai</trd>
  <trd lng="it">asiatico<klr> (sost.)</klr></trd>
  <trd lng="nl">Aziaat</trd>
  <trd lng="pl">azjata</trd>
  <trd lng="ru">&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
  <trd lng="sk">&Aacute;zij&ccaron;an</trd>
  <trd lng="sv">asiat</trd>
  <trd lng="vo">silopel</trd>
</drv>

<drv mrk="azi.OrientaA0o">
  <kap>Orienta <tld/>o</kap>
  <snc mrk="azi.OrientaA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La tuta orienta parto de <ref tip="malprt"
      cel="azi.A0o"><tld/>o</ref>
      en la akvokolekta baseno de <ref cel="pacifik.P0o">Pacifiko</ref>,
      de la <ref cel="kol.BeringaMar0o">Beringa Markolo</ref> norde
      &gcirc;is la Malaka Markolo sude, kun la apudaj insuloj.
    </dif>
    <trdgrp lng="ca">
      <trd>Extrem <ind>Orient</ind></trd>,
      <trd>Orient Lluny&agrave;</trd>
    </trdgrp>
    <trd lng="de">Ostasien</trd>
    <trdgrp lng="en">
      <trd>East <ind>Asia</ind></trd>,
      <trd>Far <ind>East</ind></trd>
    </trdgrp>
    <trd lng="it">Estremo <ind>Oriente</ind></trd>
    <trdgrp lng="nl">
      <trd>Oost-<ind>Azi&euml;</ind></trd>,
      <trd>Verre <ind>Oosten</ind></trd>
    </trdgrp>
    <trd lng="pl">Daleki Wsch&oacute;d</trd>
    <trd lng="ru">&c_D;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;
    &c_V;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="azi.OrientaA0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      La suda parto de la Orienta
      <tld/>o,<sncref ref="azi.A0o.orienta.GEOG"/>
      kiel fermita regiono de la &ccirc;ina, japana, korea kaj vjetnama
      civilzo kaj kulturo.
    </dif>
    <trd lng="ru">
    &c_Ju;&c_g;&c_o;-&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_A;&c_z;&c_i;&c_ja;</trd>
    <trd lng="de">S&uuml;dostasien</trd>
    <trdgrp lng="en">
      <trd>Southeast <ind>Asia</ind></trd>
    </trdgrp>
    <trd lng="it"><ind>Sudest</ind> Asiatico</trd>
    <trdgrp lng="nl">
      <trd>Oost-<ind>Azi&euml;</ind></trd>,
      <trd>Verre <ind>Oosten</ind></trd>
    </trdgrp>
    <trd lng="pl">Azja po&lstroke;udniowo-wschodnia</trd>
  </snc>
  <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
  <trd lng="de">Ostasien</trd>
  <trd lng="fr">Extr&ecirc;me-Orient</trd>
  <trdgrp lng="hu">
    <trd>Kelet-&Aacute;zsia</trd>,
    <trd>T&aacute;vol-Kelet</trd>
  </trdgrp>
  <trd lng="sk">Ďaleký východ</trd>
  <trdgrp lng="cs">
    <trd>Dálný Východ</trd>,
    <trd>Jihovýchodní Asie</trd>
  </trdgrp>
</drv>

<drv mrk="azi.OkcidentaA0o">
  <kap>Okcidenta <tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La okcidenta parto de <ref tip="malprt" cel="azi.A0o">
      <tld/>o</ref>
      (<ref tip="prt" cel="anatoli.0o">Anatolio</ref>,
      Ka&ubreve;kazo,
      <ref tip="prt" cel="mezopotami.M0o">Mezopotamio</ref>,
      Irano, Arabujo).
    </dif>
  </snc>
  <trd lng="ca">Orient Mitj&agrave;</trd>
  <trd lng="cs">Západní Asie</trd>
  <trd lng="de">Westasien</trd>
  <trdgrp lng="en">
    <trd>Middle <ind>East</ind></trd>,
    <trd>Near <ind>East</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Moyen-Orient</trd>
  </trdgrp>
  <trd lng="hu">Nyugat-&Aacute;zsia</trd>
  <trd lng="it">Medio <ind>Oriente</ind></trd>
  <trdgrp lng="nl">
    <trd>West-<ind>Azi&euml;</ind></trd>,
    <trd>Nabije <ind>Oosten</ind></trd>
  </trdgrp>
  <trd lng="pl">Bliski Wsch&oacute;d</trd>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja;
  &c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">Západná Ázia</trd>
</drv>

<drv mrk="azi.SudaA0o">
  <kap>Suda <tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La suda parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>,
      precipe la parto de la baseno de la Hinda Oceano
      &ccirc;irka&ubreve;ita
      de la orienta, interna kaj okcidenta partoj de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">S&uuml;dasien</trd>
  <trdgrp lng="en">
    <trd>South Asia</trd>,
    <trd>Southern Asia</trd>,
  </trdgrp>
  <trd lng="hu">D&eacute;l-&Aacute;zsia</trd>
  <trd lng="nl">Zuid-<ind>Azi&euml;</ind></trd>
  <trd lng="pl">Azja po&lstroke;udniowa</trd>
  <trd lng="ru">&c_Ju;&c_zh;&c_n;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
</drv>

<drv mrk="azi.InternaA0o">
  <kap>Interna <tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La meza parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
      suden de Siberio, la baseno senelfluaj riveroj: la
      Arala baseno, Tibeto.
    </dif>
    <rim>
      Oni anka&ubreve; diras &leftquot;la Centra <tld/>o&rightquot;,
      kiu por iuj sinonimas kun la tuta Interna <tld/>o, por la aliaj,
      kun ties Tibeta parto (dum la Aralan kaj Bal&hcirc;a&scirc;an
      basenojn oni iam nomas "la Meza <tld/>o&rightquot; a&ubreve;
      Turkestano).
    </rim>
  </snc>
  <trd lng="cs">Střední Asie</trd>
  <trd lng="de">Innerasien</trd>
  <trdgrp lng="en">
    <trd>Central <ind>Asia</ind></trd>
  </trdgrp>
  <trd lng="hu">Bels&odblac;-&Aacute;zsia</trd>
  <trd lng="it"><ind>Asia</ind> Centrale</trd>
  <trd lng="nl">Midden-<ind>Azi&euml;</ind></trd>
  <trdgrp lng="pl">
    <trd>Azja &sacute;rodkowa</trd>,
    <trd>Azja wewn&eogonek;trzna</trd>
  </trdgrp>
  <trd lng="ru">&c_V;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;
  &c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">Stredná Ázia</trd>
</drv>

<drv mrk="azi.MalgrandA0o">
  <kap>Malgrand-<tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="anatoli.0o">Anatolio</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>&Agrave;sia Menor</trd>,
    <trd>Anat&ograve;lia</trd>
  </trdgrp>
  <trd lng="de">Kleinasien</trd>
  <trd lng="el">&My;&jota;&kappa;&rho;&alfa_ton;
  &Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="en">Asia Minor</trd>
  <trd lng="hu">Kis-&Aacute;zsia</trd>
  <trd lng="it"><ind>Asia</ind> Minore</trd>
  <trd lng="lat">Asia minor</trd>
  <trd lng="nl">Klein-<ind>Azi&euml;</ind></trd>
  <trd lng="pl">Azja Mniejsza</trd>
  <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="tr">
    <trd>Anadolu</trd>,
    <trd>K&uuml;&ccedil;&uuml;k Asya</trd>
  </trdgrp>
</drv>
</art>
