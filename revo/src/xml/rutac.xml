<art mrk="$Id: rutac.xml,v 1.8 2013/09/16 16:10:15 revo Exp $">
<kap>
  <rad>rutac</rad>/oj
</kap>
<drv mrk="rutac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rutaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_je;&c_d;&c_je;&c_f;&c_ch;&c_je;&c_v;&c_i;</trd>
  <trd lng="de">Rautengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>rutaceae</trd>,
    <trd>rue family</trd>
  </trdgrp>
  <trd lng="hu">rutaf&eacute;l&eacute;k</trd>
  <trd lng="nl">wijnruitfamilie</trd>
  <trd lng="pt">rut&aacute;ceas</trd>
  <trd lng="ru">&c_r;&c_u;&c_t;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
