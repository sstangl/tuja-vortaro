<art mrk="$Id: elatr.xml,v 1.16 2015/06/29 16:29:37 revo Exp $">
<kap>
  <rad>elatr</rad>/o
</kap>
<drv mrk="elatr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="elatre.0oj"><tld/>edoj</ref>
      (<trd lng="la">Elater</trd>).
    </dif>
  </snc>
  <trd lng="de">Schnellk&auml;fer</trd>
  <trd lng="en">click <ind>beetle</ind></trd>
  <trd lng="hu">pirosh&aacute;t&uacute;
  <ind>pattan&oacute;bog&aacute;r</ind></trd>
  <trd lng="nl">elater (<klr>uit de fam. der kniptorren</klr>)</trd>
  <trd lng="pl">spr&eogonek;&zdot;yk</trd>
  <trdgrp lng="pt">
    <trd>elater&iacute;deo</trd>,
    <trd>escaravelho</trd>,
    <trd>besouro</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_k;&c_u;&c_n; <klr
  tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
  <trd lng="sk">kováčik</trd>
</drv>
</art>
