<vortaro>
<art mrk="$Id: enteri.xml,v 1.18 2015/10/07 15:10:18 revo Exp $">
  <kap>
    <rad>enterit</rad>/o
  </kap>
  <drv mrk="enteri.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
        <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
        de la maldika intesto.  
      </dif>
    </snc>
  <trd lng="be">&c_e;&c_n;&c_t;&c_e;&c_r;&c_y;&c_t;</trd>
  <trd lng="bg">&c_je;&c_n;&c_t;&c_je;&c_r;&c_i;&c_t;</trd>
  <trd lng="ca">enteritis</trd>
  <trdgrp lng="cs">
    <trd>enteritida</trd>,
    <trd>enteritis</trd>,
    <trd>z&aacute;n&ecaron;t st&rcaron;ev</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Darmentz&uuml;ndung</trd>,
       <trd>Enteritis<klr> [fak]</klr></trd>
    </trdgrp>
    <trd lng="en">enteritis</trd>
  <trd lng="es">enteritis</trd>
  <trd lng="fr">ent&eacute;rite</trd>
  <trdgrp lng="hu">
      <trd>b&eacute;lhurut</trd>,
      <trd>enteritis</trd>
    </trdgrp>
  <trd lng="id">enteritis</trd>
  <trdgrp lng="nl">
      <trd>enteritis</trd>,
      <trd>darmontsteking</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd><klr tip="amb">wirusowe </klr>zaka&zdot;enie przewodu
      pokarmowego</trd>,
      <trd>grypa &zdot;o&lstroke;&aogonek;dkowa</trd>,
      <trd>grypa jelitowa</trd>
    </trdgrp>
  <trd lng="pt">enterite</trd>
  <trd lng="ru">&c_e;&c_n;&c_t;&c_je;&c_r;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>enterit&iacute;da</trd>,
    <trd>&ccaron;revn&yacute; katar</trd>
  </trdgrp>
</drv>
</art>
