<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: spekta.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>spektakl</rad>/o
</kap>
<drv mrk="spekta.0o">
  <kap><tld/>o</kap>
  <snc mrk="spekta.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <ref tip="dif" cel="spekt.0ajxo">spekta&jcirc;o</ref>
    <ekz>
      por ni, sekve, la milito estas nur kinofilmoj, <tld/>oj, libroj
      <fnt><aut>Nina Langlet</aut>: <vrk>Kaoso en
      Budape&scirc;to</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
      26a</lok></fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd>Auff&uuml;hrung</trd>,
      <trd>Vorf&uuml;hrung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>el&odblac;ad&aacute;s</trd>,
      <trd>bemutat&oacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="spekta.0o.evento">
    <dif>
      &Ccirc;ia vida&jcirc;o, kiu altiras atenton kaj intereson.
    </dif>
    <trdgrp lng="de">
      <trd>Spektakel</trd>,
      <trd>&ouml;ffentliches Ereignis</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>l&aacute;tv&aacute;nyoss&aacute;g</trd>,
      <trd>show</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_k;&c_t;&c_a;&c_k;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>,
    <trd>&c_sh;&c_o;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pod&iacute;van&aacute;</trd>,
    <trd>představení</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>spectacle</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schouwspel</trd>,
    <trd>spektakel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_k;&c_t;&c_a;&c_k;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_sh;&c_o;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predstavenie <klr>(v cirkuse, kine, divadle ap.)</klr></trd>,
    <trd>predstavenie <klr>(v cirkuse| kine| divadle ap.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="spekta.0a">
  <kap><tld/>a</kap>
  <snc mrk="spekta.0a.TEA">
    <dif>
      Rilata al spekta&jcirc;o:
      <ekz>
        en 1844, 19-jara Johann Strauss-filo faris sian <tld/>an debuton
        kiel muzikisto<fnt><aut>Evgeni Georgiev</aut>: <vrk>Jubileo de
        valsre&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 9a, p.
        22a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>el&odblac;ad&oacute;</trd>,
      <trd>bemutat&oacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_c;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="spekta.0a.eventa">
    <dif>
      Spektinda, impresa:
      <ekz>
        la plej <tld/>a konservativa kolapso de la jarcento anka&ubreve; rekorde
        enparlamentigis virinojn - pli ol 100<fnt><aut>Paul Gubbins</aut>:
        <vrk>Velura revolucio</vrk>, <bib>Monato</bib><lok>jaro 1997a,
        numero 6a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;monstratif<klr> (spectaculaire)</klr></trd>,
      <trd>spectaculaire</trd>
    </trdgrp>
    <trd lng="hu">l&aacute;tv&aacute;nyos</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_e;&c_f;&c_f;&c_je;&c_k;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
