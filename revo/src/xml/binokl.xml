<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: binokl.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>binokl</rad>/o
</kap>

<drv mrk="binokl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Malgranda dutuba <ref tip="super" cel="lorn.0o">lorno</ref>,
      uzata en teatro a&ubreve; dum ekskursoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="lorn.du0eto">dulorneto</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_ib;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Prismenfernglas</trd>,
    <trd>Fernglas</trd>,
    <trd>Feldstecher</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gemelos</trd>,
    <trd>prism&aacute;ticos</trd>,
    <trd>binoculares</trd>
  </trdgrp>
  <trd lng="en">binoculars</trd>
  <trd lng="fr">jumelles <klr>(optique)</klr></trd>
  <trdgrp lng="hu">
    <trd>l&aacute;tcs&odblac;</trd>,
    <trd>binokul&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">toneelkijker</trd>
  <trd lng="pl">lornetka</trd>
  <trd lng="ru">&c_b;&c_i;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>binokel</trd>,
    <trd>malý ďalekohľad</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>binokl</trd>,
    <trd>kukátko</trd>,
    <trd>malý dalekohled</trd>,
    <trd>triedr</trd>
  </trdgrp>
</drv>
</art>
