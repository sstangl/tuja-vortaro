<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kompro.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kompromis</rad>/o
</kap>
<drv mrk="kompro.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="konsen.inter0o">Interkonsento</ref>,
      aran&gcirc;ita pro reciprokaj cedoj de du kontra&ubreve;uloj
      pri la diskutita&jcirc;o:
      <ekz>
        <ind>finveni al <tld/>o</ind>;
        <trd lng="fr">atteindre un <ind>compromis</ind></trd>
      </ekz>
      <ekz>
        aran&gcirc;i,
      </ekz>
      <ekz>
        <ind>fari <tld/>on</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>faire un <ind>compromis</ind></trd>,
          <trd>composer<klr tip="ind"> (transiger)</klr></trd>,
          <trd>transiger</trd>
        </trdgrp>
        <trd lng="nl">een <ind>compromis</ind> sluiten</trd>
      </ekz>
      <ekz>
        ne ser&ccirc;u vanajn <tld/>ojn, kiuj flikas unu lokon
        krevigante alian<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0o">akordo</ref>,
      <ref cel="ced.0o">cedo</ref>,
      <ref cel="konced.0o">koncedo</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_r;&c_a;&c_m;&c_ib;&c_s;</trd>
    <trd lng="ca">comprom&iacute;s</trd>
    <trdgrp lng="de">
      <trd>Kompromiss</trd>,
      <trd>Vergleich</trd>,
      <trd>Ausgleich</trd>
    </trdgrp>
    <trd lng="en">compromise</trd>
    <trdgrp lng="fr">
      <trd>compromis<klr tip="ind"> (subst.)</klr></trd>,
      <trd>accommodement</trd>,
      <trd>arrangement<klr tip="ind"> (accord avec concessions)</klr>
      </trd>,
      <trd>transaction<klr tip="ind"> (accord avec concessions)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>tussenoplossing</trd>,
      <trd>schikking<klr tip="ind"> (compromis)</klr></trd>
      <trd>compromis</trd>
    </trdgrp>
    <trd lng="pl">kompromis</trd>
    <trd lng="pt">solu&ccedil;&atilde;o de <ind>compromisso</ind></trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_r;&c_o;&c_m;&c_i;&c_s;&c_s;
    </trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kompromis</trd>,
    <trd>narovnání</trd>,
    <trd>ústupek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kompromis</trd>,
    <trd>ústupok</trd>
  </trdgrp>
</drv>

<drv mrk="kompro.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        la bur&gcirc;aro <tld/>is kaj cedadis, kio donis
        al Yuan Shikai, &ccirc;efo de la nordaj militaristoj,
        &scirc;ancon uzurpi la frukton de la revolucio
        <fnt>
        <vrk>Anta&ubreve;moderna historio</vrk>
        <lok>
        <url ref="http://www.chinareport.com.cn/18s/01/index-02-2.htm">
        El Popola &Ccirc;inio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">comprometre's</trd>
  <trdgrp lng="cs">
    <trd>uzavřít kompromis</trd>,
    <trd>uzavřít narovnání</trd>,
    <trd>učinit kompromis</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire un <ind>compromis</ind></trd>,
    <trd>composer<klr tip="ind"> (transiger)</klr></trd>,
    <trd>transiger</trd>
  </trdgrp>
  <trd lng="nl">een <ind>compromis</ind> sluiten</trd>
  <trd lng="pl">dochodzi&cacute; do <ind>kompromisu</ind></trd>
  <trd lng="sk">urobiť kompromis</trd>
</drv>
</art>
