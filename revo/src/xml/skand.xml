<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: skand.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
    <kap>
      <rad>skand</rad>/i
    </kap>

    <drv mrk="skand.0i">
      <kap><tld/>i</kap>
      <uzo tip="stl">RAR</uzo>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="skand.0i.KOMUNE">
        <dif>
          <ref tip="super" cel="parol.el0i.KOMUNE">Elparoli</ref>,
          klare disigante la silabojn:
          <ekz>
            nokte vi a&ubreve;dis la parolon de l' dioj kaj sentis ilian manon
            &ndash;
            ripetis la Feniciano, <tld/>ante la vortojn
            <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
          </ekz><ekz>
            ili svingis flagojn kaj <tld/>is diversajn sloganojn
            <fnt>
              <aut>Theodor Kilian, red. Dieter Berndt kaj Miroslav
              Malovec</aut>:
              <vrk>U&ccaron;ebnice esperanta</vrk>.
              <lok><url
              ref="http://esperanto.czechian.net/ucebnice/leciono17/leciono17.html"
                  >17a leciono (Printempa festo)</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="en">chant</trd>
      </snc>
      <snc mrk="skand.0i.POE">
        <uzo tip="fak">POE</uzo>
        <dif>
          Parole analizi verson la&ubreve; silaboj a&ubreve; la&ubreve; verseroj.
        </dif>
        <trd lng="en">scan</trd>
      </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">escandir</trd>
  <trdgrp lng="cs">
    <trd>ritmizovaně recitovat</trd>,
    <trd>skandovat</trd>
  </trdgrp>
  <trd lng="de">skandieren</trd>
  <trd lng="fr">scander</trd>
  <trdgrp lng="hu">
        <trd>skand&aacute;l</trd>,
        <trd>&uuml;temez</trd>
      </trdgrp>
  <trd lng="nl">scanderen</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>skandovať</trd>,
    <trd>slabikovať</trd>
  </trdgrp>
</drv>
  </art>
