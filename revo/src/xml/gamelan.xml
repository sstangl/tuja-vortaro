<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gamelan.xml,v 1.11 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>gamelan</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="gamelan.0o">
  <kap><tld/>o</kap>
  <snc mrk="gamelan.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzikilaro el Javo, Balio a&ubreve; sundaj insuloj,
      el akordaj gongaj kaj metaltubaj kariljonoj,
      ordinare kompletigita de kelkaj blov- kaj kord-instrumentoj:
      <ekz>
        <ctl>Pagodes</ctl> spegulas sonorojn de la perkutilaro de
        <tld/>o
        <fnt>
        <aut>MusicExpress</aut>,
        <vrk><url
        ref="http://www.musicexpress.com.br/Artisto.asp?Artista=55">Sonia
        Rubinsky</url></vrk>,
        <lok>[vidita je 2006-04-11]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_g;&c_a;&c_m;&c_je;&c_l;&c_a;&c_n;</trd>
  <trd lng="ca">gamelan</trd>
  <trd lng="cs">gamalanový orchestr</trd>
  <trd lng="de">Gamelan</trd>
  <trd lng="en">gamelan</trd>
  <trd lng="fr">gamelan</trd>
  <trd lng="hu">gamel&aacute;n</trd>
  <trd lng="nl">gamelan</trd>
  <trd lng="pt">gamel&atilde;o</trd>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_je;&c_l;&c_a;&c_n;</trd>
  <trd lng="sk">gamalanový orchester</trd>
  <trd lng="sv">gamelan</trd>
</drv>
</art>
