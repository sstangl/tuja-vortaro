<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tajg.xml,v 1.13 2015/07/04 08:37:44 revo Exp $">
<kap>
   <rad>tajg</rad>/o
</kap>

<drv mrk="tajg.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">GEOG</uzo>
   <snc>
     <dif>
       Subarkta praarbarego konifera, formanta zonon &ccirc;irka&ubreve;
       la norda duonglobo, plejparte inter
       <ref cel="tundr.0o.norda">tundro</ref> kaj
       <ref cel="step.0o.ebena">stepo</ref>, de
       <ref cel="siberi.S0o">Siberio</ref> &gcirc;is Skandinavio
       E&ubreve;razie, kaj en Alasko kaj Kanado Nordamerike.
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_a;&c_j;&c_g;&c_a;</trd>
  <trd lng="ca">taig&agrave;</trd>
  <trd lng="cs">tajga</trd>
  <trd lng="de">Taiga</trd>
  <trdgrp lng="en">
     <trd>taiga</trd>,
     <trd>boreal forest</trd>
   </trdgrp>
  <trd lng="fa">&a_t;&a_A;&f_y;&f_g;&a_A;</trd>
  <trd lng="fr">ta&iuml;ga</trd>
  <trd lng="hu">tajga</trd>
  <trd lng="nl">taiga</trd>
  <trd lng="pl">tajga</trd>
  <trdgrp lng="pt">
   <trd>taiga</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_j;&c_g;&c_a;</trd>
  <trd lng="sk">tajga</trd>
</drv>

</art>

