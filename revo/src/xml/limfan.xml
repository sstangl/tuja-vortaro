<vortaro>
<art mrk="$Id: limfan.xml,v 1.12 2013/10/09 19:11:29 revo Exp $">
<kap>
  <rad>limfangit</rad>/o
</kap>
<drv mrk="limfan.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de limfaj vazoj.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;&c_n;&c_g;&c_ib;&c_t;</trd>
  <trd lng="ca">limfoangitis</trd>
  <trdgrp lng="de">
     <trd>Lymphgef&auml;&szlig;entz&uuml;ndung</trd>,
     <trd>Lymphangitis</trd>
  </trdgrp>
  <trd lng="hu">nyirok&eacute;rgyullad&aacute;s</trd>
  <trd lng="nl">lymfangitis</trd>
  <trdgrp lng="pl">
  <trd>zapalenie naczy&nacute; <ind>limfatycznych</ind></trd>,
  <trd>zapalenie uk&lstroke;adu <ind>limfatycznego</ind></trd>,
  <trd>zapalenie naczy&nacute; <ind>ch&lstroke;onnych</ind></trd>,
  <trd>zapalenie uk&lstroke;adu <ind>ch&lstroke;onnego</ind></trd>
  </trdgrp>
  <trd lng="pt">linfangite</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;&c_n;&c_g;&c_i;&c_t;</trd>
</drv>
</art>
