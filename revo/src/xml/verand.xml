<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: verand.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>verand</rad>/o
</kap>

<drv mrk="verand.0o">
  <kap><tld/>o</kap>
  <snc mrk="verand.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tegmenthava kaj vitrita teraso a&ubreve; balkono:
      <ekz>
        Manjo <klr>...</klr> eliris sur la postan <tld/>on
        <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>;
      </ekz>
      <ekz>
        plantoj kreskis dense sur la lar&gcirc;a anta&ubreve;a <tld/>o
        <fnt><bib>DOz</bib>, <lok>&leftquot;la valo de vo&ccirc;oj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="bg">&c_v;&c_je;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="ca">veranda</trd>
  <trd lng="cs">veranda <klr>(zasklená nebo otevřená)</klr></trd>
  <trd lng="de">Veranda</trd>
  <trd lng="el">&my;&pi;&alfa;&lambda;&kappa;&omikron_ton;&ny;&jota;</trd>
  <trd lng="en">veranda</trd>
  <trd lng="es">veranda</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&f_y;&a_w;&a_A;&a_n;</trd>,
    <trd>&a_m;&a_h;&a_t;&a_A;&a_b;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">veranda</trd>
  <trd lng="hu">veranda</trd>
  <trd lng="nl">veranda</trd>
  <trd lng="pt">varanda</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">veranda</trd>
</drv>
</art>
