<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korset.xml,v 1.18 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>korset</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korset.0o">
  <kap><tld/>o</kap>
  <snc mrk="korset.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      La&ccirc;ebla virina <ref tip="super" cel="vest.sub0o">subvesto</ref>,
      kelkfoje garnita per balena&jcirc;oj, por subteni la bruston:
      <ekz>
        la <tld/>o estas kontra&ubreve;higiena;
      </ekz>
      <ekz>
        &scirc;i kombis siajn blondajn harojn, metis belan <tld/>on
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk>Felo de Azeno</vrk>, 
          <lok><url
          ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">Rakontoj
          pri feinoj</url>, Hachette, 1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        surhavante nur ru&gcirc;silkan <tld/>on kaj malnovan veston
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 482a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_s;&c_e;&c_t;</trd>
  <trd lng="ca">cotilla</trd>
  <trdgrp lng="cs">
    <trd>korzáž</trd>,
    <trd>šněrovačka</trd>
  </trdgrp>
  <trd lng="de">Korsett</trd>
  <trd lng="en">corset</trd>
  <trd lng="es">cors&eacute;</trd>
  <trd lng="fr">corset</trd>
  <trdgrp lng="hu">
    <trd>f&udblac;z&odblac;</trd>,
    <trd>corset</trd>,
    <trd>m&iacute;der</trd>        
  </trdgrp>
  <trdgrp lng="nl">
    <trd>corset</trd>,
    <trd>keurslijf</trd>
  </trdgrp>
  <trd lng="pl">gorset</trd>
  <trdgrp lng="pt">
    <trd>espartilho</trd>,
    <trd>colete <klr tip="amb">(&ecirc;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_s;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>korzet</trd>,
    <trd>šnurovačka</trd>
  </trdgrp>
</drv>
</art>
