    <trd>špinavý</trd>
  </trdgrp>
  <trd lng="de">schmutzig</trd>
  <trd lng="en">dirty</trd>
  <trd lng="es">sucio</trd>
  <trdgrp lng="fr">
    <trd>impur</trd>,
    <trd>sale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>piszkos</trd>,
    <trd>szennyes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vuil</trd>,
    <trd>vies</trd>
  </trdgrp>
  <trdgrp lng="pt">
