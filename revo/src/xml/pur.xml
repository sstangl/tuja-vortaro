<drv mrk="pur.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.mal0igi.KOMUNE">
    <dif>
      Enmeti, enmiksi fremdan, misan a&ubreve; na&ubreve;zan substancon:
      <ekz>
        mal<tld/>igi pantalonon per sa&ubreve;co.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="makul.0i">makuli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>embrutar</trd>,
    <trd>embrutir</trd>
  </trdgrp>
  <trd lng="cs">zašpinit</trd>
  <trdgrp lng="de">
    <trd>beschmutzen</trd>,
    <trd>verschmutzen</trd>
  </trdgrp>
  <trd lng="en">soil</trd>
  <trd lng="es">ensuciar</trd>
  <trdgrp lng="fr">
    <trd>salir</trd>,
    <trd>souiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bepiszkol</trd>,
    <trd>beszennyez</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vuil maken</trd>,
    <trd>vervuilen<klr tip="ind"> (vuil maken)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sujar</trd>,
    <trd>emporcalhar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zamazať</trd>,
    <trd>zašpiniť</trd>,
    <trd>znečistiť</trd>
  </trdgrp>
  <trd lng="sv"><ind>smutsa</ind> ner</trd>
  <trd lng="tp">jaki</trd>
</drv>

<drv mrk="pur.mal0igxi">
  <kap><ofc>*</ofc>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pur.mal0igxi.KOMUNE">
    <dif>
      Difekti&gcirc;i pro almeto de io fremda, malboniga:
      <ekz>
        en la kota vetero mia vesto forte mal<tld/>i&gcirc;is
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        en ilia bu&scirc;o la sankta vorto amo
        mal<tld/>i&gcirc;as<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">get dirty</trd>
  <trd lng="es">ensuciarse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>salir</ind></trd>,
    <trd>se <ind>souiller</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bepiszkol&oacute;dik</trd>,
    <trd>beszennyez&odblac;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vuil worden</trd>,
    <trd>vervuilen<klr tip="ind"> (vuil worden)</klr></trd>
  </trdgrp>
  <trd lng="pt">sujar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_ch;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pur.naz0igi">
  <kap>naz<tld/>igi</kap>
  <snc mrk="pur.naz0igi.KOS">
    <dif>
      Senmukigi la nazon;
      <ref tip="sin" cel="mung.0i.KOS">mungi</ref>:
      <ekz>
        Elvira prenis paperan po&scirc;tukon, brue naz<tld/>igis 
        en &gcirc;in, kaj ploris
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url
          ref="http://www.esperanto.org.uk/eldonoj/piron/tekstoj/Ili_kaptis_Elzan.pdf">
          Ili kaptis Elzan!</url></vrk>,
          <lok>1985</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>smrkat</trd>,
    <trd>čistit si nos</trd>
  </trdgrp>
  <trd lng="fr">se <ind>moucher</ind></trd>
  <trdgrp lng="sk">
    <trd>smrkať</trd>,
    <trd>čistiť nos</trd>
  </trdgrp>
</drv>

<drv mrk="pur.ripoz0igi">
  <kap>ripoz<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.ripoz0igi.KOMUNE">
    <dif>
      <ref tip="super" cel="pur.0igi"><tld lit="P"/>igi</ref>
      per ripozigo, precipe likva&jcirc;on, kies malpura&jcirc;oj tiel
      alfundi&gcirc;as:
      <ekz>
        mi ripoz<tld/>igos iliajn akvojn
        <fnt><bib>MT</bib>, <lok>&Jeh; 32:14</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekant.0i">dekanti</ref>,
      <ref cel="precip1.0i.KEM">precipiti</ref>
    </refgrp>
  </snc>
  <trd lng="de">dekantieren</trd>
  <trd lng="en">decant</trd>
  <trd lng="fr">d&eacute;canter</trd>
  <trdgrp lng="hu">
    <trd>der&iacute;t</trd>,
    <trd>&uuml;lep&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">decanteren</trd>
  <trd lng="pt">decantar</trd>
  <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="pur.visx0igi">
  <kap>vi&scirc;<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pur.visx0igi.HEJM">
    <dif>
      <ref tip="super" cel="pur.0igi"><tld lit="P"/>igi</ref>
      per <ref tip="vid" cel="visx.0i.MOV">vi&scirc;ado</ref>:
      <ekz>
        ni demetis la bluzojn kaj per ili
        vi&scirc;<tld/>igis la plankon
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>astiquer</trd>,
    <trd>essuyer</trd>
  </trdgrp>
</drv>
