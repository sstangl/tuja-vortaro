<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adapt.xml,v 1.33 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>3</ofc><rad>adapt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="adapt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="adapt.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ta&ubreve;ge, &gcirc;uste kunigi ion materian al io:
      <ekz>
        <tld/>i tenilon al vazo, seruron al pordo.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>incorporar</trd>,
      <trd>incloure</trd>,
      <trd>ajustar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>adapt</trd>,
      <trd>fit</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>adaptar</trd>,
      <trd>ajustar</trd>,
      <trd>ajeitar</trd>,
      <trd>acomodar</trd>
    </trdgrp>
  </snc>
  <snc mrk="adapt.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konformigi por specialaj celoj a&ubreve; kondi&ccirc;oj:
      <ekz>
        oni devas
        <tld/>i sian parolon al la cirkonstancoj;
      </ekz>
      <ekz>
        tiuj versoj estas malbone <tld/>itaj al la muziko.
      </ekz>
    </dif>
    <trd lng="ca">adaptar</trd>
    <trdgrp lng="en">
      <trd>adapt</trd>,
      <trd>adjust</trd>
    </trdgrp>
    <trd lng="sk">prisp&ocirc;sobi&tcaron;</trd>
    <trd lng="sv">anpassa</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="far.al0i">alfari</ref>,
    <ref cel="gxust.al0igi">al&gcirc;ustigi</ref>,
    <ref cel="akomod.0i">akomodi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">adaptar</trd>
  <trd lng="de">anpassen</trd>
  <trd lng="es">adaptar</trd>
  <trdgrp lng="fr">
    <trd>adapter</trd>,
    <trd>approprier</trd>,
    <trd>ajuster</trd>,
    <trd>assortir</trd>,
    <trd>conformer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>adapt&aacute;l</trd>,
    <trd>alkalmass&aacute; tesz</trd>,
    <trd>hozz&aacute;igaz&iacute;t</trd>
  </trdgrp>
  <trd lng="it">adattare</trd>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pl">
    <trd>adaptowa&cacute;</trd>,
    <trd>przystosowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prispôsobiť</trd>,
    <trd>upraviť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>adaptovat</trd>,
    <trd>přizpůsobit</trd>,
    <trd>zadaptovat</trd>
  </trdgrp>
</drv>

<drv mrk="adapt.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="adapt.0igxi.KOMUNE">
    <dif>
      Akordi&gcirc;i al io, konformi&gcirc;i al premo, al 
      cirkonstanco:
      <ekz>
        pri sia bofrato Elias havis la impreson, ke li estas tre agema 
        kaj rapide komprenanta homo, kiu scipovas <tld/>i&gcirc;i al la 
        cirkonstancoj
        <fnt>
          <aut>P. Kuusberg, trad. A. Lekko</aut>,
          <vrk><url
          ref="http://miresperanto.narod.ru/biblioteko/somermeze.htm">
          Somermeze</url></vrk>,
          <lok>1981</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akomod.0igxi.KOMUNE">akomodi&gcirc;i</ref>,
      <ref cel="kongru.0i.KOMUNE">kongrui</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_a;&c_p;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca"><ind>adaptar</ind>-se</trd>
  <trd lng="fr">s'<ind>adapter</ind></trd>
  <trdgrp lng="pl">
    <trd>adaptowa&cacute; si&eogonek;</trd>,
    <trd>przystosowa&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="adapt.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="adapt.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="arangx.0ajxo.TEK">Aran&gcirc;a&jcirc;o</ref>
      destinita por ebligi kunan laboron de diversaj aparatoj,
      cirkvitoj, <ref cel="organ.0o.TEK">organoj</ref> de
      ma&scirc;ino a&ubreve; komputilo ktp:
      <ekz>
        ekrana <tld/>ilo SVGA.
      </ekz>
    </dif>
    <rim>
      <tld lit="A"/>ilo estas pli inteligenta
      <ref tip="super" cel="fac.inter0o">interfaco</ref> ol simpla
      <ref cel="junt.0o.ilo">junto</ref>,
      <ref cel="konver3.0i.KOMP">konvertanta</ref> signalojn
      a&ubreve; <tld/>anta fizikajn parametrojn de la kunigataj
      aparatoj.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_a;&c_p;&c_t;&c_a;&c_r;</trd>,
    <!-- &c_a;&c_d;&c_a;&c_p;&c_t;&c_e;&c_r;? -->
    <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_d;&c_a;&c_p;&c_t;&c_o;&c_r;</trd>
  <trd lng="ca">adaptador</trd>
  <trdgrp lng="cs">
    <trd>adaptér</trd>,
    <trd>přizpůsobovací zařízení</trd>,
    <trd>redukce</trd>
  </trdgrp>
  <trd lng="de">Adapter</trd>
  <trdgrp lng="en">
    <trd>adapter</trd>,
    <trd>controller</trd>
  </trdgrp>
  <trd lng="es">adaptador</trd>
  <trd lng="fr">adaptateur<klr tip="ind"> (dispositif)</klr></trd>
  <trd lng="hu">adapter</trd>
  <trd lng="nl">adapter</trd>
  <trdgrp lng="pl">
    <trd>adapter</trd>,
    <trd>przetwornik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_a;&c_p;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_o;&c_l;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>adapt&eacute;r</trd>,
    <trd>redukcia</trd>
  </trdgrp>
</drv>
</art>
