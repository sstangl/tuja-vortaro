<art mrk="$Id: finland.xml,v 1.10 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>Finland</rad>/o
</kap>

<drv mrk="finland.0o">
  <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo> 
      <ref tip="dif" cel="finn.F0lando">Finnlando</ref>
      <ekz>
        Prosperu kaj floru la bela <tld/>o . . . Feli&ccirc;aj estu la
        &ccirc;armaj <tld lit="F"/>anoj.<fnt><aut>John Merchant</aut>:
        <vrk>Kompatinda Klem.
        Londono: Esperanto Publishing Company, 1931</vrk>,
        <lok>p. 40</lok></fnt>
      </ekz>
  </snc>
  <trd lng="bg">&c_F;&c_i;&c_n;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="cs">Finsko</trd>
  <trdgrp lng="de">
    <trd>Finnland</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Finlande</trd>,
    <trd>R&eacute;publique de Finlande</trd>
  </trdgrp>
  <trd lng="pl">Finlandia</trd>
  <trd lng="pt">Finl&acirc;ndia</trd>
  <trd lng="sk">FÃ­nsko</trd>
</drv>

<drv mrk="finland.f0ano">
  <kap><tld lit="f"/>ano</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="finn.0o">Finno</ref>; finnlandano:
      <ekz>
        Sed kvankam la belegeco kaj la allogeco de la lando
        &gcirc;ojigas
        la korojn, vizitantoj trovas ke la &ccirc;armo de la
        <tld lit="f"/>anoj grandamezure superas la naturajn
        vidinda&jcirc;ojn.<fnt><aut>John Merchant</aut>:
        <vrk>Kompatinda Klem.
        Londono: Esperanto Publishing Company, 1931</vrk>,
        <lok>p. 38</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_f;&c_i;&c_n;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  <trdgrp lng="de">
    <trd>Finne</trd>
  </trdgrp>
  <trd lng="fr">Finlandais<klr> (habitant)</klr></trd>
  <trd lng="pl">Fin</trd>
</drv>
</art>

