<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirliton.xml,v 1.6 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mirliton</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mirliton.0o">
  <kap><tld/>o</kap>
  <snc mrk="mirliton.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="muzik.0ilo"
      lst="voko:muzikiloj">Muzikilo</ref> kaj <ref tip="lst"
      cel="lud.0ilo" lst="voko:ludiloj">ludilo</ref> konsistanta el
      tubeto
      fermita per membrano, kiu vibras samsone
      kun la vo&ccirc;o, &gcirc;in obtuzigante kaj la&ubreve;tigante:
      <ekz>
        je &ccirc;iu movo <tld/>oj kaj fifroj kariljonis
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">mirlit&oacute;</trd>
  <trd lng="cs">mirliton</trd>
  <trdgrp lng="fr">
    <trd>bigophone</trd>,
    <trd>kazoo</trd>,
    <trd>mirliton<klr> (musique)</klr></trd>
  </trdgrp>
  <trd lng="sk">mirliton</trd>
</drv>
</art>

