<kap>
  <rad>varik</rad>/o
</kap>
<drv mrk="varik.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la vejnaj parioj.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;
    <ind>&c_v;&c_je;&c_n;&c_a;&c_w;</ind></trd>
  <trdgrp lng="ca">
    <trd>vari&ccedil;a</trd>,
    <trd>variu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&rcaron;e&ccaron;ov&aacute; &zcaron;&iacute;la</trd>,
    <trd>m&ecaron;stka</trd>,
    <trd>varix</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Krampfader</trd>,
    <trd>Varize</trd>
  </trdgrp>
  <trd lng="el">&kappa;&jota;&rho;&sigma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">varix</trd>
  <trd lng="es">variz</trd>
  <trd lng="fa">&a_w;&a_A;&a_r;&f_y;&a_s;</trd>
  <trd lng="fr">varice</trd>
  <trdgrp lng="hu">
    <trd>visszaeres csom&oacute;</trd>,
    <trd>g&ouml;rcs&eacute;r</trd>
  </trdgrp>
  <trd lng="id">varises</trd>
  <trd lng="nl">spatader</trd>
  <trd lng="pl">&zdot;ylak</trd>
  <trd lng="pt">variz</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je; &c_v;&c_je;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>k&racute;&ccaron;ov&aacute; &zcaron;ila</trd>,
    <trd>z&aacute;pal vn&uacute;torn&eacute;ho povrchu ciev</trd>
  </trdgrp>
</drv>
</art>
