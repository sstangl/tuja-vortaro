<kap>
  <ofc>1</ofc>
  <rad>kazern</rad>/o
</kap>
<drv mrk="kazern.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Konstrua&jcirc;o destinita por lo&gcirc;igi soldatojn.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
  <trd lng="br">kazarn</trd>
  <trdgrp lng="cs">
    <trd>kasárna</trd>,
    <trd>kasárny</trd>
  </trdgrp>
  <trd lng="de">Kaserne</trd>
  <trdgrp lng="en">
    <trd>barracks</trd>,
    <trd>soldiers' quarters</trd>
  </trdgrp>
  <trd lng="fr">caserne</trd>
  <trdgrp lng="hu">
    <trd>laktanya</trd>,
    <trd>kasz&uacute;rnya</trd>
  </trdgrp>
  <trd lng="it">caserma</trd>
  <trd lng="nl">kazerne</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_a;&c_r;&c_m;&c_a;</trd>
  <trd lng="sk">kasáreň</trd>
</drv>
</art>
