<kap>
   <ofc>4</ofc>
   <rad>stamen</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="stamen.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Maskla organo de la <ref tip="malprt"
       cel="flor.0o.BOT">floro</ref>, ordinare konsistanta el fadeno
       portanta anteron kun la polenujoj:
       <ekz>
         la flavaj <tld/>oj de lilioj<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_y;&c_ch;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">estam</trd>
  <trd lng="cs">tyčinka</trd>
  <trd lng="de">Staubblatt</trd>
  <trd lng="en">stamen</trd>
  <trd lng="fa">&f_p;&a_r;&f_ch;&a_m;<klr tip="ind"> (&f_g;&f_y;&a_A;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;)</klr></trd>
  <trd lng="fr">&eacute;tamine</trd>
  <trd lng="hu">porz&oacute;</trd>
  <trd lng="nl">meeldraad</trd>
  <trd lng="ru">&c_t;&c_y;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">tyčinka</trd>
  <trd lng="sv">st&aring;ndare</trd>
</drv>
</art>
