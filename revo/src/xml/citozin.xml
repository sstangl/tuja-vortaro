<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: citozin.xml,v 1.4 2013/12/18 14:10:11 revo Exp $">
<kap>
  <rad>citozin</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="citozin.0o">
  <kap><tld/>o</kap>
  <snc mrk="citozin.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Iu el la 
      <ref tip="super" cel="nukleotid.0o.GNT">nukleotido</ref>specoj
      de ADN a&ubreve; ARN;
      simbole <frm>C<sub>4</sub>H<sub>5</sub>N<sub>3</sub>O</frm>:
      <ekz>
        adenino kaj timino &ccirc;iam trovi&gcirc;as la&ubreve; proporcio 1:1, kiel anka&ubreve; 
        <tld/>o kaj guanino
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Nuklea_acido">
          nuklea acido</url>, 2010-08-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_c;&c_i;&c_t;&c_o;&c_z;&c_i;&c_n;</trd>
  <trd lng="ca">citosina</trd>
  <trd lng="da">cytosin</trd>
  <trd lng="de">Cytosin</trd>
  <trd lng="en">cytosine</trd>
  <trd lng="es">citosina</trd>
  <trd lng="fr">cytosine</trd>
  <trd lng="hu">citozin</trd>
  <trd lng="nl">cytosine</trd>
  <trd lng="sv">cytosin</trd>
</drv>
</art>

