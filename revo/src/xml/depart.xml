<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: depart.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>departement</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="depart.0o">
  <kap><tld/>o</kap>
  <snc mrk="depart.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ampleksa <ref tip="super" cel="sekci.0o.POL">sekcio</ref> de 
      ministrejo a&ubreve; de granda administrejo:
      <ekz>
        <tld/>o pri fervojoj.
      </ekz>
      <ekz>
        ili mem devas pagi al la &scirc;tataj komunikaj <tld/>oj la&ubreve;
        la oficialaj prezoj
        <fnt><aut>Liu Xiaozhe</aut>, <vrk>Interreto en
        malfacilo</vrk>, <bib>Monato</bib>, <lok>jaro 1997a, numero 8a,
        p.
        15a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Beh&ouml;rde</trd>
    </trdgrp>
    <trd lng="en">administrative department</trd>
    <trdgrp lng="hu">
      <trd>&uuml;gyoszt&aacute;ly</trd>,
      <trd>f&odblac;oszt&aacute;ly</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_v;&c_je;&c_d;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="depart.0o.franca">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="gubern1.0o.POL">Gubernio</ref> en Francio:
      <ekz>
        ili estis vizitintaj <klr>...</klr> la marbordon de la <tld/>o 
        <em>Finist&egrave;re</em>
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">D&eacute;partement</trd>
    <trd lng="be">&c_d;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>
    <trd lng="en">French administrative district</trd>
  </snc>
  <trd lng="ca">departament</trd>
  <trdgrp lng="cs">
    <trd>departement ve Francii</trd>,
    <trd>odbor</trd>,
    <trd>oddělení <klr>(úřadu / instituce)</klr></trd>
  </trdgrp>
  <trd lng="es">departamento</trd>
  <trd lng="fr">d&eacute;partement</trd>
  <trd lng="hu">megye</trd>
  <trd lng="pl">departament</trd>
  <trd lng="pt">departamento</trd>
  <trd lng="ru">&c_d;&c_je;&c_p;&c_a;&c_r;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>odbor</trd>,
    <trd>oddelenie</trd>
  </trdgrp>
</drv>
</art>
