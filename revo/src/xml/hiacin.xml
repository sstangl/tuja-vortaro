<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hiacin.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>hiacint</rad>/o
</kap>
<drv mrk="hiacin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Hyacinthus</trd>),
      bulba vegeta&jcirc;o kun forte odorantaj origine malhelbluaj
      floroj, kun diverskoloraj &gcirc;ardene kulturataj specioj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_ja;&c_c;&c_y;&c_n;&c_t;</trd>
  <trd lng="bg">&c_z;&c_ju;&c_m;&c_b;&c_ju;&c_l;</trd>
  <trdgrp lng="br">    
    <trd>louzaouenn-ar-goukoug</trd>,
    <trd>pourenn-vran</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hyacint</trd>,
    <trd>hyacinta</trd>
  </trdgrp>
  <trd lng="de">Hyazinthe</trd>
  <trd lng="el">&ypsilon;&alfa_ton;&kappa;&jota;&ny;&theta;&omikron;&sigma_fina;</trd>
  <trd lng="en">hyacinth</trd>
  <trd lng="es">jacinto</trd>
  <trd lng="fa">&a_s;&a_n;&a_b;&a_l;</trd>
  <trd lng="fr">jacinthe</trd>
  <trd lng="hu">j&aacute;cint</trd>
  <trd lng="it">giacinto</trd>
  <trd lng="nl">hyacint</trd>
  <trd lng="pl">hiacynt</trd>
  <trd lng="pt">jacinto</trd>
  <trd lng="ru">&c_g;&c_i;&c_a;&c_c;&c_i;&c_n;&c_t;</trd>
  <trd lng="sk">hyacint</trd>
</drv>
</art>
