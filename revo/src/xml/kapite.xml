  </trdgrp>
  <trd lng="hu">oszlopf&odblac;</trd>
  <trd lng="nl">kapiteel</trd>
  <trdgrp lng="pl">
    <trd>kapitel</trd>
    <trd>g&lstroke;owica</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">hlavica stÄºpu</trd>
