  <rad>Albert</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="albert.0o">
  <kap><tld/>o</kap>
  <snc mrk="albert.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        <tld/>o kaj Viktoro estas fizikistoj
        <fnt>
        <aut>Y. Kotilevski</aut>
        <vrk>
    &c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_o;-&c_k;&c_l;&c_u;&c_b;:
&c_u;&c_r;&c_o;&c_k; 2, &c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_je;&c_m;
        </vrk>
        <lok>fido7.ru.baby.mail, 2000-02-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_l;&c_mol;&c_b;&c_e;&c_r;&c_t;</trd>
  <trd lng="cs">Albert</trd>
  <trd lng="de">Albert</trd>
  <trd lng="en">Albert</trd>
  <trd lng="fr">Albert</trd>
  <trd lng="hu">Albert</trd>
  <trd lng="it">Alberto</trd>
  <trd lng="nl">Albert</trd>
