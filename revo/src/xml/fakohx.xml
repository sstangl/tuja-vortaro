<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: fakohx.xml,v 1.6 2013/08/06 09:10:18 revo Exp $">
    <kap>
      <rad>fako&hcirc;er</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="fakohx.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ZOO</uzo>

      <ref tip="dif" cel="fakocx.0o">fako&ccirc;ero</ref>
      <ref tip="sin" cel="apr.veruk0o">verukapro</ref>
      <trd lng="bg">&c_b;&c_r;&c_a;&c_d;&c_a;&c_v;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
      <trd lng="ca">facoquer</trd>
      <trd lng="de">Warzenschwein</trd>
      <trd lng="en">warthog</trd>
      <trd lng="fr">phacoch&egrave;re</trd>
      <trd lng="hu">varacskos <ind>diszn&oacute;</ind></trd>
      <trd lng="it">facocero</trd>
      <trd lng="nl">wrattenzwijn</trd>
      <trdgrp lng="pl">
        <trd>guziec</trd>,
        <trd>&sacute;winia afryka&nacute;ska</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>javali-africano</trd>,
        <trd>facochero</trd>,
        <trd>porco-de-verrugas</trd>
      </trdgrp>
    </drv>

  </art>

