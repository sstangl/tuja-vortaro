<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: irtisx.xml,v 1.7 2013/09/06 19:10:38 revo Exp $">
<kap>
  <rad>Irti&scirc;</rad>/o
  <fnt>
  <aut>E. Bokarev</aut>: <vrk>Esperanta-rusa vortaro</vrk>,
  <lok>Moskvo, 1982. P. 468.</lok></fnt>
</kap>
<drv mrk="irtisx.I0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda kaj longa <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">rivero</ref> en
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>,
      maldekstra alfluanto de <ref cel="ob.O0o">Obo</ref> (je 61&#730;20&#8242;N).
      <tld/>o fontas el &Ccirc;inia <ref cel="altaj.A0o">Altajo</ref>
      (88&#730;10&#8242;E, 47&#730;30&#8242;N), fluas tra Kaza&hcirc;ujo kaj Rusujo
      (la longo 4248km, la basenareo 1643km&quadrat;).
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_r;&c_t;&c_y;&c_sh;</trd>
  <trd lng="de">Irtisch</trd>
  <trd lng="en">Irtysh</trd>
  <trd lng="fr">Irtych</trd>
  <trd lng="hu">Irtis</trd>
  <trdgrp lng="pt">
    <trd>Irtych</trd>,
    <trd>Irtiche</trd>
  </trdgrp>
  <trd lng="ru">&c_I;&c_r;&c_t;&c_y;&c_sh;</trd>
</drv>
</art>
