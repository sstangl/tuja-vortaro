<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pastec.xml,v 1.14 2009/01/22 17:30:46 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>paste&ccirc;</rad>/o
    </kap>

    <drv mrk="pastec.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">KUI</uzo>
      <snc mrk="pastec.0o.pirogo">
        <ref tip="dif" cel="pirog.0o.KUI">pirogo<sncref/></ref>
        <trd lng="en">pie</trd>
        <trd lng="fr">p&acirc;t&eacute;<klr> (en
        cro&ucirc;te)</klr></trd>
        <trdgrp lng="pl">
          <trd>pasztecik</trd>,
          <trd>krokiet</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_g;</trd>
      </snc>
      <snc mrk="pastec.0o.bovla">
        <dif>
          Hakita spicita viando a&ubreve; hepato miksita kun graso kaj
          bakita en terpoto, ofte uzata kiel
          anta&ubreve;man&gcirc;a&jcirc;o (kun pano).
        </dif>
        <trd lng="en">p&acirc;t&eacute;</trd>
        <trd lng="fr">p&acirc;t&eacute;<klr> (en terrine)</klr></trd>
        <trd lng="pl">pasztet</trd>
        <trd lng="ru">&c_p;&c_a;&c_sh;&c_t;&c_je;&c_t;</trd>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_sh;&c_t;&c_e;&c_t;</trd>
      <trd lng="br">formaj-kig</trd>
      <trd lng="de">Pastete</trd>
      <trd lng="hu">p&aacute;st&eacute;tom</trd>
      <trd lng="nl">pastei</trd>
      <trdgrp lng="pt">
        <trd>pastel</trd>,
        <trd>empada</trd>
      </trdgrp>
    </drv>
  </art>
