<vortaro>
<art mrk="$Id: orgi.xml,v 1.18 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>2</ofc> <rad>orgi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="orgi.0o">
  <kap><tld/>o</kap>
  <snc mrk="orgi.0o.MOR">
    <dif>
      Man&gcirc;a, trinka, precipe seksumada <ref tip="super"
      cel="dibocx.0o.MOR">dibo&ccirc;o</ref>:
      <ekz>
        <ctl>Sep Aferoj Edifaj</ctl> <klr>...</klr> fine venis al la 
        konkludo, ke la <tld/>a vivo estas malpli delektinda ol trafaj 
        vortoj kaj spritaj rezonoj
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url
        ref="http://esperanto.cri.cn/1/2004/07/15/1@12298.htm">
        La fu-o de Han-dinastio</url></vrk>,
        <lok>2004-07-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_o;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>hýření</trd>,
    <trd>orgie</trd>
  </trdgrp>
  <trd lng="de">Orgie</trd>
  <trd lng="el">&omikron_ton;&rho;&gamma;&jota;&omikron;</trd>
  <trd lng="en">orgy</trd>
  <trdgrp lng="fr">
    <trd>orgie</trd>,
    <trd>partouze</trd>
  </trdgrp>
  <trd lng="hu">orgia</trd>
  <trd lng="nl">orgie</trd>
  <trdgrp lng="pt">
    <trd>orgia</trd>,
    <trd>bacanal</trd>,
    <trd>suruba</trd>,
    <trd>surubada</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>hýrenie</trd>,
    <trd>orgia</trd>
  </trdgrp>
  <trd lng="sv">orgie</trd>
</drv>
</art>
