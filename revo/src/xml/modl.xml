<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: modl.xml,v 1.12 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>modl</rad>/i
</kap>
<drv mrk="modl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Formi el moligita materio iun plastikan objekton,
      kiun oni knedas kaj reliefigas per fingroj a&ubreve; per speciala ilo. 
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>formovat</trd>,
    <trd>modelovat</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>moldear</trd>,
    <trd>modelar</trd>
  </trdgrp>
  <trd lng="fr">modeler</trd>
  <trdgrp lng="nl">
    <trd>modelleren</trd>,
    <trd>boetseren</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">modelova≈•</trd>
</drv>
<drv mrk="modl.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo por doni plastikan formon al moligita materio.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_ib;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">forma na odlitky</trd>
  <trdgrp lng="fr">
    <trd>&eacute;bauchoir</trd>,
    <trd>mirette</trd>
  </trdgrp>
  <trd lng="nl">boetseerhoutje</trd>
  <trd lng="pt">modelar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">forma na odliatky</trd>
</drv>
</art>
