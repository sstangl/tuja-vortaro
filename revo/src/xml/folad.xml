<vortaro>
<art mrk="$Id: folad.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>folad</rad>/o
</kap>
<drv mrk="folad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="folade.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pholas</trd>), mara konko, kiu boras en rokoj.
    </dif>
    <ref tip="sin" cel="konk.bor0o">borkonko</ref>
  </snc>
  <trd lng="ca">f&ograve;lada</trd>
  <trd lng="cs">šaškování</trd>
  <trd lng="de">Bohrmuschel</trd>
  <trd lng="fr">pholade</trd>
  <trd lng="hu">f&uacute;r&oacute;kagyl&oacute;</trd>
  <trd lng="nl">boormossel</trd>
  <trd lng="pt">f&oacute;lade</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_l;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_n;&c_je;&c_t;&c_o;&c_ch;&c_je;&c_c; <klr tip="amb">(&c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="sk">šaškovanie</trd>
</drv>
</art>
