<art mrk="$Id: nugat.xml,v 1.14 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>nugat</rad>/o
</kap>
<drv mrk="nugat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Franda&jcirc;o el miela a&ubreve; karamela pasto enhavanta migdalojn
kaj
      pistakojn; migdalmiela&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_g;&c_a;</trd>
  <trd lng="cs">nugát</trd>
  <trd lng="de">Nugat</trd>
  <trd lng="el">&ny;&omikron;&ypsilon;&gamma;&kappa;&alfa_ton;</trd>
  <trd lng="fr">nougat</trd>
  <trd lng="hu">nug&aacute;t</trd>
  <trd lng="nl">noga</trd>
  <trd lng="pl">nugat</trd>
  <trd lng="pt">nug&aacute;</trd>
  <trd lng="ru">&c_n;&c_u;&c_g;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nugat</trd>,
    <trd>nugát</trd>
  </trdgrp>
</drv>
</art>
