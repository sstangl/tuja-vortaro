<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tred.xml,v 1.9 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>tred</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tred.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tred.0i.JUV">
    <dif>
      <ref tip="super" cel="tra.0igi">Traigi</ref>, tiri de unu 
      flanko &gcirc;is alia flanko de truo:
      <ekz>
        alia ludo <klr>...</klr> postulas &ccirc;iujn ludantojn <tld/>i
        ru&gcirc;an 
        fadenon en 7 kudriltruojn en sama vico sur tabulo
        <fnt>
          <aut>Gao Xin</aut>,
          <vrk>
<url ref="http://esperanto.china.org.cn/kulturo/2007-06/12/
content_8377993.htm">
          Geamanta tago en &Ccirc;inio</url></vrk>,
          <lok>[2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>prostrčit</trd>,
    <trd>protahovat</trd>
  </trdgrp>
  <trd lng="de">einf&auml;deln</trd>
  <trd lng="en">thread</trd>
  <trd lng="fa">&a_n;&a_kh; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">enfiler</trd>
  <trd lng="hu">bef&udblac;z</trd>
  <trd lng="it">infilare<klr> (attraverso buchi)</klr></trd>
  <trd lng="nl">vademen<klr tip="ind"> (draad in naald
  bijv.)</klr></trd>
  <trd lng="sk">navliecť <klr>(niť| šnúrku ap.)</klr></trd>
</drv>
</art>

