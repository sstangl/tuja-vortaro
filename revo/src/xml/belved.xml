<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: belved.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <rad>belveder</rad>/o
    <fnt>V</fnt>
  </kap>
  <drv mrk="belved.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Konstruo, teraso a&ubreve; alta loko, el kiu oni &gcirc;uas
        malproksiman 
        vida&jcirc;on.
      </dif>
      <refgrp tip="vid">
        <ref cel="altan.0o">altano</ref>,
        <ref cel="balkon.0o">balkono</ref>.
      </refgrp>
      <trd
      lng="be">&c_b;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_d;&c_e;&c_r;</trd>
      <trdgrp lng="de">
        <trd>Aussichtsplattform</trd>,
        <trd>Aussichtsterrasse</trd>,
        <trd>Aussichtspunkt</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>viewing platform</trd>,
        <trd>scenic overlook</trd>
      </trdgrp>
      <trd lng="fr">belv&eacute;d&egrave;re</trd>
      <trdgrp lng="hu">
        <trd>kil&aacute;t&oacute;</trd>,
        <trd>belvedere</trd>
      </trdgrp>
      <trd lng="pl">belweder</trd>
      <trd
      lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>belvedér</trd>,
    <trd>letohrádek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>letohrádok</trd>,
    <trd>rozhľadňa</trd>
  </trdgrp>
</drv>
</art>
