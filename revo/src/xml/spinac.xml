<drv mrk="spinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kenopo1.0oj">kenopodiacoj</ref>
      (<trd lng="la">Spinacia</trd>); la verdajn foliojn de unu specio
      (<trd lng="la">Spinacia oleracea</trd>) oni man&gcirc;as
      kiel legomo.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_ib;&c_n;&c_a;&c_t;</trd>
  <trd lng="bg">&c_s;&c_p;&c_i;&c_n;&c_a;&c_k;</trd>
  <trd lng="cs">코pen치t</trd>
  <trd lng="de">Spinat</trd>
  <trd lng="en">spinach</trd>
  <trd lng="es">espinaca</trd>
  <trd lng="fa">&a_A;&a_s;&a_f;&a_n;&a_A;&a_j;</trd>
  <trd lng="fr">&eacute;pinard</trd>
  <trdgrp lng="hu">
    <trd>spen&oacute;t</trd>,
    <trd>paraj</trd>
  </trdgrp>
  <trd lng="id">bayam</trd>
  <trd lng="nl">spinazie</trd>
  <trd lng="pl">szpinak</trd>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_n;&c_a;&c_t;</trd>
  <trd lng="sk">코pen치t</trd>
  <trd lng="tr">&inodot;spanak</trd>
</drv>
</art>
