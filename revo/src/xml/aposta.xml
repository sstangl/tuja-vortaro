<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aposta.xml,v 1.15 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>apostat</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="aposta.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="renega.0o">renegato</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_r;&c_e;&c_n;&c_e;&c_g;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">ap&ograve;stata</trd>
  <trdgrp lng="cs">
    <trd>apostata</trd>,
    <trd>odpadlík</trd>,
    <trd>odpadlík <klr>(od víry, nauky, polit. strany)</klr></trd>,
    <trd>odrodilec</trd>,
    <trd>odštěpenec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Abgefallener</trd>,
     <trd>Abtr&uuml;nniger</trd>,
     <trd>Renegat [pol.]</trd>
  </trdgrp>
  <trd lng="en">apostate</trd>
  <trd lng="es">ap&oacute;stata</trd>
  <trd lng="fr">apostat</trd>
  <trdgrp lng="hu">
     <trd>aposztata</trd>,
     <trd>hitehagyott</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
     <trd>apostata</trd>,
     <trd>odpadl&iacute;k</trd>
  </trdgrp>
</drv>
</art>
