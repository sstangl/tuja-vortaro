<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fuk.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fuk</rad>/o
</kap>
<drv mrk="fuk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro de maraj brun<ref tip="super" cel="alg.0oj">algoj</ref>
      (<trd lng="la">Fucus</trd>).
    </dif>
  </snc>
  <trd lng="cs">chaluha</trd>
  <trdgrp lng="de">
    <trd>Blasentang</trd>,
    <trd>S&auml;getang</trd> k.a.
  </trdgrp>
  <trd lng="fr">fucus</trd>
  <trdgrp lng="hu">
    <trd>fukusz</trd>,
    <trd>tengeri moszat</trd>,
    <trd>tengeri f&udblac;</trd>
  </trdgrp>
  <trd lng="pt">fuco</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_u;&c_k;&c_u;&c_s;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;&c_ja;
&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_s;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">chaluha</trd>
</drv>
</art>
