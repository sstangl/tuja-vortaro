<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: portugali.xml,v 1.11 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>Portugali</rad>/o
</kap>

<drv mrk="portugali.P0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <ref tip="dif" cel="portugal.P0ujo">Portugalujo</ref>
      <ekz>
        <tld lit="P"/>o estas e&ubreve;ropa lando trovi&gcirc;anta en
        sudokcidenta 
        terlango de la kontinento, en la okcidenta parto de Iberio.
        <fnt><bib>Viki</bib>, <lok><url        
        ref="http://eo.wikipedia.org/wiki/Portugalio">Portugalio</url></lok></fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_P;&c_o;&c_r;&c_t;&c_u;&c_g;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="cs">Portugalsko</trd>
  <trd lng="de">Portugal</trd>
  <trd lng="en">Portugal</trd>
  <trd lng="fa">&f_p;&a_r;&a_t;&a_gh;&a_A;&a_l;<klr tip="ind"> (&f_k;&a_sh;&a_w;&a_r;)</klr></trd>
  <trdgrp lng="fr">
    <trd>Portugual</trd>,
    <trd>R&eacute;publique portuguaise</trd>
  </trdgrp>
  <trd lng="id">Portugal</trd>
  <trd lng="nl">Portugal</trd>
  <trd lng="no">Portugal</trd>
  <trd lng="pl">Portugalia</trd>
  <trd lng="sk">Portugalsko</trd>
</drv>
</art>

