<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: invers.xml,v 1.31 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>invers</rad>/a
</kap>
<drv mrk="invers.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en mala, kontra&ubreve;a pozicio a&ubreve; direkto rilate al alia:
      <ekz>
        la antipodoj estas rilate al ni en <tld/>a pozicio;
      </ekz>
      <ekz>
        la gvidisto erarinte pri la vojo returniris en <tld/>a direkto;
      </ekz>
      <ekz>
        <tld/>a bildo;
      </ekz>
      <ekz>
        <tld/>a rilato
        <klr>(rilato de du kvantoj, kies unu pligrandi&gcirc;as la&ubreve; la
        mezuro kiel la alia malpligrandi&gcirc;as)</klr>;
      </ekz>
      <ekz>
        la korpoj sin altiras la&ubreve; <tld/>a rilato de la kvadrato
        de sia distanco.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>inverse<klr tip="ind"> (adj.)</klr></trd>,
      <trd>contraire<klr tip="ind"> (adj.)</klr></trd>,
      <trd>oppos&eacute;<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ford&iacute;tott</trd>,
      <trd>ellenkez&odblac;</trd>,
      <trd>ellent&eacute;tes</trd>
    </trdgrp>
  </snc>
  <snc mrk="invers.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Estanta <ref cel="invers.0o.MAT"><tld/>o <sncref/></ref>.
    </dif>
    <trd lng="de">inverse</trd>
    <trd lng="en">inverse</trd>
    <trd lng="fr">inverse<klr tip="ind"> (math.,adj.)</klr></trd>
    <trd lng="hu">inverz</trd>
    <trd lng="pl">odwrotny</trd>
    <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>inverzní</trd>,
    <trd>opačný</trd>,
    <trd>převrácený</trd>,
    <trd>reciproký</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umgekehrt</trd>,
    <trd>invers</trd>
  </trdgrp>
  <trd lng="es">inverso</trd>
  <trd lng="nl">omgekeerd</trd>
  <trd lng="pt">inverso</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>inverzný</trd>,
    <trd>opačný</trd>,
    <trd>prevrátený</trd>
  </trdgrp>
</drv>

<drv mrk="invers.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Io <tld/>a.
    </dif>
    <trdgrp lng="de">
      <trd>Umkehrung</trd>,
      <trd>umgekehrte Richtung</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>inverse<klr tip="ind"> (subst.)</klr></trd>,
      <trd>contraire<klr tip="ind"> (subst.)</klr></trd>,
      <trd>oppos&eacute;<klr tip="ind"> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ford&iacute;tottja</trd>,
      <trd>ellenkez&odblac;je</trd>
    </trdgrp>
    <trd lng="nl">iets <ind>omgekeerds</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_je; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>,
      <trd>&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
    </trdgrp>
    <trd lng="pt">inverso</trd>
  </snc>
  <snc mrk="invers.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="invers.0o.neuxtriganto">
      <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
      <dif>
        <ref cel="neuxtr.0iganto.MAT">Ne&ubreve;triganto</ref> rilate
        al multiplike signata <ref cel="operac.0o.MAT">operacio</ref>:
        <ekz>
          la <tld/>o de <frm>3</frm> estas <frm>&#8531;</frm>;
        </ekz>
        <ekz>
          por &ccirc;iu elemento de grupo ekzistas &gcirc;ia <tld/>o.
        </ekz>
      </dif>
      <rim>
        Verdire la citita fonto limigas la nocion al <tld/>oj de
        nombroj, sed &gcirc;i nature vasti&gcirc;as al &ccirc;ia
        multipliko, kiel aperas en
        <fnt><bib>MatStokTerm</bib><lok>p. 23</lok></fnt>.
      </rim>
      <trdgrp lng="de">
        <trd>Inverses</trd>,
        <trd>Reziprokes</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>inverse <ind>element</ind></trd>,
        <trd>reciprocal <ind>element</ind></trd>,
        <trd>multiplicative <ind>inverse</ind></trd>
      </trdgrp>
      <trd lng="fr">&eacute;l&eacute;ment inverse</trd>
      <trd lng="hu">reciprok</trd>
      <trdgrp lng="pl">
        <trd>element odwrotny</trd>,
        <trd>odwrotno&sacute;&cacute;</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; <ind>&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;</ind></trd>
    </subsnc>
    <subsnc mrk="invers.0o.per0igo">
      <dif>
        <ref cel="bild.0o.MAT">Bildo</ref> de punkto a&ubreve; figuro per
        <ref cel="invers.0igo.transformo"><tld/>igo</ref>:
        <ekz>
          la <tld/>o de rekto ne trairanta la centron estas cirklo
          &gcirc;in trairanta.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>inverser <ind>Punkt</ind></trd>,
        <trd>inverse <ind>Figur</ind></trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>inverse<klr tip="amb"> (of a point)</klr></trd>,
        <trd>inverse <ind>figure</ind></trd>
      </trdgrp>
      <trd lng="fr">inverse<klr tip="ind"> (subst., d'un point, d'une figure)</klr></trd>
      <trd lng="hu">inverz</trd>
      <trd lng="pl">obraz<klr tip="ind"> (punktu, figury)</klr> w inwersji</trd>
      <trdgrp lng="ru">
        <trd>&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_n;&c_a;&c_ja; <ind>&c_t;&c_o;&c_ch;&c_k;&c_a;</ind></trd>,
        <trd>&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_n;&c_a;&c_ja; <ind>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</ind></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="invers.0o.variaj">
      <refgrp tip="vid">
        <ref cel="par.inversa0o"><tld/>a paro</ref>,
        <ref cel="rilat.inversa0o"><tld/>a rilato</ref>,
        <ref cel="bild.inversa0o.elemento"><tld/>a bildo</ref> de elemento,
        <ref cel="bild.inversa0o.subaro"><tld/>a bildo</ref> de subaro,
        <ref cel="bild.inversa0igo"><tld/>a bildigo</ref>.
      </refgrp>
    </subsnc>
  </snc>
  <adm>
    PIV konas trian sencon, sinoniman de "reverso" (ne en REVO), 
    neologismo el la sama familio kiel "averso" kaj "roverso"...
    Valoras esplori, cxu tiu senco vere ekzistas (cxi-okaze aldonendas
    francaj tradukoj: envers, verso) kaj kiel eventuale gxin
    esprimi pli precize sen neologismo.
    <aut>MB</aut>
  </adm>
  <trd lng="sk">inverzné zobrazenie</trd>
  <trd lng="cs">inverzní zobrazení</trd>
</drv>

<drv mrk="invers.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        la akvo spegulas <tld/>e la objektojn;
      </ekz>
      <ekz>
        movi pi&scirc;ton de supre malsupren kaj <tld/>e.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="renver">renverse</ref>,
      <ref cel="turn.re0e">returne</ref>,
      <ref cel="senc.kontraux0e">kontra&ubreve;sence</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>naopak</trd>,
    <trd>obráceně</trd>
  </trdgrp>
  <trd lng="de">umgekehrt</trd>
  <trd lng="es">inversamente</trd>
  <trdgrp lng="fr">
    <trd>inversement</trd>,
    <trd>au <ind>contraire</ind></trd>,
    <trd>&agrave; l'<ind>envers</ind></trd>,
    <trd>&agrave; <ind>rebours</ind></trd>
  </trdgrp>
  <trd lng="hu">ford&iacute;tva</trd>
  <trd lng="nl">omgekeerd</trd>
  <trdgrp lng="pt">
    <trd>inversamente</trd>,
    <trd>vice-versa</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>naopak</trd>,
    <trd>naruby</trd>
  </trdgrp>
</drv>

<drv mrk="invers.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="invers.0igi.KOMUNE">
    <dif>
      Meti en <tld/>an pozicion a&ubreve; direkton:
      <ekz>
        <tld/>igi bildon, elektran fluon, la rolojn.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umkehren</trd>,
      <trd>invertieren</trd>
    </trdgrp>
    <trd lng="fr">inverser</trd>
    <trdgrp lng="hu">
      <trd>megford&iacute;t</trd>,
      <trd>kiford&iacute;t <klr tip="ind">(pl. ruh&aacute;t)</klr></trd>
    </trdgrp>
    <trd lng="nl">omkeren</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_n;&c_v;&c_je;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="invers.0igi.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Kalkuli la <ref cel="invers.0o.MAT"><tld/>on <sncref/></ref>:
      <ekz>
        <tld/>igi matricon,
      </ekz>
      <ekz>
        bildigon,
      </ekz>
      <ekz>
        figuron.
      </ekz>
    </dif>
    <trd lng="fr">inverser</trd>
    <trd lng="hu">invert&aacute;l</trd>
    <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>otočit do protisměru</trd>,
    <trd>zvrátit</trd>
  </trdgrp>
  <trd lng="es">invertir</trd>
  <trd lng="pt">inverter</trd>
  <trd lng="sk">vypočítať inverziu</trd>
</drv>

<drv mrk="invers.0igo">
  <kap><tld/>igo</kap>
  <snc mrk="invers.0igo.KOMUNE">
    <dif>
      Ago <tld/>igi <sncref ref="invers.0igi.KOMUNE"/>.
    </dif>
    <trd lng="fr">inversion</trd>
    <trdgrp lng="hu">
      <trd>megford&iacute;t&aacute;s</trd>,
      <trd>kiford&iacute;t&aacute;s <klr tip="ind">(pl. ruh&aacute;&aacute;)</klr></trd>
    </trdgrp>
    <trd lng="pl">odwracanie</trd>
    <trd lng="pt">invers&atilde;o</trd>
  </snc>
  <snc mrk="invers.0igo.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="invers.0igo.gxenerale">
      <uzo tip="klr">(fakula &jcirc;argono)</uzo>
      <fnt><bib>MatVort</bib><lok>&leftquot;de matrico&rightquot;</lok></fnt>
      <dif>
        Ago <ref cel="invers.0igi.MAT"><tld/>igi <sncref/></ref>.
      </dif>
    </subsnc>
    <subsnc mrk="invers.0igo.transformo">
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>MatTerm</bib><lok>p. 29</lok></fnt>
      <dif>
        <klr>(en dudimensia afina
        <ref cel="spac.euxklida0o">e&ubreve;klida spaco</ref>, kun centro
        <frm>O</frm> kaj potenco <frm>&lambda;<sup>2</sup></frm>)</klr>
        <ref tip="super" cel="form.trans0o.MAT">Transformo</ref>, kiu
        &jcirc;etas punkton <frm>M</frm> al tia punkto <frm>M'</frm> de
        la duonrekto <frm>OM</frm>, ke
        <frm>OM.OM' = &lambda;<sup>2</sup></frm>:
        <ekz>
          &ccirc;iu punkto de la cirklo kun centro en <frm>O</frm> kaj radiuso
          <frm>&lambda;</frm> estas sen&scirc;an&gcirc;a per la <tld/>igo;
        </ekz>
        <ekz>
          <tld/>igo estas <ref cel="involu1.0o">involucio</ref>.
        </ekz>
      </dif>
      <rim>
        Tian transformon oni anka&ubreve; nomas <ctl><tld/>igo rilate al cirklo
        kun centro en <frm>O</frm> kaj radiuso <frm>&lambda;</frm></ctl>.
        Troveblas sinonima termino <ctl>inversio</ctl> en
        <fnt><bib>MatVort</bib></fnt>, sed neniu el niaj
        aliaj fontoj &gcirc;in konfirmas.
      </rim>
      <trd lng="de">Inversion<klr tip="ind"> (am Kreis)</klr></trd>
      <trd lng="en">inversion<klr tip="ind"> (with respect to a circle)</klr></trd>
      <trd lng="fr">inversion<klr tip="ind"> (par rapport &agrave; un cercle)</klr></trd>
      <trd lng="pl">inwersja<klr tip="ind"> (wzgl&eogonek;dem okr&eogonek;gu)</klr></trd>
      <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_i;&c_ja;<klr tip="ind"> (&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_k;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
    </subsnc>
    <trdgrp lng="hu">
      <trd>invert&aacute;l&aacute;s</trd>,
      <trd>inverzi&oacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
</drv>

<drv mrk="invers.0igebla">
  <kap><tld/>igebla</kap>
  <snc mrk="invers.0igebla.KOMUNE">
    <dif>
      Tia, ke eblas &gcirc;in <tld/>igi.
    </dif>
    <trdgrp lng="hu">
      <trd>megford&iacute;that&oacute;</trd>,
      <trd>kiford&iacute;that&oacute; <klr tip="ind">(pl. ruha)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="invers.0igebla.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;185</lok></fnt>
    <dif>
      <klr>(p.p. elemento en multiplike signata
      <ref cel="grup.0o.MAT">grupo</ref> a&ubreve; en
      <ref cel="ring.0o.MAT">ringo</ref>)</klr>
      Tia, ke ekzistas &gcirc;ia
      <ref cel="invers.0o.neuxtriganto"><tld/>o</ref>:
      <ekz>
        en la ringo de entjeroj nur <frm>1</frm> kaj <frm>-1</frm>
        estas <tld/>igeblaj.
      </ekz>
    </dif>
    <ref tip="sin" cel="invers.0hava">invershava.</ref>
    <rim>
      La <tld/>igeblajn elementojn de ringo oni foje nomas
      <ctl>unuoj</ctl>, kio estas evitinda pro la ebla
      konfuzo kun la <ref cel="unu.0o.neuxtra">unuo</ref> de la ringo.
      Ial la citita fonto uzas <ctl><tld/>igebla</ctl> nur p.p. bildigoj.
      Ni ne opinias tiun limigon utila.
    </rim>
    <trd lng="en">invertible</trd>
    <trd lng="hu">invert&aacute;lhat&oacute;</trd>
    <trd lng="pl">odwracalny</trd>
    <trd lng="ru">&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="cs">reverzibilní <klr>(turbína)</klr></trd>
  <trd lng="de">umkehrbar</trd>
  <trd lng="es">invertible</trd>
  <trd lng="fr">inversible</trd>
  <trd lng="nl">omkeerbaar</trd>
  <trd lng="pt">invert&iacute;vel</trd>
  <trdgrp lng="sk">
    <trd>vratný</trd>,
    <trd>zvratný</trd>
  </trdgrp>
</drv>

<drv mrk="invers.ne0igebla">
  <kap>ne<tld/>igebla</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Ne <ref cel="invers.0igebla"><tld/>igebla</ref>.
    </dif>
  </snc>
  <trd lng="cs">neinverzibilní</trd>
  <trd lng="de">unumkehrbar</trd>
  <trd lng="en">non-inversible</trd>
  <trd lng="fr">non inversible</trd>
  <trd lng="hu">nem invert&aacute;lhat&oacute;</trd>
  <trd lng="nl">onomkeerbaar</trd>
  <trd lng="pl">nieodwracalny</trd>
  <trd lng="pt">n&atilde;o invert&iacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">neinverzibilný</trd>
</drv>

<drv mrk="invers.0hava">
  <kap><tld/>hava</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;186</lok></fnt>
  <snc>
    <ref tip="dif" cel="invers.0igebla.MAT"><tld/>igebla.</ref>
  </snc>
  <trd lng="de">umkehrbar</trd>
  <trd lng="en">inversible</trd>
  <trd lng="fr">inversible</trd>
  <trd lng="hu">invert&aacute;lhat&oacute;</trd>
  <trd lng="nl">omkeerbaar</trd>
  <trd lng="pl">odwracalny</trd>
  <trd lng="ru">&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="pt">invert&iacute;vel</trd>
</drv>

<drv mrk="invers.ne0hava">
  <kap>ne<tld/>hava</kap>
  <snc>
    <ref tip="dif" cel="invers.ne0igebla">ne<tld/>igebla.</ref>
    <dif>
      Ne <ref cel="invers.0igebla"><tld/>igebla</ref>.
    </dif>
  </snc>
  <trd lng="de">unumkehrbar</trd>
