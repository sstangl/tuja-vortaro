<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kacuon.xml,v 1.7 2015/09/22 13:10:15 revo Exp $">
<kap>
  <rad>kacuon</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="kacuon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kacuon.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref> 
      de mezvarmaj kaj varmaj maroj, &gcirc;is unu metro longa, kun sep
      la&ubreve;longaj 
      malhelaj strioj sur la ar&gcirc;entkolora ventro,
      kun bongusta karno, simila al <ref cel="tinus.0o">tinuso</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="skombr1.0oj">skombredoj</ref> (<trd
      lng="la">Katsuwonus</trd>):
      <ekz>
        mi rigardis procedon vaporkuiri <tld/>on
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;man&gcirc;o kaj imagpovo&rightquot;, p.
        41a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Echter Bonito</trd>
  <trdgrp lng="fr">
    <trd>bonite &agrave; ventre ray&eacute;</trd>,
    <trd>thon listao</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bonito</trd>,
    <trd>skipjack tuna</trd>
  </trdgrp>
  <trd lng="es">bonito<klr tip="amb"> (pescado)</klr></trd>
  <trdgrp lng="pl">
    <trd>bonito</trd>,
    <trd>tunek pasiasty</trd>,
    <trd>tu&nacute;czyk pasiasty</trd>,
    <trd>tu&nacute;czyk skoczek</trd>,
    <trd>bonito lataj&aogonek;cy</trd>,
    <trd>bonito paskowany</trd>
  </trdgrp>
</drv>
</art>

