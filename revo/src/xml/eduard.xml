<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eduard.xml,v 1.10 2015/03/30 22:10:18 revo Exp $">
<kap>
  <rad>Eduard</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="eduard.0o">
  <kap><tld/>o</kap>
  <snc mrk="eduard.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        sinjoro <tld/>o havis simpation al mi
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_Je;&c_d;&c_u;&c_a;&c_r;&c_d;</trd>
  <trd lng="ca">Eduard</trd>
  <trdgrp lng="de">
    <trd>Eduard</trd>,
    <trd>Edward</trd>
  </trdgrp>
  <trd lng="es">Eduardo</trd>
  <trd lng="fr">&Eacute;douard</trd>
   <trdgrp lng="it">
    <trd>Eduardo</trd>,
    <trd>Edoardo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Edmund</trd>,
    <trd>Edek<klr> (zdrobnienie)</klr></trd>,
    <trd>Edziu<klr> (zdrobnienie)</klr></trd>,
  </trdgrp>
  <trd lng="pt">Eduardo</trd>
  <trdgrp lng="ru">
    <trd>&c_E;&c_d;&c_u;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_E;&c_d;&c_v;&c_a;&c_r;&c_d;</trd>
  </trdgrp>
</drv>
</art>

