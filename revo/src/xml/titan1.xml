<drv mrk="titan1.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ti</mlg>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
