  <trd lng="br">titan</trd>
  <trd lng="ca">titani</trd>
  <trdgrp lng="cs">
    <trd>Ti</trd>,
    <trd>titan</trd>
  </trdgrp>
  <trd lng="de">Titan</trd>
  <trd lng="en">titanium</trd>
  <trd lng="es">titanio</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_y;&a_t;&a_A;&a_n;&f_y;&a_m;</trd>,
    <trd>&a_t;&f_y;&a_t;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">titane</trd>
  <trd lng="hu">tit&aacute;n <klr tip="ind">(elem)</klr></trd>
  <trd lng="id">titanium</trd>
  <trd lng="it">titanio</trd>
  <trd lng="la">Titanium</trd>
  <trd lng="pl">tytan<klr> (pierwiastek)</klr></trd>
  <trd lng="pt">tit&acirc;nio</trd>
  <trdgrp lng="sk">
    <trd>Ti</trd>,
    <trd>tit√°n</trd>
  </trdgrp>
  <trd lng="sv">titan</trd>
</drv>
