<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sentim1.xml,v 1.15 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sentimental</rad>/a
</kap>
<drv mrk="sentim1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta a&ubreve; afektanta romanecan sentemon:
      <ekz>
        li <ind><tld/>e</ind> levis la okulojn kaj mieltone flustris al
        Margarita<fnt><bib>MkM</bib>, <lok>&ccirc;apitro 24a, p.
        xa</lok></fnt><klr>...</klr>
          <trd lng="ca">sentimentalment</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_t;&c_y;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ch;&c_u;&c_l;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_ch;&c_u;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">sentimental</trd>
  <trdgrp lng="cs">
    <trd>citový</trd>,
    <trd>přecitlivělý</trd>,
    <trd>sentimentální</trd>
  </trdgrp>
  <trd lng="de">sentimental</trd>
  <trd lng="en">sentimental</trd>
  <trdgrp lng="fr">
    <trd>romanesque<klr> (sentimental)</klr></trd>,
    <trd>sentimental</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rzelmes</trd>,
    <trd>&eacute;rzelg&odblac;s</trd>,
    <trd>szentiment&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">sentimentale</trd>
  <trd lng="nl">sentimenteel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_n;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>citový</trd>,
    <trd>precitlivený</trd>,
    <trd>sentimentálny</trd>
  </trdgrp>
</drv>

<drv mrk="sentim1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Forta <ref tip="super" cel="sent.0emo">sentemo</ref>, sinlaso al
      emocioj, aparte al amaj kaj dol&ccirc;aj imagoj.
    </dif>
  </snc>
  <trd lng="ca">sentimentalisme</trd>
  <trd lng="en">sentimentality</trd>
  <trdgrp lng="fr">
    <trd>sentimentalisme</trd>,
    <trd>sentimentalit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rzelmess&eacute;g</trd>,
    <trd>&eacute;rzelg&odblac;ss&eacute;g</trd>,
    <trd>szentimentalizmus</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>sentimentalismo</trd>,
    <trd>sentimentalita</trd>
  </trdgrp>
</drv>
</art>
