    <trd>aguafiestas</trd>,
    <trd>inoportuno</trd>,
    <trd>pe&ntilde;azo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;neur</trd>,
    <trd>casse-pieds</trd>,
    <trd>importun<klr tip="ind"> (subst.)</klr></trd>,
    <trd>f&acirc;cheux<klr tip="ind"> (subst.)</klr></trd>,
    <trd>raseur<klr tip="ind"> (g&ecirc;neur)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>seccatore</trd>,
    <trd>guastafeste</trd>,
    <trd>importuno<klr tip="ind"> (sost.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_u;&c_ch;&c_l;&c_i;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_j;&c_l;&c_i;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
