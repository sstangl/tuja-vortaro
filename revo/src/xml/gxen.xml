<drv mrk="gxen.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="gxen.sen0a.negxenata">
    <dif>
      Ne<tld/>ata, ne<tld/>ita:
      <ekz>
        &scirc;i penis aperigi sen<tld/>an rideton<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="liber.0a">libera</ref>.
    </refgrp>
  </snc>
  <snc mrk="gxen.sen0a.negxenanta">
    <dif>
      Ne <tld/>anta sin kontra&ubreve; aliaj, senceremonia, intimeca:
      <ekz>
        sen<tld/>a homo, junulo;
      </ekz>
      <ekz>
        sen<tld/>a invito, kunveno, man&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>
    </refgrp>
  </snc>
  <snc mrk="gxen.sen0a.senhonta">
    <dif>
      Ne sufi&ccirc;e <tld/>anta sin kontra&ubreve; aliaj; tro senceremonia;
      malkonvena; senhonta; sen<tld/>a&ccirc;a; tro sen<tld/>a:
      <ekz>
        sen<tld/>a konduto, vesto;
      </ekz>
      <ekz>
        sen<tld/>e paroli kontra&ubreve; iu.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arogan.0a">aroganta</ref>,
      <ref cel="impert.0a">impertinenta</ref>,
      <ref cel="respek">malrespekta</ref>
    </refgrp>
   <trdgrp lng="it">
    <trd>scanzonato</trd>,
    <trd>sbracato</trd>
  </trdgrp>
  </snc>
  <adm>
    PIV1 transdonis la lastan sencon al "sen~acxa" (uzata
    en la nuna difino) = "trosen~a". <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_m;&c_u;&c_sh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_o;&c_l;&c_je;&c_n;&c_y;
</trd>,
    <trd>&c_b;&c_je;&c_s;&c_c;&c_y;&c_r;&c_y;&c_m;&c_o;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungeniert</trd>,
    <trd>unvermittelt</trd>,
    <trd>unverbl&uuml;mt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>soberbio</trd>,
    <trd>altivo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sans <ind>g&ecirc;ne</ind></trd>,
    <trd>cavalier</trd>,
    <trd>d&eacute;gag&eacute;<klr tip="ind"> (qui a de l'aisance et du
    naturel)</klr></trd>,
    <trd>d&eacute;sinvolte</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>disinvolto</trd>,
    <trd>franco</trd>,
    <trd>indisturbato</trd>,
    <trd>spigliato</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongehinderd</trd>,
    <trd>onverbloemd</trd>
  </trdgrp>
  <trd lng="pt">desembara&ccedil;ado</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_n;&c_u;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="gxen.sin0a">
  <kap>sin<tld/>a</kap>
  <snc mrk="gxen.sin0a.sindonema">
    <dif>
      Sin <tld/>anta por aliaj:
      <ekz>
        li estas afable sin<tld/>a por &ccirc;iuj kaj &ccirc;iam preta helpi.
      </ekz>
    </dif>
    <adm>
      Tiu senco malaperis el PIV1. <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="komple4.0ema">komplezema</ref>,
      <ref cel="don.sin0a">sindona</ref>
    </refgrp>
