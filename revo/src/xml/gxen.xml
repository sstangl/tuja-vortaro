  <trdgrp lng="pt">
    <trd>inc&oacute;modo<klr tip="ind"> (adj.)</klr></trd>,
    <trd>inc&ocirc;modo<klr tip="ind"> (bras. adj.)</klr></trd>,
    <trd>inconveniente<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_s;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;  <klr>(&c_o;&c_b; &c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;&c_h;)</klr></trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_d;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="gxen.0ateco">
  <kap><tld/>ateco, <var><kap><tld/>eco<fnt>Z</fnt></kap></var></kap>
  <snc>
    <dif>
      Sento de tiu, kiu sin <tld/>as:
      <ekz>
        lia <tld/>ateco faris lin iom ridinda, kaj mallerta;
      </ekz>
      <ekz>
        <tld/>eca mieno<fnt>Z</fnt>, rideto.
      </ekz>
    </dif>
  </snc>
  <adm>
    PIV1 splitis ~ateco/~eco, el kio rezultas tri malsamaj
    sencoj. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_jo;&c_m;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Geniertheit</trd>
  <trdgrp lng="es">
    <trd>malestar</trd>,
    <trd>indisposici&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;ne<klr tip="ind"> (malaise)</klr></trd>,
    <trd>malaise<klr tip="ind"> (g&ecirc;ne)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fastidio</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_s;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

