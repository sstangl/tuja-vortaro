<vortaro>
<art mrk="$Id: gxen.xml,v 1.27 2015/07/15 15:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;en</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="gxen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="gxen.0i.maloportuni">
    <dif>
      Malfaciligi a&ubreve; malhelpi ies movojn; maloportuni al iu,
      malkomfortigi iun:
      <ekz>
        &ccirc;u la paka&jcirc;o <tld/>as vin?;
      </ekz>
      <ekz>
        ne <tld/>u la iradon, al la irado;
      </ekz>
      <ekz>
        tiu sen&ccirc;esa bruo <tld/>as min.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0i">bari</ref>,
      <ref cel="embara.0i">embarasi</ref>,
      <ref cel="obstru.0i">obstrukci</ref>
    </refgrp>
  </snc>
  <snc mrk="gxen.0i.morale">
    <dif>
      Malfaciligi ies agojn a&ubreve; decidojn pro moralaj malhelpoj:
      <ekz>
        via &ccirc;eesto <tld/>is lin;
      </ekz>
      <ekz>
        li havis mienon tre <tld/>itan
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        tre <tld/>as min, ke mi devas fari tiun viziton, tiun klopodon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hezit.0i">heziti</ref>,
      <ref cel="konfuz.0i">konfuzi</ref>,
      <ref cel="ted.0i">tedi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_ja;&c_zh;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>st&ouml;ren</trd>,
    <trd>hindern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bother</trd>,
    <trd>trouble</trd>,
    <trd>worry</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>molestar</trd>,
    <trd>estorbar</trd>,
    <trd>incomodar</trd>,
    <trd>importunar</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hinderen</trd>,
    <trd>storen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;ner</trd>,
    <trd>d&eacute;ranger<klr tip="ind"> (g&ecirc;ner)</klr></trd>,
    <trd>embarrasser</trd>,
    <trd>emb&ecirc;ter</trd>,
    <trd>ennuyer<klr tip="ind"> (emb&ecirc;ter)</klr></trd>,
    <trd>importuner</trd>,
    <trd>mettre mal &agrave; l'<ind>aise</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>disturbare</trd>,
    <trd>importunare</trd>,
    <trd>imbarazzare<klr> (perturbare)</klr></trd>,
    <trd>molestare</trd>,
    <trd>infastidire</trd>,
    <trd>scocciare</trd>,
    <trd>ostacolare</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atrapalhar</trd>,
    <trd>incomodar</trd>,
    <trd>importunar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gxen.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <dif>
      Heziti, ne kura&gcirc;i fari ion pro skrupulo, honto, timo ktp:
      <ekz>
        ne <tld/>u vin pro mi;
      </ekz>
      <ekz>
        malfeli&ccirc;o sin ne <tld/>as, faru geston, &gcirc;i
        tuj venas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li forte sin <tld/>is saluti tiun friponon<fnt>Z</fnt>
        <klr>(li malvolonte, sindevige salutis)</klr>;
      </ekz>
      <ekz>
        eble ili sin <tld/>as fari al mi malagrabla&jcirc;on<fnt>Z</fnt>
        <klr>(sin <tld/>as, por ne fari; ili malvolonte faras; ili
        hezitas
        fari)</klr>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> ofte sin <tld/>as eldiri <klr>(sin <tld/>as por
        ne
        eldiri; malvolonte, timeme, hezite eldiras)</klr> libere sian
        opinion
        por ne kontra&ubreve;batali publike tiun, kiun la esperantistoj nomas
        sia majstro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo anta&ubreve;
        la Oka Kongreso Esperantista en Krak&oacute;w, 1912</vrk>)</fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;ar <em>sin <tld/>i</em> kun infinitivo estas dubasenca (1
      sin <tld/>i por fari; 2 sin <tld/>i por ne fari), estas
      rekomendinde uzi &ccirc;iam post
      &gcirc;i <em>por</em> a&ubreve; <em>por ne</em>, la&ubreve; la senco.
    </rim>
    <adm>
      &Scirc;ajnas al mi, ke "~igxi" estas sinonimo. <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_o;&c_m;&c_je;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_n;&c_je;&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>go to the <ind>trouble</ind> of</trd>,
    <trd>go to the <ind>bother</ind> of</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sin <ind>molestar</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>g&ecirc;ner</ind></trd>,
    <trd>se <ind>d&eacute;ranger</ind></trd>,
    <trd>ne pas <ind>oser</ind></trd>,
    <trd>&ecirc;tre <ind>g&ecirc;n&eacute;</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="gxen.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc>
    <dif>
      Maloportuna&jcirc;o, embaraso:
      <ekz>
        la diverseco de la lingvoj estas granda <tld/>o por la
        internaciaj
        rilatoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_r;&c_u;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>St&ouml;rung</trd>,
    <trd>Hindernis</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trouble</trd>,
    <trd>inconvenience</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>molestia</trd>,
    <trd>incomodidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;ne<klr tip="ind"> (incommodit&eacute;)</klr></trd>,
    <trd>incommodit&eacute;</trd>,
    <trd>inconv&eacute;nient</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fastidio<klr tip="ind"> (inconveniente)</klr></trd>, 
    <trd>inconveniente</trd>, 
    <trd>problema<klr tip="ind"> (inconveniente)</klr></trd>, 
    <trd>ostacolo<klr tip="ind"> (problema)</klr></trd>
  </trdgrp>
  <trd lng="nl">hinder</trd>
  <trdgrp lng="pt">
    <trd>inc&oacute;modo<klr tip="ind"> (subst.)</klr></trd>,
    <trd>inc&ocirc;modo<klr tip="ind"> (bras. subst.)</klr></trd>,
    <trd>inconveniente<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_s;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_d;&c_o;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_h;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_o;&c_v;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gxen.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Maloportuna, malhelpanta, embarasanta, malagrabla, &ccirc;agrena:
      <ekz>
        jen tre <tld/>a afero, okaza&jcirc;o, prokrasto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_ib;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_y;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">st&ouml;rend</trd>
  <trdgrp lng="en">
    <trd>troubling</trd>,
    <trd>inconvenient</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inoportuno</trd>,
    <trd>molesto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&ecirc;nant</trd>,
    <trd>embarrassant</trd>,
    <trd>emb&ecirc;tant</trd>,
    <trd>ennuyeux</trd>,
    <trd>f&acirc;cheux<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>problematico<klr tip="ind"> (fastidioso)</klr></trd>,
    <trd>fastidioso</trd>, 
    <trd>molesto</trd>, 
    <trd>importuno</trd>, 
    <trd>scomodo<klr tip="ind"> (problematico)</klr></trd>
  </trdgrp>
