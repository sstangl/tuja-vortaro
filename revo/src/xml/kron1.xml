<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kron1.xml,v 1.11 2012/07/31 09:10:28 revo Exp $">
<kap>
  <rad>Kron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kron1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kron1.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="titan.0o">titano</ref>,
      <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de tempo, unue la re&gcirc;o de la &ccirc;ielo, la patro de
      <ref tip="vid" cel="zeuxs.0o">Ze&ubreve;so</ref>.
    </dif>
    <ref tip="vid" cel="saturn.0o.mita">Saturno</ref>
  </snc>
  <trd lng="be">&c_H;&c_r;&c_o;&c_n;&c_a;&c_s;</trd>
  <trd lng="ca">Cronos</trd>
  <trd lng="de">Kronos</trd>
  <trd lng="el">&Kappa;&rho;&omikron_ton;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Cronos</trd>,
    <trd>Cronus</trd>,
    <trd>Kronos</trd>
  </trdgrp>
  <trd lng="fr">Cronos</trd>
  <trd lng="nl">Cronos</trd>
  <trd lng="pl">Kronos</trd>
  <trd lng="pt">Cronos</trd>
  <trd lng="hu">Kronosz</trd>
  <trd lng="ru">&c_K;&c_r;&c_o;&c_n;&c_o;&c_s;</trd>
</drv>
</art>
