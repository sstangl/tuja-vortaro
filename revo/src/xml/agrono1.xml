<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: agrono1.xml,v 1.20 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>agronomi</rad>/o
</kap>
<drv mrk="agrono1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Scienco pri agrokulturo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="cs">agronomie <klr>(věd.)</klr></trd>
  <trdgrp lng="de">
     <trd>Agronomie</trd>,
     <trd>Landwirtschaft</trd>
  </trdgrp>
  <trd lng="en">agronomy</trd>
  <trd lng="es">agronom&iacute;a</trd>
  <trd lng="fr">agronomie</trd>
  <trdgrp lng="hu">
    <trd>mez&odblac;gazdas&aacute;gtan</trd>,
    <trd>agron&oacute;mia</trd></trdgrp>
  <trd lng="it">agronomia</trd>
  <trd lng="nl">agronomie</trd>
  <trd lng="pl">agronomia</trd>
  <trd lng="pt">agronomia</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>agron&oacute;mia</trd>,
    <trd>agronómia</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>tar&inodot;m bilim</trd>,
    <trd>agronomi</trd>
  </trdgrp>
</drv>

<drv mrk="agrono1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <ref tip="vid" cel="agrono1.0o"><tld/>o</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="agrono.0o">agronomo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
  <trd lng="cs">agronom</trd>
  <trdgrp lng="de">
     <trd>Agronom</trd>,
     <trd>studierter <ind>Landwirt</ind></trd>
  </trdgrp>
  <trd lng="en">agronomist</trd>
  <trd lng="fr">agronome</trd>
  <trdgrp lng="hu">
    <trd>mez&odblac;gazd&aacute;sz</trd>,
    <trd>agron&oacute;mus</trd></trdgrp>
  <trd lng="it">agronomo</trd>
  <trd lng="nl">agronoom</trd>
  <trd lng="pl">agronom</trd>
  <trd lng="pt">agr&ocirc;nomo</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>agron&oacute;m</trd>,
    <trd>agronóm</trd>
  </trdgrp>
  <trd lng="sv">agronom</trd>
</drv>
</art>
