<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fabac.xml,v 1.19 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>fabac</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fabac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="fabac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Fabaceae</trd>),
      kun nesimetriaj papilisimilaj floroj kaj gu&scirc;oj.
    </dif>
    <ref tip="sin" cel="legume1.0oj">legumenacoj</ref>
    <refgrp tip="sub">
      <ref cel="fab.0o">fabo</ref>,
      <ref cel="fazeol.0o">fazeolo</ref>,
      <ref cel="piz.0o">pizo</ref>,
      <ref cel="vici.0o">vicio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_o;&c_v;&c_y;&c_ja;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">fab&agrave;cies</trd>
  <trd lng="cs">motýlokvěté <klr>(čeleď)</klr></trd>
  <trd lng="de">Schmetterlingsbl&uuml;tengew&auml;chse</trd>
  <trd lng="es">leguminosas</trd>
  <trd lng="fr">fabac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;velyesek<klr tip="ind"> (csal&aacute;d)</klr></trd>,
    <trd>pillang&oacute;svir&aacute;g&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fabacee</trd>,
    <trd>leguminose</trd>
  </trdgrp>
  <trd lng="nl">vlinderbloemigen</trd>
  <trd lng="pl">motylkowate</trd>
  <trd lng="pt">fab&aacute;ceas</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_o;&c_v;&c_y;&c_je;</trd>
  <trd lng="sk">motýľokveté <klr>(čeľaď)</klr></trd>
  <trd lng="sv">&auml;rtv&auml;xter</trd>
</drv>
</art>
