<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxakon.xml,v 1.8 2013/09/08 15:10:25 revo Exp $">
<kap>
  <rad>&ccirc;akon</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>

<drv mrk="cxakon.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxakon.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropa <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref>
      hispan-devena, tri-takta, jam nur muzike uzata ekde la 17a
      jarcento:
      <ekz>
        la temo por <tld/>o kutime da&ubreve;ras nur kvar mezurojn
        <fnt>
        <bib>Viki</bib>,
        <lok>artikolo &leftquot;Goldberga varia&jcirc;aro&rightquot;, 2006-03-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cxakon.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o tri-takta, malrapida, konsistanta el
      temo kaj varia&jcirc;oj akompanataj de ripeta baso,
      foje uzata kiel parto de <ref tip="malprt"
      cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_k;&c_o;&c_n;&c_a;</trd>
  <trd lng="de">Chaconne</trd>
  <trd lng="es">chacona</trd>
  <trd lng="fr">chaconne</trd>
  <trd lng="hu">chaconne</trd>
  <trd lng="it">ciaccona</trd>
  <trd lng="pt">chacona</trd>
  <trd lng="ru">&c_ch;&c_a;&c_k;&c_o;&c_n;&c_a;</trd>
</drv>
</art>
