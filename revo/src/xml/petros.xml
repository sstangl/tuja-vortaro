<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: petros.xml,v 1.22 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>petrosel</rad>/o
</kap>

<drv mrk="petros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Petroselinum</trd>),
      uzata kiel <ref tip="lst" cel="spic.0o.KUI"
      lst="voko:spicoj">spic</ref>herbo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_t;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_d;&c_a;&c_n;&c_o;&c_z;</trd>
  <trd lng="br">perisilh</trd>
  <trd lng="cs">petržel</trd>
  <trd lng="de">Petersilie</trd>
  <trd lng="en">parsley</trd>
  <trd lng="es">perejil</trd>
  <trd lng="fa">&a_j;&a_ayn;&a_f;&a_r;&f_y;</trd>
  <trd lng="fr">persil</trd>
  <trd lng="hu">petrezselyem</trd>
  <trd lng="id">peterseli</trd>
  <trd lng="nl">peterselie</trd>
  <trd lng="pl">pietruszka</trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">petržlen</trd>
  <trd lng="tr">maydanoz</trd>
</drv>
</art>
