<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: valent.xml,v 1.23 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>valent</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="valent.0o">
  <kap><tld/>o</kap>
  <snc mrk="valent.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Valoro, indikanta, kiom da hidrogenatomoj povas ligi&gcirc;i kun
      atomo.
    </dif>
    <refgrp tip="sub">
      <ref cel="divale.0o">divalento</ref>,
      <ref cel="kvater.0a.KEM">kvaternara</ref>
    </refgrp>
    <trd
    lng="be">&c_v;&c_a;&c_l;&c_je;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="de">Valenz</trd>
    <trd lng="es">valencia</trd>
    <trdgrp lng="fa">
      <trd>&a_w;&a_A;&a_l;&a_A;&a_n;&a_s;</trd>,
      <trd>&a_Z;&a_r;&a_f;&f_y;&a_t;<klr tip="ind"> (&a_sh;&f_y;&a_m;&f_y;)</klr></trd>
    </trdgrp>
    <trd lng="fr">valence<klr tip="ind"> (chimie)</klr></trd>
    <trd lng="hu">vegy&eacute;rt&eacute;k</trd>
    <trd lng="it">valenza<klr tip="ind"> (ch.)</klr></trd>
    <trdgrp lng="nl">
      <trd>valentiegetal</trd>,
      <trd>waardigheid<klr tip="ind"> (valentie)</klr></trd>
    </trdgrp>
    <trd lng="pl">warto&sacute;ciowo&sacute;&cacute;</trd>
    <trd lng="pt">val&ecirc;ncia</trd>
    <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="valent.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Nombro de komplementoj, kiun vorto &mdash; precipe verbo &mdash;
      normale havas:
      <ekz>
        &leftquot;pluvas&rightquot; estas nul-<tld/>a: &gcirc;i regas
        nek subjekton, nek ion
        alian; 
      </ekz>
      <ekz>
        tio estu instigo al estontaj gramatikistoj <klr>...</klr>
        prilabori teorion pri la verbaj <tld/>oj kaj la verba regado
        <fnt>
        <aut>M. Duc Goninaz</aut>,
        <bib>PIV2</bib>,
        <lok>Anta&ubreve;parolo, p. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">valence<klr tip="amb"> (linguistique)</klr></trd>
    <trd lng="it">valenza<klr tip="ind"> (ling.)</klr></trd>
    <trd lng="hu">valencia<klr tip="amb">
    (ig&eacute;&eacute;)</klr></trd>
    <trd lng="nl">valentie<klr tip="ind"> (taalk.)</klr></trd>
    <trd lng="pt">cebolinha</trd>
  </snc>
  <trd lng="ca">val&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>mocenstv√≠</trd>,
    <trd>valence</trd>,
    <trd>vazba</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>valence</trd>,
    <trd>valency</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mocenstvo</trd>,
    <trd>valencia</trd>
  </trdgrp>
</drv>

<drv mrk="valent.0a">
  <kap><tld/>a</kap>
  <snc mrk="valent.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Rilata al la <ref tip="vid" cel="valent.0o.KEM"><tld/>o</ref>.
    </dif>
    <trd lng="ca">valent</trd>
    <trd lng="it">valente<klr tip="ind"> (ch.)</klr></trd>
    <trd lng="nl">betr. de <ind>valentie</ind></trd>
    <trd lng="pl">walencyjny</trd>
  </snc>
</drv>

</art>
