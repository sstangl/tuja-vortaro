<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fork.xml,v 1.36 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fork</rad>/o
</kap>

<drv mrk="fork.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fork.0o.mangxilo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="mangx.0ilo">Man&gcirc;ilo</ref> konsistanta
      el tenilo kaj plateto provizita per pluraj pikiloj, servanta por
      piki ion kaj levi &gcirc;in al la bu&scirc;o:
      <ekz>
        Oni metis anta&ubreve; mi man&gcirc;ilaron, kiu konsistis el
        telero, <ref tip="vid" cel="kuler.0o">kulero</ref>,
        <ref tip="super" cel="trancx.0ilo">tran&ccirc;ilo</ref>,
        <tld/>o, glaseto por brando, glaso por vino kaj telertuketo
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        la ansero elsaltis el la plado kaj kun <tld/>o kaj tran&ccirc;ilo
        en sia dorso balanci&gcirc;ante ekiris sur la planko
        <fnt><bib>Fab2</bib>, <lok>knabineto kun alumetoj</lok></fnt>;
      </ekz>
      <ekz>
        se iu bu&ccirc;is oferon tiam venadis knabo de la pastro
        dum la kuirado de la viando havante en sia mano tridentan
        <tld/>on
        kaj li enpu&scirc;adis &gcirc;in en la kaldronon
        a&ubreve; en la poton kaj &ccirc;ion, kion trafis
        la <tld/>o prenadis la pastro
        <fnt><bib>MT</bib>, <lok>&Sa1; 2:13</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fruktodona valo kiu havis formon de <tld/>o, kies dentoj
        estis ka&scirc;itaj inter la &scirc;tonoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">villa</trd>
    <trd lng="id">garpu</trd>
    <trd lng="pl">widelec</trd>
    <trd lng="ru">&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="fork.0o.objekto">
    <dif>
      Samforma objekto:
      <ekz>
        kaj ili <klr>(<ref tip="malprt"
        cel="forfik.0o">forfikuloj</ref>)</klr>
        uzis la <tld/>on, kiun ili havis malanta&ubreve;e &ccirc;e la vosto
        por pin&ccirc;i la skarabon je lia barbo
        <fnt><bib>Fab4</bib>, <lok>sterkskarabo</lok></fnt>;
      </ekz>
      <ekz>
        <ind>son<tld/>eto</ind>
        <klr>(<ref tip="dif" cel="diapaz.0o">diapazono</ref>)</klr>.
        <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_t;&c_o;&c_n;</trd>
        <trd lng="de">Stimmgabel</trd>
        <trd lng="en">tuning fork</trd>
        <trd lng="fa">&a_d;&f_y;&a_A;&f_p;&a_A;&a_z;&a_w;&a_n;</trd>
        <trd lng="fr">diapason</trd>
        <trd lng="hu">hangvilla</trd>
        <trd lng="id"><ind>garpu</ind> tala</trd>
        <trd lng="nl">stemvork</trd>
        <trd lng="pt">diapas&atilde;o</trd>
        <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_t;&c_o;&c_n;</trd>
        <trd lng="sv">st&auml;mgaffel</trd>
        <trd lng="tr">diyapazon</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="hu">villa</trd>
    <trd lng="nl">vork</trd>
    <trd lng="pl">widelec</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="sv">gaffel</trd>
  </snc>
  <snc mrk="fork.0o.disbrancxigxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disbran&ccirc;i&gcirc;o:
      <ekz>
        <tld/>o de arbo, bran&ccirc;o;
      </ekz>
      <ekz>
        <ind>voj<tld/>o</ind> <klr>(kie vojo pluiras al du
        direktoj)</klr>.
        <trd lng="be">&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
        <trd lng="de">Weggabelung</trd>
        <trd lng="fr">bifurcation</trd>
        <trdgrp lng="id">
          <trd>bifurkasi</trd>,
          <trd>[<ind>cabang</ind>] percabangan dua</trd>
        </trdgrp>
        <trd lng="hu">&uacute;tel&aacute;gaz&aacute;s</trd>
        <trd lng="nl">splitsing</trd>
      </ekz>
      <ekz>
        <tld/>i&gcirc;o de komputila programo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brak.0o">brako</ref>,
      <ref cel="brancx.0o">bran&ccirc;o</ref>,
      <ref cel="kruc.0o">kruc.0o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_ib;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">bifurcaci&oacute;n</trd>
    <trd lng="fa">&a_A;&a_n;&a_sh;&a_ayn;&a_A;&a_b;</trd>
    <trd lng="hu">el&aacute;gaz&aacute;s</trd>
    <trd lng="pl">ga&lstroke;&aogonek;&zacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_l;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>vidle</trd>,
    <trd>vidli&ccaron;ka</trd>,
    <trd>vidlice</trd>
  </trdgrp>
  <trd lng="de">Gabel</trd>
  <trd lng="en">fork</trd>
  <trd lng="es">tenedor</trd>
  <trd lng="fa">&f_ch;&a_n;&f_g;&a_A;&a_l;</trd>
  <trd lng="fr">fourchette</trd>
  <trd lng="pt">garfo</trd>
  <trd lng="sk">predná vidlica</trd>
  <trd lng="tr">&ccedil;atal</trd>
</drv>

<drv mrk="fork.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="fork.0eto.KUI">
    <dif>
      Malgranda <tld/>o, uzata por man&gcirc;i kukon kaj similajn:
      <ekz>
        sep malgrandaj teleroj, &ccirc;iu telereto kun sia <tld/>eto
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
  <trd lng="cs">jídelní vidlička</trd>
  <trd lng="de">G&auml;belchen</trd>
  <trd lng="hu">kisvilla</trd>
  <trd lng="id"><ind>garpu</ind> kecil</trd>
  <trd lng="nl">vorkje</trd>
  <trd lng="pl">widelczyk</trd>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">vidlička</trd>
  <trd lng="sv">gaffel</trd>
</drv>

<drv mrk="fork.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">AGR</uzo>
  <snc mrk="fork.0ego.AGR">
    <dif>
      Granda <tld/>o, kun fortikaj dentoj, uzata por levi fojnon,
      garbon,
      herbofaskon kaj ceterajn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_h;&c_o;&c_r; <klr>(&c_d;&c_l;&c_ja;
    &c_n;&c_a;&c_v;&c_o;&c_z;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">vidle</trd>
  <trd lng="de">Forke</trd>
  <trdgrp lng="en">
    <trd>pitch fork</trd>,
    <trd>hay fork</trd>
  </trdgrp>
  <trd lng="es">trinche</trd>
  <trd lng="fa">&f_ch;&a_n;&f_g;&f_k;</trd>
  <trd lng="fr">fourche</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;navilla</trd>,
    <trd>vasvilla</trd>
  </trdgrp>
  <trd lng="id">garu</trd>
  <trd lng="nl">riek</trd>
  <trd lng="pl">wid&lstroke;y</trd>
  <trdgrp lng="pt">
   <trd>forcado</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_y;</trd>
  <trd lng="sk">vidly</trd>
  <trdgrp lng="sv">
    <trd>h&ouml;gaffel</trd>,
    <trd>grep</trd>
  </trdgrp>
</drv>

<drv mrk="fork.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Disbran&ccirc;i&gcirc;i kiel <tld/>o:
      <ekz>
        la kanalo dis<tld/>i&gcirc;is la&ubreve; du direktoj.
      </ekz>
    </dif>
    <trd
    lng="be">&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="de"><klr>sich </klr>gabeln</trd>
    <trdgrp lng="en">
      <trd>bifurcate</trd>,
      <trd>branch (off)</trd>,
      <trd>split (off)</trd>
    </trdgrp>
    <trd lng="es">bifurcarse</trd>
    <trdgrp lng="fa">
      <trd>&a_d;&a_w;&a_sh;&a_A;&a_kh;&a_h; &a_sh;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_n;&a_sh;&a_ayn;&a_b; &a_sh;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_n;&a_sh;&a_ayn;&a_A;&a_b; &f_y;&a_A;&a_f;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bifurquer</trd>,
      <trd>fourcher<klr tip="ind"> (intr.)</klr></trd>
    </trdgrp>
    <trd lng="hu">el&aacute;gazik</trd>
    <trd lng="nl">afsplitsen</trd>
    <trdgrp lng="pl">
      <trd>rozwidla&cacute; si&eogonek;</trd>,
      <trd>rozwidli&cacute; si&eogonek;</trd>,
      <trd>rozga&lstroke;&eogonek;zia&cacute; si&eogonek;</trd>,
      <trd>rozga&lstroke;&eogonek;zi&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="fork.voj0o">
  <kap>voj<tld/>o, <var><kap>vojo<tld/>o</kap></var></kap>
  <snc>
    <ref tip="dif" cel="voj.dis0igxo.TRA">disvoji&gcirc;o</ref>
    <ekz>
      alproksimi&gcirc;is al tiu voj<tld/>o, de la alia flanko, kvar
      voja&gcirc;antoj,
      rajdantaj sur kameloj
      <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 4a, Ijob akceptas
      fremdlandajn
      vizitantojn</lok></fnt>;
    </ekz>
    <ekz>
      la viro alproksimi&gcirc;is tre malrapide al la vojo<tld/>o
      <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 21a, Zalmuna batalas
      kontra&ubreve; la
      &ccirc;efpastroj</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>rozcestí</trd>,
    <trd>trojcestí</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gabelung</trd>,
    <trd>Verzweigung</trd>,
    <trd>Abzweigung</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_w;&a_r;&a_A;&a_h;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>bifurcation</trd>,
    <trd>embranchement<klr> (de chemin)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&pe;&resh;&shin;&tav; &dalet;&resh;&kaf;&yod;&fmem;</trd>,
    <trd>&mem;&samekh;&ayin;&fpe;</trd>
  </trdgrp>
  <trd lng="hu">&uacute;tel&aacute;gaz&aacute;s</trd>
  <trdgrp lng="id">
    <trd>bifurkasi</trd>,
    <trd>[<ind>cabang</ind>] percabangan dua</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozga&lstroke;&eogonek;zienie</trd>,
    <trd>rozwidlenie</trd>,
    <trd>odga&lstroke;&eogonek;zienie</trd>,
    <trd>rozstaj</trd>
  </trdgrp>
  <trd lng="pt">bifurca&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">rázcestie</trd>
</drv>
</art>
