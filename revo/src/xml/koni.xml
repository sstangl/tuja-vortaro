<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koni.xml,v 1.12 2013/08/26 08:10:43 revo Exp $">
<kap>
  <rad>koni</rad>/o
</kap>
<drv mrk="koni.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Conium</trd>), tre venena vegeta&jcirc;o kun
      blueta, ofte ru&gcirc;e makulita tigo kaj museca malbonodoro,
      simila al <ref tip="vid" cel="cikut.0o">cikuto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Schierling</trd>
  <trd lng="en">hemlock</trd>
  <trd lng="nl">scheerling</trd>
  <trdgrp lng="pt">
    <trd>cicuta-da-europa</trd>,
    <trd>funcho-selvagem</trd>,    
    <trd>ansarinha-malhada</trd>,    
    <trd>cegude</trd>    
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_i;&c_g;&c_o;&c_l;&c_o;&c_v;</trd>
</drv>
</art>
