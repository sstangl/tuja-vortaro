<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brutal.xml,v 1.6 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>brutal</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="brutal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="brutal.0a.KOMUNE">
    <ref tip="dif" cel="mild.mal0a.KOMUNE">malmilda</ref>
    <ekz>
      post la film' mi provis kisi &scirc;in
      sed &scirc;i batadis min <tld/>e
      <fnt>
        <aut>La Mondanoj</aut>,
        <vrk><url
        ref="http://www.kafejo.com/muziko/notoj/MuskolaBelulino.html">
        Muskola belulino</url></vrk>,
        <lok>1995</lok>
      </fnt>;
    </ekz>
    <ekz>
      ni multe a&ubreve;das pri <tld/>aj viroj en tuta mondo, pri
      malmoraleco de edzoj
      <fnt>
        <aut>S. Vivekananda, trad. R. Rudzitis, trad. Z. Z.</aut>,
        <vrk><url
       ref="http://miresperanto.narod.ru/diversajhoj/karma-jogo/04.htm">
        Karma Jogo</url></vrk>,
        <lok>[vidita en 2008]</lok>
      </fnt>.
    </ekz>
    <trd lng="en">brutal</trd>
    <trd lng="nl">brutaal</trd>
    <trd lng="pl">brutalny</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>brutální</trd>,
    <trd>brutální</trd>,
    <trd>hrubý</trd>,
    <trd>násilnický</trd>,
    <trd>ukrutný</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezohľadný</trd>,
    <trd>hrubý</trd>
  </trdgrp>
</drv>
</art>

