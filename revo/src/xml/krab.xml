<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krab.xml,v 1.16 2015/04/04 23:10:14 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>krab</rad>/o
</kap>
<drv mrk="krab.0o">
   <kap>
     <tld/>o
   </kap>
   <snc>
     <dif>
       Iu el kraboj:
       <ekz>
         sur la bordo malaltaj fortika&jcirc;oj plati&gcirc;is kiel 
<tld/>oj.
       </ekz>
     </dif>
     <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>
   </snc>
   <trd lng="be">&c_k;&c_r;&c_a;&c_b;</trd>
   <trd lng="de">Krabbe</trd>
   <trd lng="en">crab</trd>
   <trdgrp lng="es">
     <trd>cangrejo</trd>,
     <trd>jaiba</trd>
   </trdgrp>
   <trd lng="fr">crabe</trd>
   <trd lng="hu">r&aacute;k<klr tip="ind"> (&aacute;llat)</klr></trd>
   <trdgrp lng="id">
     <trd>kepiting</trd>,
     <trd>ketam</trd>
   </trdgrp>
   <trd lng="nl">krab</trd>
   <trd lng="pt">caranguejo</trd>
   <trd lng="ru">&c_k;&c_r;&c_a;&c_b;</trd>
</drv>

<drv mrk="krab.0oj">
   <kap>
     <tld/>oj
   </kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Subordo el la ordo de
       <ref tip="super" cel="krust.0uloj">dekpiedaj krustuloj</ref>
       (<trd lng="la">Brachyura</trd>), kies malgranda abdomeno estas
       ka&scirc;ita sub la cetera korpo.
     </dif>
   </snc>
   <trd lng="de">Krabben <klr tip="ind">(Unterordn.)</klr></trd>
   <trd lng="es">braquiuros</trd>
   <trd lng="hu">r&ouml;vidfark&uacute; <ind>r&aacute;kok</ind> <klr 
tip="ind">(alrend)</klr></trd>
   <trd lng="nl">krabben</trd>
   <trdgrp lng="ru">
     <trd>&c_k;&c_r;&c_a;&c_b;&c_y;</trd>,
 
<trd>&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_je; 
&c_r;&c_a;&c_k;&c_i;</trd>
   </trdgrp>
</drv>
</art>
