          <aut>W. Zelazny</aut>,
          <vrk>Pri la rilatoj inter inteligencio...</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:4 (66)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>politologie</trd>,
    <trd>science politique</trd>
  </trdgrp>
  <trd lng="pl">politologia</trd>
