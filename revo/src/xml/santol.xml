<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: santol.xml,v 1.13 2015/10/26 12:11:55 revo Exp $">
<kap>
  <rad>santolin</rad>/o
</kap>
<drv mrk="santol.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Santolina</trd>), spicodoranta arbedo kun grize
      lanecaj folioj kaj rompi&gcirc;emaj bran&ccirc;oj.
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Santolina</trd>
  <trdgrp lng="de">
    <trd>Heiligenblume</trd>,
    <trd>Zypressenkraut</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Santolina</trd>,
    <trd>santolina</trd>
  </trdgrp>
  <trd lng="fr">santoline</trd>
  <trd lng="hu">cipruska</trd>
  <trd lng="it">crespolina</trd>
  <trd lng="nl">heiligenbloem</trd>
  <trdgrp lng="pt">
   <trd>santolina</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_n;&c_t;&c_o;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_l;&c_i;&c_s;&c_t;&c_o;&c_g;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
