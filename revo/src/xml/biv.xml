<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: biv.xml,v 1.9 2013/09/08 18:11:14 revo Exp $">
<kap>
  <rad>biv</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>

<drv mrk="biv.0o">
  <kap><tld/>o</kap>
  <snc mrk="biv.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antikva orientazia
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      kun 4 a&ubreve; 5 kordoj, 4 a&ubreve; 5 pontoj, 4 agordaj kejloj, kiun oni ludas
      per granda plektro:
      <ekz>
	la princo havis favoratan kromedzinon, kiu lertis en <tld/>ado
	<fnt>
	<aut>Pu Songling, trad. guozhu</aut>,
	<vrk>Bravulo Baozhu</vrk>,
	<lok><url ref="http://www.elerno.net/elibro/liaozhai/">La
	stranga&jcirc;oj de Liaozhai</url>, 2000-12-09</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="liut.0o">liuto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_v;&c_a;</trd>
  <trd lng="de">Biwa</trd>
  <trd lng="en">biwa</trd>
  <trd lng="fr">biwa</trd>
  <trd lng="it">biwa</trd>
  <trd lng="ja">&#29749;&#29750; [<ind>&#12403;&#12431;</ind>]</trd>
  <trd lng="ru">&c_b;&c_i;&c_v;&c_a;</trd>
</drv>
</art>
