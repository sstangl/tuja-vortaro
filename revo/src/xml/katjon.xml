<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katjon.xml,v 1.3 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>katjon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="katjon.0o">
  <kap><tld/>o</kap>
  <snc mrk="katjon.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="jon.0o.KEM">Jono</ref>
      ricevinta pozitivan elektran &scirc;argon:
      <ekz>
        se oni anta&ubreve;e acidigis la solva&jcirc;on, kiel
        kutime &ccirc;e aminoacidoj, &ccirc;i tiuj ligi&gcirc;as
        kiel <tld/>oj
        <fnt>
          <aut>I. E. G&aacute;l</aut>,
          <vrk><url
          ref="http://www.eventoj.hu/steb/kemio/proteino1978/analizo-de-proteinoj.htm">
          Pri la analizo de proteinoj el nutradologia
          vidpunkto</url></vrk>,
          <lok>Scienca Revuo, 1978:2 (130), p. 61a-70a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anjon.0o.KEM">anjono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kation</trd>,
    <trd>pozitivní ion</trd>
  </trdgrp>
  <trd lng="en">cation</trd>
  <trd lng="fr">cation</trd>
  <trd lng="sk">katión</trd>
</drv>
</art>

