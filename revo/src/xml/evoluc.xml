<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: evoluc.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>evoluci</rad>/o
</kap>
<drv mrk="evoluc.0o">
  <kap><tld/>o</kap>
  <snc>
  <ref tip="dif" cel="evolu.0o">evoluo.</ref>
  </snc>
  <trd lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>průběh</trd>,
    <trd>vývoj</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Entwicklung</trd>,
     <trd>Evolution</trd>
  </trdgrp>
  <trd lng="en">evolution</trd>
  <trd lng="es">evoluci&oacute;n</trd>
  <trd lng="fr">&eacute;volution</trd>
  <trdgrp lng="hu">
     <trd>evol&uacute;ci&oacute;</trd>,
     <trd>fejl&odblac;d&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">evolutie<klr tip="ind"> (arch.)</klr></trd>
  <trdgrp lng="pl">
    <trd>ewolucja</trd>,
    <trd>rozw&oacute;j</trd>
  </trdgrp>
  <trd lng="pt">evolu&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>priebeh</trd>,
    <trd>vývoj</trd>
  </trdgrp>
</drv>

<drv mrk="evoluc.0i">
  <kap><tld/>i</kap>
  <snc>
  <ref tip="dif" cel="evolu.0i">evolui.</ref>
  </snc>
  <trd
  lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>sich <ind>entwickeln</ind></trd>,
     <trd>sich <ind>herausbilden</ind></trd>
  </trdgrp>
  <trd lng="en">evolve</trd>
  <trd lng="es">evolucionar</trd>
  <trd lng="fr">&eacute;voluer</trd>
  <trdgrp lng="hu">
     <trd>fejl&odblac;dik</trd>,
     <trd>evol&uacute;ci&oacute;n megy &aacute;t</trd>
  </trdgrp>
  <trd lng="nl">evolueren<klr tip="ind"> (arch.)</klr></trd>
  <trdgrp lng="pl">
    <trd>ewoluowa&cacute;</trd>,
    <trd>rozwija&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
