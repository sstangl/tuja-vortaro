<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mastik.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mastik</rad>/o
</kap>
<drv mrk="mastik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mastik.0o.KOMUNE">
    <dif>
      Nomo de diversaj pastecaj substancoj uzataj por &scirc;topi truojn a&ubreve;
      kunigi objektojn:
      <ekz>
        li sigelis la &ccirc;erkon per <tld/>o
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 32a, p.
        464a</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="mastik.0o.kreta">
      <dif>
        Miksa&jcirc;o el kreto kaj oleo, kiu malmoli&gcirc;as seki&gcirc;ante kaj servas
        por
        alfiksi vitrojn al fenestroj.
      </dif>
      <trd lng="ca">m&agrave;stic</trd>
      <trdgrp lng="hu">
        <trd>gitt</trd>,
        <trd>kitt</trd>,
        <trd>ablakragasz</trd>
      </trdgrp>
      <trd lng="nl">mastiek</trd>
      <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </subsnc>
    <trd lng="de">Kitt</trd>
    <trdgrp lng="hu">
      <trd>masztix</trd>,
      <trd>gyantaragaszt&oacute;</trd>,
      <trd>ragaszt&oacute;m&eacute;zga</trd>
    </trdgrp>
    <trd lng="nl">stopverf</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mastik</trd>
  <trd lng="ca">massilla</trd>
  <trdgrp lng="cs">
    <trd>kyt</trd>,
    <trd>lepidlo</trd>,
    <trd>mastix</trd>,
    <trd>tmel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mastic</trd>,
    <trd>putty</trd>
  </trdgrp>
  <trd lng="fr">mastic</trd>
  <trd lng="pl">kit</trd>
  <trdgrp lng="pt">
    <trd>m&aacute;stique</trd>,
    <trd>massa de vidraceiro</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>git</trd>,
    <trd>tmel</trd>
  </trdgrp>
</drv>

<drv mrk="mastik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi a&ubreve; kunigi per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">mastika&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>massillar</trd>,
    <trd>empastar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kytovat</trd>,
    <trd>tmelit</trd>
  </trdgrp>
  <trd lng="de">kitten</trd>
  <trdgrp lng="en">
    <trd>stop<klr tip="amb"> (hole)</klr></trd>,
    <trd>putty</trd>,
    <trd>fill in</trd>
  </trdgrp>
  <trd lng="fr">mastiquer<klr tip="amb"> (mettre du mastic)</klr></trd>
  <trd lng="hu">gittel</trd>
  <trd lng="nl">plamuren</trd>
  <trd lng="pl">kitowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>calafetar</trd>,
    <trd>almecegar</trd>,
    <trd>aplicar <ind>m&aacute;stique</ind></trd>    
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_z;&c_a;&c_m;&c_a;&c_z;&c_k;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>tmeliť</trd>,
    <trd>zatmeliť</trd>,
    <trd>zatrieť tmelom</trd>
  </trdgrp>
</drv>
</art>
