<vortaro>
<art mrk="$Id: kormor.xml,v 1.16 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kormoran</rad>/o
</kap>
<drv mrk="kormor.0o">
  <kap><tld/>o</kap>
  <snc mrk="kormor.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="kormor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phalacrocorax</trd>), sin nutranta per fi&scirc;oj:
      <ekz>
        en &Ccirc;inujo oni dresas <tld/>ojn al fi&scirc;kaptado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kormor&aacute;n</trd>,
    <trd>krkavec vodní</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kormoran</trd>,
    <trd>Scharbe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cormorant</trd>,
    <trd>shag</trd>
  </trdgrp>
  <trd lng="es">cormor&aacute;n</trd>
  <trd lng="fr">cormoran</trd>
  <trd lng="hu">k&aacute;r&oacute;katona</trd>
  <trd lng="nl">aalscholver</trd>
  <trd lng="pl">kormoran</trd>
  <trdgrp lng="pt">
    <trd>bigu&aacute;</trd>,
    <trd>cormor&atilde;o</trd>,    
    <trd>corvo-marinho</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;</trd>
  <trd lng="sk">kormorán</trd>
</drv>
</art>
