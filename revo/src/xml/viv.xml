    <trd>&c_m;&c_ja;&c_r;&c_c;&c_mol;&c_v;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">mrtvý <klr>(čl.)</klr></trd>
  <trd lng="de">Toter</trd>
  <trdgrp lng="fr">
    <trd>mort</trd>,
    <trd>mort-vivant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;lettelen l&eacute;ny</trd>,
    <trd>holt</trd>
  </trdgrp>
  <trd lng="nl">overledene</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_t;&c_v;&c_je;&c_c;</trd>,
