<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: citr.xml,v 1.18 2013/09/08 18:11:17 revo Exp $">
<kap><ofc>*</ofc>
<rad>citr</rad>/o</kap>
<drv mrk="citr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="citr.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumentoj</ref>
      kun bloka sonkesto &ccirc;e kiu kordoj estas fiksitaj
      de unu ekstremo al alia, kontraste al
      <ref tip="vid" cel="harp.0o">harpoj</ref>,
      <ref tip="vid" cel="lir.0o.MUZ">liroj</ref>,
      <ref tip="vid" cel="liut.0o">liutoj</ref>.
    </dif>
    <rim>
      Pluraj vortaroj nomas <ctl>citro</ctl>
      anka&ubreve; e&ubreve;ropan liuteton pir-siluetan
      kun plata sonkesto, longa kolo, 30 &gcirc;is 45
      metalaj, pluke sonigataj kordoj
      pro ties simila nomo (france: <ctl>cistre</ctl>,
      ruse: <ctl>&c_c;&c_i;&c_s;&c_t;&c_r;&c_a;</ctl>).
    </rim>
    <trd lng="be">&c_c;&c_y;&c_t;&c_r;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;.
    &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
    <trdgrp lng="en">
      <trd>zither</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Zither</trd>
    </trdgrp>
    <trd lng="fr">cithare<klr> (moderne)</klr></trd>
    <trd lng="hu">citera</trd>
    <trdgrp lng="it">
  <trd>cetra<klr tip="amb"> (moderna o famiglia)</klr></trd>, 
  <trd>citara<klr tip="amb"> (moderna o famiglia)</klr></trd>
  </trdgrp>
    <trd lng="nl">citer</trd>
    <trd lng="pl">cytra</trd>
    <trd lng="pt">c&iacute;tara</trd>
    <trd lng="ru">&c_c;&c_i;&c_t;&c_r;&c_a;</trd>
  </snc>
  <snc mrk="citr.0o.citaro">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="citar.0o">citaro</ref>
  </snc>
  <trd lng="ca">c&iacute;tara</trd>
</drv>
</art>
