<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: falar.xml,v 1.16 2013/08/28 16:10:23 revo Exp $">
<kap>
  <rad>falar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="falar.0o">
  <kap><tld/>o</kap>
  <snc mrk="falar.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbospeco trovebla en &ccirc;iuj kontinentoj,
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Phalaris</trd>):
      <ekz>
        &ccirc;u vi bredas fringelojn, Molly? Sed &ccirc;u vi havas
        <tld/>on por man&gcirc;igi ilin? Respondu, mi petas!
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_o;&c_t;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="ca">escaiola</trd>
  <trdgrp lng="de">
    <trd>Glanzgras</trd>,
    <trd>Kanariengras</trd>
  </trdgrp>
  <trd lng="en">canary grass</trd>
  <trd lng="fr">alpiste</trd>
  <trd lng="hu">kan&aacute;rik&ouml;les</trd>
  <trdgrp lng="it">
    <trd>canaria</trd>,
    <trd>scagliuola</trd>
  </trdgrp>
  <trd lng="nl">rietgras</trd>
  <trdgrp lng="pt">
    <trd>alpiste</trd>,
    <trd>f&aacute;laris</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_r;&c_je;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
