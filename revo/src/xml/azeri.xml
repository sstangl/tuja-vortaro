<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: azeri.xml,v 1.6 2014/06/09 14:10:16 revo Exp $">
    <kap><rad>Azeri</rad>/o</kap>

    <drv mrk="azeri.A0o">
      <kap><tld/>o</kap>
      <snc mrk="azeri.A0o.respubliko">
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <ref tip="dif" cel="azerbajg.A0o">Azerbaj&gcirc;ano</ref>
        <ekz>
          <tld/>o (Azerbaj&gcirc;ano) prenis latinan alfabeton por
          alproksimi&gcirc;i al Turkio, kiu grandparte pagis la novan
          eldonon de lernolibroj
          <fnt>
            <vrk>&leftquot;Expolangues&rightquot; &#8208; la 14a
            lingvofoiro en
            Parizo</vrk>.
            <lok><url ref="http://www.eventoj.hu/arkivo/eve-102.htm"
                >Eventoj, n-ro 102, 2 (majo 1996)</url></lok>
          </fnt>
        </ekz>
      </snc>
      <ref tip="sin" cel="azer.A0ujo">Azerujo</ref>
      <ref tip="vid" cel="azer.0o">azero</ref>
      <trd
      lng="bg">&c_A;&c_z;&c_je;&c_r;&c_b;&c_a;&c_j;&c_d;&c_zh;&c_a;&c_n;</trd>
      <trd lng="de">Aserbaidschan</trd>
      <trd lng="en">Azerbaijan</trd>
      <trd lng="fr">Azerba&iuml;djan</trd>
      <trd lng="it">Azerbaigian</trd>
      <trd lng="pl">Azerbejd&zdot;an</trd>
      <trd lng="ru">&c_A;&c_z;&c_je;&c_r;&c_b;&c_a;&c_j;&c_d;&c_zh;&c_a;&c_n;</trd>
      <trd lng="tr">Azerbaycan</trd>
    </drv>

  </art>

