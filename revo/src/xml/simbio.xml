<vortaro>
<art mrk="$Id: simbio.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>simbioz</rad>/o
</kap>
<drv mrk="simbio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Kuni&gcirc;o de du a&ubreve; pluraj plantoj a&ubreve; animaloj por
kune vivi:
      <ekz>
        likeno estas <tld/>o de algo kaj fungo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_m;&c_b;&c_ib;&c_jo;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>soužití</trd>,
    <trd>spolužití</trd>,
    <trd>symbióza</trd>
  </trdgrp>
  <trd lng="de">Symbiose</trd>
  <trd lng="en">symbiosis</trd>
  <trd lng="fa">&a_h;&a_m;&zwnj;&a_z;&f_y;&a_s;&a_t;&f_y;</trd>
  <trdgrp lng="hu">
    <trd>egy&uuml;tt&eacute;l&eacute;s</trd>,
    <trd>szimbi&oacute;zis</trd>
  </trdgrp>
  <trd lng="nl">symbiose</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_b;&c_i;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>spolužitie</trd>,
    <trd>symbióza</trd>
  </trdgrp>
</drv>
</art>
