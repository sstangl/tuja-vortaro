<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hard.xml,v 1.26 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hard</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hard.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hard.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Prilabori la internan strukturon de metalo,
      por akiri rezistemon kontra&ubreve;
      foruzi&gcirc;o, fleksado, premado, tordado, pere de
      varmigo, &hcirc;emia trakto a&ubreve; amba&ubreve;:
      <ekz>
        <tld/>ita glavo, martelo, &ccirc;izilo.
      </ekz>
    </dif>
    <trd lng="ca">trempar</trd>
    <trd lng="es">templar</trd>
    <trd lng="nl">stalen<klr tip="ind"> (staal harden)</klr></trd>
    <trd lng="pt">temperar<klr> a&ccedil;o</klr></trd>
  </snc>
  <snc>
    <dif>
      Malmoligi:
      <ekz>
        <tld/>i gipson, cementon.
      </ekz>
    </dif>
    <trd lng="ca">endurir</trd>
    <trd lng="es">endurecer</trd>
    <trd lng="nl">harden</trd>
    <trdgrp lng="pt">
      <trd>endurecer</trd>,
      <trd>enrijecer</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi nesentema, krudigi, fortigi:
      <ekz>
        <ind><tld/>ita</ind> kontra&ubreve; la frosto, la malsano;
        <trdgrp lng="fr">
          <trd>tremp&eacute;<klr tip="ind"> (durci)</klr></trd>,
          <trd>endurci</trd>,
          <trd>inv&eacute;t&eacute;r&eacute;</trd></trdgrp>
      </ekz>
      <ekz>
        mizero <tld/>as korojn;
      </ekz>
      <ekz>
        maljunulo <tld/>ita en malfeli&ccirc;oj;
      </ekz>
      <ekz>
        bone <tld/>ita soldato;
      </ekz>
      <ekz>
        favorkoreco kaj veremeco <tld/>as re&gcirc;on
        <fnt><bib>MT</bib>, <lok>&Sen; 20:28</lok></fnt>;
      </ekz>
      <ekz>
        nia bravega anaro,
        obstine &gcirc;i pa&scirc;as, provita, <tld/>ita
        <fnt>(<aut>Zamenhof</aut>: <vrk>La Vojo</vrk>)</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">insensibilitzar</trd>
    <trd lng="de">abh&auml;rten</trd>
    <trdgrp lng="nl">
      <trd>hard maken</trd>,
      <trd>verharden<klr tip="ind"> (tr.)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>endurecer</trd>,
      <trd>enrijecer</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>otužit</trd>,
    <trd>otužovat</trd>,
    <trd>utužit</trd>,
    <trd>utužovat</trd>
  </trdgrp>
  <trd lng="de">h&auml;rten</trd>
  <trd lng="en">harden</trd>
  <trdgrp lng="fr">
    <trd>tremper<klr tip="ind"> (le m&eacute;tal, le caract&egrave;re)</klr></trd>,
    <trd>aguerrir</trd>,
    <trd>durcir<klr tip="ind"> (tr.)</klr></trd>,
    <trd>endurcir</trd></trdgrp>
  <trdgrp lng="hu">
      <trd>megedz</trd>,
      <trd>megkem&eacute;ny&iacute;t</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>temprare</trd>,
      <trd>indurire<klr> (tr.)</klr></trd>
    </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>otužiť</trd>,
    <trd>otužovať</trd>
  </trdgrp>
  <trd lng="sv">h&auml;rda</trd>
</drv>

<drv mrk="hard.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="hard.0ado.KOMUNE">
    <dif>
      Procedo per kiu oni <tld/>as ion:
      <ekz>
        <tld/>ado en akvo, en oleo, per aerblovo
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kalení</trd>,
    <trd>tvrzení</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trempe<klr tip="ind"> (traitement qui
    endurcit)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>edz&eacute;s<klr tip="ind"> (h&odblac;kezel&eacute;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kalenie</trd>,
    <trd>tvrdenie</trd>
  </trdgrp>
</drv>

<drv mrk="hard.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hard.0igxi.KOMUNE">
    <dif>
      I&gcirc;i rezistema kontra&ubreve; eksteraj atakoj,
      malmola, fortika:
      <ekz>
        <tld/>i&gcirc;inta krimulo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>endurir-se</trd>,
    <trd>empedreir-se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>aguerrir</ind></trd>,
    <trd>se <ind>durcir</ind></trd>,
    <trd>s'<ind>endurcir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>edzett</trd>,
    <trd>megedz&odblac;d&ouml;tt</trd>
  </trdgrp>
</drv>

<drv mrk="hard.0iteco">
  <kap><tld/>iteco, <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="hard.0iteco.KOMUNE">
    <dif>
      Kvalito de io <tld/>ita a&ubreve; &gcirc;enerale rezista kontra&ubreve;
      la enigo de alia korpo:
      <ekz>
        <tld/>iteco de gipso, karaktero, volo; hardeco de &scirc;talo,
        gisfero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tremp</trd>,
    <trd>fortalesa</trd>
  </trdgrp>
  <trd lng="cs">tvrdost</trd>
  <trd lng="de">H&auml;rte</trd>
  <trdgrp lng="fr">
    <trd>duret&eacute;</trd>,
    <trd>endurcissement</trd>,
    <trd>trempe<klr tip="ind"> (caract&egrave;re endurci)</klr></trd>,
  </trdgrp>
  <trd lng="hu">edzetts&eacute;g</trd>
  <trdgrp lng="it">
      <trd>tempra</trd>,
      <trd>durezza</trd>
    </trdgrp>
  <trd lng="nl">gehardheid</trd>
  <trd lng="pt">dureza</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">tvrdosť</trd>
</drv>

<drv mrk="hard.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hard.mal0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Renversi la rezulton de <tld/>ado, donante al
      metalo unuforman, konstantan strukturon, kiu
      estis dama&gcirc;e influita de anta&ubreve;aj procedoj.
    </dif>
    <trd lng="ca">destrempar</trd>
    <trd lng="de">weichgl&uuml;hen</trd>
    <trd lng="en">annealing (metallurgy)</trd>
        <trdgrp lng="hu">
      <trd>megereszt<klr tip="ind"> (f&eacute;met)</klr></trd>,
      <trd>kil&aacute;gy&iacute;t<klr tip="ind"> (f&eacute;met)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Moligi, malfortigi:
      <ekz>
        la ri&ccirc;eco mal<tld/>as la kura&gcirc;on.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>ablanir</trd>,
      <trd>estovar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elk&eacute;nyelmes&iacute;t</trd>,
      <trd>ellust&iacute;t</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">měkčit</trd>
  <trdgrp lng="de">
  <trd>erweichen</trd>,
  <trd>schw&auml;chen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tremper<klr tip="ind"> (un m&eacute;tal)</klr></trd>,
    <trd>amollir</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>stemprare</trd>, 
      <trd>stemperare<klr tip="amb"> (stemprare)</klr></trd>,
      <trd>far <ind>rinvenire</ind><klr> (metal.)</klr></trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>zacht maken</trd>,
    <trd>week maken</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(z)</klr>mäkčiť</trd>,
    <trd>oslabiť</trd>,
    <trd>popustiť</trd>,
    <trd>zoslabiť</trd>
  </trdgrp>
</drv>
</art>
