<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kresto.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>krestomati</rad>/o
    </kap>
    <drv mrk="kresto.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
	<dif>
	  Instrucela <ref tip="super" cel="tekst.0aro">tekstaro</ref>
	  da prozaj kaj poeziaj pecoj, ofte en unu sama lingvo, a&ubreve; el
	  verkoj de unu sama a&ubreve;toro:
          <ekz>
          cedante al la peto de multaj esperantistoj, mi eldonis la
          Fundamenton <tld lit="K"/>on, kiu povos servi al &ccirc;iuj kiel
          modelo de esperanta stilo<fnt><bib>FK</bib></fnt>;
          </ekz>
	  <ekz>
	    atentan tralegon de la Fundamenta <tld lit="K"/>o mi rekomendas al
	    &ccirc;iu<fnt><bib>FK</bib></fnt>.
	  </ekz>
	</dif>
	<ref tip="vid" cel="antolo.0o.BELE">antologio</ref>
      </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_s;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">crestomatia</trd>
  <trdgrp lng="cs">
    <trd>výbor z četby</trd>,
    <trd>čítanka</trd>
  </trdgrp>
  <trdgrp lng="de">
         <trd>Chrestomathie</trd>,
         <trd>Lernlesebuch</trd>
      </trdgrp>
  <trd lng="en">chrestomathy</trd>
  <trd lng="fr">chrestomathie</trd>
  <trdgrp lng="hu">
         <trd>sz&ouml;veggy&udblac;jtem&eacute;ny</trd>,
         <trd>kresztom&aacute;tia</trd>
      </trdgrp>
  <trd lng="nl">bloemlezing</trd>
  <trdgrp lng="pt">
        <trd>crestomatia</trd>,
        <trd>antologia</trd>
      </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_je;&c_s;&c_t;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>chrestomatia</trd>,
    <trd>pedagogická zbierka poézie a prózy</trd>
  </trdgrp>
</drv>
  </art>
