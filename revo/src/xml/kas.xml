<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kas.xml,v 1.27 2015/10/01 15:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kas</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kas.0o.monkesto">
    <ref tip="dif" cel="kest.mon0o">monkesto</ref>
    <ekz>
      dum tiu incidento el la <tld/>o iel mistere malaperis
      la tuta taga enspezo
      <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 27</lok></fnt>;
    </ekz>
    <ekz>
      la unua <klr>[desegna&jcirc;o]</klr> estis trovita en
      la malplena <tld/>o de a&ubreve;tomata alkoholvendejo
      <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
    </ekz>
    <ekz>
      plena <tld/>o &scirc;teliston altiras
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="be">&c_k;&c_a;&c_s;&c_a;</trd>
    <trd lng="bg">&c_k;&c_a;&c_s;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Kasse</trd>,
      <trd>Geldkassette</trd>,
      <trd>Geldkasten</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cash box</trd>,
      <trd>money box</trd>,
      <trd>till</trd>
    </trdgrp>
    <trd lng="fr">caisse<klr tip="ind">
    (bo&icirc;te pour l'argent)</klr></trd>
    <trdgrp lng="he">
      <trd>&qof;&vav;&pe;&he;<klr tip="ind">
      (&kaf;&samekh;&fpe;&tav;)</klr></trd>,
      <trd>&alef;&vav;&tsadi;&resh;<klr tip="ind">
      (&qof;&vav;&pe;&he;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>p&eacute;nzesl&aacute;da</trd>,
      <trd>kassza</trd>,
      <trd>k&eacute;zip&eacute;nzt&aacute;r</trd>,
      <trd>p&eacute;nzkazetta</trd>
    </trdgrp>
    <trd lng="nl">kas</trd>
  <trdgrp lng="pt">
    <trd>caixa <klr tip="amb">(de dinheiro)</klr></trd>,
    <trd>cofre</trd>,
    <trd>caixa-forte</trd>,
    <trd>burra</trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
    <trd lng="sv">kassa</trd>
  </snc>
  <snc mrk="kas.0o.ejo">
    <fnt><bib>VdE</bib></fnt>
    <dif>
      &Ccirc;ambro, destinita por enspezado kaj elspezado de mono, kaj
      en kiu oni konservas la <tld/>on:
      <ekz>
        kiam &scirc;i estis paganta &ccirc;e la <tld/>o,
        &scirc;iaj &scirc;uoj malaperis
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 19</lok></fnt>;
      </ekz>
      <ekz>
        jam mi estis anta&ubreve;saltinta, kubute for&scirc;ovante
        maljunulinon de la <tld/>ofenestro
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kas.0ejo"><tld/>ejo</ref>
    <trd lng="be">&c_k;&c_a;&c_s;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Kasse</trd>,
      <trd>Kassenraum</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cashier's office</trd>,
      <trd>cashpoint</trd>,
      <trd>cashdesk</trd>
    </trdgrp>
    <trd lng="fr">caisse<klr tip="ind"> (lieu o&ugrave; l'on paie)</klr>
    </trd>
    <trd lng="he">&qof;&vav;&pe;&he;<klr tip="ind">
    (&nun;&qof;&vav;&dalet;&tav; &mem;&kaf;&yod;&resh;&he;)</klr></trd>
    <trdgrp lng="hu">
      <trd>p&eacute;nzt&aacute;r</trd>,
      <trd>p&eacute;nzt&aacute;rhelyis&eacute;g</trd>,
      <trd>p&eacute;nzt&aacute;rterem</trd>
    </trdgrp>
    <trd lng="nl">kassa</trd>
  <trdgrp lng="pt">
    <trd>caixa <klr tip="amb">(local)</klr></trd>,
    <trd>tesouraria</trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
    <trd lng="sv">kassa</trd>
  </snc>
  <snc mrk="kas.0o.monrimedoj">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Monaj rimedoj:
      <ekz>
        por kio rompi se&gcirc;ojn? &gcirc;i estas malprofito por
        la <ind>regna <tld/>o</ind>;
        <trd lng="fr">tr&eacute;sor public</trd>
        <fnt><bib>Revizoro</bib>, <lok>1:1</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;a <tld/>o havos malprofiton
        <fnt><bib>MT</bib>, <lok>&Ezr; 4:13</lok></fnt>;
      </ekz>
      <ekz>
        mi jam multe &scirc;uldas al nia <ind><tld/>o de reciproka
        helpo</ind>
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 1</lok></fnt>;
        <trdgrp lng="fr">
          <trd>caisse d'entraide</trd>,
          <trd>caisse de solidarit&eacute;</trd>
        </trdgrp>
        <trd lng="nl">solidariteitskas</trd>
      </ekz>
      <ekz>
        al kuko kaj <tld/>o &ccirc;iam venas amaso
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivi de <ind>&scirc;parita <tld/>o</ind>
        <klr>(mono)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr"><ind>&eacute;conomie</ind>s</trd>
        <trd lng="he">&het;&samekh;&kaf;&vav;&nun;&vav;&tav;</trd>
        <trd lng="nl">spaargeld</trd>
        <trd lng="hu">megtakar&iacute;&aacute;s</trd>
      </ekz>
      <ekz>
        <ind>kiu rompis la glason, tiu ordigu la <tld/>on</ind>
        <fnt>Z</fnt>
        <klr>(pagante la perdon, &ccirc;ar &ccirc;iu devas
        prirespondi pri siaj faroj)</klr>.
        <trd lng="fr">il faut payer pour ses <ind>erreur</ind>s</trd>
        <trd lng="nl">wie het <ind>potje</ind> breekt, moet het
        betalen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posx.0o">po&scirc;o</ref>,
      <ref cel="sak.0o">sako</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Kasse</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>coffer</trd>,
      <trd>purse</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caisse<klr tip="ind"> (moyens financiers)</klr></trd>,
      <trd>argent</trd>
    </trdgrp>
    <trd lng="nl">kas</trd>
  <trdgrp lng="pt">
    <trd>caixa <klr tip="amb">(recursos)</klr></trd>,
    <trd>recursos financeiros</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pen&iacute;ze v pokladn&ecaron;</trd>,
    <trd>pokladn&iacute; hotovost</trd>
  </trdgrp>
  <trd lng="pl">kasa</trd>
  <trd lng="sk">pokladni&ccaron;n&aacute; miestnos&tcaron;</trd>
</drv>

<drv mrk="kas.0ejo">
  <kap><tld/>ejo</kap>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="kas.0ejo.KOMUNE">
    <ref tip="dif" cel="kas.0o.ejo"><tld/>o<sncref/></ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;</trd>
  <trd lng="cs">pokladna - m&iacute;stnost</trd>
  <trdgrp lng="de">
    <trd>Kasse</trd>,
    <trd>Kassenraum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cashier's office</trd>,
    <trd>cashpoint</trd>,
    <trd>cashdesk</trd>
  </trdgrp>
  <trd lng="fr">caisse<klr tip="ind"> (lieu o&ugrave; l'on paie)</klr>
  </trd>
  <trd lng="he">&qof;&vav;&pe;&he;<klr tip="ind">
  (&nun;&qof;&vav;&dalet;&tav; &mem;&kaf;&yod;&resh;&he;)</klr></trd>
  <trdgrp lng="hu">
    <trd>p&eacute;nzt&aacute;r</trd>,
    <trd>p&eacute;nzt&aacute;rhelyis&eacute;g</trd>,
    <trd>p&eacute;nzt&aacute;rterem</trd>
  </trdgrp>
  <trd lng="nl">kassa</trd>
  <trd lng="pl">kasa</trd>
  <trdgrp lng="pt">
    <trd>caixa <klr tip="amb">(local)</klr></trd>,
    <trd>tesouraria</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
  <trd lng="sk">pokladni&ccaron;n&aacute; miestnos&tcaron;</trd>
  <trd lng="sv">kassa</trd>
</drv>

<drv mrk="kas.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kas.0isto.EKON">
    <dif>
      Tiu, kiu mastrumas <tld/>on.
    </dif>
    <subsnc>
      <dif>
        Oficisto, kiu en- kaj elspezas monon en <tld/>o
        <sncref ref="kas.0o.ejo"/>.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        Societano, kies tasko estas kolekti la
        kotizojn kaj prizorgi la monon.
      </dif>
    </subsnc>
    <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Kassierer</trd>,
      <trd>Kassenf&uuml;hrer</trd>,
      <trd>Kassenverwalter</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>treasurer</trd>,
      <trd>paymaster</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kas&iacute;r</trd>,
    <trd>pokladn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caissier</trd>,
    <trd>tr&eacute;sorier</trd>
  </trdgrp>
  <trd lng="he">&gimel;&zayin;&bet;&resh;</trd>
  <trd lng="hu">p&eacute;nzt&aacute;ros</trd>
  <trdgrp lng="nl">
    <trd>kassier</trd>,
    <trd>penningmeester</trd>
  </trdgrp>
  <trd lng="pl">kasjer</trd>
  <trdgrp lng="pt">
    <trd>caixa <klr tip="amb">(funcion&aacute;rio)</klr></trd>,
    <trd>tesoureiro</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;</trd>
  <trd lng="sk">pokladn&iacute;k</trd>
  <trd lng="sv">kass&ouml;r</trd>
</drv>

<drv mrk="kas.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kas.en0igi.KOMUNE">
    <dif>
      Ricevi monon,
      <ref tip="vid" cel="spez.en0i.EKON">enspezi</ref>,
      <ref tip="vid" cel="posx.en0igi.FIG">enpo&scirc;igi</ref>.
    </dif>
    <trd lng="be">&c_ib;&c_n;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>einnehmen</trd>,
      <trd>einziehen</trd>,
      <trd>einkassieren</trd>
    </trdgrp>
    <trd lng="en">pay in</trd>
    <trd lng="fr">encaisser<klr tip="ind"> (mettre en caisse)</klr>
    </trd>
    <trd lng="he">&lamed;&he;&kaf;&nun;&yod;&samekh;<klr tip="ind">
    (&kaf;&samekh;&fpe;)</klr></trd>
    <trdgrp lng="hu">
      <trd>bev&eacute;telez</trd>,
      <trd>behajt</trd>,
      <trd>p&eacute;nzt <ind>beszed</ind></trd>,
      <trd>inkassz&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">incasseren</trd>
    <trd lng="pl">inkasowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>recolher <klr tip="amb">(dinheiro)</klr></trd>,
    <trd>arrecadar dinheiro</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&aacute;t do pokladny</trd>,
    <trd>inkasovat</trd>,
    <trd>kas&iacute;rovat</trd>
  </trdgrp>
  <trd lng="sk">inkasova&tcaron;</trd>
</drv>

<drv mrk="kas.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <snc mrk="kas.0masxino.KOMUNE">
    <dif>
      Ma&scirc;ino entenanta tirkeston por kasa funkcio,
      kaj plenumanta rilatajn monkalkulojn,
      ofte uzata en vendejo:
      <ekz>
        restis sur la prezentilo antikva pesilo
        kaj klasika <tld/>ma&scirc;ino
        <fnt>
          <aut>I. Ertl</aut>,
          <vrk><url
          ref="http://pistike65.wordpress.com/2011/08/29/buckeye-road/">
          Buckeye Road</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cash register</trd>
  <trd lng="fr">caisse enregistreuse</trd>
</drv>
</art>
