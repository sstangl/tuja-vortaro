<vortaro>
<art mrk="$Id: taoist.xml,v 1.12 2015/07/04 08:37:44 revo Exp $">
<kap>
   <rad>taoist</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="taoist.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">REL</uzo>
   <snc>
     <ref tip="dif" cel="daux.0isto">da&ubreve;isto</ref>
   </snc>
   <rim mrk="taoist.0o.RIM">
     &leftquot;Tao&rightquot; &leftquot;taoismo&rightquot; kaj
     &leftquot;taoisto&rightquot; estas pli uzataj, sed malpli regulaj
     formoj ol &leftquot;da&ubreve;o&rightquot;,
     &leftquot;da&ubreve;ismo&rightquot;, &leftquot;da&ubreve;isto.
     <aut>&jcirc;v</aut>
   </rim>
   <trd lng="ca">taoista</trd>
   <trd lng="fr">tao&iuml;ste</trd>
   <trd lng="hu">taoista</trd>
   <trd lng="pl">taoista</trd>
   <trd lng="pt">taoista</trd>
   <trd lng="ru">&c_d;&c_a;&c_o;&c_s;</trd>
  <trd lng="sk">taoista</trd>
  <trd lng="cs">taoista</trd>
</drv>
</art>
