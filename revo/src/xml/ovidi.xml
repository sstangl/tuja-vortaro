<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ovidi.xml,v 1.8 2015/02/26 19:11:18 revo Exp $">
<kap>
  <rad>Ovidi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ovidi.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovidi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
    <trd lng="lat">Ovidius</trd>
  </snc>
  <snc mrk="ovidi.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      <ref tip="lst" cel="beletr.0isto"
      lst="voko:historiaj_personoj">Beletristo</ref> latina (43 &ndash; 17 a&ubreve;
      18) naskita en Italio, vivinta en
      Romo, kaj vivofine ekzile apud la Nigra Maro (nuna Rumanio),
      a&ubreve;toro
      de ampoemoj, tragedio, versleteroj kaj mitpoemo
      <ctl>Metamorfozoj</ctl>:
      <ekz>
        kelkaj fabloj de <tld/>o
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 6a, p.
        59a</lok></fnt>
      </ekz>
    </dif>
    <trd lng="lat">Publius <ind>Ovidius Naso</ind></trd>
  </snc>
  <trd lng="be">&c_A;&c_v;&c_ib;&c_d;&c_y;&c_j;</trd>
  <trd lng="de">Ovid</trd>
  <trd lng="fr">Ovide</trd>
  <trd lng="hu">Ovidius</trd>
  <trd lng="it">Ovidio</trd>
  <trd lng="pt">Ov&iacute;dio</trd>
  <trd lng="ru">&c_O;&c_v;&c_i;&c_d;&c_i;&c_j;</trd>
</drv>
</art>

