<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hipotr.xml,v 1.6 2009/09/12 16:31:18 revo Exp $">
  <kap>
    <rad>hipotro&hcirc;oid</rad>/o
  </kap>
<drv mrk="hipotr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <refgrp tip="sub">
      <ref cel="hipoci.longigita0o">longigita hipocikloido</ref>,
      <ref cel="hipoci.mallongigita0o">mallongigita hipocikloido</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="trohxo.0o">tro&hcirc;oido</ref>,
      <ref cel="epitro.0o">epitro&hcirc;oido</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_a;&c_t;&c_r;&c_a;&c_h;&c_o;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Hypotrochoide</trd>
  <trd lng="en">hypotrochoid</trd>
  <trd lng="fr">hypotrocho&iuml;de</trd>
  <trd lng="hu">hipotrochoid</trd>
  <trd lng="it">ipotrocoide</trd>
  <trd lng="nl">hypotrochoide</trd>
  <trd lng="pl">hipotrochoida</trd>
  <trdgrp lng="pt">
    <trd>hipocicl&oacute;ide</trd>,
    <trd>hipotroc&oacute;ide</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_t;&c_r;&c_o;&c_h;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

