<vortaro>
<art mrk="$Id: redut.xml,v 1.14 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>redut</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="redut.0o">
  <kap><tld/>o</kap>
  <snc mrk="redut.0o.MIL">
    <uzo tip="fak">ARKI</uzo> 
    <uzo tip="fak">MIL</uzo>
    <dif>
      Kampa fortika&jcirc;o:
      <ekz>
        sur la <tld/>o <ctl>Cruys</ctl> sonis seninterrompe la voko:
        &leftquot;Bone gardu! Bone gardu!&rightquot;
        <fnt>
          <aut>W. Raabe, trad. F. Wicke</aut>,
          <vrk>La Nigra Galero</vrk>,
          <lok>2a eld., 1922</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_u;&c_t;</trd>
  <trd lng="bg">&c_r;&c_je;&c_d;&c_u;&c_t;</trd>
  <trd lng="cs">maškarní ples</trd>
  <trd lng="de">Redoute</trd>
  <trd lng="en">redoubt</trd>
  <trd lng="fr">redoute</trd>
  <trdgrp lng="hu">
    <trd>redut</trd>,
    <trd>z&aacute;rt <ind>s&aacute;nc</ind></trd>
  </trdgrp>
  <trd lng="it">ridotta<klr> (mil.)</klr></trd>
  <trdgrp lng="nl">
    <trd>redoute</trd>,
    <trd>schans</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_u;&c_t;</trd>
  <trd lng="sk">poľná pevnosť</trd>
</drv>
</art>
