<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: asklep.xml,v 1.12 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>asklepiad</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="asklep.0o">
  <kap><tld/>o</kap>
  <snc mrk="asklep.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <tld/>acoj, en kiu abundas
      blanka laktosimila suko
      (<trd lng="la">Asclepias</trd>).
    </dif>
    <trd lng="en">milkweed</trd>
    <trd lng="de">Seidenpflanze</trd>
    <trdgrp lng="fr">
      <trd>ascl&eacute;piade</trd>,
      <trd>ascl&eacute;pias</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>selyemk&oacute;r&oacute;</trd>,
      <trd>krepin</trd></trdgrp>
    <trd lng="it">asclepiadi</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="asklep.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Speco de verso konsistanta el &hcirc;orjamboj.
    </dif>
    <trd lng="fr">ascl&eacute;piade</trd>
    <trd lng="hu">aszklepiad&eacute;szi sor</trd>
    <trdgrp lng="it">
      <trd>asclepiadei<klr> (poes.)</klr></trd>,
      <trd><ind>dodecasillabi</ind> asclepiadei</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>asklep</trd>,
    <trd>asklepiadický verš</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>asklep</trd>,
    <trd>asklepiatický verš</trd>
  </trdgrp>
</drv>
</art>
