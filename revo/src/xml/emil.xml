<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: emil.xml,v 1.9 2015/02/26 20:10:37 revo Exp $">
<kap>
  <rad>Emil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="emil.0o">
  <kap><tld/>o</kap>
  <snc mrk="emil.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        poste ni iris al Soro kaj trovis tie <tld/>on
        <fnt><bib>Fab4</bib>, <lok>peco da perlovico, 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Emili</trd>
  <trd lng="de">Emil</trd>
  <trd lng="fr">&Eacute;mile</trd>
  <trd lng="hu">Emil</trd>
  <trd lng="it">Emilio</trd>
  <trd lng="pl">Emil</trd>
  <trd lng="pt">Em&iacute;lio</trd>
  <trd lng="ru">&c_E;&c_m;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>

