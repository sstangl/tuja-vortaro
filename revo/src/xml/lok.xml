<drv mrk="lok.du0a">
  <kap>du<tld/>a</kap>
  <fnt><bib>KompLeks</bib></fnt>
  <snc mrk="lok.du0a.operacio">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Kies <ref cel="lok.0nombro.argumenta">loknombro</ref> estas 2:
      <ekz>
        subtraho estas du<tld/>a operacio;
      </ekz>
      <ekz>
        <ctl>superi</ctl> estas du<tld/>a rilato.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>binary</trd>,
      <trd>dyadic</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>dyadisch</trd>,
      <trd>zweistellig</trd>,
      <trd>zweiwertig</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de dos <ind>argumentos</ind></trd>,
      <trd>binario<klr> (de dos argumentos)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&agrave; deux <ind>arguments</ind></trd>,
      <trd>binaire<klr> (&agrave; deux arguments)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>diadikus</trd>,
      <trd>bin&eacute;r</trd>,
