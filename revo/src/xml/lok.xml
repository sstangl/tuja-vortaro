    </trdgrp>
    <trd lng="id">biner</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;<klr> (&c_o;&c_b;
      &c_o;&c_p;&c_je;&c_r;&c_a;&c_c;&c_i;&c_i;)</klr></trd>,
      <trd>&c_b;&c_i;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">dvojm&iacute;stn&yacute;</trd>
  <trd lng="sk">dvojmiestny</trd>
</drv>

