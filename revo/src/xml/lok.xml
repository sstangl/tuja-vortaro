  <trdgrp lng="es">
    <trd>dispersar</trd>,
    <trd>diseminar</trd>,
    <trd>propagar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diss&eacute;miner</trd>,
    <trd>disperser</trd>
