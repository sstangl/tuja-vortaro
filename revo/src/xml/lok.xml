  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;ekl&aacute;dat <klr>(zbo&zcaron;&iacute;)</klr></trd>,
    <trd>p&rcaron;em&iacute;stit</trd>,
    <trd>p&rcaron;est&ecaron;hovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umsetzen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>move/transfer/place <klr>(something)</klr> elsewhere</trd>,
    <trd>bring over</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desplazar</trd>,
    <trd>trasladar</trd>,
    <trd>transportar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;placer</trd>,
    <trd>transporter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;thelyez</trd>,
    <trd>&aacute;ttesz</trd>
  </trdgrp>
  <trd lng="nl">overbrengen</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>premiestni&tcaron;</trd>,
    <trd>pres&tcaron;ahova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>flytta<klr> (n&aring;got)</klr></trd>,
    <trd>transportera</trd>
  </trdgrp>
</drv>

<drv mrk="lok.trans0igxi">
  <kap>trans<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Iri en alian <tld/>on:
      <ekz>
        multaj kapitaloj trans<tld/>i&gcirc;is el E&ubreve;ropo al Ameriko.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd><ind>mover</ind>se</trd>,
      <trd><ind>trasladar</ind>se</trd>
    </trdgrp>
    <trd lng="en">be moved/transferred/relocated</trd>
    <trdgrp lng="fr">
      <trd>se <ind>d&eacute;placer</ind></trd>,
      <trd>se <ind>transf&eacute;rer</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;thelyez&odblac;dik</trd>,
      <trd>&aacute;tk&ouml;lt&ouml;zik</trd>
    </trdgrp>
    <trd lng="id">[<ind>pindah</ind>] memindahkan</trd>
    <trd
    lng="ru">&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="nl">verhuizen</trd>
  <trdgrp lng="de">
    <trd>umziehen</trd>,
    <trd>abwandern</trd>
  </trdgrp>
</drv>

<drv mrk="lok.sid0o">
  <kap>sid<tld/>o</kap>
  <snc mrk="lok.sid0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Okupebla <tld/>o por sido ekzemple en veturilo, en 
      atendejo...:
      <ekz>
        &ccirc;iu pasa&gcirc;ero rajtas okupi nur unu sid<tld/>on
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&iacute;sto k sezen&iacute;</trd>
  <trdgrp lng="en">
    <trd>seat <klr>(in a legislature)</klr></trd>,
    <trd>sitting place</trd>
  </trdgrp>
  <trd lng="es">asiento</trd>
  <trd lng="fr">place<klr> (si&egrave;ge)</klr></trd>
  <trd lng="id"><ind>tempat</ind> duduk</trd>
  <trdgrp lng="sk">
    <trd>miesto na sedenie</trd>,
    <trd>sedadlo</trd>
  </trdgrp>
</drv>

<drv mrk="lok.unu0a">
  <kap>unu<tld/>a</kap>
  <fnt><bib>KompLeks</bib></fnt>
  <snc mrk="lok.unu0a.operacio">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Kies <ref cel="lok.0nombro.argumenta">loknombro</ref> estas 1:
      <ekz>
        signum&scirc;an&gcirc;o estas unu<tld/>a operacio;
      </ekz>
      <ekz>
        <ctl>pozitiva</ctl> estas unu<tld/>a predikato.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>einstellig</trd>,
      <trd>einwertig</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>unary</trd>,
      <trd>monadic</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de un <ind>argumento</ind></trd>,
      <trd>unario</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&agrave; un <ind>argument</ind></trd>,
      <trd>unaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>un&aacute;ris</trd>,
      <trd>monadikus</trd>,
      <trd>egy operandus&uacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_n;&c_o;&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; <klr>(&c_o;&c_b;
      &c_o;&c_p;&c_je;&c_r;&c_a;&c_c;&c_i;&c_i;)</klr></trd>,
      <trd>&c_u;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="lok.0sidigxi">
  <kap><tld/>sidi&gcirc;i</kap>
  <snc>
    <dif>
      Restade lo&gcirc;i&gcirc;i.
    </dif>
    <trd lng="be">&c_a;&c_s;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="hu">
      <trd><ind>asentar</ind>se</trd>,
      <trd><ind>establecer</ind>se</trd>
    </trdgrp>
    <trd lng="en">be/remain fixed <klr>(in abode)</klr></trd>
    <trd lng="fr">se <ind>fixer</ind><klr> (pour loger)</klr></trd>
    <trdgrp lng="hu">
      <trd>megtelepszik</trd>,
      <trd>letelepedik</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_s;&c_je;&c_s;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="lok.0nombro">
  <kap><tld/>nombro</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc mrk="lok.0nombro.argumenta">
    <dif>
      Nombro indikanta kiom da argumentoj akceptas operacio,
      funkcio, rilato, predikato, proceduro ktp &#8208; a&ubreve; simbolo
      indikanta operacion ktp:
      <ekz>
        la operacisignoj pluso kaj minuso havas po du
        <tld/>nombrojn, ili povas esti unu<tld/>aj a&ubreve; du<tld/>aj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>arity</trd>,
      <trd>adicity</trd>
    </trdgrp>
    <trd lng="de">Stellenzahl</trd>
    <trdgrp lng="es">
      <trd>numero de argumentos<klr> (mat., inf.)</klr></trd>,
      <trd>rango</trd>
    </trdgrp>
    <trd lng="fr">nombre d'arguments<klr> (math., inf.)</klr></trd>
    <trdgrp lng="hu">
      <trd>arit&aacute;s</trd>,
      <trd>argumentumsz&aacute;m</trd>,
      <trd>operandussz&aacute;m</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr> (&c_o;&c_p;&c_je;&c_r;&c_a;&c_c;&c_i;&c_i; &c_i;
      &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_ch;&c_i;&c_s;&c_l;&c_o; &c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;&c_o;&c_v;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
