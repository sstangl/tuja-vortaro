<art mrk="$Id: kadmi.xml,v 1.27 2015/07/04 08:37:36 revo Exp $">
<kap>
   <rad>kadmi</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="kadmi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <mlg kod="IUPAC">Cd</mlg>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
       lst="voko:&hcirc;emiaj_elementoj" val="48">elemento</ref> kun
       atomnumero 48, atompezo 112,40;
       blanka <ref tip="super" cel="metal.0o">metalo</ref>,
       uzata por alojoj kaj pro siaj saloj.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_m;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_k;&c_a;&c_d;&c_m;&c_i;&c_j;</trd>
  <trd lng="br">kadmiom</trd>
  <trd lng="ca">cadmi</trd>
  <trd lng="cs">kadmium</trd>
  <trd lng="de">Cadmium</trd>
  <trd lng="en">cadmium</trd>
  <trd lng="es">cadmio</trd>
  <trd lng="fa">&f_k;&a_A;&a_d;&a_m;&f_y;&a_w;&a_m;</trd>
  <trd lng="fi">kadmium</trd>
  <trd lng="fr">cadmium</trd>
  <trd lng="he">&qof;&dalet;&mem;&yod;&vav;&fmem;</trd>
  <trd lng="hu">kadmium</trd>
  <trd lng="id">kadmium</trd>
  <trd lng="it">cadmio</trd>
  <trd lng="la">Cadmium</trd>
  <trd lng="nl">cadmium</trd>
  <trd lng="pl">kadm</trd>
  <trd lng="pt">c&aacute;dmio</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_m;&c_i;&c_j;</trd>
  <trd lng="sk">kadmium</trd>
  <trd lng="sv">kadmium</trd>
  <trd lng="tr">kadmiyum</trd>
</drv>
</art>
