<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gronland.xml,v 1.10 2014/11/05 13:10:25 revo Exp $">
<kap>
  <rad>Gronland</rad>/o
</kap>

<drv mrk="gronland.G0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gronland.G0o.POL">
    <uzo tip="fak">POL</uzo>
    <mlg>GL</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      en la samnoma insulo kun &ccirc;efurbo <ref tip="prt"
      cel="nuk.0o">Nuko</ref>,
      teritorio de A&ubreve;tonoma Teritorio Gronlando,
      dependa de <ref tip="vid" cel="dan.D0ujo">Danujo</ref>
    </dif>
    <trd lng="da">Gr&oslash;nland</trd>
    <trdgrp lng="pl">
      <trd>Grenlandia</trd>
    </trdgrp>
    <trd lng="sv">Gr&ouml;nland</trd>
  </snc>
  <snc mrk="gronland.G0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda <ref tip="lst" cel="insul.0o"
      lst="voko:insuloj">insulo</ref> de la mondo,
      trovi&gcirc;anta en la norda <ref tip="vid"
      cel="atlant.A0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="da">Gr&oslash;nland</trd>
    <trdgrp lng="pl">
      <trd>Grenlandia</trd>
    </trdgrp>
    <trd lng="sv">Gr&ouml;nland</trd>
  </snc>
  <trd
  lng="bg">&c_G;&c_r;&c_je;&c_n;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Grenl&agrave;ndia</trd>,
    <trd>Groenl&agrave;ndia</trd>
  </trdgrp>
  <trd lng="de">Gr&ouml;nland</trd>
  <trd lng="en">Greenland</trd>
  <trdgrp lng="fr">
  <trd>Groenland</trd>,
  <trd>Gro&euml;nland</trd>
</trdgrp>
  <trdgrp lng="id">
    <trd>Greenland</trd>,
    <trd><ind>Tanah</ind> Hijau</trd>
  </trdgrp>
  <trd lng="nl">Groenland</trd>
  <trd lng="no">Gr&oslash;nland</trd>
  <trd lng="pt">Gronel&acirc;ndia</trd>
</drv>

</art>

