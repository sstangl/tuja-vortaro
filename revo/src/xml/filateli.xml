<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: filateli.xml,v 1.6 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>filateli</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="filateli.0o">
  <kap><tld/>o</kap>
  <snc mrk="filateli.0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <ref tip="dif" cel="filate.0o.POSX">filatelo</ref>
    <ekz>
      la urbeto <klr>...</klr> enhavas muzeojn pri pulvaj armiloj, po&scirc;to
      kaj <tld/>o
      <fnt>
      <aut>Reinhard F&ouml;&szlig;meier</aut>,
      <vrk><url
      ref="http://www.forst.uni-muenchen.de/EXT/AIS/mapoj/rsmurb/klar.html">urbomapeto
      de San-Marino</url></vrk>,
      <lok>1996-10-15</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_t;&c_e;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_F;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_ja;</trd>
  <trd lng="cs">filatelie</trd>
  <trdgrp lng="de">
    <trd>Philatelie</trd>,
    <trd>Briefmarkenkunde</trd>,
    <trd>Briefmarkensammeln</trd>
  </trdgrp>
  <trd lng="es">filatelia</trd>
  <trd lng="fr">philat&eacute;lie</trd>
  <trd lng="hr">filatelija</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;lyeggy&udblac;jt&eacute;s</trd>,
    <trd>filat&eacute;lia</trd>
  </trdgrp>
  <trd lng="mk">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&#1112;&c_a;</trd>
  <trd lng="nl">filatelie</trd>
  <trd lng="pt">filatelia</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">filatelia</trd>
  <trd lng="sr">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&#1112;&c_a;</trd>
  <trd lng="uk">&c_f;&c_ib;&c_l;&c_a;&c_t;&c_je;&c_l;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="filateli.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="filateli.0isto.POSX">
    <ref tip="dif" cel="filate.0isto.POSX">filatelisto</ref>
    <ekz>
      komercisto kaj <tld/>isto
      <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Ren&eacute; Lemaire&rightquot;,
      2006-01-17</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_t;&c_e;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Philatelist</trd>,
    <trd>Briefmarkensammler</trd>
  </trdgrp>
  <trd lng="fr">philat&eacute;liste</trd>
  <trd lng="hr">filatelist</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;lyeggy&udblac;jt&odblac;</trd>,
    <trd>filat&eacute;lista</trd>
  </trdgrp>
  <trd lng="mk">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="nl">
    <trd>filatelist</trd>,
    <trd>postzegelverzamelaar</trd>
  </trdgrp>
  <trd lng="pt">filatelista</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sr">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="uk">&c_f;&c_ib;&c_l;&c_a;&c_t;&c_je;&c_l;&c_ib;&c_s;&c_t;</trd>
</drv>
</art>
