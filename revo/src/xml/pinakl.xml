<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pinakl.xml,v 1.12 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pinakl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pinakl.0o">
  <kap><tld/>o</kap>
  <snc mrk="pinakl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pinta&jcirc;o staranta sur <ref tip="malprt"
      cel="tegmen.0o">tegmento</ref>, arka&jcirc;o a&ubreve; apoga&jcirc;o,
      stabilige a&ubreve; orname:
      <ekz>
        &gcirc;i prezentis sin al li en formo de kastelo kun siaj kvar turoj
        kaj
        <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 2a, p.
        34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pinacle</trd>
  <trdgrp lng="cs">
    <trd>cimbuří</trd>,
    <trd>pinakl</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pinakel</trd>,
    <trd>Spitzs&auml;ule</trd>,
    <trd>Zinne</trd>
  </trdgrp>
  <trd lng="es">pin&aacute;culo</trd>
  <trd lng="fr">pinacle</trd>
  <trd lng="hu">fiatorony</trd>
  <trd lng="nl">pinakel</trd>
  <trdgrp lng="pl">
    <trd>pinakiel</trd>,
    <trd>fiala</trd>,
    <trd>sterczyna</trd>
  </trdgrp>
  <trd lng="pt">pin&aacute;culo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_n;&c_a;&c_k;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_sh;&c_je;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ozdoba oporných oblúkov striech</trd>
</drv>
</art>
