<vortaro>
<art mrk="$Id: protein.xml,v 1.10 2016/02/15 12:10:19 revo Exp $">
<kap>
  <rad>protein</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="protein.0o">
  <kap><tld/>o</kap>
  <snc mrk="protein.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Granda <ref tip="super" cel="moleku.0o.KEM">molekulo</ref>
      el <ref tip="prt" cel="acid.amin0o">aminacidoj</ref>,
      plenumanta esencan funkcion en vivula <ref tip="malprt"
      cel="cxel.0o.BIO">&ccirc;elo</ref>:
      <ekz>
        oni priskribas la kvaliton de <tld/>o per unuo nomita 
        <klr>...</klr> neta <tld/>a utileco
        <fnt>
          <aut>JesCed</aut>,
          <vrk><url
          ref="http://eo.lernu.net/komunikado/forumo/temo.php?t=1014">
          &Ccirc;u vi estas vegetarano...</url></vrk>,
          <lok>lernu!, 2007-07-18</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jednoduch&aacute; b&iacute;lkovina</trd>,
    <trd>protein</trd>
  </trdgrp>
  <trd lng="de">Protein</trd>
  <trd lng="en">protein</trd>
  <trd lng="es">prote&iacute;na</trd>
  <trd lng="fa">&f_p;&a_r;&a_w;&a_t;&a_y_hamza;&f_y;&a_n;</trd>
  <trd lng="fr">prot&eacute;ine</trd>
  <trd lng="hu">feh&eacute;rje</trd>
  <trd lng="nl">prote&iuml;ne</trd>
  <trdgrp lng="pl">
    <trd>proteina</trd>,
    <trd>bia&lstroke;ko</trd>
  </trdgrp>
  <trd lng="pt">prote&iacute;na</trd>
  <trdgrp lng="sk">
    <trd>druh bielkoviny</trd>,
    <trd>prote&iacute;n</trd>
  </trdgrp>
  <trd lng="sv">protein</trd>
</drv>
</art>

