<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: megavat.xml,v 1.10 2016/04/12 06:10:14 revo Exp $">
<kap>
  <rad>megavat</rad>/o
</kap>
<drv mrk="megavat.0o">
  <kap><tld/>o</kap>
  <snc mrk="megavat.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="unu.mezur0o.SCI">Mezurunuo</ref> de 
      <ref cel="pov.0o.FIZ">povo</ref>, egala al miliono da <ref 
      tip="prt" cel="vat1.v0o.FIZ">vatoj</ref>; simbole MW. 
      <ekz>
        en Ignalina funkcias unu reaktoro kun potenco de 1350 <tld/>oj
        <fnt>
          <aut>Last</aut>,
          <vrk>Nuklea energio restu en Litovio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2007a, numero 1a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>megawatt</trd>,
    <trd>MW</trd>
  </trdgrp>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_v;&c_a;&c_t;</trd>
  <trd lng="cs">megawatt</trd>
  <trd lng="de">Megawatt</trd>
  <trd lng="en">megawatt</trd>
  <trd lng="es">megavatio</trd>
  <trd lng="fr">m&eacute;gawatt</trd>
  <trd lng="it">megawatt</trd>
  <trd lng="pl">megawat</trd>
  <trd lng="sk">megawatt</trd>
</drv>
</art>

