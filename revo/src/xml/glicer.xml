<art mrk="$Id: glicer.xml,v 1.15 2015/10/14 08:10:18 revo Exp $">
<kap>
  <rad>gliceri</rad>/o
</kap>
<drv mrk="glicer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Glyceria</trd>).
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Glyceria</trd>
  <trdgrp lng="de">
     <trd>Schwaden</trd>,
     <trd>Mannagras</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Glyceria</trd>,
    <trd>mannagrass</trd>, <trd>sweetgrass</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_y;&a_r;&f_y;&a_n;&zwnj;&f_ch;&a_m;&a_n;</trd>
  <trd lng="nl">liesgras</trd>
  <trd lng="pl">manna</trd>
  <trd lng="pt">glic&eacute;ria</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
