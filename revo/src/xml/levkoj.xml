<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: levkoj.xml,v 1.22 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>levkoj</rad>/o
</kap>
<drv mrk="levkoj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="levkoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Matthiola</trd>), 
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herbo</ref>
      a&ubreve; duonarbusto kun bonodoraj, blankaj, 
      violet- a&ubreve; purpuretkoloraj floroj:
      <ekz>
        graciaj, delikataj <tld/>oj, blankaj kiel la unua ne&gcirc;o
        <fnt>
        <aut>Ivan G. &Scirc;irjaev</aut>,
        <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_w;&c_k;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">violer</trd>
  <trdgrp lng="cs">
    <trd>fiala letní</trd>,
    <trd>levkoje</trd>
  </trdgrp>
  <trd lng="de">Levkoje</trd>
  <trdgrp lng="en">
    <trd>wallflower</trd>,
    <trd>gilliflower</trd>,
    <trd>stock</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_l;&f_y;&f_p;&a_A;</trd>
  <trd lng="fr">girofl&eacute;e des jardins</trd>
  <trd lng="hu">viola</trd>
  <trd lng="nl">violier</trd>
  <trd lng="pl">lewkonia</trd>
  <trdgrp lng="pt">
    <trd>goiveiro</trd>,
    <trd>goivo</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_k;&c_o;&c_j;</trd>
  <trd lng="sk">fiala nočná</trd>
</drv>
</art>
