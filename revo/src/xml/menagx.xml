    <dif>
      Tia kolekto da bestoj.
    </dif>
    <trdgrp lng="en">
      <trd>zoo</trd>,
      <trd>menagerie</trd>
    </trdgrp>
    <trd lng="hu">&aacute;llatsereglet</trd>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_a;&c_p;&c_a;&c_r;&c_k;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_y;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>Tierschau</trd>,
      <trd>Menagerie</trd>
    </trdgrp>
  <trd lng="nl">menagerie</trd>
  <trd lng="pt">cole&ccedil;&atilde;o de <ind>animais</ind></trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_r;&c_i;&c_n;&c_je;&c_c;</trd>
</drv>
