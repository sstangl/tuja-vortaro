<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: selen.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>selen</rad>/o
</kap>
<drv mrk="selen.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Se</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="34">elemento</ref> kun
      atomnumero 34, atompezo 78,96;
      <ref tip="super" cel="metalo.0o">metaloido</ref>:
      <ekz>
        oni el&ccirc;erpas anka&ubreve; rarajn metalojn, interalie: 
        <ref tip="vid" cel="indi.0o">indion</ref>, 
        <ref tip="vid" cel="german.0o">germanion</ref>,
        <ref tip="vid" cel="galium.0o">galion</ref>, <tld/>on
        <fnt><aut>Bardhyl Mezini</aut>: <vrk>La vera ka&ubreve;zo de la
        milito</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 5a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_l;&c_je;&c_n;</trd>
  <trd lng="bg">&c_s;&c_je;&c_l;&c_je;&c_n;</trd>
  <trd lng="br">seleniom</trd>
  <trd lng="ca">seleni</trd>
  <trdgrp lng="cs">
    <trd>selen</trd>,
    <trd>selenium</trd>
  </trdgrp>
  <trd lng="de">Selen</trd>
  <trd lng="en">selenium</trd>
  <trd lng="es">selenio</trd>
  <trd lng="fa">&a_s;&a_l;&a_n;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">s&eacute;l&eacute;nium</trd>
  <trd lng="hu">szel&eacute;n</trd>
  <trd lng="id">selenium</trd>
  <trd lng="it">selenio</trd>
  <trd lng="la">Selenium</trd>
  <trd lng="nl">selenium</trd>
  <trd lng="pl">selen</trd>
  <trd lng="pt">sel&eacute;nio</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_je;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>Se</trd>,
    <trd>selén</trd>,
    <trd>selénium</trd>
  </trdgrp>
  <trd lng="sv">selen</trd>
</drv>
</art>
