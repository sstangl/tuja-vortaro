<art mrk="$Id: diaton.xml,v 1.14 2010/11/27 17:30:24 revo Exp $">
  <kap>
    <rad>diaton</rad>/a <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="diaton.0a">
    <kap><tld/>a</kap>
    <snc mrk="diaton.0a.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Enhavanta tonojn kaj duontonojn:
        <ekz>
          la muzikilo estas <tld/>a kaj disponas pri
          apena&ubreve; 30 kordoj
          <fnt>
            <aut>S. Rodin</aut>,
            <vrk>Bildoj pri Norda Lando</vrk>,
            <lok>2006</lok>
          </fnt>.
        </ekz>  
      </dif>
    </snc>
    <trd lng="be">&c_d;&c_y;&c_ja;&c_t;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
    <trd lng="ca">diat&ograve;nic</trd>
    <trd lng="de">diatonisch</trd>
    <trd lng="es">diat&oacute;nico</trd>
    <trd lng="fa">&a_d;&f_y;&a_A;&a_t;&a_w;&a_n;&f_y;&f_k;</trd>
    <trd lng="fr">diatonique</trd>
    <trd lng="hu">diatonikus</trd>
    <trd lng="it">diatonico</trd>
    <trd lng="pt">diat&ocirc;nico</trd>
    <trd lng="pl">diatoniczny</trd>
    <trd lng="ru">&c_d;&c_i;&c_a;&c_t;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </drv>
</art>
