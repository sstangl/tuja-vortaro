<vortaro>
  <art mrk="$Id: hieronim.xml,v 1.10 2015/02/26 19:12:19 revo Exp $">
    <kap>
      <rad>Hieronim</rad>/o
    </kap>
    <drv mrk="hieronim.H0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
 <dif>
   Grekdevena vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
   <ekz>
     <uzo tip="fak">PERS</uzo>
     <ref cel="vulgat.0o">Vulgaton</ref>
     plejparte tradukis s-ta
     <ind><tld/>o el Stridono</ind> (m.420).
            <trdgrp lng="de">
               <trd>Hl. Hieronymus zu Stridon</trd>,
               <trd>Hieronymus Sophronius Eusebius</trd>
            </trdgrp>
     <trdgrp lng="it">
       <trd>Sofronio Esusebio <ind>Girolamo</ind></trd>, 
       <trd>San <ind>Gerolamo</ind><klr> (padre della
       Chiesa)</klr></trd>, 
       <trd>San <ind>Girolamo</ind><klr> (padre della
       Chiesa)</klr></trd>, 
       <trd>San <ind>Geronimo</ind><klr> (padre della
       Chiesa)</klr></trd> 
      </trdgrp>
     <trd lng="ru">&c_b;&c_l;. <ind>&c_I;&c_je;&c_r;&c_o;&c_n;&c_i;&c_m;
     &c_S;&c_t;&c_r;&c_i;&c_d;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;</ind></trd>
   </ekz>
 </dif>
      </snc>                           
      <trd lng="br">Yerom</trd>
      <trd lng="ca">Jeroni</trd>
      <trd lng="de">Hieronymus</trd>
      <trd lng="en">Jerome</trd>
      <trd lng="es">Jer&oacute;nimo</trd>
      <trd lng="fr">J&eacute;r&ocirc;me</trd>
      <trd lng="hu">Jeromos</trd>
      <trdgrp lng="it">
       <trd>Geronimo</trd>, 
       <trd>Gerolamo</trd>, 
       <trd>Girolamo</trd>
      </trdgrp>
      <trd lng="la">Hieronymus</trd>
      <trd lng="nl">Jeroen</trd>
      <trd lng="pt">Jer&ocirc;nimo</trd>
      <trd lng="ru">&c_I;&c_je;&c_r;&c_o;&c_n;&c_i;&c_m;</trd>
    </drv>

  </art>

