<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ibis.xml,v 1.20 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ibis</rad>/o
</kap>

<drv mrk="ibis.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="ibised.0oj">
      <tld/>edoj</ref> (<trd lng="la">Threskiornis</trd>),
      kun longaj kruroj kaj longa kurba beko, man&gcirc;anta serpentojn
      kaj aliajn rampulojn, pro kio &gcirc;i estis rigardata kiel sankta
      de la antikvaj egiptoj.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_b;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_i;&c_b;&c_i;&c_s;</trd>
  <trd lng="cs">ibis</trd>
  <trd lng="de">Ibis</trd>
  <trd lng="en">ibis</trd>
  <trd lng="fr">ibis</trd>
  <trd lng="hu">&iacute;bisz</trd>
  <trd lng="nl">ibis</trd>
  <trd lng="pl">ibis</trd>
  <trd lng="pt">&iacute;bis</trd>
  <trd lng="ru">&c_i;&c_b;&c_i;&c_s;</trd>
  <trd lng="sk">ibis</trd>
</drv>

</art>
