<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karon.xml,v 1.6 2015/07/04 08:37:37 revo Exp $">
<kap>
<rad>Karon</rad>/o
</kap>
<drv mrk="karon.0o">
  <kap><tld/>o</kap>
  <snc mrk="karon.0o.MIT">
<uzo tip="fak">MIT</uzo>
<uzo tip="fak">BELE</uzo>
<ref tip="dif" cel="hxaron.0o.MIT">&Hcirc;arono</ref>
  </snc>

<snc mrk="karon.0o.AST">
<uzo tip="fak">AST</uzo>
<ref tip="dif" cel="hxaron.0o.AST">&Hcirc;arono</ref>
  </snc>
  <trd lng="ca">Caront</trd>
  <trdgrp lng="cs">
    <trd>Charon</trd>,
    <trd>Charón</trd>
  </trdgrp>
  <trd lng="it">Caronte</trd>
  <trd lng="nl">Charon</trd>
  <trd lng="sk">Cháron</trd>
</drv>
</art>

