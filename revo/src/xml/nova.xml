<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nova.xml,v 1.5 2015/06/29 16:33:21 revo Exp $">
<kap>
  <rad>nova</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="nova.0o">
  <kap><tld/>o</kap>
  <snc mrk="nova.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="stel.0o.AST">Stelo</ref> abrupte aperanta,
      brilega dum kelkaj tagoj kaj poste malheli&gcirc;anta:
      <ekz>
        masegaj steloj kaj <tld/>oj
        <fnt>
        <aut>Helio J. Rocha-Pinto</aut>,
        <vrk>La Kemia Evoluado de la Galaksio</vrk>,
        <lok><url
        ref="http://esperanto.org/AEK/biblioteko.html">Virtuala
        Biblioteko de AEKo</url>, 2000-01-21</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nova</trd>
  <trd lng="de">Nova</trd>
  <trd lng="en">nova</trd>
  <trd lng="fr">nova</trd>
  <trd lng="hu">n&oacute;va</trd>
  <trd lng="it">nova</trd>
  <trd lng="nl">nova</trd>
  <trd lng="pt">nova</trd>
  <trd lng="ru">&c_n;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sk">nova</trd>
</drv>

<drv mrk="nova.super0o">
  <kap>super<tld/>o</kap>
  <snc mrk="nova.super0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="stel.0o.AST">Stelo</ref>, kiu eksplodante
      brilegas e&ccirc; pli forte ol <tld/>o, kaj poste finofare kolapsas:
      <ekz>
        la fama nebulozo en la konstelacio Krabo estis kreita en la
        super<tld/>o spektita de la &ccirc;inoj en la jaro 1054
        <fnt>
        <aut>Amri Wandel</aut>,
        <vrk>La moderna astrofiziko</vrk>,
        <lok><url
        ref="http://esperanto.org/AEK/biblioteko.html">Virtuala
        Biblioteko de AEKo</url>, [vidita je 2006-01-24]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">supernova</trd>
  <trd lng="de">Supernova</trd>
  <trd lng="en">supernova</trd>
  <trd lng="fr">supernova</trd>
  <trd lng="hu">szupern&oacute;va</trd>
  <trd lng="it">supernova</trd>
  <trd lng="nl">supernova</trd>
  <trd lng="pt">supernova</trd>
  <trd lng="sk">supernova</trd>
</drv>
</art>

