<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: izocel.xml,v 1.12 2015/07/04 08:37:36 revo Exp $">
  <kap>
    <rad>izocel</rad>/a
  </kap>
<drv mrk="izocel.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="angul.tri0o">triangulo</ref>)</klr>
      Havanta du <ref cel="egal.0a.izometria">egalajn</ref> laterojn:
      <ekz>
        <tld/>a triangulo havas du egalajn angulojn;
      </ekz>
      <ekz>
        <tld/>an triangulon oni anka&ubreve; nomas 
        <ref cel="simetr.0a.figuro">simetria</ref>, &ccirc;ar la 
        <ref cel="ort.mez0anto">mezortanto</ref> de 
        &gcirc;ia tria latero estas simetriakso.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_w;&c_n;&c_a;&c_b;&c_a;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="cs">rovnoramenný</trd>
  <trd lng="de">gleichschenklig</trd>
  <trd lng="en">isosceles</trd>
  <trd lng="es">is&oacute;sceles</trd>
  <trd lng="fr"><klr tip="ind">(triangle) </klr>isoc&egrave;le</trd>
  <trd lng="hu">egyenl&odblac; sz&aacute;r&uacute; <klr tip="ind"> (h&aacute;romsz&ouml;g)</klr></trd>
  <trd lng="nl">gelijkbenig</trd>
  <trd lng="pl"><klr tip="ind">(tr&oacute;jk&aogonek;t) </klr>r&oacute;wnoramienny</trd>
  <trdgrp lng="pt">
   <trd>is&oacute;scele<klr tip="amb"> (matem&aacute;tica)</klr></trd>, 
   <trd>is&oacute;sceles<klr tip="amb"> (matem&aacute;tica)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_b;&c_je;&c_d;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">rovnoramenný <klr>(geom)</klr></trd>
</drv>
</art>

