<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dizert.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>dizert</rad>/i
</kap>
<drv mrk="dizert.0i">
  <kap><tld/>i</kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fu&gcirc;i el <ref cel="serv.milit0o">militservo</ref> kun la
      intenco 
      ne plu reveni al &gcirc;i.
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="desert.0o">deserto</ref>,
      <ref cel="dezert.0o">dezerto</ref>,
      <ref cel="disert.0i">diserti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_z;&c_e;&c_r;&c_t;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_z;&c_je;&c_r;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>desertovat</trd>,
    <trd>dezertovat</trd>,
    <trd>uprchnout</trd>,
    <trd>utéci</trd>,
    <trd>zběhnout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>desertieren</trd>,
    <trd>fahnenfl&uuml;chtig werden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>desert</trd>,
    <trd>run away from</trd>
  </trdgrp>
  <trd lng="es">desertar</trd>
  <trd lng="fr">d&eacute;serter</trd>
  <trd lng="it">disertare</trd>
  <trd lng="nl">deserteren</trd>
  <trd lng="pl">dezerterowa&cacute;</trd>
  <trd lng="pt">desertar</trd>
  <trd lng="ru">&c_d;&c_je;&c_z;&c_je;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dezertova&tcaron;</trd>,
    <trd>zbehnúť</trd>
  </trdgrp>
</drv>
</art>
