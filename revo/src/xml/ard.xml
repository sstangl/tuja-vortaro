<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ard.xml,v 1.22 2015/07/04 08:37:30 revo Exp $">
    <kap>
      <ofc>3</ofc>
      <rad>ard</rad>/i
    </kap>

    <drv mrk="ard.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="ard.0i.brulegi">
        <dif>
          Esti blanke varmega, brulegi:
          <ekz>
            fajro, fero <tld/>as.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="inkand.0i">inkandeski</ref>,
          <ref cel="flam.0i">flami</ref>.
        </refgrp>
      </snc>
      <snc mrk="ard.0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Esti tre pasia, tre fervora, ekstreme viveca:
          <ekz>
            koro, okuloj <tld/>as;
          </ekz><ekz>
            febra volo <tld/>as en liaj vejnoj;
          </ekz><ekz>
            interna kolero, &gcirc;ojo <tld/>is sur liaj vangoj.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="bol.0i">boli</ref>,
          <ref cel="febr.0i.anime">febri</ref>,
          <ref cel="fervor.0i">fervori</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>planout</trd>,
    <trd>žhnout</trd>
  </trdgrp>
  <trd lng="de">gl&uuml;hen</trd>
  <trd lng="en">be <ind>ardent</ind></trd>
  <trd lng="es">arder</trd>
  <trdgrp lng="fr">
        <trd>&ecirc;tre <ind>ardent</ind></trd>,
        <trd>&ecirc;tre <ind>incandescent</ind></trd>
      </trdgrp>
  <trd lng="hu">izzik</trd>
  <trd lng="ru">&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>&zcaron;iari&tcaron;</trd>,
    <trd>planúť</trd>
  </trdgrp>
  <trd lng="sv">gl&ouml;da</trd>
</drv>

    <drv mrk="ard.0a">
      <kap><tld/>a</kap>
      <snc mrk="ard.0a.varmega">
        <dif>
          <ref tip="sin" cel="varm.0ega.fizike">Varmega<sncref/></ref>,
          brulanta:
          <ekz>
            <tld/>a fajro, suno, karbeto.
          </ekz>
        </dif>
        <ref tip="vid" cel="inkand.0a">inkandeska.</ref>
        <trdgrp lng="be">
          <trd>&c_p;&c_a;&c_l;&c_k;&c_ib;</trd>,
          <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_y;&c_l;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
          <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc mrk="ard.0a.fervora">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Pasia, fervora; tre viveca:
          <ekz>
            <tld/>a &ccirc;evalo, atako, intereso, atento, deziro;
          </ekz><ekz>
            <tld/>aj paroloj;
          </ekz><ekz>
            homo <tld/>a en la diskutado.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="flam.0a.anime">flama</ref>,
          <ref cel="varm.0ega.anime">varmega</ref>.
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_y;</trd>,
          <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_y;&c_l;&c_k;&c_i;&c_j;</trd>,
          <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>horoucí</trd>,
    <trd>vroucí</trd>,
    <trd>řeřavý</trd>,
    <trd>žárný</trd>
  </trdgrp>
  <trd lng="de">gl&uuml;hend</trd>
  <trd lng="en">ardent</trd>
  <trd lng="es">ardiente</trd>
  <trdgrp lng="fr">
        <trd>ardent</trd>,
        <trd>chaud<klr tip="ind"> (de temp&eacute;rament)</klr></trd>,
        <trd>chauff&eacute; &agrave; blanc</trd>,
        <trd>incandescent</trd>,
        <trd>passionn&eacute;</trd>,
        <trd>torride</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>izz&oacute;</trd>,
        <trd>par&aacute;zs<klr tip="amb"> (mell&eacute;kn&eacute;v)</klr></trd>
        <trd>par&aacute;zsl&oacute;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;erav&yacute;</trd>,
    <trd>rozpálený</trd>
  </trdgrp>
  <trd lng="sv">gl&ouml;dande</trd>
</drv>

    <drv mrk="ard.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Io <tld/>anta.
        </dif>
      </snc>
      <trd lng="de">Glut</trd>
      <trdgrp lng="hu">
        <trd>par&aacute;zs</trd>,
        <trd>zsar&aacute;tnok</trd>
      </trdgrp>
      <trd lng="sv">gl&ouml;d</trd>
      <trd lng="es">brasa</trd>
    </drv>

    <drv mrk="ard.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Stato de tio, kio <tld/>as:
          <ekz>
            Moskvo estis redonanta la <tld/>on akumulitan en la
            asfalto dum la tago
            <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p.
            48a</lok></fnt>;</ekz>
        </dif>
        <ref tip="vid" cel="inkand.0o">inkandesko;</ref>
        <refgrp tip="vid">
          <ref cel="entuzi.0o">entuziasmo</ref>,
          <ref cel="ekzalt">ekzalteco</ref>,
          <ref cel="fajr.0o.fervoro">fajro</ref>,
          <ref cel="fervor.0o">fervoro</ref>,
          <ref cel="flam.0o.fervoro">flamo</ref>,
          <ref cel="pasi.0o">pasio</ref>,
          <ref cel="verv.0o">vervo</ref>,
          <ref cel="varm.0o">varmo</ref>,
          <ref cel="vigl">vigleco</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
        <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
        <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>výheň</trd>,
    <trd>žeh</trd>,
    <trd>žár</trd>
  </trdgrp>
  <trd lng="de">Gl&uuml;hen</trd>
  <trd lng="en">ardor</trd>
  <trd lng="es">ardor</trd>
  <trdgrp lng="fr">
        <trd>ardeur</trd>,
        <trd>incandescence</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_p;&c_y;&c_l;</trd>,
        <trd>&c_zh;&c_a;&c_r;</trd>,
        <trd>&c_n;&c_a;&c_k;&c_a;&c_l;</trd>,
        <trd>&c_r;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>vrelosť <klr>(pren.)</klr></trd>,
    <trd>vrúcnosť</trd>,
    <trd>žeravosť</trd>
  </trdgrp>
</drv>

    <drv mrk="ard.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="stl">ARK</uzo>
      <snc>
        <dif>
          <tld lit="A"/>anta fajrejo:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>ejo de agitado.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="fokus.0o">fokuso</ref>,
          <ref cel="centr.0o">centro</ref>,
          <ref cel="kern.0o">kerno</ref>,
          <ref cel="brul">brulejo</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>ohnisko</trd>,
    <trd>žároviště</trd>
  </trdgrp>
  <trd lng="en">kiln</trd>
  <trd lng="sk">ohnisko</trd>
</drv>

    <drv mrk="ard.0ilo">
      <kap><tld/>ilo</kap>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <ref tip="dif" cel="gas.0mufo">gasmufo.</ref>
      </snc>
      <trd lng="de">Z&uuml;nder</trd>
      <trd lng="en">kiln</trd>
      <trd lng="fr">manchon<klr tip="ind"> (de flamme)</klr></trd>
      <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_t;&c_je;&c_h;.)</klr></trd>
    </drv>

    <drv mrk="ard.blank0a">
      <kap>blank<tld/>a</kap>
      <snc>
        <dif>
          Blanka pro <tld/>o.
        </dif>
      </snc>
  <trdgrp lng="cs">
    <trd>žhavý</trd>,
    <trd>žhoucí</trd>
  </trdgrp>
  <trd lng="de">wei&szlig;gl&uuml;hend</trd>
  <trd lng="en">ashen</trd>
  <trd lng="es">(ardiendo) al blanco</trd>
  <trd lng="fr">chauff&eacute; &agrave; blanc</trd>
  <trd lng="hu">feh&eacute;ren <ind>izz&oacute;</ind></trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;
&c_d;&c_o;&c_b;&c_je;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>žeravý</trd>,
    <trd>žiariaci</trd>
  </trdgrp>
</drv>

    <drv mrk="ard.rugx0a">
      <kap>ru&gcirc;<tld/>a</kap>
      <snc>
        <dif>
          Ru&gcirc;e <tld/>anta.
        </dif>
      </snc>
      <trd lng="es">al rojo vivo</trd>
    </drv>

    <drv mrk="ard.karb0ajxo">
      <kap>karb<tld/>a&jcirc;o</kap>
      <uzo tip="stl">ARK</uzo>
      <fnt><bib>PV</bib></fnt>
      <snc>
        <ref tip="dif" cel="bragx.0o">bra&gcirc;o.</ref>
      </snc>
      <trd lng="en">charcoal</trd>
      <trd lng="de">Kohlenglut</trd>
      <trd lng="hu">sz&eacute;npar&aacute;zs</trd>
    </drv>
  </art>
