<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anser.xml,v 1.33 2015/09/11 02:10:21 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>anser</rad>/o
</kap>
<drv mrk="anser.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anser.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genroj</ref> el la familio
      <ref tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Anser</trd>, <trd lng="la">Branta</trd> k.a.):
      <ekz>
        celante <tld/>on li trafis aeron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_u;&c_s;&c_mol;</trd>
    <trd lng="br">gwaz<klr tip="ind"> (evn)</klr></trd>
    <trd lng="cs">husa</trd>
    <trd lng="de">Gans</trd>
    <trd lng="en">goose</trd>
    <trd lng="fr">oie</trd>
    <trd lng="he">&alef;&vav;&vav;&zayin;</trd>
    <trd lng="hu">l&uacute;d</trd>
    <trd lng="id">angsa</trd>
    <trd lng="nl">gans</trd>
    <trd lng="no">g&aring;s</trd>
    <trd lng="pl">g&eogonek;&sacute;</trd>
    <trd lng="pt">ganso</trd>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_mol;</trd>
  </snc>
  <snc mrk="anser.0o.hejmbesto">
    <uzo tip="fak">AGR</uzo>
    <dif>
       <ref tip="lst" cel="bird.korto0o"
       lst="voko:birdoj">Kortobirdo</ref>,
        deveninta de la specio <trd lng="la">Anser anser</trd>.
    </dif>
    <bld lok="../bld/anser.jpg">korta <tld/>o</bld>
    <trd lng="de">Gans</trd>
    <trd lng="en">goose</trd>
    <trd lng="he">&alef;&vav;&vav;&zayin; &alef;&pe;&vav;&resh;</trd>
    <trd lng="hu">liba</trd>
    <trd lng="id">angsa</trd>
    <trd lng="pl">g&eogonek;&sacute;</trd>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_mol;</trd>
  </snc>
  <snc mrk="anser.0o.malsagxulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malsa&gcirc;a homo.
    </dif>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_a;&c_k;</trd>
  </snc>
  <trd lng="de">Gans (dumme)</trd>
  <trd lng="es">ganso</trd>
  <trd lng="hu">liba</trd>
  <trd lng="it">oca</trd>
  <trd lng="sk">hus</trd>
</drv>

<drv mrk="anser.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Anseriformes</trd>).
    </dif>
  </snc>
  <trd lng="de">G&auml;nsev&ouml;gel</trd>
  <trd lng="fr">ans&eacute;riformes</trd>
  <trd lng="hu">l&uacute;dalak&uacute;ak</trd>
  <trd lng="it">anseriformi</trd>
  <trd lng="ru">&c_g;&c_u;&c_s;&c_i;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
