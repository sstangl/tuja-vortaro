<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kler.xml,v 1.25 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kler</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kler.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kler.0a.PED">
    <dif>
      Kapabla pro instruiteco kaj kulturo ju&gcirc;i klare kaj rekte:
      <ekz>
        akademie <tld/>a homo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>osvícený</trd>,
    <trd>vzdělaný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gebildet</trd>,
    <trd>gelehrt</trd>,
    <trd>aufgekl&auml;rt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>educated</trd>,
    <trd>cultured</trd>,
    <trd>enlightened</trd>,
    <trd>learned</trd>,
    <trd>well <ind>informed</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiv&eacute;<klr tip="ind"> (ayant de la culture)</klr></trd>,
    <trd>&eacute;clair&eacute;<klr tip="ind"> (cultiv&eacute;)</klr></trd>,
    <trd>instruit<klr tip="ind"> (cultiv&eacute;)</klr></trd>,
    <trd>lettr&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>savant<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;velt</trd>,
    <trd>k&eacute;pzett</trd>,
    <trd>kultur&aacute;lt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontwikkeld</trd>,
    <trd>geleerd</trd>,
    <trd>knap</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>osvietený</trd>,
    <trd>vzdelaný</trd>
  </trdgrp>
</drv>

<drv mrk="kler.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kler.0eco.PED">
    <dif>
      Kompetento pri kultura temo:
      <ekz>
        observi la &gcirc;ustan mezuron estas arto,
        kiu montras altan <tld/>econ<fnt>Z</fnt>;
      </ekz>
      <ekz>
        artisma <tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        artista <tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        muzika <tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun fini&gcirc;as mia <tld/>eco
        <klr>(<ref tip="vid" cel="fin.0o">fino</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sciencon oni ne mendas, <tld/>econ oni ne vendas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kompet.0o">kompetento</ref>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>. vzdělání</trd>,
    <trd>osvícenost</trd>,
    <trd>osvěta</trd>,
    <trd>vzdělanost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bagage<klr tip="ind"> (culturel)</klr></trd>,
    <trd>culture<klr tip="ind"> (personnelle)</klr></trd>,
    <trd>instruction<klr tip="ind"> (fait d'&ecirc;tre instruit)</klr></trd>
  </trdgrp>
  <trd lng="hu">zenei m&udblac;velts&eacute;g</trd>
  <trdgrp lng="nl">
    <trd>ontwikkeling</trd>,
    <trd>scholing</trd>,
    <trd>kennis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_jo;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>osvietenosť</trd>,
    <trd>vzdelanosť</trd>
  </trdgrp>
</drv>

<drv mrk="kler.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kler.0igi.PED">
    <dif>
      <ref tip="super" cel="instru.0i">Instrui</ref> iun pri kulture
      utilaj temoj:
      <ekz>
        la <tld/>igantoj de la popolo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igu min, por ke mi vivu
        <fnt>
        <bib>MT</bib>,
        <lok>&Psa; 119:144</lok>
        </fnt>;
      </ekz>
      <ekz>
        talento ne sole natura, sed anka&ubreve; <tld/>igita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ek<tld/>igi iun pri matematiko;
      </ekz>
      <ekz>
        ministro pri publika <ind><tld/>igado</ind>.
        <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
        <trdgrp lng="fr">
          <trd>instruction<klr tip="ind"> (fait
          d'instruire)</klr></trd>,
          <trd>&eacute;ducation</trd>,
          <trd>enseignement</trd>
        </trdgrp>
        <trd lng="nl">onderwijs</trd>
        <trd lng="hu">k&ouml;zm&udblac;vel&odblac;d&eacute;si miniszter</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>vzdělávat</trd>,
    <trd>šířit osvětu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bilden</trd>,
    <trd>ausbilden</trd>,
    <trd>belehren</trd>,
    <trd>aufkl&auml;ren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>educate</trd>,
    <trd>enlighten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiver<klr tip="ind"> (r&eacute;pandre la culture)</klr></trd>,
    <trd>&eacute;duquer</trd>,
    <trd>instruire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;vel</trd>,
    <trd>k&eacute;pez</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>scholen</trd>,
    <trd>onderwijzen</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>poučovať</trd>,
    <trd>vzdelávať</trd>
  </trdgrp>
</drv>

<drv mrk="kler.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kler.0ismo.HIS">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Idea movado en la 18a jarcento en E&ubreve;ropo kaj Nord-Ameriko,
      apelacianta al la racio por reformi la
      tradiciajn ideojn kaj instituciojn:
      <ekz>
        foje kristanoj estis allogitaj de la doktrinoj kontra&ubreve; kiuj ili
        estis batalantaj: oni pripensu <klr>(...)</klr> certan
        <tld/>ismon kiu emas igi la naturon io absoluta
        <fnt>
        <lok><url ref="http://www.arpnet.it/~esper/bult54.htm">
        Naturo kristanismo ekologismo, 54</url>.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="raci.0ismo">raciismo</ref>
  <trdgrp lng="en">
    <trd>Enlightenment</trd>,
    <trd>Age of Reason</trd>
  </trdgrp>
  <trd lng="fr">les <ind>Lumi&egrave;res</ind><klr tip="ind">
  (phil.)</klr></trd>
  <trd lng="hu">felvil&aacute;gosod&aacute;s</trd>
  <trd lng="nl">verlichting<klr tip="ind"> (geschiedk.
  stroming)</klr></trd>
  <trd lng="ru">&c_P;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">osvietenstvo</trd>
  <trd lng="cs">osvícenství</trd>
</drv>

<drv mrk="kler.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="kler.0ulo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <tld lit="K"/>a homo.
    </dif>
    <refgrp tip="vid">
      <ref cel="erudic.0ulo">erudiciulo</ref>,
      <ref cel="instru.0itulo">instruitulo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trd lng="cs">vzdělanec</trd>
  <trd lng="de">Gebildeter</trd>
  <trdgrp lng="fr">
    <trd>clerc<klr tip="ind"> (personne cultiv&eacute;e)</klr></trd>,
    <trd>lettr&eacute;<klr tip="ind"> (subst.)</klr></trd>,
    <trd>savant<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;velt ember</trd>,
    <trd>k&eacute;pzett ember</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geletterde</trd>,
    <trd><ind>belezen</ind> iemand</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">vzdelanec</trd>
</drv>

<drv mrk="kler.0ularo">
  <kap><tld/>ularo, <var><kap><tld/>a mondo</kap></var></kap>
  <snc mrk="kler.0ularo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      La instruituloj, rigardataj kiel socia grupo:
      <ekz>
        la vortaron mi kreis ne arbitre, sed kiom eble el vortoj konataj
        
        al la tuta <tld/>a mondo
        <fnt><bib>FK</bib>, <lok>El la unua libro...</lok></fnt>;
      </ekz>
      <ekz>
        li intencis plej frue prezenti la proponon al E&ubreve;ropa <tld/>ularo
        
        por prikonsidero
        <fnt>
          <aut>J. Nov&aacute;k, trad. Rondeto de esperantistinoj</aut>, 
          <vrk><url
          ref="http://www.gutenberg.org/files/21951/21951-0.txt">
          Jan Amos Komensk&yacute;</url></vrk>,
          <lok>1921</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>intelligentsia</trd>,
    <trd>monde savant</trd>
  </trdgrp>
</drv>

<drv mrk="kler.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="kler.mal0a.ulo">
    <dif>
       <klr>(iu)</klr>
       Kiu malhavas <tld/>on:
      <ekz>
        Osip parolas ne per stilo tute logika kaj literatura, sed
        per stilo de mal<tld/>a rusa servisto
        <fnt><bib>LR</bib>, <lok>Respondo 1, La Revuo, 1906,
        Decembro</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a kritiko la&ubreve;das mal<tld/>ajn verkistojn, tiuj,
        siavice, provizas la kritikon per samnivela lega&jcirc;o
        <fnt>
        <aut>J. Mura&scirc;kovskij</aut>:
        <vrk>Pritakso de fantastikaj verkoj</vrk>.
        <lok><url
        ref="http://www.esperanto.mv.ru/Arkivoj/Fantastiko/Pritakso.html"
        >Survoje</url>.</lok>.
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ungebildet</trd>,
      <trd>unwissend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>barbarous</trd>,
      <trd>ignorant</trd>,
      <trd>illiterate</trd>,
      <trd>simple</trd>,
      <trd>uncultured</trd>,
      <trd>uneducated</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>b&eacute;otien<klr tip="ind"> (adj., fig.)</klr></trd>,
      <trd>ignare<klr tip="ind"> (adj.)</klr></trd>,
      <trd>ignorant<klr tip="ind"> (adj.)</klr></trd>,
      <trd>inculte<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>m&udblac;veletlen</trd>,
      <trd>k&eacute;pzetlen</trd>
    </trdgrp>
    <trd lng="nl">ongeletterd</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kler.mal0a.ajxo">
    <dif>
      <klr>(io)</klr>
      Kiu atestas pri manko de <tld/>o:
      <ekz>
        mal<tld/>a lingva&jcirc;o, letero, argumento, parolmaniero.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>grossier</trd>,
      <trd>inculte<klr tip="ind"> (adj.)</klr></trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_v;&c_je;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nevzdělaný</trd>,
    <trd>tupý</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>faragatlan</trd>,
    <trd>b&aacute;rdolatlan</trd>,
    <trd>tudatlan</trd>
  </trdgrp>
  <trd lng="nl">iets <ind>ongeletterd</ind>s</trd>
  <trdgrp lng="sk">
    <trd>neučený</trd>,
    <trd>nevzdelaný</trd>
  </trdgrp>
</drv>

<drv mrk="kler.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc mrk="kler.mal0ulo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Homo sen <tld/>o, sen baza scio kaj kompreno pri iu temo:
      <ekz>
        ili <klr>[...]</klr> komprenis, ke ili estas mal<tld/>uloj kaj
        nesciuloj
        <fnt><bib>NT</bib>,<lok>&Ago; 4:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>b&eacute;otien<klr tip="ind"> (subst., fig.)</klr></trd>,
    <trd>ignare<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ignorant<klr tip="ind"> (subst.)</klr></trd>,
    <trd>inculte<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_zh;&c_d;&c_a;</trd>
</drv>

<drv mrk="kler.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc mrk="kler.sen0ulo.PED">
    <ref tip="dif" cel="kler.mal0ulo">mal<tld/>ulo</ref>
    <ekz>
      tiam mi estis sen<tld/>ulo kaj mi ne komprenis; mi estis kiel
      bruto
      <fnt><bib>MT</bib>, <lok>&Psa; 73:22</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>b&eacute;otien<klr tip="ind"> (subst., fig.)</klr></trd>,
    <trd>ignare<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ignorant<klr tip="ind"> (subst.)</klr></trd>,
    <trd>inculte<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongeletterde<klr tip="ind"> (zn.)</klr></trd>,
    <trd>onwetende<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_zh;&c_d;&c_a;</trd>
</drv>
</art>
