<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: erizip.xml,v 1.15 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>erizipel</rad>/o
</kap>
<drv mrk="erizip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Infekta <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>, karakterizata de febro kaj
      inflamo de la
      ha&ubreve;to,
      precipe kaj plej ofte de la viza&gcirc;a ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="ca">erisipela</trd>
  <trdgrp lng="cs">
    <trd>erysipel</trd>,
    <trd>růže</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wundrose</trd>,
    <trd>Erysipel</trd>
  </trdgrp>
  <trd lng="es">erisipela</trd>
  <trdgrp lng="fr">
    <trd>&eacute;r&eacute;sip&egrave;le</trd>,
    <trd>&eacute;rysip&egrave;le</trd>
  </trdgrp>
  <trd lng="hu">orb&aacute;nc</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;a</trd>,
    <trd>r&oacute;&zdot;yczka<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">erisipela</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_zh;&c_a;<klr tip="amb">
    (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>,
    <trd>&c_r;&c_o;&c_zh;&c_i;&c_s;&c_t;&c_o;&c_je;
    &c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">ruža</trd>
</drv>
</art>
