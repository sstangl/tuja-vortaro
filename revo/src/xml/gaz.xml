<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gaz.xml,v 1.18 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gaz</rad>/o
</kap>

<drv mrk="gaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="gaz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Delikata travidebla silka a&ubreve; lina <ref tip="super"
      cel="sxtof.0o.TEKS">&scirc;tofo</ref>:
      <ekz>
        kulvualo el blua <tld/>o<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 40a, p. 576a</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="gaz.0o.RIM_paronimo">
      Ne konfuzu kun: <ref tip="vid" cel="gas.0o">gaso</ref>,
      aero kaj aliaj maldensaj substancoj.
    </rim>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_t;&c_je;&c_n;&c_z;&c_u;&c_h;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">gasa</trd>
  <trdgrp lng="cs">
    <trd>gáz</trd>,
    <trd>gáza</trd>,
    <trd>mul</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gaze</trd>,
    <trd>Flor</trd>,
    <trd>Schleierstoff</trd>
  </trdgrp>
  <trd lng="en">gauze</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_A;&a_r;&a_s;</trd>,
    <trd>&f_k;&a_r;&f_y;&a_sh;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">gaze</trd>
  <trd lng="hu">g&eacute;z</trd>
  <trd lng="nl">gaas</trd>
  <trd lng="pt">gaze<klr tip="amb"> (tecido)</klr></trd>
  <trd lng="ru">&c_g;&c_a;&c_z; <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">gáza</trd>
</drv>
</art>
