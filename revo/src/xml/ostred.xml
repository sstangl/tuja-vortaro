<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ostred.xml,v 1.10 2015/07/04 08:37:40 revo Exp $">
<kap><rad>ostred</rad>/oj</kap>
<drv mrk="ostred.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> de 
      <ref tip="malprt" cel="valv.du0uloj">duvalvuloj</ref>
      (<trd lng="la">Ostreidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_t;&c_r;&c_y;&c_c;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">ústřicovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Austern<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">osztrigaf&eacute;l&eacute;k</trd>
  <trd lng="nl">oesters<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">ostre&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_i;&c_c;&c_y;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_u;&c_s;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">ustricovité <klr>(čeľaď)</klr></trd>
</drv>
</art>
