<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: majxor.xml,v 1.13 2012/08/19 11:10:24 revo Exp $">
<kap>
   <rad>ma&jcirc;or</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="majxor.0a">
   <kap><tld/>a</kap>
   <uzo tip="fak">MUZ</uzo>
   <snc>
     <uzo tip="klr">(pri gamo)</uzo>
     <dif>
       Konsistanta el kvin tonoj kaj du duontonoj.
     </dif>
     <trd lng="de">in <ind>Dur</ind></trd>
   </snc>
   <snc>
     <uzo tip="klr">(pri tercio)</uzo>
     <dif>
       Konsistanta el du tonoj.
     </dif>
     <refgrp tip="vid">
       <ref cel="minor.0a">minora</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_zh;&c_o;&c_r;&c_n;&c_y;</trd>
   <trd lng="ca">major<klr> (m&uacute;s)</klr></trd>
   <trd lng="de">Dur</trd>
   <trd lng="en">major</trd>
   <trd lng="hu">d&uacute;r</trd>
   <trd lng="nl">majeur</trd>
   <trd lng="fr">majeur</trd>
   <trd lng="pt">maior <klr>(m&uacute;s.)</klr></trd>
   <trd lng="ru">&c_m;&c_a;&c_zh;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
