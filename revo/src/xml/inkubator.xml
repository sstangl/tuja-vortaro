  <rad>inkubator</rad>/o
</kap>
<drv mrk="inkubator.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BIO</uzo>
  <fnt><bib>PIV1</bib></fnt>
