<kap>
  <rad>stilus</rad>/o
</kap>
<drv mrk="stilus.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de la <ref tip="malprt"
      cel="pistil.0o">pistilo</ref> portanta la stigmon,
      sed ofte mankanta.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>stylus</trd>,
    <trd>čnělka</trd>
  </trdgrp>
  <trd lng="de">Griffel</trd>
  <trd lng="fr">style</trd>
  <trd lng="hu">bibesz&aacute;l</trd>
  <trd lng="nl">stijl<klr tip="ind"> (v..bloem)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_b;&c_i;&c_k; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_mol; &c_p;&c_je;&c_s;&c_t;&c_i;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">čnelka</trd>
</drv>
</art>
