<vortaro>
<art mrk="$Id: forfik.xml,v 1.15 2015/10/14 08:10:19 revo Exp $">
<kap>
  <rad>forfikul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="forfik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super"
      cel="dermap.0oj">dermapteroj</ref>
      (<trd lng="la">Forficula</trd>), kies abdomeno fini&gcirc;as per
      pin&ccirc;ilo.
    </dif>
  </snc>
  <trd lng="ca">papaorelles</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;&iacute;havka</trd>,
    <trd>&scaron;kvor</trd>
  </trdgrp>
  <trd lng="de">Ohrwurm</trd>
  <trd lng="en">earwig</trd>
  <trdgrp lng="fr">
    <trd>forficule</trd>,
    <trd>perce-oreille</trd></trdgrp>
  <trd lng="hu">f&uuml;lbem&aacute;sz&oacute;</trd>
  <trd lng="nl">oorworm</trd>
  <trdgrp lng="pl">
    <trd>skorek</trd>,
    <trd>szczypawka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lacrainha</trd>,
    <trd>bicha-cadela</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_h;&c_o;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>ucholak</trd>,
    <trd>&scaron;kvor</trd>
  </trdgrp>
</drv>
</art>
