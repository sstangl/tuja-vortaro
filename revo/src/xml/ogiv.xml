<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ogiv.xml,v 1.13 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ogiv</rad>/o
</kap>
<drv mrk="ogiv.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KON</uzo>
  <uzo tip="fak">MIL</uzo>
    <dif>
      Akuta angulo formata de du renkonti&gcirc;antaj samaj kurblinioj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>gotické klenební žebro</trd>,
    <trd>lomený gotický oblouk</trd>,
    <trd>ogival</trd>
  </trdgrp>
  <trd lng="de">Spitzbogen</trd>
  <trd lng="el">&alfa;&psi;&jota_ton;&delta;&alfa;</trd>
  <trd lng="fr">ogive</trd>
  <trd lng="hu">cs&uacute;cs&iacute;v</trd>
  <trd lng="nl">spitsboog</trd>
  <trd lng="pt">ogiva</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_a; &c_s;&c_v;&c_o;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>lomený oblúk</trd>,
    <trd>ogival</trd>
  </trdgrp>
</drv>

<drv mrk="ogiv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a arkitekturo, fenestro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="romp">rompita</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">obloukový s lomem ve vrcholu</trd>
  <trd lng="de">spitzbogig</trd>
  <trd lng="fr">ogival</trd>
  <trd lng="hu">cs&uacute;cs&iacute;ves</trd>
  <trdgrp lng="nl">
    <trd>in een <ind>spitsboog</ind></trd>,
    <trd>met een <ind>spitsboog</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">oblúkový s lomom vo vrchole</trd>
</drv>
</art>
