<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oksige.xml,v 1.32 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>oksigen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oksige.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">O</mlg>
  <snc mrk="oksige.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="8">&Hcirc;emia elemento</ref> kun
      atomnumero 8, atompezo 15,9994;
      senkolora, senodora gaso, ne bruligebla, sed nepre necesa por
      brulado kaj spirado, kiu miksite kun <ref tip="vid" 
      cel="azot.0o">nitrogeno (azoto)</ref> kaj aliaj gasoj
      formas aeron kaj kombinite kun 
      <ref tip="vid" cel="hidrog.0o">hidrogeno</ref> formas akvon:
      <ekz>
        trejnitaj helpantoj povas atingi viktimojn inter tri kaj kvar
        minutoj: tiam ankora&ubreve; restas <tld/>o en la pulmoj
        <fnt><aut>J. Ipfelkofer</aut>, <vrk>Esploroj kortu&scirc;aj</vrk>,
        <bib>Monato</bib>, <lok>2001:9, p. 12a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="oksid.0o">oksido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_s;&c_l;&c_a;&c_r;&c_o;&c_d;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_l;&c_o;&c_r;&c_o;&c_d;</trd>
  <trd lng="br">oksigen</trd>
  <trd lng="ca">oxigen</trd>
  <trdgrp lng="cs">
    <trd>kyslík</trd>,
    <trd>oxygenium</trd>
  </trdgrp>
  <trd lng="de">Sauerstoff</trd>
  <trd lng="el">&omikron;&xi;&ypsilon;&gamma;&omikron_ton;&ny;&omikron;</trd>
  <trd lng="en">oxygen</trd>
  <trd lng="es">ox&iacute;geno</trd>
  <trd lng="fa">&a_A;&f_k;&a_s;&f_y;&f_zh;&a_n;</trd>
  <trd lng="fr">oxyg&egrave;ne</trd>
  <trd lng="hu">oxig&eacute;n</trd>
  <trd lng="id">oksigen</trd>
  <trd lng="it">ossigeno</trd>
  <trd lng="la">Oxygenium</trd>
  <trd lng="nl">zuurstof</trd>
  <trd lng="pl">tlen</trd>
  <trd lng="pt">oxig&ecirc;nio</trd>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_o;&c_r;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>kyslík</trd>,
    <trd>oxygénium</trd>
  </trdgrp>
  <trd lng="sv">syre</trd>
</drv>

<drv mrk="oksige.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="O"/>izi, enmeti <tld/>on en substancon,
      saturi ion per <tld/>o:
      <ekz>
        La pulmoj <tld/>as la sangon;
      </ekz>
      <ekz>
        la efiko estas plilar&gcirc;igi arteriojn, kaj tio plifaciligas la
        fluon de <tld/>ita sango al la koro
        <fnt><aut>D. Curtis</aut>,
        <vrk>Koratako</vrk>, <bib>Monato</bib>,
        <lok>2002:1, p. 28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Temas ne pri &hcirc;emia kombina&jcirc;o de oksigeno kaj alia elemento
  </rim>
  <trd lng="be">&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_r;&c_o;&c_d;&c_a;&c_m;</ind></trd>
  <trd lng="br">oksigena&ntilde;</trd>
  <trd lng="ca">oxigenar</trd>
  <trd lng="de">mit Sauerstoff <ind>anreichern</ind></trd>
  <trd lng="en">oxigenate</trd>
  <trd lng="es">oxigenar</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&f_k;&a_s;&f_y;&f_zh;&a_n;&zwnj;&a_d;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&f_k;&a_s;&f_y;&f_zh;&a_n; &a_d;&a_A;&a_d;&a_n;<klr> (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="fr">oxyg&eacute;ner</trd>
  <trdgrp lng="hu">
    <trd>oxig&eacute;nez</trd>,
    <trd>oxig&eacute;nnel d&uacute;s&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">met <klr tip="ind">zuurstof</klr> verrijken</trd>
  <trdgrp lng="pl">
    <trd>nasyca&cacute; tlenem</trd>,
    <trd>natlenia&cacute;</trd>,
    <trd>dotlenia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_y;&c_shch;&c_a;&c_t;&c_mol; <ind>&c_k;&c_i;&c_s;&c_l;&c_o;&c_r;&c_o;&c_d;&c_o;&c_m;</ind></trd>
  <trd lng="sv">syres&auml;tta</trd>
  <trd lng="sk">okysličovať</trd>
  <trdgrp lng="cs">
    <trd>okysličovat</trd>,
    <trd>oxygenovat</trd>
  </trdgrp>
</drv>
</art>
