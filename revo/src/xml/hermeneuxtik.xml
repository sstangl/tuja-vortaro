<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hermeneuxtik.xml,v 1.8 2009/09/07 16:31:14 revo Exp $">
<kap>
  <rad>hermene&ubreve;tik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hermeneuxtik.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="hermeneuxtik.0o.BELE">
    <dif>
      Interpretarto aplikata al sistemoj de simboloj, al malfacilaj
      tekstoj:
      <ekz>
        <klr>...</klr> kiuj rekonas la gravecon de antikva <tld/>o kaj
        kulturo
        <fnt>
          <aut>B. Masala</aut>,
          <vrk>Re: Esperanto kaj fundamentismo</vrk>,
          <lok>soc.culture.esperanto, 2001-07-31</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzege.0o">ekzegezo</ref>,
      <ref cel="semiotik.0o.SCI">semiotiko</ref>
    </refgrp>
  </snc>
  <trd lng="de">Hermeneutik</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_fatha;&a_f;&a_r;&a_fatha;&a_n;&f_g;</trd>,
    <trd>&a_ayn;&a_l;&a_m; &a_t;&a_A_hamza_sure;&a_w;&f_y;&a_l;</trd>,
    <trd>&a_h;&a_r;&a_m;&a_n;&a_w;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">herm&eacute;neutique</trd>
  <trdgrp lng="hu">
    <trd>hermeneutika</trd>,
    <trd>&iacute;r&aacute;smagyar&aacute;zat</trd>,
    <trd>&eacute;rtelmez&eacute;stan</trd>
  </trdgrp>
  <trd lng="it">ermeneutica</trd>
  <trd lng="nl">hermeneutiek</trd>
  <trd lng="pt">hermen&ecirc;utica</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_n;&c_je;&c_v;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_l;&c_k;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je; &c_t;&c_je;&c_k;&c_s;&c_t;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>

