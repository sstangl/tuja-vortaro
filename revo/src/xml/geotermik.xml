<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geotermik.xml,v 1.5 2015/10/24 15:10:17 revo Exp $">
<kap>
  <rad>geotermik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="geotermik.0o">
  <kap><tld/>o</kap>
  <snc mrk="geotermik.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Tuto de la procedoj, per kiuj oni
      produktas home uzeblan
      <ref tip="vid" cel="energi.tervarma0o.TEK">tervarman
      energion</ref>:
      <ekz>
        en Novzelando 70 % de la produktita elektro devenas 
        el renovi&gcirc;antaj fontoj, kiel akvofluo, vento, 
        <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Renovi%C4%9Danta_energio">
          Renovi&gcirc;anta energio</url>, 2014-11-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">geotermika</trd>
  <trdgrp lng="en">
    <trd>geothermics</trd>,
    <trd>geothermal systems</trd>
  </trdgrp>
  <trd lng="fr">g&eacute;othermie<klr> (technique)</klr></trd>
  <trdgrp lng="nl">
    <trd>aardwarmte</trd>,
    <trd>geothermie</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>jordvarme</trd>,
    <trd>geotermisk energi</trd>
  </trdgrp>
  <trd lng="sk">geotermika</trd>
</drv>
</art>

