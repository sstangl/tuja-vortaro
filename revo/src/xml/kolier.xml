<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolier.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kolier</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kolier.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juvel&scirc;tona korpornama&jcirc;o, portata &ccirc;irka&ubreve; la kolo.
    </dif>
    <refgrp tip="sin">
      <ref cel="kol.cxirkaux0o.ornamajxo">&ccirc;irka&ubreve;kolo</ref>      
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_je;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>collaret</trd>,
    <trd>enfilall</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>halže</trd>,
    <trd>koliér</trd>,
    <trd>náhrdelník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Collier</trd>,
    <trd>Kollier</trd>
  </trdgrp>
  <trd lng="fr">collier<klr tip="ind"> (ornement)</klr></trd>
  <trdgrp lng="hu">
    <trd>nyak&eacute;k</trd>,
    <trd>nyakl&aacute;nc</trd></trdgrp>
  <trd lng="nl">halssnoer</trd>
  <trd lng="pt">colar</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">náhrdelník</trd>
</drv>
</art>

