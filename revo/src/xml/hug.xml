<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hug.xml,v 1.7 2015/02/26 19:12:15 revo Exp $">
<kap>
  <rad>Hug</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hug.0o">
  <kap><tld/>o</kap>
  <snc mrk="hug.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        li &scirc;ovis al onklo <tld/>o la partibileton
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hugo</trd>
  <trd lng="es">Hugo</trd>
  <trdgrp lng="fr">
    <trd>Hugo</trd>,
    <trd>Hugues</trd>
  </trdgrp>
  <trd lng="it">Ugo</trd>
  <trd lng="pt">Hugo</trd>
  <trdgrp lng="ru">
    <trd>&c_U;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_G;&c_u;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_H;&c_mol;&c_ju;&c_g;&c_o;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>

