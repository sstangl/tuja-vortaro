<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alun.xml,v 1.17 2015/10/13 18:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>alun</rad>/o
</kap>
<drv mrk="alun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Duobla sulfato de aluminio kaj de alia metalo (kalio, amonio,
      fero,
      kromo):
      <ekz>
        la kalian <tld/>on oni nomas ofte simple <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_y;&c_n;</trd>
  <trd lng="bg">&c_s;&c_t;&c_i;&c_p;&c_c;&c_a;</trd>
  <trd lng="ca">alum</trd>
  <trd lng="cs">kamenec</trd>
  <trd lng="de">Alaun</trd>
  <trd lng="en">alum</trd>
  <trd lng="es">alumbre</trd>
  <trd lng="fr">alun</trd>
  <trd lng="nl">aluin</trd>
  <trdgrp lng="pt">
    <trd>alume</trd>,
    <trd>pedra-ume</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_s;&c_c;&c_y;</trd>
  <trdgrp lng="sk">
    <trd>kamenec</trd>,
    <trd>kamenec <klr>(hlinito draseln&yacute;)</klr></trd>
  </trdgrp>
</drv>
</art>
