<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: medit.xml,v 1.20 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>medit</rad>/i
</kap>
<drv mrk="medit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="medit.0i.KOMUNE">
    <dif>
      Longe <ref tip="super" cel="pens.0i.ideo">pensi</ref>, fiksante
      kaj
      koncentrante sian atenton; enprofundi&gcirc;i en pensado:
      <ekz>
        stomako malsata nur pri pano <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la fidela servisto longe <tld/>is, kion fari
        <fnt><bib>ElektFab</bib>, <lok>fidela johano</lok></fnt>;
      </ekz>
      <ekz>
        li <klr>...</klr> reiris al sia &ccirc;ambro por <tld/>i sian
        paroladon al la &jcirc;urio
        <fnt><bib>DOz</bib>, <lok>&leftquot;la proceso kontra&ubreve; la katido
        He&ubreve;reka&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="absorb.0i">absorbi</ref>,
      <ref cel="cerb.0umi">cerbumi</ref>,
      <ref cel="pens.en0igxi">enpensi&gcirc;i</ref>,
      <ref cel="pens.pri0i">pripensi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_d;&c_y;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">meditar</trd>
  <trdgrp lng="cs">
    <trd>dumat</trd>,
    <trd>hloubat</trd>,
    <trd>meditovat</trd>,
    <trd>usilovně přemýšlet</trd>
  </trdgrp>
  <trd lng="de">meditieren</trd>
  <trd lng="en">meditate</trd>
  <trd lng="es">meditar</trd>
  <trd lng="fr">m&eacute;diter</trd>
  <trdgrp lng="hu">
    <trd>elm&eacute;lkedik</trd>,
    <trd>medit&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pijnzen</trd>,
    <trd>mediteren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>medytowa&cacute;</trd>,
    <trd>rozmy&sacute;la&cacute;</trd>,
    <trd>rozwa&zdot;a&cacute;</trd>,
    <trd>duma&cacute;</trd>
  </trdgrp>
  <trd lng="pt">meditar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_sh;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>meditovať</trd>,
    <trd>rozjímať</trd>,
    <trd>zamýšľať sa</trd>
  </trdgrp>
</drv>

<drv mrk="medit.0o">
  <kap><tld/>o</kap>
  <snc mrk="medit.0o.meditado">
    <ref tip="dif" cel="medit.0ado.KOMUNE"><tld/>ado</ref>
    <ekz>
      &ccirc;e &ccirc;iuj tiuj <tld/>oj mi forgesis la tempon kaj horon
      <fnt><bib>Fab4</bib>, <lok>turgardisto Ole, Unua
      vizito</lok></fnt>.
    </ekz>
    <trd lng="ca">meditaci&oacute;</trd>
    <trdgrp lng="pl">
      <trd>medytacja</trd>,
      <trd>zamy&sacute;lenie</trd>,
      <trd>rozmy&sacute;lania</trd>,
      <trd>rozwa&zdot;ania</trd>,
      <trd>zaduma</trd>
    </trdgrp>
  </snc>
  <snc mrk="medit.0o.meditajxo">
    <dif>
      <tld lit="M"/>a&jcirc;o, temo, kiun oni pripensas:
      <ekz>
        fremda <tld/>o estas ka&scirc;ita
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">pensament</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>meditace</trd>,
    <trd>přemítání</trd>
  </trdgrp>
  <trd lng="en">meditation</trd>
  <trd lng="fr">m&eacute;ditation</trd>
  <trd lng="sk">meditácia</trd>
</drv>

<drv mrk="medit.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="medit.0ado.KOMUNE">
    <dif>
      Longa, absorba pensado:
      <ekz>
        apud la tablo, kovrita de geografiaj kartoj, sidas en profunda
        <tld/>ado la granda admiralo de Hispanujo, Kristoforo Kolumbo
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">meditaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>rozjímání</trd>,
    <trd>uvažování</trd>
  </trdgrp>
  <trd lng="en">meditation</trd>
  <trd lng="fr">m&eacute;ditation</trd>
  <trdgrp lng="pl">
    <trd>medytacja</trd>,
    <trd>zamy&sacute;lenie</trd>,
    <trd>rozmy&sacute;lania</trd>,
    <trd>rozwa&zdot;ania</trd>,
    <trd>zaduma</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>meditovanie</trd>,
    <trd>rozjímanie</trd>,
    <trd>uvažovanie</trd>
  </trdgrp>
</drv>

<drv mrk="medit.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="medit.0ema.PSI">
    <dif>
      Momente a&ubreve; karaktere inklina al pensado, al <tld/>o:
      <ekz>
        Anno-Doroteo, <klr>...</klr> nur dekkvarjara infano, estis
        silentema kaj <tld/>ema
        <fnt><bib>Fab4</bib>, <lok>la vento rakontas pri Valdemaro Doe
        kaj
        pri liaj filinoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>meditatiu</trd>,
    <trd>pensar&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hloubavý</trd>,
    <trd>přemítavý</trd>,
    <trd>rozjímavý</trd>
  </trdgrp>
  <trd lng="en">meditative</trd>
  <trd lng="fr">m&eacute;ditatif</trd>
  <trdgrp lng="sk">
    <trd>hĺbavý</trd>,
    <trd>premietavý</trd>,
    <trd>rozjímavý</trd>
  </trdgrp>
</drv>
</art>
