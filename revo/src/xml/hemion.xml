<vortaro>
<art mrk="$Id: hemion.xml,v 1.14 2013/10/19 15:10:23 revo Exp $">
<kap>
  <rad>hemion</rad>/o
</kap>
<drv mrk="hemion.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="cxeval.0o.genro">&ccirc;evalo</ref>
      (<trd lng="la">Equus hemionus</trd>), vivanta en centra kaj
      okcidenta
      Azio kaj iom simila al <ref tip="vid" cel="azen.0o">azeno</ref>.
    </dif>
    <ref tip="lst" cel="huf.nepar0ulo"
    lst="voko:neparhufuloj">neparhufulo</ref>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  <trd lng="de">Halbesel</trd>
  <trd lng="fr">h&eacute;mione</trd>
  <trdgrp lng="hu">
    <trd>kul&aacute;n</trd>,
    <trd>f&eacute;lszam&aacute;r</trd></trdgrp>
   <trdgrp lng="it">
    <trd>onagro</trd>,
    <trd>emione</trd>
    </trdgrp>
  <trd lng="nl">wilde <ind>ezel</ind></trd>
  <trd lng="pt">hem&iacute;ono</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_a;&c_n;</trd>
</drv>
</art>
