  <dif>
    Prefikso kun la senco <sncref ref="cxirka.0.space"/>:
  </dif>
  <snc>
    <subsnc mrk="cxirka.0.pref_1">
      <dif>
        <klr>(<tld/><frm><k>X</k></frm>i:
          <frm><k>X</k></frm>i (ntr) &ccirc;e/al la <tld/>o,
          <frm><k>X</k></frm>i &ccirc;iuflanke(n))</klr>
