            <ref cel="man.cxirkaux0o"><tld/>mano</ref>...
          </refgrp>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="cxirka.0.pref_7">
      <dif>
        <klr>(<tld/><frm><k>X</k></frm>o:
          <tld/>(ant)a <frm><k>X</k></frm>o,
          <frm><k>X</k></frm>o rilata al &ccirc;irka&ubreve;o)</klr>
        <ekz>
          <refgrp tip="vid">
            <ref cel="bord.cxirkaux0o"><tld/>bordo</ref>,
            <ref cel="mezur.cxirkaux0o"><tld/>mezuro</ref>,
            <ref cel="mur.cxirkaux0o"><tld/>muro</ref>,
            <ref cel="urb.cxirkaux0o"><tld/>urbo</ref>,
            <ref cel="voj.cxirkaux0o"><tld/>vojo</ref>...
          </refgrp>
        </ekz>
      </dif>
    </subsnc>
