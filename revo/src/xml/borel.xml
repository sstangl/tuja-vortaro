<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: borel.xml,v 1.7 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>Borel</rad>/o
</kap>
<drv mrk="borel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve:
      &Eacute;mile <trd lng="fr">Borel</trd>, 1871-1956. Franca matematikisto.
    </dif>
  </snc>
  <trd lng="cs">Borel</trd>
  <trd lng="de">Borel</trd>
  <trd lng="en">Borel</trd>
  <trd lng="it">Borel</trd>
  <trd lng="nl">Emile <ind>Borel</ind></trd>
  <trd lng="pl">Borel</trd>
  <trd lng="ru">&c_B;&c_o;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">Borel</trd>
</drv>
<drv mrk="borel.b0a">
  <kap><tld lit="b"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="algebr.borelasigma0o"><tld lit="b"/>a &sigma;-algebro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Borelsche</trd>
  <trd lng="en">Borel['s]</trd>
  <trd lng="fr">de <ind>Borel</ind></trd>
  <trd lng="it">di <ind>Borel</ind></trd>
  <trd lng="nl">van <ind>Borel</ind></trd>
  <trdgrp lng="pl">
    <trd>Borela</trd>,
    <trd>borelowski</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_r;&c_je;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_B;&c_o;&c_r;&c_je;&c_l;&c_ja;</trd></trdgrp> 
</drv>
</art>

