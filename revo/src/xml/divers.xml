<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: divers.xml,v 1.23 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>divers</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="divers.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="divers.0a.malsamspeca">
    <dif>
      Malsamspeca:
      <ekz>
        la&ubreve; <tld/>a maniero;
      </ekz>
      <ekz>
        &scirc;tonoj de <tld/>a grandeco;
      </ekz>
      <ekz>
        monpaperoj de <tld/>a valoro;
      </ekz>
      <ekz>
        homoj de <tld/>a ekstera&jcirc;o, de <tld/>aj
        specoj<fnt>Z</fnt>, agoj k karakteroj;
      </ekz>
      <ekz>
        tiun &ccirc;i flankon <tld/>aj esp-istoj povas klarigi al si
        en la plej <tld/>a maniero k en la plej <tld/>aj gradoj
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>gentaj lo&gcirc;antoj
        <fnt><bib>MT</bib>, <lok>&Jer; 50:37</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>konsista<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="ali.0a">alia</ref>,
      <ref cel="difere.0a">diferenca</ref>,
      <ref cel="kalejd">kalejdoskopa</ref>,
      <ref cel="mozaik">mozaika</ref>
    </refgrp>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;ny</trd>,
    <trd>przer&oacute;&zdot;ny</trd>,
    <trd>rozmaity</trd>,
    <trd>urozmaicony</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>diverso</trd>,
    <trd>v&aacute;rio</trd>
  </trdgrp>
  </snc>
  <snc mrk="divers.0a.kelkaj">
    <uzo tip="klr">(nur plurale)</uzo>
    <dif>
      Kelke da malsamaj:
      <ekz>
        ni a&ccirc;etis <tld/>ajn ilojn;
      </ekz>
      <ekz>
        <tld/>aj detaloj<fnt>Z</fnt>,
        cirkonstancoj<fnt>Z</fnt>, lokoj<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="plur.0aj">pluraj</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>r&oacute;&zdot;ne</trd>
    </trdgrp>
    <trd lng="pt">v&aacute;rios</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lecjaký</trd>,
    <trd>r&uring;zn&yacute;</trd>,
    <trd>rozdílný</trd>,
    <trd>rozličný</trd>,
    <trd>rozmanitý</trd>,
    <trd>různý</trd>,
    <trd>všelijaký</trd>,
    <trd>všeliký</trd>
  </trdgrp>
  <trd lng="de">verschieden</trd>
  <trd lng="en">diverse</trd>
  <trd lng="es">diverso</trd>
  <trd lng="fr">divers</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
    <trd>k&uuml;l&ouml;nf&eacute;le</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>
    &c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozličný</trd>,
    <trd>rôzny</trd>
  </trdgrp>
</drv>

<drv mrk="divers.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="divers.0i.KOMUNE">
    <dif>
      Prezenti inter si malsamojn, diferencojn:
      <ekz>
        la lokaj kondi&ccirc;oj tiel <tld/>as, ke neniu
        kohera tapeto teksi&gcirc;as el tiuj fadenoj
        <fnt>
          <aut>M. Fettes</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/56240/255977">
          Kuniga arto</url></vrk>,
          <lok>2010-06-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">provádět diverzní činnost</trd>
  <trdgrp lng="fr">
    <trd>diverger</trd>,
    <trd>pr&eacute;senter des diff&eacute;rences</trd>
  </trdgrp>
  <trd lng="sk">prevádzať diverznú činnosť</trd>
</drv>

<drv mrk="divers.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="divers.0eco.KOMUNE">
    <dif>
      Malsameco:
      <ekz>
        la <tld/>eco de la klimatoj.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;
  &c_mol;</trd>
  <trdgrp lng="cs">
    <trd>diverzita</trd>,
    <trd>různost</trd>
  </trdgrp>
  <trd lng="de">Verschiedenheit</trd>
  <trd lng="es">diversidad</trd>
  <trd lng="fr">diversit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;s&eacute;g</trd>,
    <trd>diverzit&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>r&oacute;&zdot;nica</trd>,
    <trd>r&oacute;&zdot;no&sacute;&cacute;</trd>,
    <trd>r&oacute;&zdot;norodno&sacute;&cacute;</trd>,
    <trd>urozmaicenie</trd>,
    <trd>dywersyfikacja</trd>
  </trdgrp>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_je;</trd>
  <trd lng="sk">rôznosť</trd>
</drv>

<drv mrk="divers.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="divers.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, <ref tip="vid" cel="difere.0igi">diferencigi</ref>,
      <ref tip="vid" cel="vari.0igi">variigi</ref>,
      <ref tip="vid" cel="sxangx.0i">&scirc;an&gcirc;i</ref>. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;&c_m;</ind>
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oddělit</trd>,
    <trd>rozlišovat</trd>,
    <trd>rozrůznit</trd>
  </trdgrp>
  <trd lng="es">diversificar</trd>
  <trd lng="fr">diversifier</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;v&eacute; tesz</trd>,
    <trd>diverzifik&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>por&oacute;&zdot;nia&cacute;</trd>,
    <trd>dywersyfikowa&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>oddeliť</trd>,
    <trd>rozlišovať</trd>
  </trdgrp>
</drv>

<drv mrk="divers.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="divers.mal0a.KOMUNE">
    <dif>
      Unueca, <ref tip="vid" cel="ton.unu0a">unutona</ref>, nevaria:
      <ekz>
        pri la nutra&jcirc;o, mi povas diri ke &gcirc;i estas bona sed
        iomete 
        mal<tld/>a: ni ricevis sen&scirc;an&gcirc;e tomatojn, kukumojn
        kaj
        kaprinan 
        froma&gcirc;on je la matenman&gcirc;o...
        <fnt>
          <aut>C. Pierroux</aut>,
          <vrk>
          <url
          ref="http://www.internacia-gazeto.com/articoli/articolo_0079_claude_kreto.htm">
          Ok tagoj en Kreto</url></vrk>,
          <lok>Internacia Gazeto, 2005-07-13</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">uni<klr> (sans vari&eacute;t&eacute;)</klr></trd>
  <trdgrp lng="pl">
    <trd>jednostajny</trd>,
    <trd>monotonny</trd>,
    <trd>niezr&oacute;&zdot;nicowany</trd>
  </trdgrp>
</drv>
</art>
