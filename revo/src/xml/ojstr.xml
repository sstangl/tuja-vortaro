<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ojstr.xml,v 1.16 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>ojstr</rad>/o
</kap>
<drv mrk="ojstr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ojstre.0oj"><tld/>edoj</ref>
      (<trd lng="la">Oestrus</trd>), kies larvoj
      parazitas en la kapo de &scirc;afoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">střeček</trd>
  <trd lng="de">Schafbremse</trd>
  <trdgrp lng="en">
    <trd>bot fly</trd>,
    <trd>gadfly</trd>
    <trd>sheep fly</trd>,
    <trd>warble fly</trd>
  </trdgrp>
  <trd lng="hu">juhbag&oacute;cs</trd>
  <trd lng="nl">schapenhorzel</trd>
  <trd lng="ru">&c_o;&c_v;&c_o;&c_d;</trd>
  <trd lng="sk">strečok ovčí</trd>
</drv>
</art>
