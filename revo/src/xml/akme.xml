<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akme.xml,v 1.9 2015/07/04 08:37:30 revo Exp $">
<kap>
   <rad>akme</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="akme.0o">
   <kap><tld/>o</kap>
   <snc mrk="akme.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       <ref tip="super" cel="paroks.0o">Paroksismo</ref> de malsano,
       kiam
       simptomoj intense aperas.
     </dif>
     <trd lng="de">Akme</trd>
     <trd lng="es">acm&eacute;</trd>
     <trd lng="fr">acm&eacute;</trd>
     <trd lng="pl">akme</trd>
     <trd lng="ru">&c_k;&c_r;&c_i;&c_z;&c_i;&c_s;<klr tip="amb"> (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_i;)</klr></trd>
   </snc>
   <snc mrk="akme.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <uzo tip="stl">RAR</uzo>
     <ref tip="dif" cel="kulmin.0o.FIG">kulmino <sncref/></ref>
     <ekz>
       &scirc;i trovi&gcirc;is en <tld/>o de kolero
       <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 77a, p. 620a</lok></fnt>
     </ekz>
     <trd lng="de">H&ouml;hepunkt</trd>
     <trdgrp lng="pl">
     <trd>szczyt</trd>,
     <trd>punkt <ind>kulminacyjny</ind></trd>,
     <trd>kulminacja</trd>
     </trdgrp>
     <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_m;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
   </snc>
  <trd lng="be">&c_a;&c_k;&c_m;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>akme - vyvrcholení</trd>,
    <trd>akmé</trd>
  </trdgrp>
  <trd lng="pt">acme</trd>
  <trd lng="sk">vyvrcholenie</trd>
</drv>
</art>

