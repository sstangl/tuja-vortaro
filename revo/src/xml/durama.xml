<kap>
<rad>duramatr</rad>/o
</kap>
<drv mrk="durama.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<snc>
<dif>
La plej ekstera k la plej forta el la tri meningoj, kiuj envolvas la
cerbon. 
</dif>
<refgrp tip="vid">
<ref cel="piamat.0o">piamatro</ref>.
</refgrp>
</snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_ja; &c_m;&c_a;&c_z;&c_g;&c_a;&c_v;&c_a;&c_ja; &c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_d;&c_u;&c_r;&c_a; &c_m;&c_a;&c_t;&c_je;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>podlebnice</trd>,
    <trd>tvrdá blána mozková</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>harte Hirnhaut</trd>,
   <trd>Pachymeninx</trd></trdgrp>
  <trd lng="en">dura mater</trd>
  <trd lng="fr">dure-m&egrave;re</trd>
  <trdgrp lng="hu">
   <trd>dura mater</trd>,
   <trd>kem&eacute;ny <ind>agyh&aacute;rtya</ind></trd></trdgrp>
  <trdgrp lng="it">
   <trd>dura madre</trd>,
   <trd>pachimeninge</trd>
</trdgrp>
  <trd lng="la">Dura mater</trd>
  <trd lng="pl">opona twarda m&oacute;zgu</trd>
  <trd lng="pt">dura-m&aacute;ter</trd>
  <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_ja; &c_m;&c_o;&c_z;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">tvrdá mozgová blana</trd>
</drv>
