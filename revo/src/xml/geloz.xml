<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geloz.xml,v 1.13 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>geloz</rad>/o
</kap>
<drv mrk="geloz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>10</sub>O<sub>5</sub>.
      Substanco simila al <ref cel="gelate.0o">gelateno</ref>,
      ekstraktata 
      el iuj algoj kaj uzata kiel kulturmedio por bakterioj.  
    </dif>
    <ref tip="sin" cel="agarag.0o">agaragaro.</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>gelosa</trd>,
    <trd>agar-agar</trd>
  </trdgrp>
  <trd lng="cs">geloza</trd>
  <trd lng="de">Gelose</trd>
  <trd lng="fr">g&eacute;lose</trd>
  <trd lng="hu">agaragar</trd>
  <trd lng="nl">agar-agarcultuur</trd>
  <trdgrp lng="pt">
    <trd>&aacute;gar</trd>,
    <trd>&aacute;gar-&aacute;gar</trd>,
    <trd>gelose</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>agar</trd>,
    <trd>želé z morských rias</trd>
  </trdgrp>
</drv>
</art>
