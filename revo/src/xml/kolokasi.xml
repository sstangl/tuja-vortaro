<art mrk="$Id: kolokasi.xml,v 1.7 2013/05/17 01:10:19 revo Exp $">
<kap>
  <rad>kolokasi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kolokasi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <uzo tip="fak">BOT</uzo>
  <ref tip="dif" cel="kolokazi.0o">kolokazio</ref>
  <ekz>
    ili povus travivi per <tld/>oj nutrori&ccirc;aj
    <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;tragedio de man&gcirc;o en
    la insulo Mindanao&rightquot;, p. 40a</lok></fnt>.
  </ekz>
  <!-- formon evitindan prefere ne traduku -->
</drv>
</art>

