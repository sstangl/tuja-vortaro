<kap><ofc>*</ofc>
<rad>fab</rad>/o</kap>
<drv mrk="fab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="vici.0o">vicio</ref>
      (<trd lng="la">Vicia faba var. major</trd>), kun grandaj plataj
      grajnoj en
      gu&scirc;o, kulturata kiel legomo.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="fazeol.0o">fazeolo</ref>.
    </rim>
  </snc>
  <trd lng="kek">aaws</trd>
  <trd lng="be">&c_b;&c_o;&c_b;</trd>
  <trd lng="br">favenn</trd>
  <trdgrp lng="ca">
    <trd>fava</trd>,
    <trd>favera</trd>
  </trdgrp>
  <trd lng="cs">bob</trd>
  <trdgrp lng="de">
    <trd>Saubohne</trd>,
    <trd>Pferdebohne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>broad bean</trd>,
    <trd>fava bean</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_q;&a_l;&a_A;</trd>
  <trd lng="fr">f&egrave;ve<klr tip="ind"> (l&eacute;gume)</klr></trd>
  <trdgrp lng="hu">
    <trd>l&oacute;bab</trd>,
    <trd>laposbab</trd>,
    <trd>diszn&oacute;bab</trd>,
    <trd>l&oacute;b&uuml;kk&ouml;ny</trd>
  </trdgrp>
  <trd lng="nl">tuinboon</trd>
  <trd lng="pl">b&oacute;b</trd>
  <trd lng="pt">fava</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;</trd>
  <trd lng="sk">b√¥b</trd>
</drv>

<drv mrk="fab.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <ref tip="dif" cel="fazeol.0o">fazeolo</ref>
    <trd lng="pl">fasola</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>fesol</trd>,
    <trd>mongeta</trd>
  </trdgrp>
</drv>
</art>
