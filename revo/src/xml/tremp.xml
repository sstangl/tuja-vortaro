<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tremp.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tremp</rad>/i
</kap>
<drv mrk="tremp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti ion en akvon a&ubreve; alian flua&jcirc;on:
      <ekz>
        <tld/>i plumon en inkon;
      </ekz>
      <ekz>
        <tld/>i tukon en tinkturon;
      </ekz>
      <ekz>
        <tld/>i panon en vinon;
      </ekz>
      <ekz>
        atentu, via maniko <tld/>i&gcirc;as en la sa&ubreve;co.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ban.0i">bani</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_a;&c_k;&c_u;&c_n;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>močit</trd>,
    <trd>máčet</trd>,
    <trd>namočit</trd>,
    <trd>namáčet</trd>,
    <trd>omočit</trd>,
    <trd>smočit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eintauchen</trd>,
    <trd>eintunken</trd>
  </trdgrp>
  <trd lng="en">dip</trd>
  <trd lng="es">mojar<klr tip="amb"> (algo en un l&iacute;quido)</klr></trd>
  <trd lng="fr">tremper</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;rt</trd>,
    <trd>belem&aacute;rt</trd>
  </trdgrp>
  <trd lng="nl">indopen</trd>
  <trd lng="pt">imergir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>máčať</trd>,
    <trd>namočiť</trd>,
    <trd>omočiť</trd>,
    <trd>zmočiť</trd>
  </trdgrp>
</drv>
</art>
