<vortaro>
<art mrk="$Id: gudr.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap><ofc>*</ofc>
<rad>gudr</rad>/o</kap>
<drv mrk="gudr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nigra glua substanco, resta&jcirc;o el distilado de
      <refgrp tip="vid">
        <ref cel="ken.0o">keno</ref>
        a&ubreve; <ref cel="karb.0o">karbo</ref>.
      </refgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="bitum.0o">bitumo</ref>,
      <ref cel="makada.0o">makadamo</ref>,
      <ref cel="pecx.ter0o">terpe&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">quitr&agrave;</trd>
  <trd lng="cs">dehet</trd>
  <trd lng="de">Teer</trd>
  <trd lng="en">tar</trd>
  <trd lng="es">brea</trd>
  <trd lng="fr">goudron</trd>
  <trd lng="hu">k&aacute;tr&aacute;ny</trd>
  <trd lng="nl">teer</trd>
  <trd lng="pl">smo&lstroke;a</trd>
  <trd lng="pt">alcatr&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_jo;&c_g;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_d;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>decht</trd>,
    <trd>tér</trd>
  </trdgrp>
</drv>

<drv mrk="gudr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        <tld/>i &scirc;ipon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &scirc;nuron;
      </ekz>
      <ekz>
        <tld/>ita pavimo de strato.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">enquitranar</trd>
  <trdgrp lng="cs">
    <trd>dehtovat</trd>,
    <trd>natírat dehtem</trd>
  </trdgrp>
  <trd lng="de">teeren</trd>
  <trd lng="en">tar</trd>
  <trd lng="fr">goudronner</trd>
  <trd lng="hu">bek&aacute;tr&aacute;nyoz</trd>
  <trd lng="nl">teren</trd>
  <trd lng="pl">smo&lstroke;owa&cacute;</trd>
  <trd lng="pt">alcatroar</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dechtovať</trd>,
    <trd>natierať dechtom</trd>
  </trdgrp>
</drv>
</art>
