<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mucila.xml,v 1.11 2015/09/21 07:10:16 revo Exp $">
<kap>
  <rad>mucilag</rad>/o
</kap>
<drv mrk="mucila.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Viskeca likva&jcirc;o, uzata en la farmacio kaj kiun oni ricevas
      solvante
      ian guma&jcirc;on en akvo.
    </dif>
    <trdgrp lng="en"><trd>mucilage</trd>, <trd>gum</trd></trdgrp>
    <trd lng="hu">ny&aacute;lka<klr tip="ind"> (n&ouml;v&eacute;nyi)</klr></trd>
    <trdgrp lng="pl">
      <trd>mucilago</trd>,
      <trd>klej ro&sacute;linny</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_l;&c_je;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Viskeca substanco, ekzistanta preska&ubreve; en &ccirc;iuj plantoj.
    </dif>
    <trd lng="en">mucilage</trd>
    <trd lng="hu">m&eacute;zga</trd>
    <trd lng="pl">&sacute;luz ro&sacute;linny</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_l;&c_i;&c_z;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>rostlinn&yacute; sliz</trd>,
    <trd>&scaron;lem</trd>
  </trdgrp>
  <trd lng="nl">slijm<klr tip="ind"> (v.gom of planten)</klr></trd>
  <trd lng="pt">mucilagem <klr tip="amb">(bot.)</klr></trd>
  <trd lng="sk">rastlinn&yacute; sliz</trd>
</drv>
</art>
