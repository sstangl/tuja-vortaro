<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: patrik.xml,v 1.9 2015/02/26 19:11:16 revo Exp $">
<kap>
  <rad>Patrik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="patrik.0o">
  <kap><tld/>o</kap>
  <snc mrk="patrik.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o ne estis la unua Kristana misiito al Irlando
        <fnt>
        <bib>Viki</bib>,
        <lok>sankta Patriko, 2005-08-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_P;&c_a;&c_t;&c_r;&c_i;&c_k;</trd>
  <trd lng="ca">Patrici</trd>
  <trd lng="de">Patrick </trd>
  <trd lng="en">Patrick </trd>
  <trd lng="fr">Patrick</trd>
  <trd lng="it">Patrizio</trd>
  <trd lng="nl">Patrick</trd>
  <trd lng="pt">Patrick</trd>
  <trd lng="ru">&c_P;&c_a;&c_t;&c_r;&c_i;&c_k;</trd>
</drv>
</art>

