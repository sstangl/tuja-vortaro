<vortaro>
<art mrk="$Id: serasalm.xml,v 1.2 2011/12/11 16:10:11 revo Exp $">

<kap>
  <rad>serasalm</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="serasalm.0o">
  <kap><tld/>o</kap>

  <snc mrk="serasalm.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro (<trd lng="la">Serrasalmus</trd>), de dol&ccirc;akvaj Sud-
      kaj
      Centroamerikaj ciprinformaj fi&scirc;oj kun akregaj dentoj kies
      plej konata
      specio estas <ref tip="sub" cel="piranj.0o.specio">piranjo</ref>
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Serrasalmus</trd>
</drv>
</art>

