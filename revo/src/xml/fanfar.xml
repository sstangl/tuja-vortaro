<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fanfar.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fanfar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fanfar.0o">
  <kap><tld/>o</kap>
  <snc mrk="fanfar.0o.MUZ_muzikistaro">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzikistaro, kiu uzas nur kuprajn kaj latunajn muzikilojn. 
    </dif>
    <trd lng="de">Blaskapelle</trd>
    <trdgrp lng="hu">
     <trd>f&uacute;v&oacute;szenekar</trd>,
     <trd>rezesbanda</trd></trdgrp>
  </snc>
  <snc mrk="fanfar.0o.MUZ_muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Gaja ario por kupraj kaj latunaj muzikiloj: <ekz>&ccirc;asa, militista
      <tld/>o.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Fanfare</trd>,
     <trd>Trompetengeschmetter</trd>,
     <trd>Tusch</trd></trdgrp>
    <trd lng="en">fanfare</trd>
    <trdgrp lng="hu">
      <trd>fanf&aacute;r</trd>,
      <trd>darab <ind>r&eacute;zf&uacute;v&oacute;sokra</ind></trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>fanfára</trd>,
    <trd>intráda</trd>,
    <trd>tuš</trd>
  </trdgrp>
  <trd lng="es">fanfarria</trd>
  <trd lng="fr">fanfare</trd>
  <trd lng="nl">fanfare</trd>
  <trdgrp lng="pt">
    <trd>fanfarra</trd>,
    <trd>charanga</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>fanfáry</trd>,
    <trd>tuš</trd>
  </trdgrp>
</drv>
</art>

