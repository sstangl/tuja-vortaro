<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hucxin.xml,v 1.6 2013/09/08 18:11:25 revo Exp $">
<kap>
  <rad>hu&ccirc;in</rad>/o
</kap>

<drv mrk="hucxin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Fremdadevena, &ccirc;ina <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      kun diversforma sonkesto, tra kiu la kolo trabas,
      sonigata per ar&ccirc;o tenata inter la du a&ubreve; kvar kordoj:
      <ekz>
        Fangguan kantu la melodion <klr>...</klr>, kaj &scirc;in nur <tld/>o
        kaj
        &scirc;akuha&ccirc;o akompanu
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 54a, p. 211a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Huqin</trd>
  <trdgrp lng="fr">
    <trd>huqin</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>huqin</trd>, <trd><ind>violino</ind> cinese</trd>
  </trdgrp>
  <trd lng="pt">huqin</trd>
  <trd lng="ru">&c_h;&c_u;&c_c;&c_i;&c_n;&c_mol;</trd>
</drv>
</art>

