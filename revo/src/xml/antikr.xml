<vortaro>
<art mrk="$Id: antikr.xml,v 1.14 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>antikrist</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="antikr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Kontra&ubreve;ulo de Kristo, kiu aperos anta&ubreve; la fino de la mondo kaj
      estos venkata de Kristo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_h;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">nepřítel Krista a křesťanství</trd>
  <trd lng="de">Antichrist</trd>
  <trd lng="en">antichrist</trd>
  <trd lng="es">anticristo</trd>
  <trd lng="fr">ant&eacute;christ</trd>
  <trd lng="hu">Antikrisztus</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_h;&c_r;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>antikrist</trd>,
    <trd>nepriateľ Krista</trd>
  </trdgrp>
</drv>
</art>
