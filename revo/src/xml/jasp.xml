<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jasp.xml,v 1.23 2015/09/18 13:10:28 revo Exp $">
<kap>
<rad>jasp</rad>/o
</kap>
<drv mrk="jasp.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MIN</uzo>
   <snc>
     <dif>
       Malmola kaj nediafana &scirc;tono, simila al agato, havanta 
       multkolorajn striojn kaj makulojn.
     </dif>
   </snc>
   <trd lng="en">jasper</trd>
  <trd lng="be">&c_ja;&c_sh;&c_m;&c_a;</trd>
  <trd lng="bg">&c_ja;&c_s;&c_p;&c_i;&c_s;</trd>
  <trd lng="ca">jaspi</trd>
  <trd lng="cs">jaspis</trd>
  <trd lng="de">Jaspis</trd>
  <trd lng="el">&jota_ton;&alfa;&sigma;&pi;&jota;&sigma_fina;</trd>
  <trd lng="es">jaspe</trd>
  <trd lng="fr">jaspe</trd>
  <trd lng="he">&yod;&shin;&pe;&he;</trd>
  <trd lng="hu">j&aacute;spis</trd>
  <trd lng="it">diaspro</trd>
  <trd lng="nl">jaspis</trd>
  <trd lng="pt">jaspe</trd>
  <trd lng="ru">&c_ja;&c_sh;&c_m;&c_a;</trd>
  <trd lng="sk">jaspis</trd>
  <trd lng="sv">jaspis</trd>
</drv>

<drv mrk="jasp.0i">
<kap><tld/>i</kap>
  <snc>
   <dif>
    Farbi multkolore imitante la kolorojn de <tld/>o:
    <ekz><tld/>i la randojn de libro.</ekz>
   </dif>
  </snc>
  <trd lng="ca">jaspiar</trd>
   <trd lng="fr">jasper</trd>
   <trd lng="hu">m&aacute;rv&aacute;nyoz</trd>
   <trd lng="it">marmorizzare</trd>
</drv>
</art>

