<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rugx.xml,v 1.40 2015/06/26 22:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ru&gcirc;</rad>/a
</kap>
<drv mrk="rugx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Samkolora kiel sango:
      <ekz>
        <tld/>a kiel tomato, koralo, rubeno, papavo,
        grenato, cinabro, minio;
      </ekz>
      <ekz>
        flav<tld/>aj haroj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skarla">skarlata</ref>,
      <ref cel="punc.0a">punca</ref>,
      <ref cel="karmin">karmina</ref>,
      <ref cel="karmez">karmezina</ref>,
      <ref cel="purpur">purpura</ref>,
      <ref cel="roz.0a">roza</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_v;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;erven&yacute;</trd>,
    <trd>rud&yacute;</trd>
  </trdgrp>
  <trd lng="de">rot</trd>
  <trd lng="en">red</trd>
  <trd lng="es">rojo</trd>
  <trd lng="fr">rouge<klr> (adj.)</klr></trd>
  <trd lng="he">&alef;&dalet;&vav;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>piros</trd>,
    <trd>v&ouml;r&ouml;s</trd>
  </trdgrp>
  <trd lng="id">merah</trd>
  <trd lng="ja">&#36196;&#12356; [<ind>&#12354;&#12363;&#12356;</ind>]</trd>
  <trd lng="nl">rood</trd>
  <trd lng="pl">czerwony</trd>
  <trd lng="pt">vermelho</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">r&ouml;d</trd>
  <trdgrp lng="tr">
    <trd>k&inodot;rm&inodot;z&inodot;</trd>,
    <trd>k&inodot;z&inodot;l</trd>
  </trdgrp>
  <trd lng="tp">loje</trd>
  <trd lng="vo">ledik</trd>
  <trd lng="zh">红 [<ind>hóng</ind>]</trd>
</drv>

<drv mrk="rugx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="R"/>a <ref tip="super"
      cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_v;&c_je;&c_n;&c_o;</trd>
  <trd lng="de">Rot</trd>
  <trd lng="en">red</trd>
  <trdgrp lng="fr">
    <trd>rouge<klr> (subst..)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&dalet;&vav;&fmem;</trd>,
    <trd>&alef;&vav;&dalet;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>piros sz&iacute;n</trd>,
    <trd>v&ouml;r&ouml;s sz&iacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#36196; [<ind>&#12354;&#12363;</ind>]</trd>
  <trd lng="ku">Sor</trd>
  <trd lng="nl">rood<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>czerwie&nacute;</trd>,
    <trd>kolor <ind>czerwony</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; 
&c_c;&c_v;&c_je;&c_t;</trd>
</drv>

<drv mrk="rugx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="rugx.0igxi.ajne">
    <dif>
      I&gcirc;i <tld/>a.
    </dif>
    <trd lng="de">sich rot f&auml;rben</trd>
    <trd lng="en">redden</trd>
    <trd lng="he">&lamed;<ind>&he;&alef;&dalet;&yod;&fmem;</ind></trd>
    <trdgrp lng="hu">
      <trd>kipirosodik</trd>,
      <trd>kipirul</trd>
    </trdgrp>
    <trd lng="ja">&#36196;&#12425;&#12416;
    [<ind>&#12354;&#12363;&#12425;&#12416;</ind>]</trd>
    <trd lng="nl">rood worden</trd>
  </snc>
  <snc mrk="rugx.0igxi.vizagxe">
    <dif>
      Viza&gcirc;e ek<tld/>i pro emocia sangalfluo, ekzemple
      pro honto, pudoro, kolero:
      <ekz>
        <tld/>i&gcirc;i pro honto, de honto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">err&ouml;ten</trd>
    <trd lng="en">blush</trd>
    <trd lng="he">&lamed;<ind>&he;&samekh;&mem;&yod;&qof;</ind></trd>
    <trd lng="id">[<ind>merah</ind>] memerah</trd>
    <trd lng="ja">&#36196;&#12425;&#12417;&#12427;
    [<ind>&#12354;&#12363;&#12425;&#12417;&#12427;</ind>]</trd>
    <trdgrp lng="nl">
      <trd>blozen</trd>,
      <trd>kleuren</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zarumieni&cacute; si&eogonek;</trd>,
      <trd>rumieni&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">rot werden</trd>
  <trdgrp lng="fr">
    <trd>rougir<klr> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elpirul</trd>,
    <trd>elv&ouml;r&ouml;s&ouml;dik</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zaczerwieni&cacute; si&eogonek;</trd>,
    <trd>czerwieni&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_a;&c_s;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rugx.trans0a">
  <kap>trans<tld/>a</kap>
  <snc mrk="rugx.tran0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Kvalifikas nevideblajn radiojn situantajn trans la ru&gcirc;olimo
      de la videbla parto de la optika
      <ref cel="spektr.0o.FIZ">spektro</ref>:
      <ekz>
        la ondolongo de trans<tld/>aj radioj estas inter 800
        nanometroj kaj 1 milimetro;
      </ekz>
      <ekz>
        kiel stabila molekulo en nia atmosfero, &gcirc;i
        <klr>(la akvo)</klr> grave rolas kiel absorbilo de
        trans<tld/>a radiado
        <fnt><bib>Viki</bib>,
        <lok><url
        ref="http://eo.wikipedia.org/wiki/Akvo#Biologio">Akvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en julio 1928 li <klr>(John Logie Baird)</klr> faris la
        unuan televidan transsendon en koloro kaj anka&ubreve; per
        trans<tld/>a lumo
        <fnt>
        <aut>Jack Casey</aut>:
        <vrk><url
        ref="http://www.skotlando.org/En%20Esperanto/Skota%20Kulturo/John%20Logie%20Baird.htm"
        >La unua publika elmontro en la mondo de vera
        televido</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="rugx.tran0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kvalifikas te&hcirc;nikojn uzantajn tian radiadon, kaj iliajn
      produktojn:
      <ekz>
        trans<tld/>aj fotoj bildigas surterajn temperaturojn
        <fnt>
        <aut> HORI Jasuo</aut>:
        <vrk>Japanio: Erupcias maltrankvilo</vrk>.
        <bib>Monato</bib>
        </fnt>.
      </ekz>
      <ekz>
        <ind>trans<tld/>a pordo</ind>:
        Pordo, konektita al trans<tld/>a sentilo, ebliganta
        sendi (eligi) a&ubreve; ricevi (enigi) datumojn per trans<tld/>a
        kanalo, ekz-e inter po&scirc;komputilo kaj po&scirc;telefono a&ubreve; 
printilo
        <fnt><bib>KompLeks</bib></fnt>.
        <trd lng="en">infrared port</trd>
        <trd 
lng="ru">&c_i;&c_n;&c_f;&c_r;&c_a;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
        &c_p;&c_o;&c_r;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="viol.trans0a">transviola</ref>
  <trd 
lng="be">&c_ib;&c_n;&c_f;&c_r;&c_a;&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>
  <trd lng="de">infrarot</trd>
  <trd lng="en">infrared</trd>
  <trd lng="es">infrarrojo</trd>
  <trd lng="fr">infrarouge</trd>
  <trd lng="he">&alef;&yod;&nun;&pe;&resh;&alef;-
&alef;&dalet;&vav;&fmem;</trd>
  <trd lng="hu">infrav&ouml;r&ouml;s</trd>
  <trd lng="id">inframerah</trd>
  <trd lng="nl">infrarood</trd>
  <trd lng="pl">podczerwony</trd>
  <trd 
lng="ru">&c_i;&c_n;&c_f;&c_r;&c_a;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">infrar&ouml;d</trd>
  <trd lng="tr">k&inodot;rm&inodot;z&inodot;&ouml;te</trd>
</drv>

</art>
