<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arkite1.xml,v 1.30 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>8</ofc><rad>arkitektur</rad>/o
</kap>
<drv mrk="arkite1.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkite1.0o.BELA">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      La <ref tip="lst" cel="art.bel0o" lst="voko:artoj">belarto</ref>
      de la
      konstruado:
      <ekz>
        kial vi vizitis tiujn urbojn?
        &Ccirc;u por vidi la ma&ubreve;ran <tld/>on
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="arhxitektur.0o">ar&hcirc;itekturo</ref>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>  
  <trd lng="bg">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;</trd> 
  <trdgrp lng="br">      
    <trd>adeiladouriezh</trd>,    
    <trd>tisavouriezh</trd>
  </trdgrp>
  <trd lng="ca">arquitectura</trd>
  <trd lng="de">Architektur</trd>
  <trd lng="en">architecture</trd>
  <trd lng="es">arquitectura</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_m;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">architecture</trd>
  <trdgrp lng="hu">
    <trd>&eacute;p&iacute;t&odblac;m&udblac;v&eacute;szet</trd>,
    <trd>architekt&uacute;ra</trd>
  </trdgrp>
  <trd lng="id">arsitektur</trd>
  <trd lng="it">architettura</trd>
  <trd lng="pl">architektura</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sv">arkitektur</trd>
  <trd lng="sk">architekt√∫ra</trd>
  <trd lng="cs">architektura</trd>
</drv>

<drv mrk="arkite1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="arkite1.0isto.BELA">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sin">
        <ref cel="arhxitektur.0isto">ar&hcirc;itekturisto</ref>,
        <ref cel="arkite1.0o">arkitekto</ref>,
        <ref cel="arhxitekt.0o">ar&hcirc;itekto</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_e;&c_k;&c_t;&c_a;&c_r;</trd> 
  <trd lng="bg">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;</trd>  
  <trdgrp lng="br">      
    <trd>adeiladour</trd>,    
    <trd>tisavour</trd>
  </trdgrp>
  <trd lng="ca">arquitecte</trd>
  <trd lng="de">Architekt</trd>
  <trd lng="en">architect</trd>
  <trd lng="es">arquitecto</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_ayn;&a_m;&a_A;&a_r;</trd>,
    <trd>&a_m;&a_h;&a_n;&a_d;&a_s; <ind>&a_m;&a_ayn;&a_m;&a_A;&a_r;&f_y;</ind></trd>,
    <trd>&a_A_madda;&a_r;&a_sh;&f_y;&a_t;&f_k;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">architecte</trd>
  <trdgrp lng="hu">
    <trd>&eacute;p&iacute;t&odblac;m&udblac;v&eacute;sz</trd>,
    <trd>&eacute;p&iacute;t&eacute;sz</trd>
  </trdgrp>
  <trd lng="id">arsitek</trd>
  <trd lng="it">architetto</trd>
  <trd lng="pl">architekt</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">architekt</trd>
  <trd lng="sv">arkitekt</trd>
</drv>
</art>
