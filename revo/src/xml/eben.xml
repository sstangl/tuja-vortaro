<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eben.xml,v 1.43 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>eben</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="eben.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="eben.0a.plata">
    <dif>
      <klr>(pri supra&jcirc;o)</klr>
      Tia, ke &ccirc;iuj &gcirc;iaj punktoj
      estas proksimume &ccirc;e la sama nivelo sen kava&jcirc;oj
      a&ubreve; elstara&jcirc;oj:
      <ekz>
        la supra&jcirc;o de la maro estas hodia&ubreve; <tld/>a;
      </ekz>
      <ekz>
        mal<tld/>a regiono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>,
      <ref cel="plat.0a">plata</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>eben</trd>,
      <trd>glatt</trd>,
      <trd>flach</trd>,
      <trd>platt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>even</trd>,
      <trd>level</trd>,
      <trd>smooth</trd>,
      <trd>plane</trd>
    </trdgrp>
    <trd lng="fa">&a_h;&a_m;&a_w;&a_A;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>&eacute;gal<klr tip="ind"> (plan)</klr></trd>,
      <trd>plan<klr tip="ind"> (adj.)</klr></trd>,
      <trd>plat<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>s&iacute;k</trd>,
      <trd>sima</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>r&oacute;wny</trd>,
      <trd>p&lstroke;aski</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>plano</trd>,
    <trd>chato</trd>,
    <trd>liso</trd>,
    <trd>igual <klr tip="amb">(superf&iacute;cie)</klr></trd>,
    <trd>ch&atilde;o <klr tip="amb">(adj.)</klr></trd>,
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="eben.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 38</lok></fnt>
    <dif>
      Rilata al <ref cel="eben.0o"><tld/>o</ref>;
      inkluzivata de <tld/>o:
      <ekz>
        <ref cel="simetr.0o.orta"><tld/>a simetrio</ref>;
      </ekz>
      <ekz>
        <tld/>a a&ubreve; ne<tld/>a <ref cel="kurb.0o.MAT">kurbo</ref>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_ib;</trd>
    <trd lng="de">eben</trd>
    <trd lng="en">plane</trd>
    <trd lng="fa">&a_m;&a_s;&a_T;&a_H;</trd>
    <trd lng="fr">plan<klr tip="ind"> (adj., math.)</klr></trd>
    <trd lng="pl">p&lstroke;aszczyznowy</trd>
    <trd lng="pt">plano</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="eben.0a.facila">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facila, prezentanta nenian malhelpa&jcirc;on:
      <ekz>
        flua kaj <tld/>a stilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0a">glata</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="fa">&a_S;&a_A;&a_f;</trd>
    <trd lng="fr">lisse</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;wny</trd>,
      <trd>p&lstroke;aski</trd>
    </trdgrp>
    <trd lng="pt">f&aacute;cil</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>plochý</trd>,
    <trd>rovinný</trd>,
    <trd>rovný</trd>
  </trdgrp>
  <trd lng="de">glatt</trd>
  <trd lng="hu">s&iacute;k-</trd><trd lng="hu">sima</trd>
  <trdgrp lng="nl">
    <trd>vlak<klr tip="ind"> (nw.)</klr></trd>,
    <trd>effen</trd>,
    <trd>plat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladký</trd>,
    <trd>plochý</trd>,
    <trd>rovný</trd>
  </trdgrp>
</drv>

<drv mrk="eben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="eben.0o.elementa">
    <fnt><bib>MatTerm</bib><lok>p. 25</lok></fnt>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, kiu inkluzivas
      &ccirc;iun <ref cel="rekt.0o.elementa">rekton</ref> trairantan du
      punktojn en &gcirc;i:
      <ekz>
        tra tri punktoj povas trairi nur unu <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0o.MAT">
    <dif>
      <ref cel="eben.afina0o">Afina <tld/>o</ref> a&ubreve;
      <ref cel="eben.vektora0o">vektora <tld/>o</ref>,
      la&ubreve; la kunteksto.
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>plocha</trd>,
    <trd>plošina</trd>,
    <trd>rovina</trd>
  </trdgrp>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plane<klr tip="ind"> (geom.)</klr></trd>
  <trd lng="es">plano</trd>
  <trd lng="fa">&a_S;&a_f;&a_H;&a_h;<klr tip="ind"> (&a_h;&a_n;&a_d;&a_s;&a_h;)</klr></trd>
  <trd lng="fr">plan<klr tip="ind"> (subst., g&eacute;om.)</klr></trd>
  <trd lng="hu">s&iacute;k</trd>
  <trd lng="nl">plat <ind>vlak</ind></trd>
  <trd lng="pl">p&lstroke;aszczyzna</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>planina</trd>,
    <trd>plocha</trd>,
    <trd>rovina</trd>
  </trdgrp>
</drv>

<drv mrk="eben.0ajxo">
  <kap><tld/>a&jcirc;o,
    <var><kap><tld/>ejo</kap></var>,
    <var><kap><tld/>o</kap></var>
  </kap>
  <snc mrk="eben.0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Loko a&ubreve; lando <tld/>a:
      <ekz>
        la ruslanda <tld/>o estas
        plilongiga&jcirc;o de la vastega nordazia <tld/>o;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_w;&c_n;&c_ja;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plocha</trd>,
    <trd>plošina</trd>,
    <trd>rovina</trd>
  </trdgrp>
  <trd lng="de">Ebene</trd>
  <trd lng="en">plain</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_sh;&a_t;</trd>,
    <trd>&a_j;&a_l;&f_g;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">plaine</trd>
  <trd lng="hu">s&iacute;ks&aacute;g</trd>
  <trd lng="nl">vlakte</trd>
  <trd lng="pl">r&oacute;wnina</trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>planina</trd>,
    <trd>plocha</trd>,
    <trd>rovina</trd>
  </trdgrp>
</drv>

<drv mrk="eben.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eben.0igi.platigi">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.plata"><tld/>a<sncref/></ref>:
      <ekz>
        <tld/>igi vojon;
      </ekz>
      <ekz>
        &ccirc;io tie estis <tld/>igita kun la tero
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="eben.0igi.FIG">
    <dif>
      Igi <ref tip="vid" cel="eben.0a.facila"><tld/>a<sncref/></ref>:
      <ekz>
        <tld/>igi &ccirc;iujn malfacila&jcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glat.0igi">glatigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>planýrovat</trd>,
    <trd>srovnat povrch</trd>,
    <trd>urovnat</trd>,
    <trd>vyrovnat</trd>
  </trdgrp>
  <trd lng="de">ebenen</trd>
  <trdgrp lng="en">
    <trd>level</trd>,
    <trd>smooth</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_w;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_s;&a_T;&a_H; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">aplanir</trd>
  <trdgrp lng="hu">
    <trd>elegyenget</trd>,
    <trd>plan&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">vlak maken</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;wna&cacute;</trd>,
    <trd>wyr&oacute;wnywa&cacute;</trd>,
    <trd>sp&lstroke;aszcza&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">vyrovnávať</trd>
</drv>

<drv mrk="eben.0bati">
  <kap><tld/>bati<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="E"/>igi batante.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klepáním vyrovnat</trd>,
    <trd>poklepem vyrovnat</trd>,
    <trd>udusat do roviny</trd>,
    <trd>vyklepat do roviny</trd>
  </trdgrp>
  <trd lng="hu">sim&aacute;ra d&ouml;ng&ouml;l</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vyklepať</trd>,
    <trd>vyrovnať na plocho</trd>
  </trdgrp>
</drv>

<drv mrk="eben.alt0ajxo">
  <kap>alt<tld/>a&jcirc;o</kap>
  <snc mrk="eben.alt0ajxo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Ebena&jcirc;o tre alta super la marnivelo.
      <ekz>
        Tibeto estas alt<tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_o;</trd>
  <trd lng="de">Hochebene</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_t;</trd>
  <trd lng="fr">plateau<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="nl">hoogvlakte</trd>
  <trd lng="pl">p&lstroke;askowy&zdot;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_g;&c_o;&c_r;&c_mol;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="eben.duon0o">
  <kap>duon<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      &Ccirc;iu el la du partoj de ebeno, situantaj amba&ubreve;flanke
      de iu rekto inkluzivata de &gcirc;i:
      <ekz>
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_a;&c_w;&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">polorovina</trd>
  <trd lng="de">Halbebene</trd>
  <trd lng="en">half-plane</trd>
  <trd lng="fa">&a_n;&f_y;&a_m;&a_S;&a_f;&a_H;&a_h;</trd>
  <trd lng="fr">demi-plan</trd>
  <trd lng="hu">f&eacute;ls&iacute;k</trd>
  <trd lng="pl">p&oacute;&lstroke;p&lstroke;aszczyzna</trd>
  <trd
  lng="ru">&c_p;&c_o;&c_l;&c_u;&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">polorovina</trd>
</drv>

<drv mrk="eben.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <klr>(pri <ref cel="punkt.0o.spaco">punktoj</ref> a&ubreve; aroj
      da punktoj)</klr>
      Apartenantaj al <klr>(a&ubreve; inkluzivataj de)</klr> unu sama
      <ref cel="eben.0o.MAT"><tld/>o</ref>:
      <ekz>
        tri punktoj estas &ccirc;iam sam<tld/>aj;
      </ekz>
      <ekz>
        sam<tld/>aj kurboj;
      </ekz>
      <ekz>
        &ccirc;iu <ref cel="centr.pezo0o.MAT">pezocentro</ref> de tri
        punktoj
        estas sam<tld/>a kun ili.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_k;&c_a;&c_m;&c_p;&c_l;&c_a;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">ležící ve stejné rovině</trd>
  <trd lng="de">komplanar</trd>
  <trd lng="en">coplanar</trd>
  <trd lng="fa">&a_h;&a_m;&a_S;&a_f;&a_H;&a_h;</trd>
  <trd lng="fr">coplanaire</trd>
  <trdgrp lng="hu">
    <trd>egy <ind>s&iacute;kba</ind> es&odblac;</trd>,
    <trd>k&ouml;z&ouml;s s&iacute;k&uacute;</trd>,
    <trd>komplan&aacute;ris</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;p&lstroke;aszczyznowy</trd>,
    <trd>komplanarny</trd>
  </trdgrp>
  <trd
  lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_a;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ležiaci v rovnakej rovine</trd>
</drv>

<drv mrk="eben.simetri0o">
  <kap>simetri<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="eben.simetri0o.de_simetrio">
    <dif>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, kiu
      difinas
      <tld/>an <ref cel="simetr.0o.orta">simetrion</ref>.
    </dif>
  </snc>
  <snc mrk="eben.simetri0o.de_figuro">
    <dif>
      <klr>(de <ref cel="figur.geometria0o">geometria
      figuro</ref>)</klr>
      <ref tip="super" cel="eben.0o.MAT"><tld lit="E"/>o</ref>, rilate
      al kiu
      la figuro estas <ref cel="simetr.0a.figuro">simetria</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;
  &c_s;&c_y;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>rovina souměrnosti</trd>,
    <trd>rovina symetrie</trd>
  </trdgrp>
  <trd lng="de">Symmetrieebene</trd>
  <trd lng="en">plane of symmetry</trd>
  <trd lng="fa">&a_S;&a_f;&a_H;&a_h;&a_hamza_sure; &a_t;&a_q;&a_A;&a_r;&a_n;</trd>
  <trd lng="fr">plan de sym&eacute;trie</trd>
  <trd lng="hu">szimmetrias&iacute;k</trd>
  <trd lng="pl">p&lstroke;aszczyzna symetrii</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;
  &c_s;&c_i;&c_m;&c_m;&c_je;&c_t;&c_r;&c_i;&c_i;</trd>
  <trd lng="sk">rovina súmernosti</trd>
</drv>

<drv mrk="eben.afina0o">
  <kap>afina <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.afina0o">afina spaco</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_ib;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  <trd lng="de">affine <ind>Ebene</ind></trd>
  <trd lng="en">affine <ind>plane</ind></trd>
  <trd lng="fa">&a_S;&a_f;&a_H;&a_h;&a_hamza_sure; &a_A_madda;&a_f;&f_y;&a_n;</trd>
  <trd lng="fr">plan affine</trd>
  <trd lng="hu">affin <ind>s&iacute;k</ind></trd>
  <trd lng="pl">p&lstroke;aszczyzna afiniczna</trd>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_i;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
</drv>

<drv mrk="eben.vektora0o">
  <kap>vektora <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="dimens.n0a">Dudimensia</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>.
    </dif>
