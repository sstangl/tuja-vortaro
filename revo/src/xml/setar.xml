<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: setar.xml,v 1.10 2013/09/08 18:11:37 revo Exp $">
<kap>
  <rad>setar</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="setar.0o">
  <kap><tld/>o</kap>
  <snc mrk="setar.0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      Persa <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      liuteto origine trikorda, de la 1900aj jaroj kun plusa
      kunvibra kordo, kun ligna duongutforma sonkesto, longa 25-freta
      kolo:
      <ekz>
        la ri&ccirc;uloj turnis sin al tradiciaj instrumentoj,
        kiel ekzemple la <tld/>o
        <fnt>
          <aut>S. Ahmadi, trad. J.-M. Cash</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article1848.html">
          En Gorgan, la irana &ccirc;iutaga vivo</url></vrk>,
          <lok>le monde diplomatique, 2012-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="setar.0o.RIM_PARONIMO">
    Ne konfuzu kun la hinda <ref tip="vid" cel="sitar.0o">sitaro</ref>, 
    kiu probable ricevis sian nomon de setaro, sed en sia nuntempa
    formo sufi&ccirc;e malsimilas.
  </rim>
  <adm>
    2006-06 - kelkaj tradukoj efektive konfuzas kun sitaro.
    <aut>&jcirc;v</aut>
  </adm>
  <trd lng="be">&c_s;&c_ib;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Setar</trd>
  <trd lng="ca">c&iacute;tara</trd>
  <trd lng="en">setar</trd>
  <trd lng="es">setar</trd>
  <trd lng="fa">&a_s;&a_h;&zwnj;&a_t;&a_A;&a_r;</trd>
  <trd lng="fr">set&acirc;r</trd>
  <trd lng="hu">szit&aacute;r</trd>
  <trd lng="ru">&c_s;&c_je;&c_t;&c_a;&c_r;</trd>
</drv>
</art>

