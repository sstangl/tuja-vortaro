<kap>
  <rad>jurisdikci</rad>/o
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jurisdikci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="jurisdikci.0o.JUR">
    <dif>
      Teritoria a&ubreve; tema kompetento de ju&gcirc;ejo:
      <ekz>
        &ccirc;u li ne scias, ke la vagantaj kavaliroj dependas
        de nenia <tld/>o
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>&ccirc;apitro 45a, p. 343a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_ju;&c_r;&c_y;&c_s;&c_d;&c_y;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">jurisdicci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>jurisdikce</trd>,
    <trd>rozsah a obvod soudn&iacute; pravomoci</trd>,
    <trd>soudn&iacute; pravomoc</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jurisdiktion</trd>,
    <trd>rechtsprechende Gewalt</trd>,
    <trd>Judikative</trd>
  </trdgrp>
  <trd lng="en">jurisdiction</trd>
  <trd lng="es">jurisdicci&oacute;n</trd>
  <trd lng="fr">juridiction</trd>
  <trd lng="hu">joghat&oacute;s&aacute;g</trd>
  <trd lng="it">giurisdizione</trd>
  <trd lng="pl">jurysdykcja</trd>
  <trd lng="pt">jurisdi&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>1.s&uacute;dna pr&aacute;vomoc</trd>,
    <trd>2.&uacute;zemie s&uacute;dnej pr&aacute;vomoci</trd>
  </trdgrp>
</drv>
</art>

