<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karmez.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>karmezin</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="karmez.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malhela ru&gcirc;a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref> kun
      viola nuanco.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_m;&c_a;&c_z;&c_y;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_m;&c_a;&c_z;&c_y;&c_n;&c_a;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">tmavočervené barvivo</trd>
  <trd lng="de">Karmesinrot</trd>
  <trdgrp lng="en">
    <trd>crimson</trd>,
    <trd>deep <ind>red</ind><klr> inclining to purple</klr></trd>
  </trdgrp>
  <trd lng="es">carmes&iacute;</trd>
  <trdgrp lng="fr">
    <trd>couleur <ind>cramoisie</ind></trd>,
    <trd>rouge fonc&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karmazsin sz&iacute;n</trd>,
    <trd>lil&aacute;spiros sz&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>karmozijn</trd>,
    <trd>donkerrood</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_m;&c_n;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_i;&c_n;&c_o;&c_v;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_m;&c_a;&c_z;&c_i;&c_n;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">karmazín</trd>
</drv>

<drv mrk="karmez.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malhele ru&gcirc;a kun viola nuanco:
      <ekz>
        Jaspa tuj surmetis botetojn el ru&gcirc;a &scirc;afledo <klr>...</klr> kaj
        <tld/>an silkan mantelon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;&c_z;&c_y;&c_n;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>tmavočervený</trd>,
    <trd>zářivě rudý</trd>
  </trdgrp>
  <trd lng="de">karmesinrot</trd>
  <trd lng="en">crimson</trd>
  <trd lng="es">carmes&iacute;</trd>
  <trd lng="fr">cramoisi</trd>
  <trdgrp lng="hu">
    <trd>karmazsin</trd>,
    <trd>lil&aacute;spiros</trd>
  </trdgrp>
  <trd lng="sk">tmavočervený</trd>
</drv>
</art>
