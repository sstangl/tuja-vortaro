<art mrk="$Id: pantei.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap><ofc>2</ofc><rad>panteism</rad>/o</kap>
<drv mrk="pantei.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
      Filozofia sistemo, kiu identigas Dion kun universo, instruante, ke
      &ccirc;io en materia kaj morala mondo estas nur manifesti&gcirc;o de unika
      substanco.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_t;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">panteisme</trd>
  <trdgrp lng="cs">
    <trd>panteizmus</trd>,
    <trd>všebožství</trd>
  </trdgrp>
  <trd lng="de">Pantheismus</trd>
  <trd lng="en">pantheism</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_h;&zwnj;&a_kh;&a_d;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_w;&a_H;&a_d;&a_t; &a_w;&a_j;&a_w;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">panth&eacute;isme</trd>
  <trd lng="nl">panthe&iuml;sme</trd>
  <trd lng="pt">pante&iacute;smo</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">panteizmus</trd>
</drv>
</art>
