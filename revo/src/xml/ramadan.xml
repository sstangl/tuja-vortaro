<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ramadan.xml,v 1.15 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>ramadan</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ramadan.0o">
  <kap><tld/>o</kap>
  <snc mrk="ramadan.0o.KAL">
    <uzo tip="fak">ISL</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      La na&ubreve;a <ref tip="lst" cel="monat.0o.KAL"
      lst="voko:monatoj" val="9">monato</ref> de la
      <ref tip="vid" cel="islam.0o">islama</ref>
      jaro, dum kies &ccirc;iuj taghoroj la
      islamanoj strikte fastas:
      <ekz>
        ni havas la sanktan monaton, kiu nomi&gcirc;as <tld/>o
        <fnt>
          <aut>M'hammed</aut>,
          <vrk>Ekster Esperantio mi spertas rasismon</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fast.0o">fasto</ref>,
      <ref cel="karesm.0o">Karesmo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">ramad&agrave;</trd>
  <trdgrp lng="cs">
    <trd>ramad치n</trd>,
    <trd>ramaz치n</trd>
  </trdgrp>
  <trd lng="en">Ramadan</trd>
  <trd lng="es">ramad&aacute;n</trd>
  <trd lng="fr">ramadan</trd>
  <trd lng="hu">ramad&aacute;n</trd>
  <trd lng="id">Ramadan</trd>
  <trd lng="nl">ramadan</trd>
  <trd lng="pl">ramadan</trd>
  <trd lng="pt">ramad&atilde;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ramad치n</trd>,
    <trd>ramaz치n</trd>
  </trdgrp>
</drv>
</art>
