<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krev.xml,v 1.17 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krev</rad>/i
</kap>
<drv mrk="krev.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Subite rompi&gcirc;i pro interna tro granda premforto:
      <ekz>
        li ridadis tiel, ke al li preska&ubreve; <tld/>is la
ventro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as bombo, veziko, pafilo, pne&ubreve;matikoj de bicikleto;
      </ekz>
      <ekz>feli&ccirc;o vezike sin levas, sed balda&ubreve; falas kaj
<tld/>as<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>maro &ccirc;iujn riverojn ricevas kaj tamen ne
<tld/>as<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        bulketoj, kiuj <tld/>is de abundego de sekvinberoj<fnt>Z</fnt>;
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        de tro multa scio <tld/>as la
        kranio<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &gcirc;is ekstremeco stre&ccirc;ita kordo de sentoj
<tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malgra&ubreve; la naciismo <tld/>as la landlimoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="eksplo.0i">eksplodi</ref>,
      <ref cel="dis.0igi">disigi</ref>,
      <ref cel="krak.0i">kraki</ref>,
      <ref cel="split">splitigi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>bersten</trd>,
      <trd>zerspringen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>crever<klr tip="ind"> (intransitif)</klr></trd>,
      <trd>&eacute;clater</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>barsten</trd>,
      <trd>springen<klr tip="ind"> (barsten)</klr></trd>,
      <trd>creperen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>p&eogonek;ka&cacute;</trd>,
      <trd>rozsadza&cacute;</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>estourar</trd>,
    <trd>explodir</trd>,
    <trd>pipocar</trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      Fendi&gcirc;i pro malvarmo, sekeco kc.
    </dif>
    <trdgrp lng="de">
      <trd>platzen</trd>,
      <trd>aufplatzen</trd>,
      <trd>Risse bekommen</trd>
    </trdgrp>
    <trd lng="nl">barsten</trd>
    <trdgrp lng="pl">
      <trd>p&eogonek;ka&cacute;</trd>,
      <trd>trzaska&cacute;<klr> (np. o mrozie)</klr></trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>rachar</trd>,
    <trd>fender-se</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_o;&c_p;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_e;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>praskat</trd>,
    <trd>pukat</trd>,
    <trd>rozpraskat</trd>,
    <trd>rozpuknout</trd>
  </trdgrp>
  <trd lng="en">burst</trd>
  <trdgrp lng="es">
    <trd>quebrarse</trd>,
    <trd>romperse</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>praskať</trd>,
    <trd>pukať</trd>,
    <trd>rozpukať</trd>
  </trdgrp>
</drv>

<drv mrk="krev.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Fendo plimalpli profunda en io, kio <tld/>is:
      <ekz>
        <tld/>a&jcirc;oj en muro;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj en la manoj
        <klr>(ka&ubreve;zitaj de malvarmo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="de">
      <trd>Spalt</trd>,
      <trd>Riss</trd>,
      <trd>Furche</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quebradura</trd>,
    <trd>ruptura</trd>
  </trdgrp>
  <trd lng="fr">crevasse</trd>
  <trdgrp lng="pl">
    <trd>p&eogonek;kni&eogonek;cie</trd>,
    <trd>rysa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>barst</trd>,
    <trd>spleet</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="krev.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        <tld/>igi nukson, &scirc;tonon per pulvo, absceson, pordon;
      </ekz>
      <ekz>
        <ind><tld/>iga malvarmo</ind>
        <klr>(tiel kruda, ke &scirc;tonoj <tld/>as)</klr>;
        <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_u;&c_ch;&c_i;&c_j;
&c_m;&c_o;&c_r;&c_o;&c_z;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio <tld/>igas la koron
        <klr>(pro dolora kompato)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxir">kor&scirc;iri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drtit</trd>,
    <trd>rozbíjet</trd>,
    <trd>rozdrtit</trd>,
    <trd>tříštit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knacken</trd>,
    <trd>aufbrechen</trd>,
    <trd>sprengen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quebrar</trd>,
    <trd>romper</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>crever<klr tip="ind"> (transitif)</klr></trd>,
      <trd>faire &eacute;clater</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>barsten</ind></trd>
  <trdgrp lng="pl">
    <trd>powodowa&cacute; <ind>p&eogonek;kanie</ind></trd>,
    <trd>rozsadzi&cacute;</trd>,
    <trd>roztrzaska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozdrviť</trd>,
    <trd>rozpučiť</trd>
  </trdgrp>
</drv>
</art>
