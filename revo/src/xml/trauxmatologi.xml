<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trauxmatologi.xml,v 1.5 2015/10/17 12:10:20 revo Exp $">
<kap>
  <rad>tra&ubreve;matologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trauxmatologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="trauxmatologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Parto de kuracarto, precipe
      <ref tip="vid" cel="hxirur.0o.MED">&hcirc;irurgia</ref>,
      kiu koncernas gravajn vundojn, <ref tip="vid" 
      cel="trauxmat.0o.MED">tra&ubreve;matojn</ref>:
      <ekz>
        la scienco pri diversspecaj vundi&gcirc;oj kaj ilia trakto nomi&gcirc;as 
        <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;tra&ubreve;mato&rightquot;, 
        2008-05-13</lok></fnt>;
      </ekz>
      <ekz>
        la usona registaro malpermesis al usonaj kuracistoj partopreni 
        la 20an Internacian Kongreson de Ortopedio kaj <tld lit="T"/>o
        <fnt>
          <aut>M. Gutierrez Gonz&aacute;lez</aut>,
          <vrk><url
          ref="http://www.radiohc.cu/esperanto/a_resumensemanal/septiembre/09/27/sciencistoj.htm">
          Malspermesas Usono...</url></vrk>,
          <lok>Radio Havano Kubo, 2009-09-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">traumatology</trd>
  <trd lng="fr">traumatologie</trd>
  <trd lng="it">traumatologia</trd>
  <trd lng="nl">traumatologie</trd>
</drv>

<drv mrk="trauxmatologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="trauxmatologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <ref tip="vid"
      cel="trauxmatologi.0o.MED">tra&ubreve;matologio</ref>.
    </dif>
  </snc>
  <trd lng="en">traumatologist</trd>
  <trd lng="fr">traumatologiste</trd>
  <trd lng="it">traumatologo</trd>
  <trd lng="nl">traumatoloog</trd>
</drv>
</art>

