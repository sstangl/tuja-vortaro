<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stip.xml,v 1.21 2015/10/07 17:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stip</rad>/o
</kap>
<drv mrk="stip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="stip.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Stipa</trd>),
      unu specio estas <ref tip="sub" cel="alf.0o">alfo</ref>.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_v;&c_y;&c_l;&c_mol;</trd>
    <trd lng="bg">&c_k;&c_o;&c_i;&c_l;&c_o;</trd>
    <trd lng="de">Federgras</trd>
    <trd lng="fr">stipe</trd>    
    <trdgrp lng="hu">
      <trd>&aacute;rval&aacute;nyhaj</trd>,
      <trd>hajka</trd>
    </trdgrp>
    <trd lng="nl">vedergras</trd>
    <trd lng="ru">&c_k;&c_o;&c_v;&c_y;&c_l;&c_mol;</trd>
    <trd lng="sv">fj&auml;dergr&auml;s</trd>
  </snc>
  <snc mrk="stip.0o.tigo">
    <dif>
      Trunketo, tigo.
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_c;&c_ja;&c_b;&c_l;&c_o;</trd>
    <trdgrp lng="de">
      <trd>Stiel</trd>,
      <trd>St&auml;ngel</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&aacute;r</trd>,
      <trd>ny&eacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_b;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>noha houby</trd>,
    <trd>t&rcaron;e&ncaron; houby</trd>
  </trdgrp>
  <trd lng="sk">noha huby</trd>
</drv>
</art>
