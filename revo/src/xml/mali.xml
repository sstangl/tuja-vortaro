<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mali.xml,v 1.12 2014/06/18 19:10:17 revo Exp $">
<kap>
  <rad>mali</rad>/o
</kap>

<drv mrk="mali.M0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="mali.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la 
      nordokcidenta Afriko kun &ccirc;efurbo Bamako.
      Teritorio de <em>Respubliko Malio</em>.
      ISO-3166-1 kodo: ML.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      Ebur-Bordo,
      <ref tip="vid" cel="gvine.G0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="mauxrit.M0o.GEOG">Ma&ubreve;ritanio</ref>,
      <ref tip="vid" cel="nigxer.N0o.GEOG">Ni&gcirc;ero</ref>
      kaj <ref tip="vid" cel="senegal.S0o.GEOG">Senegalo</ref>.
    </dif>
    <trd lng="be">&c_M;&c_a;&c_l;&c_ib;</trd>
    <trd lng="bg">&c_M;&c_a;&c_l;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>Mali</trd>,
      <trd>Rep&uacute;blica de Mali</trd>
    </trdgrp>
    <trd lng="de">Mali</trd>
    <trd lng="en">Mali</trd>
    <trdgrp lng="fr">
      <trd>Mali</trd>,
      <trd>R&eacute;publique du Mali</trd>
    </trdgrp>
    <trd lng="hu">Mali</trd>
    <trd lng="id">Mali</trd>
    <trd lng="nl">Mali</trd>
    <trdgrp lng="pl">
      <trd>Mali</trd>,
      <trd>Republika <ind>Mali</ind></trd>
    </trdgrp>
    <trd lng="pt">Mali</trd>
  </snc>
</drv>

</art>

