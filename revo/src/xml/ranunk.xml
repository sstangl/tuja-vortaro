<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ranunk.xml,v 1.22 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ranunkol</rad>/o
</kap>
<drv mrk="ranunk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Ranunculus</trd>), tre ofta herbo kun plej ofte
      brilflavaj kvinfoliaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/ranunk.jpg">
    <ind>akra <tld/>o</ind>
    <trd lng="hu">r&eacute;ti <ind>bogl&aacute;rka</ind></trd>
    <trd lng="la">Ranunculus acris</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_z;&c_ja;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_p;</trd>
  </trdgrp>
  <trd lng="ca">ranuncle</trd>
  <trd lng="de">Hahnenfu&szlig;</trd>
  <trd lng="es">ran&uacute;nculo</trd>
  <trd lng="hu">bogl&aacute;rka</trd>
  <trd lng="nl">boterbloem</trd>
  <trdgrp lng="pt">
    <trd>ran&uacute;nculo</trd>,
    <trd>rain&uacute;nculo</trd>,    
    <trd>bot&atilde;o-de-ouro</trd>    
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">iskiernik</trd>
  <trd lng="cs">pryskyřník</trd>
</drv>
</art>
