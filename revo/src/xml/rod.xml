<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rod.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rod</rad>/o
</kap>
<drv mrk="rod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Eksterhavena parto de maro nature a&ubreve; arte &scirc;irmita kontra&ubreve; la
      ventoj,
      kie &scirc;ipoj povas sendan&gcirc;ere haltadi.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_j;&c_d; <klr>(&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kotviště</trd>,
    <trd>rejda</trd>
  </trdgrp>
  <trd lng="de">Reede</trd>
  <trdgrp lng="en">
    <trd>road<klr> (nautical)</klr></trd>,
    <trd>roads<klr> (nautical)</klr></trd>,
    <trd>roadstead</trd>,
    <trd>anchorage</trd>
  </trdgrp>
  <trd lng="fr">rade</trd>
  <trdgrp lng="hu">
    <trd>&ouml;b&ouml;l</trd>,
    <trd>ny&iacute;lt <ind>r&eacute;v</ind></trd>,
    <trd>k&uuml;ls&odblac; <ind>horgonyz&oacute;hely</ind></trd>
  </trdgrp>
  <trd lng="nl">rede</trd>
  <trd lng="pt">enseada</trd>
  <trd lng="ru">&c_r;&c_je;&c_j;&c_d; <klr tip="amb">(&c_m;&c_o;&c_r;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>kotvište</trd>,
    <trd>otvorené prístavisko</trd>,
    <trd>rejda</trd>
  </trdgrp>
</drv>
</art>
