<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klementin.xml,v 1.4 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klementin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klementin.0o">
  <kap><tld/>o</kap>
  <snc mrk="klementin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, dol&ccirc;a, sensema oran&gcirc;eto:
      <ekz>
        mi kutime nomas tiajn oran&gcirc;etojn <tld/>oj
        <fnt>
          <aut>E. Piateski</aut>,
          <vrk>en: <url
          ref="http://vortaro-blogo.blogspot.fr/2009/08/la-nomado-de-diversaj-citrusfruktoj.html">
          vortaro-blogo</url></vrk>,
          <lok>2009-09-06</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klementina</trd>,
    <trd>klementinka</trd>
  </trdgrp>
  <trd lng="en">clementine</trd>
  <trd lng="fr">cl&eacute;mentine</trd>
  <trdgrp lng="pl">
    <trd>klementynka</trd>,
    <trd>mandarynka &sacute;r&oacute;dziemnomorska</trd>
  </trdgrp>
  <trd lng="sk">klementínka</trd>
</drv>

<drv mrk="klementin.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="klementin.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o">Citruso</ref>
      rezultanta de krucigo de mandarino kaj
      dol&ccirc;a oran&gcirc;o (<trd lng="la">Citrus clementina</trd>):
      <ekz>
        <tld/>ujo estas specio de citruso
        proksime parenca al la mandarinujo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Klementinujo">
          klementinujo</url>, 2013-03-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">mandarinka <klr>(strom)</klr></trd>
  <trd lng="fr">cl&eacute;mentinier</trd>
  <trdgrp lng="pl">
    <trd>klementynka</trd>,
    <trd>mandarynka &sacute;r&oacute;dziemnomorska</trd>
  </trdgrp>
  <trd lng="sk">mandarínka <klr>(strom)</klr></trd>
</drv>
</art>

