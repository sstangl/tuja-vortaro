<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sav.xml,v 1.24 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sav</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Liberigi iun el morta a&ubreve; grava dan&gcirc;ero:
      <ekz>
        longa konsidero <tld/>as de sufero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiu sin gardas, tiu sin <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i iun de morto, de fremda edzino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u min de la hundo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is ilin el la manoj de la Egiptoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u min kontra&ubreve; liaj manoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is sin <klr>(forkurante)</klr> sur &ccirc;evalo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u vin <klr>(forkuru)</klr> sur la monton<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>ratowa&cacute;</trd>,
      <trd>wybawia&cacute;</trd>,
      <trd>ocala&cacute;</trd>,
      <trd>zbawia&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Gardi ion de difekto a&ubreve; pereo:
      <ekz>
        tio ne <tld/>os nian aferon el pereo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;io, kio restis <tld/>ita kontra&ubreve; la hajlo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li foriris por <tld/>i sian vivon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dank' al tio la entrepreno povis esti <tld/>ata el absoluta
        malsukceso;
      </ekz>
      <ekz>
        la lerteco de la prezidanto <tld/>is la situacion;
      </ekz>
      <ekz>
        <tld/>i <klr>(konservi netu&scirc;ita)</klr> la dignon;
      </ekz>
      <ekz>
        <tld/>i sian animon
        <klr>(certigi al si postmortan feli&ccirc;on)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>pr&eacute;server</trd>,
      <trd>sauvegarder</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ratowa&cacute;</trd>,
      <trd>ocala&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">salvar</trd>
  <trdgrp lng="cs">
    <trd>uchovat</trd>,
    <trd>uložit</trd>,
    <trd>vkládat</trd>,
    <trd>zapsat do paměti</trd>
  </trdgrp>
  <trd lng="de">retten</trd>
  <trd lng="en">save</trd>
  <trd lng="es">salvar</trd>
  <trd lng="eu">salbatu</trd>
  <trd lng="fr">sauver</trd>
  <trd lng="hu">megment</trd>
  <trd lng="it">salvare</trd>
  <trd lng="nl">redden</trd>
  <trd lng="pt">salvar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_a;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uložiť</trd>,
    <trd>zapísať do pamäte</trd>
  </trdgrp>
  <trd lng="vo">sav&ouml;n</trd>
</drv>

<drv mrk="sav.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sendan&gcirc;erigita vivo.
    </dif>
    <trdgrp lng="ca">
      <trd>rescat</trd>,
      <trd>salvament</trd>
    </trdgrp>
    <trd lng="de">Rettung</trd>
    <trd lng="hu">megment&eacute;s</trd>
    <trdgrp lng="it">
     <trd>salute<klr> (salvezza)</klr></trd>, 
     <trd>salvezza</trd>
     </trdgrp>
    <trdgrp lng="pl">
      <trd>ratunek</trd>,
      <trd>wybawienie</trd>,
      <trd>ocalenie</trd>
    </trdgrp>
    <trd lng="vo">sav</trd>
  </snc>
  <snc>
    <dif>
      Postmorta eterna feli&ccirc;o:
      <ekz>
        ekster la eklezio ne estas <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="arme.sav0o">savarmeo</ref>.
    </refgrp>
    <trd lng="ca">salvaci&oacute;</trd>
    <trd lng="de">Heil</trd>
    <trd lng="hu">&uuml;dv&ouml;z&uuml;l&eacute;s</trd>
    <trd lng="it">salvezza</trd>
    <trd lng="pl">zbawienie</trd>
    <trd lng="vo">san</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>osvobození</trd>,
    <trd>spása</trd>,
    <trd>záchrana</trd>
  </trdgrp>
  <trd lng="es">salvaci&oacute;n</trd>
  <trd lng="fr">salut</trd>
  <trdgrp lng="nl">
    <trd>redding</trd>,
    <trd>heil</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>oslobodenie</trd>,
    <trd>spása</trd>,
    <trd>záchrana</trd>
  </trdgrp>
</drv>

<drv mrk="sav.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">sauvetage</trd>
</drv>

<drv mrk="sav.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sav.0igxi.KOMUNE">
    <dif>
      <tld lit="S"/>i sin mem,
      <ref tip="vid" cel="eskap.0i">eskapi</ref> el
      dan&gcirc;ero:
      <ekz>
        David foriris de tie, kaj <tld/>i&gcirc;is en la kaverno
        <fnt><bib>MT</bib>, <lok>&Sa1;, 22:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>sauver</ind></trd>
</drv>

<drv mrk="sav.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <snc mrk="sav.antaux0i.KOMUNE">
    <dif>
      Anticipe <tld/>i de eventuala dan&gcirc;ero:
      <ekz>
        mi tion faros kun plena neka&scirc;emeco pro tio, ke mi
        anta&ubreve;<tld/>u vin de novaj trompi&gcirc;oj kaj disrevi&gcirc;oj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gard.antaux0i">anta&ubreve;gardi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>prevenir</trd>,
    <trd>preservar</trd>,
    <trd>premunir</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;server</trd>
  <trd lng="it">preservare<klr> (salvezza)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
