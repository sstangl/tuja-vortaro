<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hidroz.xml,v 1.13 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hidrozo</rad>/oj
</kap>
<drv mrk="hidroz.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">Klaso</ref>
       el la filumo <ref tip="super"
      cel="knid.0uloj">kniduloj</ref>
      (<trd lng="la">Hydrozoa</trd>), kies anoj havas du generaciojn de
      la
      formoj <ref tip="prt" cel="meduz.0o">meduzo</ref> kaj
      <ref tip="prt" cel="polip.0o">polipo</ref>.
    </dif>
  </snc>
  <trd lng="ca">hidrozous</trd>
  <trd lng="cs">polyp</trd>
  <trd lng="de">Hydrozoen</trd>
  <trd lng="fr">hydrozoaires</trd>
  <trd lng="hu">hidra&aacute;llatok</trd>
  <trd lng="it">idrozoi</trd>
  <trdgrp lng="nl">
    <trd>kwalpoliepen</trd>,
    <trd>hydro&iuml;dpoliepen</trd>
  </trdgrp>
  <trd lng="pl">stu&lstroke;biop&lstroke;awy</trd>
  <trd lng="pt">hidrozo&aacute;rios</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_i;&c_d;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="sk">polyp</trd>
</drv>
</art>
