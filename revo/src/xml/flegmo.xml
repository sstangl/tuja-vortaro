    <dif>
      Etendita pusa inflamo, abcesego.  
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca">flegm&oacute;</trd>
  <trd lng="cs">flegmona</trd>
  <trd lng="de">Phlegmone</trd>
  <trd lng="es">flem&oacute;n</trd>
  <trd lng="fr">phlegmon</trd>
  <trd lng="hu">phlegmone</trd>
  <trd lng="nl">flegmon</trd>
  <trdgrp lng="pt">
    <trd>flegm&atilde;o</trd>,
    <trd>fleim&atilde;o</trd>,
    <trd>freim&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_o;&c_n;&c_a;</trd>
  <trd lng="sk">zápal podkožného tkaniva</trd>
</drv>
</art>
