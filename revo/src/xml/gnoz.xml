<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gnoz.xml,v 1.7 2015/10/13 13:10:22 revo Exp $">
<kap>
  <rad>gnoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="gnoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="gnoz.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Doktrino de <ref tip="vid" cel="gnostik.0o.REL">gnostikoj</ref>,
      sekretaj instruoj pri diaj kaj transmondaj misteroj la&ubreve; figuraj
      interpretoj de bibliaj libroj kaj aliaj verkoj:
      <ekz>
        Jesuo Kristo ne mortis por niaj pekoj, sed por transdoni al ni
        la
        sekretan doktrinon, la <tld/>on
        <fnt>
        <bib>Viki</bib>,
        <lok>gnostikismo, 2005-07-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">gnosi</trd>
  <trd lng="cs">gn&oacute;ze</trd>
  <trd lng="de">Gnosis</trd>
  <trd lng="en">gnosis</trd>
  <trd lng="fr">gnose</trd>
  <trd lng="pt">gnose</trd>
  <trd lng="sk">gn&oacute;za</trd>
</drv>
</art>

