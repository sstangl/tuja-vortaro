<vortaro>
<art mrk="$Id: ekspoz.xml,v 1.15 2015/10/09 13:10:22 revo Exp $">
<kap>
  <rad>ekspoz</rad>/i
</kap>
<drv mrk="ekspoz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="ekspoz1.0i">ekspozicii.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="ca">exposar</trd>
  <trd lng="cs">vystavovat</trd>
  <trd lng="de">ausstellen</trd>
  <trd lng="en">to exhibit</trd>
  <trd lng="es">exponer</trd>
  <trd lng="fr">exposer<klr tip="ind"> (mettre en
  exposition)</klr></trd>
  <trd lng="hu">ki&aacute;ll&iacute;t</trd>
  <trdgrp lng="pl">
    <trd>eksponowa&cacute;</trd>,
    <trd>wystawia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">expor</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vylo&zcaron;i&tcaron; na prehliadku</trd>,
    <trd>vystavova&tcaron;</trd>
  </trdgrp>
</drv>
</art>
