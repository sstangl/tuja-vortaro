<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: edif.xml,v 1.18 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>edif</rad>/i
</kap>
<drv mrk="edif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni bonan ekzemplon al iu, inspiri piajn a&ubreve; virtajn
      sentojn:
      <ekz>
        lia fervoro <tld/>is &ccirc;iujn &ccirc;eestantojn;
      </ekz>
      <ekz>
        homo, kies vivo <tld/>as &ccirc;iujn;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> prediko;
        <trd
        lng="be">&c_p;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trdgrp lng="de">
          <trd>erbaulich</trd>,
          <trd>aufbauend</trd>
        </trdgrp>
        <trd lng="fr">&eacute;difiant</trd>
        <trdgrp lng="hu">
          <trd>&eacute;p&uuml;letes</trd>,
          <trd>tanuls&aacute;gos</trd>,
          <trd>nevel&odblac; c&eacute;lzat&uacute;</trd>
        </trdgrp>
        <trd lng="nl">stichtelijk</trd>
      </ekz>
      <ekz>
        mi ne amas la <tld/>ajn morala&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dávat příklad</trd>,
    <trd>povznášet</trd>,
    <trd>ušlechtile působit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>erbauen<klr> [moralisch]</klr></trd>,
     <trd>bessern</trd>
  </trdgrp>
  <trd lng="en">edify</trd>
  <trd lng="es">edificar<klr tip="amb"> (moralmente)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_h;&a_dh;&f_y;&a_b; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_kh;&a_l;&a_A;&a_q; &a_A_madda;&a_m;&a_w;&a_kh;&a_t;&a_n;</trd>,
    <trd>&a_A;&a_r;&a_sh;&a_A;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;difier<klr tip="ind"> (inciter &agrave; la
  pi&eacute;t&eacute;)</klr></trd>
  <trdgrp lng="hu">
    <trd>nevel</trd>,
    <trd>jobb&iacute;t</trd>,
    <trd>nemes&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">stichten<klr tip="ind"> (voorbeeld geven)</klr></trd>
  <trdgrp lng="pl">
    <trd>budowa&cacute;<klr> (moralnie)</klr></trd>,
    <trd>podbudowywa&cacute;<klr> (moralnie)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>edificar <klr tip="amb">(moralmente)</klr></trd>,
    <trd>levar &agrave; <ind>virtude</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_z;&c_i;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadchnúť</trd>,
    <trd>povznášať</trd>
  </trdgrp>
</drv>
</art>
