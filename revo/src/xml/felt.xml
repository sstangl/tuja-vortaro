<art mrk="$Id: felt.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>felt</rad>/o
</kap>
<drv mrk="felt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Scirc;tofo ne teksita, farita el kungluitaj kaj kunpremitaj haroj a&ubreve;
      laneroj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_m;&c_je;&c_c;</trd>,
    <trd>&c_f;&c_e;&c_t;&c_a;&c_r;</trd>,
  </trdgrp>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_c;</trd>
  <trd lng="ca">feltre</trd>
  <trdgrp lng="cs">
    <trd>filc</trd>,
    <trd>plsť</trd>
  </trdgrp>
  <trd lng="de">Filz</trd>
  <trd lng="en">felt</trd>
  <trd lng="fr">feutre<klr tip="ind"> (mati&egrave;re)</klr></trd>
  <trdgrp lng="hu">
    <trd>nemez</trd>,
    <trd>filc</trd>
  </trdgrp>
  <trd lng="nl">vilt</trd>
  <trd lng="pt">feltro</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_j;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_f;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>filc</trd>,
    <trd>plsť</trd>
  </trdgrp>
</drv>
</art>
