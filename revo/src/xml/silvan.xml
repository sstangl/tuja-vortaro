<vortaro>
<art mrk="$Id: silvan.xml,v 1.7 2015/02/26 19:10:52 revo Exp $">
<kap>
  <rad>Silvan</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="silvan.0o">
  <kap><tld/>o</kap>
  <snc mrk="silvan.0o.nom">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o, la fidela frato
        <fnt><bib>NT</bib>, <lok>&Pe1; 5:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Silvain</trd>,
    <trd>Sylvain</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Silvan</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Silv&aacute;nus</trd>,
    <trd>Szilv&aacute;nusz</trd>
  </trdgrp>
  <trd lng="it">Silvano</trd>
  <trd lng="nl">Sylvain</trd>
</drv>
</art>

