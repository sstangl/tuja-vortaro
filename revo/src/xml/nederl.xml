<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nederl.xml,v 1.21 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>Nederland</rad>/o
</kap>

<drv mrk="nederl.N0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente kaj norde al la Norda Maro, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref> kaj sude al
      <ref tip="vid" cel="belg.B0ujo">Belgujo</ref>:
      <ekz>
        ekde 1830 Belgio kaj <tld/>o estas apartaj najbaraj
        &scirc;tatoj<fnt><aut>Johan Van Eenoo</aut>: <vrk>La bru&gcirc;a Centuriae
        plantarum rariorum retrovita en Krakovo</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_N;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_G;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_N;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>,
    <trd>&c_H;&c_o;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Izelvroio&ugrave;</trd>,
    <trd>Nederland</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>Nizozemsko</trd>,
    <trd>Nizozem√≠</trd>
  </trdgrp>
  <trd lng="de">Niederlande</trd>
  <trd lng="el">&Omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
      <trd>The <ind>Netherlands</ind></trd>
      <trd>Holland</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Pays-Bas</trd>,
    <trd>Royaume des Pays-Bas</trd>
  </trdgrp>
  <trd lng="hu">Hollandia</trd>
  <trd lng="id">Belanda</trd>
  <trd lng="nl">Nederland</trd>
  <trd lng="no">Nederland</trd>
  <trd lng="pl">Holandia</trd>
  <trdgrp lng="pt">
    <trd>Pa&iacute;ses Baixos</trd>,
    <trd>Holanda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_N;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_G;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">Holandsko</trd>
</drv>

<drv mrk="nederl.0ano">
  <kap><tld lit="n"/>ano</kap>
  <snc mrk="nederl.0ano.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="nederl.N0o"><tld/>o</ref>:
      <ekz>
        <tld/>anoj de surinama deveno<fnt><aut>Roland Rotsaert</aut>:
        <vrk>Piramidaj investoj: ne nur en Albanio!</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 7a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">hollandiai</trd>
  </snc>
  <snc mrk="nederl.0ano.nacieco">
    <dif>
      Homo kun <tld lit="n"/>a nacieco.
    </dif>
    <trd lng="hu">holland</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>
  </trdgrp>      
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="br">Izelvroad</trd>
  <trd lng="de">Niederl&auml;nder</trd>
  <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Dutch</trd>
  <trd lng="fr">n&eacute;erlandais<klr> (subst.)</klr></trd>
  <trd lng="nl">Nederlander</trd>
  <trd lng="no">nederlender</trd>
  <trd lng="pl">Holender</trd>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="nederl.0a">
  <kap><tld lit="n"/>a</kap>
  <snc mrk="nederl.0a.gxen">
    <dif>
      Rilatanta al <tld lit="n"/>anoj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        jen rakonto de <tld/>a adoleskulo, kreskanta tra la periodo de
        la
        dua mondmilito<fnt><aut>Stefan MacGill</aut>: <vrk>Rutine tra
        tumulto</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 4a, p.
        20a</lok></fnt>.
      </ekz>
    </dif>               
    <trd lng="br">izelvroat</trd>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  </snc>
  <snc mrk="nederl.0a.lingv">
    <dif>
      <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> parolata de
      la <ref tip="vid" cel="nederl.0ano"><tld lit="n"/>anoj</ref> kaj
      <ref tip="vid" cel="flandr.0o">flandroj</ref>:
      <ekz>
        estas &gcirc;ojige, ke &ndash; e&ccirc; preska&ubreve; samtempe kun interalie la
        <tld/>a
        versio &ndash; anka&ubreve; en Esperanto aperis lia &jcirc;usa
        studa&jcirc;o<fnt><aut>Gerrit Berveling</aut>: <vrk>Pri kelkaj
        radikoj
        de la kristana kontra&ubreve;judismo (Peter J. Tomson, La afero Jesuo
        kaj
        la Judoj)</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 10a,
        p.
        25a</lok></fnt>.
      </ekz>
    </dif>    
    <trd lng="br">izelvroeg</trd>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&alfa_ton;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="de">niederl&auml;ndisch</trd>
  <trd lng="en">Dutch</trd>
  <trd lng="fr">n&eacute;erlandais<klr> (adj.)</klr></trd>
  <trd lng="hu">holland</trd>
  <trd lng="nl">Nederlands</trd>
  <trd lng="no">nederlandsk</trd>
  <trdgrp lng="pl">
    <trd>niderlandzki</trd>,
    <trd>holenderski</trd>
  </trdgrp>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
