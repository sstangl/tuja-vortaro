<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amanit.xml,v 1.17 2013/10/27 14:10:18 revo Exp $">
<kap>
  <rad>amanit</rad>/o
</kap>
<drv mrk="amanit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref>
       el familio <ref tip="malprt"
      cel="agarik1.0oj">agarikacoj</ref>
      (<trd lng="la">Amanita</trd>), blanksporaj 
      <ref tip="lst" cel="fung.0o.fruktokorpo"
      lst="voko:fungoj">fungoj</ref>: 
      <ekz>kelkaj
      <tld/>oj estas tre venenaj, ekz.
      la <ind>verda <tld/>o</ind>, 
      <trd lng="de">gr&uuml;ner <ind>Knollenbl&auml;tterpilz</ind></trd>
      <trd lng="la">Amanita phalloides</trd>
      <trd lng="nl">groene <ind>knolamaniet</ind></trd>
      </ekz>
      <ekz>la <ind>pantero<tld/>o</ind>
      <trd lng="de">Pantherpilz</trd>
      <trd lng="la">Amanita pantherina</trd>
      <trd lng="nl">panteramaniet</trd>
      </ekz>
      <ekz>kaj la
      <ref tip="sub" cel="fung.musx0o">mu&scirc;fungo</ref>,
      aliaj bongustaj kiel la <ind>perlo<tld/>o</ind>.
      <trd lng="la">Amanita rubescens</trd>
      <trd lng="de">Perlpilz</trd>
      <trd lng="nl">parelamaniet</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_h;&c_a;&c_m;&c_o;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Knollenbl&auml;tterpilz</trd>,
    <trd>Wulstling</trd>
  </trdgrp>
  <trd lng="en">Amanita mushroom</trd>
  <trd lng="fr">amanite</trd>
  <trd lng="hu">gal&oacute;ca</trd>
  <trd lng="nl">amaniet</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_o;&c_m;&c_o;&c_r;</trd>
  <trd lng="sk">muchotr&aacute;vka</trd>
</drv>
</art>
