<vortaro>
<art mrk="$Id: biohxemi.xml,v 1.8 2010/09/18 16:30:19 revo Exp $">
<kap>
  <rad>bio&hcirc;emi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="biohxemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="biohxemi.0o.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Biologia &hcirc;emio, studo de la &hcirc;emiaj substancoj kaj
      procezoj en la viva&jcirc;oj:
      <ekz>
        Asimov laboris kiel profesoro de <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="http://eo.wikipedia.org/wiki/Isaac_Asimov">
          Isaac Asimov</url></vrk>,
          <lok>2010-08-09</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">bioqu&iacute;mica</trd>
  <trd lng="de">Biochemie</trd>
  <trd lng="en">biochemistry</trd>
  <trd lng="es">bioqu&iacute;mica</trd>
  <trd lng="fr">biochimie</trd>
  <trd lng="he">&bet;&yod;&vav;&kaf;&yod;&mem;&yod;&he;</trd>
  <trd lng="nl">biochemie</trd>
  <trd lng="pl">biochemia</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="biohxemi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="biohxemi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">bioqu&iacute;mic</trd>
  <trd lng="de">Biochemiker</trd>
  <trd lng="en">biochemist</trd>
  <trd lng="es">bioqu&iacute;mico</trd>
  <trd lng="fr">biochimiste</trd>
  <trd lng="he">&bet;&yod;&vav;&kaf;&yod;&mem;&alef;&yod;</trd>
  <trd lng="nl">biochemicus</trd>
  <trd lng="pl">biochemik</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_h;&c_i;&c_m;&c_i;&c_k;</trd>
</drv>
</art>
