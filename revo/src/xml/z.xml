<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: z.xml,v 1.9 2015/12/07 10:10:15 revo Exp $">
    <kap>
      <ofc>*</ofc><rad>z</rad>  <fnt><bib>F</bib>,
      <lok>Ekzercaro</lok></fnt>
    </kap>
    <subart>
    <drv mrk="z.0.konsonanto">
        <kap><tld/></kap>
        <uzo tip="fak">LIN</uzo>
        <uzo tip="fak">GRA</uzo>
        <uzo tip="fak">FON</uzo>
        <snc>
        <dif>
        <ref tip="super" cel="sibl.0anto">Sibla</ref>
              <ref tip="super" cel="konson1.0o">konsonanto</ref>, vo&ccirc;a
              <ref tip="super" cel="frot.0sono">frotsono</ref>
              <ref tip="super"
              cel="dent.lang0akonsonanto">lang-denta</ref> [z].
        </dif>
        <trd lng="fr">z<klr> (consonne)</klr></trd>
        </snc>
        <snc>
          <dif>
            La dudekoka (kaj lasta) <ref tip="dif"
            cel="grafem.0o">grafemo</ref> de la
            Esperanta alfabeto, uzata i.a. kiel:
           </dif>
           <trd lng="fr">z<klr> (lettre)</klr></trd>
         <subsnc>
         <dif>
             (maj.) signo de formo: <ekz>Z-fero</ekz>;
         </dif>
         <trd lng="fr">Z<klr> (forme de)</klr></trd>
         </subsnc>
         <subsnc>
             <uzo tip="fak">ESP</uzo>
             <dif> (maj.) mallongigo de <ctl>Zamenhofo</ctl>;
             </dif>
             <trd lng="fr">Z.</trd>
          </subsnc>
          <subsnc>
             <uzo tip="fak">MAT</uzo>
              <dif>(maj. kaj grase) simbolo por la aro de &ccirc;iuj entjeroj
              -2, -1,
              0, 1, 2, ...:
              <em>Z</em>;
              </dif>
              <trd lng="fr">Z<klr> (ensemble des entiers)</klr></trd>
          </subsnc>
          <subsnc>
             <uzo tip="fak">FIZ</uzo>
             <dif>
               (min.) simbolo de zepto.
             </dif>
             <trd lng="fr">z.<klr> (zepto)</klr></trd>
          </subsnc>
        </snc>
   </drv>
   </subart>
   
   <subart>
   <drv mrk="z.0.nomo">
  <kap><tld/>o</kap>
  <snc>  
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif> 
  </snc>
  <trd lng="fr">z</trd>
  </drv>
   </subart>
</art>

