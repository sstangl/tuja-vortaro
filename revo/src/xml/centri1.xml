<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: centri1.xml,v 1.23 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>centripet</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="centri1.0a">
  <kap><tld/>a</kap>
  <snc mrk="centri1.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Movanta, aganta al la centro, alcentrokura.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_v;&c_y;</trd>,
      <trd>&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_ib;&c_m;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">centr&iacute;pet</trd>
    <trdgrp lng="de">
       <trd>zentripetal</trd>,
       <trd>zum <ind>Mittelpunkt</ind> hinstrebend</trd>
    </trdgrp>
    <trd lng="el">&kappa;&epsilon;&ny;&tau;&rho;&omikron;&my;&omikron_ton;&lambda;&omikron;&sigma_fina;</trd>
    <trd lng="en">centripetal</trd>
    <trd lng="es">centr&iacute;peta</trd>
    <trd lng="fr">centrip&egrave;te</trd>
    <trd lng="hu">centripet&aacute;lis</trd>
    <trd lng="it">centripeto</trd>
    <trdgrp lng="nl">
      <trd>middelpuntzoekend</trd>,
      <trd>centripetaal</trd>
    </trdgrp>
    <trd lng="pl">do&sacute;rodkowy</trd>
    <trd lng="pt">centr&iacute;peta</trd>
    <trd lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;&c_o;&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    <trd lng="sk">dostrediv&yacute;</trd>
  </snc>
  <snc mrk="centri1.0a.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <klr>(pri <ref cel="inflor.0o">infloresko</ref>)</klr>
      &Ccirc;e kiu la eksteraj a&ubreve; la malsupraj floroj elvolvi&gcirc;as
      la unuaj.
    </dif>
    <trdgrp lng="ca">
      <trd>centr&iacute;pet</trd>,
      <trd>racem&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>zentripetal</trd>,
       <trd>botrytisch</trd>
    </trdgrp>
    <trd lng="hu">f&uuml;rt&ouml;s<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
    <trd lng="nl">centripetaal</trd>
  </snc>
  <ref tip="ant" cel="centri.0a">centrifuga</ref>
  <trdgrp lng="cs">
    <trd>dostředivý</trd>,
    <trd>přívodný</trd>
  </trdgrp>
</drv>
</art>
