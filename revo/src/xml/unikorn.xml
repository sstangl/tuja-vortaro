<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: unikorn.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
<kap>
   <rad>unikorn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="unikorn.0o">
   <kap><tld/>o</kap>
   <snc>
     <uzo tip="fak">MIT</uzo>
     <dif>
       Mita <ref tip="vid" cel="korn.unu0ulo">unukornulo</ref> el speco
       figurata kun &ccirc;evala korpo kaj rekta frunta korno tre longa:
       <ekz>
         tiu taoisto donis malgrandan oran <tld/>on al nia sinjorido
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 83a, volumo 3a, p.
         39a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_r;&c_o;&c_g;
   <klr tip="amb">(&c_m;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">unicorn</trd>
  <trd lng="cs">jednorožec</trd>
  <trd lng="en">unicorn</trd>
  <trd lng="es">unicornio</trd>
  <trd lng="fi">yksisarvinen
       <klr tip="amb"> (taruel&auml;in)</klr>
   </trd>
  <trd lng="fr">licorne</trd>
  <trdgrp lng="hu">
     <trd>egyszarv&uacute;</trd>,
     <trd>unikornis</trd>
   </trdgrp>
  <trd lng="it">unicorno</trd>
  <trd lng="nl">eenhoorn</trd>
  <trd lng="no">enhj&oslash;rning</trd>
  <trd lng="pl">jednoro&zdot;ec</trd>
  <trd lng="pt">unic&oacute;rnio</trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_r;&c_o;&c_g;</trd>
  <trd lng="sk">jednorožec</trd>
  <trd lng="sv">enh&ouml;rning</trd>
</drv>
</art>

