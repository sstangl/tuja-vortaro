<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jug.xml,v 1.25 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>jug</rad>/o
</kap>
<drv mrk="jug.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Transversa ligna peco, kiun oni ligas &ccirc;e la kornoj
      a&ubreve; sur la kolo de bovoj, por ilin
      <ref cel="jung.0i">jungi</ref>.
    </dif>
    <trd lng="be">&c_ja;&c_r;&c_m;&c_o;</trd>
    <trd lng="bg">&c_i;&c_g;&c_o;</trd>
    <trd lng="en">yoke</trd>
    <trdgrp lng="hu">
      <trd>j&aacute;rom</trd>,
      <trd>iga</trd>
    </trdgrp>    
    <trdgrp lng="pt">
      <trd>canga</trd>,
      <trd>jugo</trd>
    </trdgrp>
    <trd lng="ru">&c_ja;&c_r;&c_m;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Transversa lanco, ligita al du aliaj fiksitaj en la tero, sub kiu
      la
      antikvaj Romanoj devigis la venkitojn trapasi.
    </dif>
    <trd lng="en">Roman yoke</trd>
    <trd lng="hu">r&oacute;mai iga</trd>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sklaveco, servuto, trudo de ia ajn a&ubreve;toritato:
      <ekz>
        iri sub <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        forskui, de&jcirc;eti la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kurbi&gcirc;i sub la <tld/>o de la despotismo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la decoj, devo, le&gcirc;oj;
      </ekz>
      <ekz>
        <ind><tld/>o propravola ne estas malmola</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">&ouml;nk&eacute;nt v&aacute;llalt <ind>iga</ind> nem 
t&ouml;ri a nyakat</trd>
      </ekz>
      <ekz>
        la popolo sin liberigis de (a&ubreve; el sub) la absolutista <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="liber">mallibera</ref>,
      <ref cel="katen.0o">kateno</ref>,
      <ref cel="karcer.0o">karcero</ref>,
      <ref cel="tiran">tiraneco</ref>,
      <ref cel="despot">despoteco</ref>,
      <ref cel="prem.0i">premi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ja;&c_r;&c_m;&c_o;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_jo;&c_t;</trd>,
      <trd>&c_n;&c_ja;&c_v;&c_o;&c_l;&c_ja;</trd>
    </trdgrp>
    <trd lng="en">subjugation</trd>
    <trdgrp lng="hu">
      <trd>iga</trd>,
      <trd>rabiga</trd>
    </trdgrp>
   <trd lng="it">giogo</trd>    
    <trd lng="pt">jugo</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_r;&c_m;&c_o;</trd>,
      <trd>&c_g;&c_n;&c_jo;&c_t;</trd>,
      <trd>&c_i;&c_g;&c_o;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="br">
    <trd>yev</trd>,
    <trd>suj</trd>
  </trdgrp>
  <trd lng="ca">jou</trd>
  <trdgrp lng="cs">
    <trd>jho vychylovací cívky</trd>,
    <trd>třmen</trd>
  </trdgrp>
  <trd lng="de">Joch</trd>
  <trd lng="es">yugo</trd>
  <trdgrp lng="fr">
    <trd>joug</trd>,
    <trd>oppression</trd>,
    <trd>carcan</trd>,
    <trd>servitude</trd>,
  </trdgrp>
  <trd lng="nl">juk</trd>
  <trdgrp lng="sk">
    <trd>jarmo vychyľovacej cievky</trd>,
    <trd>strmeň</trd>
  </trdgrp>
  <trd lng="sv">ok</trd>
</drv>

<drv mrk="jug.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="jug.0i.propre">
    <dif>
      Jungi per <tld/>o.
    </dif>
    <trd lng="ca">junyir</trd>
    <trd lng="fr">enjuguer</trd>
  <trdgrp lng="hu">
    <trd>j&aacute;romba fog</trd>,
    <trd>ig&aacute;ba fog</trd>
  </trdgrp>
    <trd lng="ru">&c_v;&c_p;&c_r;&c_ja;&c_g;&c_a;&c_t;&c_mol; &c_v; 
<ind>&c_ja;&c_r;&c_m;&c_o;</ind></trd>
  </snc>
  <snc mrk="jug.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sklavigi, subpremi.
    </dif>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">lakaat <ind>dindan gazel-yev</ind></trd>
  <trd lng="de">unterjochen</trd>
  <trd lng="en">subjugate</trd>
  <trd lng="es">subyugar</trd>
  <trdgrp lng="fr">
    <trd>subjuguer<klr tip="ind"> (assujettir)</klr></trd>,
    <trd>assujettir</trd>
    <trd>asservir</trd>
    <trd>soumettre<klr tip="ind"> (assujettir)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leig&aacute;z</trd>,
    <trd>ig&aacute;ba t&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>aggiogare</trd>,
      <trd>soggiogare</trd>
    </trdgrp>
  <trd lng="nl">onderdrukken</trd>
  <trd lng="pt">escravizar</trd>
  <trd lng="ru">&c_u;&c_g;&c_n;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="br">
    <trd>bazhyeva&ntilde;</trd>,
    <trd>yeva&ntilde;</trd>,
    <trd>suja&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">subjugar</trd>
  <trd lng="cs">zapřáhnout</trd>
  <trd lng="sk">zapriahnuť</trd>
</drv>

<drv mrk="jug.sub0igi">
  <kap>sub<tld/>igi</kap>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="jug.0i.FIG"><tld/>i <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_l;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">subjugar</trd>
  <trd lng="cs">ujařmit</trd>
  <trd lng="de">unterjochen</trd>
  <trd lng="en">subjugate</trd>
  <trd lng="es">subyugar</trd>
  <trdgrp lng="fr">
    <trd>subjuguer<klr tip="ind"> (assujettir)</klr></trd>,
    <trd>assujettir</trd>
    <trd>asservir</trd>
    <trd>soumettre<klr tip="ind"> (assujettir)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leig&aacute;z</trd>,
    <trd>ig&aacute;ba t&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>aggiogare</trd>,
      <trd>soggiogare</trd>
    </trdgrp>
  <trd lng="nl">onderdrukken</trd>
  <trd lng="pt">escravizar</trd>
  <trd lng="ru">&c_u;&c_g;&c_n;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>podmaniť</trd>,
    <trd>podrobiť</trd>,
    <trd>ujarmiť</trd>,
    <trd>zotročiť</trd>
  </trdgrp>
</drv>
</art>
