<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gratif.xml,v 1.18 2015/10/13 09:10:20 revo Exp $">
<kap>
  <rad>gratifik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gratif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gratif.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni la&ubreve;volan kromsalajron a&ubreve; rekompencan mondonacon:
      <ekz>
        <tld/>i iun per 100 spesoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_e;&c_m;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_y;&c_m;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gratificar</trd>,
    <trd>recompensar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gratifikovat</trd>,
    <trd>mimo&rcaron;&aacute;dn&ecaron; odm&ecaron;nit</trd>,
    <trd>ud&ecaron;lit pen&ecaron;&zcaron;n&iacute; odm&ecaron;nu ,, poskytnout slevu z ochoty</trd>
  </trdgrp>
  <trd lng="de">eine <ind>Sonderzuwendung</ind> machen</trd>
  <trdgrp lng="en">
    <trd>tip</trd>,
    <trd>gratuity</trd>
  </trdgrp>
  <trd lng="es">gratificar</trd>
  <trd lng="fr">gratifier<klr tip="ind"> (une r&eacute;compense
  financi&egrave;re)</klr></trd>
  <trdgrp lng="hu">
    <trd>jutalmaz</trd>,
    <trd>premiz&aacute;l</trd>,
    <trd>gratifik&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>begunstigen</trd>,
    <trd>begiftigen met</trd>,
    <trd>trakteren</trd>
  </trdgrp>
  <trd lng="pt">gratificar<klr tip="amb"> (em dinheiro)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gratifikova&tcaron;</trd>,
    <trd>mimoriadne odmeni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="gratif.0o">
  <kap><tld/>o</kap>
  <snc mrk="gratif.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Krompago, mondono libervola,
      kontraste al regula
      <ref tip="vid" cel="bonifik.0o.EKON">bonifiko</ref>;
      <ref tip="sin" cel="gratif1.0o">gratifikacio</ref>:
      <ekz>
        &ccirc;u venis la <tld/>o de Lia Majesto por la printempa ofero
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 53a, p.
        184a</lok>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="trink.0mono">trinkmono</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_e;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_u;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_a; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gratificaci&oacute;</trd>,
    <trd>recompensa</trd>,
    <trd>propina</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gratifikace</trd>,
    <trd>mimo&rcaron;&aacute;dn&aacute; pen&ecaron;&zcaron;. odm&ecaron;na</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sonderzuwendung</trd>,
    <trd>Gratifikation</trd>,
    <trd>Extraverg&uuml;tung</trd>
  </trdgrp>
  <trd lng="es">gratificaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>gratification<klr tip="ind"> (r&eacute;compense
    financi&egrave;re)</klr></trd>,
    <trd>pourboire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jutalom</trd>,
    <trd>pr&eacute;mium</trd>,
    <trd>gratifik&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gratificatie</trd>,
    <trd>bonus</trd>,
    <trd>drinkgeld</trd>,
    <trd>fooi</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gratyfikacja</trd>,
    <trd>premia</trd>,
    <trd>nagroda</trd>,
    <trd>napiwek</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gratifica&ccedil;&atilde;o<klr tip="amb"> (em dinheiro)</klr></trd>, 
   <trd>b&ocirc;nus<klr tip="amb"> (em dinheiro)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_m;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gratifik&aacute;cia</trd>,
    <trd>mimoriadna pe&ncaron;a&zcaron;n&aacute; odmena</trd>
  </trdgrp>
</drv>
</art>
