<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gland.xml,v 1.26 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gland</rad>/o
</kap>
<drv mrk="gland.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gland.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organo konsistigita de epiteliaj &ccirc;eloj, kies funkcio estas
      <ref tip="vid" cel="sekrec.0i">sekrecii</ref>.
      La&ubreve; la medio, al kiu la <tld/>o ellasas sian sekrecia&jcirc;on, oni
      distingas la <ref tip="prt"
      cel="endokrin.0a.FIZL">endokrinajn</ref>, <ref tip="prt"
      cel="ekzokrin.0a.FIZL">ekzokrinajn</ref> kaj <ref tip="prt"
      cel="amfikrin.0o.FIZL">amfikrinajn</ref> <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_o;&c_z;&c_a;</trd>
  <trd lng="ca">gl&agrave;ndula</trd>
  <trdgrp lng="cs">
    <trd>glandula</trd>,
    <trd>žláza</trd>
  </trdgrp>
  <trd lng="de">Dr&uuml;se</trd>
  <trd lng="en">gland</trd>
  <trd lng="es">gl&aacute;ndula</trd>
  <trd lng="fr">glande</trd>
  <trd lng="it">ghiandola</trd>
  <trd lng="nl">klier</trd>
  <trd lng="pl">gruczo&lstroke;</trd>
  <trd lng="pt">gl&acirc;ndula</trd>
  <trd lng="ro">gland&abreve;</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="sk">žľaza</trd>
  <trd lng="sv">k&ouml;rtel</trd>
  <trd lng="tr">salg&inodot;bezi</trd>
</drv>

<drv mrk="gland.amfikrina0o">
  <kap>amfikrina <tld/>o</kap>
  <snc mrk="gland.amfikrina0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="amfikrin.0o.FIZL">amfikrina <sncref/></ref>
  </snc>
  <trd lng="en">amficrine <ind>gland</ind></trd>
  <trd lng="fr"><ind>glande</ind> amficrine</trd>
</drv>

<drv mrk="gland.ekzokrina0o">
  <kap>ekzokrina <tld/>o</kap>
  <snc mrk="gland.ekzokrina0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="ekzokrin.0a.FIZL">ekzokrina <sncref/></ref>
  </snc>
  <trd lng="cs">exokrinní žláza</trd>
  <trd lng="en">exocrine <ind>gland</ind></trd>
  <trd lng="fr"><ind>glande</ind> exocrine</trd>
  <trd lng="sk">exokrinná žľaza</trd>
</drv>

<drv mrk="gland.endokrina0o">
  <kap>endokrina <tld/>o</kap>
  <snc mrk="gland.endokrina0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="endokrin.0a.FIZL">endokrina <sncref/></ref>
  </snc>
  <trdgrp lng="cs">
    <trd>endokrinní žláza</trd>,
    <trd>žl. s vnitřní sekrecí</trd>
  </trdgrp>
  <trd lng="en">endocrine <ind>gland</ind></trd>
  <trd lng="fr"><ind>glande</ind> endocrine</trd>
  <trdgrp lng="sk">
    <trd>endokrinná žľaza</trd>,
    <trd>žľaza s vnútorným vylučovaním</trd>
  </trdgrp>
</drv>

<drv mrk="gland.surrena0o">
  <kap>surrena <tld/>o</kap>
  <snc mrk="gland.surrena0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la malgrandaj <ref cel="endokrin.0a.FIZL">endokrinaj</ref>
      <tld/>oj, kiuj trovi&gcirc;as super la
      <ref cel="ren.0o">renoj</ref>.
    </dif>
  </snc>
  <trd lng="ca"><ind>gl&agrave;ndula</ind> suprarenal</trd>
  <trd lng="cs">nadledvina</trd>
  <trd lng="de">Nebenniere</trd>
  <trd lng="en">adrenal gland</trd>
  <trd lng="fr">glande surr&eacute;nale</trd>
  <trd lng="it"><ind>ghiandola</ind> surrenale</trd>
  <trd lng="nl">bijnier</trd>
  <trd lng="pl">gruczo&lstroke; nadnerczowy</trd>
  <trd lng="ro">gland&abreve; suprarenal&abreve;</trd>
  <trd lng="sk">nadľadvina</trd>
  <trd lng="sv">binjure</trd>
</drv>
</art>
