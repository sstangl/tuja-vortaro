<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akomod.xml,v 1.20 2015/07/21 19:10:15 revo Exp $">
<kap>
  <rad>akomod</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akomod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akomod.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="gxust.al0igi.KOMUNE">Al&gcirc;ustigi</ref>, <ref 
      tip="vid" cel="adapt.0i">adapti</ref> ion, por ke &gcirc;i povu 
      funkcii:
      <ekz>
        sin <tld/>i al nova situacio;
      </ekz>
      <ekz>
        gravas <tld/>i liajn instruojn al la nuntempa reala&jcirc;o
        <fnt>
          <aut>L. F. Pereira Garupe, trad. &Gcirc;. Vrajto</aut>,
          <vrk><url
          ref="http://www.soulfoodministry.org/docs/Esperanto/Esp_Practices.htm">
          La Kutimoj Jesuaj...</url></vrk>,
          <lok>2007-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acomodar</trd>,
    <trd>adaptar</trd>,
    <trd>adequar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nastavit</trd>,
    <trd>se&rcaron;&iacute;dit</trd>,
    <trd>upravit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>einstellen</trd>,
    <trd><klr>sich </klr>anpassen</trd>,
    <trd>akkommodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>accommodate</trd>,
    <trd>adapt</trd>,
    <trd>adjust</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>acomodar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accommoder</trd>,
    <trd>adapter</trd>
  </trdgrp>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
    <trd>acomodar</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akomodova&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">anpassa</trd>
</drv>

<drv mrk="akomod.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akomod.0igxi.KOMUNE">
    <dif>
      Bone sin akordigi kun io:
      <ekz>
        la homaj okuloj tre rapide <tld/>i&gcirc;as al diversaj distancoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0igxi.KOMUNE">konformi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>acomodar-se</trd>,
    <trd>adaptar-se</trd>,
    <trd>emmotllar-se</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>adapter</ind></trd>
</drv>

<drv mrk="akomod.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="akomod.0igxema.PSI">
    <dif>
      Kiu facile a&ubreve; volonte <tld/>i&gcirc;as al
      homoj, al kondi&ccirc;oj:
      <ekz>
        instruitulo Zhang restis &ccirc;iam kvieta,
        <tld/>i&gcirc;ema kaj neniam malbonkonduta
        <fnt>
          <aut>Yuan Zhen, trad. Guozhu</aut>,
          <vrk><url ref="www.elerno.cn/elibro/cuiying.pdf">
          La Amo de Fra&ubreve;lino Cui YingYing</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>accomodant</trd>,
    <trd>souple<klr> (caract&egrave;re)</klr></trd>
  </trdgrp>
</drv>
</art>
