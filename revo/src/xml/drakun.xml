  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_r;&c_a;&c_g;&c_o;&c_n;</trd>
  <trd lng="ca">estrag&oacute;</trd>
  <trd lng="cs">paldran</trd>
  <trd lng="de">Estragon</trd>
  <trd lng="fr">estragon</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rkony(&uuml;r&ouml;m)</trd>,
    <trd>esztragon</trd></trdgrp>
  <trd lng="nl">dragon</trd>
  <trd lng="pl">estragon</trd>
  <trd lng="pt">estrag&atilde;o</trd>
  <trd lng="ru">&c_e;&c_s;&c_t;&c_r;&c_a;&c_g;&c_o;&c_n;</trd>
  <trd lng="sk">palina draÄia</trd>
  <trd lng="sv">dragon</trd>
</drv>
</art>
