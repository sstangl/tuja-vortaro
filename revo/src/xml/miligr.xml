<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miligr.xml,v 1.16 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>miligram</rad>/o
</kap>
<drv mrk="miligr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso, milono
      de
      <ref tip="malprt" cel="gram.0o">gramo</ref>; simb. mg.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">mil&middot;ligram</trd>
  <trd lng="cs">miligram</trd>
  <trd lng="de">Milligramm</trd>
  <trd lng="en">milligram</trd>
  <trd lng="es">miligramo</trd>
  <trd lng="fr">milligramme</trd>
  <trd lng="hu">milligramm</trd>
  <trd lng="nl">milligram</trd>
  <trd lng="pl">miligram</trd>
  <trd lng="pt">miligrama</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="sk">miligram</trd>
</drv>
</art>
