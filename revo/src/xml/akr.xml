    <trd>breolim</trd>,
    <trd>maen da lemma&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">esmolador</trd>
  <trdgrp lng="cs">
    <trd>brousek</trd>,
    <trd>brus</trd>
  </trdgrp>
  <trd lng="de">Schleifstein</trd>
  <trd lng="en">sharpener</trd>
  <trdgrp lng="es">
    <trd>afilador</trd>,
    <trd>piedra de afilar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&f_y;&a_z;&f_k;&a_n;</trd>,
    <trd>&a_s;&a_A;&a_w;</trd>,
    <trd>&a_f;&a_s;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
