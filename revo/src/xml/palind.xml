<vortaro>
<art mrk="$Id: palind.xml,v 1.13 2015/07/04 08:37:40 revo Exp $">
<kap>
<rad>palindrom</rad>/o
</kap>
<drv mrk="palind.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
     Vorto a&ubreve; frazo, kiu havas saman sencon &ccirc;u legate 
     dekstren &ccirc;u maldekstren:
       <ekz>
       jen <tld/>o: ne mateno, bone tamen.
       </ekz>
    </dif>
    <trd lng="pt">pal&iacute;ndromo</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_n;&c_d;&c_r;&c_o;&c_m;</trd>
  <trd lng="ca">pal&iacute;ndrom</trd>
  <trd lng="cs">palindrom</trd>
  <trd lng="de">Palindrom</trd>
  <trd lng="en">palindrome</trd>
  <trd lng="fr">palindrome</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;k&ouml;rmondat</trd>,
    <trd>palindr&oacute;ma</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omkeerwoord</trd>,
    <trd>spiegelwoord</trd>,
    <trd>palindroom</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_i;&c_n;&c_d;&c_r;&c_o;&c_m;</trd>
  <trd lng="sk">palindrom</trd>
  <trd lng="sv">palindrom</trd>
</drv>
</art>

