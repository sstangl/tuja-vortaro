<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hurd.xml,v 1.14 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hurd</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hurd.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      A&jcirc;ura plata plekta&jcirc;o el bran&ccirc;etoj, latoj a&ubreve;
      metalfadenoj por sekigi froma&gcirc;ojn, fruktojn
      a&ubreve; por aliaj uzoj:
      <ekz>
        inter la trunkoj de kelkaj arboj oni metis <tld/>ojn, fortigis 
        ilin per fostoj en la interspacoj kaj malanta&ubreve; tiu barikado oni
        
        glatigis la teron
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>košatina</trd>,
    <trd>ošatka</trd>,
    <trd>proutěná překážka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hurde</trd>,
    <trd>Horde</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clisse<klr tip="ind"> (claie)</klr></trd>,
    <trd>lattis</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;r&iacute;t&oacute;r&aacute;cs</trd>,
    <trd>l&eacute;sza</trd>
  </trdgrp>
  <trd lng="it">graticcio<klr> (seccatoio)</klr></trd>
  <trd lng="nl">hord</trd>
  <trdgrp lng="pt">
    <trd>latada</trd>,
    <trd>joeira</trd>,
    <trd>cani&ccedil;ado</trd>,
    <trd>cani&ccedil;ada</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">prútená prekážka</trd>
</drv>
</art>
