<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prokra.xml,v 1.23 2016/01/11 21:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prokrast</rad>/i
</kap>
<drv mrk="prokra.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transmeti al pli malfrua tempo agon, kies tujan plenumon oni
      rezignas:
      <ekz>
        la regno <tld/>is subskribi la traktaton;
      </ekz>
      <ekz>ne <tld/>u &gcirc;is morga&ubreve;, kion vi povas fari 
hodia&ubreve;
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        lia varmege sopirata kaj longe <tld/>ata reveno;
      </ekz>
      <ekz>
        la fondado de Ligo montri&gcirc;is tro frua kaj devis esti
        <tld/>ita<fnt>Z</fnt>;
      </ekz>
      <ekz><tld/>ita ne estas perdita,
<fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        ne estas for&jcirc;etita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la katastrofo devis okazi, kaj ne estis plu <tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    
<trd>&c_a;&c_d;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odkl&aacute;dat</trd>,
    <trd>odro&ccaron;it</trd>,
    <trd>posunout</trd>,
    <trd>prot&aacute;hnout</trd>,
    <trd>p&rcaron;elo&zcaron;it</trd>,
    <trd>&ccaron;asov&ecaron; odlo&zcaron;it</trd>
  </trdgrp>
  <trd lng="de">verz&ouml;gern</trd>
  <trd lng="en">procrastinate</trd>
  <trd lng="es">posponer</trd>
  <trd lng="fr">procrastiner</trd>
  <trdgrp lng="he">
    <trd>&lamed;&dalet;&het;&vav;&tav; /<ind>&dalet;&het;&he;</ind>/</trd>,
    <trd>&lamed;<ind>&ayin;&qof;&bet;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elhalaszt</trd>,
    <trd>elod&aacute;z</trd>,
    <trd>elnapol</trd>
  </trdgrp>
  <trd lng="nl">uitstellen</trd>
  <trdgrp lng="pl">
    <trd>odk&lstroke;ada&cacute;</trd>,
    <trd>odracza&cacute;</trd>,
    <trd>zwleka&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>procrastinar</trd>,
    <trd>adiar</trd>,
    <trd>demorar</trd>,
    <trd>transferir</trd>,
    <trd>deter</trd>,
    <trd>dilatar</trd>,
    <trd>prorrogar</trd>,
    <trd>retardar</trd>,
    <trd>protelar</trd>,
  </trdgrp>
  <trd lng="ro">a &icirc;nt&acirc;rzia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_r;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odlo&zcaron;i&tcaron;</trd>,
    <trd>odro&ccaron;i&tcaron;</trd>,
    <trd>odsun&uacute;&tcaron;</trd>,
    <trd>prelo&zcaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="prokra.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        akcepti <tld/>on de sia proceso;
      </ekz>
      <ekz>
        supera oficiro kondamnita al morto kun dujara <tld/>o.
        <fnt>
          <aut>Yu Rongkang</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/581/2015/08/11/179s179061.htm">Eksa
          supera oficiro kondamnita al morto...</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_k;&c_a;</trd>,
    
<trd>&c_a;&c_d;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odklad</trd>,
    <trd>odlo&zcaron;en&iacute;</trd>,
    <trd>odro&ccaron;en&iacute;</trd>,
    <trd>prodlen&iacute;</trd>,
    <trd>pr&uring;tah</trd>,
    <trd>skluz</trd>,
    <trd>zdr&zcaron;en&iacute;</trd>,
    <trd>zpo&zcaron;d&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Verz&ouml;gerung</trd>
  <trd lng="es">posposici&oacute;n</trd>
  <trd lng="fr">procrastination</trd>
  <trdgrp lng="he">
    <trd>&dalet;&het;&yod;&he;</trd>,
    <trd>&ayin;&yod;&qof;&vav;&bet;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>halaszt&aacute;s</trd>,
    <trd>halad&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">uitstel</trd>
  <trdgrp lng="pl">
    <trd>prolongata</trd>,
    <trd>zawieszenie</trd>,
    <trd>zw&lstroke;oka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_d;&c_je;&c_r;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_r;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odklad</trd>,
    <trd>odro&ccaron;enie</trd>,
    <trd>odsun</trd>,
    <trd>prie&tcaron;ah</trd>
  </trdgrp>
</drv>

<drv mrk="prokra.sen0e">
  <kap>sen<tld/>e</kap>
  <snc>
    <dif>
      Tuj:
      <ekz>
        li sen<tld/>e haltigis la vagonaron por eviti
        katastrofon<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>bez pr&uring;tah&uring;</trd>,
    <trd>bezodkladn&ecaron;</trd>,
    <trd>hned</trd>,
    <trd>ihned</trd>,
    <trd>neprodlen&ecaron;</trd>,
    <trd>neprodlen&ecaron;</trd>,
    <trd>obratem</trd>
  </trdgrp>
  <trd lng="de">unverz&uuml;glich</trd>
  <trd lng="fr">sans d&eacute;lai</trd>
  <trd lng="he">&lamed;&lamed;&alef; &dalet;&yod;&het;&vav;&yod;</trd>
  <trd lng="hu">halad&eacute;ktalanul</trd>
  <trd lng="nl">onmiddellijk</trd>
  <trd lng="pl">bezzw&lstroke;ocznie</trd>
  <trd
lng="ru">&c_n;&c_je;&c_z;&c_a;&c_m;&c_je;&c_d;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>bez prie&tcaron;ahov</trd>,
    <trd>bezodkladne</trd>,
    <trd>ihne&dcaron;</trd>
  </trdgrp>
</drv>
</art>
