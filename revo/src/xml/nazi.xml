<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nazi.xml,v 1.13 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>nazi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nazi.0o">
  <kap><tld/>o</kap>
  <snc mrk="nazi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano, subtenanto de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">nacista</trd>
  <trd lng="de">Nazi</trd>
  <trd lng="en">Nazi</trd>
  <trd lng="es">nazi</trd>
  <trd lng="fr">nazi<klr> (subst.)</klr></trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;</trd>
  <trd lng="hu">n&aacute;ci</trd>
  <trd lng="nl">nazi</trd>
  <trdgrp lng="pt">
    <trd>nazista <klr>(subst.)</klr></trd>,
    <trd>nazi <klr>(subst.)</klr></trd>,
    <trd>hitlerista <klr>(subst.)</klr></trd>,
    <trd>hitleriano <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_c;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">nacista</trd>
  <trd lng="sv">nazist</trd>
</drv>

<drv mrk="nazi.0a">
  <kap><tld/>a</kap>
  <snc mrk="nazi.0a.POL">
    <dif>
      Rilata al <tld/>oj, al ilia doktrino kaj agado:
      <ekz>
        <tld/>a studenta unui&gcirc;o
        <fnt><bib>Dan&gcirc;Lng</bib>, <lok>&ccirc;ap. 2.5, p. 117a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_s;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">nationalsozialistisch</trd>
  <trd lng="en">Nazi</trd>
  <trdgrp lng="es">
  	<trd>nazi</trd>,
  	<trd>nacionalsocialista</trd>
  </trdgrp>
  <trd lng="fr">nazi<klr> (adj.)</klr></trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;</trd>
  <trd lng="hu">n&aacute;ci</trd>
  <trd lng="nl">nazistisch</trd>
  <trdgrp lng="pt">
    <trd>nazista <klr>(adj.)</klr></trd>,
    <trd>nazi <klr>(adj.)</klr></trd>,
    <trd>hitlerista <klr>(adj.)</klr></trd>,
    <trd>hitleriano <klr>(adj.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_c;&c_i;&c_s;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">nazistisk</trd>
</drv>

<drv mrk="nazi.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="nazi.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ekstrem-dekstra politika movado ekinta en E&ubreve;ropo en la 1930aj
      jaroj,
      naciisma, diktatorema kaj kruele rasisma aparte kontra&ubreve; judoj,
      unue
      sukcesinta en Germanujo, precipa ka&ubreve;zo de la dua mondmilito.
    </dif>
    <refgrp tip="vid">
      <ref cel="fasxis.0o">fa&scirc;ismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_c;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">nacismus</trd>
  <trd lng="de">Nationalsozialismus</trd>
  <trd lng="en">Nazism</trd>
  <trdgrp lng="es">
  	<trd>nazismo</trd>,
  	<trd>nacionalsocialismo</trd>
  </trdgrp>
  <trd lng="fr">nazisme</trd>
  <trd lng="he">&nun;&alef;&tsadi;&yod;&zayin;&fmem;</trd>
  <trd lng="hu">n&aacute;cizmus</trd>
  <trd lng="nl">nazisme</trd><trd lng="nl">nazismo</trd>
  <trd lng="ru">&c_n;&c_a;&c_c;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">nacizmus</trd>
  <trd lng="sv">nazism</trd>
</drv>
</art>
