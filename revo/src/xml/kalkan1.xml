<kap>
  <rad>kalkane</rad>/o
</kap>
<drv mrk="kalkan1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kalkana osto.  
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>calcani</trd>,
    <trd>calcany</trd>
  </trdgrp>
  <trd lng="cs">patní kost</trd>
  <trd lng="de">Fersenbein</trd>
  <trd lng="en">heel bone</trd>
  <trd lng="es">calc&aacute;neo</trd>
  <trd lng="fr">calcan&eacute;um</trd>
  <trd lng="hu">sarokcsont</trd>
  <trd lng="it">calcagno</trd>
  <trd lng="la">calcaneum</trd>
  <trd lng="nl">hielbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; pi&eogonek;towa</trd>
  <trd lng="pt">calc&acirc;neo</trd>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">pätná kosť</trd>
</drv>
</art>
