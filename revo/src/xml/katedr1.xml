<kap>
  <ofc>2</ofc>
  <rad>katedral</rad>/o
</kap>
<drv mrk="katedr1.0o">
  <kap><ofc>2</ofc><tld/>o</kap>
  <snc>
    <dif>
      Episkopa pre&gcirc;ejo:
      <ekz>
        hispana proverbo diras, ke por konstrui pre&gcirc;ejeton,
        oni devas projekti <tld/>on.
      </ekz>
    </dif>
    <trd lng="en">cathedral<klr> (church with episcopal
    chair)</klr></trd>
    <trd lng="nl">kathedraal</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_e;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_e;&c_d;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_a;&c_b;&c_o;&c_r;</trd>
  </trdgrp>      
  <trd lng="br">iliz-veur</trd>
  <trd lng="ca">catedral</trd>
  <trdgrp lng="de">
     <trd>Dom</trd>,
     <trd>M&uuml;nster</trd>,
     <trd>Kathedrale</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_l;&f_y;&a_s;&a_A;&f_y; &a_j;&a_A;&a_m;&a_ayn;</trd>
  <trd lng="fr">cath&eacute;drale</trd>
  <trdgrp lng="hu">
     <trd>sz&eacute;kesegyh&aacute;z</trd>,
     <trd>katedr&aacute;lis</trd>,
     <trd>d&oacute;m</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_f;&c_je;&c_d;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_s;&c_o;&c_b;&c_o;&c_r;</ind></trd>
  <trd lng="es">catedral</trd>
</drv>
</art>
