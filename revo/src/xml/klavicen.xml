<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klavicen.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klavicen</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap>klaveceno</kap></var>
</kap>
<drv mrk="klavicen.0o">
  <kap><tld/>o</kap>
  <snc mrk="klavicen.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;rop-devena granda klavara
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      populara de la 15a &gcirc;is la 18a jarcentoj,
      en kiu kordojn pin&ccirc;as plumtuboj:
      <ekz>
        la <tld/>o estis tiel malnova, ke ties kordoj kantis da&ubreve;re kaj
        malla&ubreve;te responde al &ccirc;iuj &ccirc;irka&ubreve;e estigitaj sonoj
        <fnt>
        <aut>Konstantin Paustovskij, trad. A. Sidorov</aut>,
        <vrk><url ref="">Blinda kuiristo</url></vrk>,
        <lok>Nica Literatura Revuo, 1957-09 &gcirc;is 10, numero 3/1, p.
        10-14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a nun &ccirc;e Johano / muziko la&ubreve; olda emano 
        <klr>[...]</klr> e&ccirc; se sonoriloj sonoris /
        klaveceno bruon perforis / &gcirc;i estas forta instrumento 
        <fnt>
          <aut>Jan Stanis&lstroke;aw Skorupski</aut>,
          <vrk><url
          ref="www.skorupski.com/sonets/sonet.php?year=2003&amp;month=6&amp;day=29&amp;id=1&amp;lang=eo">Soneto
          &ccirc;ifoje
          johana</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_e;&c_s;&c_ib;&c_n;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_l;&c_a;&c_v;&c_je;&c_s;&c_i;&c_n;</trd>,
    <trd>&c_ch;&c_je;&c_m;&c_b;&c_a;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cembalo</trd>,
    <trd>clavecin</trd>,
    <trd>clavicembalo</trd>,
    <trd>harpsichord</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Cembalo</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harpsichord</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clavecin</trd>
  </trdgrp>
  <trd lng="hu">csembal&oacute;</trd>
  <trd lng="nl">klavecimbel</trd>
  <trd lng="pl">klawesyn</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_je;&c_s;&c_i;&c_n;</trd>
  <trd lng="sk">clavec√≠n</trd>
</drv>

<drv mrk="klavicen.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Senkrura, portebla
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      simila al <ref tip="vid" cel="klavicen.0o"><tld/>o</ref>
      pli&scirc;atata por privata uzo de muzik-diletantoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="spinet.0o">spineto</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_e;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Spinett</trd>,
    <trd>Ottavino</trd>,
    <trd>Virginal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>spinet</trd>,
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pinette</trd>,
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>spin&eacute;t</trd>,
    <trd>virgin&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>virginaal</trd>,
    <trd>spinet</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_je;&c_t;</trd>
</drv>
</art>
