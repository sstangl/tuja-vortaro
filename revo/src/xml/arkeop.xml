<art mrk="$Id: arkeop.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>arkeopterik</rad>/o
</kap>

<drv mrk="arkeop.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkeop.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tre kurioza fosilia besto, kies strukturo pruvas, ke la birdoj
      devenas de la lacertoj kaj la plumoj de la skvamoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_a;&c_p;&c_t;&c_e;&c_r;&c_y;&c_k;&c_s;</trd>
  <trd lng="ca">arque&ograve;pterix</trd>
  <trd lng="cs">archeopteryx</trd>
  <trd lng="de">Archaeopteryx</trd>
  <trd lng="en">archeopteryx</trd>
  <trd lng="es">arqueopteryx</trd>
  <trd lng="fr">arch&eacute;opt&eacute;ryx</trd>
  <trd lng="hu">archeopteryx</trd>
  <trd lng="it">archeopteryx</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_p;&c_t;&c_je;&c_r;&c_i;&c_k;&c_s;</trd>
  <trd lng="sk">archeopteryx</trd>
</drv>
</art>
