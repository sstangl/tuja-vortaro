<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pul1.xml,v 1.8 2016/03/20 09:10:18 revo Exp $">
<kap>
  <rad>pul</rad>/o
  <fnt>
    <bib>PIV2</bib>
  </fnt>
</kap>

<drv mrk="pul1.0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>
        <url ref="http://www.esperanto.be/fel/jar/landoj.php">La landoj
        de l' tero</url>
      </vrk>
  </fnt>
  </kap>
  <snc mrk="pul1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Eta <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref> de <ref
      tip="vid" cel="afgan.A0ujo.GEOG">Afganujo</ref>.
      <ekz>
        1 <ref tip="super" cel="afgani.0o">afganio</ref> = 100
        puloj
        <fnt>
          <vrk>
            <url ref="http://www.esperanto.be/fel/jar/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>
      </ekz>
    </dif>
    </snc>
    <trd lng="ca">pul</trd>
    <trd lng="fr">pul</trd>
    <trdgrp lng="pl">
      <trd>pul</trd>
    </trdgrp>
    <trd lng="nl">pul</trd>
</drv>

</art>

