<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxameleoned.xml,v 1.5 2013/08/31 15:10:23 revo Exp $">
<kap>
  <rad>&hcirc;ameleoned</rad>/oj
</kap>
<drv mrk="hxameleoned.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="hxameleoned.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Rampuloj kun envolvebla vosto,
      el&jcirc;etebla lango por kapti insektojn
      kaj kolor&scirc;an&gcirc;ema ha&ubreve;to,
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">familio</ref> el ordo
      <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Chamaeleonidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_e;&c_l;&c_je;&c_o;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">camale&ograve;ntids</trd>
  <trd lng="de">Cham&auml;leons<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">chamaeleonidae</trd>
  <trdgrp lng="fr">
    <trd>cam&eacute;l&eacute;onid&eacute;s</trd>,
    <trd>chamael&eacute;onid&eacute;s</trd>
  </trdgrp>
  <trd lng="hu">kam&eacute;leonf&eacute;l&eacute;k</trd>
  <trd lng="nl">kameleons<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="ru">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

