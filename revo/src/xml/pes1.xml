<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pes1.xml,v 1.22 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pes</rad>/o
</kap>
<drv mrk="pes1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de Filipinoj kaj de 
pluraj hispanlingvaj landoj de Ameriko.
    </dif>
    <ref tip="prt" cel="centav.0o">centavo.</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_s;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>vážení</trd>,
    <trd>zvážení</trd>
  </trdgrp>
  <trd lng="de">Peso</trd>
  <trd lng="en">peso</trd>
  <trd lng="es">peso</trd>
  <trd lng="fr">peso</trd>
  <trd lng="hu">pez&oacute;</trd>
  <trd lng="id">peso</trd>
  <trd lng="nl">peso</trd>
  <trd lng="pl">peso</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_o;</trd>
  <trd lng="sk">váženie</trd>
</drv>
</art>
