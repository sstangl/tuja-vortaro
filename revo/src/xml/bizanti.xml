<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: bizanti.xml,v 1.7 2015/07/04 08:37:31 revo Exp $">
    <kap><rad>Bizanti</rad>/o<fnt>Z</fnt></kap>

    <drv mrk="bizanti.B0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">HIS</uzo>
        <ref tip="dif" cel="bizanc.0o">Bizanco</ref>
        <dif>
        <ekz>
          oni veturas al Bosforo, por tie halti por kelke da horoj
          kaj rigardi la lokon, kie staris <tld/>o
          <fnt><bib>Fab3</bib>, <lok>Post jarmiloj</lok></fnt>.
        </ekz>
        </dif>
        </snc>
  <trd lng="lat">Byzantium</trd>
  <trd lng="ca">Bizanci</trd>
  <trdgrp lng="cs">
    <trd>Byzancie</trd>,
    <trd>Byzantská říše</trd>
  </trdgrp>
  <trd lng="fr">Byzance</trd>
  <trd lng="it">Bisanzio</trd>
  <trd lng="pl">Bizancjum</trd>
  <trd lng="sk">Byzantská ríša</trd>
</drv>
  </art>

