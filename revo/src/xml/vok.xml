<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vok.xml,v 1.30 2015/10/05 13:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vok</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vok.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.0i.KOMUNE">
    <dif>
      La&ubreve;tigi la vo&ccirc;on por komprenigi al iu, ke li devas veni:
      <ekz>
        <tld/>i helpanton kontra&ubreve; rabisto<fnt>B</fnt>;
      </ekz>
      <ekz>
        al vi, ho viroj, mi <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>ne <tld/>u diablon, &ccirc;ar li povas aperi
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        oni <tld/>as la bovojn ne festeni, sed treni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i por helpo, helpon, al batalo;
      </ekz>
      <ekz>
        kaj en tiu tago la Eternulo <tld/>is, ke oni ploru kaj
        kriu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i sian hundon fajfante;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sonoriloj <tld/>as al pre&gcirc;ejo kaj mem neniam eniras<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vok.0i.alvoki">
    <dif>
      Sciigi al iu, ke li devas veni,
      <ref tip="vid" cel="vok.al0i.iun">al<tld/>i</ref>,
      inviti:
      <ekz>
        <tld/>i iun per gesto, letero;
      </ekz>
      <ekz>
        vi alvenas kvaza&ubreve; <tld/>ita kaj petita<fnt>Z</fnt>
        <klr>(&gcirc;ustatempe)</klr>;
      </ekz>
      <ekz>
        oni <tld/>is min al la kortego;
      </ekz>
      <ekz>
        sendi <tld/>i la kuraciston;
      </ekz>
      <ekz>
        mia sorto min <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tasko <tld/>as min;
      </ekz>
      <ekz>
        multaj <tld/>itoj, sed ne multaj elektitoj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vok.0i.elvoki">
    <ref tip="dif" cel="vok.el0i.FIG_kauxzi">el<tld/>i</ref>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_v;&c_i;&c_k;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_i;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">cridar</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;i&ccaron;et</trd>,
    <trd>obeslat</trd>,
    <trd>volat</trd>,
    <trd>vyzvat</trd>,
    <trd>zavolat</trd>
  </trdgrp>
  <trd lng="de">rufen</trd>
  <trd lng="el">&phi;&omega;&ny;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">call</trd>
  <trd lng="es">llamar</trd>
  <trd lng="fr">appeler</trd>
  <trd lng="he">&lamed;&qof;&resh;&vav;&alef; &lamed;-</trd>
  <trd lng="hu">h&iacute;v</trd>
  <trd lng="nl">roepen</trd>
  <trd lng="oc">apelar</trd>
  <trd lng="pt">chamar</trd>
  <trdgrp lng="sk">
    <trd>vykr&iacute;knu&tcaron;</trd>,
    <trd>zavola&tcaron;</trd>,
    <trd>zvola&tcaron;</trd>
  </trdgrp>
  <trd lng="vo">vok&ouml;n</trd>
</drv>

<drv mrk="vok.0o">
<kap><tld/>o</kap>
  <snc mrk="vok.0o.vokado">
    <dif>
      Ago voki:
      <ekz>
        li ne a&ubreve;dis vian <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Rufen</trd>
    <trd lng="en">call</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_o;&c_v;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;</trd>,
      <trd>&c_k;&c_l;&c_i;&c_ch;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vok.0o.vokiteco">
    <dif>
      Rezulto de la ago voki.
    </dif>
    <trd lng="ca">crida</trd>
    <trd lng="de">Ruf</trd>
    <trd lng="en">calling</trd>
    <trd lng="es">llamada</trd>
    <trd lng="hu">h&iacute;v&aacute;s</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_z;&c_o;&c_v;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;</trd>
    </trdgrp>
    <trd lng="vo">vok</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>instrukce pro vyvol&aacute;n&iacute; programu</trd>,
    <trd>vol&aacute;n&iacute;</trd>,
    <trd>vyvol&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="fr">appel</trd>
  <trdgrp lng="sk">
    <trd>in&scaron;trukcia pre vyvolanie programu</trd>,
    <trd>volanie</trd>,
    <trd>vyvolanie</trd>
  </trdgrp>
</drv>

<drv mrk="vok.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.al0i.iun">
    <dif>
      <tld lit="V"/>i al si:
      <ekz>
        al<tld/>u la laboristojn, kaj donu al ili salajron;
      </ekz>
      <ekz>
        al<tld/>i iun kiel atestanton<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">to summon</trd>
    <trd lng="hu">(mag&aacute;hoz) <ind>h&iacute;v</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vok.al0i.ion">
    <dif>
      Peti ion <tld/>ante:
      <ekz>
        al<tld/>i ven&gcirc;on<fnt>B</fnt>,
      </ekz>
      <ekz>
        helpon;
      </ekz>
      <ekz>
        mi al<tld/>as la ateston de &ccirc;iuj, kiuj partoprenis en la
        kongreso<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al<tld/>i &ccirc;iujn kolekti&gcirc;i sub la standardon de la partio
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admon.0i">admoni</ref>,
      <ref cel="instig.0i">instigi</ref>,
      <ref cel="invit.0i">inviti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">to invoke</trd>
    <trdgrp lng="hu">
      <trd>felh&iacute;v</trd>,
      <trd>felsz&oacute;l&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>cridar</trd>,
    <trd>clamar, invocar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obeslat</trd>,
    <trd>p&rcaron;ivolat</trd>,
    <trd>p&rcaron;ivol&aacute;vat</trd>,
    <trd>vyvol&aacute;vat</trd>,
    <trd>vyzvat</trd>,
    <trd>vyz&yacute;vat</trd>,
    <trd>zavolat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appeler</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&qof;&resh;&vav;&alef; &lamed;-</trd>,
    <trd>&lamed;&bet;&qof;&shin;</trd>
  </trdgrp>
  <trd lng="nl">oproepen</trd>
  <trdgrp lng="sk">
    <trd>privola&tcaron;</trd>,
    <trd>vyzva&tcaron;</trd>,
    <trd>zavola&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vok.al0igxo">
  <kap>al<tld/>i&gcirc;o<fnt>Z</fnt></kap>
  <snc mrk="vok.al0igxo.KOMUNE">
    <dif>
      Inklino por ia profesio.
    </dif>
    <refgrp tip="vid">
      <ref cel="destin.0o">destino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">vocaci&oacute;</trd>
  <trd lng="en">calling</trd>
  <trd lng="fr">vocation</trd>
  <trdgrp lng="hu">
    <trd>hivat&aacute;s</trd>,
    <trd>elhivatotts&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">roeping</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="vok.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.el0i.KOMUNE">
    <dif>
      <tld lit="V"/>i por elirigi:
      <ekz>ne el<tld/>u lupon el la arbaro
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        el<tld/>i iun al duelo;
      </ekz>
      <ekz>
        el<tld/>i mortintojn<fnt>Z</fnt>
        <klr>(per sor&ccirc;o)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to call forth</trd>,
      <trd>summon</trd>
    </trdgrp>
    <trd lng="hu">kih&iacute;v</trd>
    <trd lng="nl">te <ind>voorschijn</ind> roepen</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_kauxzi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Okazigi, estigi, ka&ubreve;zi:
      <ekz>
        tiu &ccirc;i demando per sia naiveco el<tld/>os ridon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu neologismo el<tld/>as konfuzojn kun aliaj
        vortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu propono celas nur el<tld/>i skandalon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu artikolo el<tld/>is malpacon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        multon da intereso;
      </ekz>
      <ekz>
        konflikto, kiu povas el<tld/>i militon;
      </ekz>
      <ekz>
        feli&ccirc;o kaj ri&ccirc;o envion el<tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la propono el<tld/>is &ccirc;e ili la timon, ke...<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vek.0i">veki</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to evoke</trd>,
      <trd>elicit</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kiv&aacute;lt</trd>,
      <trd>el&odblac;id&eacute;z</trd>
    </trdgrp>
    <trd lng="nl">ten <ind>gevolge</ind> hebben</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prezenti al la spirito vivan vidbildon:
      <ekz>
        la propraj nomoj el<tld/>as en ni bildojn pli konkretajn ol la
        komunaj substantivoj;
      </ekz>
      <ekz>
        en poezio oni devas precipe atenti la el<tld/>i-povon de la
        vortoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>cridar (per fer sortir)</trd>,
    <trd>evocar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vyvolat</trd>,
    <trd>vyzvat</trd>
  </trdgrp>
  <trd lng="en">to evoke</trd>
  <trdgrp lng="fr">
    <trd>&eacute;voquer</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&ayin;&lamed;&vav;&tav;</trd>,
    <trd>&lamed;&ayin;&vav;&resh;&resh;</trd>,
    <trd>&lamed;&zayin;&mem;&fnun;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;id&eacute;z</trd>,
    <trd>megjelen&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyvola&tcaron;</trd>
</drv>

<drv mrk="vok.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.kun0i.KOMUNE">
    <dif>
      <tld lit="V"/>i al kunveno:
      <ekz>
        kun<tld/>i la membrojn, kunvenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">convocar</trd>
  <trd lng="cs">svolat</trd>
  <trd lng="de">zusammenrufen</trd>
  <trdgrp lng="en">
    <trd>to convoke</trd>,
    <trd>convene</trd>
  </trdgrp>
  <trd lng="el">&sigma;&ypsilon;&gamma;&kappa;&alfa;&lambda;&omega_ton;</trd>
  <trd lng="es">convocar</trd>
  <trdgrp lng="fr">
    <trd>convoquer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&mem;&fnun;</trd>
  <trd lng="hu">&ouml;sszeh&iacute;v</trd>
  <trd lng="nl">samenroepen</trd>
  <trd lng="pt">convocar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zvola&tcaron;</trd>
</drv>

<drv mrk="vok.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.re0i.KOMUNE">
    <dif>
      <tld lit="V"/>i, por ke iu revenu:
      <ekz>
        re<tld/>i forsenditan serviston<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi uzas la okazon por re<tld/>i en vian memoron la nomon
        de...<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">cridar (per fer tornar)</trd>
  <trdgrp lng="cs">
    <trd>odvolat</trd>,
    <trd>odvol&aacute;vat</trd>,
    <trd>od&rcaron;eknout</trd>,
    <trd>zru&scaron;it</trd>
  </trdgrp>
  <trd lng="de">zur&uuml;ckrufen</trd>
  <trdgrp lng="en"><trd>to revoke</trd>, <trd>summon back</trd></trdgrp>
  <trd lng="fr">rappeler</trd>
  <trd lng="hu">visszah&iacute;v</trd>
  <trd lng="nl">terugroepen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_n;&c_a;&c_z;&c_a;&c_d;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odrieknu&tcaron;</trd>,
    <trd>odvola&tcaron;</trd>,
    <trd>zru&scaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="vok.ju&gcirc;eja0isto">
