<vortaro>
<art mrk="$Id: ara.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>ara</rad>/o
</kap>
<drv mrk="ara.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="papage.0oj">papagedoj</ref>
      (<trd lng="la">Ara</trd>), longvostaj, brilkoloraj amerikaj
      birdoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a; <klr>(&c_p;&c_a;&c_p;&c_u;&c_g;&c_a;&c_j;)</klr></trd>
  <trd lng="cs">papoušek ara</trd>
  <trd lng="de">Ara</trd>
  <trd lng="en">macaw</trd>
  <trd lng="fr">ara</trd>
  <trd lng="hu">arapapag&aacute;ly</trd>
  <trd lng="ru">&c_a;&c_r;&c_a; <klr tip="amb">(&c_p;&c_o;&c_p;&c_u;&c_g;&c_a;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>ara<klr> (papag&aacute;j)</klr></trd>,
    <trd>ara<klr>(ra)</klr> (papagáj ara)</trd>
  </trdgrp>
</drv>
</art>
