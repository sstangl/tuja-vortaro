<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apokop.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>apokop</rad>/o
</kap>
<drv mrk="apokop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Forlaso de unu a&ubreve; kelkaj silaboj en fino de
      vorto:
      <ekz>
        a&ubreve;t(omobil)o, fot(ografa&jcirc;)o, tram(veturil)o.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_p;&c_o;&c_k;&c_o;&c_p;&c_a;</trd>
  <trd lng="ca">ap&ograve;cope</trd>
  <trd lng="cs">apokopa</trd>
  <trdgrp lng="de">
     <trd>Apokope</trd>,
     <trd>Abfall eines
     Auslauts</trd>
  </trdgrp>
  <trd lng="en">apocope</trd>
  <trd lng="es">ap&oacute;cope</trd>
  <trd lng="fr">apocope</trd>
  <trd lng="nl">apocope</trd>
  <trd lng="pl">apokopa</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_k;&c_o;&c_p;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apokopa</trd>,
    <trd>hlások <klr>(aut(omobilo)</klr>o)</trd>,
    <trd>vynechanie slabík</trd>
  </trdgrp>
</drv>
</art>
