<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvarta.xml,v 1.13 2006/09/22 12:27:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvartal</rad>/o
</kap>
<drv mrk="kvarta.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Parto de urbo:
      <ekz>
        lo&gcirc;i en komerca <tld/>o;
      </ekz>
      <ekz>
        la latina (studenta) <tld/>o en Parizo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_u;
&c_g;&c_o;&c_r;&c_a;&c_d;&c_z;&c_je;)</klr></trd>
  <trd lng="ca">barri</trd>
  <trdgrp lng="de">
    <trd>Viertel</trd>,
    <trd>Stadtviertel</trd>,
    <trd>Stadtteil</trd>,
    <trd>Bezirk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>neighborhood</trd>,
    <trd>district</trd>,
    <trd>quarter</trd>
  </trdgrp>
  <trd lng="es">barrio</trd>
  <trd lng="eu">auzo</trd>
  <trd lng="fr">quartier</trd>
  <trd lng="hu">lak&oacute;negyed</trd>
  <trd lng="it">quartiere</trd>
  <trdgrp lng="nl">
    <trd>wijk</trd>,
    <trd>buurt</trd>
  </trdgrp>
  <trd lng="pl">dzielnica</trd>
  <trd lng="pt">bairro</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;
<klr>(&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j;)</klr></trd>
</drv>
</art>
