<vortaro>
<art mrk="$Id: rubi.xml,v 1.14 2013/09/05 14:10:15 revo Exp $">
<kap>
  <rad>rubiac</rad>/oj
</kap>
<drv mrk="rubi.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="rubi.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rubiaceae</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_sh;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">rubi&agrave;cies</trd>
  <trd lng="de">R&ouml;tegew&auml;chse</trd>
  <trd lng="fa">&a_r;&a_w;&a_n;&a_A;&a_s;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">rubiac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>buz&eacute;rf&eacute;l&eacute;k</trd>,
    <trd>galajf&eacute;l&eacute;k</trd>,
    <trd>k&aacute;v&eacute;f&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">walstrofamilie</trd>
  <trd lng="pt">rubi&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_je;&c_n;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr>
  </trd>
</drv>
</art>
