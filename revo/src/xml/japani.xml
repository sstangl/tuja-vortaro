<kap>
  <rad>Japani</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="japani.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <ref tip="dif" cel="japan.0ujo.GEOG">Japanujo</ref>
