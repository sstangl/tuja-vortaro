<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monat.xml,v 1.37 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>monat</rad>/o
</kap>

<drv mrk="monat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="monat.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="dauxr.0o" lst="voko:da&ubreve;roj">Da&ubreve;ro</ref> 
      de unu luna ciklo, proksimume 30 tagoj:
      <ekz>
        post tri <tld/>oj estos la edzi&gcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        voja&gcirc;o de du <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="monat.0o.KAL">
    <dif>
      &Ccirc;iu el la proksimume 30-tagaj partoj la&ubreve; kiuj oni
      dividas la kalendaran <ref tip="malprt"
      cel="jar.0o.KAL">jaron</ref>:
      <ekz>
        Januaro estas la unua <tld/>o de la jaro
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="ekz" cel="ramadan.0o">ramadano</ref>
    <lstref lst="voko:monatoj">monatoj</lstref>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
  <trd lng="bg">&c_m;&c_je;&c_s;&c_je;&c_c;</trd>
  <trd lng="br">miz</trd>
  <trd lng="ca">mes</trd>
  <trdgrp lng="cs">
    <trd>m&ecaron;s&iacute;c</trd>,
    <trd>měsíc <klr>(v kalendáři)</klr></trd>
  </trdgrp>
  <trd lng="de">Monat</trd>
  <trd lng="en">month</trd>
  <trd lng="es">mes</trd>
  <trd lng="fr">mois</trd>
  <trdgrp lng="he">
    <trd>&het;&vav;&dalet;&shin;</trd>,
    <trd>&yod;&resh;&het;<klr tip="ind">
    (&het;&vav;&dalet;&shin;)</klr></trd>
  </trdgrp>
  <trd lng="hu">h&oacute;nap</trd>
  <trd lng="id">bulan</trd>
  <trd lng="nl">maand</trd>
  <trd lng="pl">miesi&aogonek;c</trd>
  <trd lng="pt">m&ecirc;s</trd>
  <trd lng="ro">lun&abreve; <klr>(calendaristic&abreve;)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>
  <trdgrp lng="sk">
    <trd>mesiac <klr>(v kalend&aacute;ri)</klr></trd>,
    <trd>mesiac <klr>(časová jednotka)</klr></trd>
  </trdgrp>
  <trd lng="sv">m&aring;nad</trd>
  <trd lng="tr">ay<klr>(ocak, &scedil;ubat, mart, nisan ...)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#26376;<klr> [yu&egrave;]</klr></trd>,
    <trd>&#26376;&#20221;<klr> [yu&egrave;f&egrave;n]</klr></trd>,
    <trd>&#26376;&#24230;<klr> [yu&egrave;d&ugrave;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="monat.0a">
  <kap><tld/>a</kap>
  <snc mrk="monat.0a.KAL">
    <dif>
      Rilata al <tld/>o, precipe okazanta unufoje en <tld/>o:
      <ekz>
        la societo eldonas <tld/>an gazeton
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">miziek</trd>
  <trd lng="ca">mensual</trd>
  <trd lng="de">monatlich</trd>
  <trd lng="fr">mensuel</trd>
  <trd lng="nl">maandelijks</trd>
  <trd lng="pl">miesi&eogonek;czny</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_je;&c_zh;&c_je;&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">m&aring;natlig</trd>
  <trd lng="tr">ayl&inodot;k</trd>
</drv>

<drv mrk="monat.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="monat.0ajxo.menstruo">
    <ref tip="dif" cel="menstr.0o">menstruo</ref>
    <ekz>
      se iu estas virta kaj agas la&ubreve;le&gcirc;e kaj juste
      <klr>...</klr> ne
      alproksimi&gcirc;as al virino en la tempo de &scirc;ia
      <tld/>a&jcirc;o <klr>...</klr>
      <fnt><bib>MT</bib>, <lok>&Jeh; 18:6</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_e;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>amzerio&ugrave;<klr tip="ind"> (mizio&ugrave;)</klr></trd>,
    <trd>mizio&ugrave;<klr tip="ind"> (amzerio&ugrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>regla</trd>,
    <trd>menstruaci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monatsblutung</trd>,
    <trd>Regel</trd>,
    <trd>Menstruation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&egrave;gles<klr tip="amb"> (menstrues)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>havi v&eacute;rz&eacute;s</trd>,
    <trd>menstru&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>haid</trd>,
    <trd>menstruasi</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>maandstonden</trd>,
    <trd>regel</trd>,
    <trd>menstruatie</trd>
  </trdgrp>
  <trd lng="pl">miesi&aogonek;czka</trd>
  <trdgrp lng="ro">
    <trd>ciclu</trd>,
    <trd>menstre</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">menstruation</trd>
  <trdgrp lng="tr">
    <trd>adet<klr>(kad&inodot;nlar ayda bir adet olur)</klr></trd>,
    <trd>ayba&scedil;&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="monat.tri0o">
  <kap>tri<tld/>o</kap>
  <snc mrk="monat.tri0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <ref tip="dif" cel="kvar.jar0ono.KAL">jarkvarono</ref>
    <ekz>
      oficistoj proponis cedon de po unu salajra horo &ccirc;iun tri<tld/>on
      <fnt>
      <aut>C. Gacond</aut>,
      <vrk><url
      ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego-0019.pdf">19a
      radioprelego</url></vrk>,
      <lok>1963-04-10</lok>
      </fnt>;
    </ekz>
    <ekz>
      plinombrigi la bestkvanton de 600 &gcirc;is 1.000 kokidoj dum la unua
      tri<tld/>o 2005
      <fnt>
      <aut>P. Lovesse, trad. D. Durand</aut>,
      <vrk>Bohicon evoluas !</vrk>,
      <lok><url
      ref="http://www.globidar.org/solidareco/mseo84.htm">monda
      solidareco, informilo 2005-03</url></lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">trimestre</trd>
  <trdgrp lng="de">
    <trd>Trimester</trd>,
    <trd>Quartal</trd>
  </trdgrp>
  <trd lng="fr">trimestre</trd>
  <trdgrp lng="id">
    <trd>kuartal</trd>,
    <trd>trimester</trd>,
    <trd>triwulan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trimester</trd>,
    <trd>kwartaal</trd>
  </trdgrp>
</drv>

<drv mrk="monat.miel0o">
  <kap>miel<tld/>o</kap>
  <snc mrk="monat.miel0o.EDZ">
    <dif>
      La monato a&ubreve; la semajnoj post la edzi&gcirc;o.
      <ekz>
        Dum ilia mielmonato, Anne kaj Kames voja&gcirc;is al New
        Providence...
        <fnt><bib>Viki</bib>
        <vrk><url
        ref="http://eo.wikipedia.org/wiki/Anne_Bonny">Anne
        Bonny</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_je;&c_d;&c_je;&c_n; &c_m;&c_je;&c_s;&c_je;&c_c;</trd>
  <trd lng="ca">lluna de <ind>mel</ind></trd>
  <trd lng="cs">líbánky</trd>
  <trdgrp lng="de">
    <trd>Flitterwochen</trd>
  </trdgrp>
  <trd lng="en">honeymoon</trd>
  <trd lng="id"><ind>bulan</ind> madu</trd>
  <trd lng="nl">wittebroodsweken</trd>
  <trd lng="sk">medové týždne</trd>
  <trd lng="sv">smekm&aring;nad</trd>
  <trd lng="tr">bal ay&inodot;</trd>
</drv>
</art>
