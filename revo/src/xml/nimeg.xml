  <trd lng="fr">Nim&egrave;gue</trd>
  <trd lng="hu">Nijmegen</trd>
  <trd lng="nl">Nijmegen</trd>
  <trd lng="it">Nimega</trd>
  <trd lng="lat">Noviomagus</trd>
  <trdgrp lng="pt">
    <trd>Nijmegen</trd>,
    <trd>Nimega</trd>,
    <trd>Nimegue</trd>
  </trdgrp>
  </drv>
</art>

