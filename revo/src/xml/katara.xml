<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katara.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katarakt</rad>/o
</kap>
<drv mrk="katara.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Maldiafani&gcirc;o de la okullenso, ka&ubreve;zanta kuraceblan blindecon.  
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;</trd>
  <trd lng="ca">cataracta</trd>
  <trdgrp lng="cs">
    <trd>katarakt</trd>,
    <trd>peřeje</trd>,
    <trd>stupňovitý vodopád</trd>
  </trdgrp>
  <trdgrp lng="de">
  <trd>Katarakt</trd>, 
  <trd>Grauer Star</trd>
  </trdgrp>
  <trd lng="en">cataract</trd>
  <trd lng="es">catarata</trd>
  <trd lng="fr">cataracte<klr tip="ind"> (m&eacute;d.)</klr></trd>
  <trd lng="hu">sz&uuml;rkeh&aacute;lyog</trd>
  <trd lng="id">katarak</trd>
  <trdgrp lng="nl">
    <trd>cataract</trd>,
    <trd>grauwe <ind>staar</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>beľmo</trd>,
    <trd>katarakta</trd>,
    <trd>šedý očný zákal</trd>
  </trdgrp>
</drv>
</art>
