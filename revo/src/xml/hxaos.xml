<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxaos.xml,v 1.36 2014/10/24 15:10:24 revo Exp $">
<kap>
     <ofc>*</ofc>
     <rad>&hcirc;aos</rad>/o
</kap>
<drv mrk="hxaos.0o">
     <kap><ofc>*</ofc><tld/>o</kap>
     <snc mrk="hxaos.0o.MIT">
       <uzo tip="fak">MIT</uzo>
       <dif>
	 La&ubreve; la greka kaj aliaj antikvaj konceptoj pri
	 mondorigino, konfuza stato de &ccirc;iuj elementoj de la
	 universo, anta&ubreve; la organizado al ili de la dioj.
       </dif>
       <trd lng="ca">Caos</trd>
       <trdgrp lng="de">
	 <trd>Chaos</trd>,
	 <trd>Unordnung</trd>
       </trdgrp>
       <trdgrp lng="fi">
	 <trd>kaaos <klr tip="amb"> (mytol.)</klr></trd>
       </trdgrp>
       <trd lng="no">kaos</trd>
       <trd lng="pl">chaos</trd>
       <trd lng="pt">caos<klr tip="amb"> (mitologia)</klr></trd>
       <trd lng="it">Caos</trd>
     </snc>
     <snc mrk="hxaos.0o.FIG">
       <uzo tip="stl">FIG</uzo>
       <dif>
	 Senorda konfuzo:
	 <ekz>
	   <tld/>o de ruinoj;
	 </ekz>
	 <ekz>
	   internacia lingvo estas nepre necesa por forigi la lingvan
	   <tld/>on<fnt>Z</fnt>;
	 </ekz>
	 <ekz>
	   bolanta <tld/>o da vanaj imaga&jcirc;oj;
	 </ekz>
	 <ekz>
	   la konferenco fini&gcirc;is en <tld/>o, sen iuj rezultoj
	   <fnt><aut>Roland Rotsaert</aut>:
	   <vrk>Jarmila konferenco &hcirc;aose malsukcesis</vrk>,
	   <bib>Monato</bib><lok>jaro 2000a, numero 2a, p. 10a</lok>
	   </fnt>.
	 </ekz>
       </dif>
       <trd lng="ca">caos</trd>
       <trdgrp lng="de">
	 <trd>Chaos</trd>,
	 <trd>Durcheinander</trd>,
	 <trd>Unordnung</trd>,
	 <trd>Tohuwabohu</trd>
       </trdgrp>
       <trdgrp lng="fi">
	 <trd>kaaos</trd>,
	 <trd>sekasorto</trd>,
	 <trd>ep&auml;j&auml;rjestys</trd>
       </trdgrp>
       <trdgrp lng="no">
	 <trd>kaos</trd>,
	 <trd>rot</trd>
       </trdgrp>
       <trdgrp lng="pl">
	 <trd>chaos</trd>,
	 <trd>zamieszanie</trd>,
	 <trd>rozgardiasz</trd>,
	 <trd>ba&lstroke;agan</trd>,
	 <trd>burdel<klr> (pot., wulg.)</klr></trd>
       </trdgrp>
       <trd lng="pt">caos<klr tip="amb"> (f)</klr></trd>
       <trd lng="it">caos</trd>
     </snc>
     <trd lng="be">&c_h;&c_a;&c_o;&c_s;</trd>
     <trd lng="de">Chaos</trd>
     <trd lng="en">chaos</trd>
     <trd lng="es">caos</trd>
     <trd lng="fr">chaos</trd>
     <trd lng="hu">k&aacute;osz</trd>
     <trd lng="id">kaos</trd>
     <trd lng="nl">chaos</trd>
     <trd lng="ru">&c_h;&c_a;&c_o;&c_s;</trd>
     <trd lng="sk">chaos</trd>
     <trd lng="sv">kaos</trd>
</drv>

<drv mrk="hxaos.0a">
     <kap><tld/>a</kap>
     <snc mrk="hxaos.0a.MIT">
       <dif>
	 Rilata al <tld/>o.
       </dif>
     </snc>
     <snc mrk="hxaos.0a.FIG">
       <uzo tip="stl">FIG</uzo>
       <dif>
	 Senorda:
	 <ekz>
	   la tujaj reagoj al tiu okazinta&jcirc;o estis panikaj kaj
	   <tld/>aj<fnt><aut>Stefan Maul</aut>: 
	   <vrk>En giganta truo</vrk>,
	   <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 6a</lok>
	   </fnt>.
	 </ekz>
       </dif>
     </snc>
     <trd lng="be">&c_h;&c_a;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
     <trdgrp lng="de">
       <trd>chaotisch</trd>,
       <trd>durcheinander</trd>,
       <trd>ungeordnet</trd>,
       <trd>unordentlich</trd>
     </trdgrp>
     <trd lng="ca">ca&ograve;tic</trd>
     <trd lng="en">chaotic</trd>
     <trdgrp lng="fi">
       <trd>sekasortoinen</trd>,
       <trd>kaoottinen</trd>,
       <trd>sotkuinen</trd>
     </trdgrp>
     <trd lng="es">ca&oacute;tico</trd> 
     <trd lng="fr">chaotique</trd>
     <trd lng="hu">kaotikus</trd>
     <trd lng="id">kaotis</trd>
     <trd lng="nl">chaotisch</trd>
     <trd lng="no">kaotisk</trd>
     <trd lng="pl">chaotyczny</trd>
     <trd lng="pt">ca&oacute;tico</trd>
     <trdgrp lng="ru">
       <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
       </trd>,
       <trd>&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
     </trdgrp>
     <trd lng="sk">chaotick&yacute;</trd>
     <trd lng="sv">kaotisk</trd>
     <trd lng="it">caotico</trd>
</drv>

<drv mrk="hxaos.0e">
     <kap><tld/>e</kap>
     <snc>
       <dif>
	 Malordege:
	 <ekz>
	   tiu multegekosta transportado parte okazis <tld/>e
	   <fnt><aut>Stefan Maul</aut>:
	    <vrk>En giganta truo</vrk>,
	   <bib>Monato</bib><lok>jaro
	   1999a, numero 9a, p. 6a</lok></fnt>.
	 </ekz>
       </dif>
     </snc>
     <trd lng="be">&c_h;&c_a;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;</trd>
     <trd lng="ca">ca&ograve;ticament</trd>
     <trd lng="de">chaotisch</trd>
     <trd lng="en">chaotically</trd>
     <trdgrp lng="fi">
       <trd>sekasortoisesti</trd>,
       <trd>kaoottisesti</trd>,
       <trd>sotkuisesti</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>chaotiquement</trd>
     </trdgrp>
     <trd lng="nl">chaotisch</trd>
     <trd lng="no">kaotisk</trd>
     <trd lng="pl">chaotycznie</trd>
     <trd lng="pt">caoticamente</trd>
     <trd lng="ru">&c_h;&c_a;&c_o;&c_t;&c_i;&c_ch;&c_n;&c_o;</trd>
     <trd lng="sv">kaotiskt</trd>
     <trd lng="it">caoticamente</trd>
</drv>

<drv mrk="hxaos.0igi">
     <kap><tld/>igi</kap>
     <snc>
       <dif>
	 Malordigi, konfuzi &gcirc;is plena neregeblo:
	 <ekz>
	   la agresemaj radikaluloj tial ne nur ne sukcesis <tld/>igi
	   a&ubreve; malebligi la traktadojn de financistoj, sed
	   samokaze ili perdis la simpation de la &ccirc;e&hcirc;a socio
	   <fnt><aut>Ji&rcaron;i Patera</aut>:
	   <vrk>Post la kunsido de financistoj en Prago</vrk>,
	   <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 12a</lok>
	   </fnt>.
	 </ekz>
       </dif>
     </snc>
     <trdgrp lng="ca">
       <trd>embullar</trd>,
       <trd>generar el <ind>caos</ind></trd>
     </trdgrp>
     <trd lng="en">cause <ind>chaos</ind></trd>
     <trdgrp lng="fi">
       <trd>sotkea t&auml;ysin</trd>,
       <trd>tehd&auml; <klr>(jostakin)</klr> <ind>kaaos</ind></trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>chambouler</trd>,
       <trd>d&eacute;sorganiser</trd>,
       <trd>d&eacute;structurer</trd>
     </trdgrp>
     <trd lng="pl">robi&cacute;
     <ind>chaos</ind><klr> (zamieszanie)</klr></trd>
     <trd lng="pt">caotizar</trd>
     <trdgrp lng="ru">
       <trd>&c_d;&c_je;&c_z;&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
       <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_k; <ind>&c_h;&c_a;&c_o;&c_s;&c_u;</ind></trd>
     </trdgrp>
     <trdgrp lng="sv">
       <trd>oordna</trd>,
       <trd>orsaka <ind>kaos</ind></trd>
     </trdgrp>
     <trdgrp lng="it">
       <trd>mettere a <ind>soqquadro</ind></trd>,
       <trd>incasinare<klr> (volg.)</klr></trd>
     </trdgrp>
</drv>
</art>
