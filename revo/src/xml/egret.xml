<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: egret.xml,v 1.17 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>egret</rad>/o
</kap>
<drv mrk="egret.0o">
  <kap><tld/>o</kap>
  <snc mrk="egret.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tufo da surkapaj plumoj de iuj birdoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="tuf.plum0o">plumtufo</ref>
  </refgrp>
  <trd lng="ca">plomall</trd>
  <trd lng="de">Federkrone</trd>
  <trd lng="fr">aigrette<klr tip="ind"> (touffe de plumes)</klr></trd>
  <trd lng="hu">b&oacute;bita</trd>
  <trd lng="nl">aigrette</trd>
  <trdgrp lng="pl">
    <trd>pi&oacute;ropusz</trd>,
    <trd>czub</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>penacho</trd>,
    <trd>topete <klr tip="amb">(de ave)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;&c_o;&c_k;<klr tip="ind"> (&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>,
    <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n; <klr tip="amb">(&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>
  </trdgrp>
  <trd lng="sk">iskrenie el. pri vysokom napätí</trd>
  <trdgrp lng="cs">
    <trd>chochol</trd>,
    <trd>hřeben</trd>
  </trdgrp>
</drv>
</art>
