<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stras.xml,v 1.14 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stras</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="stras.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juvel-imita&jcirc;o el senvalora materialo:
      <ekz>
        l' urbon ornamas ne gemoj sed <tld/>o
        <fnt>
          <aut>Krys Ungar</aut>,
          <vrk>Meznokto metropola</vrk>,
          <lok>p. 97</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="fals.0i">falsi</ref>,
    <ref cel="tombak.0o">tombako</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_z;</trd>,
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_a;&c_g;&c_a;
    &c_k;&c_a;&c_m;&c_je;&c_n;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Strass</trd>
  </trdgrp>
  <trd lng="es">bisuter&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>faux bijou</trd>,
    <trd>strass</trd>,
    <trd>toc<klr> (subst., faux)</klr></trd>,
    <trd>verroterie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>strassz</trd>,
    <trd>dr&aacute;gak&odblac;ut&aacute;nzat</trd>,
    <trd>hamis <ind>gy&eacute;m&aacute;nt</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stras</trd>,
    <trd>namaakjuweel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bijuteria</trd>,
    <trd>mi&ccedil;anga</trd>,
    <trd>"<ind>strass</ind>"</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>strass</trd>,
    <trd>talmovina</trd>,
    <trd>umelé šperky</trd>
  </trdgrp>
  <trd lng="cs">strass</trd>
</drv>

<drv mrk="stras.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El imita&jcirc;o de valora materialo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;efo <klr>...</klr> taksas la planon &leftquot;<tld/>a labora&ccirc;o&rightquot;
        <fnt>
        <aut>Irl</aut>,
        <vrk>&Ccirc;u polica plano nelica?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 11a, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_z;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>aus Strass</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>factice</trd>,
    <trd>faux<klr> (adj., bijou, m&eacute;tal)</klr></trd>,
    <trd>tape-&agrave;-l'oeil</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hamis</trd>,
    <trd>ut&aacute;nzat</trd>
  </trdgrp>
  <trd lng="nl">namaak-</trd>
  <trd lng="pt">de <ind>bijuteria</ind></trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
