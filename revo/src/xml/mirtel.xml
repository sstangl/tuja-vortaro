<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirtel.xml,v 1.19 2015/08/14 06:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mirtel</rad>/o
</kap>
<drv mrk="mirtel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="vakcin1.0o">vakcinio</ref>
      (<trd lng="la">Vaccinium myrtillus</trd>), kun blue nigraj,
      man&gcirc;eblaj, vitaminri&ccirc;aj beroj.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_ib;&c_c;&c_y;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;&c_o;&c_v;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">lusenn</trd>
  <trd lng="ca">nabinera</trd>
  <trdgrp lng="cs">
    <trd>bor&uring;vka</trd>,
    <trd>bor&uring;vka &ccaron;ern&aacute;</trd>,
    <trd>brusnice bor&uring;vka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heidelbeere</trd>,
    <trd>Blaubeere</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bilberry</trd>,
    <trd>blueberry</trd>
  </trdgrp>
  <trd lng="es">ar&aacute;ndano</trd>
  <trd lng="fr">myrtille</trd>
  <trd lng="hu">fekete <ind>&aacute;fonya</ind></trd>
  <trd lng="nl">blauwe <ind>bosbes</ind></trd>
  <trdgrp lng="pl">
    <trd>bor&oacute;wka czarna</trd>,
    <trd>jagoda czarna</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>brusnica oby&ccaron;ajn&aacute;</trd>,
    <trd>&ccaron;u&ccaron;oriedka</trd>
  </trdgrp>
</drv>
</art>
