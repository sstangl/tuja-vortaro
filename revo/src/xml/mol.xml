<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mol.xml,v 1.37 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mol</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mol.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="mol.0a.cedema">
    <dif>
      Ne kontra&ubreve;staranta al premo, facile cedanta:
      <ekz>
        <tld/>a kiel pasto;
      </ekz>
      <ekz>
        <tld/>a viando, <tld/>e kuirita ovo;
      </ekz>
      <ekz>
        <tld/>a pano, argilo, lano, kuseno, kanapeto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        karaktero <tld/>a kiel vakso<fnt>Z</fnt>
        <klr>(kp <ref cel="ole.0a">olea</ref>)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="fleks.0ebla">fleksebla</ref>,
      <ref cel="kned">knedebla</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd>blot</trd>,
      <trd>bouk</trd>,
      <trd>gwak</trd>
    </trdgrp>
    <trd lng="es">blando</trd>
  </snc>
  <snc mrk="mol.0a.milda">
    <dif>
      Ne akra, milda:
      <ekz>
        <tld/>a karaktero, vo&ccirc;o;
      </ekz>
      <ekz>
        <tld/>aj (neprecizaj) konturoj.
      </ekz>
    </dif>
  <trd lng="es">suave</trd>
  </snc>
  <snc mrk="mol.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <klr>(Parolsono)</klr> karakterizata per levo de la langodorso
      al la palato; <ref tip="sin" cel="palat.0a">palata</ref>.
    </dif>
    <trd lng="br">glebiet<klr tip="ind"> (soniadoniezh)</klr></trd>
    <trd lng="fr">mouill&eacute;<klr tip="amb"> (phonologie)</klr></trd>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_m;&c_je;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>hebký</trd>,
    <trd>m&ecaron;kk&yacute;</trd>,
    <trd>měkký</trd>
  </trdgrp>
  <trd lng="de">weich</trd>
  <trd lng="en">soft</trd>
  <trd lng="fa">&a_n;&a_r;&a_m;</trd>
  <trd lng="fr">mou</trd>
  <trd lng="nl">zacht</trd>
  <trd lng="pl">mi&eogonek;kki</trd>
  <trdgrp lng="pt">
   <trd>mole</trd>,
   <trd>fofo</trd>,
   <trd>suave</trd>,
   <trd>macio</trd>,
   <trd>brando</trd>,
   <trd>frouxo</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mäkký</trd>,
    <trd>poddajný</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="mol.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a a&ubreve; <tld/>a parto de io:
      <ekz>
        la <ind><tld/>a&jcirc;o de pano</ind><fnt>B</fnt>,
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_i;&c_sh;</trd>       
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;o de frukto</ind> <fnt><bib>PV</bib></fnt>.
        <trd lng="en">pulp</trd>
        <trd lng="pl">mi&aogonek;&zdot;sz</trd>
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>       
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">mou<klr tip="amb"> (subst., partie molle)</klr></trd>
  <trdgrp lng="pl">
    <trd>mi&aogonek;&zdot;sz</trd>,
    <trd>mi&eogonek;kisz</trd>,
    <trd>s&lstroke;abizna</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mol.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi vakson per varmo;
      </ekz>
      <ekz>
        <tld/>igi (kompatigi, mildigi) la koron de iu;
      </ekz>
      <ekz>
        <uzo tip="fak">FON</uzo>
        la sono j ordinare <tld/>igas la konsonanton, kiu
        staras anta&ubreve; &gcirc;i
        <fnt><bib>LR</bib>, <lok></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>boukaat</trd>,
    <trd>gwakaat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>měkčit</trd>,
    <trd>obměkčovat</trd>
  </trdgrp>
  <trd lng="es">ablandar</trd>
  <trd lng="fa">&a_n;&a_r;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">amollir</trd>
  <trd lng="pl">zmi&eogonek;kcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obmäkčiť</trd>,
    <trd>zmäkčiť</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mol.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        lia koro <tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>amollir</ind></trd>,
    <trd>se <ind>ramollir</ind></trd>
  </trdgrp>
</drv>

<drv mrk="mol.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="mol.mal0a.fizike">
    <dif>
      Kiu senti&gcirc;as &ccirc;e palpado kiel kompakta, malcedema,
      kontra&ubreve;staranta al premo:
      <ekz>
        mal<tld/>a viando, pano, ligno, &scirc;tono;
      </ekz>
      <ekz>
        mal<tld/>e kuirita ovo;
      </ekz>
      <ekz>
        mal<tld/>a se&gcirc;o;
      </ekz>
      <ekz>
        mal<tld/>aj haroj.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="mol.0a.cedema">mola <sncref/></ref>,
      <ref cel="fleks.0ebla">fleksebla</ref>,
      <ref cel="kned">knedebla</ref>
    </refgrp>
  </snc>
  <snc mrk="mol.mal0a.morale">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senindulda, senkompata, maldelikata, rezista:
      <ekz>
        mal<tld/>a koro;
      </ekz>
      <ekz>
        neceseco kontra&ubreve;vola estas le&gcirc;o mal<tld/>a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bronz.0a">bronza</ref>,
      <ref cel="fer.0a">fera</ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="rigid.0a">rigida</ref>,
      <ref cel="sever.0a">severa</ref>,
      <ref cel="sxton.0a">&scirc;tona</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>
  <trd lng="bg">&c_t;&c_v;&c_malmol;&c_r;&c_d;</trd>
  <trd lng="br">kalet</trd>
  <trd lng="cs">tvrdý</trd>
  <trd lng="de">hart</trd>
  <trd lng="en">hard</trd>
  <trd lng="es">duro</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kh;&a_t;</trd>,
    <trd>&a_s;&a_f;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">dur</trd>
  <trd lng="nl">hard</trd>
  <trd lng="pl">twardy</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">tvrdý</trd>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="mol.sxvel0a">
  <kap>&scirc;vel<tld/>a</kap>
  <snc mrk="mol.sxvel0a.KOMUNE">
    <dif>
      Dike, elaste <tld/>a:
      <ekz>
        &scirc;vel<tld/>a tapi&scirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">moelleux</trd>
</drv>

<drv mrk="mol.0matura">
  <kap><tld/>matura</kap>
  <uzo tip="klr">(pri frukto)</uzo>
  <snc mrk="mol.0matura.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="M"/>i&gcirc;inta kaj duone putri&gcirc;anta pro
      tromatureco.
    </dif>
  </snc>
  <trd lng="br">pezhell</trd>
  <trd lng="en">overripe</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&f_y;&a_A;&a_d;&a_r;&a_s;&f_y;&a_d;&a_h;<klr tip="ind">
    (&a_m;&f_y;&a_w;&a_h;)</klr></trd>,
    <trd>&a_l;&a_h;&f_y;&a_d;&a_h;<klr tip="ind">
    (&a_m;&f_y;&a_w;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="fr">blet</trd>
  <trd lng="nl">overrijp</trd>
  <trd lng="pl">przejrza&lstroke;y</trd>
</drv>
</art>
