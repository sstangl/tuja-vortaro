<vortaro>
<art mrk="$Id: tink.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tink</rad>/o
</kap>
<drv mrk="tink.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Bongusta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      vivanta surfunde de malrapidflua a&ubreve; senflua nesala akvo,
      kun ha&ubreve;to malhele olivkolora, muka kun malgrandaj skvamoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Tinca</trd>), 
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;</trd>
  <trd lng="ca">tenca</trd>
  <trd lng="cs">lín obecný</trd>
  <trd lng="de">Schleie</trd>
  <trd lng="fr">tanche</trd>
  <trdgrp lng="hu">
    <trd>comp&oacute;</trd>,
    <trd>cig&aacute;nyhal</trd></trdgrp>
  <trd lng="nl">zeelt</trd>
  <trd lng="pl">lin</trd>
  <trdgrp lng="pt">
    <trd>tenca</trd>,
    <trd>carpa</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_mol;</trd>
  <trd lng="sk">lieň</trd>
</drv>
</art>
