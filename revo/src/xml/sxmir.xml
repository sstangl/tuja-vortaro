<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sxmir.xml,v 1.24 2010/11/19 17:30:40 revo Exp $">
    <kap>
      <ofc>*</ofc> <rad>&scirc;mir</rad>/i
    </kap>

    <drv mrk="sxmir.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="sxmir.0i.surmeti">
        <dif>
          Froti sur ion maldikan tavolon da grasa a&ubreve; 
          alglui&gcirc;anta substanco:
          <ekz>
            mi forlavis de vi vian sangon, kaj mi <tld/>is vin per oleo
            <fnt><bib>MT</bib>, <lok>&Jeh; 16:9</lok></fnt>;
          </ekz><ekz>
            mi detruos la muron, kiun vi <tld/>is per malta&ubreve;ga mortero
            <fnt><bib>MT</bib>, <lok>&Jeh; 13:14</lok></fnt>;
          </ekz><ekz>
            bulkon per butero<fnt>Z</fnt>;
          </ekz><ekz>
            mi <tld/>os per veneno mian glavon
            <fnt><aut>&Scirc;ekspiro, tr. &Z;</aut>: <vrk>Hamleto</vrk></fnt>;
          </ekz><ekz>
            la falsa <tld/>o kovros la makulon<fnt>Z</fnt>;
          </ekz><ekz>
            dol&ccirc;e <tld/>i al iu la lipojn
            <klr>(f: multe promesi al iu.
              <refgrp tip="vid">
                <ref cel="miel.0o">mielo</ref>,
                <ref cel="or.0a">ora</ref>
              </refgrp>)</klr>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <rim>
          Fojfoje renkonti&gcirc;as iom dubinda permuto de la signifoj de
          objekto kaj komplemento:
          <ekz>
            li <tld/>as buteron sur pantran&ccirc;on
            <fnt><aut>&Scirc;timec</aut>:
              <vrk>Ombro sur interna pejza&gcirc;o</vrk></fnt>.
          </ekz>
        </rim>
        <trdgrp lng="be">
          <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_a;&c_b;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trd lng="de">schmieren</trd>
        <trdgrp lng="fr">
          <trd>graisser</trd>,
          <trd>&eacute;taler<klr> (une substance grasse)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>beken</trd>,
          <trd>bed&ouml;rzs&ouml;l<klr> (ken&odblac;ccsel)</klr></trd>
        </trdgrp>
        <trdgrp lng="it">
          <trd>ungere</trd>,
          <trd>ingrassare<klr> (ungere)</klr></trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>smarowa&cacute;</trd>,
          <trd>posmarowa&cacute;</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>friccionar</trd>,
          <trd>ungir</trd>,
          <trd>besuntar</trd>,
          <trd>lambuzar</trd>    
        </trdgrp>
        <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="sxmir.0i.lubriki">
        <dif>
          <ref tip="dif" cel="lubrik.0i">Lubriki</ref>:
          <ekz>
            kiu bone <tld/>as
            <klr>(la radojn. Vortludo, Vd <tld/>i 3)</klr>
            bone veturas
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_z;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>schmieren</trd>,
          <trd>&ouml;len</trd>
        </trdgrp>
        <trd lng="hu">ken</trd>
        <trd lng="it">lubrificare</trd>
        <trd lng="ru">&c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="sxmir.0i.EKON">
        <uzo tip="fak">EKON</uzo>
        <uzo tip="stl">FIG</uzo>
        <ref tip="dif" cel="acxet.sub0i">suba&ccirc;eti</ref>
        <ekz>
          <tld/>i iun<fnt>Z</fnt>.
        </ekz>
        <trd lng="be">&c_p;&c_a;&c_d;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
        <trdgrp lng="de">
          <trd>schmieren</trd>,
          <trd>bestechen</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>acheter<klr tip="ind"> (soudoyer)</klr></trd>,
          <trd>corrompre<klr tip="ind"> (par de l'argent)</klr></trd>,
          <trd>payer<klr tip="ind"> (soudoyer)</klr></trd>,
          <trd>graisser la patte (&agrave;)</trd>,
          <trd>soudoyer</trd>,
          <trd>suborner<klr tip="ind"> (par de l'argent)</klr></trd>
        </trdgrp>
        <trd lng="hu">megken<klr tip="ind"> (p&eacute;nzzel)</klr></trd>
        <trdgrp lng="it">
          <trd>ungere<klr> (corrompere)</klr></trd>,
          <trd>corrompere</trd>
        </trdgrp>
        <trd lng="nl">omkopen</trd>
        <trdgrp lng="pl">
          <trd>smarowa&cacute;</trd>,
          <trd>posmarowa&cacute;</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>subornar</trd>,
          <trd>molhar a m&atilde;o</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_o;&c_d;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;
          <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
      </snc>
      <trd lng="en">smear</trd>
      <trd lng="nl">smeren</trd>
      <trd lng="es">untar</trd>
      <trd lng="ca">untar</trd>
    </drv>

    <drv mrk="sxmir.cxirkaux0i">
      <kap>&ccirc;irka&ubreve;<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <tld lit="&Scirc;"/>i &ccirc;e la &ccirc;irka&ubreve;o:
          <ekz>
            starigu al vi grandajn &scirc;tonojn kaj &ccirc;irka&ubreve;<tld/>u
            ilin per kalko
            <fnt><bib>MT</bib>, <lok>&Rea; 27:2</lok></fnt>.
          </ekz>
        </dif>
      </snc>
    </drv>

    <drv mrk="sxmir.paper0i">
      <kap>paper<tld/>i</kap>
      <snc mrk="sxmir.paper0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Skribi multajn sed senvalorajn aferojn:
          <ekz>
            trovi&gcirc;os plumknaristo, paper<tld/>isto, kiu e&ccirc; 
            en komedion vin enmetos
            <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 8a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">paperaji&ntilde;</trd>
      <trdgrp lng="de">
        <trd>aufs Papier schmieren</trd>,
        <trd>hinschmieren</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>faire de la <ind>paperasse</ind></trd>,
        <trd>gratter le papier</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>firk&aacute;l</trd>,
        <trd>csak pazarolja a <ind>pap&iacute;rt</ind></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>scarabocchiare<klr> (fig.)</klr></trd>, 
        <trd>scribacchiare<klr> (senza valore)</klr></trd>
      </trdgrp>
      <trd lng="nl">bekladden</trd>
      <trdgrp lng="pl">
        <trd>smarowa&cacute;<klr> (pisa&cacute; g&lstroke;upoty)</klr></trd>,
        <trd>obsmarowa&cacute;<klr> kogo&sacute; (pisa&cacute; o kim&sacute;
            &zacute;le)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_a;&c_r;&c_a;&c_t;&c_mol;
        &c_b;&c_u;&c_m;&c_a;&c_g;&c_u;</trd>
    </drv>
  </art>
