<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suficx.xml,v 1.25 2014/11/08 16:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sufi&ccirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="suficx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="suficx.0a.KOMUNE">
    <dif>
      Estanta en tia kvanto, kia estas bezona:
      <ekz>
        unu fajrero estas <tld/>a por eksplodigi pulvon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>suficient</trd>,
    <trd>adequat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausreichend</trd>,
    <trd>gen&uuml;gend</trd>,
    <trd>hinreichend</trd>,
    <trd>hinl&auml;nglich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>enough</trd>,
    <trd>sufficient</trd>,
    <trd>adequate</trd>
  </trdgrp>
  <trd lng="es">suficiente</trd>
  <trd lng="fr">suffisant</trd>
  <trd lng="he">&mem;&samekh;&pe;&yod;&qof;</trd>
  <trdgrp lng="hu">
    <trd>el&eacute;g</trd>,
    <trd>el&eacute;gs&eacute;ges</trd>,
    <trd>elegend&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>genoeg</trd>,
    <trd>voldoende</trd>
  </trdgrp>
  <trd lng="no">nok</trd>
  <trdgrp lng="pl">
    <trd>dostateczny</trd>,
    <trd>wystarczaj&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>suficiente</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="suficx.0e">
  <kap><tld/>e</kap>
  <snc mrk="suficx.0e.KOMUNE">
    <dif>
      Tiom, kiom estas bezone:
      <ekz>
        estas <tld/>e da lakto por vin nutri<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi pensas, ke estos <tld/>e dek metroj<fnt>K</fnt>;
      </ekz>
      <ekz>
        ni havas ne<tld/>e energian policestron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e pri tio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estos <tld/>e, se ni diros la jenon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi havas <tld/>e da tempo por.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sat">sate</ref>
    </refgrp>
    <rim>
      Ne uzu <em>sufi&ccirc;e</em> kun la senco
      de <em>iom, ete, meze</em>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_o;&c_l;&c_ib;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>suficientment</trd>,
    <trd>prou</trd>
  </trdgrp>
  <trd lng="cs">dost</trd>
  <trdgrp lng="de">
    <trd>genug</trd>,
    <trd>gen&uuml;gend</trd>,
    <trd>zur <ind>Gen&uuml;ge</ind></trd>,
    <trd>ausreichend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>enough</trd>,
    <trd>sufficiently</trd>
  </trdgrp>
  <trd lng="es">suficientemente</trd>
  <trdgrp lng="fr">
    <trd>suffisamment</trd>,
    <trd>assez</trd>
  </trdgrp>
  <trd lng="he">&mem;&samekh;&pe;&yod;&qof;</trd>
  <trdgrp lng="nl">
    <trd>voldoende</trd>,
    <trd>genoeg</trd>
  </trdgrp>
  <trd lng="no">nok</trd>
  <trdgrp lng="hu">
    <trd>el&eacute;g</trd>,
    <trd>eleget</trd>,
    <trd>el&eacute;gg&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dostatecznie</trd>,
    <trd>wystarczaj&aogonek;co</trd>,
    <trd>do&sacute;&cacute;</trd>,
    <trd>dosy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="suficx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suficx.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        tio <tld/>os por ni;
      </ekz>
      <ekz>
        tiu sumo ne <tld/>as;
      </ekz>
      <ekz>
        <tld/>as, ke vi promesis.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;&c_m;</trd>,
    <trd>&c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">bastar</trd>
  <trd lng="cs">sta&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>ausreichen</trd>,
    <trd>gen&uuml;gen</trd>,
    <trd>langen</trd>
  </trdgrp>
  <trd lng="en">suffice</trd>
  <trd lng="es">ser <ind>suficiente</ind></trd>
  <trd lng="fr">suffire</trd>
  <trd lng="he">&lamed;<ind>&he;&samekh;&pe;&yod;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>elegend&odblac;</trd>,
    <trd>el&eacute;gs&eacute;ges</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende zijn</trd>,
    <trd>genoeg zijn</trd>
  </trdgrp>
  <trd lng="pl">wystarcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind">(&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="suficx.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="suficx.0ega.KOMUNE">
    <dif>
      Abunda, pli ol <tld/>a.
    </dif>
  </snc>
  <trd lng="ca"><ind>m&eacute;s que</ind> suficient</trd>
  <trd lng="en">ample</trd>
  <trd lng="fr">grandement <ind>suffisant</ind></trd>
  <trdgrp lng="hu">
    <trd>b&odblac;ven elegend&odblac;</trd>,
    <trd>kiad&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rijkelijk</trd>,
    <trd>overvloedig</trd>
  </trdgrp>
  <trd lng="no">rikelig</trd>
</drv>

<drv mrk="suficx.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="suficx.mem0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kontentigo de siaj bezonoj per propra agado,
      ebliganta
      <ref tip="vid" cel="pend.sende0a.KOMUNE">sendependecon</ref>;
      <ref tip="sin" cel="auxtarci.0o.EKON">a&ubreve;tarcio</ref>:
      <ekz>
        tiu agrikultura aktiveco <klr>...</klr> preska&ubreve; garantias 
        mem<tld/>on por &ccirc;irka&ubreve; 50 lo&gcirc;antoj
        <fnt>
          <aut>Esperanto.be</aut>,
          <vrk><url ref="http://www.uea.org/vikio/Bona_Espero">
          Bona Espero</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>autarcie</trd>,
      <trd>autosuffisance</trd>
    </trdgrp>
    <trd lng="nl">zelfvoorzienend</trd>
    <trd lng="no">selvforsynt</trd>
    <trd lng="pl">samowystarczalno&sacute;&cacute;</trd>
  </snc>
</drv>

<drv mrk="suficx.ne0e">
  <kap>ne <tld/>e,
  <var><kap>ne<tld/>e</kap></var>,
  <var><kap>mal<tld/>e</kap></var></kap>
  <snc mrk="suficx.ne0e.KOMUNE">
    <dif>
      Tro malmulte, tro malabunde, tro malintense,
      <ref tip="vid" cel="tro.mal0.KOMUNE">maltro</ref>:
      <ekz>
        ne <tld/>e kuirita viando;
      </ekz>
      <ekz>
        ankora&ubreve; ne <tld/>e bone posedi la lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lanternoj en la tute nelumigita urbo mal<tld/>e
        heligis la vojon
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>insuficientment</trd>,
    <trd>exiguament</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>insuffisamment</trd>,
    <trd>pas assez</trd>
  </trdgrp>
  <trd lng="nl">onvoldoende</trd>
  <trdgrp lng="pl">
    <trd>niewystarczaj&aogonek;co</trd>,
    <trd>niedostatecznie</trd>,
    <trd>za ma&lstroke;o</trd>,
    <trd>nie do&sacute;&cacute;</trd>
  </trdgrp>
</drv>
</art>
