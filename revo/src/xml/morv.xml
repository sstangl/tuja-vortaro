<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: morv.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>morv</rad>/o
</kap>
<drv mrk="morv.0o">
  <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Speco de &ccirc;eval<ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>
      infekta al homo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;</trd>
  <trd lng="ca">borm</trd>
  <trd lng="cs">ozhřivka</trd>
  <trdgrp lng="de">
    <trd>Rotz<klr> (Krankheit)</klr></trd>,
    <trd>Hautwurm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>glanders</trd>,
    <trd>equinia</trd>,
    <trd>farcy</trd>,    
    <trd>malleus</trd>
  </trdgrp>
  <trd lng="fr">morve<klr> (maladie)</klr></trd>
  <trd lng="nl">droes</trd>
  <trd lng="pl">nosacizna</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;</trd>
  <trd lng="sk">sopľavka <klr>(u koňa)</klr></trd>
</drv>
</art>
