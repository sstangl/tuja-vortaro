<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sombr.xml,v 1.9 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sombr</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sombr.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Senparola kaj malgaja pro neeldiritaj zorgoj:
      <ekz>
        li do eliris, tute <tld/>a
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 57a, p. 255a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="melank.0a.melankolisenta">melankolia <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chmurný</trd>,
    <trd>zasmušilý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwerm&uuml;tig</trd>,
    <trd>schweigsam</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sombre<klr> (taciturne)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>komor</trd>,
    <trd>s&ouml;t&eacute;t</trd>,
    <trd>b&uacute;s</trd>
  </trdgrp>
  <trd lng="nl">somber</trd>
  <trdgrp lng="pl">
    <trd>ponury</trd>,
    <trd>pos&eogonek;pny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_u;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">zachmúrený</trd>
</drv>
</art>

