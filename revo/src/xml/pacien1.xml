<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pacien1.xml,v 1.17 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>1</ofc> <rad>pacient</rad>/o
</kap>

<drv mrk="pacien1.0o">
  <kap><tld/>o</kap>
  <snc mrk="pacien1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Iu kuracata de medicinisto a&ubreve; operaciata de &hcirc;irurgiisto,
      <ref tip="sin" cel="kurac.0ato.MED">kuracato</ref>:
      <ekz>
        transplantado de organoj fari&gcirc;is ordinara, sava rimedo por
        sennombraj <tld/>oj
        <fnt><aut>P. S. Viana</aut>:
          <vrk>&Ccirc;u fordoni siajn organojn?</vrk>,
          <bib>Monato</bib>
          <lok>2001:11, p. 21a</lok></fnt>;
      </ekz>
      <ekz>
        scienca kontrolo de <ind><tld/>ino</ind>j pruvis, ke la
        progresado de la tumoro kaj la nombro de la mortoj
        malplii&gcirc;is
        <fnt>
          <aut>E. Georgiev</aut>,
          <vrk><url
          ref="http://www.esperanto.be/fel/mon/007233.html"
              >Novaj kuracmetodoj kaj prevento</url></vrk>,
          <bib>Monato</bib>
        </fnt>.
        <trd lng="ru">&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t;&c_k;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_c;&c_y;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">pacient</trd>
  <trd lng="de">Patient</trd>
  <trd lng="en">patient</trd>
  <trd lng="fr">patient<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>p&aacute;ciens</trd>,
    <trd>beteg</trd>
  </trdgrp>
  <trd lng="it">paziente</trd>
  <trd lng="nl">pati&euml;nt</trd>
  <trd lng="pt">paciente</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;<klr> (&c_v; &c_z;&c_n;. &c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">pacient</trd>
</drv>
</art>
