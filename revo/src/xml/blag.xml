<vortaro>
<art mrk="$Id: blag.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <rad>blag</rad>/o
    <fnt>Z</fnt>
  </kap>
  <drv mrk="blag.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        &Scirc;erca mensogo. 
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>humbuk</trd>,
    <trd>tlach</trd>,
    <trd>žvást</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Flunkerei</trd>,
      <trd>Humbug</trd>,
      <trd>Prahlerei</trd>,
      <trd>Unsinn</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>blarney</trd>,
      <trd>fib</trd>,
      <trd>humbug</trd>,
      <trd>nonsense</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>blague<klr tip="ind">(mystification)</klr></trd>,
      <trd>bobard</trd></trdgrp>
  <trd lng="hu">l&oacute;d&iacute;t&aacute;s</trd>
  <trdgrp lng="pl">
      <trd>blaga</trd>,
      <trd>bajer<klr> (pot.)</klr></trd>,
      <trd>wkr&eogonek;t<klr> (pot.)</klr></trd>,
      <trd>kit<klr> (pot.)</klr></trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_z;&c_y;&c_g;&c_r;&c_y;&c_sh;</trd>,
      <trd>&c_sh;&c_u;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
  <trd lng="sk">lož zo žartu</trd>
</drv>
  
  <drv mrk="blag.0i">
    <kap><tld/>i<fnt>Z</fnt></kap>
    <gra><vspec>ntr</vspec></gra>
    <snc>
      <dif>
        &Scirc;erce, mistifike mensogi.  
      </dif>
    </snc>
  <trdgrp lng="cs">
    <trd>nalhat</trd>,
    <trd>napovídat</trd>,
    <trd>prášit</trd>,
    <trd>žertem lhát</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>aufschneiden</trd>,
      <trd>flunkern</trd>,
      <trd>prahlen</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>pull someone's leg</trd>,
      <trd>talk tongue in cheek</trd>,
    </trdgrp>
  <trdgrp lng="fr">
      <trd>blaguer</trd>,
      <trd>raconter des <ind>bobards</ind></trd></trdgrp>
  <trd lng="hu">l&oacute;d&iacute;t</trd>
  <trdgrp lng="pl">
      <trd>blagowa&cacute;</trd>,
      <trd>bajerowa&cacute;</trd>,
      <trd>wkr&eogonek;ca&cacute;<klr> kogo&sacute;</klr></trd>,
      <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w
      kana&lstroke;</trd>,
      <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w maliny</trd>,
      <trd>wciska&cacute;<klr> komu&sacute;</klr> kit</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_d;&c_u;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_zh;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">klamať zo žartu</trd>
</drv>
  <ekz>
<ind><tld/>ulo</ind>;
<trdgrp lng="de">
<trd>Aufschneider</trd>,
<trd>Prahlhans</trd>,
</trdgrp>
</ekz> 
</art>
