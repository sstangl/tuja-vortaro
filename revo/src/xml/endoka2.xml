<kap>
  <rad>endokardit</rad>/o
</kap>
<drv mrk="endoka2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref cel="endoka.0o">endokardo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_k;&c_a;&c_r;&c_d;&c_y;&c_t;</trd>
  <trd lng="bg">&c_je;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>endokarditida</trd>,
    <trd>endokarditis</trd>,
    <trd>zánět vnitřní blány srdeční</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Endokarditis<klr> [fak]</klr></trd>,
    <trd>Herzinnenhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">endocarditis</trd>
  <trd lng="fr">endocardite</trd>
  <trd lng="hu">sz&iacute;vbelh&aacute;rtya-gyullad&aacute;s</trd>
  <trd lng="id">endokarditis</trd>
  <trd lng="nl">endocarditis</trd>
  <trd lng="pl">wsierdzia zapalenie</trd>
  <trd lng="pt">endocardite</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_d;&c_i;&c_t;</trd>
  <trd lng="sk">zápal vnútornej srdcovej blany</trd>
</drv>
</art>
