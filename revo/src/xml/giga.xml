<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: giga.xml,v 1.2 2013/09/09 09:10:16 revo Exp $">
<kap>
  <rad>giga</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="giga.0">
  <kap><tld/>-</kap>
  <mlg>G</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="giga.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Miliardo da&rightquot;, 10<sup>9</sup>
      en la Internacia Sistemo de unuoj (SI):
        <ekz>
          kajta generatoro povus eligi unu <tld/>vaton, do havos povumon
          
          similan al tiu de nuklea centralo 
          <fnt>
            <bib>Viki</bib>,
            <lok><url
            ref="http://eo.wikipedia.org/wiki/Kajta_generatoro">
            Kajta generatoro</url>, 2010-05-07</lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_g;&c_a;-</trd>
  <trd lng="bg">&c_g;&c_i;&c_g;&c_a;-</trd>
  <trd lng="ca">giga-</trd>
  <trd lng="de">Giga-</trd>
  <trd lng="en">giga-</trd>
  <trd lng="es">giga-</trd>
  <trd lng="fa">&f_g;&f_y;&f_g;&a_A;</trd>
  <trd lng="fr">giga-</trd>
  <trd lng="hu">giga-</trd>
  <trd lng="it">giga-</trd>
  <trd lng="nl">giga-</trd>
  <trd lng="no">giga-</trd>
  <trd lng="pl">giga-</trd>
  <trd lng="pt">giga-</trd>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_a;-</trd>
  <trd lng="sv">giga-</trd>
</drv>
</art>

