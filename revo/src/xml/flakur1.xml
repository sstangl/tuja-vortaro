    <trd>maronszilvaf&eacute;l&eacute;k</trd>,
    <trd>maroncsif&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">wierzbowate</trd>
  <trdgrp lng="pt">
    <trd>flacourti&aacute;ceas</trd>,
    <trd>flacurti&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_k;&c_u;&c_r;&c_t;&c_i;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

