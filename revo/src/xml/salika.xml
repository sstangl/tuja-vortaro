<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: salika.xml,v 1.12 2011/12/04 10:10:08 revo Exp $">
<kap>
  <rad>salikari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="salika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="litrum.0o">litrumo</ref>
      (<trd lng="la">Lythrum salicaria</trd>), kun rozkoloraj floroj.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_k; &c_v;&c_je;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="ca">salic&agrave;ria</trd>
  <trd lng="de">Blutweiderich</trd>
  <trd lng="fr">salicaire</trd>
  <trd lng="hu">r&eacute;ti <ind>f&uuml;z&eacute;ny</ind></trd>
  <trd lng="it">salicaria</trd>
  <trd lng="nl">grote <ind>kattenstaart</ind><klr tip="ind">
  (plant)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n;-&c_t;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k; &c_i;&c_v;&c_o;&c_l;&c_i;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
