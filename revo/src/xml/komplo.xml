<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komplo.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>komplot</rad>/o
</kap>
<drv mrk="komplo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sekreta ligo por malica a&ubreve; krima celo kontra&ubreve; iu:
      <ekz>
        la &scirc;telistoj faris <tld/>on inter si.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="intrig.0o">intrigo</ref>,
      <ref cel="konspi.0o">konspiro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="ca">complot</trd>
  <trdgrp lng="cs">
    <trd>komplot</trd>,
    <trd>sdružení</trd>,
    <trd>spiknutí</trd>,
    <trd>spolčení</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verschw&ouml;rung</trd>,
    <trd>Intrige</trd>,
    <trd>Komplott</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspiracy</trd>,
    <trd>intrigue</trd>,
    <trd>plot</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>complot</trd>,
    <trd>cabale</trd>,
    <trd>conspiration</trd>,
    <trd>intrigue<klr tip="ind"> (machination)</klr></trd>,
    <trd>machination</trd>,
    <trd><ind>manigance</ind>s</trd>
    <trd><ind>men&eacute;e</ind>s</trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszeesk&uuml;v&eacute;s</trd>,
    <trd>b&udblac;nsz&ouml;vetkez&eacute;s</trd>,
    <trd>b&udblac;nsz&ouml;vev&eacute;ny</trd>,
    <trd>komplott</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complot</trd>,
    <trd>samenzwering</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>trama</trd>,
    <trd>conjura&ccedil;&atilde;o</trd>,
    <trd>conluio</trd>,
    <trd>compl&ocirc;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>komplot</trd>,
    <trd>sprisahanie</trd>
  </trdgrp>
</drv>

<drv mrk="komplo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;
  <ind>&c_z;&c_m;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="ca">complotar</trd>
  <trdgrp lng="cs">
    <trd>kout pikle</trd>,
    <trd>spiknout se</trd>,
    <trd>zosnovat komplot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>verschw&ouml;ren</ind></trd>,
    <trd>ein <ind>Komplott</ind> schmieden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspire</trd>,
    <trd>plot</trd>,
    <trd>scheme</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comploter</trd>,
    <trd>conspirer</trd>,
    <trd>intriguer<klr tip="ind"> (comploter)</klr></trd>,
    <trd>manigancer</trd>,
    <trd>ourdir un complot</trd>,
    <trd>tramer un complot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszesz&ouml;vetkezik</trd>,
    <trd>cimbor&aacute;l</trd>,
    <trd>&ouml;sszeesk&uuml;szik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>samenzweren</trd>,
    <trd>complotteren</trd>,
    <trd>samenspannen</trd>,
    <trd>conspireren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;
  &c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>robiť úklady</trd>,
    <trd>sprisahať sa</trd>
  </trdgrp>
</drv>
</art>
