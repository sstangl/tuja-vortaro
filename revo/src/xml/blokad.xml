<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blokad.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>blokad</rad>/o
</kap>
<drv mrk="blokad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;sie&gcirc;ado, &ccirc;irka&ubreve;baro de
      fortika&jcirc;o, urbo, haveno per
      seninterrompa gardo, por malhelpi la eniron de necesa&jcirc;oj:
      <ekz>
        mi trovis vian adreson en jarlibro de 1991, &ccirc;ar pro la
        <tld/>o de
        Sarajevo, mi ne povis ricevi novan<fnt><aut>Spomenka
        &Scirc;timec</aut>:
        <vrk>Saluton el Absurdistano</vrk>, <bib>Monato</bib><lok>jaro
        1993a, numero 12a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="siegx.0o">sie&gcirc;o</ref>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Blockierung</trd>,
     <trd>Blockade</trd>,
     <trd>Absperrung</trd>
  </trdgrp>
  <trd lng="es">bloqu&eacute;o</trd>
  <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h;</trd>
  <trd lng="fr">blocus</trd>
  <trd lng="pl">blokada</trd>
  <trd lng="ru">&c_b;&c_l;&c_o;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>blokáda</trd>,
    <trd>obliehanie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blokování</trd>,
    <trd>blokáda</trd>,
    <trd>uzavření přístupu</trd>
  </trdgrp>
</drv>

<drv mrk="blokad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;sie&gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>blokovat</trd>,
    <trd>vázat <klr>(sílu)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>belagern</trd>,
     <trd>umzingeln</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>assi&eacute;ger</trd>,
    <trd>faire le <ind>blocus</ind> (de)</trd>,
    <trd>faire le <ind>si&egrave;ge</ind> (de)</trd>
  </trdgrp>
  <trd lng="pl">prowadzi&cacute; <ind>blokad&eogonek;</ind></trd>
  <trd
  lng="ru">&c_b;&c_l;&c_o;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>blokovať</trd>,
    <trd>obliehať</trd>
  </trdgrp>
</drv>
</art>
