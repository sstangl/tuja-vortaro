<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: butan.xml,v 1.8 2014/06/18 18:10:11 revo Exp $">
<kap>
  <rad>Butan</rad>/o
</kap>

<drv mrk="butan.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="butan.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la <ref tip="vid" cel="himalaj.H0o">Himalajo</ref>
      kun &ccirc;efurbo Timbuo.
      Teritorio de Re&gcirc;ujo Butano.
      ISO-3166 kodo: BT.
      &Gcirc;i limas kun
      <ref tip="vid" cel="cxin.CX0ujo">&Ccirc;inujo</ref>
      kaj <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>.
    </dif>
    </snc>
    <trd lng="bg">&c_B;&c_u;&c_t;&c_a;&c_n;</trd>
    <trd lng="de">Bhutan</trd>
    <trd lng="en">Bhutan</trd>
    <trdgrp lng="fr">
      <trd>Bhoutan</trd>,
      <trd>Royaume du Bhoutan</trd>
    </trdgrp>
    <trd lng="id">Bhutan</trd>
    <trd lng="it">Bhutan</trd>
    <trd lng="nl">Bhoetan</trd>    
    <trdgrp lng="pl">
      <trd>Bhutan</trd>,
      <trd>Kr&oacute;lestwo <ind>Bhutanu</ind></trd>
    </trdgrp>
    <trd lng="pt">But&atilde;o</trd>
</drv>

</art>

