<art mrk="$Id: musli.xml,v 1.8 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>musli</rad>/o
</kap>
<drv mrk="musli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Man&gcirc;a&jcirc;o konsistanta el cerealoj, sekigitaj fruktoj, nuksoj,
      man&gcirc;ata ofte kun lakto, jahurto a&ubreve; suko.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ju;&c_s;&c_mol;&c_l;&c_ib;</trd>
  <trd lng="cs">müsli</trd>
  <trdgrp lng="de">
    <trd>M&uuml;esli</trd>,
    <trd>M&uuml;sli</trd>
  </trdgrp>
  <trd lng="en">muesli</trd>
  <trd lng="fr">muesli</trd>
  <trd lng="hu">m&uuml;zli</trd>
  <trd lng="nl">m&uuml;sli</trd>
  <trd lng="pl">muesli</trd>
  <trd lng="ru">&c_m;&c_ju;&c_s;&c_l;&c_i;</trd>
  <trd lng="sk">müsli</trd>
</drv>
</art>
