  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Aquifoliaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Stechpalmengew&auml;chse</trd>
  <trd lng="en">Aquifoliaceae</trd>
  <trd lng="fr">aquifoliac&eacute;es</trd>
  <trd lng="hu">magyalf&eacute;l&eacute;k</trd>
  <trd lng="nl">hulstfamilie</trd>
  <trd lng="pt">aq&uuml;ifoli&aacute;ceas</trd>
  <trd lng="ru">&c_p;&c_a;&c_d;&c_u;&c_b;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">j&auml;rneksv&auml;xter</trd>
</drv>
</art>
