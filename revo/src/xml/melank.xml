<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: melank.xml,v 1.25 2015/07/04 08:37:39 revo Exp $">
    <kap>
      <ofc>3</ofc>
      <rad>melankoli</rad>/o
    </kap>

    <drv mrk="melank.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Da&ubreve;ra reva, ordinare milda
          <ref tip="super" cel="gaj.mal0o">malgajo</ref>:
          <ekz>
            la franca vorto <klr>...</klr> elvokas ian dol&ccirc;an
            <tld/>on, mankon de forto
            <fnt><bib>BonaLingvo</bib>, <lok>p.
            104a</lok></fnt><klr>...</klr>
          </ekz>
        </dif>
      </snc>
      <ref tip="vid" cel="splen.0o">spleno</ref>
      <trd lng="be">&c_m;&c_e;&c_l;&c_ja;&c_n;&c_h;&c_o;&c_l;&c_ib;&c_ja;</trd>
      <trdgrp lng="bg">
        <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ja;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_i;&c_s;&c_n;&c_a;&c_t;&c_o;&c_s;&c_t;</trd>
      </trdgrp>
      <trd lng="br">melkoni</trd>
      <trdgrp lng="ca">
        <trd>malenconia</trd>,
        <trd>melangia</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Melancholie</trd>,
        <trd>Schwermut</trd>,
        <trd>Tr&uuml;bsinn</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>melancholy</trd>,
        <trd>melancholia</trd>,
        <trd>gloom</trd>,
        <trd>depression</trd>,
        <trd>dejection</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>mal de vivre</trd>,
        <trd>m&eacute;lancolie</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>melank&oacute;lia</trd>,
        <trd>m&eacute;lab&uacute;</trd>,
        <trd>b&uacute;skomors&aacute;g</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>melancholie</trd>,
        <trd>zwaarmoedigheid</trd>
      </trdgrp>
      <trd lng="pt">melancolia</trd>
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_n;&c_y;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_h;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>,
        <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ja;</trd>
      </trdgrp>
      <trd lng="it">melanconia</trd>
      <trd lng="sk">melanchólia</trd>
  <trdgrp lng="cs">
    <trd>melancholie</trd>,
    <trd>stesk</trd>
  </trdgrp>
</drv>

    <drv mrk="melank.0a">
      <kap><tld/>a</kap>
      <snc mrk="melank.0a.melankolisenta">
        <dif>
          Malgajeta, sentanta <tld/>on:
          <ekz>
            la viro sidis <tld/>a dum longa tempo
            <fnt>
              <aut>Will Green</aut>,
              <vrk>Gantoj</vrk>,
              <bib>Monato</bib>
              <lok>jaro 1997a, numero 5a, p. 24a</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>malenconi&oacute;s</trd>,
          <trd>melangi&oacute;s</trd>
        </trdgrp>
        <trd lng="de">melankolisch</trd>
        <trdgrp lng="hu">
          <trd>melankolikus</trd>,
          <trd>m&eacute;lab&uacute;s</trd>,
          <trd>b&uacute;skomor</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc mrk="melank.0a.PSI">
        <uzo tip="fak">PSI</uzo>
        <dif>
          Propra al <tld/>ulo:
          <ekz>
            la <tld/>a <ref tip="super"
            cel="temper.0o">temperamento</ref>.
          </ekz>
        </dif>
        <trd lng="ru">&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <snc mrk="melank.0a.melankoliiga">
        <ref tip="dif" cel="morn.0a">morna</ref>
        <ekz>
          jen lia vida&jcirc;o de tiu &leftquot;<tld/>a montetaro&rightquot;
          <fnt>
            <aut>A. Giridhar Rao</aut>,
            <vrk>La triumfo de rezono (Arthur Conan Doyle, La 
            &Ccirc;ashundo de la Baskerviloj)</vrk>,
            <bib>Monato</bib>
            <lok>jaro 2000a, numero 10a, p. 30a</lok></fnt>.
        </ekz>
        <trd lng="hu">komor</trd>
        <trdgrp lng="ru">
          <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_u;&c_n;&c_y;&c_l;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <rim mrk="melank.0a.RIM.1">
        <ctl>Melankolia</ctl> celas precipe la internan senton de homo,
        <ctl>Moroza</ctl> prefere lian eksteran aspekton.
        <ctl>Morna</ctl> prefere la etoson de loko a&ubreve; cirkonstancoj.
        Praktike, &ccirc;ar etoso, sento kaj aspekto sin influas, &ccirc;iu el 
        la vortoj povas surpreni alterne &ccirc;iun nuancon.
        <aut>&jcirc;v</aut>
      </rim>
      <trdgrp lng="bg">
        <trd>&c_t;&c_malmol;&c_zh;&c_je;&c_n;</trd>,
        <trd>&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_i;&c_s;&c_n;&c_a;&c_t;</trd>
      </trdgrp>
      <trd lng="br">melkonius</trd>
      <trdgrp lng="en">
        <trd>melancholic</trd>,
        <trd>melancholy</trd>,
        <trd>gloomy</trd>,
        <trd>depressed</trd>,
        <trd>dejected</trd>
      </trdgrp>
      <trd lng="fr">m&eacute;lancolique</trd>
      <trdgrp lng="nl">
        <trd>melancholisch</trd>,
        <trd>treurig</trd>
      </trdgrp>
      <trd lng="pt">melanc&oacute;lico</trd>
      <trd lng="it">melanconico</trd>
      <trdgrp lng="sk">
    <trd>melancholický</trd>,
    <trd>trudnomyseľný</trd>,
    <trd>ťažkomyseľný</trd>
  </trdgrp>
  <trd lng="cs">melancholicky</trd>
</drv>

    <drv mrk="melank.0ulo">
      <kap><tld/>ulo</kap>
      <snc mrk="melank.0ulo.PSI">
        <uzo tip="fak">PSI</uzo>
        <dif>
          Homo kies <ref tip="super" cel="temper.0o">temperamenton</ref>
          karakterizas malforta ekscitemo, emocioj profundaj kaj 
          da&ubreve;remaj:
          <ekz>
            la sinjorinoj nomis lin &ccirc;arma <tld/>ulo,
            sed la virinoj ne ekzistis plu por li
            <fnt>
              <aut>I. Turgenjev, tr. Kabe</aut>:
              <vrk>Patroj kaj filoj</vrk>.
              <lok>Inko Tyreso 2001. &#8208; &Ccirc;ap. 7a.</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">malenconi&oacute;s <klr>(individu amb temperament
      malenconi&oacute;s)</klr></trd>
  <trd lng="cs">melancholik</trd>
  <trd lng="de">Melankoliker</trd>
  <trd lng="en">melancholic</trd>
  <trd lng="it">melanconico</trd>
  <trd lng="pl">melancholik</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_k;</trd>
  <trd lng="sk">melancholik</trd>
</drv>

  </art>
