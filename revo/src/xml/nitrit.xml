<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nitrit.xml,v 1.5 2015/07/04 08:37:40 revo Exp $">
<kap><rad>nitrit</rad>/o</kap>
<drv mrk="nitrit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Salo de <ref cel="nitrit.0a.acido">nitrita acido</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_y;&c_t;</trd>
  <trd lng="cs">dusitan</trd>
  <trd lng="de">Nitrit</trd>
  <trd lng="en">nitrite</trd>
  <trd lng="fr">nitrite</trd>
  <trd lng="hu">nitrit</trd>
  <trd lng="nl">nitriet</trd>
  <trd lng="pt">nitrito</trd>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_i;&c_t;</trd>
  <trd lng="sk">dusitan</trd>
  <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_i;&c_t;</trd>
</drv>
<drv mrk="nitrit.0a.acido">
  <kap><tld/>a acido</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Neorganika <ref tip="super" cel="acid.0o">acido</ref>
      (<frm>HNO<sub>2</sub></frm>), 
      uzata en industrio.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_o;&c_c;&c_ib;&c_s;&c_t;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd>
  <trd lng="cs">kyselina dusitá</trd>
  <trd lng="de">salpetrige S&auml;ure</trd>
  <trd lng="en">nitrous acid</trd>
  <trd lng="fr">acide nitreux</trd>
  <trd lng="nl">salpeterigzuur</trd>
  <trd lng="pt">&aacute;cido nitroso</trd>
  <trd lng="ru">&c_a;&c_z;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">kyselina dusitá</trd>
  <trd lng="uk">&c_a;&c_z;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
</drv>

</art>

