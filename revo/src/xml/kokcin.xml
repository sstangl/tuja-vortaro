<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kokcin.xml,v 1.21 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kokcinel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kokcin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kokcin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="skarab.mari0o.ZOO">mariskarabo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_w;&c_k;&c_a;-&c_b;&c_a;&c_g;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_i;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>buoc'h-Doue</trd>,
    <trd>c'hwilig-Doue</trd>,
    <trd>yarig-Doue</trd>
  </trdgrp>
  <trd lng="cs">slunéčko</trd>
  <trd lng="de">Marienk&auml;fer <klr tip="ind">(Gattung)</klr></trd>
  <trdgrp lng="en">
    <trd>ladybird</trd>,
    <trd>ladybird beetle</trd>,
    <trd>lady beetle</trd>,
    <trd>ladybug</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mariquita</trd>,
    <trd>vaquita de San Antonio</trd>,
    <trd>vaquita de San Ant&oacute;n</trd>,
    <trd>chinita</trd>,
    <trd>mariquitilla</trd>,
    <trd>sarantont&oacute;n</trd>,
    <trd>catarina</trd>,
    <trd>San Antonio</trd>
  </trdgrp>
  <trd lng="fr">coccinelle</trd>
  <trd lng="hu">katicabog&aacute;r</trd>
  <trd lng="nl">lieveheersbeestje</trd>
  <trd lng="ru">&c_b;&c_o;&c_zh;&c_mol;&c_ja; &c_k;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">lienka</trd>
</drv>
</art>
