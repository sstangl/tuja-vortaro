<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akurat.xml,v 1.23 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>akurat</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akurat.0a">
  <kap><tld/>a</kap>
  <snc mrk="akurat.0a.gxustatempa">
    <dif>
      Faranta la farota&jcirc;on <ref tip="vid" 
      cel="temp.gxusta0e.kiam">&gcirc;ustatempe</ref>, je la
      interkonsentita horo:
      <ekz>
        <tld/>a oficisto;
      </ekz>
      <ekz>
        <tld/>e veni al la servo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li venas &ccirc;iun tagon <tld/>e je la oka;
      </ekz>
      <ekz>
        <tld/>e pagi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>eco, kun kiu la suno levi&gcirc;as matene<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi promesas <tld/>e servi al viaj mo&scirc;toj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e resendi la pruntitajn librojn;
      </ekz>
      <ekz>
        observi <tld/>ege la interkonsentojn de internacia juro.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">p&uuml;nktlich</trd>
    <trdgrp lng="en">
      <trd>punctual</trd>,
      <trd>on-time</trd>
    </trdgrp>
    <trd lng="es">puntual</trd>
    <trdgrp lng="fa">
      <trd>&a_w;&a_q;&a_t;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>,
      <trd>&a_kh;&a_w;&a_sh;&zwnj;&a_q;&a_w;&a_l;</trd>
    </trdgrp>
  </snc>
  <snc mrk="akurat.0a.ekzakta">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Ekzakta:
      <ekz>
        ni amu nin frate, sed ni kalkulu <tld/>e<fnt>
        <bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_k;&c_u;&c_r;&c_a;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>precise</trd>,
      <trd>exact</trd>
    </trdgrp>
    <trd lng="es">exacto</trd>
    <trd lng="fa">&a_d;&a_q;&f_y;&a_q;</trd>
  </snc>
  <trd lng="bg">&c_t;&c_o;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>akorátní</trd>,
    <trd>dochvilný</trd>,
    <trd>pořádný</trd>,
    <trd>přesný</trd>,
    <trd>řádný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>genau</trd>,
    <trd>pr&auml;zise</trd>,
    <trd>exakt</trd>,
    <trd>akkurat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ponctuel</trd>,
    <trd>exact</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>accuraat</trd>,
    <trd>punctueel</trd>,
    <trd>stipt</trd>
  </trdgrp>
  <trd lng="pl">punktualny</trd>
  <trdgrp lng="pt">
    <trd>pontual</trd>,
    <trd>acurado</trd>,
    <trd>exato</trd>
  </trdgrp>
  <trd lng="ro">punctual</trd>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">presn&yacute;</trd>
</drv>
</art>
