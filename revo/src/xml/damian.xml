<kap>
  <rad>Damian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="damian.0o">
  <kap><tld/>o</kap>
  <snc mrk="damian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        sanktuloj de la popola pieco: Kosmo kaj <tld/>o
        <fnt>
        <aut>trad. A. De Salvo</aut>,
        <vrk>Ekspozicio de dankaj vota&jcirc;oj</vrk>,
        <lok><url
        ref="http://www.esperokatolika.org/jarkolektoj/ek1983_09.htm">Espero
        Katolika, 1983-09</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fablo <ctl>La fi&scirc;supo de <tld/>o</ctl> de Krilov
        <fnt>
        <bib>MkM</bib>,
        <lok>&ccirc;apitro 5a, notoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Dami&agrave;</trd>
  <trd lng="de">Damian</trd>
  <trdgrp lng="en">
    <trd>Damian</trd>,
    <trd>Damien</trd>
  </trdgrp>
  <trd lng="fr">Damien</trd>
  <trd lng="hu">Damj&aacute;n</trd>
  <trd lng="it">Damiano</trd>
  <trd lng="pl">Damian</trd>
  <trd lng="pt">Dami&atilde;o</trd>
</drv>
</art>
