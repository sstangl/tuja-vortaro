<vortaro>
<art mrk="$Id: vivere.xml,v 1.10 2015/09/14 14:10:20 revo Exp $">
<kap>
  <rad>vivered</rad>/oj
</kap>
<drv mrk="vivere.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Viverridae</trd>).
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Viverridae</trd>,
    <trd>viverrids</trd>
  </trdgrp>
  <trd lng="be">&c_v;&c_ib;&c_v;&c_e;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">viv&egrave;rrids</trd>
  <trd lng="de">Schleichkatzen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">cibetmacskaf&eacute;l&eacute;k</trd>
  <trd lng="nl">civetkatten<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">viverr&iacute;deos</trd>
  <trd lng="ru">&c_v;&c_i;&c_v;&c_je;&c_r;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
