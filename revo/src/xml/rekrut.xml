<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rekrut.xml,v 1.18 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rekrut</rad>/o
</kap>
<drv mrk="rekrut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      Nova soldato:
      <ekz>
        instrui la <tld/>ojn.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_e;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_z;&c_y;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Rekrut</trd>
    <trd lng="en">recruit</trd>
    <trd lng="fr">recrue</trd>
    <trdgrp lng="hu">
      <trd>&uacute;jonc</trd>,
      <trd>regruta</trd>
    </trdgrp>
    <trd lng="nl">rekruut</trd>
    <trd lng="pl">rekrut</trd>
    <trd lng="pt">recruta</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>,
      <trd>&c_r;&c_je;&c_k;&c_r;&c_u;&c_t;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
    <trd lng="sv">rekryt</trd>
  </snc>
  <snc>
    <dif>
      Novevarbito:
      <ekz>
        nia societo bezonas <tld/>ojn.
      </ekz>   
    </dif>
      <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      <trd lng="hu">&uacute;jonc</trd>  
      <trdgrp lng="nl">
        <trd>nieuweling</trd>,
        <trd>nieuwkomer</trd>
      </trdgrp>    
      <trd lng="ru">&c_n;&c_o;&c_v;&c_o;&c_b;&c_r;&c_a;&c_n;&c_je;&c_c;</trd>
      </snc>
  <trd lng="ca">recluta</trd>
  <trdgrp lng="cs">
    <trd>branec</trd>,
    <trd>nováček</trd>,
    <trd>odvedenec</trd>,
    <trd>zast. rekrut</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>branec</trd>,
    <trd>nováčik</trd>,
    <trd>odvedenec</trd>,
    <trd>regrút</trd>
  </trdgrp>
</drv>

<drv mrk="rekrut.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="rekrut.0igi.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
       Fari iun <tld/>o:
       <ekz>
         kial vi <tld/>igas lin, se li estas forta kaj sana?
         <fnt>      
           <bib>SatirRak</bib>
         </fnt>
       </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reclutar</trd>,
      <trd>allistar</trd>
    </trdgrp>
    <trd lng="de">einziehen</trd>
    <trdgrp lng="nl">
      <trd>rekruteren</trd>,
      <trd>aanwerven</trd>,
      <trd>lichten</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekrut.0igi.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Kolekti, &ccirc;erpi:
      <ekz>
        tiu slango <tld/>igas siajn vortradikojn plejparte 
        el hispana kaj franca lingvoj.
      </ekz> 
      </dif> 
      <trd lng="ca">recollir</trd>
      <trd lng="de">rekrutieren</trd> 
      <trd lng="nl">rekruteren</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>odvést na vojnu</trd>,
    <trd>povolat do armády</trd>,
    <trd>verbovat</trd>
  </trdgrp>
  <trd lng="en">recruit</trd>
  <trd lng="fr">recruter</trd>
  <trdgrp lng="sk">
    <trd>odviesť na vojnu</trd>,
    <trd>povolať do armády</trd>,
    <trd>verbovať</trd>
  </trdgrp>
</drv>

</art>
