<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lepr.xml,v 1.15 2012/05/24 00:10:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lepr</rad>/o
</kap>
<drv mrk="lepr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Infekta ha&ubreve;ta malsano, karakterizata per ulceroj kaj gangrenoj,
      kiuj
      iom post iom difektas kaj man&gcirc;etas la ha&ubreve;ton kaj karnon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_k;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="br">lovrnez</trd>
  <trd lng="ca">lepra</trd>
  <trdgrp lng="de">
     <trd>Aussatz</trd>,
     <trd>Lepra</trd>,
     <trd>Hansensche Krankheit<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon_ton;&pi;&rho;&alfa;</trd>
  <trd lng="en">leprosy</trd>
  <trd lng="es">lepra</trd>
  <trd lng="fr">l&egrave;pre</trd>
  <trd lng="hu">lepra</trd>
  <trdgrp lng="nl">
    <trd>lepra</trd>,
    <trd>melaatsheid</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hansen&iacute;ase</trd>,
    <trd>lepra</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_a; (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</trd>
</drv>
</art>
