<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tik.xml,v 1.15 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tik.0i.MED">
    <dif>
      Pretervole tiri iun muskolon:
      <ekz>
        unu el ni okule <tld/>is kaj tro okulfrape rigardis al iu
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        41a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mít tik</trd>,
    <trd>mít škubání</trd>
  </trdgrp>
  <trd lng="fr">tiquer<klr tip="amb"> (avoir un tic)</klr></trd>
  <trdgrp lng="pl">
    <trd>mie&cacute; <ind>tik</ind></trd>,
    <trd>mie&cacute; <ind>kurcz</ind></trd>,
    <trd>mie&cacute; <ind>skurcz</ind></trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cukať</trd>,
    <trd>mykať</trd>,
    <trd>pošklbávať <klr>(z nervozity)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="tik.0o">
  <kap><tld/>o</kap>
  <snc mrk="tik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nerva kuntiro de muskoloj, plej ofte tiuj de la viza&gcirc;o.
    </dif>
  </snc>
  <rim mrk="tik.0o_HOM">
    Samformas la nomo de &scirc;tofo
    <ref tip="hom" cel="tik1.0o.TEKS">tiko</ref>.
  </rim>
  <trd lng="de">Tick</trd>
  <trd lng="es">tic</trd> 
  <trd lng="fr">tic</trd>
  <trdgrp lng="hu">
    <trd>izomr&aacute;ng&aacute;s</trd>,
    <trd>arcr&aacute;ng&aacute;s</trd>,
    <trd>tic</trd>
  </trdgrp>
  <trd lng="nl">tic</trd>
  <trdgrp lng="pl">
    <trd>nerwowy <ind>kurcz</ind><klr> (zazw. twarzy)</klr></trd>,
    <trd>tik</trd>,
    <trd>skurcz nerwowy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tique</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_k; <klr
  tip="amb">(&c_n;&c_je;&c_r;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">nervózne mykanie svalu</trd>
  <trdgrp lng="cs">
    <trd>tic</trd>,
    <trd>tik</trd>,
    <trd>škubání</trd>
  </trdgrp>
</drv>
</art>
