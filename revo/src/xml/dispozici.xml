<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dispozici.xml,v 1.7 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>dispozici</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="dispozici.0o">
  <kap><tld/>o</kap>
  <snc mrk="dispozici.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="klauxz.0o">kla&ubreve;zo</ref>
    <ekz>
      li formulis en la testamento aliajn
      <tld/>ojn
      <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 12a, p.
      87a</lok></fnt>.
    </ekz>
    <trd lng="ca">disposici&oacute;</trd>
    <trd lng="es">disposici&oacute;n<klr> (jur.)</klr></trd>
    <trd lng="fr">disposition<klr> (jur.)</klr></trd>
    <trd lng="pl">dyspozycja</trd>
    <trdgrp lng="pt">
      <trd>disposi&ccedil;&atilde;o <klr tip="amb">(jur.)</klr></trd>,
      <trd>cl&aacute;usula</trd>
    </trdgrp>
    <trd
    lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr
    tip="amb"> (&c_ju;&c_r;.)</klr></trd>
  </snc>
  <snc mrk="dispozici.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Faciliga stato a&ubreve; ecaro por io, ekzemple por malsano:
      <ekz>
        vi frenezi&gcirc;is, &ccirc;ar vi, ver&scirc;ajne, havas
        <tld/>on al tio
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 13a, p.
        117a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="latent.0o">latento</ref>
    </refgrp>
    <trd lng="ca">predisposici&oacute;</trd>
    <trd lng="es">predisposici&oacute;n</trd>
    <trd lng="fr">pr&eacute;disposition</trd>
    <trd lng="pl">predyspozycja</trd>
    <trdgrp lng="pt">
      <trd>disposi&ccedil;&atilde;o</trd>,
      <trd>predisposi&ccedil;&atilde;o</trd>,
      <trd>inclina&ccedil;&atilde;o</trd>,
      <trd>tend&ecirc;ncia</trd>
    </trdgrp>
    <trd
    lng="ru">&c_p;&c_r;&c_je;&c_d;&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dispozice</trd>,
    <trd>náklonnost</trd>,
    <trd>předpoklad</trd>,
    <trd>příkaz</trd>,
    <trd>rozmístění</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>náklonnosť</trd>,
    <trd>nálada</trd>,
    <trd>príkaz</trd>,
    <trd>smernica</trd>
  </trdgrp>
</drv>
</art>

