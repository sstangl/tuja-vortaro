<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: filame.xml,v 1.21 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>filament</rad>/o
</kap>
<drv mrk="filame.0o">
  <kap><tld/>o</kap>
  <snc mrk="filame.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Tre maldika <ref tip="super" cel="drat.0o.fadeno">drato</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Draht</trd>,
      <trd>Faser</trd>,
      <trd>St&auml;bchen</trd>,
      <trd>Filament</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&aacute;l</trd>,
      <trd>rost</trd>
    </trdgrp>
    <trd lng="pl">w&lstroke;&oacute;kno</trd>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_g;&c_u;&c_t;</trd>,
      <trd>&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="filame.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tiuforma anatomia elemento.
    </dif>
    <trdgrp lng="de">
      <trd>Faser</trd>,
      <trd>Filament</trd>
    </trdgrp>
    <trd lng="hu">rost</trd>
    <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>
  </snc>
  <snc mrk="filame.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La malsupra parto de <ref tip="malprt"
      cel="stamen.0o">stameno</ref>.
    </dif>
    <adm>
      Koherigi kun la difino de stameno, kiu parolas pri "fadeno".
      <aut>MB</aut>
    </adm>
    <trd lng="de">Staubfaden</trd>
    <trdgrp lng="hu">
      <trd>porz&oacute;sz&aacute;l</trd>,
      <trd>filamentum</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_g;&c_u;&c_t;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="filame.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Tiuforma metala objekto, kiu, varmigate de elektro, produktas
      lumon en
      <ref cel="ampol.0o.elektra">ampolo</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Gl&uuml;hdraht</trd>,
      <trd>Gl&uuml;hwendel</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_i;&c_t;&c_mol; &c_n;&c_a;&c_k;&c_a;&c_l;&c_a;</trd>
  </snc>
  <trd lng="ca">filament</trd>
  <trd lng="cs">nitka <klr>(tyčinky)</klr></trd>
  <trd lng="es">filamento</trd>
  <trdgrp lng="fr">
    <trd>filament</trd>,
    <trd>filandre</trd>,
    <trd>filet<klr tip="ind"> (bot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>izz&oacute;sz&aacute;l</trd>,
    <trd>f&udblac;t&odblac;sz&aacute;l</trd>
  </trdgrp>
  <trd lng="it">filamento</trd>
  <trdgrp lng="pt">
    <trd>filamento</trd>,
    <trd>fibra</trd>
  </trdgrp>
  <trd lng="sk">nitka tyčinky</trd>
</drv>

<drv mrk="filame.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta karakteron de <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">filament&oacute;s</trd>
  <trd lng="cs">vláknitý</trd>
  <trd lng="en">filament</trd>
  <trd lng="fr">filamenteux</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;las</trd>,
    <trd>rostos</trd>
  </trdgrp>
  <trd lng="it">filamentoso</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>niťovitý</trd>,
    <trd>vláknitý</trd>
  </trdgrp>
</drv>
</art>
