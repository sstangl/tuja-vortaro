<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kuvajt.xml,v 1.7 2014/06/18 19:10:15 revo Exp $">
<kap>
  <rad>Kuvajt</rad>/o
</kap>

<drv mrk="kuvajt.K0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kuvajt.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kun &ccirc;efurbo Kuvajturbo.
      Teritorio de Emirejo Kuvajto.
      ISO-3166 kodo: KW.
      &Gcirc;i limas kun
      <ref tip="vid" cel="irak.I0o.GEOG">Irako</ref>,
      <ref tip="vid" cel="arab.SaudaA0ujo">Sauda Arabujo</ref>
      kaj <ref tip="vid" cel="pers.P0aGolfo.GEOG">Persa Golfo</ref>.
    </dif>
    </snc>
    <trd lng="ar">&a_d;&a_w;&a_l;&a_t_marbuta; &a_A;&a_l;&a_k;&a_w;&a_y;&a_t;</trd>
    <trd lng="bg">&c_K;&c_u;&c_v;&c_je;&c_j;&c_t;</trd>
    <trd lng="de">Kuwait</trd>
    <trd lng="en">Kuwait</trd>
    <trdgrp lng="fr">
      <trd>Kowe&iuml;t</trd>,
      <trd>Etat du Kowe&iuml;t</trd>
    </trdgrp>
    <trd lng="id">Kuwait</trd>
    <trd lng="nl">Koeweit</trd>
    <trdgrp lng="pl">
      <trd><klr tip="amb">Pa&nacute;stwo </klr>Kuwejt</trd>
    </trdgrp>
    <trd lng="pt">Kuwait</trd>
</drv>

</art>

