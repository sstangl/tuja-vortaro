<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirist.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>miristik</rad>/o
</kap>
<drv mrk="mirist.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="mirist1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Myristica</trd>), kun la specio
      <ref tip="sub" cel="muskat.0arbo">muskatujo</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>muškátovník</trd>,
    <trd>muškátový strom</trd>
  </trdgrp>
  <trd lng="de">Muskatbaum</trd>
  <trdgrp lng="hu">
    <trd>szerecsendi&oacute;(fa)</trd>,
    <trd>musk&aacute;tdi&oacute;(fa)</trd>
  </trdgrp>
  <trd lng="nl">muskaatboom</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr
tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trdgrp lng="sk">
    <trd>muškátovník</trd>,
    <trd>muškátový strom</trd>
  </trdgrp>
</drv>
</art>
