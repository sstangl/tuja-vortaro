<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekscep.xml,v 1.12 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>ekscepci</rad>/o
</kap>
<drv mrk="ekscep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Defenda rimedo por prokrasti la finon de la proceso a&ubreve; por
      kritiki &gcirc;ian formon.
    </dif>
  </snc>
  <trd
    lng="be">&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;
    <klr tip="amb">(&c_u; &c_s;&c_u;&c_d;&c_z;&c_je;)</klr></trd>
  <trdgrp lng="cs">
    <trd>excepce</trd>,
    <trd>námitka <klr>(procesní)</klr></trd>,
    <trd>odpor (procesní obrana)</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Vorbehalt</trd>,
       <trd>Einwand</trd>,
       <trd>Einrede</trd>
    </trdgrp>
  <trd lng="fr">exception<klr tip="ind"> (droit)</klr></trd>
  <trdgrp lng="hu">
       <trd>kifog&aacute;s</trd>,
       <trd>ellentmond&aacute;s</trd>,
       <trd>&oacute;v&aacute;s</trd>
    </trdgrp>
  <trd lng="pl">wy&lstroke;&aogonek;czenie</trd>
  <trd lng="pt">exce&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>excepcia</trd>,
    <trd>námietka</trd>,
    <trd>obrana</trd>
  </trdgrp>
</drv>
</art>
