<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anter.xml,v 1.13 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anter</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="anter.0o">
  <kap><tld/>o</kap>
  <snc mrk="anter.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Supra parto de la <ref tip="malprt" cel="stamen.0o">stameno</ref>
      konsistanta el du paroj da po du polenujoj:
      <ekz>
        sufi&ccirc;as nur tu&scirc;i la <tld/>ojn de iu floro,
        kaj poste la stigmon de alia per la sama peniko
        por certigi fekundigon
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">prašník</trd>
  <trdgrp lng="de">
    <trd>Anthere</trd>,
    <trd>Staubbeutel</trd>
  </trdgrp>
  <trd lng="es">antera</trd>
  <trd lng="fr">anth&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>portok</trd>,
    <trd>porz&oacute;tok</trd></trdgrp>
  <trd lng="ru">&c_p;&c_y;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">prašník</trd>
</drv>
</art>
