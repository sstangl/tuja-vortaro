<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vermiljon.xml,v 1.9 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vermiljon</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vermiljon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Dense <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref> kun oran&gcirc;eta
      nuanco, kiel <ref tip="vid"
      cel="cinabr.0o">cinabro</ref>:
      <ekz>
        jen marmora &scirc;tuparo borderita per <tld/>aj balustradoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 5a, volumo 1a, p.
        65a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">vermell&oacute;<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cin&oacute;berpiros</trd>,
    <trd>cin&oacute;berv&ouml;r&ouml;s</trd>
    <trd>cin&oacute;bersz&iacute;n&udblac;</trd>
  </trdgrp>
  <trd lng="pl">cynobrowy</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vermiljon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="V"/>a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="ca">vermell&oacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>rumělka <klr>(ohnivě červené barvivo)</klr></trd>,
    <trd>rumělková červeň</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">cin&oacute;bersz&iacute;n</trd>
  <trd lng="pl">cynober<klr> (kolor)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
    &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;
    <ind>&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_i;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>2.rumelková červeň <klr>(farba)</klr></trd>,
    <trd>rumelka</trd>
  </trdgrp>
</drv>
</art>
