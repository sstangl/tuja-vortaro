<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: minotauxr.xml,v 1.7 2012/08/01 09:10:16 revo Exp $">
<kap>
  <rad>Minota&ubreve;r</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="minotauxr.0o">
  <kap><tld/>o</kap>
  <snc mrk="minotauxr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      La&ubreve; antikva greka legendo unuopa <ref tip="super"
      cel="monstr.0o.BIO">monstro</ref> kun homa korpo kaj bova kapo,
      kiu vivis en labirinto:
      <ekz>
        enlo&gcirc;is <klr>...</klr> sova&gcirc;uloj, la Bovkapoj, kiuj
        havas kornojn, kiel &ccirc;e ni oni bildigas la <tld/>on
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url
          ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
          rakontoj</url>. - 2005</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">minotaure</trd>
  <trd lng="de">Minotauros</trd>
  <trd lng="fr">Minotaure</trd>
  <trd lng="hu">Min&oacute;taurosz</trd>
  <trd lng="pl">Minotaur</trd>
  <trd lng="pt">Minotauro</trd>
  <trd lng="ru">&c_M;&c_i;&c_n;&c_o;&c_t;&c_a;&c_v;&c_r;</trd>
</drv>
</art>

