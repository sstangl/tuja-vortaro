<kap>
  <rad>flamberg</rad>/o
</kap>
<drv mrk="flambe.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      Speco de longa amba&ubreve;mana <ref tip="super"
      cel="glav.0o.MIL">glavo</ref>,
      kies klingo ofte havis ondan formon de flamo.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_b;&c_e;&c_r;&c_g;</trd>
  <trd lng="cs">flamberg</trd>
  <trdgrp lng="de">
    <trd>Flamberg</trd>,
    <trd>Landsknechtschwert mit geflammter Klinge</trd>
  </trdgrp>
  <trd lng="fr">flamberge</trd>
  <trdgrp lng="hu">
    <trd>pallos</trd>,
    <trd>l&aacute;ngpallos</trd>,
    <trd>flamberzs</trd>
  </trdgrp>
  <trd lng="it">flamberga</trd>
  <trdgrp lng="pt">
    <trd>durindana</trd>,
    <trd>espadag&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_b;&c_je;&c_r;&c_g;</trd>
  <trd lng="sk">dlh√Ω kord</trd>
</drv>
</art>
