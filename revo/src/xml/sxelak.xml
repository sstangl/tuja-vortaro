<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxelak.xml,v 1.13 2013/07/16 08:10:14 revo Exp $">
<kap>
  <rad>&scirc;elak</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sxelak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Gumo de arbo, kreskanta en Hindujo, produktata per pikvundoj de
      insektoj, renkontata en la komerco kiel flavaj a&ubreve; brunaj 
      folioj kaj uzata multe en elektro por &scirc;miri ekz. bobenan 
      volvaron.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_l;&c_a;&c_k;</trd>
  <trd lng="bg">&c_sh;&c_je;&c_l;&c_l;&c_a;&c_k;</trd>
  <trd lng="ca">goma laca</trd>
  <trd lng="de">Schellack</trd>
  <trd lng="en">shellac</trd>
  <trd lng="nl">schellak</trd>
  <trdgrp lng="hu">
    <trd>sellak</trd>,
    <trd>lakkgumi</trd>,
    <trd>lakkm&eacute;zga</trd></trdgrp>
  <trd lng="it">gommalacca</trd>
  <trd lng="pt">goma-laca</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_l;&c_a;&c_k;</trd>
</drv>
</art>
