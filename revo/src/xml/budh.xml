<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: budh.xml,v 1.12 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>budh</rad>/o
</kap>

<drv mrk="budh.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BUD</uzo>
      <ref tip="dif" cel="buda.0o">Budao</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_B;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">Buda</trd>
  <trd lng="de">Buddha</trd>
  <trd lng="en">buddha</trd>
  <trd lng="es">Buda</trd>
  <trd lng="fr">bouddha</trd>
  <trd lng="pl">Budda</trd>
  <trd lng="ru">&c_B;&c_u;&c_d;&c_d;&c_a;</trd>
</drv>

<drv mrk="budh.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">BUD</uzo>
  <uzo tip="fak">FIL</uzo>
      <ref tip="dif" cel="buda.0ismo">Budaismo</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">budisme</trd>
  <trdgrp lng="cs">
    <trd>buddhismus</trd>,
    <trd>křesťanství a islám</trd>,
    <trd>tři světová náboženství</trd>
  </trdgrp>
  <trd lng="de">Buddhismus</trd>
  <trd lng="en">buddhism</trd>
  <trd lng="es">budismo</trd>
  <trd lng="fr">bouddhisme</trd>
  <trd lng="pl">buddyzm</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_d;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">tri svetové náboženstvá</trd>
</drv>

<drv mrk="budh.0isto">
  <kap><tld/>isto, <var><kap><tld/>ano</kap></var></kap>
  <snc>
      <ref tip="dif" cel="buda.0isto">Budaisto</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">budista</trd>
  <trd lng="de">Buddhist</trd>
  <trd lng="en">buddhist</trd>
  <trd lng="es">budista</trd>
  <trd lng="fr">bouddhiste</trd>
  <trd lng="pl">buddysta</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_d;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
