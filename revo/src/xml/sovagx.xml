<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sovagx.xml,v 1.17 2014/06/28 19:10:12 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>sova&gcirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sovagx.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="sovagx.0a.KOMUNE">
     <dif>
       Proksima al naturo kaj malproksima al civilizo.
     </dif>
     <subsnc mrk="sovagx.0a.ETN">
       <uzo tip="klr">(pri homo, popolo)</uzo>
       <dif>
         Ne havanta agrikulturan a&ubreve; urban civilizon,
         sin vivtenanta per &ccirc;aso kaj rikolto en la naturo.
       </dif>
     </subsnc>
     <subsnc mrk="sovagx.0a.ZOO">
       <uzo tip="fak">ZOO</uzo>
       <dif>
         Ne hejmigita:
         <ekz>
           <tld/>a bovo;
         </ekz>
       </dif>
     </subsnc>
     <subsnc mrk="sovagx.0a.BOT">
       <uzo tip="fak">BOT</uzo>
       <dif>
         Ne kultivata.
         <ekz>
           <tld/>a greno<fnt>Z</fnt>;
         </ekz>
         <ekz>
           <tld/>a vinberujo.
         </ekz>
       </dif>
     </subsnc>
     <trdgrp lng="pl">
       <trd>dziki</trd>,
       <trd>nieokrzesany <klr>(o cz&lstroke;owieku)</klr></trd>
     </trdgrp>
   </snc>
   <snc mrk="sovagx.0a.FIG">
     <dif>
       <ref tip="vid" cel="mild.mal0a.KOMUNE">Malmilda</ref>,
       kruda.
     </dif>
     <subsnc mrk="sovagx.0a.akra">
       <dif>
         Akre kaj malagrable impresanta la sentumojn pro sia krudeco kaj
         maldelikateco:
         <ekz>
           <tld/>a koloro;
         </ekz>
         <ekz>
           <tld/>e vestita;
         </ekz>
         <ekz>
           Volap&uuml;k sonas <tld/>e kaj maldelikate<fnt>Z</fnt>;
         </ekz>
         <ekz>
           al &ccirc;io oni povas alkutimi&gcirc;i, kaj kio hiera&ubreve;
           &scirc;ajnis <tld/>a,
           tio morga&ubreve; aperas kiel io tute natura kaj
           bela<fnt>Z</fnt>.
         </ekz>
       </dif>
       <trdgrp lng="pl">
         <trd>krzykliwy</trd>,
         <trd>ostry</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="sovagx.0a.intensa">
       <dif>
         Senbride fortega:
         <ekz>
           <tld/>a ludo, gesto;
         </ekz>
         <ekz>
           <tld/>e pu&scirc;i iun;
         </ekz>
         <ekz>
           <tld/>torenta pluvo<fnt>Z</fnt>.
         </ekz>
       </dif>
       <refgrp tip="vid">
         <ref cel="furioz.0a">furioza</ref>,
         <ref cel="impet.0ega">impetega</ref>,
         <ref cel="brid.sen0a.FIG">senbrida<sncref/></ref>
       </refgrp>
       <trd lng="pl">gwa&lstroke;towny</trd>
       <trd lng="ru">&c_s;&c_v;&c_i;&c_r;&c_je;&c_p;&c_y;&c_j;</trd>
     </subsnc>
   </snc>
   <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_ib;</trd>
   <trd lng="de">wild</trd>
   <trd lng="fr">sauvage</trd>
   <trdgrp lng="hu">
     <trd>vas</trd>,
     <trd>nyers</trd>,
     <trd>durva</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>wild</trd>,
     <trd>woest</trd>
   </trdgrp>
   <trd lng="ru">&c_d;&c_i;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="sovagx.0i">
  <kap><tld/>i</kap>
  <snc mrk="sovagx.0i.KOMUNE">
    <dif>
      Abrupte, senbride efiki, kiel natura&jcirc;o:
      <ekz>
        la elementoj ekstere <tld/>as<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen">el&ccirc;eni&gcirc;i</ref>
    </refgrp>
  </snc>
</drv>

<drv mrk="sovagx.0ejo">
   <kap><tld/>ejo</kap>
   <snc mrk="sovagx.0ejo.EKOL">
     <uzo tip="fak">EKOL</uzo>
     <dif>
       Necivilizita kaj nekultivata lando:
      <ekz>
        ni volas en &ccirc;i tiu <tld/>ejo konstrui domon al ni
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_d;&c_z;&c_ib;&c_k;&c_a;&c_ja; &c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trd lng="de">Wildnis</trd>
   <trdgrp lng="fr">
     <trd>d&eacute;sert</trd>,
     <trd>nature sauvage</trd>
   </trdgrp>
   <trd lng="hu">vadon</trd>   
   <trd lng="nl">wildernis</trd>
   <trd lng="pl">dzicz</trd>
   <trd lng="ru">&c_d;&c_i;&c_k;&c_a;&c_ja;
   &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sovagx.mal0a">
   <kap>mal<tld/>a</kap>
   <snc mrk="sovagx.mal0a.BIO">
     <uzo tip="fak">BOT</uzo>
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Kultivata a&ubreve; <ref tip="vid"
       cel="hejm.0igita">hejmigita</ref>:
       <ekz>
         trajtoj, kiuj aperas &ccirc;e la maskloj de niaj
         mal<tld/>aj rasoj ofte estas transdonataj
         ekskluzive, a&ubreve; &ccirc;efe, al maskloj
         <fnt>
           <bib>OriginoDeSpecioj</bib>,
           <lok>&ccirc;apitro 1a</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_r;&c_u;&c_ch;&c_a;&c_n;&c_y;</trd>,
     <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="de">zahm</trd>
   <trdgrp lng="fr">
     <trd>cultiv&eacute;<klr> (plante)</klr></trd>,
     <trd>domestique<klr> (animal)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>szel&iacute;d</trd>,
     <trd>kezes</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>tam</trd>,
     <trd>mak</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>udomowiony<klr> (o zwierz&eogonek;ciu)</klr></trd>,
     <trd>oswojony<klr> (o zwierz&eogonek;ciu lub
     cz&lstroke;owieku)</klr></trd>,
     <trd>uprawny <klr>(o polu)</klr></trd>
   </trdgrp>
   <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="sovagx.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sovagx.mal0igi.KOMUNE">
    <dif>
      Igi mal<tld/>a:
      <ekz>
        la granda besto apartenis al speco, kiun la ru&gcirc;aj 
        homoj neniam mal<tld/>igas
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>apprivoiser</trd>,
    <trd>domestiquer</trd>
  </trdgrp>
</drv>
</art>
