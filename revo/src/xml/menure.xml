  <trd lng="hu">lantfark&uacute; madarak<klr tip="ind"> (csal&aacute;d)</klr></trd>
  <trd lng="nl">liervogels<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">menur&iacute;deos</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_o;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">lyrf&aring;glar</trd>
</drv>
