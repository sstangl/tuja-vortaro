  <trd lng="be">&c_l;&c_ib;&c_r;&c_a;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
