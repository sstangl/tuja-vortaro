<kap>
  <rad>bufed</rad>/oj
</kap>
<drv mrk="bufed1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="vost.sen0uloj">senvostuloj</ref>
      (<trd lng="la">Bufonidae</trd>), kun veruka ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_p;&c_u;&c_h;&c_ib; 
    <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr>
  </trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i; &c_zh;&c_a;&c_b;&c_i;<klr tip="amb">
  (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="ca">buf&ograve;nids</trd>
  <trd lng="de">Kr&ouml;ten</trd>
  <trd lng="hu">varangyf&eacute;l&eacute;k</trd>
  <trd lng="it">bufonidi</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_y; 
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr>
  </trd>
</drv>
</art>

