<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marion.xml,v 1.20 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>marionet</rad>/o
</kap>
<drv mrk="marion.0o">
  <kap><tld/>o</kap>
  <snc mrk="marion.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="pup.0o.ludilo">Pupo</ref>
      kun mane, fadene a&ubreve; risorte movataj membroj
      uzata en <ref tip="malprt" cel="teatr.pup0o">pupteatro</ref>.
    </dif>
    <trd lng="en">marionette</trd>
    <trd lng="fa">&a_ayn;&a_r;&a_w;&a_s;&f_k; &a_kh;&f_y;&a_m;&a_h;&zwnj;&a_sh;&a_b;&zwnj;&a_b;&a_A;&a_z;&f_y;</trd>
    <trdgrp lng="it">
    <trd>marionetta</trd>, 
    <trd>burattino</trd>,
  </trdgrp>
  </snc>
  <snc mrk="marion.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo sen karaktero, sen propra volo.
    </dif>
    <trdgrp lng="fa">
      <trd>&a_b;&a_A;&a_z;&f_y;&f_ch;&a_h;</trd>,
      <trd>&a_A_madda;&a_l;&a_t; &a_d;&a_s;&a_t;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>fantoche</trd>
    </trdgrp>
   <trdgrp lng="it">
    <trd>fantoccio</trd>
    <trd>burattino</trd>
    <trd>marionetta</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_e;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">margodenn</trd>
  <trd lng="ca">marioneta</trd>
  <trdgrp lng="cs">
    <trd>marioneta  ,, člověk bez vlastního rozhodování</trd>,
    <trd>závěsná loutka</trd>
  </trdgrp>
  <trd lng="de">Marionette</trd>
  <trdgrp lng="fr">
    <trd>marionnette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&aacute;b</trd>,
    <trd>b&aacute;bu</trd>
  </trdgrp>
  <trd lng="nl">marionet</trd>
  <trdgrp lng="pt">
    <trd>marionete</trd>,
    <trd>marioneta</trd>,    
    <trd>boneco <klr tip="amb">(marionete)</klr></trd>,
    <trd>bonifrate</trd>,
    <trd>fantoche</trd>,
    <trd>mamulengo</trd>,
    <trd>presepe <klr tip="amb">(marionete)</klr></trd>,
    <trd>t&iacute;tere</trd>,
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">divadelná bábka</trd>
</drv>
</art>
