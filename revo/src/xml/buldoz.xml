<art mrk="$Id: buldoz.xml,v 1.10 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>buldoz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="buldoz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fortika kaj peza ra&ubreve;pveturilo destinita por senradikigi kaj
      niveli
      grundon.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_z;&c_e;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>buldozer</trd>,
    <trd>čelní shrnovač</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Planierraupe</trd>,
    <trd>Schubraupe</trd>,
    <trd>Bulldozer</trd>,
    <trd>Flachbagger</trd>
  </trdgrp>
  <trd lng="en">bulldozer</trd>
  <trd lng="es">bulldozer</trd>
  <trdgrp lng="fr">
    <trd>bulldozer</trd>,
    <trd>bouteur</trd>
  </trdgrp>
  <trd lng="hu">buld&oacute;zer</trd>
  <trdgrp lng="pl">
    <trd>buldo&zdot;er</trd>,
    <trd>spychacz</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_z;&c_je;&c_r;</trd>
  <trd lng="sk">buldoz&eacute;r</trd>
</drv>
</art>
