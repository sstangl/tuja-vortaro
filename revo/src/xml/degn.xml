<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: degn.xml,v 1.6 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>degn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="degn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="degn.0i.KOMUNE">
    <dif>
      Bonvole komplezi al subulo:
      <ekz>
        nur grandaj sinjoroj scias esti tiom <ind><tld/>a</ind>j
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 2a, &ccirc;apitro 1a</lok>
        </fnt>;
        <trd lng="ca">digne</trd>
      </ekz>
      <ekz>
        vi tion, ver&scirc;ajne, ne <tld/>as kompreni
        <fnt>
        <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
        <vrk>Noticoj el la kelo</vrk>,
        <bib>LOdE</bib>,
        <lok>2002:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is lin per respondo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>.
        </fnt>        
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>dignar-se</trd>,
    <trd>condescendir</trd>
  </trdgrp>
  <trd lng="cs">ráčit</trd>
  <trdgrp lng="fr">
    <trd>condescendre</trd>,
    <trd>daigner</trd>
  </trdgrp>
  <trd lng="pt">dignar-se</trd>
  <trdgrp lng="sk">
    <trd>preukázať láskavosť nižšie postavenému</trd>,
    <trd>uponížiť sa</trd>
  </trdgrp>
</drv>
</art>

