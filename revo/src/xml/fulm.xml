<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fulm.xml,v 1.33 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fulm</rad>/o
</kap>

<drv mrk="fulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fulm.0o.atmosfera">
    <uzo tip="fak">MET</uzo>
    <dif>
      Atmosfera fenomeno konsistanta el elektra mal&scirc;argo inter
      nuboj
      a&ubreve; nuboj kaj tero, akompanata de bran&ccirc;i&gcirc;anta,
      rapide pasanta, fajra kanalo:
      <ekz>
        altan arbon batas la <tld/>o
        <klr>(ju pli elstara, des pli en
        dan&gcirc;ero)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ekbrilis sur la &ccirc;ielo grandaj zigzagaj <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj okuloj &jcirc;etis <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        en la da&ubreve;ro de <tld/>o <klr>(tre mallonge)</klr>;
      </ekz>
      <ekz>
        <ind>kun <tld/>a rapideco</ind>.
        <trd lng="hu">vill&aacute;mgyorsan</trd>
        <trd lng="nl">bliksemsnel</trd>
        <trd lng="no">lynrask</trd>
        <trd lng="pl">b&lstroke;yskawicznie</trd>
      </ekz>
    </dif>
    <trd lng="id">kilat</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;yskawica</trd>,
      <trd>piorun</trd>,
      <trd>grom</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>rel&acirc;mpago</trd>,
      <trd>raio</trd>,
      <trd>corisco</trd>,
      <trd>fa&iacute;sca</trd>
    </trdgrp>
    <trd lng="vo">lelit</trd>
    <trd lng="sv">blixt</trd>
  </snc>
  <snc mrk="fulm.0o.ekbrilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subita kaj malda&ubreve;ra ekbrilo:
      <ekz>
        ek<tld/>oj de genio, de sa&gcirc;o.
      </ekz>
    </dif>
    <adm>
      Pli bone trovi ekz kun ~o anstataux ek~o. <aut>MB</aut>
    </adm>
    <trd lng="pl">b&lstroke;yskawica</trd>
    <trdgrp lng="pt">
      <trd>rel&acirc;mpago</trd>,
      <trd>raio</trd>,
      <trd>corisco</trd>,
      <trd>fa&iacute;sca</trd>
    </trdgrp>
    <trd lng="sv">blixt</trd>
  </snc>
  <snc mrk="fulm.0o.puno">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Timinda puno.
    </dif>
    <trd lng="pl">grom</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_g;&c_r;&c_malmol;&c_m;</trd>,
    <trd>&c_m;&c_malmol;&c_l;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">blesk</trd>
  <trd lng="de">Blitz</trd>
  <trd lng="en">lightning</trd>
  <trdgrp lng="es">
    <trd>rayo</trd>,
    <trd>rel&aacute;mpago</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_q;</trd>,
    <trd>&a_A_madda;&a_dh;&a_r;&a_kh;&a_sh;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foudre</trd>,
    <trd>&eacute;clair</trd>
  </trdgrp>
  <trd lng="hu">vill&aacute;m</trd>
  <trd lng="nl">bliksem</trd>
  <trd lng="no">lyn</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">blesk</trd>
</drv>

<drv mrk="fulm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fulm.0i.atmosfere">
    <uzo tip="fak">MET</uzo>
    <dif>
      Ekbrili, p.p. <tld/>o:
      <ekz>
        ek<tld/>as;
      </ekz>
      <ekz>
        aspektis kvaza&ubreve; la tuta mondo staris en flamoj, tiel
        forte <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_A_madda;&a_dh;&a_r;&a_kh;&a_sh;
    &a_z;&a_d;&a_n;</trd>
    <trd lng="fr">y avoir des <ind>&eacute;clairs</ind></trd>
  </snc>
  <snc mrk="fulm.0i.brili">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brili kiel <tld/>o:
      <ekz>
        <tld/>as lancoj
        <fnt><bib>MT</bib>, <lok>&Nah; 3:3</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(bolanta)</klr> knabego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>anta</ind> rigardo;
        <trdgrp lng="fr">
          <trd>foudroyant</trd>,
          <trd>fulgurant</trd>
        </trdgrp>
        <trd lng="hu">vill&aacute;ml&oacute;</trd>
        <trd lng="nl">bliksemend</trd>
      </ekz>
      <ekz>
        en la pupiloj <tld/>etadis briloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sur &scirc;iaj lipoj sen&ccirc;ese tremadis <tld/>etanta
        rideto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_d;&a_r;&a_kh;&a_sh;&f_y;&a_d;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>jeter des <ind>&eacute;clairs</ind></trd>,
      <trd>fulgurer</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="cs">blýskat se</trd>
  <trd lng="de">blitzen</trd>
  <trd lng="es">relampaguear</trd>
  <trd lng="hu">vill&aacute;mlik</trd>
  <trd lng="nl">bliksemen</trd>
  <trd lng="no">lyne</trd>
  <trd lng="pl">b&lstroke;yska&cacute;</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">blýskať sa</trd>
  <trd lng="sv">blixtra</trd>
</drv>

<drv mrk="fulm.0obati0ofrapi">
  <kap><tld/>obati, <var><kap><tld/>ofrapi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Trafi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="cs">uhodit blesk</trd>
  <trdgrp lng="fa">
    <trd>&a_S;&a_A;&a_ayn;&a_q;&a_h; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&a_q; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">foudroyer</trd>
  <trd lng="hu">vill&aacute;m s&uacute;jtja</trd>
  <trdgrp lng="nl">
    <trd>neerbliksemen</trd>,
    <trd>doodbliksemen</trd>
  </trdgrp>
  <trd lng="no">lyne ned</trd>
  <trdgrp lng="pl">
    <trd>trafia&cacute;<klr> (o piorunie,
    b&lstroke;yskawicy)</klr></trd>,
    <trd>uderza&cacute;<klr> (o piorunie,
    b&lstroke;yskawicy)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;
  &c_m;&c_o;&c_l;&c_n;&c_i;&c_je;&c_j;</trd>
  <trd lng="sk">udrieť blesk</trd>
</drv>

<drv mrk="fulm.0otondro">
  <kap><tld/>otondro</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Neda&ubreve;ra pluvego kun <tld/>oj kaj tondroj:
      <ekz>
        kiu semas venton, rikoltos
        <tld/>otondron<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uragan.0o">uragano</ref>,
      <ref cel="sxtorm.0o">&scirc;tormo</ref>.
    </refgrp>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_b;&c_u;&c_r;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>bouře</trd>,
    <trd>bouřka</trd>,
    <trd>hromobití</trd>
  </trdgrp>
  <trd lng="de">Gewitter</trd>
  <trd lng="es">tormenta el&eacute;ctrica</trd>
  <trd lng="fa">&a_t;&a_w;&a_f;&a_A;&a_n;
  &a_t;&a_n;&a_d;&a_r;&f_y;</trd>
  <trd lng="fr">orage</trd>
  <trdgrp lng="hu">
    <trd>zivatar</trd>,
    <trd>&eacute;gih&aacute;bor&uacute;</trd>
  </trdgrp>
  <trd lng="nl">onweer</trd>
  <trd lng="no">uv&aelig;r</trd>
  <trd lng="pl">burza<klr> z piorunami</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hromobitie</trd>,
    <trd>krátka búrka</trd>,
    <trd>letná búrka</trd>
  </trdgrp>
  <trd lng="sv">&aring;ska</trd>
</drv>

<drv mrk="fulm.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">FOT</uzo>
  <trd lng="en">flash</trd>
  <trd lng="nl">flits</trd>  
  <trd lng="sk">blesk <klr>(prístroj)</klr></trd>
</drv>
</art>
