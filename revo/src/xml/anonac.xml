<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anonac.xml,v 1.1 2013/11/02 11:10:14 revo Exp $">
  <kap>
    <rad>anoniac</rad>/oj
  </kap>
<drv mrk="anonac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de 
      lignaj <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref> plantoj
      (<trd lng="la">Annonaceae</trd>) kun plurfruktoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Annonengew&auml;chse</trd>,
    <trd>Flaschenbaumgew&auml;chse</trd>,
    <trd>Rahmapfelgew&auml;chse</trd>,
    <trd>Schuppenapfelgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>

