<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: impres.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>impres</rad>/o
</kap>
<drv mrk="impres.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="impres.0o.gxenerale">
    <dif>
      Influo, efiko, farita sur la sentumoj, la koro a&ubreve; la spirito:
      <ekz>
        senti <tld/>on de malvarmo;
      </ekz>
      <ekz>
        oni havis la <tld/>on kvaza&ubreve; &ccirc;io dormas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sub la <tld/>o de teruro, li forlasis la postenon;
      </ekz>
      <ekz>
        tiu parolado, pentra&jcirc;o, virino faris sur min potencan,
        profundan <tld/>on, nur malfortan <tld/>on;
      </ekz>
      <ekz>
        nia nombro faris <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekt.0o">efekto</ref>,
      <ref cel="impon.0eco">imponeco</ref>.
    </refgrp>
    <trd lng="be">&c_u;&c_r;&c_a;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Eindruck</trd>
    <trdgrp lng="fr">
      <trd>impression<klr tip="ind"> (sensation)</klr></trd>,
      <trd>sensation<klr tip="ind"> (impression)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;rz&eacute;s<klr tip="ind"> (benyom&aacute;s)</klr></trd>,
      <trd>benyom&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">indruk</trd>
    <trd lng="pt">impress&atilde;o<klr tip="amb"> (sensa&ccedil;&atilde;o)</klr></trd>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="sv">intryck</trd>
  </snc>
  <snc mrk="impres.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Fizika efiko de lumradioj sur fotografa plato:
      <ekz>
        tempo de <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je; <klr
      tip="amb">(&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_a;)</klr></trd>,
      <trd>&c_e;&c_k;&c_s;&c_p;&c_a;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Einwirkung</trd>,
      <trd>Lichteinwirkung</trd>,
      <trd>Belichtung</trd>
    </trdgrp>
    <trd lng="fr">impression<klr tip="ind"> (phot.)</klr></trd>
    <trdgrp lng="hu">
      <trd>megvil&aacute;g&iacute;t&aacute;s</trd>,
      <trd>expon&aacute;l&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">belichting</trd>
    <trd lng="pt">impress&atilde;o<klr tip="amb"> (fotografia,
    &oacute;ptica)</klr></trd>
    <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;<klr tip="amb">
    (&c_f;&c_o;&c_t;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dojem</trd>,
    <trd>imprese</trd>,
    <trd>pocit</trd>,
    <trd>účinek</trd>
  </trdgrp>
  <trd lng="es">impresi&oacute;n</trd>
  <trdgrp lng="sk">
    <trd>dojem</trd>,
    <trd>impresia</trd>,
    <trd>pocit</trd>
  </trdgrp>
</drv>

<drv mrk="impres.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Estigi, fari <tld/>on sur:
      <ekz>
        la akcidento, kongreso profunde <tld/>is lin;
      </ekz>
      <ekz>
        tiu ceremonio <tld/>is min neforgeseble;
      </ekz>
      <ekz>
        favore, melankolie <tld/>i iun;
      </ekz>
      <ekz>
        lia aserto <tld/>as kiel paradokso;
      </ekz>
      <ekz>
        <tld/>ita de la admono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gravur.0i">gravuri</ref>
    </refgrp>
  </snc>
  <adm>
    Aldoni ekz-on FOT. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_u;&c_r;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_u;&c_r;&c_a;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einwirken</trd>,
    <trd>beeindrucken</trd>
  </trdgrp>
  <trd lng="es">impresionar</trd>
  <trdgrp lng="fr">
    <trd>impressionner<klr tip="amb"> (produire une
    impression)</klr></trd>,
    <trd>impressionner<klr tip="amb"> (photographiquement)</klr></trd>,
    <trd>donner l'<ind>impression</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>benyom&aacute;st tesz<klr tip="amb"> (valakire)</klr></trd>,
    <trd>megvil&aacute;g&iacute;t<klr tip="amb"> (fot&oacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">indruk maken</trd>
  <trd lng="pt">impressionar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">g&ouml;ra <ind>intryck</ind></trd>
  <trdgrp lng="sk">
    <trd>robiť dojem</trd>,
    <trd>zapôsobiť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dojímat</trd>,
    <trd>zapůsobit</trd>
  </trdgrp>
</drv>

<drv mrk="impres.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Faranta fortan <tld/>on:
      <ekz>
        <tld/>a rakonto, propagandilo.
      </ekz>
      <ekz>
        la portantoj de tiu kulturo <klr>...</klr> estas famaj anka&ubreve;
        pro
        siaj <tld/>aj pentra&jcirc;oj en kavernoj
        <fnt>
        <aut>Werner Fuss</aut>,
        <vrk>&Ccirc;u la homaro lernis pensi per la lingvo?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2003a, numero 2a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>dojemný</trd>,
    <trd>působivý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beeindruckend</trd>
  </trdgrp>
  <trd lng="es">impresionante</trd>
  <trd lng="fr">impressionnant</trd>
  <trd lng="hu">hat&aacute;sos</trd>
  <trd lng="nl">indruk makend</trd>
  <trd lng="pt">impressionate</trd>
  <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dojemný</trd>,
    <trd>pôsobivý</trd>
  </trdgrp>
</drv>

<drv mrk="impres.0ebla0igxema">
  <kap><tld/>ebla, <var><kap><tld/>i&gcirc;ema</kap></var></kap>
  <snc>
    <dif>
      Povanta esti facile <tld/>ata:
      <ekz>
        <tld/>i&gcirc;ema karaktero, knabo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_r;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_u;&c_r;&c_a;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_ch;&c_u;&c_l;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_ch;&c_u;&c_j;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>empfindlich</trd>,
      <trd>beeindruckbar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fog&eacute;kony</trd>,
      <trd>&eacute;rz&eacute;keny</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Preparita por ricevi la efikon de la lumradioj:
      <ekz>
        <tld/>i&gcirc;ema fotografa plato.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_f;&c_o;&c_t;&c_a;&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    </trdgrp>
    <trdgrp lng="de">
      <trd>empfindlich</trd>,
      <trd>lichtempfindlich</trd>
    </trdgrp>
    <trd lng="hu">f&eacute;ny&eacute;rz&eacute;keny<klr tip="ind">
    (fotogr&aacute;fia)</klr></trd>
    <trd lng="nl">gevoelig<klr tip="ind"> (fot..)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_je;&c_t;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">impressionnable</trd>
  <trdgrp lng="pt">
   <trd>impression&aacute;vel</trd>, 
   <trd>sens&iacute;vel<klr tip="amb"> (que se impressiona)</klr></trd> 
  </trdgrp>
</drv>
</art>
