<vortaro>
<art mrk="$Id: begoni1.xml,v 1.9 2013/08/25 13:10:25 revo Exp $">
<kap>
  <rad>begoniac</rad>/oj
</kap>
<drv mrk="begoni1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Begoniaceae</trd>), kun nesimetriaj folioj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_g;&c_o;&c_n;&c_ib;&c_je;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">begoni&agrave;cies</trd>
  <trd lng="de">Schiefblattgew&auml;chse</trd>
  <trd lng="en">begonias</trd>
  <trd lng="fr">b&eacute;goniac&eacute;es</trd>
  <trd lng="hu">beg&oacute;niaf&eacute;l&eacute;k</trd>
  <trd lng="ru">&c_b;&c_je;&c_g;&c_o;&c_n;&c_i;&c_je;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

