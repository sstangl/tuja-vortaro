<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gerbil.xml,v 1.15 2013/10/23 18:10:20 revo Exp $">
<kap>
<rad>gerbil</rad>/o
</kap>
<drv mrk="gerbil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref>
      iom simila al rato, sed kun haroj sur vosto kaj plandoj kaj pli
      longaj   
      malanta&ubreve;aj kruoroj, vivanta en dezertoj kaj savanoj de 
      Azio kaj Afriko;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Gerbillus</trd>). 
    </dif>
  </snc>
  <trd lng="ca">jerbu</trd>
  <trd lng="de">Rennmaus</trd>
  <trd lng="fr">gerbille</trd>
  <trd lng="en">gerbil</trd>
  <trd lng="es">jerbo</trd>
  <trd lng="hu">versenyeg&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>gerbil</trd>,
    <trd>woestijnrat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gerbo</trd>,
    <trd>gerbila</trd>
  </trdgrp>
</drv>
</art>

