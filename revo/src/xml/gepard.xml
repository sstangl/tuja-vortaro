<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gepard.xml,v 1.21 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gepard</rad>/o
</kap>
<drv mrk="gepard.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Tre rapidkura <ref tip="lst" cel="best.rabo0o" 
      lst="voko:rabobestoj">rabobesto</ref>, kun
      flavbruna nigre makulita pelto;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Acinonyx</trd>).  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_p;&c_a;&c_r;&c_d;</trd>
  <trd lng="cs">gepard</trd>
  <trd lng="de">Gepard</trd>
  <trd lng="en">cheetah</trd>
  <trd lng="es">guepardo</trd>
  <trd lng="fa">&f_y;&a_w;&a_z;&f_p;&a_l;&a_n;&f_g;</trd>
  <trd lng="fr">gu&eacute;pard</trd>
  <trd lng="hu">gep&aacute;rd</trd>
  <trdgrp lng="id">
    <trd>cheetah</trd>,
    <trd>citah</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>jachtluipaard</trd>,
    <trd>cheeta</trd>
  </trdgrp>
  <trd lng="pt">guepardo</trd>
  <trd lng="ru">&c_g;&c_je;&c_p;&c_a;&c_r;&c_d;</trd>
  <trd lng="sk">gepard</trd>
  <trd lng="sv">gepard</trd>
</drv>
</art>
