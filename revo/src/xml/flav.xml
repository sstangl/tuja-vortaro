<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flav.xml,v 1.42 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flav</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="flav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="flav.0a.KOL">
    <dif>
      Samkolora kiel interna bulo de kokinovo, citrono, oro, matura
      tritiko:
      <ekz>
        la neimageblaj ebloj de la <tld/>a florpolvo
        <fnt><aut>R. Roze</aut>: <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>
        <lok>jaro 1997a, numero 11a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_t;&c_y;</trd>
  <trd lng="bg">&c_zh;&c_malmol;&c_l;&c_t;</trd>
  <trd lng="cs">&zcaron;lut&yacute;</trd>
  <trd lng="de">gelb</trd>
  <trd lng="en">yellow</trd>
  <trd lng="es">amarillo</trd>
  <trd lng="fr">jaune<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">s&aacute;rga</trd>
  <trd lng="id">kuning</trd>
  <trd lng="nl">geel</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;ty</trd>
  <trdgrp lng="pt">
    <trd>amarelo</trd>,
    <trd>louro</trd>
    <trd>fulvo</trd>,
    <trd>dourado</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_jo;&c_l;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">žltý</trd>
  <trd lng="sv">gul</trd>
  <trd lng="tp">jelo</trd>
  <trd lng="tr">sar&inodot;</trd>
  <trd lng="vo">yelibik</trd>
  <trd lng="zh">黄 [<ind>huáng</ind>]</trd>
</drv>

<drv mrk="flav.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="flav.0eta.KOL">
    <dif>
      Iom, proksimume <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  <trd lng="cs">nažloutlý</trd>
  <trd lng="de">gelblich</trd>
  <trd lng="en">yellowish</trd>
  <trd lng="fr">jaun&acirc;tre</trd>
  <trd lng="hu">s&aacute;rg&aacute;s</trd>
  <trdgrp lng="id">
    <trd>[<ind>kuning</ind>] kekuningan</trd>,
    <trd>[<ind>kuning</ind>] kekuning-kuningan</trd>
  </trdgrp>
  <trd lng="nl">geelachtig</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">nažltnutý</trd>
</drv>

<drv mrk="flav.0o">
  <kap><tld/>o</kap>
  <snc mrk="flav.0o.KOL">
    <dif>
      <tld lit="F"/>a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ib;&c_z;&c_n;&c_a;</trd>
  <trd lng="bg">&c_zh;&c_malmol;&c_l;&c_t;&c_o;</trd>
  <trd lng="cs">žluť</trd>
  <trd lng="de">Gelb</trd>
  <trd lng="en">yellow</trd>
  <trd lng="es">amarillo</trd>
  <trd lng="fr">jaune<klr> (couleur)</klr></trd>
  <trd lng="hu">s&aacute;rga<klr tip="ind"> (sz&iacute;n)</klr></trd>
  <trd lng="ku">zer</trd>
  <trd lng="nl">geel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pt">amarelo</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_i;&c_z;&c_n;&c_a;</trd>
  <trd lng="sk">žlť</trd>
</drv>

<drv mrk="flav.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="flav.0ulo.flavrasulo">
    <dif>
      Homo el <tld/>a raso.
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;</trd>
    <trd lng="fr">jaune<klr tip="ind"> (asiatique)</klr></trd>
    <trd lng="en">yellow man</trd>
    <trd lng="hu">s&aacute;rgab&odblac;r&udblac;<klr tip="ind">
    (ember)</klr></trd>
    <trd lng="nl">geelhuid</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;
      <ind>&c_zh;&c_jo;&c_l;&c_t;&c_o;&c_j;</ind> &c_r;&c_a;&c_s;&c_y;</trd>,
      <trd>&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="flav.0ulo.laboristo">
    <dif>
      Laboristo kontra&ubreve;a al la klasbatala politiko.
    </dif>
    <trd lng="fr">jaune<klr tip="ind"> (briseur de gr&egrave;ve)</klr></trd>
    <trd 
lng="ru">&c_sh;&c_t;&c_r;&c_je;&c_j;&c_k;&c_b;&c_r;&c_je;&c_h;&c_je;&c_r;</trd>
  </snc>
  <trd lng="cs">člověk žluté pleti</trd>
  <trd lng="sk">človek žltej pleti</trd>
</drv>

<drv mrk="flav.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="flav.0igxo.KOMUNE">
    <dif>
      La procezo i&gcirc;i <tld/>a a&ubreve; ties rezulto:
      <ekz>
        la Eternulo frapos vin per ftizo kaj per febro kaj per
        malsaneca varmego kaj per brulumo kaj per sekeco kaj per
        bruliga aero kaj per <tld/>i&gcirc;o de la kreska&jcirc;oj
        <fnt><bib>MT</bib>, <lok>&Rea; 28:22</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="flav.0igxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Simptomo de pluraj malsanoj (kaj speciale de la
      <ref cel="san.flavmal0o"><tld/>malsano</ref>),
      konsistanta en tio ke la ha&ubreve;to la okula
      konjunktivo i&gcirc;as <tld/>aj.
    </dif>
    <trd lng="en">jaundice</trd>
    <trdgrp lng="fr">
      <trd>ict&egrave;re</trd>, <trd>jaunisse</trd>
    </trdgrp>
    <trd lng="id"><ind>sakit</ind> kuning</trd>
    <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;<klr>
    (&c_s;&c_i;&c_m;&c_p;&c_t;&c_o;&c_m;)</klr></trd>
  </snc>
</drv>

<drv mrk="flav.ov0o">
  <kap>ovo<tld/>o, <var><kap>ovo<tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="flav.ov0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="F"/>a nutra parto de ovo.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_o;&c_k;</trd>
  <trd lng="es">yema</trd>
  <trd lng="en">yolk</trd>
  <trd lng="fr">jaune d'&oelig;uf</trd>
  <trd lng="de">Eigelb</trd>
  <trd lng="hu">toj&aacute;ss&aacute;rg&aacute;ja</trd>
  <trdgrp lng="id">
    <trd><ind>kuning</ind> telur</trd>,
    <trd>yolk</trd>
  </trdgrp>
  <trd lng="nl">eigeel</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_k;</trd>
  <trd lng="sv">&auml;ggula</trd>
  <trd lng="tr">yumurta sar&inodot;s&inodot;</trd>
</drv>

</art>
