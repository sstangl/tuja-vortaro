<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: forint.xml,v 1.13 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>forint</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="forint.0o">
  <kap><tld/>o</kap>
  <snc mrk="forint.0o.MON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Hungara <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        la tutan historion elpensis nur mi por eltiri 600 <tld/>ojn el 
        lia monujo
        <fnt>
        <bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_f;&c_o;&c_r;&c_i;&c_n;&c_t;</trd>
  <trd lng="cs">forint</trd>
  <trd lng="de">Forint</trd>
  <trd lng="en">forint</trd>
  <trd lng="es">forinto</trd>
  <trd lng="fr">forint</trd>
  <trd lng="hu">forint</trd>
  <trd lng="id">forint</trd>
  <trd lng="nl">forint</trd>
  <trd lng="pl">forint</trd>
  <trd lng="pt">forinte</trd>
  <trd lng="sk">forint</trd>
</drv>
</art>

