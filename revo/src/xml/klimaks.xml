<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klimaks.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klimaks</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klimaks.0o">
  <kap><tld/>o</kap>
  <snc mrk="klimaks.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Plej forta, kulmina ero de kreska serio:
      <ekz>
        amhistorio milde ironia de L&aacute;szl&oacute; B&ouml;ti, kun tragika, surpriza
        fin<tld/>o
        <fnt><bib>Plumamikoj</bib>, <lok>prozo fikcia</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kulmin.0o.FIG">kulmino <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>gradační vyvrcholení mluv. projevu</trd>,
    <trd>klimax</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Steigerung</trd>
  </trdgrp>
  <trd lng="en">climax</trd>
  <trd lng="es">cl&iacute;max</trd>
  <trd lng="fr">paroxysme</trd>
  <trdgrp lng="hu">
    <trd>tet&odblac;pont</trd>,
    <trd>betet&odblac;z&eacute;s</trd>
  </trdgrp>
  <trd lng="it">climax</trd>
  <trdgrp lng="nl">
    <trd>hoogtepunt</trd>,
    <trd>climax</trd>
  </trdgrp>
  <trd lng="pl">klimaks</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_sh;&c_a;&c_ja; &c_t;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_l;&c_mol;&c_m;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gradačné vyvrcholenie</trd>,
    <trd>klimax</trd>,
    <trd>vrchol <klr>(odb.)</klr></trd>
  </trdgrp>
</drv>
</art>

