<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brajl1.xml,v 1.11 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>brajl</rad>/i
</kap>

<drv mrk="brajl1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Faldi velon sur la jardon.
    </dif>
  </snc>
  <trd lng="cs">psát slepeckým písmem</trd>
  <trdgrp lng="de">
    <trd>aufgeien</trd>,
    <trd>geien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brail</trd>,
    <trd>draw in</trd>,
    <trd>haul up</trd>
  </trdgrp>
  <trd lng="fr">carguer</trd>
  <trdgrp lng="hu">
    <trd><klr tip="amb">(vitolrl&aacute;t)</klr> felteker</trd>,
    <trd><klr tip="amb">(vitolrl&aacute;t)</klr> becsavar</trd>
  </trdgrp>
  <trdgrp lng="ru">
   <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; 
&c_g;&c_i;&c_t;&c_o;&c_v;&c_y;</trd>,
   <trd>&c_u;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; 
&c_p;&c_a;&c_r;&c_u;&c_s;&c_a;</trd>
</trdgrp>
  <trd lng="sk">poskladať plachtu na rahno</trd>
</drv>
</art>
