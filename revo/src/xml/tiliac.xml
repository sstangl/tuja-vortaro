<art mrk="$Id: tiliac.xml,v 1.8 2013/08/25 16:11:22 revo Exp $">
<kap>
  <rad>tiliac</rad>/oj
</kap>
<drv mrk="tiliac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Tiliaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tili&agrave;cies</trd>
  <trd lng="de">Lindengew&auml;chse</trd>
  <trd lng="hu">h&aacute;rs(fa)f&eacute;l&eacute;k</trd>
  <trd lng="nl">lindefamilie</trd>
  <trdgrp lng="pt">
    <trd>tili&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_p;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
