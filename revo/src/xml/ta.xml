<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ta.xml,v 1.11 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>ta</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <ref tip="dif" cel="daux.0o">da&ubreve;o</ref>
  </snc>
  <rim mrk="ta.0o.RIM">
    &leftquot;Tao&rightquot; &leftquot;taoismo&rightquot; kaj
    &leftquot;taoisto&rightquot; estas pli uzataj, sed malpli regulaj
    formoj ol &leftquot;da&ubreve;o&rightquot;,
    &leftquot;da&ubreve;ismo&rightquot;, &leftquot;da&ubreve;isto.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="ca">
    <trd>dao</trd>,
    <trd>tao</trd>
  </trdgrp>
  <trd lng="es">tao</trd>
  <trdgrp lng="fr">
    <trd>dao<klr> (tao)</klr></trd>,
    <trd>tao</trd>
  </trdgrp>
  <trd lng="hu">tao</trd>
  <trd lng="pl">tao</trd>
  <trdgrp lng="pt">
   <trd>tao</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_o;</trd>
  <trd lng="zh">&#36947;<klr> [d&agrave;o]</klr></trd>
  <trd lng="sk">tao</trd>
  <trdgrp lng="cs">
    <trd>dao</trd>,
    <trd>tao</trd>
  </trdgrp>
</drv>
</art>
