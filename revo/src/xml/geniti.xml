<vortaro>
<art mrk="$Id: geniti.xml,v 1.19 2015/10/02 05:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>genitiv</rad>/o
</kap>
<drv mrk="geniti.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      Deklinacia <ref cel="kaz.0o.GRA">kazo</ref>, kiu montras posedon,
      kvaliton, la tuton,
      de kiu oni prenas parton ktp, ordinare tradukita en Esperanto
      per la prepozicioj <ekz>de</ekz> kaj <ekz>da.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_d;&c_n;&c_y; &c_s;&c_k;&c_l;&c_o;&c_n;</trd>
  <trdgrp lng="br">
    <trd>genitiv</trd>,
    <trd>tro-c'henel</trd>
  </trdgrp>
  <trd lng="ca">genitiu</trd>
  <trdgrp lng="cs">
    <trd>2.p&aacute;d</trd>,
    <trd>genitiv</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Genitiv</trd>,
    <trd>Wesfall</trd>,
    <trd>zweiter Fall</trd>
  </trdgrp>
  <trd lng="en">genitive</trd>
  <trd lng="es">genitivo</trd>
  <trd lng="fr">g&eacute;nitif</trd>
  <trdgrp lng="hu">
    <trd>birtokos eset</trd>,
    <trd>genitivus</trd>
  </trdgrp>
  <trd lng="nl">genitief</trd>
  <trd lng="pl">dope&lstroke;niacz</trd>
  <trd lng="pt">genitivo<klr tip="amb"> (gram&aacute;tica)</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>2. p&aacute;d</trd>,
    <trd>genit&iacute;v</trd>
  </trdgrp>
  <trd lng="sv">genitiv</trd>
</drv>
</art>
