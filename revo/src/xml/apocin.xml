<art mrk="$Id: apocin.xml,v 1.9 2013/08/25 13:10:21 revo Exp $">
<kap>
  <rad>apocinac</rad>/oj
</kap>
<drv mrk="apocin.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Apocynaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hundsgiftgew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>met&eacute;ngf&eacute;l&eacute;k</trd>,
    <trd>b&ouml;rv&eacute;nyf&eacute;l&eacute;k</trd>,
    <trd>t&eacute;liz&ouml;ldf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
