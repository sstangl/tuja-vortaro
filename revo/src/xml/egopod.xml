<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: egopod.xml,v 1.17 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>egopodi</rad>/o
</kap>
<drv mrk="egopod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Aegopodium</trd>).
    </dif>
  </snc>
  <bld lok="../bld/egopod.jpg">
    <trd lng="la">Aegopodium podagraria</trd>
  </bld>
  <trd lng="be">&c_s;&c_n;&c_y;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">g&egrave;nere Aegopodium</trd>
  <trdgrp lng="de">
    <trd>Giersch</trd>,
    <trd>Gei&szlig;fu&szlig;</trd>
  </trdgrp>
  <trd lng="en">bishop's weed</trd>
  <trdgrp lng="hu">
    <trd>podagraf&udblac;</trd>,
    <trd>vadbodza</trd>,
    <trd>baktopp</trd>
  </trdgrp>
  <trd lng="pt">egop&oacute;dio</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_y;&c_t;&c_mol; &c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_n;&c_je;&c_d;&c_mol;-&c_t;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">kirsk&aring;l</trd>
  <trd lng="sk">kozia noha</trd>
  <trd lng="cs">bršlice kozí noha</trd>
</drv>
</art>
