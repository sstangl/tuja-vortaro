<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: inflam.xml,v 1.31 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>4</ofc> <rad>inflam</rad>/o
</kap>

<drv mrk="inflam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malbona stato de iu organo de la korpo, karakterizata de
      plialti&gcirc;o
      de la temperaturo, &scirc;velo, ru&gcirc;eco kaj doloro:
      <ekz>
        la medicinistoj uzas la sciencan sufikson -ito por nomi la
        <tld/>ojn:
        bronkito, laringito, ktp.
      </ekz>
    </dif>
    <ref tip="sin" cel="brul.0umo">brulumo</ref>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>inflamace</trd>,
    <trd>vznícení</trd>,
    <trd>vznět</trd>,
    <trd>vzplanutí</trd>,
    <trd>zapálení</trd>,
    <trd>zážeh</trd>
  </trdgrp>
  <trd lng="de">Entz&uuml;ndung<klr tip="ind"> (Med.)</klr></trd>
  <trd lng="en">inflammation</trd>
  <trd lng="es">inflamaci&oacute;n</trd>
  <trd lng="fr">inflammation<klr tip="ind"> (m&eacute;d.)</klr></trd>
  <trd lng="hu">gyullad&aacute;s</trd>
  <trd lng="it">infiammazione<klr> (med.)</klr></trd>
  <trd lng="nl">ontsteking<klr tip="ind"> (med.)</klr></trd>
  <trd lng="pt">inflama&ccedil;&atilde;o<klr tip="amb"> (medicina)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>vzplanutie</trd>,
    <trd>zapálenie</trd>
  </trdgrp>
</drv>

<drv mrk="inflam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Esti en stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>být zanícen</trd>,
    <trd>zanítit se</trd>
  </trdgrp>
  <trd lng="de">entz&uuml;ndet sein</trd>
  <trd lng="en">be inflamed</trd>
  <trd lng="es">estar <ind>inflamado</ind></trd>
  <trd lng="fr">&ecirc;tre <ind>enflamm&eacute;</ind><klr tip="ind">
  (m&eacute;d.)</klr></trd>
  <trdgrp lng="hu">
    <trd>gyullad&aacute;sban van</trd>,
    <trd>gyulladt</trd>
  </trdgrp>
  <trd lng="it">essere <ind>infiammato</ind><klr> (med.)</klr></trd>
  <trd lng="nl">ontstoken zijn</trd>
  <trd lng="pt">inflamar-se<klr tip="amb"> (medicina)</klr></trd>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; <ind>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="sk">zapáliť sa <klr>(lek.)</klr></trd>
</drv>

<drv mrk="inflam.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      En stato de <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">zanícený</trd>
  <trd lng="en">inflamed</trd>
  <trd lng="sk">zapálený</trd>
</drv>

<drv mrk="inflam.0igi">
  <kap><tld/>igi</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on.
    </dif>
  </snc>
  <trd lng="de">entz&uuml;nden</trd>
  <trdgrp lng="en">
    <trd>inflame</trd>,
    <trd>irritate<klr tip="amb"> (medical)</klr></trd>
  </trdgrp>
  <trd lng="es">inflamar</trd>
  <trd lng="fr">enflammer<klr tip="ind"> (m&eacute;d.)</klr></trd>
  <trd lng="hu">gyullad&aacute;sba hoz</trd>
  <trd lng="it">infiammare<klr> (med.)</klr></trd>
  <trd lng="nl">ontsteking veroorzaken<klr tip="ind"> (med.)</klr></trd>
  <trd lng="pt">inflamar<klr tip="amb"> (medicina)</klr></trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol; <ind>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="inflam.gorgx0o">
  <kap>gor&gcirc;<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="laring1.0o">laringito</ref>
  </snc>
  <trd lng="de">Halsentz&uuml;ndung</trd>
  <trdgrp lng="en">
    <trd>throat inflammation</trd>,
    <trd>laryngitis</trd>
  </trdgrp>
  <trd lng="hu">torokgyullad&aacute;s</trd>
  <trdgrp lng="it">
    <trd>laringite</trd>,
    <trd><ind>infiammazione</ind> della gola</trd>,
    <trd>mal di <ind>gola</ind><klr> (infiammazione)</klr></trd>
  </trdgrp>
  <trd lng="nl">keelontsteking</trd>
  <trd lng="pt">inflama&ccedil;&atilde;o de garganta</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_g;&c_o;&c_r;&c_l;&c_a;</trd>
</drv>

<drv mrk="inflam.okul0o">
  <kap>okul<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="oftalm.0o">oftalmito</ref>
  </snc>
  <trd lng="cs">oční zánět</trd>
  <trd lng="de">Augenentz&uuml;ndung</trd>
  <trd lng="en">eye inflammation</trd>
  <trd lng="hu">szemgyullad&aacute;s</trd>
  <trdgrp lng="it">
    <trd>oftalmite</trd>,
    <trd><ind>infiammazione</ind> degli occhi</trd>
  </trdgrp>
  <trd lng="nl">oogontsteking</trd>
  <trd lng="pt">inflama&ccedil;&atilde;o de olho</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
  <trd lng="sk">očný zápal</trd>
</drv>

<drv mrk="inflam.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="otit.0o">otito</ref>
  </snc>
  <trd lng="de">Mittelohrentz&uuml;ndung</trd>
  <trdgrp lng="en">
    <trd>ear inflammation</trd>,
    <trd>otitis</trd>
  </trdgrp>
  <trd lng="fr">otite</trd>
  <trd lng="it">otite</trd>
  <trdgrp lng="nl">
    <trd>oorontsteking</trd>,
    <trd>otitis</trd>
  </trdgrp>
</drv>

<drv mrk="inflam.pulm0o">
  <kap>pulm<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="pneuxm2.0o">pne&ubreve;monio</ref>
  </snc>
  <trd lng="ca">pneum&ograve;nia</trd>
  <trd lng="cs">zápal plic</trd>
  <trdgrp lng="de">
    <trd>Pneumonie</trd>,
    <trd>Lungenentz&uuml;ndung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lung inflammation</trd>,
    <trd>pneumonia</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>neumon&iacute;a</trd>,
    <trd>pneumon&iacute;a</trd>
  </trdgrp>
  <trd lng="hu">t&uuml;d&odblac;gyullad&aacute;s</trd>
  <trd lng="it">polmonite</trd>
  <trdgrp lng="nl">
    <trd>longontsteking</trd>,
    <trd>pneumonie</trd>
  </trdgrp>
  <trd lng="pt">inflama&ccedil;&atilde;o de pulm&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_l;&c_jo;&c_g;&c_k;&c_i;&c_h;</trd>,
    <trd>&c_p;&c_n;&c_je;&c_v;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">zápal pľúc</trd>
</drv>
</art>
