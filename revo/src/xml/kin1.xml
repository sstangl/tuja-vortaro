<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kin1.xml,v 1.4 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kin</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="kin1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="kinkon.0o.KEM">kinkono <sncref/></ref>
    <refgrp tip="hom">
      Ne miksu kun <ref cel="kin.0o">kino</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">kina</trd>
  <trdgrp lng="nl">
    <trd>kinabast<klr tip="ind"> (arch.)</klr></trd>,
    <trd>kinaboom<klr tip="ind"> (arch.)</klr></trd>
  </trdgrp>
  <trd lng="sk">kina</trd>
</drv>
</art>

