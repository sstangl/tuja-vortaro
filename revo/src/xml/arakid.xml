<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arakid.xml,v 1.25 2015/07/04 08:37:30 revo Exp $">
<kap><rad>arakid</rad>/o</kap>

<drv mrk="arakid.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Arachis</trd>);
      <ref tip="lst" cel="herb.0o" lst="voko:faboparencoj">herbo</ref>, 
      kies oleri&ccirc;aj <ref tip="prt" cel="gusx.0o">gu&scirc;oj</ref> maturi&gcirc;as
      en la 
      tero.
    </dif>
  </snc>
  <snc mrk="arakid.0o.mangxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La man&gcirc;ebla grajno de la specio
      <ref tip="malprt" cel="ter.0nukso.BOT">ternukso<sncref 
      ref="ter.0nukso.BOT"/></ref>. 
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_h;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_f;&c_malmol;&c_s;&c_t;&c_malmol;&c_k;</trd>
  <trd lng="ca">cacauet</trd>
  <trdgrp lng="cs">
    <trd>arašíd</trd>,
    <trd>burský oříšek</trd>,
    <trd>podzemnice olejná</trd>
  </trdgrp>
  <trd lng="de">Erdnuss</trd>
  <trd lng="en">peanut</trd>
  <trd lng="es">cacahuate</trd>
  <trdgrp lng="fr">
    <trd>arachide</trd>,
    <trd>cacahu&egrave;te</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>amerikai mogyor&oacute;</trd>,
    <trd>b&uacute;rmogyor&oacute;</trd>,
    <trd>f&ouml;ldimogyor&oacute;</trd>
  </trdgrp>
  <trd lng="id">kacang</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_a;&c_h;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_o;&c_j;
&c_o;&c_r;&c_je;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arašid</trd>,
    <trd>búrsky orech</trd>
  </trdgrp>
  <trd lng="tr">yer f&inodot;st&inodot;&gbreve;&inodot;</trd>
</drv>
</art>
