    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="L"/>o, regno kun malforta industrio,
      ano de la
      <ref tip="vid" cel="mond.tria0a.POL">tria mondo</ref>:
      <ekz>
        multaj evoluantaj <tld/>oj havas multe da &scirc;uldoj 
        kaj devas repagi pruntojn kaj interezojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://monda.eu/eo/modules/poverty/2">
          Malri&ccirc;eco</url></vrk>,
          <lok>monda.eu, [vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#3611;&#3619;&#3632;&#3648;&#3607;&#3624;&#3607;&#3637;&#3656;&#3585;&#3635;&#3621;&#3633;&#3591;&#3614;&#3633;&#3602;&#3609;&#3634;</trd>,
      <trd>&#3611;&#3619;&#3632;&#3648;&#3607;&#3624;&#3650;&#3621;&#3585;&#3607;&#3637;&#3656;&#3626;&#3634;&#3617;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">rozvojov&aacute; zem&ecaron;</trd>
  <trd lng="de">Entwicklungsland</trd>
  <trd lng="es">pa&iacute;s en v&iacute;as de desarrollo</trd>
  <trd lng="fr">pays en voie de d&eacute;veloppement</trd>
  <trd lng="id"><ind>negara</ind> berkembang</trd>
  <trd lng="sk">rozvojov&aacute; krajina</trd>
</drv>

<drv mrk="land.industri0o">
  <kap>industri<tld/>o,
  <var><kap>industria <tld/>o</kap></var></kap>
  <snc mrk="land.industri0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="L"/>o, regno kun forta industrio:
      <ekz>
        la&ubreve; klasifiko de UN, industriaj <tld/>oj estas 
        Nordameriko, E&ubreve;ropo, Japanio, A&ubreve;stralio kaj 
        Nov-Zelando  
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://monda.eu/eo/glossary">
          Terminaro</url></vrk>,
          <lok>monda.eu, [vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3611;&#3619;&#3632;&#3648;&#3607;&#3624;&#3614;&#3633;&#3602;&#3609;&#3634;&#3649;&#3621;&#3657;&#3623;</trd>
  </snc>
  <trd lng="cs">pr&uring;myslov&aacute; zem&ecaron;</trd>
  <trd lng="de">Industrieland</trd>
  <trd lng="es">pa&iacute;s industrializado</trd>
  <trdgrp lng="fr">
