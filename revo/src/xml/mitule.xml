<art mrk="$Id: mitule.xml,v 1.7 2013/08/31 19:10:15 revo Exp $">
<kap><rad>mituled</rad>/oj</kap>
<drv mrk="mitule.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> de
      <ref tip="malprt" cel="valv.du0uloj">duvalvuloj</ref>
      (<trd lng="la">Mytilidae</trd>).
    </dif>
  </snc>
  <trd lng="ca">mit&iacute;lids</trd>
  <trd lng="de">Miesmuscheln</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;kkagyl&oacute;k<klr tip="ind"> (csal&aacute;d)</klr></trd>,
    <trd>feketekagyl&oacute;k<klr tip="ind"> (csal&aacute;d)</klr></trd></trdgrp>
  <trd lng="nl">mossels<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">mitil&iacute;deos</trd>
</drv>
</art>

