<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krabr.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>krabr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krabr.0o">
  <kap><tld/>o</kap>
  <snc mrk="krabr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda specio de la genro <ref tip="super"
      cel="vesp.0o.genro">vespo</ref>
      (<trd lng="la">Vespa crabro</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_r;&c_sh;&c_a;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_t;&c_malmol;&c_r;&c_sh;&c_je;&c_l;</trd>
  <trd lng="cs">sršeň obecná</trd>
  <trd lng="de">Hornisse</trd>
  <trd lng="en">hornet</trd>
  <trd lng="fr">frelon</trd>
  <trd lng="hu">l&oacute;dar&aacute;zs</trd>
  <trd lng="nl">hoornaar</trd>
  <trdgrp lng="pt">
    <trd>marimbondo</trd>,
    <trd>caba</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_je;&c_r;&c_sh;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">sršeň</trd>
</drv>
</art>
