<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vink.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>vink</rad>/o
</kap>
<drv mrk="vink.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref>
        el familio <ref tip="malprt"
       cel="apocin.0oj">apocinacoj</ref>
       (<trd lng="la">Vinca</trd>), kun &ccirc;iamverdaj folioj kaj 
       violbluaj kvinpetalaj floroj.
     </dif>
   </snc>
<bld lok="../bld/vink.jpg">
eta <tld/>o
</bld>
   <trd lng="be">&c_b;&c_a;&c_r;&c_v;&c_je;&c_n;&c_a;&c_k;</trd>
   <trd lng="de">Immergr&uuml;n</trd>
   <trdgrp lng="en">
     <trd>periwinkle</trd>,
     <trd>vinca</trd>
   </trdgrp>
   <trd lng="fr">pervenche</trd>
   <trdgrp lng="hu">
     <trd>met&eacute;ng</trd>,
     <trd>t&eacute;liz&ouml;ld</trd>
   </trdgrp>
   <trd lng="nl">maagdepalm</trd>
   <trd lng="pt">pervinca</trd>
   <trd lng="ru">&c_b;&c_a;&c_r;&c_v;&c_i;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">zimozeleň</trd>
  <trdgrp lng="cs">
    <trd>barvínek</trd>,
    <trd>brčál</trd>
  </trdgrp>
</drv>
</art>
