<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxeval.xml,v 1.68 2014/05/30 08:10:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;eval</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
  
<drv mrk="cxeval.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxeval.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="speci.0o.BIO">Specio</ref> 
      (<trd lng="la">Equus caballus</trd>) de grandaj
      <ref tip="lst" cel="huf.nepar0ulo"
      lst="voko:neparhufuloj">neparhufulaj</ref>
      <ref tip="malprt" cel="mam.0uloj">mamuloj</ref> uzataj ekde
      la antikva epoko kiel rajd- kaj tir-bestoj:
      <ekz>
        azenoj, zebroj, kaj <tld/>oj estas en la sama genro;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj kaj &ccirc;aroj de Faraono kaj liaj
        rajdantoj kaj lia militistaro atingis ilin
        <fnt><bib>MT</bib>, <lok>&Eli; 14:9</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_n;</trd>
    <trd lng="cs">k&uring;&ncaron;</trd>
    <trdgrp lng="de">
      <trd>Pferd</trd>,
      <trd>Gaul<klr> [mezokcidentgermane]</klr></trd>,
      <trd>Ross<klr> [sudgermane]</klr></trd>
    </trdgrp>
    <trdgrp lng="el">
      <trd>&alfa_ton;&lambda;&omikron;&gamma;&omikron;</trd>,
      <trd>&jota_ton;&pi;&pi;&omikron;&sigma_fina;</trd>
    </trdgrp>
    <trd lng="en">horse</trd>
    <trd lng="he">&samekh;&vav;&samekh;</trd>
    <trd lng="hu">l&oacute;</trd>
    <trd lng="ku">hesp</trd>
    <trd lng="os">&c_b;&aelig;&c_h;</trd>
    <trd lng="pl">ko&nacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_mol;</trd>,
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">at</trd>
    <trd lng="vo">jeval</trd>
  </snc>
  <snc mrk="cxeval.0o.sxaka">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="sxak.0peco">&Scirc;akpeco</ref>, 
      kutime figurata per <tld/>a kapo.
    </dif>
    <ref tip="sin" cel="kavali.0o.SPO">kavaliro</ref>
    <trd lng="de">Springer</trd>
    <trd lng="en">knight</trd>
    <trd lng="he"><ind>&pe;&resh;&shin;</ind><klr tip="amb">
        (&shin;&het;&mem;&tet;)</klr></trd>
    <trdgrp lng="hu">
      <trd>l&oacute;</trd>,
      <trd>husz&aacute;r</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_mol;</trd>
  <trd lng="ca">cavall</trd>
  <trd lng="es">caballo</trd>
  <trd lng="fr">cheval</trd>
  <trd lng="nl">paard</trd>
  <trd lng="de">Pferd</trd>
  <trd lng="id">kuda</trd>
  <trd lng="it">cavallo</trd>
  <trdgrp lng="pl">
    <trd>ko&nacute;</trd>,
    <trd>skoczek</trd>
  </trdgrp>
  <trd lng="oc">caval</trd>
  <trd lng="pt">cavalo</trd>
  <trd lng="sk">k&ocirc;&ncaron;</trd>
  <trd lng="tr">at</trd>
  <trdgrp lng="zh">
    <trd>&#39532;</trd>,
    <trd><klr>tr. </klr>&#39340;<klr> [m&#462;]</klr></trd>
  </trdgrp>
</drv>
  
<drv mrk="cxeval.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxeval.0eto.etacxevalo">
    <dif>
      Malgranda <tld/>o.
    </dif>
    <trd lng="de">Pferdchen</trd>
    <trd lng="en">pony</trd>
    <trd lng="hu">lovacska</trd>
    <trd lng="it">cavallino</trd>
    <trd lng="nl">paardje</trd>
    <trdgrp lng="pl">
      <trd>konik</trd>,
      <trd>kucyk</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
    </trdgrp>
    <trd lng="vo">jevalil</trd>
    <trd lng="fr">poney</trd>
  </snc>
  <snc mrk="cxeval.0eto.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="tem.0o">Temo</ref>, pri kiu koncernulo 
      &ccirc;iam volonte <ref cel="parol.0i.diri">parolas</ref>, 
      kiu &ccirc;iam lin <ref cel="intere.0i.per_scivolo">interesas</ref>:
      <ekz>
        tio estas lia amata <tld/>eto<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li tuj ekrajdis sian amatan <tld/>eton pri la supersigna
        reformo;
      </ekz>
      <ekz>
        la facileco de Volap&uuml;k estas la plej amata <tld/>eto,
        sur kiu ili elveturas al &ccirc;iu okazo
        <fnt><aut>&Z;</aut>,
        <lok>citite la&ubreve; PIV, p.158</lok>.
        </fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="am.0atajxo">amata&jcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="sxat.0okupo">&scirc;atokupo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Steckenpferd</trd>,
      <trd>Lieblingsbesch&auml;ftigung</trd>,
      <trd>Hobby</trd>
    </trdgrp>
    <trd lng="en">hobbyhorse</trd>
    <trdgrp lng="fr">
      <trd>dada</trd>,
      <trd>marotte</trd>
    </trdgrp>
    <trd lng="he">&shin;&yod;&gimel;&yod;&vav;&fnun;</trd>
    <trd lng="hu">vessz&odblac;paripa</trd>
    <trd lng="it"><ind>cavallo</ind> di battaglia<klr>
    (fig.)</klr></trd>
    <trdgrp lng="nl">
      <trd>stokpaardje</trd>, <trd>hobby</trd>
    </trdgrp>
    <trd lng="pl">konik</trd>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
    <trd lng="sk">kon&iacute;&ccaron;ek</trd>
  </snc>
</drv>
  
<drv mrk="cxeval.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="cxeval.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Pferdefleisch</trd>
  <trd lng="en">horsemeat</trd>
  <trd lng="es">carne de <ind>caballo</ind></trd>
  <trd lng="fr">viande de <ind>cheval</ind></trd>
  <trd lng="it">carne di <ind>cavallo</ind></trd>
  <trd lng="pl">konina</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_i;&c_n;&c_a;</trd>
</drv>
  
<drv mrk="cxeval.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="cxeval.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Konstrua&jcirc;o en kiu oni tenas <tld/>ojn:
      <ekz>
        en la <tld/>ejo blekis la nigraj <tld/>oj, kiuj meritis 
        admiradon, kaj oni ilin admiris
  	<fnt>
  	  <bib>Fab4</bib>,
  	  <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
  	  filinoj</lok>
  	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pferdestall</trd>
  <trd lng="en">stables</trd>
  <trd lng="es">caballeriza</trd>
  <trd lng="fr">&eacute;curie</trd>
  <trd lng="id">istal</trd>
  <trd lng="it">scuderia</trd>
  <trd lng="pl">stajnia</trd>
  <trd lng="pt">est&aacute;bulo</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_ju;&c_sh;&c_n;&c_ja;</trd>
</drv>
  
<drv mrk="cxeval.0ido">
  <kap><ofc>*</ofc><tld/>ido</kap>
  <snc mrk="cxeval.0ido.ZOO">
    <dif>
      Ido de <tld/>o:
      <ekz>
        <tld/>ido estas nematura <tld/>o
        <fnt><bib>F</bib>, <lok>&FE;36.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_r;&c_a;&c_b;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_a;&c_b;&c_jo;</trd>
  </trdgrp>
  <trd lng="de">Fohlen</trd>
  <trdgrp lng="en">
    <trd>foal</trd>,
    <trd>colt</trd>,
    <trd>filly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>potro</trd>,
    <trd>potranco</trd>
  </trdgrp>
  <trd lng="fr">poulain</trd>
  <trd lng="it">puledro</trd>
  <trdgrp lng="pl">
    <trd>&zacute;rebi&eogonek;</trd>,
    <trd>&zacute;rebak</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>
</drv>
  
<drv mrk="cxeval.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="cxeval.0ino.ZOO">
    <dif>
      Inseksa <tld/>o:
      <ekz>
        Al la <tld/>ino en la &ccirc;aroj de Faraono /
        Mi komparas vin, ho mia amatino
        <fnt><bib>MT</bib>, <lok>&Alt; 1:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_y;&c_l;&c_a;</trd>
  <trd lng="de">Stute</trd>
  <trd lng="en">mare</trd>
  <trd lng="es">yegua</trd>
  <trd lng="fr">jument</trd>
  <trd lng="it">giumenta</trd>
  <trd lng="pl">klacz</trd>
  <trd lng="pt">&eacute;gua</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_b;&c_y;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_b;&c_y;&c_l;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
  
<drv mrk="cxeval.de0igxi">
  <kap>de<tld/>i&gcirc;i</kap>
  <snc mrk="cxeval.de0igxi.RAJ">
    <dif>
      Lasi la rajdadon de <tld/>o:
      <ekz>
        de<tld/>i&gcirc;u, amiko, kaj senbridigu la bestojn
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro
        12a, p. 457a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_l;&c_je;&c_z;&c_mol;&c_c;&c_ib; &c_z; &c_k;&c_a;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">absitzen<klr tip="amb"> (Pferd)</klr></trd>
  <trd lng="fr">descendre de cheval</trd>
  <trd lng="hu">lesz&aacute;ll a <ind>l&oacute;r&oacute;l</ind></trd>
  <trd lng="it"><ind>smontare</ind> da cavallo</trd>
  <trd lng="pl">zsiada&cacute;<klr> z konie</klr></trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
  
<drv mrk="cxeval.mar0eto">
  <kap>mar<tld/>eto</kap>
  <snc mrk="cxeval.mar0eto.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara fi&scirc;eto kun arka korpo
      kaj fleksebla vosto, per kiu &gcirc;i tenas sin al objektoj;
      anka&ubreve; nomata <ref tip="sin" 
      cel="hipokamp1.0o.ZOO">hipokampo</ref>;
      genro el familio singnatedoj
      (<trd lng="la">Hippocampus</trd>):
      <ekz>
        plej okulfrapa estas la mar<tld/>eto <klr>...</klr> &gcirc;ia kapo 
        vere similas al la &ccirc;evala <klr>...</klr> Pro tio &ccirc;i tiu fi&scirc;o 
        ricevis en &ccirc;iuj lingvoj la saman nomon &mdash; mar<tld/>eto
        <fnt>
          <aut>P. Giunio</aut>,
          <vrk>Sekretoj de la maresta&jcirc;oj</vrk>,
          <lok>Kroatia Esperanto-Servo, 1960</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cheval de mer</trd>
  <trd lng="id">kuda laut</trd>
  <trdgrp lng="pl">
    <trd>konik morski</trd>,
    <trd>p&lstroke;awikonik</trd>
  </trdgrp>
</drv>

<drv mrk="cxeval.sur0igxi">
  <kap>sur<tld/>i&gcirc;i</kap>
  <snc mrk="cxeval.sur0igxi.RAJ">
    <dif>
      Eksidi sur la dorso de <tld/>o, enseli&gcirc;i.
    </dif>
  </snc>
  <trd lng="de">aufsitzen<klr tip="amb"> (Pferd)</klr></trd>
  <trd lng="en">mount</trd>
  <trd lng="fr">monter &agrave; cheval</trd>
  <trd lng="hu">felsz&aacute;ll a <ind>l&oacute;ra</ind></trd>
  <trd lng="it"><ind>montare</ind> a cavallo</trd>
  <trdgrp lng="pl">
    <trd>dosiada&cacute;<klr> konia</klr></trd>,
    <trd>wsiada&cacute;<klr> na konia</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_s;&c_t;&c_mol; &c_n;&c_a; &c_k;&c_o;&c_n;&c_ja;</trd>
  <trd lng="tr">ata binmek</trd>
</drv>
  
<drv mrk="cxeval.troja0o">
  <kap>troja <tld/>o</kap>
  <snc mrk="cxeval.troja0o.KOMUNE">
    <dif>
      Insida enigo de malutilaj homoj a&ubreve; rimedoj inter
      malamikojn <klr>(kiel faris antikvaj grekoj kontra&ubreve;
      trojanoj per &ccirc;evalstatuo la&ubreve;
      <ref tip="vid" cel="iliad.0o.BELE">Iliado</ref>)</klr>:
      <ekz>
        la popola fronto estis elpensita de la Tria Internacio kiel
        troja <tld/>o por akiri la kontrolon de la laboristaj
        organizoj
        <fnt>
          <aut>P. A. Foche</aut>,
          <vrk>Re: Pri la nomo Franco</vrk>,
          <lok>
  <url ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>,
            2004-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cxeval.troja0o.KOMP"> <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Scirc;ajne utila, reale malutila programo, kiu ka&scirc;e
      difektas, rompas sekurecon, malfermas konekton al eksterulo
      ktp:
      <ekz>
        pkzip.300 <klr>...</klr> estas Troja <tld lit="&Ccirc;"/>o,
        ne viruso, kaj tre dan&gcirc;era
        <fnt>
          <aut>P. Jelenc</aut>,
          <vrk>Re: pri viruso "zip"</vrk>,
          <lok>
  <url ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
           soc.culture.esperanto</url>,
            1996-10-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_ja;&c_n;&c_s;&c_k;&c_ib; &c_k;&c_o;&c_n;&c_mol;</trd>
  <trd lng="de">Trojanisches Pferd</trd>
  <trd lng="fr">cheval de troie</trd>
  <trd lng="en">Trojan horse</trd>
  <trd lng="id">Kuda Troya</trd>
  <trd lng="it"><ind>cavallo</ind> di Troia</trd>
  <trd lng="pl">ko&nacute; troja&nacute;ski</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j; &c_k;&c_o;&c_n;&c_mol;</trd>
  <trd lng="tr">Truva at&inodot;</trd>
</drv>
  
<drv mrk="cxeval.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="cxeval.vir0o.ZOO">
    <dif>
      Virseksa <tld/>o:
      <ekz>
        <klr>(juna zebrino)</klr> rifu&gcirc;as hazarde en stalon,
        kaj vir<tld/>o rigardas &scirc;in per mirigitaj okuloj
        <fnt>
          <vrk>L. Beaucaire</vrk>,
          <vrk>Kruko kaj Baniko el Bervalo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_r;&c_a;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_a;&c_b;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Hengst</trd>
  <trd lng="en">stallion</trd>
  <trd lng="es">caballo macho</trd>
  <trd lng="fr">&eacute;talon<klr tip="ind"> (cheval)</klr></trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;d&ouml;r</trd>,
    <trd>m&eacute;n</trd>
  </trdgrp>
   <trd lng="it">stallone</trd>
  <trdgrp lng="pl">
    <trd>ogier</trd>,
    <trd>wa&lstroke;ach<klr> (wykastrowany ogier)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_je;&c_b;&c_je;&c_c;</trd>
</drv>

<drv mrk="cxeval.0povo">
  <kap><tld/>povo</kap>
  <snc mrk="cxeval.0povo.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <mlg>&Ccirc;P</mlg>
    <dif>
      Malnova komunuza
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref> de
      me&hcirc;anika povo responda al forto de avera&gcirc;a &ccirc;evalo, 
      egala en E&ubreve;ropo al 75
      <ref cel="kilogr.0metro">kilogrammetroj</ref> en sekundo
      <klr>(735,75 W)</klr>, en Usono al 550 futfuntoj en sekundo
      <klr>(745,7 W)</klr>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_ib;&c_l;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Pferdest&auml;rke</trd>,
    <trd>PS</trd>
  </trdgrp>
  <trd lng="el">&jota;&pi;&pi;&omikron;&delta;&ypsilon_ton;&ny;&alfa;&my;&eta;</trd>
  <trd lng="en">horsepower</trd>
  <trd lng="es"><ind>caballo</ind> de fuerza</trd>
  <trd lng="fr">cheval-vapeur</trd>
  <trd lng="he">&kaf;&vav;&het;-&samekh;&vav;&samekh;</trd>
  <trd lng="hu">l&oacute;er&odblac;</trd>
  <trd lng="id">tenaga kuda</trd>
  <trd lng="it"><ind>cavallo</ind> vapore</trd>
  <trd lng="nl">paardekracht</trd>
  <trd lng="pt">cavalo-vapor</trd>
  <trdgrp lng="pl">
    <trd>ko&nacute; mechaniczny</trd>,
    <trd>KM</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_l;&c_a;</trd>
  <trd lng="sk">konsk&aacute; sila</trd>
</drv>
</art>
