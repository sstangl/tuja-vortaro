<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sved.xml,v 1.27 2015/08/21 22:10:14 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>sved</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="sved.0o">
  <kap><tld/>o
  <fnt><bib>ZR</bib></fnt></kap>
  <snc mrk="sved.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="sved.S0ujo"><tld lit="S"/>ujo</ref>.
    </dif>
    <trd lng="de">schwedischer Staatsb&uuml;rger</trd>
    <trd lng="nl">Zweed</trd>
    <trd lng="pt">sueco</trd>
  </snc>
  <snc mrk="sved.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        intertempe, krom <tld/>ojn, vi renkontas rusojn, osetojn kaj
        talibanojn, kvankam tre mar&gcirc;ene
        <fnt><aut>S. Derevjanni&hcirc;</aut>,
        <vrk><url ref="&amp;Monato/rec/nknu.html">Jen la maltroigo de
        hodia&ubreve;</url></vrk>,
        <bib>Monato</bib>,
        <lok>2001:07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">suec</trd>
    <trd lng="de">Schwede</trd>
    <trd lng="nl">Zweed</trd>    
    <trd lng="pt">sueco</trd>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_e;&c_d;</trd>
  <trd lng="bg">&c_sh;&c_v;&c_je;&c_d;</trd>
  <trd lng="en">Swede</trd>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;&f_y;</trd>
  <trd lng="fr">Su&eacute;dois<klr> (subst.)</klr></trd>
  <trd lng="hu">sv&eacute;d</trd>
  <trd lng="it">svedese<klr> (sost.)</klr></trd>  
  <trd lng="no">svenske</trd>
  <trd lng="pl">Szwed</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_d;</trd>
  <trd lng="sv">svensk</trd>
</drv>

<drv mrk="sved.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco, lingvo a&ubreve; &scirc;tato:
      <ekz>
        el la <tld/>a tradukis Sabira St&aring;hlberg
        <fnt><aut>G. Mattos</aut>,
        <vrk><url ref="&amp;Monato/rec/lmal.html">Etoso de liberi&gcirc;o kaj de
        liberigo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        Hjalmar S&ouml;derberg (1869-1941) estis tre sukcesa
        verkisto <tld/>a
        <fnt><aut>D. Broadribb</aut>,
        <vrk><url ref="&amp;Monato/rec/pelt.html">Facile
        delogebla</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000:04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_e;&c_d;&c_z;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_sh;&c_v;&c_je;&c_d;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">suec</trd>
  <trd lng="cs">&scaron;v&eacute;dsk&yacute;</trd>
  <trd lng="de">schwedisch</trd>
  <trd lng="en">Swedish</trd>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;&f_y;</trd>
  <trd lng="fr">su&eacute;dois<klr> (adj.)</klr></trd>
  <trd lng="hu">sv&eacute;d</trd>
  <trd lng="it">svedese<klr> (agg.)</klr></trd>
  <trd lng="nl">Zweeds</trd>
  <trd lng="no">svensk</trd>
  <trd lng="pl">szwedzki</trd>
  <trd lng="pt">sueco</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">&scaron;v&eacute;dsky</trd>
  <trd lng="sv">svensk</trd>
</drv>

<drv mrk="sved.S0ujo">
  <kap><tld lit="S"/>ujo
  <fnt><bib>ZR</bib></fnt></kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta
      en norda <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      inter Norvegujo kaj Finnlando; alinome
      <ref tip="sin" cel="svedi.0o">Svedio</ref>.
    </dif>
  </snc>
  <trd lng="af">Swede</trd>
  <trd lng="be">&c_Sh;&c_v;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_Sh;&c_v;&c_je;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">Su&egrave;cia</trd>
  <trd lng="cs">&Scaron;v&eacute;dsko</trd>
  <trd lng="cy">Sweden</trd>
  <trd lng="de">Schweden</trd>
  <trd lng="da">Sverige</trd>
  <trd lng="el">&Sigma;&omikron;&ypsilon;&epsilon;&delta;&jota_acute;&alfa;</trd>
  <trd lng="en">Sweden</trd>
  <trd lng="es">Suecia</trd>
  <trd lng="eu">Suedia</trd>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;</trd>
  <trd lng="fi">Ruotsi</trd>
  <trdgrp lng="fr">
    <trd>Su&egrave;de</trd>,
    <trd>Royaume de Su&egrave;de</trd>
  </trdgrp>
  <trd lng="gd">an <ind>tSualainn</ind></trd>
  <trd lng="gl">Suecia</trd>
  <trd lng="hr">&Scaron;vedska</trd>
  <trd lng="hu">Sv&eacute;dorsz&aacute;g</trd>
  <trd lng="ia">Svedia</trd>
  <trd lng="id">Swedia</trd>
  <trd lng="is">Sv&iacute;&#254;j&oacute;&#240;</trd>
  <trd lng="it">Svezia</trd>
  <trd lng="la">Suecia</trd>
  <trd lng="lt">&Scaron;vedija</trd>
  <trd lng="lv">Zviedrija</trd>
  <trd lng="nl">Zweden</trd>
  <trd lng="no">Sverige</trd>
  <trd lng="oc">Su&egrave;cia</trd>
  <trd lng="pl">Szwecja</trd>
  <trd lng="pt">Su&eacute;cia</trd>
  <trd lng="ru">&c_Sh;&c_v;&c_je;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">&Scaron;v&eacute;dsko</trd>
  <trd lng="sv">Sverige</trd>
  <trd lng="sw">Uswidi</trd>
  <trd lng="tr">&Idot;sve&ccedil;</trd>
  <trd lng="vo">Sved&auml;n</trd>
</drv>
</art>
