<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: acetilhxolin.xml,v 1.1 2013/11/23 10:10:17 revo Exp $">
<kap>
  <rad>acetil&hcirc;olin</rad>/o
</kap>
<drv mrk="acetilhxolin.0o">
  <kap><tld/>o</kap>
  <snc mrk="acetilhxolin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super"
      cel="trans.nervo0igilo.FIZL">Nervotransigilo</ref>
      aktiva en la centra kaj en la malcentra nervaro;
      simbole 
      <frm>C<sub>7</sub>H<sub>16</sub>NO<sub>2</sub></frm>;
      <ref tip="sin" cel="acetilkolin.0o">acetilkolino</ref>:
      <ekz>
        bazo de la kuracado estas meza dozo de kortikoidoj 
        <klr>...</klr>
        kun <tld/>o en funkcio kiel antioksida aganto
        <fnt>
          <aut>J. Hradil</aut>,
          <vrk>Idiopatia pulma fibrozo</vrk>,
          <lok>en: <url
          ref="http://www.eventoj.hu/steb/medicino/med-mallonge.htm">
          Mallongaj artikoloj</url>, [vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">acetylcholine</trd>
  <trd lng="fr">ac&eacute;tylcholine</trd>
</drv>
</art>

