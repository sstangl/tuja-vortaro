<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sporad.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>sporad</rad>/a
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sporad.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tute malofta, dissemita, renkontebla nur dise:
      <ekz>
        &gcirc;is nun tiu tasko estis nur <tld/>e plenumita;
      </ekz>
      <ekz>
        strikoj <tld/>e okazis en la distrikto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rar.0a">rara</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_r;&c_e;&c_g;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_p;&c_o;&c_r;&c_a;&c_d;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="ca">espor&agrave;dic</trd>
  <trdgrp lng="cs">
    <trd>ojedinělý</trd>,
    <trd>sporadický</trd>,
    <trd>zřídkavý</trd>
  </trdgrp>
  <trd lng="de">sporadisch</trd>
  <trd lng="en">sporadic</trd>
  <trd lng="fr">sporadique</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;rv&aacute;nyos</trd>,
    <trd>esetenk&eacute;nti</trd>
  </trdgrp>
  <trd lng="nl">sporadisch</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_r;&c_a;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_je;&c_g;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ojedinelý</trd>,
    <trd>sporadický</trd>
  </trdgrp>
  <trd lng="sv">sporadisk</trd>
</drv>
</art>
