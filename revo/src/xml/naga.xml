<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: naga.xml,v 1.1 2014/09/17 01:11:01 revo Exp $">
<kap>
  <rad>naga</rad>/o
</kap>
<drv mrk="naga.0o">
  <kap><tld/>o</kap>
  <snc mrk="naga.0o.ETN">
    <dif>
      Ano de <ref tip="lst" cel="popol.0o.etno"
      lst="voko:popolanoj">popolo</ref> plurgenta,
      vivanta en nordokcidenta Birmo kaj nordorienta
      Hindujo, precipe en <tld lit="N"/>lando:
      <ekz>
        anka&ubreve; inter la &jcirc;uangoj trovi&gcirc;as tiu 
        bambua danco, kiun mi anta&ubreve;e vidis inter la 
        aborigenoj de la <tld/>a nacieco
        <fnt>
          <aut>[P. Dasgupta]</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/362/2008/12/05/1s89595.htm">
          3-an de decembro</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2008-12-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">naga</trd>
</drv>

<drv mrk="naga.0lando">
  <kap><tld lit="N"/>lando <fnt><bib>NePiv</bib></fnt>
  <fnt><bib>LanLin</bib></fnt></kap>
  <snc mrk="naga.0lando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la orienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        tiu regiono en 1962 fari&gcirc;is federacia &scirc;tato <tld lit="N"/>lando 
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Nagaoj">
          nagaoj</url>, 2013-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Nagaland</trd>
</drv>
</art>

