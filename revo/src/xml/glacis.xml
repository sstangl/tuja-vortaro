<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glacis.xml,v 1.13 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>glacis</rad>/o
</kap>
<drv mrk="glacis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Neabrupta deklivo, natura a&ubreve; artefarita, kiu etendi&gcirc;as
      anta&ubreve; fortika&jcirc;o, paflinio ktp.  
    </dif>
  </snc>
  <trd lng="ca">glacis</trd>
  <trdgrp lng="cs">
    <trd>glacis</trd>,
    <trd>koliště</trd>,
    <trd>parkán</trd>,
    <trd>předhradí</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Glacis</trd>,
     <trd>Vorgel&auml;nde</trd></trdgrp>
  <trd lng="en">glacis</trd>
  <trd lng="fr">glacis<klr tip="ind"> (mil.)</klr></trd>
  <trdgrp lng="hu">
     <trd>s&iacute;kozat</trd>,
     <trd>v&aacute;rmez&odblac;</trd>,
     <trd>glacis</trd></trdgrp>
  <trd lng="nl">glacis</trd>
  <trdgrp lng="pt">
    <trd>glacis</trd>,
    <trd>esplanada</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_s;&c_i;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>glacis</trd>,
    <trd>mierny svah <klr>(pevnostný, zákopový)</klr></trd>
  </trdgrp>
</drv>
</art>
