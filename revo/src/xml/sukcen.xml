<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sukcen.xml,v 1.17 2016/03/12 13:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sukcen</rad>/o
</kap>
<drv mrk="sukcen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola flava diafana fosilia 
      <ref tip="super" cel="rezin.0o">rezino</ref>, el kiu oni faras 
      diversajn objektojn, ekz. ornama&jcirc;ojn a&ubreve; enbu&scirc;a&jcirc;ojn de pipoj:
      <ekz>
        <tld/>o <klr>[...]</klr> facile &scirc;argi&gcirc;as pro frotado per 
        ta&ubreve;ga materialo
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Elektrostatiko">Elektrostatiko</url></vrk>,
        </fnt>;
      </ekz>
      <ekz>
        flavaj kiel sukceno rondaj kiel ovoj de birdoj
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_sh;&c_t;&c_y;&c_n;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_je;&c_h;&c_l;&c_i;&c_b;&c_a;&c_r;</trd>,
    <trd>&c_ja;&c_n;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">ambre</trd>
  <trdgrp lng="cs">
    <trd>jantar</trd>,
    <trd>sukcinit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bernstein</trd>,
    <trd>gelber <ind>Amber</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amber</trd>,
    <trd>succinite</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>&aacute;mbar</trd>,
    <trd>succino</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_h;&a_r;&a_b;&a_A;</trd>
  <trd lng="fr">ambre</trd>
  <trd lng="hu">borosty&aacute;nk&odblac;</trd>
  <trd lng="nl">barnsteen</trd>
  <trd lng="ru">&c_ja;&c_n;&c_t;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">jant&aacute;r</trd>
</drv>
</art>
