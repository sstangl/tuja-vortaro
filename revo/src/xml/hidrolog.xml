<vortaro>
<art mrk="$Id: hidrolog.xml,v 1.5 2014/04/12 14:10:18 revo Exp $">
<kap>
  <rad>hidrolog</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hidrolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrolog.0o.SCI">
    <ref tip="dif" cel="akv.0osciencisto.SCI">akvosciencisto</ref>
    <ekz>
      merkrede alvenis in&gcirc;enieroj, agronomoj, <tld/>oj, topografoj 
      kaj ter-mezuristoj
      <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
      Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="en">hydrologist</trd>
  <trd lng="fr">hydrologue</trd>
  <trd lng="it">idrologo</trd>
  <trdgrp lng="nl">
    <trd>waterbouwkundige</trd>,
    <trd>hydroloog</trd>
  </trdgrp>
</drv>
</art>

