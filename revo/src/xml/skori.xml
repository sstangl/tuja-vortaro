<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skori.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>skori</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skori.0o">
  <kap><tld/>o</kap>
  <snc mrk="skori.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de vitreca materio, kiu per fandado aparti&gcirc;as de
      metalmina&jcirc;oj;
      <ref tip="sin" cel="sxlak.0o.TEK">&scirc;lako</ref>;
      metal&scirc;a&ubreve;mo:
      <ekz>
        dum la fabrikado de &scirc;talo el gisfero, formi&gcirc;as,
        kiel rezulto de puriga procezo, <tld/>o, kiu
        konsistas &ccirc;efe el kompleksaj kombina&jcirc;oj de kalcio
        kun fosforo
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>, 
      <trd>&c_a;&c_k;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_sh;&c_l;&c_a;&c_k;&c_a;</trd>
    <trdgrp lng="en">
      <trd>cinder</trd>,
      <trd>cinders</trd>,
      <trd>clinker</trd>,
      <trd>dross</trd>,
      <trd>scalia</trd>,
      <trd>slag</trd>
    </trdgrp>
    <trd lng="hu">salak</trd>    
    <trd lng="it">scoria<klr> (metallurgica)</klr></trd>
    <trd lng="nl">metaalschuim</trd>
    <trdgrp lng="pl">
      <trd>&zdot;u&zdot;el</trd>, 
      <trd>szlaka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_o;&c_k;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skori.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      For&jcirc;etinda&jcirc;o, senvalora&jcirc;o:
      <ekz>
        via ar&gcirc;ento fari&gcirc;is <tld/>o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fecx.0o">fe&ccirc;o</ref>,
      <ref cel="bala">forbalaa&jcirc;o</ref>,
      <ref cel="rekrem.0o">rekremento</ref>,
      <ref cel="vent">ventuma&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_k;&c_ib;&c_d;&c_y;</trd>, 
      <trd>&c_p;&c_a;&c_z;&c_a;&c_d;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>dross</trd>,
      <trd>slag</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ocs&uacute;</trd>,
      <trd>szem&eacute;t</trd>
    </trdgrp>
    <trd lng="nl">droesem</trd>
    <trd lng="pt">esc&oacute;ria</trd>
    <trd lng="pl">ch&lstroke;am</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_y;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>přen. brak</trd>,
    <trd>struska</trd>,
    <trd>škvára</trd>
  </trdgrp>
  <trd lng="de">Schlacke</trd>
  <trd lng="fr">scorie</trd>
  <trdgrp lng="sk">
    <trd>pren. odpad</trd>,
    <trd>trosky</trd>,
    <trd>škvara</trd>
  </trdgrp>
</drv>
</art>
