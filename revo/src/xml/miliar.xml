<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miliar.xml,v 1.20 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>miliard</rad>/o
</kap>
<drv mrk="miliar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mil milionoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="bilion.0o">biliono</ref>
    </refgrp>
     <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj">nombro</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
  <trd lng="bg">&c_m;&c_i;&c_l;&c_i;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>miliard</trd>,
    <trd>mil milions</trd>
  </trdgrp>
  <trd lng="cs">miliarda</trd>
  <trd lng="de">Milliarde</trd>
  <trd lng="en">billion <klr>(US)</klr></trd>
  <trd lng="fr">milliard</trd>
  <trd lng="hu">milli&aacute;rd</trd>
  <trd lng="id">miliar</trd>
  <trd lng="nl">miljard</trd>
  <trdgrp lng="pt">
  <trd>bilh&atilde;o <klr>(1.000.000.000)</klr></trd>,
  <trd>bili&atilde;o <klr>(1.000.000.000)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_a;&c_r;&c_d;</trd>
  <trd lng="sk">miliarda</trd>
  <trd lng="sv">miljard</trd>
  <trd lng="vo">baliad</trd>
</drv>
</art>
