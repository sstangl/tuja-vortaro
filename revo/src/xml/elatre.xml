  </snc>
  <trd lng="de"><klr>[Familie der] </klr>Schnellk&auml;fer</trd>
  <trd lng="fr">&eacute;lat&eacute;rid&eacute;s</trd>
  <trd lng="hu">pattan&oacute;bogarak</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_k;&c_u;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_u;&c_k;&c_o;&c_v;)</klr></trd>
</drv>
</art>
