<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rikan.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>rikan</rad>/i
</kap>
<drv mrk="rikan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Duonridi a&ubreve; pro stulteco a&ubreve; pro malica moko.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_k;&c_a;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_l;&c_ja; &c_s;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_h;&c_i;&c_l;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>posmívat se</trd>,
    <trd>pošklebovat se</trd>,
    <trd>šklebit se zlomyslně</trd>
  </trdgrp>
  <trd lng="de">grinsen</trd>
  <trdgrp lng="en">
    <trd>sneer</trd>,
    <trd>laugh derisively</trd>
  </trdgrp>
  <trd lng="fr">ricaner</trd>
  <trd lng="hu">vigyorog</trd>
  <trd lng="nl">grijnzen</trd>
  <trdgrp lng="pt">
    <trd>ca&ccedil;oar</trd>,
    <trd>escarnecer</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_o;&c_s;&c_k;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>uškeriť sa</trd>,
    <trd>škľabiť sa</trd>
  </trdgrp>
</drv>
</art>
