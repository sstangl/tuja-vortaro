<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bergam.xml,v 1.23 2015/10/21 14:10:21 revo Exp $">
<kap>
<rad>bergamot</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bergam.0o">
  <kap><tld/>o</kap>
  <snc mrk="bergam.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Frukto de varia&jcirc;o de
      <ref tip="super" cel="orangx.0arbo.BOT">oran&gcirc;arbo</ref>
      (<trd lng="la">Citrus aurantium bergamia</trd>),
      el kies &scirc;elo oni ekstraktas bonodoran oleon.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
  <trd lng="ca">bergamota</trd>
  <trdgrp lng="cs">
    <trd>Sevillsk&yacute; pomeran&ccaron;</trd>,
    <trd>ho&rcaron;k&yacute; pomeran&ccaron;</trd>,
    <trd>mo&rcaron;sk&yacute; pomeran&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Bergamotte-Frucht</trd>
  <trd lng="en">bergamot orange</trd>
  <trd lng="es">bergamota</trd>
  <trd lng="fr">bergamote</trd>
  <trd lng="hu">bergamott</trd>
  <trdgrp lng="nl">
    <trd>bergamot</trd>,
    <trd>maatjespeer</trd>
  </trdgrp>
  <trd lng="pl">bergamota</trd>
  <trd lng="pt">bergamota</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>bergamota</trd>,
    <trd>druh citr&oacute;novn&iacute;ka</trd>
  </trdgrp>
  <trd lng="sv">bergamott</trd>
</drv>

<drv mrk="bergam.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="bergam.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="citrus.0o">citruso</ref>.
    </dif>
  </snc>
  <trd lng="cs">bergamot strom</trd>
  <trd lng="de">Bergamotte</trd>
  <trd lng="en">bergamot tree</trd>
  <trdgrp lng="nl">
    <trd>bergamotboom</trd>,
    <trd>maatjesperenboom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bergamota</trd>,
    <trd>bergamotka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bergamot</trd>,
    <trd>strom</trd>
  </trdgrp>
</drv>
</art>
