<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: terebi.xml,v 1.16 2016/01/19 10:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>terebint</rad>/o
</kap>
<drv mrk="terebi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terebi.0o.vera">
    <dif>
      Rezineca diafana duonflua&jcirc;o, kiu elfluas &scirc;vite el
      <ref tip="malprt" cel="terebi.0arbo"><tld/>arbo</ref>.
    </dif>
    <trd lng="pl">terpentyna</trd>
  </snc>
  <snc mrk="terebi.0o.konifera">
    <dif>
      Simila substanco el&scirc;vitata el diversaj koniferoj.
    </dif>
  </snc>
  <trd lng="ca">trementina</trd>
  <trdgrp lng="cs">
    <trd>pist&aacute;cie terebintov&aacute;</trd>,
    <trd>terebint <klr>(strom)</klr></trd>,
    <trd>&rcaron;e&ccaron;&iacute;k terebintov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Terpentin</trd>
  <trdgrp lng="en">
    <trd>oleoresin</trd>,
    <trd>turpentine</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;r&eacute;benthine</trd>
  <trd lng="hu">terpentin</trd>
  <trd lng="id">terpentin</trd>
  <trd lng="nl">terpentijn</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">pist&aacute;cia terebintov&aacute;</trd>
</drv>

<drv mrk="terebi.0arbo">
  <kap><tld/>arbo</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="pistak.0ujo">pistakujo</ref>
      (<trd lng="la">Pistacia terebinthus</trd>), malgranda arbo.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>noguerola</trd>,
    <trd>terebint</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pist&aacute;cie terebintov&aacute;</trd>,
    <trd>terebint</trd>,
    <trd>&rcaron;e&ccaron;&iacute;k terebintov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Terpentinpistazie</trd>
  <trd lng="en">terebinth</trd>
  <trd lng="fr">t&eacute;r&eacute;binthe</trd>
  <trdgrp lng="hu">
    <trd>terebint piszt&aacute;cia</trd>,
    <trd>terebintfa</trd>
  </trdgrp>
  <trd lng="nl">terpentijnboom</trd>
  <trd lng="pl">pistacja <ind>terpentynowa</ind></trd>
  <trd lng="ru">
	&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">terebinta</trd>
</drv>

<drv mrk="terebi.0oleo">
  <kap><tld/>oleo</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      Hidrokarbonido ricevita per distilado de la <tld/>o kaj
      servanta precipe por solvi la grasa&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="ca">ess&egrave;ncia de <ind>trementina</ind></trd>
  <trd lng="cs">terpent&yacute;nov&yacute; olej</trd>
  <trd lng="de">Terpentin&ouml;l</trd>
  <trdgrp lng="en">
    <trd>turpentine</trd>,
    <trd>oil of turpentine</trd>
  </trdgrp>
  <trd lng="fr">essence de <ind>t&eacute;r&eacute;benthine</ind></trd>
  <trd lng="hu">terpentinolaj</trd>
  <trd lng="nl">terpentijnolie</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je;
    &c_m;&c_a;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_p;&c_i;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">terpent&iacute;nov&yacute; olej</trd>
</drv>
</art>
