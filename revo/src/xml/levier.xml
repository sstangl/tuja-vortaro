<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: levier.xml,v 1.7 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>levier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="levier.0o">
  <kap><tld/>o</kap>
  <snc mrk="levier.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Komandostango kun baza akso, kiun oni pu&scirc;as supren, malsupren a&ubreve;
      flanken, uzebla kiel <ref tip="super"
      cel="reg.0ilo.TEK">regilo</ref> de aparato:
      <ekz>
        dum minuto a&ubreve;dis li longajn zumojn, poste premis la <tld/>on
        kaj
        telefonis al Jamburg
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 16a, p. 100a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lev.0ilo">levilo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">páka</trd>
  <trd lng="de">Joystick</trd>
  <trdgrp lng="fr">
    <trd>manette</trd>,
    <trd>levier</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>knuppel<klr tip="ind"> (stuurstang)</klr></trd>,
    <trd>stuurstang</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>manete</trd>,
    <trd>joystick <klr>(EN)</klr></trd>
  </trdgrp>
  <trd lng="sk">páka</trd>
</drv>
</art>

