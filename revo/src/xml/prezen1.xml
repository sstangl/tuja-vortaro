  </trdgrp>
</drv>

<drv mrk="prezen1.mis0i">
  <kap>mis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mal&gcirc;uste, false <tld/>i:
      <ekz>
        a&ubreve; pro nescio a&ubreve; pro pasio li mis<tld/>is la decidojn de
        la Akademio<fnt>PIV</fnt>;
      </ekz>
      <ekz>
        vortaroj mis<tld/>as tiujn homonimojn samradikaj vortoj
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>deformar</trd>,
    <trd>tergiversar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nesprávně přeložit</trd>,
    <trd>překroutit</trd>,
    <trd>zkomolit smysl</trd>,
    <trd>zkreslit</trd>
  </trdgrp>
  <trd lng="en">misrepresent</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lre&eacute;rtelmez</trd>,
    <trd>t&eacute;vesen <ind>ismertet</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v; &c_l;&c_o;&c_zh;&c_n;&c_o;&c_m; &c_s;&c_v;&c_je;&c_t;&c_je;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skomoliť zmysel</trd>,
    <trd>skresliť</trd>
  </trdgrp>
</drv>

<drv mrk="prezen1.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Denove, ree <tld/>i:
      <ekz>
        ne re<tld/>u vin de nun anta&ubreve; mi;
      </ekz>
      <ekz>
        re<tld/>i malakceptitan proponon;
      </ekz>
      <ekz>
        ili ankora&ubreve; trifoje re<tld/>is tiun teatra&jcirc;on.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="reprezent.0i">reprezent&middot;i</ref>
  <trd lng="ca">presentar <klr>(representar)</klr> de nou</trd>
  <trdgrp lng="hu">
    <trd>&uacute;jra <ind>bemutat</ind></trd>,
    <trd>ism&eacute;t <ind>beterjeszt</ind></trd>,
    <trd>ism&eacute;t <ind>bead</ind></trd>
  </trdgrp>
  <trd lng="nl">vertegenwoordigen</trd>
  <adm>
    &Ccirc;u la nl-traduko plu &gcirc;ustas post dispartigo de "reprezent"?
    <aut>Sergio</aut>
  </adm>
  <trdgrp lng="sk">
    <trd>predstavovať</trd>,
    <trd>reprezentovať</trd>,
    <trd>zastupovať</trd>,
    <trd>znovu predniesť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>představovat</trd>,
    <trd>reprezentovat</trd>,
    <trd>zastupovat</trd>
  </trdgrp>
</drv>

<drv mrk="prezen1.grafika0o">
  <kap>grafika <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de reela <ref cel="funkci.0o.MAT">funkcio</ref>
      <frm><k>f</k></frm>)</klr>
      Tia <ref cel="kurb.0o.MAT">ebena kurbo</ref>, ke la paro de
      karteziaj koordinatoj <frm>(<k>x</k>,<k>y</k>)</frm> de &ccirc;iu
      punkto de &gcirc;i apartenas al la
      <ref cel="grafik.0ajxo.MAT">grafika&jcirc;o</ref>
      de <frm><k>f</k></frm> kaj inverse:
      <ekz>
        la grafika <tld/>o de funkcio <frm><k>x</k><sup>2</sup></frm>
        estas
        <ref cel="parabo.0o.linio">parabolo</ref>.
      </ekz>
    </dif>
    <rim>
      Ne ekzistas esenca diferenco inter &ccirc;i tiu nocio kaj
      <ctl>grafika&jcirc;o</ctl>.
    </rim>
  </snc>
  <trdgrp lng="ca">
    <trd>representaci&oacute; <ind>gr&agrave;fica</ind></trd>,
    <trd>diagrama</trd>
  </trdgrp>
  <trd lng="cs">grafické vyjádření <klr>(reálné funkce)</klr></trd>
  <trdgrp lng="de">
    <trd>graphische <ind>Darstellung</ind><klr tip="ind"> (einer
    Funktion)</klr></trd>,
    <trd>Bildkurve<klr tip="ind"> (einer Funktion)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>graphic <ind>representation</ind><klr tip="ind"> (of a
    function)</klr></trd>,
    <trd>plot<klr tip="ind"> (of a function)</klr></trd>,
    <trd>image curve<klr tip="ind"> (of a function)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>repr&eacute;sentation graphique<klr tip="ind"> (d'une
    fonction)</klr></trd>,
    <trd>graphe<klr tip="ind"> (d'une fonction)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>grafikus <ind>&aacute;br&aacute;zol&aacute;s</ind><klr tip="ind">
    (f&uuml;ggv&eacute;ny&eacute;)</klr></trd>,
    <trd>grafikon<klr tip="ind"> (f&uuml;ggv&eacute;ny&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="pl">wykres<klr tip="ind"> (funkcji)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind><klr tip="ind">
    (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_i;)</klr></trd>,
    <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_k;<klr tip="ind"> (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_i;)</klr></trd>
  </trdgrp>
  <trd lng="sk">grafické vyjadrenie <klr>(reálnej funkcie)</klr></trd>
</drv>

<drv mrk="prezen1.numa0o">
  <kap>n-uma <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <subsnc mrk="prezen1.numa0o.entjero">
      <dif>
        <klr>(de <ref cel="entjer.0o">entjero</ref> la&ubreve;
        <ref cel="baz.0o.nombrosistema">bazo <sncref/></ref>
        <frm><k>n</k></frm>)</klr>
        Skriba&jcirc;o de la tipo
        <frm><k>c</k><sub><k>p</k></sub>...<k>c</k><sub>1</sub><k>c</k><sub>0</sub></frm>,
        konsistanta el ciferoj <frm><k>c</k><sub><k>i</k></sub></frm>
        inter <frm>0</frm> kaj <frm><k>n</k>-1</frm>, kaj prezentanta
        la nombron
        <frm><k>c</k><sub><k>p</k></sub><k>n</k><sup><k>p</k></sup> +...
        +
        <k>c</k><sub>2</sub><k>n</k><sup>2</sup> +
        <k>c</k><sub>1</sub><k>n</k> + <k>c</k><sub>0</sub></frm>:
        <ekz>
          la dekuma <tld/>o de la nombro cent estas <frm>100</frm>, kaj
          &gcirc;ia okuma <tld/>o estas <frm>144</frm>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="prezen1.numa0o.reelo">
      <dif>
        <klr>(de <ref cel="reel.0o.MAT">reelo</ref> la&ubreve;
        <ref cel="baz.0o.nombrosistema">bazo <sncref/></ref>
        <frm><k>n</k></frm>)</klr>
        Skriba&jcirc;o de la tipo
        <frm><k>c</k><sub><k>p</k></sub>...<k>c</k><sub>1</sub><k>c</k><sub>0</sub>,<k>c</k><sub>-1</sub><k>c</k><sub>-2</sub>...<k>c</k><sub>-<k>k</k></sub>...</frm>,
        a&ubreve;
        <frm><k>c</k><sub><k>p</k></sub>...<k>c</k><sub>1</sub><k>c</k><sub>0</sub>.<k>c</k><sub>-1</sub><k>c</k><sub>-2</sub>...<k>c</k><sub>-<k>k</k></sub>...</frm>,
        konsistanta el ciferoj <frm><k>c</k><sub><k>i</k></sub></frm>
        inter <frm>0</frm> kaj <frm><k>n</k>-1</frm>, kaj prezentanta
        la nombron
        <frm><k>c</k><sub><k>p</k></sub><k>n</k><sup><k>p</k></sup> +...
        +
        <k>c</k><sub>2</sub><k>n</k><sup>2</sup> +
        <k>c</k><sub>1</sub><k>n</k> + <k>c</k><sub>0</sub> +
        <k>c</k><sub>-1</sub>/<k>n</k> +
        <k>c</k><sub>-2</sub>/<k>n</k><sup>2</sup> + ... +
        <k>c</k><sub><k>k</k></sub>/<k>n</k><sup><k>k</k></sup>...</frm>:
        <ekz>
          la triuma <tld/>o de <frm>&#8531;</frm> estas <frm>0,1</frm>, sed
          &gcirc;ia dekuma <tld/>o estas senfina <frm>0,3333...</frm>;
        </ekz>
        <ekz>
          la duuma prezento de <frm>&pi;</frm> estas
          <frm>11,0010010000111111011010101000...</frm>
        </ekz>
      </dif>
      <ref tip="sub" cel="frakci.pozicia0o">pozicia frakcio.</ref>
    </subsnc>
    <rim>
      La rangon de cifero <frm><k>c</k><sub><k>i</k></sub></frm> a&ubreve;
      <frm><k>c</k><sub>-<k>i</k></sub></frm> oni kutime
      referencas pere de la nombro
      <frm><k>n</k><sup><k>i</k></sup></frm> a&ubreve;
      <frm>1/<k>n</k><sup><k>i</k></sup></frm>,
      kiun &gcirc;i multiplikas. Ekz-e en la dekuma prezento oni havas
      la sekvan sistemon:
      <ekz>
        cifero de la milonoj
        <klr>(<frm><k>c</k><sub>-3</sub></frm>)</klr>,
      </ekz>
      <ekz>
        centonoj <klr>(<frm><k>c</k><sub>-2</sub></frm>)</klr>,
      </ekz>
      <ekz>
        dekonoj <klr>(<frm><k>c</k><sub>-1</sub></frm>)</klr>,
      </ekz>
      <ekz>
        unuoj <klr>(<frm><k>c</k><sub>0</sub></frm>)</klr>,
      </ekz>
      <ekz>
        dekoj <klr>(<frm><k>c</k><sub>1</sub></frm>)</klr>,
      </ekz>
      <ekz>
        centoj <klr>(<frm><k>c</k><sub>2</sub></frm>)</klr>,
      </ekz>
      <ekz>
        miloj <klr>(<frm><k>c</k><sub>3</sub></frm>)</klr>...
      </ekz>
    </rim>
    <trd lng="ca">representaci&oacute; num&egrave;rica en base n <ind>(d'un enter o un
    fraccionari)</ind></trd>
    <trd lng="de">Zahlendarstellung</trd>
    <trd lng="en">representation<klr tip="ind"> (of a number in base
    n)</klr></trd>
    <trdgrp lng="fr">
      <trd>repr&eacute;sentation<klr tip="ind"> (d'un nombre en base
      n)</klr></trd>,
      <trd>&eacute;criture<klr tip="ind"> (d'un nombre en base n)</klr></trd>
    </trdgrp>
    <trd lng="hu">n-alap&uacute; <ind>sz&aacute;m&aacute;br&aacute;zol&aacute;s</ind></trd>
    <trd lng="pl">rozwini&eogonek;cie<klr tip="ind"> (liczby podstawie
    n)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_ch;&c_i;&c_s;&c_l;&c_a; &c_v; n-&c_i;&c_ch;&c_n;&c_o;&c_j;
    &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_je;)</klr></trd>
  </snc>
  <trd lng="cs">zápis čísla</trd>
  <trd lng="sk">zápis čísla</trd>
</drv>
