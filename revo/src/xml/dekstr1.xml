<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dekstr1.xml,v 1.14 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>dekstrin</rad>/o
</kap>

<drv mrk="dekstr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="dekstr1.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Amela gumo:
      <ekz>
        la gluo devas esti el <tld/>o
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url
        ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve;
        fabeloj</url></vrk>,
        <lok>Fabelo po&scirc;tista, [2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_s;&c_t;&c_r;&c_y;&c_n;</trd>
  <trd lng="cs">dextrin</trd>
  <trd lng="de">Dextrin</trd>
  <trd lng="es">dextrina</trd>
  <trd lng="fr">dextrine</trd>
  <trd lng="hu">dextrin</trd>
  <trd lng="pl">dekstryna</trd>
  <trd lng="pt">dextrina</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_s;&c_t;&c_r;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>dextrín</trd>,
    <trd>škrobové lepidlo    *</trd>
  </trdgrp>
</drv>
</art>
