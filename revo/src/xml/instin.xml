<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: instin.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>instinkt</rad>/o
</kap>
<drv mrk="instin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      A&ubreve;tomata aktiveco, ekzistanta precipe &ccirc;e la bestoj,
      kiu evidenti&gcirc;as per nevolaj, nelernitaj kaj neperfektigeblaj
      agoj:
      <ekz>
        la kruelaj <tld/>oj de la tigro;
      </ekz>
      <ekz>
        la mirinda <tld/>o de la abelo, kastoro, formiko;
      </ekz>
      <ekz>
        la migrantaj birdoj sin orientas per nekomprenebla <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de memkonservado<fnt>Z</fnt>,
      </ekz>
      <ekz>
        memkonserva <tld/>o
        <klr>(<tld/>o per kiu la besto a&ubreve; la homo faras la agojn
        necesajn al konservado de sia vivo)</klr>;
      </ekz>
      <ekz>
        kvaza&ubreve; kondukita de natura <tld/>o;
      </ekz>
      <ekz>
        <tld/>a movo <klr>(ne regata de la volo)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="impuls.0o">impulso</ref>,
      <ref cel="intuic.0o">intuicio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_s;&c_t;&c_y;&c_n;&c_k;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>instinkt</trd>,
    <trd>pud</trd>
  </trdgrp>
  <trd lng="de">Instinkt</trd>
  <trd lng="en">instinct</trd>
  <trd lng="es">instinto</trd>
  <trd lng="fa">&a_gh;&a_r;&f_y;&a_z;&a_h;</trd>
  <trd lng="fr">instinct</trd>
  <trd lng="hu">&ouml;szt&ouml;n</trd>
  <trd lng="nl">instinct</trd>
  <trd lng="pl">instynkt</trd>
  <trd lng="pt">instinto</trd>
  <trd lng="ru">&c_i;&c_n;&c_s;&c_t;&c_i;&c_n;&c_k;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>in≈°tinkt</trd>,
    <trd>pud</trd>
  </trdgrp>
</drv>
</art>
