<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: foneti1.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fonetism</rad>/o
</kap>
<drv mrk="foneti1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <ref tip="dif" cel="sistem.son0o.FON">sonsistemo</ref>
    <refgrp tip="vid">
      <ref cel="foneti.0o">fonetiko</ref>,
       <ref cel="sistem.skrib0o">skribsitemo</ref>,
      <ref cel="grafis.0o">grafismo</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">fonetisme</trd>
  <trd lng="cs">fonetizmus</trd>
  <trdgrp lng="de">
    <trd>phonetisches System</trd>,
    <trd>Lautsystem</trd>
  </trdgrp>
  <trd lng="es">fonetismo</trd>
  <trd lng="fr">phon&eacute;tisme<klr tip="ind"> (caract&eacute;ristiques
  phon&eacute;tiques)</klr></trd>
  <trdgrp lng="hu">
    <trd>hangrendszer</trd>,
    <trd>fonetizmus</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fonetismo</trd>,
    <trd>foneticismo</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_v;&c_u;&c_k;&c_o;&c_v;&c_o;&c_j; &c_s;&c_t;&c_r;&c_o;&c_j;</trd>
  <trd lng="sk">fonetizmus</trd>
</drv>
</art>
