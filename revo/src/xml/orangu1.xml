<vortaro>
<art mrk="$Id: orangu1.xml,v 1.5 2013/08/31 08:11:02 revo Exp $">
<kap><rad>orangutanged</rad>/oj</kap>
<drv mrk="orangu1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="simi.0oj">simioj</ref>
      (<trd lng="la">Pongidae</trd>), plej similaj al la homo.
    </dif>
  </snc>
  <trd lng="de">Menschenaffen</trd>
  <trd lng="hu">emberszab&aacute;s&uacute; <ind>majmok</ind></trd>
  <trd lng="nl">mensapen</trd>
  <trd lng="pt">pong&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_g;&c_i;&c_d;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_n;&c_y;&c_je; <ind>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
    &c_o;&c_b;&c_je;&c_z;&c_mol;&c_ja;&c_n;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

