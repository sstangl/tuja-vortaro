<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monom.xml,v 1.15 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>monom</rad>/o
</kap>
<drv mrk="monom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun unu
      <ref cel="term.0o.polinomo">termo</ref>; alidire, en elementa
      algebro: algebra esprimo, kiu entenas nek adicion, nek subtrahon.
    </dif>
    <ref tip="vid" cel="term.n0o">n-termo.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">monomi</trd>
  <trdgrp lng="cs">
    <trd>jednočlen</trd>,
    <trd>monom</trd>
  </trdgrp>
  <trd lng="de">Monom</trd>
  <trd lng="en">monomial</trd>
  <trd lng="es">monomio</trd>
  <trd lng="fr">mon&ocirc;me</trd>
  <trdgrp lng="hu">
    <trd>egytag&uacute; kifejez&eacute;s</trd>,
    <trd>mon&oacute;m</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eenterm</trd>,
    <trd>monoom</trd>
  </trdgrp>
  <trd lng="pl">jednomian</trd>
  <trd lng="pt">mon&ocirc;mio</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_n;&c_o;&c_ch;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_o;&c_m;</trd></trdgrp>
  <trdgrp lng="sk">
    <trd>jednočlen</trd>,
    <trd>monóm</trd>
  </trdgrp>
</drv>
</art>
