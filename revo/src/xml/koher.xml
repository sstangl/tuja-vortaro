<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koher.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>9</ofc><rad>koher</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="koher.0a">
  <kap><ofc>9</ofc><tld/>a</kap>
  <snc mrk="koher.0a.materia_korpo">
    <dif>
      <klr>(p.p. materia korpo)</klr>
      Kunteni&gcirc;anta, ekz-e pro reciproka altiro de ties partoj:
      <ekz>
        la enketo <klr>[...]</klr> devis el &ccirc;iuj &ccirc;i multspecaj
        kaj konfuzaj eventoj okazintaj en diversaj lokoj de Moskvo fari
        unu <tld/>an bulon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 27</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_c;&c_e;&c_l;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_k;&c_a;&c_g;&c_je;&c_r;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zusammenh&auml;ngend</trd>,
      <trd>verbunden</trd>,
      <trd>koh&auml;rent</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>coherent</trd>,
      <trd>stuck together</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>coh&eacute;rent<klr tip="ind"> (qui consiste un seul
      bloc)</klr></trd>,
      <trd>formant un <ind>bloc</ind></trd>,
      <trd>homog&egrave;ne<klr tip="ind"> (qui consiste un seul
      bloc)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>koherens</trd>,
      <trd>&ouml;sszef&uuml;gg&odblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>samenhangend</trd>,
      <trd>coherent</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="koher.0a.penso">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(p.p. rezono, skriba&jcirc;o, parolado ks)</klr>
      Konsekvence aran&gcirc;ita, sen superfluaj a&ubreve;
      kontra&ubreve;diraj partoj:
      <ekz>
        el tiaj for&scirc;iri&gcirc;intaj parolpecetoj Margarita
        iom post iom kunmetis ion <tld/>an
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>coh&eacute;rent<klr tip="ind"> (raisonnement)</klr></trd>,
      <trd>harmonieux</trd>,
      <trd>qui se <ind>tient</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&ouml;sszef&uuml;gg&odblac;</trd>,
      <trd>&eacute;rtelmes</trd>
    </trdgrp>
    <trd lng="nl">samenhangend</trd>
    <trdgrp lng="ru">
      <trd>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="koher.0a.ondoj">
    <uzo tip="fak">FIZ</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(p.p. ondoj)</klr>
      Tiaj, ke la faza diferenco inter ili estas konstanta.
    </dif>
    <trd lng="fr">coh&eacute;rent<klr tip="ind"> (ondes)</klr></trd>
    <trd lng="hu">koherens</trd>
    <trd lng="ru">&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <refgrp tip="ant">
    <ref cel="koher.mal0a">mal<tld/>a</ref>,
    <ref cel="koher.ne0a">ne<tld/>a</ref>,
    <ref cel="koher.sen0a">sen<tld/>a</ref>.
  </refgrp>
</drv>

<drv mrk="koher.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>&ouml;sszef&uuml;gg</trd>,
    <trd>&ouml;sszetartozik</trd>,
    <trd>&ouml;sszetart</trd>
  </trdgrp>
  <trd lng="nl">samenhangen</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_c;&c_je;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_m;</ind></trd>
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="koher.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="koher.0eco"><tld/>eco</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>koherentnost</trd>,
    <trd>spojitost</trd>
  </trdgrp>
  <trd lng="fr">coh&eacute;rence</trd>
  <trdgrp lng="hu">
    <trd>koherencia</trd>,
    <trd>&ouml;sszetart&aacute;s</trd>,
    <trd>&ouml;sszef&uuml;gg&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">cohesie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>koherentnosť</trd>,
    <trd>spojitosť</trd>
  </trdgrp>
</drv>

<drv mrk="koher.0eco">
  <kap><tld/>eco</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Eco de io <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>koherentnost</trd>,
    <trd>spojitost</trd>
  </trdgrp>
  <trd lng="fr">coh&eacute;rence</trd>
  <trdgrp lng="hu">
    <trd>koherencia</trd>,
    <trd>&ouml;sszetart&aacute;s</trd>,
    <trd>&ouml;sszef&uuml;gg&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">samenhang</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>koherentnosť</trd>,
    <trd>spojitosť</trd>
  </trdgrp>
</drv>

<drv mrk="koher.0emo">
  <kap><tld/>emo</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Forto, per kiu molekuloj kunteni&gcirc;as.
    </dif>
  </snc>
  <trd lng="cs">molekulární přitažlivost</trd>
  <trd lng="fr">coh&eacute;sion<klr tip="ind"> (phys.)</klr></trd>
  <trdgrp lng="hu">
    <trd>koh&eacute;zi&oacute;</trd>,
    <trd>&ouml;sszetart&oacute; er&odblac;</trd>
  </trdgrp>
  <trd lng="nl">cohesie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_g;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_i;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_l;&c_i;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">molekulová príťažlivosť</trd>
</drv>

<drv mrk="koher.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Primitiva formo de <ref cel="detekt.0ilo">detektilo</ref>, kiu
      ebligis <ref cel="radiot1.0o">radiotelegrafio</ref>:
      <ekz>
        la <ref cel="fajl.0ajxo">fajla&jcirc;a</ref>
        <tld/>ilo de Branly.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">koherer</trd>
  <trd lng="fr">coh&eacute;reur</trd>
  <trd lng="hu">koh&eacute;rer</trd>
  <trd lng="nl">coherer</trd>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_r;</trd>
  <trd lng="sk">koherér</trd>
</drv>

<drv mrk="koher.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <ref tip="ant" cel="koher.0a"><tld/>a</ref>
    <ekz>
      malgra&ubreve; evidenta enuo Azazello
      fina&ubreve;skultis tiun mal<tld/>an paroladon
      <fnt><bib>MkM</bib><lok>&ccirc;ap. 19</lok></fnt>;
    </ekz>
    <ekz>
      <ind>mal<tld/>eco</ind>;
      <trd lng="fr">incoh&eacute;rence<klr tip="ind"> (qualit&eacute;)</klr></trd>
      <trdgrp lng="hu">
        <trd>inkoherencia</trd>,
        <trd>sz&eacute;ttart&aacute;s</trd>,
        <trd>&ouml;sszef&uuml;gg&eacute;stelens&eacute;g</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_b;&c_je;&c_s;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_n;&c_je;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_n;&c_je;&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </trdgrp>
    </ekz>
    <ekz>
      <ind>mal<tld/>a&jcirc;o</ind>.
      <trdgrp lng="fr">
        <trd>incoh&eacute;rence<klr tip="ind"> (chose)</klr></trd>,
        <trd>contradiction</trd>
        <trd>incons&eacute;quence<klr tip="ind"> (incoh&eacute;rence)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>inkoherens dolog</trd>,
        <trd>&ouml;sszef&uuml;gg&eacute;stelen dolog</trd>,
        <trd>zagyvas&aacute;g</trd>
     </trdgrp>
    </ekz>
  </snc>
  <trd lng="fr">incoh&eacute;rent</trd>
  <trdgrp lng="hu">
    <trd>inkoherens</trd>,
    <trd>sz&eacute;ttart&oacute;</trd>,
    <trd>&ouml;sszef&uuml;gg&eacute;stelen</trd>
  </trdgrp>
  <trd lng="nl">incoherent</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="koher.ne0a">
  <kap>ne<tld/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="koher.mal0a">mal<tld/>a</ref>
  </snc>
  <trd lng="fr">incoh&eacute;rent</trd>
  <trdgrp lng="hu">
    <trd>inkoherens</trd>,
    <trd>sz&eacute;ttart&oacute;</trd>,
    <trd>&ouml;sszef&uuml;gg&eacute;stelen</trd>
  </trdgrp> 
  <trd lng="nl">incoherent</trd> 
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="koher.sen0a">
  <kap>sen<tld/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="koher.mal0a">mal<tld/>a</ref>
  </snc>
  <trd lng="fr">incoh&eacute;rent</trd>
  <trdgrp lng="hu">
    <trd>inkoherens</trd>,
    <trd>sz&eacute;ttart&oacute;</trd>,
    <trd>&ouml;sszef&uuml;gg&eacute;stelen</trd>
  </trdgrp>  
  <trd lng="nl">incoherent</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_o;&c_g;&c_je;&c_r;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<adm>
  La radiko aperas en VdE sub formo verba (p.p. molekuloj).
  PIV1 kaj PIV2 konservas cxi tiun radikan karakteron (stata verbo).
  Mi opinias la adjektivan karakteron pli logika, sed cxiuokaze tio
  ne havas praktikajn konsekvencojn. Indus trovi ekz-ojn de verba uzo,
  kaj ankaux de uzo de "kohera" ekster gxia lasta senco.
  <aut>MB</aut>
</adm>
</art>
