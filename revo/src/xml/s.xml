<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: s.xml,v 1.10 2015/12/07 11:10:15 revo Exp $">
    <kap>
      <ofc>*</ofc><rad>s</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>
    <subart>
      <drv mrk="s.0.konsonanto">
        <kap><tld/></kap>
        <uzo tip="fak">LIN</uzo>
        <uzo tip="fak">GRA</uzo>
        <uzo tip="fak">FON</uzo>
        <snc mrk="s.0.FON">    
            <dif>
              <ref tip="super" cel="sibl.0anto">Sibla</ref>
              <ref tip="super" cel="konson1.0o">konsonanto</ref>,
              senvo&ccirc;a
              <ref tip="super" cel="frot.0sono">frotsono</ref>
              <ref tip="super"
              cel="dent.lang0akonsonanto">lang-denta</ref>
              [s].
            </dif>
            <trd lng="fr">s<klr> (consonne)</klr></trd>  
        </snc>      
        <snc mrk="s.0.litero">
          <dif>
            La dudekdua
            <ref tip="super" cel="liter.0o.propra">litero</ref> de la
            Esperanta alfabeto, uzata i.a. kiel:
          </dif>
          <trd lng="fr">s<klr> (lettre)</klr></trd>
          <subsnc>
            <dif>
              (maj.) signo de formo: <ekz><tld lit="S"/>-hokoj;</ekz>
            </dif>
            <trd lng="fr">S<klr> (forme de)</klr></trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">GEOG</uzo>
            <dif>
              (maj.) mallongigo de sudo:
              <ekz>32&deg;<tld lit="S"/>;</ekz>
            </dif>
            <trd lng="fr">sud</trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">KEM</uzo>
            <dif>
              (maj.) simbolo de sulfuro: <ekz><tld lit="S"/>;</ekz>
            </dif>
            <trd lng="fr">sulfure</trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">FIZ</uzo>
            <dif>
              (maj.) mallongigo de simenso: <ekz>22<tld lit="S"/>;</ekz>
            </dif>
            <trd lng="fr">Siemens</trd>
          </subsnc>
          <subsnc>
            <uzo tip="fak">KAL</uzo>
            <dif>
              (min.) simbolo de sekundo:
              <ekz>60<tld/>.</ekz>
            </dif>
            <trd lng="fr">seconde<klr> (abr&eacute;viation)</klr></trd>
          </subsnc>
        </snc>
      </drv>
    </subart>

    <subart>
      <drv mrk="s.0.nomo">
        <kap><tld/>o</kap>
        <snc>    
          <dif>
            Nomo de tiu <ref tip="super"
            cel="liter.0o.propra">litero</ref>.
          </dif>
          <trd lng="fr">s</trd>
        </snc>
  <trd lng="cs">sol</trd>
  <trd lng="sk">sol</trd>
</drv>
    </subart>
  </art>

