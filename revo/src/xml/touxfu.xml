<vortaro>
<art mrk="$Id: touxfu.xml,v 1.12 2014/11/12 09:10:36 revo Exp $">
<kap>
  <rad>to&ubreve;fu</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="touxfu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <ref tip="dif" cel="kaze.soj0o">sojkazeo</ref>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_f;&c_u;</trd>
  <trd lng="ca">tofu</trd>
  <trdgrp lng="de">
      <trd>Tofu</trd>,
      <trd>Sojabohnenpaste</trd>,
      <trd>Sojabrei</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>tofu</trd>, <trd>bean curd</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>tofu</trd>
  </trdgrp>
  <trdgrp lng="hu">
      <trd>sz&oacute;jasajt</trd>,
      <trd>tofu</trd>
   </trdgrp>
   <trd lng="id">tahu</trd>
   <trdgrp lng="nl">
    <trd>tofoe</trd>,
    <trd>sojakaas</trd>
  </trdgrp>
  <trd lng="pl">serek <ind>tofu</ind></trd>
  <trd lng="pt">tofu</trd>

</drv>
</art>

