<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tonel.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>tonel</rad>/o
</kap>
<drv mrk="tonel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Internacia mezurunuo de volumeno por &scirc;ipoj, valoranta 2,83
      m&cubic;
      (100 kubaj britaj <ref tip="vid" cel="fut.0o">futoj</ref>).
    </dif>
  </snc>
  <trd
  lng="be">&c_r;&c_e;&c_g;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_ja;
  &c_t;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca"><ind>tona</ind> Moorson</trd>
  <trdgrp lng="cs">
    <trd>RT</trd>,
    <trd>lodní tuna <klr>(2,83 m3)</klr></trd>,
    <trd>registrovaná tuna</trd>,
    <trd>rejstříková tuna</trd>
  </trdgrp>
  <trd lng="de">Registertonne</trd>
  <trd lng="en">ton</trd>
  <trd lng="hu">regisztertonna</trd>
  <trd lng="nl">ton<klr tip="ind"> (inhoudsmaat v.schepen)</klr></trd>
  <trd lng="pl">tona rejestrowa</trd>
  <trd lng="pt">tonelada de arquea&ccedil;&atilde;o</trd>
  <trd
  lng="ru">&c_r;&c_je;&c_g;&c_i;&c_s;&c_t;&c_r;&c_o;&c_v;&c_a;&c_ja;
  &c_t;&c_o;&c_n;&c_n;&c_a;</trd>
  <trd lng="sk">tona <klr>(priestorová)</klr></trd>
</drv>
</art>
