<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekvizet.xml,v 1.4 2015/07/04 08:37:33 revo Exp $">
<kap>
    <rad>ekvizet</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="ekvizet.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekvizet.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara 
      <ref tip="lst" cel="veget.0ajxo"
      lst="voko:sporoplantoj">vegeta&jcirc;o</ref>,
      plimulti&gcirc;antaj per sporoj kaj per rizomoj,
      kun verda tigo multartika, kiu &ccirc;e &ccirc;iu artiko surhavas 
      skatolecan ingon, 
      konsistantan el kuni&gcirc;intaj skvamformaj folioj kun liberaj pintoj,
      kun kirlile elkreskantaj fadenoformaj folioj; ofte kreksanta sur
      malseka 
      grundo;
      sola plu vivanta 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
      (<trd lng="la">Equisetum</trd>) el <ref tip="malprt"
      cel="ekvizet.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trd lng="cs">přeslička</trd>
  <trd lng="de">Schachtelhalm</trd>
  <trd lng="sk">praslička</trd>
</drv>

<drv mrk="ekvizet.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ekvizet.0oj.klaso">
    <dif>
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:botanikaj_klasoj">Klaso</ref>
      de <ref tip="malprt" cel="filik.0oplantoj">filikoplantoj</ref>
      (<trd lng="la">Equisetopsida</trd>), el kiu
      vivas plu nur la genro <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Schachtelhalme</trd>
</drv>
</art>

