<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vestal.xml,v 1.17 2015/09/22 12:10:18 revo Exp $">
<kap>
  <rad>vestal</rad>/o
</kap>
<drv mrk="vestal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj Romanoj, pastrino de Vesta, devigata al virgeco kaj
      al
      konstanta subtenado de la sankta fajro.
    </dif>
    <trdgrp lng="en">
      <trd>vestal virgin</trd>,
      <trd>vestal <klr>(seldom)</klr></trd>
    </trdgrp>
    <trd lng="es">vestal</trd>
    <trd lng="de">Vestalin</trd>
    <trd lng="hu">Vesta-sz&udblac;z</trd>    
    <trd lng="nl">Vestaalse <ind>maagd</ind></trd>
    <trd lng="pl">westalka</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre &ccirc;asta virino.
    </dif>
    <trd lng="de">keusche Frau</trd>
    <trdgrp lng="hu">
      <trd>tiszta n&odblac;</trd>,
      <trd>sz&udblac;zies n&odblac;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_s;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">vestal</trd>
  <trd lng="fr">vestale</trd>
  <trd lng="pt">vestal</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
