<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: maniok.xml,v 1.14 2014/09/03 01:10:18 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>maniok</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="maniok.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="maniok.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbusto vivanta en tropikaj landoj, el kies radikoj oni eltiras
      <ref tip="vid" cel="tapiok.0o.KUI">tapiokon</ref>;
      genro el familio
      <ref tip="malprt" cel="euxfor1.0oj.BOT">e&ubreve;forbiacoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_ib;&c_jo;&c_k;&c_a;</trd>
  <trd lng="ca">mandioca</trd>
  <trd lng="de">Maniok</trd>
  <trdgrp lng="en">
    <trd>manioc</trd>,
    <trd>cassava</trd>,
    <trd>cassada</trd>,
    <trd>yuca</trd>,
    <trd>mogo</trd>,
    <trd>mandioca</trd>
  </trdgrp>
  <trd lng="fr">manioc</trd>
  <trd lng="hu">mani&oacute;ka</trd>
  <trdgrp lng="nl">
    <trd>maniok</trd>,
    <trd>broodwortel</trd>
  </trdgrp>
  <trd lng="pl">maniok</trd>
  <trd lng="pt">mandioca</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_i;&c_o;&c_k;&c_a;</trd>
</drv>
</art>
