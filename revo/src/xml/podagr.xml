    <trd>Zehengicht</trd>
  </trdgrp>
  <trd lng="en">gout</trd>
  <trd lng="fa">&a_n;&a_q;&a_r;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>goutte<klr tip="amb"> (maladie)</klr></trd>,
    <trd>podagre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>podagra</trd>,
    <trd>l&aacute;bk&ouml;szv&eacute;ny</trd>
  </trdgrp>
  <trd lng="id">pirai</trd>
  <trdgrp lng="nl">
    <trd>podagra</trd>,
    <trd>jicht</trd>
  </trdgrp>
  <trd lng="pt">gota <klr>(med.)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_a;&c_g;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dna</trd>,
    <trd>l√°mka</trd>
  </trdgrp>
</drv>
