  <ofc>1</ofc>
  <rad>podagr</rad>/o
</kap>
<drv mrk="podagr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Dolora <ref tip="lst" cel="san.mal0o.MED"
