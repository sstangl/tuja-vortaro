<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klavikord.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klavikord</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="klavikord.0o">
  <kap><tld/>o</kap>
  <snc mrk="klavikord.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropdevena klavara
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      uzata de la 14a &gcirc;is la 17a jarcento,
      pratipo de piano,
      en kiu kordojn frapas internaj marteletoj:
      <ekz>
        jen la <tld/>o! / Sur &gcirc;i notoj kaj libroj, sed &ccirc;io sen ordo
        <fnt>
        <aut>A. Mickiewicz, trad. A. Grabowski</aut>,
        <vrk><url ref="http://esperanto.pl/page.php?tid=3004">Sinjoro
        Tadeo</url></vrk>,
        <lok>Libro unua. - 1918</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_ib;&c_k;&c_o;&c_r;&c_d;&c_y;</trd>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_v;&c_i;&c_k;&c_o;&c_r;&c_d;</trd>
  <trd lng="cs">klavichord</trd>
  <trdgrp lng="de">
    <trd>Klavichord</trd>,
    <trd>Clavichord</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>clavichord</trd>
  </trdgrp>
  <trd lng="es">clavicordio</trd>
  <trdgrp lng="fr">
    <trd>clavicorde</trd>,
    <trd>manicordion</trd>
  </trdgrp>
  <trd lng="hu">klavichord</trd>
  <trd lng="nl">klavichord</trd>
  <trd lng="pl">klawikord</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_a;&c_v;&c_i;&c_k;&c_o;&c_r;&c_d;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_v;&c_i;&c_k;&c_o;&c_r;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="sk">klavichord</trd>
</drv>
</art>
