<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kort.xml,v 1.29 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kort</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kort.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kort.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Sentegmenta spaco, &ccirc;irka&ubreve;ita de konstruoj, muroj
      a&ubreve; bariloj apud domo:
      <ekz>
        oni malfermis la pordegon, kaj la kale&scirc;o enveturis en la
        <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i malsupreniris sur la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en sia <tld/>o &ccirc;iu kok' estas forta
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        starus la morto jam en la <tld/>o<fnt>Z</fnt>
        <klr>(tre proksime)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_o;&c_r;</trd>
  <trd lng="bg">&c_d;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>dv&uring;r</trd>,
    <trd>dvorec</trd>,
    <trd>dvůr</trd>,
    <trd>nádvoří</trd>
  </trdgrp>
  <trd lng="de">Hof</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>courtyard</trd>,
    <trd>enclosed area</trd>,
    <trd>patio</trd>,
    <trd>quadrangle</trd>,
    <trd>yard</trd>
  </trdgrp>
  <trd lng="es">patio (interno)</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_y;&a_A;&a_T;</trd>,
    <trd>&a_S;&a_H;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cour<klr tip="ind"> (de maison)</klr></trd>,
    <trd>parvis</trd>
  </trdgrp>
  <trd lng="hu">udvar</trd>
  <trdgrp lng="nl">
    <trd>hof</trd>,
    <trd>binnenplaats</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podw&oacute;rko</trd>,
    <trd>podw&oacute;rze</trd>,
    <trd>dziedziniec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>p&aacute;tio</trd>,
    <trd>quintal</trd>,
    <trd>terreiro</trd>,
    <trd>eirado</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>dvor</trd>,
    <trd>nádvorie</trd>
  </trdgrp>
  <trd lng="sv">g&aring;rdsplan</trd>
</drv>

<drv mrk="kort.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="kort.0ego.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Ccirc;irka&ubreve;antaro de regnestro a&ubreve; princo:
      <ekz>
        en la <tld/>ego li ellernis paciencon kaj malsincerecon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_o;&c_r; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;.,
&c_k;&c_a;&c_r;&c_a;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>panovnický dvůr</trd>,
    <trd>šlechtický dvůr</trd>
  </trdgrp>
  <trd lng="de">Hofstaat</trd>
  <trdgrp lng="en">
    <trd>court</trd>,
    <trd>king's retinue</trd>,
    <trd>royal household</trd>
  </trdgrp>
  <trd lng="es">corte (de un soberano)</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;</trd>,
    <trd>&a_d;&a_r;&a_b;&a_A;&a_r;&f_y;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">cour<klr tip="ind"> (d'un souverain)</klr></trd>
  <trdgrp lng="hu">
    <trd>udvar <klr tip="ind">(pl. kir&aacute;lyi)</klr></trd>,
    <trd>udvartart&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">hof<klr tip="ind"> (koninklijk)</klr></trd>
  <trd lng="pl">dw&oacute;r<klr> (kr&oacute;lewski, ksi&aogonek;&zdot;&eogonek;cy itd.)</klr></trd>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_r;<klr> (&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_i;&c_j;, &c_c;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">panovnícky dvor</trd>
  <trd lng="sv">hov <klr tip="ind">(hos furste, kung, etc.)</klr></trd>
</drv>

<drv mrk="kort.0egano">
  <kap><tld/>egano</kap>
  <snc>
    <dif>
      Persono apartenanta al la <tld/>ego:
      <ekz>
        feste vestitaj <tld/>eganoj atendis la invititojn anta&ubreve; la
        impona
        pordego de la palaco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a, Babel</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">dvořan</trd>
  <trd lng="de">H&ouml;fling</trd>
  <trd lng="en">courtier</trd>
  <trd lng="es">cortesano</trd>
  <trd lng="fa">&a_d;&a_r;&a_b;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">courtisan</trd>
  <trdgrp lng="hu">
    <trd>udvaronc</trd>,
    <trd>udvari ember</trd>
  </trdgrp>
  <trd lng="nl">hoveling</trd>
  <trdgrp lng="pl">
    <trd>dworzanin</trd>,
    <trd>dw&oacute;rka</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_d;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j;  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">dvoran</trd>
</drv>

<drv mrk="kort.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kort.0isto.pordisto">
    <ref tip="dif" cel="pord.0isto">pordisto</ref>
    <ekz>
      &scirc;i <klr>...</klr> kuris malsupren al la lo&gcirc;ejo de la
      <tld/>isto
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="cs">domovník</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_b;&a_A;&a_n;</trd>,
    <trd>&a_s;&a_r;&a_A;&f_y;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concierge</trd>,
    <trd>portier</trd>
  </trdgrp>
  <trd lng="nl">portier</trd>
  <trd lng="sk">domovník</trd>
</drv>

<drv mrk="kort.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="kort.0umo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Nomo de diversaj ju&gcirc;instancoj, precipe de supera nivelo.
    </dif>
    <ekz>
      apelacia, kasacia <tld/>umo
      <fnt><bib>PIV2</bib></fnt>.
    </ekz>
    <ekz>
      alta <tld/>umo
      <fnt><bib>PIV2</bib></fnt>.
    </ekz>
    <ref tip="vid" cel="tribun1.0o.jugxistaro">tribunalo</ref>
  </snc>
  <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_y;&c_s;&c_u;&c_d;</trd>,
      <trd>&c_s;&c_u;&c_d; &c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_a;&c_j; &c_ib;&c_n;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ib;</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>dvůr <klr>(soudní, apelační)</klr></trd>,
    <trd>soud</trd>
  </trdgrp>
  <trd lng="de">Gerichtshof</trd>
  <trd lng="en">court of law </trd>
  <trdgrp lng="es">
      <trd>corte (jur&iacute;dica)</trd>,
      <trd>tribunal</trd>
    </trdgrp>
  <trdgrp lng="fa">
      <trd>&a_d;&f_y;&a_w;&a_A;&a_n;</trd>,
      <trd>&a_d;&a_A;&a_d;&f_g;&a_A;&a_h;</trd>,
      <trd>&a_m;&a_H;&f_k;&a_m;&a_h;</trd>
    </trdgrp>
  <trd lng="fr">cour<klr tip="ind"> (de justice)</klr></trd>
  <trd lng="hu">b&iacute;r&oacute;s&aacute;g
    <klr tip="ind">(fels&odblac; fok&uacute;)</klr></trd>
  <trdgrp lng="nl">
      <trd>hof<klr tip="ind"> (gerechtshof)</klr></trd>,
      <trd>gerechtshof</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>s&aogonek;d</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_n;&c_y;&c_j; &c_s;&c_u;&c_d;</trd>,
      <trd>&c_s;&c_u;&c_d; &c_v;&c_y;&c_s;&c_sh;&c_je;&c_j; &c_i;&c_n;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_i;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>dvor <klr>(súdny, apelačný)</klr></trd>,
    <trd>súd</trd>
  </trdgrp>
</drv>

<drv mrk="kort.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="kort.antaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="K"/>o, anta&ubreve;anta la &ccirc;efan <tld/>on:
      <ekz>
        li aran&gcirc;is la <tld/>on de la pastroj kaj la grandan
        anta&ubreve;<tld/>on kaj pordojn por la anta&ubreve;<tld/>o
        <fnt><bib>MT</bib>, <lok>&Kr2; 4:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fa">&a_H;&f_y;&a_A;&a_T; &a_b;&f_y;&a_r;&a_w;&a_n;&f_y;</trd>
  <trd lng="fr">avant-cour</trd>
  <trd lng="nl">voorhof</trd>
  <trd lng="hu">el&odblac;udvar</trd>
  <adm>
    Mi transprenis la difinon de PIV, sed mi ne certas, cxu
    la interpreto estas gxusta kadre de la Biblia ekzemplo
    (templo de Salomono).
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="kort.birdo0o">
  <kap>birdo<tld/>o</kap>
  <snc mrk="kort.birdo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Speciala <tld/>o, bredejo de birdoj kaj malgrandaj mamuloj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_y;&c_n;&c_y; &c_d;&c_v;&c_o;&c_r;</trd>
  <trd lng="en">barnyard</trd>
  <trd lng="fa">&a_m;&a_r;&a_gh;&a_d;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">basse-cour</trd>
  <trd lng="hu">baromfiudvar</trd>
  <trd lng="nl">pluimveehouderij</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_ch;&c_i;&c_j; &c_d;&c_v;&c_o;&c_r;</trd>
</drv>
</art>
