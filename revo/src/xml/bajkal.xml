<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bajkal.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>Bajkal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bajkal.B0o">
  <kap><tld/>o</kap>
  <snc mrk="bajkal.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda volumene kaj la plej profunda (1741 m)
      nesalakva lago de la Tero, situanta sude de la
      Orienta <ref tip="malprt" cel="siberi.S0o">Siberio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trd lng="bg">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trd lng="ca">Baikal</trd>
  <trd lng="cs">Bajkal</trd>
  <trd lng="de">Baikalsee</trd>
  <trd lng="en">Baikal</trd>
  <trd lng="fa">&a_d;&a_r;&f_y;&a_A;&f_ch;&a_h;&a_hamza_sure; <ind>&a_b;&a_A;&f_y;&f_k;&a_A;&a_l;</ind></trd>
  <trd lng="fr">Ba&iuml;kal</trd>
  <trd lng="hu">Bajk&aacute;l-t&oacute;</trd>
  <trd lng="pl">Bajka&lstroke;</trd>
  <trd lng="ru">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>,
    <trd>Bajkal</trd>,
    <trd>Bajkalsk&eacute; jazero</trd>
  </trdgrp>
</drv>
</art>
