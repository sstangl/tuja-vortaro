<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: idili.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>idili</rad>/o
</kap>
<drv mrk="idili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="idili.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Poemeto pri la&ubreve;natura, kampara vivo, kaj plejofte pri naivaj amoj
      de pa&scirc;tistoj:
      <ekz>
        la <tld/>oj de Teokrito, Vergilio;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> pejza&gcirc;o
        <trd lng="fr">idyllique</trd>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eklog.0o">eklogo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Idylle</trd>,
      <trd>Idyll</trd>
    </trdgrp>
  </snc>
  <snc mrk="idili.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trankvila kaj naiva amo:
      <ekz>
        la sennuba <tld/>o de &scirc;ia infaneco
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
    </dif>
    <trd lng="de">zarte Liebe</trd>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_y;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_i;&c_d;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>idyla</trd>,
    <trd>selanka</trd>
  </trdgrp>
  <trd lng="en">idyll</trd>
  <trd lng="fr">idylle</trd>
  <trd lng="hu">idill</trd>
  <trd lng="nl">idylle</trd>
  <trd lng="pt">id&iacute;lio</trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_l;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">idyla</trd>
</drv>
</art>
