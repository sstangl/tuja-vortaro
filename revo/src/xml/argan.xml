<art mrk="$Id: argan.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap><rad>argan</rad>/o</kap>
<drv mrk="argan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala &ccirc;arpenta&jcirc;o, portanta la borilegon de petrolputo.
    </dif>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a;&c_f;&c_t;&c_a;&c_v;&c_a;&c_ja;)</klr></trd>
    <trd lng="ca">torre de perforaci&oacute;</trd>
    <trd lng="en">oil well <ind>derrick</ind></trd>
    <trd lng="fr">derrick</trd>
    <trd lng="hu">f&uacute;r&oacute;torony</trd>
    <trd lng="nl">boortoren</trd>
    <trd lng="ru">&c_n;&c_je;&c_f;&c_t;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_v;&c_y;&c_sh;&c_k;&c_a;</ind></trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="gru.0o.TEK">gruo</ref>.
    </dif>
  </snc>
  <trd lng="cs">vrtná věž</trd>
  <trd lng="sk">vrtná veža</trd>
</drv>
</art>
