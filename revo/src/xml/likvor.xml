<art mrk="$Id: likvor.xml,v 1.17 2014/03/28 08:10:44 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>likvor</rad>/o
</kap>
<drv mrk="likvor.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="alkoho.0ajxo">Alkohola&jcirc;o</ref>
      farita el <ref tip="prt" cel="brand.0o">brando</ref>, 
      aroma&jcirc;oj kaj sukero:
      <ekz>
        <ind>ovo<tld/>o</ind>;
        <trd lng="bg">&c_ja;&c_j;&c_ch;&c_je;&c_n;</trd>
        <trd lng="de">Eierlik&ouml;r</trd>
        <trd lng="en">eggnog</trd>
        <trd lng="nl">advocaat</trd>
      </ekz>
      <ekz>
        aniza <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_k;&c_jo;&c_r;</trd>
  <trd lng="bg">&c_l;&c_i;&c_k;&c_mol;&c_o;&c_r;</trd>
  <trd lng="de">Lik&ouml;r</trd>
  <trd lng="en">liqueur</trd>
  <trd lng="es">licor</trd>
  <trd lng="fr">liqueur</trd>
  <trd lng="hu">lik&ouml;r</trd>
  <trd lng="nl">likeur</trd>
  <trd lng="pt">licor</trd>
  <trd lng="ru">&c_l;&c_i;&c_k;&c_jo;&c_r;</trd>
</drv>
</art>
