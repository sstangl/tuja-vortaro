<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sponde.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sponde</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sponde.0o">
  <kap><tld/>o</kap>
  <snc mrk="sponde.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du longaj a&ubreve; akcentaj silaboj; simbole /&dash;&dash;/:
      <ekz>
        &ccirc;ar du akcentaj silaboj ne povas stari sinsekve, en Esperanto
        la <tld/>o ne estas ebla <klr>(krom en okazo de
        <ref cel="karamb.0o.licenco">karambolo</ref>)</klr>
        <fnt><bib>LSF</bib>, <lok>p. 131</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_n;&c_d;&c_e;&c_j;</trd>
  <trd lng="cs">spondej</trd>
  <trd lng="de">Spondeus</trd>
  <trd lng="en">spondee</trd>
  <trd lng="fr">spond&eacute;e</trd>
  <trd lng="hu">spondeus</trd>
  <trd lng="it">spondeo</trd>
  <trd lng="nl">spondee</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_n;&c_d;&c_je;&c_j;</trd>
  <trd lng="sk">spondej</trd>
</drv>
</art>
