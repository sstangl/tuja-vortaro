<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amel.xml,v 1.22 2015/09/10 11:10:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>amel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="amel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="amel.0o.HEJM">
    <dif>
      Substanco ordinare tirita el grenoj, uzata por
      rigidigi tola&jcirc;on a&ubreve; densigi
      baka&jcirc;ojn kaj sa&ubreve;cojn.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_h;&c_m;&c_a;&c_l;</trd>
  <trd lng="bg">&c_n;&c_i;&c_sh;&c_je;&c_s;&c_t;&c_je;</trd>
  <trd lng="br">ampez</trd>
  <trd lng="cs">&scaron;krob <klr>(polysacharid)</klr></trd>
  <trd lng="de">St&auml;rke</trd>
  <trd lng="en">starch</trd>
  <trd lng="es">almid&oacute;n</trd>
  <trd lng="fa">&a_n;&a_sh;&a_A;&a_s;&a_t;&a_h;</trd>
  <trd lng="fr">amidon</trd>
  <trdgrp lng="nl">
    <trd>stijfsel</trd>,
    <trd>zetmeel</trd>
  </trdgrp>
  <trd lng="pt">amido</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_h;&c_m;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>&scaron;krob</trd>,
    <trd>&scaron;krobov&aacute; m&uacute;&ccaron;ka</trd>
  </trdgrp>
  <trd lng="sv">st&auml;rkelse</trd>
</drv>

<drv mrk="amel.0i">
  <kap><tld/>i,
  <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amel.0i.TEKS">
    <dif>
      En&scirc;miri a&ubreve; enmiksi <tld/>on:
      <ekz>
        jam de tre longe uzi&gcirc;as la kolumoj <tld/>itaj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 44a, p.
        636a</lok></fnt>;
      </ekz>
      <ekz>
        supre estis du <tld/>umitaj blankaj kufoj
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/I.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">ampeza&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>na&scaron;krobit</trd>,
    <trd>&scaron;krobit</trd>
  </trdgrp>
  <trd lng="de">st&auml;rken</trd>
  <trd lng="es">almidonar</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_sh;&a_A;&a_s;&a_t;&a_h; &a_z;&a_d;&a_n;<klr> (&a_A_madda;&a_sh;&f_p;&a_z;&f_y;)</klr></trd>,
    <trd>&a_A_madda;&a_h;&a_A;&a_r; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">amidonner</trd>
  <trd lng="sk">&scaron;krobi&tcaron;</trd>
</drv>
</art>
