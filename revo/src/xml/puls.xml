<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: puls.xml,v 1.21 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>puls</rad>/o
</kap>
<drv mrk="puls.0o">
  <kap><tld/>o</kap>
  <snc mrk="puls.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Batado de la arterioj, ka&ubreve;zita de la sangofluo:
      <ekz>
        palpi al iu la <tld/>on
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mia <tld/>o en takto batas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiom da <tld/>obatoj minute?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por irigi la <tld/>on de la monda societo<fnt>Z</fnt>
        <klr>(por vigligi &gcirc;in)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_u;&c_l;&c_mol;&c_s;</trd>
    <trd lng="ca">pols</trd>
    <trd lng="de">Puls</trd>
    <trd lng="es">pulso</trd>
    <trd lng="fr">pulsation</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rver&eacute;s</trd>,
      <trd>pulzus</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pols</trd>,
      <trd>polsslag</trd>
    </trdgrp>
    <trd lng="pt">pulso</trd>
    <trd lng="ru">&c_p;&c_u;&c_l;&c_mol;&c_s;</trd>
  </snc>
  <snc mrk="puls.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Bato, &ccirc;iu el la akcentoj en
      <ref tip="malprt" cel="mezur.0o.MUZ">mezuro</ref>:
      <ekz>
        la valsa mezuro estas <ind>tri<tld/>a</ind>;
        <trd lng="fr">&agrave; trois <ind>temps</ind></trd>
        <trd lng="nl">driekwarts-</trd>
        <trd 
lng="ru">&c_t;&c_r;&c_jo;&c_h;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        en la takto &#190; &ccirc;iu mezuro havas tri <tld/>ojn
        <fnt>
        <aut>B. </aut>:
        <vrk>Rokmuzika Terminaro</vrk>,
        <lok><url
        ref="http://purl.oclc.org/net/bertilow?d=roko/tuvz.php#takto"
        >&leftquot;takto&rightquot;</url></lok>.
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_o;&c_l;&c_ja; <klr>(&c_u; 
&c_m;&c_u;&c_z;&c_y;&c_c;&c_y;)</klr></trd>
    <trd lng="ca">temps<klr> (musical)</klr></trd>
    <trdgrp lng="en">
      <trd>beat</trd>,
      <trd>pulse</trd>
    </trdgrp>
    <trd lng="hu">l&uuml;ktet&eacute;s<klr> (zene)</klr></trd>
    <trd lng="nl">tel<klr tip="ind"> (muziek)</klr></trd>
    <trdgrp lng="pl">
      <trd>t&eogonek;tno</trd>,
      <trd>puls</trd>
    </trdgrp>
    <trd lng="ro">puls</trd>
    <trd lng="ru">&c_d;&c_o;&c_l;&c_ja; 
<klr>(&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;&c_a;, 
&c_t;&c_a;&c_k;&c_t;&c_a;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>puls</trd>,
    <trd>tep</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pulz</trd>,
    <trd>tep</trd>
  </trdgrp>
</drv>

<drv mrk="puls.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sentigi batadon, pp arterioj:
      <ekz>
        sub la kadavrokolora ha&ubreve;to jam ne <tld/>as vivo.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>l&uuml;ktet</trd>,
      <trd>ver</trd>,
      <trd>dobog</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sentigi sian energion per drastaj efikoj:
      <ekz>
        vigla vivo <tld/>as en la urbo, en nia asocio;
      </ekz>
      <ekz>
        li esprimas la sentojn, kiuj <tld/>as en li;
      </ekz>
      <ekz>
        printempo <tld/>is tra l' &gcirc;ardeno;
      </ekz>
      <ekz>
        <tld/>anta trafiko.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_l;&c_mol;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pulzovat</trd>,
    <trd>tepat</trd>
  </trdgrp>
  <trd lng="de">pulsieren</trd>
  <trd lng="fr">pulser</trd>
  <trd lng="hu">l&uuml;ktet</trd>
  <trdgrp lng="nl">
    <trd>pulseren</trd>,
    <trd>kloppen</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>t&eogonek;tni&cacute;</trd>,
      <trd>pulsowa&cacute;</trd>
    </trdgrp>
  <trdgrp lng="ru">
    
<trd>&c_p;&c_u;&c_l;&c_mol;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> (&c_o; 
&c_p;&c_u;&c_l;&c_mol;&c_s;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pulzova≈•</trd>
</drv>

<drv mrk="puls.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      La unua pulso de
      <ref tip="malprt" cel="mezur.0o.MUZ">mezuro</ref>,
      kiu normale havas la &ccirc;efan akcenton.
    </dif>
    <trd lng="en">downbeat</trd>
    <trd lng="nl">eerste <ind>tel</ind> v.e. maat</trd>
  </snc>
</drv>

<drv mrk="puls.lum0i">
  <kap>lum<tld/>i</kap>
  <fnt><lok><url
  ref="&KompLeks;/PR.html#PULSADO">KompLeks</url></lok></fnt>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Regule intermite lumi.
    </dif>
  </snc>
  <trd lng="en">blink</trd>
  <trdgrp lng="nl">
    <trd>knipperen</trd>,
    <trd>flikkeren</trd>
  </trdgrp>
  <trd lng="pl">pulsowa&cacute; <klr>(&sacute;wiat&lstroke;em)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    
<trd>&c_p;&c_u;&c_l;&c_mol;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr 
tip="amb"> (&c_o; &c_s;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
  </trdgrp>
</drv>

</art>
