<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanel.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kanel</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanel.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanel.0o.ARKI">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Longa rekta ornama sulko.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_n;&c_e;&c_l;&c_ju;&c_r;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>estria</trd>,
      <trd>canaleta</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kannelur</trd>,
      <trd>Kannel&uuml;re</trd>
    </trdgrp>
    <trd lng="fr">cannelure</trd>
    <trdgrp lng="hu">
      <trd>kannel&uacute;ra</trd>,
      <trd>rov&aacute;tka</trd>,
      <trd>hornyol&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>stria</trd>,
      <trd>solco</trd>,
      <trd>scanalatura</trd>,
      <trd>incavo<klr> (scanalatura)</klr></trd>
      <trd>glifo<klr> (tec.)</klr></trd>
      <trd>fuga<klr> (edil.)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>cannelure</trd>,
      <trd>groef<klr tip="ind"> (op zuil)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_n;&c_n;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>,
      <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_o;&c_k;</trd>,
      <trd>&c_v;&c_y;&c_je;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_z;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kanel.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Spirala sulketo, strekita interne de paftubo kaj destinita
      rotaciigi la pafa&jcirc;on kaj sekve stabiligi ties trajektorion.
    </dif>
    <trd lng="be">&c_n;&c_a;&c_r;&c_e;&c_z;&c_k;&c_a; <klr tip="amb">(&c_u; &c_r;&c_u;&c_l;&c_ib;
    &c_z;&c_b;&c_r;&c_o;&c_ib;)</klr></trd>
    <trd lng="ca">estria</trd>
    <trd lng="fr">rayure<klr tip="ind"> (d'un canon d'arme &agrave;
    feu)</klr></trd>
    <trd lng="hu">huzagol&aacute;s</trd>
    <trd lng="it">rigatura<klr> (di bocca da fuoco)</klr></trd>
    <trd lng="nl">trek<klr tip="ind"> (v.e.geweerloop)</klr></trd>
    <trd lng="ru">&c_n;&c_a;&c_r;&c_je;&c_z;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="kanel.0o.TEK">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Spirala sulketo, strekita surface de gramofondisko kaj destinita
      gvidi kaj vibrigi la nadlon a&ubreve; safiron adaptitan al la diafragmo.
    </dif>
    <trd lng="be">&c_d;&c_a;&c_r;&c_o;&c_zh;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a;
    &c_k;&c_r;&c_u;&c_zh;&c_e;&c_l;&c_c;&c_y;)</klr></trd>
    <trd lng="ca">solc</trd>
    <trd lng="de">Rille</trd>
    <trd lng="fr">sillon<klr tip="ind"> (d'un disque)</klr></trd>
    <trd lng="hu">hangbar&aacute;zda</trd>
    <trdgrp lng="it">
      <trd>microsolco</trd>,
      <trd>solco<klr> (di dischi)</klr></trd>
    </trdgrp>
    <trd lng="nl">groef</trd>
    <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_zh;&c_k;&c_a; <klr>(&c_n;&c_a;
    &c_g;&c_r;&c_a;&c_m;&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_k;&c_je;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>drážka</trd>,
    <trd>falc</trd>,
    <trd>rýha</trd>,
    <trd>záhyb</trd>,
    <trd>žlábek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drážka</trd>,
    <trd>ryha</trd>,
    <trd>žliabok</trd>
  </trdgrp>
</drv>

<drv mrk="kanel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami per paralelaj <tld/>oj:
      <ekz>
        <tld/>itaj kolonoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_e;&c_z;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_e;&c_l;&c_ju;&c_r;&c_y;</ind></trd>,
      <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_e;&c_l;&c_ju;&c_r;&c_a;&c_m;&c_ib;</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>estriar</trd>,
      <trd>acanalar</trd>
    </trdgrp>
    <trd lng="de">kannelieren</trd>
    <trd lng="fr">canneler</trd>
    <trdgrp lng="hu">
      <trd>bar&aacute;zd&aacute;l</trd>,
      <trd>hornyol</trd>,
      <trd>huzagol</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>canneleren</trd>,
      <trd>kartelen</trd>,
      <trd>ribbelen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_a;&c_n;&c_n;&c_je;&c_l;&c_ju;&c_r;&c_y;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>drážkovat</trd>,
    <trd>falcovat</trd>,
    <trd>rýhovat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drážkovať</trd>,
    <trd>ryhovať</trd>,
    <trd>žliabkovať</trd>
  </trdgrp>
</drv>
</art>
