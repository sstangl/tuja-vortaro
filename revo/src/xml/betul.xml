<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: betul.xml,v 1.27 2015/10/05 04:10:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>betul</rad>/o
</kap>

<drv mrk="betul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="betula.0oj">betulacoj</ref>
      (<trd lng="la">Betula</trd>), arboj kaj arbedoj kun hela mola
      ligno,
      kun tre flekseblaj bran&ccirc;oj, ofte kun blanka trunk&scirc;elo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="bg">&c_b;&c_r;&c_je;&c_z;&c_a;</trd>
  <trdgrp lng="br">
    <trd>bezvenn</trd>,
    <trd>gwezenn-vezv</trd>,
  </trdgrp>
  <trd lng="cs">b&rcaron;&iacute;za</trd>
  <trd lng="de">Birke</trd>
  <trd lng="en">birch</trd>
  <trd lng="es">abedul</trd>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_A;&a_n;</trd>,
    <trd>&a_t;&a_w;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">bouleau</trd>
  <trd lng="hu">ny&iacute;r(fa)</trd>
  <trd lng="pl">brzoza</trd>
  <trd lng="pt">b&eacute;tula</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_jo;&c_z;&c_a;</trd>
  <trd lng="sk">breza</trd>
  <trd lng="sv">bj&ouml;rk</trd>
</drv>
</art>
