<kap>
<rad>pojn</rad>/o
</kap>
<drv mrk="pojn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
  <ref tip="dif" cel="man.0radiko">manradiko</ref>
   <ekz>
   sur la maldekstra <tld/>o videblis multekosta brakhorlo&gcirc;o
   <fnt><aut>Julian Modest</aut><vrk>La glita kaj dan&gcirc;era vojo</vrk> 
   <bib>LOdE</bib><lok>jaro 2002a, numero 3a</lok></fnt>.
   </ekz>
   <trd lng="ca">canell</trd>
   <trd lng="en">wrist</trd>
   <trd lng="fr">poignet</trd>
   <trd lng="id">per<ind>gelang</ind>an tangan</trd>
   <trd lng="it">polso</trd>
   <trd lng="nl">pols</trd>
   <trd lng="pl">przegub r&eogonek;ki</trd> 
   <trd lng="pt">pulso<klr tip="amb"> (anatomia humana)</klr></trd>
  </snc>
  <trd lng="cs">kloub zápěstí</trd>
  <trd lng="sk">kĺb zápästia</trd>
</drv>
</art>

