<art mrk="$Id: silvie.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>silvied</rad>/oj
</kap>
<drv mrk="silvie.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="silvie.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sylviidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_s;&c_ja;&c_n;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">pěnicovití <klr>(čeleď)</klr></trd>
  <trd lng="de">Grasm&uuml;cken<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">fauvettes<klr> (famille)</klr></trd>
  <trd lng="hu">posz&aacute;taf&eacute;l&eacute;k</trd>
  <trd lng="it">silvidi</trd>
  <trd lng="nl">zangers van de oude wereld<klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">penicovité</trd>
  <trd lng="sv">s&aring;ngare</trd>
</drv>
</art>
