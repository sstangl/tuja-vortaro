<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: abon.xml,v 1.50 2016/03/18 19:10:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>abon</rad>/i
</kap>

<drv mrk="abon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abon.0i.ricevadi">
    <dif>
      Ricevi, la&ubreve; kontrakto, gazeton a&ubreve; servon:
      <ekz>
        mi de du jaroj <tld/>as la revuon;
      </ekz>
      <ekz>
        la revuo havas multajn <ind><tld/>anto</ind>jn.
        <trd lng="bg">&c_a;&c_b;&c_o;&c_n;&c_a;&c_t;</trd>
        <trd lng="ca">abonat</trd>
        <trd lng="de">Abbonent</trd>
        <trd lng="fr">abonn&eacute;</trd>
        <trd lng="es">abonado</trd>
        <trd lng="hu">el&odblac;fizet&odblac;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="bg">&c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_a;&c_m;
    &c_s;&c_je;</trd>
    <trd lng="ca"><ind>estar abonat</ind> (a)</trd>
    <trdgrp lng="es">
      <trd>estar abonado a</trd>,
      <trd>estar suscrito a</trd>
    </trdgrp>
    <trd lng="br">beza&ntilde;<ind> koumanantet</ind> (da)</trd>
    <trd lng="fr">&ecirc;tre <ind>abonn&eacute;</ind> (&agrave;)</trd>
    <trd lng="hu">el&odblac;fizet</trd>
    <trd lng="nl">ge<ind>abonneerd</ind> zijn op</trd>
    <trdgrp lng="pl">
      <trd>abonowa&cacute;</trd>,
      <trd>prenumerowa&cacute;</trd>,
      <trd>subskrybowa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_p;&c_i;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>
      &c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">odobera&tcaron;</trd>
    <trdgrp lng="sv">
      <trd>prenumerera</trd>,
      <trd>abonnera <klr>(p&aring;)</klr></trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#3626;&#3617;&#3633;&#3588;&#3619; (&#3609;&#3636;&#3605;&#3618;&#3626;&#3634;&#3619;, &#3610;&#3619;&#3636;&#3585;&#3634;&#3619;)</trd>
    </trdgrp>
  </snc>
  <snc mrk="abon.0i.kontrakti">
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>F</bib></fnt>
    <ref tip="dif" cel="abon.ek0i">ek<tld/>i</ref>
    <ekz>
      la revuo havas multajn <tld/>intojn..
    </ekz>
    <rim>
      Kvankam &ccirc;i tiu senco estas la nura, kiu aperas en
      la Fundamento, &gcirc;i preska&ubreve; plene malaperis.
    </rim>
    <trdgrp lng="be">
      <trd>
      &c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>
      &c_a;&c_b;&c_a;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">koumananti&ntilde; (da)</trd>
    <trd lng="ca">abonar-se (a)</trd>
    <trdgrp lng="fr">
      <trd>s'<ind>abonner</ind> (&agrave;)</trd>,
      <trd>souscrire un <ind>abonnement</ind> (&agrave;)</trd>.
    </trdgrp>
    <trd lng="hu">megrendel</trd>
    <trdgrp lng="nl">
      <trd>zich <ind>abonneren</ind> op</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zaabonowa&cacute;</trd>,
      <trd>zaprenumerowa&cacute;</trd>,
      <trd>zasubskrybowa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">assinar</trd>
    <trdgrp lng="ru">
      <trd>
      &c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>
      &c_a;&c_b;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">predplati&tcaron; (si)</trd>
    <trd lng="sv">teckna ett <ind>abonnemang</ind>
    <klr>(p&aring;)</klr></trd>
  </snc>
  <snc mrk="abon.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ani&gcirc;i al a&ubreve; membri en komputilreta
      <ref tip="malprt" cel="forum.0o.komp">forumo</ref> a&ubreve;
      dissendolisto.
    </dif>
    <ekz>
      de anta&ubreve; pli ol unu jaro, mi abonas la servon "vorto de la
      tago"
      de Lernu.net
    </ekz>
    <trdgrp lng="br">
      <trd>lakaat e <ind>anv</ind> (e)</trd>,
      <trd>en em <ind>enskriva&ntilde;</ind> (e)</trd>.
    </trdgrp>
    <trdgrp lng="ca">
      <trd>unir-se (a)</trd>,
      <trd>registrar-se (en)</trd>
    </trdgrp>
    <trd lng="fr">s'<ind>inscrire</ind> (&agrave;)</trd>
    <trd lng="hu">feliratkozik</trd>
    <trdgrp lng="pl">
      <trd>subskrybowa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">inscrever-se</trd>
    <trdgrp lng="es">
      <trd>estar inscrito en</trd>,
      <trd>suscribirse a</trd>.
    </trdgrp>
    <trd lng="nl">zich <ind>inschrijven</ind> op</trd>
    <trd
    lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr>
     (&c_n;&c_a;
     &c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ju;
     &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>abonovat</trd>,
    <trd>b&yacute;t abonentem</trd>,
    <trd>odeb&iacute;rat</trd>,
    <trd>p&rcaron;edplatit <klr>(&ccaron;sp.)</klr></trd>
  </trdgrp>
  <trd lng="de">abonnieren</trd>
  <trd lng="en">subscribe</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_w;&a_n;&a_h; &a_sh;&a_d;&a_n;<klr>
    (&a_b;&a_h;)</klr></trd>,
    <trd>&a_m;&a_sh;&a_t;&a_r;&f_k; &a_sh;&a_d;&a_n;<klr>
    (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="it">abbonarsi <klr>(a)</klr></trd>
  <trdgrp lng="sk">
    <trd>abonova&tcaron;</trd>,
    <trd>plati&tcaron; predplatn&eacute;</trd>,
    <trd>prihl&aacute;si&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="abon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tiucela kontrakto:
      <ekz>
        la prezo de la <tld/>o estas multekosta.
      </ekz>
    </dif>
    <trd lng="en">subscription</trd>
  </snc>
  <snc>
    <dif>
      Da&ubreve;ro de tiu kontrakto:
      <ekz>
        via <tld/>o fini&gcirc;as en majo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_b;&c_o;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="br">koumanant</trd>
  <trd lng="ca">abonament</trd>
  <trdgrp lng="cs">
    <trd>abonm&aacute; <klr>(uzav&rcaron;en&iacute; smlouvy)</klr></trd>,
    <trd>abonnement</trd>
  </trdgrp>
  <trd lng="de">Abonnement</trd>
  <trd lng="en">subscription</trd>
  <trd lng="es">suscripci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_w;&a_n;&a_m;&a_A;&a_n;</trd>,
    <trd>&a_A;&a_sh;&a_t;&a_r;&a_A;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">abonnement</trd>
  <trd lng="hu">el&odblac;fizet&eacute;s</trd>
  <trd lng="it">abbonamento</trd>
  <trd lng="nl">abonnement</trd>
  <trdgrp lng="pl">
    <trd>abonament</trd>,
    <trd>prenumerata</trd>,
    <trd>subskrybcja</trd>
  </trdgrp>
  <trd lng="pt">assinatura</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">uzavretie zmluvy</trd>
  <trdgrp lng="sv">
    <trd>abonnemang</trd>,
    <trd>prenumeration</trd>
  </trdgrp>
</drv>

<drv mrk="abon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <ref tip="dif" cel="abon.0i.ricevadi"><tld/>i
      <sncref ref="abon.0i.ricevadi"/></ref>:
      <ekz>
        <tld/>igu al mi lo&gcirc;ion en la Operejo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">koumananti&ntilde; (unan bennak)</trd>
  <trd lng="ca">abonar</trd>
  <trd lng="cs">nechat objednat</trd>
  <trd lng="de">jemanden etwas <ind>abonnieren lassen</ind></trd>
  <trd lng="es">abonar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_w;&a_n;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_sh;&a_t;&a_r;&f_k; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">abonner</trd>
  <trd lng="hu">kib&eacute;rel</trd>
  <trd lng="nl">abonneren</trd>
  <trdgrp lng="pl">
    <trd>abonowa&cacute;<klr> (co&sacute; dla kogo&sacute;)</klr></trd>,
    <trd>prenumerowa&cacute;<klr> (co&sacute; dla kogo&sacute;)</klr>
    </trd>,
    <trd>subskrybowa&cacute;<klr> (co&sacute; dla kogo&sacute;)</klr>
    </trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;
  <klr tip="amb">
  (&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr>
  </trd>
  <trd lng="sk">necha&tcaron; predplati&tcaron;</trd>
</drv>

<drv mrk="abon.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="formul.0aro.radika">Formularo</ref> por
      aboni <klr>(plenigita a&ubreve; neplenigita)</klr>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_a;&c_n;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_a;&c_b;&c_a;&c_n;&c_e;&c_m;&c_e;&c_n;&c_t;&c_u;</ind>
      </trd>
    </trdgrp>
    <trd lng="br">follenn goumanti&ntilde;</trd>
    <trd lng="ca">formulari d'abonament</trd>
    <trdgrp lng="de">
      <trd>Bestellformular</trd>,
      <trd>Bestellzettel</trd>
    </trdgrp>
    <trd lng="en">subscription form</trd>
    <trdgrp lng="fa">
      <trd>&a_b;&a_r;&f_g;&a_h;&a_hamza_sure;
      <ind>&a_A_madda;&a_b;&a_w;&a_n;&a_m;&a_A;&a_n;</ind></trd>,
      <trd>&a_b;&a_r;&f_g;&a_h;&a_hamza_sure;
      <ind>&a_A;&a_sh;&a_t;&a_r;&a_A;&f_k;</ind></trd>
    </trdgrp>
    <trd lng="fr">bulletin d'<ind>abonnement</ind></trd>
    <trd lng="hu">megrendel&odblac;lap</trd>
    <trd lng="nl">inschrijvingsformulier<klr tip="ind"> (voor
    abonnementen)</klr></trd>
    <trdgrp lng="pl">
      <trd>formularz <ind>prenumeraty</ind></trd>,
      <trd>formularz <ind>subskrybcji</ind></trd>,
      <trd>zam&oacute;wienie</trd>
    </trdgrp>
    <trd lng="pt">formul&aacute;rio de assinatura</trd>
    <trdgrp lng="es">
      <trd>formulario de inscripci&oacute;n</trd>,
      <trd>hoja de suscripci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_a;&c_b;&c_o;&c_n;&c_je;&c_m;&c_je;&c_n;&c_t;&c_a;</ind>
      </trd>,
      <trd>&c_b;&c_l;&c_a;&c_n;&c_k;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>abonentka</trd>,
    <trd>p&rcaron;edplatn&iacute; l&iacute;stek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abonentka</trd>,
    <trd>predplatn&yacute; l&iacute;stok</trd>
  </trdgrp>
</drv>

<drv mrk="abon.ek0i">
  <kap>ek<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Kontrakti por ricevi, dum difinita tempo, gazeton
      a&ubreve; servon.
    </dif>
    <ref tip="sin" cel="abon.0i.kontrakti"><tld/>i <sncref
    ref="abon.0i.kontrakti"/>.</ref>
  </snc>
  <trd lng="br">koumananti&ntilde;</trd>
  <trd lng="ca"><ind>abonar-se</ind> a <klr>(prendre un
  abonament)</klr></trd>
  <trd lng="cs">smluvit p&rcaron;edplatn&eacute;</trd>
  <trd lng="de">abonnieren</trd>
  <trd lng="es">inscribirse en</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_w;&a_n;&a_h; &a_sh;&a_d;&a_n;<klr>
    (&a_b;&a_h;)</klr></trd>,
    <trd>&a_m;&a_sh;&a_t;&a_r;&f_k; &a_sh;&a_d;&a_n;<klr>
    (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>abonner</ind> (&agrave;)</trd>,
    <trd>souscrire un <ind>abonnement</ind> (&agrave;)</trd>.
  </trdgrp>
  <trd lng="hu">megrendel</trd>
  <trd lng="nl">een <ind>abonnement</ind> nemen</trd>
  <trdgrp lng="pl">
    <trd>zaabonowa&cacute;</trd>,
    <trd>zaprenumerowa&cacute;</trd>,
    <trd>zasubskrybowa&cacute;</trd>,
    <trd>zapisa&cacute; si&eogonek;<klr> (na co&sacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    </trd>
  </trdgrp>
  <trd lng="sk">predplati&tcaron; si</trd>
</drv>

<drv mrk="abon.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      &Ccirc;esigi sian <tld/>on.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;
      <ind>&c_p;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_a;&c_d;
      <ind>&c_a;&c_b;&c_a;&c_n;&c_e;&c_m;&c_e;&c_n;&c_t;&c_a;</ind></trd>,
      <trd>&c_a;&c_d;&c_p;&c_ib;&c_s;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="br">digoumanti&ntilde;</trd>
    <trd lng="ca"><ind>donar-se de baixa</ind> (de)</trd>
    <trd lng="de">abbestellen</trd>
    <trd lng="en">unsubscribe</trd>
    <trdgrp lng="es">
      <trd>desabonarse de</trd>,
      <trd>desuscribirse de</trd>
    </trdgrp>
    <trd lng="fa">&a_q;&a_T;&a_ayn;
    <ind>&a_A;&a_sh;&a_t;&a_r;&a_A;&f_k;</ind>
    &f_k;&a_r;&a_d;&a_n;</trd>
    <trd lng="fr">se <ind>d&eacute;sabonner</ind></trd>
    <trd lng="hu">lemond <klr>(megrendel&eacute;st)</klr></trd>
    <trd lng="nl">opzeggen<klr tip="ind"> (abonnement)</klr></trd>
    <trdgrp lng="pl">
      <trd>odwo&lstroke;a&cacute; <ind>abonament</ind></trd>,
      <trd>odwo&lstroke;a&cacute; <ind>prenumerat&eogonek;</ind></trd>,
      <trd>odwo&lstroke;a&cacute; <ind>subskrybcj&eogonek;</ind></trd>,
      <trd>wypisa&cacute; si&eogonek;<klr> (z czego&sacute;)</klr></trd>
    </trdgrp>
    <trd lng="pt">cancelar a <ind>assinatura</ind></trd>
    <trdgrp lng="ru">
      <trd>
      &c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_o;&c_t;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_i;</ind></trd>,
      <trd>
      &c_a;&c_n;&c_n;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
      <ind>&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_o;&c_t;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_v; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_je;)
      </klr></trd>
    </trdgrp>
    <trd lng="sk">odhl&aacute;si&tcaron;</trd>
  </snc>
</drv>
</art>
