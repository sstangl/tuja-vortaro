<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skvar.xml,v 1.3 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>skvar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skvar.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="skvar.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malgranda publika
      <ref tip="vid" cel="gxarde.plezur0o.HOR">plezur&gcirc;ardeno</ref>
      sur urba placo:
      <ekz>
        en Kaliningrado multas parkoj, <tld/>oj, kaj 
        proksimas la maro
        <fnt>
          <aut>M. Nosal, trad. H. Gorecka kaj A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://sezonoj.ru/2014/10/242nosal/">
          Intervjuo kun la &Gcirc;enerala konsulo de Pollando...</url></vrk>,
          <lok>La Ondo de Esperanto, 2014:11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>zelená plocha ve středu náměstí</trd>,
    <trd>čtvercové náměstí</trd>
  </trdgrp>
  <trd lng="fr">square</trd>
  <trd lng="sk">námestie</trd>
</drv>
</art>

