<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vetera.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>veteran</rad>/o
</kap>
<drv mrk="vetera.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longe servinta soldato.
    </dif>
    <refgrp tip="vid">
      <ref cel="hard.0i">hardi</ref>
    </refgrp>
  </snc>
  <snc>
    <dif>
      Longe servinta oficisto.
    </dif>
    <refgrp tip="sin">
      <ref cel="spert">spertulo</ref>,
      <ref cel="ekzerc">ekzercito</ref>,
      <ref cel="emerit">emerita</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_t;&c_e;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">veter&agrave;</trd>
  <trdgrp lng="cs">
    <trd><klr>(zasloužilý)</klr> pracovník na odpočinku</trd>,
    <trd>veterán</trd>,
    <trd>vysloužilec</trd>
  </trdgrp>
  <trd lng="de">Veteran</trd>
  <trd lng="en">veteran</trd>
  <trd lng="fr">v&eacute;t&eacute;ran</trd>
  <trd lng="hu">veter&aacute;n</trd>
  <trd lng="nl">veteraan</trd>
  <trd lng="pt">veterano</trd>
  <trd lng="ru">&c_v;&c_je;&c_t;&c_je;&c_r;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>starý pracovník kolektívu</trd>,
    <trd>veter&aacute;n</trd>,
    <trd>vojenský vyslúžilec</trd>
  </trdgrp>
</drv>
</art>
