<art mrk="$Id: herbar.xml,v 1.15 2013/09/02 16:10:17 revo Exp $">
<kap>
  <rad>herbari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="herbar.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <ref tip="dif" cel="kolekt.herbo0o">herbokolekto.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_b;&c_a;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_je;&c_r;&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Herbarium</trd>,
    <trd>Sammlung getrockneter Pflanzen</trd>
  </trdgrp>
  <trd lng="el">&beta;&omikron;&tau;&alfa;&ny;&omikron;&lambda;&omikron_ton;&gamma;&jota;&omikron;</trd>
  <trd lng="en">herbarium</trd>
  <trd lng="es">herbario</trd>
  <trd lng="fr">herbier</trd>
  <trdgrp lng="hu">
    <trd>n&ouml;v&eacute;nygy&udblac;jtem&eacute;ny</trd>,
    <trd>herb&aacute;rium</trd>
  </trdgrp>
  <trd lng="it">erbario</trd>
  <trd lng="nl">herbarium</trd>
  <trdgrp lng="pl">
    <trd>zielnik</trd>,
    <trd>herbarium</trd>
  </trdgrp>
  <trd lng="pt">herb&aacute;rio</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
