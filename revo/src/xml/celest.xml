<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: celest.xml,v 1.4 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>celest</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="celest.0o">
  <kap><tld/>o</kap>
  <snc mrk="celest.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Klavara
      <ref tip="lst" cel="instru2.frap0o"
      lst="voko:frapinstrumentoj">frapinstrumento</ref>,
      kies frapiloj trafas metalajn lamenojn,
      kreante puran kaj akutan sonoron:
      <ekz>
        <ctl>Il Cantico del Sole</ctl>, kreita de Sofia
        Gubaidulina por violon&ccirc;elo, perkutinstrumentoj,
        <tld/>o kaj koruso
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Kantiko_de_la_Kreita%C4%B5oj">
          Kantiko de la Kreita&jcirc;oj</url>, 2011-04-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">celesta</trd>
  <trd lng="fr">c&eacute;lesta</trd>
  <trd lng="sk">celesta</trd>
</drv>
</art>

