<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kosinu.xml,v 1.23 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kosinus</rad>/o
</kap>
<drv mrk="kosinu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="kosinu.0o.entriangulo">
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> <frm>&alfa;</frm>)</klr>
      <ref cel="sinus.0o">Sinuso</ref> de &gcirc;ia
      <ref cel="komple1.0o.angulo">komplemento</ref>; 
      simb. <frm>cos &alfa;</frm> a&ubreve; <frm>kos &alfa;</frm>:
      <ekz>
        la <tld/>o de orto egalas al <frm>0</frm>;
      </ekz>
      <ekz>
        la <tld/>o de angulo egalas al la 
        kontra&ubreve;egalo de la <tld/>o de
        &gcirc;ia <ref cel="suplem.0o.angulo">suplemento</ref>;
      </ekz>
      <ekz>
        la <tld/>o de konkava angulo egalas la <tld/>o de
        &gcirc;ia <ref cel="komple1.0o.aro">komplemento</ref> rilate
        al la plena angulo.
      </ekz>
    </dif>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm>&alfa;</frm> egalas al <frm>OK/OM</frm>
      -->
      la <tld/>o de angulo &alfa; egalas al OK/OM 
    </bld>
  </snc>
  <snc mrk="kosinu.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm><k>x</k></frm> al la 
      <tld/>o <sncref ref="kosinu.0o.entriangulo"/> de angulo, kies
      mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas de
      <frm><k>x</k></frm> 
      per oblo de <frm>2&pi;</frm>; simb. <frm>cos</frm> a&ubreve;
      <frm>kos</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun
        <ref cel="period.0o.funkcio">periodo <sncref/></ref>
        <frm>2&pi;</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_s;&c_ib;&c_n;&c_u;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>cosinus</trd>,
    <trd>kosinus</trd>
  </trdgrp>
  <trd lng="de">Kosinus</trd>
  <trd lng="en">cosine</trd>
  <trd lng="es">coseno</trd>
  <trd lng="fr">cosinus</trd>
  <trd lng="hu">koszinusz</trd>
  <trd lng="pl">cosinus</trd>
  <trdgrp lng="pt">
    <trd>co-seno</trd>,
    <trd>cosseno</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_n;&c_u;&c_s;</trd>
  <trd lng="sk">kosínus</trd>
</drv>

<drv mrk="kosinu.are0o">
  <kap>are<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <ref tip="dif" cel="kosinu.inversa_hiperbola0o">inversa hiperbola
    <tld/>o.</ref>
    <rim>
      Vd rimarkon sub <ref cel="are.0o.argumento">areo</ref>.
    </rim>
    <trd lng="pt">co-seno hiperb&oacute;lico inverso</trd>
  </snc>
</drv>

<drv mrk="kosinu.ark0o">
  <kap>ark<tld/>o, <var><kap>arko<tld/>o</kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="kosinu.0o.funkcio"><tld/>o</ref> al la
      intervalo <frm>[0,&pi;]</frm>; 
      simb. <frm>arccos</frm> a&ubreve; <frm>arkkos</frm>:
      <ekz>
        ark<tld/>o de <frm>0</frm> egalas al <frm>&pi;/2</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_k;&c_o;&c_s;&c_ib;&c_n;&c_u;&c_s;</trd>
  <trd lng="cs">arkuskosinus</trd>
  <trd lng="de">Arkuskosinus</trd>
  <trd lng="en">arc cosine</trd>
  <trd lng="fr">arc cosinus</trd>
  <trd lng="hu">arkusz <ind>koszinusz</ind></trd>
  <trd lng="pl">arcus cosinus</trd>
  <trd lng="pt">arco-cosseno</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_k;&c_o;&c_s;&c_i;&c_n;&c_u;&c_s;</trd>
  <trd lng="sk">arkuskosínus</trd>
</drv>

<drv mrk="kosinu.hiperbola0o">
  <kap>hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm><k>x</k></frm> al 
      <frm>&#189;(<k>e</k><sup><k>x</k></sup>+<k>e</k><sup>-<k>x</k></sup>)</frm>;
      
      simb. <frm>cosh</frm> a&ubreve; <frm>kosh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_k;&c_o;&c_s;&c_ib;&c_n;&c_u;&c_s;</ind></trd>
  <trdgrp lng="de">
    <trd>Hyperbelkosinus</trd>,
    <trd>hyperbolischer <ind>Kosinus</ind></trd>
  </trdgrp>
  <trd lng="en">hyperbolic <ind>cosine</ind></trd>
  <trd lng="fr">cosinus hyperbolique</trd>
  <trd lng="hu">koszinusz hiperbolikusz</trd>  
  <trd lng="pl">cosinus hiperboliczny</trd>
  <trd lng="pt">co-seno hiperb&oacute;lico</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  <ind>&c_k;&c_o;&c_s;&c_i;&c_n;&c_u;&c_s;</ind></trd>
</drv>

<drv mrk="kosinu.inversa_hiperbola0o">
  <kap>inversa hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref> 
      <ref cel="kosinu.hiperbola0o">hiperbola <tld/>o</ref> al la
      intervalo <frm>[0,+&#8734;[</frm>; 
      simb. <frm>arcosh</frm> a&ubreve; <frm>arkosh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y;
  <ind>&c_k;&c_o;&c_s;&c_ib;&c_n;&c_u;&c_s;</ind></trd>
  <trd lng="cs">inverzní hyperbolický kosinus</trd>
  <trd lng="de">Areakosinus</trd>
  <trdgrp lng="en">
    <trd>area-hyperbolic cosine</trd>,
    <trd>inverse hyperbolic cosine</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>argument du <ind>cosinus</ind> hyperbolique</trd>,
    <trd>cosinus hyperbolique inverse</trd>
  </trdgrp>
  <trd lng="hu">arkusz <ind>koszinusz</ind> hiperbolikusz</trd>
  <trd lng="pl">area cosinus hiperboliczny</trd>
  <trd lng="pt">co-seno hiperb&oacute;lico inverso</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_a;-&c_k;&c_o;&c_s;&c_i;&c_n;&c_u;&c_s; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    <ind>&c_k;&c_o;&c_s;&c_i;&c_n;&c_u;&c_s;</ind></trd>
  </trdgrp>
  <trd lng="sk">inverzný hyperbolický kosínus</trd>
</drv>
</art>
