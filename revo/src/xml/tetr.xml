<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tetr.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tetr</rad>/o
</kap>
<drv mrk="tetr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tetr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio el familio <ref tip="super"
      cel="tetraon1.0oj">tetraonedoj</ref>
      (<trd lng="la">Lyrurus tetrix</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_c;&c_ja;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">gall de cua forcada</trd>
  <trdgrp lng="cs">
    <trd>tet&rcaron;ivek obecn&yacute;</trd>,
    <trd>tetřívek obecný</trd>
  </trdgrp>
  <trd lng="de">Birkhuhn</trd>
  <trd lng="en">black grouse</trd>
  <trdgrp lng="fr">
    <trd>petit coq de bruy&egrave;re</trd>,
    <trd>t&eacute;tras lyre</trd>
  </trdgrp>
  <trd lng="hu">ny&iacute;rfajd</trd>
  <trd lng="nl">korhoen</trd>
  <trd lng="pl">cietrzew</trd>
  <trdgrp lng="pt">
    <trd>galinha-brava</trd>,
    <trd>tetraz-pequeno-das-serras</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;-&c_k;&c_o;&c_s;&c_a;&c_ch;</trd>,
    <trd>&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v; &c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">tetrov hoľniak</trd>
</drv>
</art>
