<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kardam.xml,v 1.18 2013/11/02 12:10:27 revo Exp $">
<kap>
  <rad>kardamin</rad>/o
</kap>
<drv mrk="kardam.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Cardamine</trd>),
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herboj</ref>, &ccirc;e kies kelkaj
      specioj
      en printempo la &scirc;a&ubreve;mocikadoj konstruas salivaspektajn &scirc;a&ubreve;monestojn
      por siaj larvoj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_j;&c_m;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">cardamina</trd>
  <trd lng="de">Schaumkraut</trd>
  <trd lng="en">bittercress</trd>
  <trd lng="fa">&a_t;&a_r;&a_t;&f_y;&a_z;&f_k; &a_b;&a_A;&a_t;&a_l;&a_A;&a_q;&f_y;</trd>
  <trd lng="fr">cardamine</trd>
  <trdgrp lng="hu">
    <trd>kakukktorma</trd>,
    <trd>foszl&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">pinksterbloem</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_d;&c_a;&c_m;&c_i;&c_n;</trd>
  </trdgrp>
</drv>
</art>
