<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ehxinoderm.xml,v 1.7 2015/10/06 14:10:17 revo Exp $">
<kap>
  <rad>e&hcirc;inoderm</rad>/oj
</kap>
<drv mrk="ehxinoderm.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ehxinoderm.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Praformaj marbestoj kun plej ofte kvinradia simetrio kaj
      kovro el pikiloj,
      <ref tip="lst" cel="filum.0o"
      lst="voko:zoologiaj_grupoj">filumo</ref> 
      de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Echinodermata</trd>).
    </dif>
  </snc>
  <trd
  lng="be">&c_ib;&c_g;&c_l;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">equinoderms</trd>
  <trd lng="de">Stachelh&auml;uter</trd>
  <trd lng="en">echinoderms</trd>
  <trd lng="fa">&a_kh;&a_A;&a_r;&f_p;&a_w;&a_s;&a_t;&a_A;&a_n;</trd>
  <trd lng="fr">&eacute;chinodermes</trd>
  <trd lng="hu">t&uuml;sk&eacute;sb&odblac;r&udblac;ek</trd>
  <trd lng="pl">szkar&lstroke;upnie</trd>
  <trd lng="pt">equinodermos</trd>
  <trd lng="ru">&c_i;&c_g;&c_l;&c_o;&c_k;&c_o;&c_zh;&c_i;&c_je;</trd>
</drv>
</art>

