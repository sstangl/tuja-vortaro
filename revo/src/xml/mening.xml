<vortaro>
<art mrk="$Id: mening.xml,v 1.15 2014/01/05 13:10:31 revo Exp $">
<kap>
<rad>mening</rad>/o
</kap>
<drv mrk="mening.0o">
  <kap>
  <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Nomo de la tri membranoj, kiuj &ccirc;irka&ubreve;as la
      <ref tip="vid" cel="cerb.0o.ANA">cerbon</ref>,
      <ref tip="vid" cel="cerb.0eto">cerbeton</ref>
      kaj la <ref tip="vid" cel="mjel.0o">mjelon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_g;&c_a;&c_v;&c_a;&c_ja; &c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">meninge</trd>
  <trd lng="de">Hirnhaut</trd>
  <trd lng="en">meninges</trd>
  <trd lng="es">meninge</trd>
  <trd lng="fr">m&eacute;ninge</trd>
  <trd lng="hu">agyh&aacute;rtya</trd>
  <trd lng="it">meninge</trd>
  <trd lng="nl">hersenvlies</trd>
  <trd lng="pl">opona m&oacute;zgowa</trd>
  <trd lng="pt">meninge</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
</drv>
</art>
