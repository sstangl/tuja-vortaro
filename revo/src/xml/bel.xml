<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bel.xml,v 1.34 2015/10/05 01:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bel</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bel.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="bel.0a.KOMUNE">
    <dif>
      Pla&ccirc;anta al vido, a&ubreve;do, spirito:
      <ekz>
        <tld/>a infano, vazo, animo, ago;
      </ekz>
      <ekz>
        li kantas tre <tld/>an kanton
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        studo pri la <tld/>o en artoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bril.0a">brila</ref>,
      <ref cel="elegan.0a">eleganta</ref>,
      <ref cel="rav">rava</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_y;</trd>,
    <trd>&c_g;&c_o;&c_zh;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_v;</trd>,
    <trd>&c_h;&c_u;&c_b;&c_a;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>brav</trd>,
    <trd>kaer</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hezk&yacute;</trd>,
    <trd>kr&aacute;sn&yacute;</trd>,
    <trd>p&ecaron;kn&yacute;</trd>
  </trdgrp>
  <trd lng="de">sch&ouml;n</trd>
  <trdgrp lng="en">
    <trd>beautiful</trd>,
    <trd>pretty</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bonito</trd>,
    <trd>bello</trd>,
    <trd>hermoso</trd>
  </trdgrp>
  <trd lng="fr">beau</trd>
  <trd lng="hu">sz&eacute;p</trd>
  <trd lng="nl">mooi</trd>
  <trdgrp lng="no">
    <trd>vakker</trd>,
    <trd>pen</trd>
  </trdgrp>
  <trd lng="oc">b&egrave;l</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;adny</trd>,
    <trd>pi&eogonek;kny</trd>,
    <trd>&sacute;liczny</trd>,
    <trd>krasny<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">bonito</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kr&aacute;sny</trd>,
    <trd>pekn&yacute;</trd>
  </trdgrp>
  <trd lng="vo">j&ouml;nik</trd>
</drv>

<drv mrk="bel.0o">
  <kap><tld/>o</kap>
  <snc mrk="bel.0o.KOMUNE">
    <ref tip="dif" cel="bel.0eco"><tld/>eco</ref>
    <ekz>
      li paroladis al ili pri la <tld/>o, vero kaj bono
      <fnt><bib>Fab3</bib>, <lok>&scirc;tono de la sa&gcirc;uloj</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="br">
    <trd>kened</trd>,
    <trd>brav<klr tip="ind"> (ak.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>B</trd>,
    <trd>bel</trd>,
    <trd>bell</trd>
  </trdgrp>
  <trd lng="de">das <ind>Sch&ouml;ne</ind></trd>
  <trdgrp lng="fr">
    <trd>beau<klr tip="amb"> (subst.)</klr></trd>,
    <trd>beaut&eacute;<klr tip="amb"> (la beaut&eacute;, 
    propri&eacute;t&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>uroda</trd>,
    <trd>krasa</trd>,
    <trd>pi&eogonek;kno</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_o;&c_t;&c_a;<klr tip="amb">
  (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>B</trd>,
    <trd>bel</trd>,
    <trd>bell</trd>
  </trdgrp>
</drv>

<drv mrk="bel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bel.0ajxo.KOMUNE">
    <dif>
      <tld lit="B"/>a parto a&ubreve; aspekto de io:
      <ekz>
        la <tld/>a&jcirc;oj de la naturo;
      </ekz>
      <ekz>
        per mantelo de justeco Li min kovris, kiel fian&ccirc;on, kiu 
        sin ornamas per <tld/>a&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Jes; 61:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">braventez<klr tip="ind"> (tra vrav)</klr></trd>
  <trd lng="fr">beaut&eacute;<klr tip="amb"> (une beaut&eacute;
  concr&egrave;te)</klr></trd>
  <trdgrp lng="pl">
    <trd>cacko</trd>,
    <trd>pi&eogonek;kno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_o;&c_t;&c_a;<klr tip="amb">
  (&c_n;&c_je;&c_ch;&c_t;&c_o;
  &c_k;&c_r;&c_a;&c_s;&c_i;&c_v;&c_o;&c_je;)
  </klr></trd>
</drv>

<drv mrk="bel.0eco">
  <kap><ofc>*</ofc><tld/>eco</kap>
  <snc mrk="bel.0eco.KOMUNE">
    <dif>
      Kvalito de io <tld/>a, agrabla al rigardo a&ubreve; konsidero:
      <ekz>
        li amas tiun &ccirc;i knabinon pro &scirc;ia <tld/>eco
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>kened</trd>,
    <trd>kaerder</trd>,
    <trd>braventez<klr tip="ind"> (perzh)</klr></trd>
  </trdgrp>
  <trd lng="cs">kr&aacute;sa</trd>
  <trd lng="de">Sch&ouml;nheit</trd>
  <trd lng="fr">beaut&eacute;<klr tip="amb"> (la beaut&eacute;,
  propri&eacute;t&eacute;)</klr></trd>
  <trd lng="nl">schoonheid</trd>
  <trd lng="no">skj&oslash;nnhet</trd>
  <trdgrp lng="pl">
    <trd>uroda</trd>,
    <trd>krasa</trd>,
    <trd>pi&eogonek;kno</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_o;&c_t;&c_a;<klr tip="amb">
    (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kr&aacute;sa</trd>
</drv>

<drv mrk="bel.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="bel.0ega.KOMUNE">
    <dif>
      Ekstreme <tld/>a, aparte spirite kaj morale, noble kaj impone
      <tld/>a:
      <ekz>
        la heredantoj faris al li <tld/>egan enterigon
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir">admirinda</ref>,
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="pomp.0a">pompa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib;
    &c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_y;</trd>,
    <trd>&c_c;&c_u;&c_d;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>hollgaer</trd>,
    <trd>kenedus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>p&rcaron;ekr&aacute;sn&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>
  </trdgrp>
  <trd lng="de">wundersch&ouml;n</trd>
  <trdgrp lng="en">
    <trd>magnificent</trd>
  </trdgrp>
  <trd lng="es">precioso</trd>
  <trdgrp lng="fr">
    <trd>superbe</trd>,
    <trd>magnifique</trd>
  </trdgrp>
  <trd lng="hu">gy&ouml;ny&ouml;r&udblac;</trd>
  <trd lng="nl">wondermooi</trd>
  <trd lng="no">smellvakker</trd>
  <trdgrp lng="pl">
    <trd>przepi&eogonek;kny</trd>,
    <trd>prze&sacute;liczny</trd>,
    <trd>cudowny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>prekr&aacute;sny</trd>
  </trdgrp>
</drv>

<drv mrk="bel.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="bel.0eta.KOMUNE">
    <dif>
      Iom <tld/>a, pli pla&ccirc;a ol konforma al la reguloj de 
      <tld/>eco:
      <ekz>
        en la dometo &ccirc;io estis malgranda, sed tiel <tld/>eta kaj 
        pura, ke
        ne eble estas tion priskribi
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>,
      <ref cel="placx.0a">pla&ccirc;a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_k;&c_ib;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>koant</trd>,
    <trd>bravik</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hezk&yacute;</trd>,
    <trd>hezou&ccaron;k&yacute;</trd>,
    <trd>ladn&yacute;</trd>,
    <trd>n&ecaron;&zcaron;n&yacute;</trd>,
    <trd>p&uring;vabn&yacute;</trd>,
    <trd>roztomil&yacute;</trd>
  </trdgrp>
  <trd lng="de">h&uuml;bsch</trd>
  <trd lng="en">cute</trd>
  <trd lng="es">lindo</trd>
  <trdgrp lng="fr">
    <trd>joli</trd>,
    <trd>mignon</trd>
  </trdgrp>
  <trd lng="hu">csinos</trd>
  <trd lng="nl">leuk</trd>
  <trd lng="no">s&oslash;t</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;adny</trd>   
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pekn&yacute;</trd>,
    <trd>roztomil&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bel.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="bel.0igajxo.KOMUNE">
    <dif>
      Ero intence metita por <tld/>igi tuton:
      <ekz>
        perloj, el kiuj &ccirc;iu sola jam estus efektiva 
        <tld/>iga&jcirc;o en la krono de re&gcirc;ino
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ornam.0ajxo">ornama&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="br">kinkladur</trd>
  <trdgrp lng="fr">
    <trd>embellissement</trd>,
    <trd>ornement</trd>
  </trdgrp>
  <trd lng="nl">versiering</trd>
  <trd lng="no">pynt</trd>
  <trdgrp lng="pl">
    <trd>ozdoba</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="bel.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="bel.mal0a.KOMUNE">
    <dif>
      Ne pla&ccirc;a, ne agrabla al vido, sentumoj, spirito:
      <ekz>
        &leftquot;kelkloke <tld/>a vetero, aliloke mal<tld/>a&rightquot;
        <fnt><bib>SatirRak</bib>, <lok>la fluidumo de la
        &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_g;&c_o;&c_zh;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_ib;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_r;&c_o;&c_z;&c_je;&c_n;</trd>
  <trd lng="br">divalav</trd>
  <trdgrp lng="cs">
    <trd>o&scaron;kliv&yacute;</trd>,
    <trd>&scaron;eredn&yacute;</trd>,
    <trd>&scaron;kared&yacute;</trd>
  </trdgrp>
  <trd lng="de">h&auml;sslich</trd>
  <trd lng="fr">laid</trd>
  <trd lng="nl">lelijk</trd>
  <trd lng="no">stygg</trd>
  <trdgrp lng="pl">
    <trd>brzydki</trd>,
    <trd>szkaradny</trd>,
    <trd>paskudny</trd>,
    <trd>szpetny</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_k;&c_r;&c_a;&c_s;&c_i;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mrzk&yacute;</trd>,
    <trd>&scaron;kared&yacute;</trd>,
    <trd>&scaron;patn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bel.pli0igi">
  <kap>pli<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bel.pli0igi.KOMUNE">
    <dif>
      Aran&gcirc;i tiel, ke io aspektu pli <tld/>e:
      <ekz>
        niaj komercistoj kolektas la trezorojn el &ccirc;iu parto de la 
        mondo por pli<tld/>igi la urbon
        <fnt><bib>SatirRak</bib>, <lok>Pythagoras, 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>bravaat</trd>,
    <trd>kaeraat<klr tip="ind"> (v.k.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>u&ccaron;init kr&aacute;sn&yacute;m</trd>,
    <trd>zkr&aacute;snit</trd>,
    <trd>zkr&aacute;&scaron;lit</trd>
  </trdgrp>
  <trd lng="fr">embellir</trd>
  <trd lng="nl">versieren</trd>
  <trd lng="no">pynte</trd>
  <trdgrp lng="pl">
    <trd>upi&eogonek;ksza&cacute;</trd>,
    <trd>okrasza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_u;&c_k;&c_r;&c_a;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_u;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">skr&aacute;&scaron;li&tcaron;</trd>
</drv>
</art>
