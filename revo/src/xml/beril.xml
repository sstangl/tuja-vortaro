<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: beril.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>beril</rad>/o
</kap>

<drv mrk="beril.0o">
  <kap><tld/>o</kap>
  <snc mrk="beril.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Mineralo. Kiel juvel&scirc;tono, 
      <ref tip="sub" cel="smeral.0o.MIN">smeraldo</ref> kun 
      fre&scirc;verda koloro, <ref tip="sub" cel="akvamarin.0o.MIN">
      akvamarino</ref> kun koloro de marakvo, 
      heliodoro (a&ubreve; ora berilo) ktp, 
      konsistanta el natura 
      <ref tip="super" cel="silici.0ato">siliciato</ref> de 
      <ref tip="super" cel="alumin1.0o">aluminio</ref> kaj 
      <ref tip="super" cel="berili.0o">berilio</ref>,
      sesangule (heksagone) <ref tip="vid" cel="krista.0igxi">
      kristali&gcirc;anta</ref>, 
<frm>
Al<sub>2</sub>Be<sub>3</sub>(Si<sub></sub>O<sub>3</sub>)<sub>6</sub>
</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_y;&c_l;</trd>
  <trd lng="ca">beril</trd>
  <trd lng="cs">beryl</trd>
  <trd lng="de">Beryll</trd>
  <trd lng="en">beryl</trd>
  <trd lng="es">berilo</trd>
  <trd lng="fr">b&eacute;ryl</trd>
  <trd lng="hu">berill</trd>
  <trd lng="pl">beryl</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_i;&c_l;&c_l;</trd>
  <trd lng="sk">beryl</trd>
</drv>
</art>
