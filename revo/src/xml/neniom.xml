<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: neniom.xml,v 1.17 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>neniom</rad>
</kap>
<drv mrk="neniom.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adv. signifanta: &leftquot;nenia kvanto da&rightquot;:
      <ekz>multe da sprito sed <tld/> da profito
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        ni havis jam <tld/> da mono;
      </ekz>
      <ekz>
        vi <tld/> &scirc;uldas al mi;
      </ekz>
      <ekz>multe da peno, <tld/> da pano
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        &ccirc;u li <tld/> pli havas por diri al ni?<fnt>Z</fnt>
      </ekz>
      <ekz>
        <tld/> da amikoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> ta&ubreve;gi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio min <tld/> tu&scirc;as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/> komprenas el iliaj strangaj lingvoj;
      </ekz>
      <ekz>
        da homoj estas tie preska&ubreve; <tld/><fnt>Z</fnt>;
      </ekz>
      <ekz>
        li havas <tld/> da persona gloramo;
      </ekz>
      <ekz>
        en tia maniero la dismembri&gcirc;o de la lingvo <tld/> embarasas la
        lernanton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi simpligis &gcirc;is nekredebleco la gramatikon, <tld/> deprenante
        per
        tio &ccirc;i de la lingvo la klarecon, precizecon,
        flekseblecon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_k;&c_o;&c_l;&c_mol;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_n;&c_i;&c_shch;&c_o;</trd>
  <trd lng="ca">gens</trd>
  <trdgrp lng="cs">
    <trd>ani trochu</trd>,
    <trd>pranic</trd>,
    <trd>vůbec nic</trd>,
    <trd>zhola nic</trd>
  </trdgrp>
  <trd lng="de">nichts <klr tip="amb">(Menge)</klr></trd>
  <trd lng="en">nothing <klr tip="amb">(quantity)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_h;&f_y;&f_ch; &a_A;&a_n;&a_d;&a_A;&a_z;&a_h;</trd>,
    <trd>&a_h;&f_y;&f_ch; &a_m;&a_q;&a_d;&a_A;&a_r;</trd>,
    <trd>&a_h;&f_y;&f_ch; &a_t;&a_ayn;&a_d;&a_A;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">rien</trd>
  <trd lng="hu">semennyi</trd>
  <trdgrp lng="nl">
    <trd>niets</trd>,
    <trd>niks</trd>
  </trdgrp>
  <trd lng="oc">pas <ind>brica</ind></trd>
  <trdgrp lng="pt">
    <trd>nada</trd>,
    <trd>em <klr tip="amb">quantidade</klr> alguma</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_k;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>ani trochu</trd>,
    <trd>nič <klr>(o množstve)</klr></trd>
  </trdgrp>
</drv>
</art>
