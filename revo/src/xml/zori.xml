<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: zori.xml,v 1.10 2015/07/04 08:37:46 revo Exp $">
<kap>
  <rad>zori</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="zori.0o">
  <kap><tld/>o</kap>
  <snc mrk="zori.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sandal.0o">Sandalo</ref>
      konsistanta el plandumo kaj nura v-forma rimeno,
      kies pinto restas inter la dika
      kaj la sekvaj pied-fingroj.
    </dif>
  </snc>
  <trd lng="ca">xancleta</trd>
  <trd lng="cs">japonský sandál</trd>
  <trdgrp lng="de">
    <trd>Zehensandale</trd>,
    <trd>Dianette</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>thong</trd>,
    <trd>flip-flop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tong</trd>
  </trdgrp>
  <trd lng="hu">vietnami <ind>papucs</ind></trd>
  <trdgrp lng="it">
    <trd>infradito</trd>,
    <trd>zori<klr> (se alla giapponese)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_mol;&c_je;&c_t;&c_n;&c_a;&c_m;&c_k;&c_a;
<klr tip="amb"> (&c_o;&c_b;&c_u;&c_v;&c_mol;)</klr></trd>
  <trd lng="sk">japonský sandál</trd>
</drv>
</art>

