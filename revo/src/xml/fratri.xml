<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fratri.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fratri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fratri.0o">
  <kap><tld/>o</kap>
  <snc mrk="fratri.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e antikvaj Grekoj religia kaj socia grupi&gcirc;o de kelkaj familioj,
      simila al la itala
      <ref tip="vid" cel="kuri.0o.HIS">kurio</ref>,
      parto de
      <ref tip="vid" cel="trib.0o.HIS">tribo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">frátrie</trd>
  <trd lng="de">Phratrie</trd>
  <trd lng="es">fratria</trd>
  <trd lng="fr">phratrie</trd>
  <trd lng="hu">fr&aacute;tria</trd>
  <trd lng="nl">fratrie</trd>
  <trd lng="pt">fratria</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">fratária</trd>
</drv>
</art>
