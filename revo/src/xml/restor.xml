    <trd>restoran</trd>,
    <trd><ind>rumah</ind> makan</trd>
  </trdgrp>
  <trd lng="nl">restaurant</trd>
  <trd lng="pt">restaurante</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_t;&c_o;&c_r;&c_a;&c_n;</trd>
