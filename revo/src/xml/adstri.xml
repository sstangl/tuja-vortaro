<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adstri.xml,v 1.16 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>adstring</rad>/a<fnt>Z</fnt>
</kap>
<drv mrk="adstri.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Havanta la econ kuntiri la histojn, kiujn &gcirc;i tu&scirc;as:
      <ekz>
        aluno estas <tld/>a substanco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_k;&c_o;&c_m;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_w;&c_k;&c_ib;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hemostatický</trd>,
    <trd>stahující</trd>
  </trdgrp>
  <trd lng="en">astringent</trd>
  <trd lng="es">astringente</trd>
  <trd lng="fr">astringent</trd>
  <trd lng="it">astringente</trd>
  <trd lng="nl">astringent</trd>
  <trd lng="pt">adstringente</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_p;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ja;&c_zh;&c_u;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hemostatický</trd>,
    <trd>s&tcaron;ahuj&uacute;ci <klr>(tkanivo)</klr></trd>,
    <trd>sťahujúci</trd>
  </trdgrp>
</drv>
</art>
