<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: omer.xml,v 1.7 2014/11/10 05:10:19 revo Exp $">
    <kap>
    <rad>omero</rad>/o<fnt>Z</fnt>
   </kap>
   <drv mrk="omer.0o">
    <kap><tld/>o</kap>
       <fnt><bib>ZR</bib></fnt>
       <fnt><bib>PIV1</bib></fnt>
       <fnt><bib>KrED</bib></fnt>
    <snc>
    <uzo tip="fak">BIB</uzo>
       <dif>
          Malnova hebrea volumena mezurunuo, ekz. por greno.
          &Ccirc; 3,5 litroj.
       </dif>
    </snc>
       <trd lng="de">Omer</trd>
       <trd lng="es">omer</trd>
       <trd lng="en">omer</trd>
       <trd lng="fr">omer</trd>
       <trd lng="hu">homer</trd>
       <trd lng="nl">omer</trd>
       <trd lng="no">omer</trd>
       <trdgrp lng="pt">
         <trd>gomor</trd>,
         <trd>omer</trd>
       </trdgrp>
   </drv>
  </art>

