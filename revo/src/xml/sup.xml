<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sup.xml,v 1.22 2015/06/29 16:33:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sup.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o konsistanta el akvo,
      en kiu oni kuiris legomojn kaj anka&ubreve; kelkfoje viandon:
      <ekz>
        inter la mano &gcirc;is la bu&scirc;o
        ofte disver&scirc;i&gcirc;as la <tld/>o
        <klr>(la esperita&jcirc;o neefektivi&gcirc;as.
        <refgrp tip="vid">
          <ref cel="spac.inter0o">interspaco</ref>,
          <ref cel="fru">tro frue</ref>
        </refgrp>)
        </klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_u;&c_p;&c_a;</trd>
  <trd lng="cs">pol&eacute;vka</trd>
  <trd lng="de">Suppe</trd>
  <trd lng="en">soup</trd>
  <trd lng="es">sopa</trd>
  <trdgrp lng="fr">
    <trd>potage</trd>,
    <trd>soupe</trd>
  </trdgrp>
  <trd lng="he">&mem;&resh;&qof;</trd>
  <trd lng="hr">juha</trd>
  <trd lng="hu">leves</trd>
  <trd lng="id">sup</trd>
  <trd lng="nl">soep</trd>
  <trd lng="pl">zupa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_p;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_l;&c_jo;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">polievka</trd>
  <trd lng="sv">soppa</trd>
  <trd lng="tr">&ccedil;orba</trd>
</drv>
</art>
