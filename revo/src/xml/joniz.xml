<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: joniz.xml,v 1.12 2015/07/04 08:37:36 revo Exp $">
<kap><rad>joniz</rad>/i</kap>
<drv mrk="joniz.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Produkti <ref tip="vid" cel="jon.0o">jonojn</ref>,
      plenigante per ili ion:
      <ekz>
        <tld/>i la aeron en la &ccirc;ambro.
      </ekz>
    </dif>
    <ref tip="vid" cel="jon.0igi">jonigi</ref>
  </snc>
  <trd lng="be">&c_ib;&c_ja;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_j;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>ionizovati</trd>,
    <trd>působit ioniz. prostředím</trd>
  </trdgrp>
  <trd lng="de">jonisieren</trd>
  <trd lng="el">&jota;&omikron;&ny;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">ionize</trd>
  <trd lng="es">ionizar</trd>
  <trd lng="fa">&f_y;&a_w;&a_n;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">ioniser<klr tip="ind"> (enrichir en ions)</klr></trd>
  <trd lng="hu">ioniz&aacute;l</trd>
  <trd lng="it">ionizzare</trd>
  <trd lng="nl">ioniseren</trd>
  <trd lng="pt">ionizar</trd>
  <trd lng="ru">
&c_i;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ionizovať</trd>
  <trd lng="sv">jonisera</trd>
</drv>
</art>

