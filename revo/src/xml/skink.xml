  <rad>skink</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skink.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro de lacertoj (<trd lng="la">Scincus</trd>) el la varmaj
      landoj.
    </dif>
  </snc>
  <trd lng="cs">scink</trd>
  <trd lng="de">Skink</trd>
  <trd lng="en">skink</trd>
  <trd lng="hu">szkink</trd>
  <trd lng="nl">skink</trd>
  <trd lng="ru">&c_s;&c_c;&c_i;&c_n;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>jašterica <klr>(tropická)</klr></trd>,
    <trd>scink</trd>
  </trdgrp>
</drv>
</art>
