<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: balanc1.xml,v 1.27 2015/10/01 12:10:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>balanc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balanc1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="balanc1.0i.MOV">
    <dif>
      Movi ion tiel, ke &gcirc;i alterne levi&gcirc;as kaj
      mallevi&gcirc;as:
      <ekz>
        sin <tld/>i sur tabulo<fnt>B</fnt>;
      </ekz>
      <ekz>
        oni <tld/>as la kapon por jesi kaj oni skuas &gcirc;in por nei;
      </ekz>
      <ekz>
        <tld/>i kontente, senkrede la kapon;
      </ekz>
      <ekz>
        <tld/>adi la kapon la&ubreve; la takto de la muziko
        <fnt><bib>Fab1</bib>, <lok>Floroj de la malgranda
        Ida</lok></fnt>;
      </ekz>
      <ekz>
        la pastroj <tld/>adis la fumilojn
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lul.0i">luli</ref>,
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="sxance.0i">&scirc;anceli</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>balancejar</trd>,
    <trd>gronxar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>balancovat</trd>,
    <trd>dr&zcaron;et rovnov&aacute;hu</trd>,
    <trd>houpat</trd>,
    <trd>kom&iacute;hat</trd>,
    <trd>kym&aacute;cet</trd>,
    <trd>k&yacute;vat <klr>(&ccaron;&iacute;m)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wiegen</trd>,
    <trd>schaukeln</trd>,
    <trd>nicken</trd>,
    <trd>wippen</trd>,
    <trd>schwenken</trd>
  </trdgrp>
  <trd lng="en">swing</trd>
  <trd lng="es">balancear</trd>
  <trdgrp lng="fr">
    <trd>balancer<klr tip="ind"> (tr.)</klr></trd>,
    <trd>bercer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hint&aacute;ztat</trd>,
    <trd>ingat</trd>,
    <trd>lenget</trd>
  </trdgrp>
  <trd lng="nl">schommelen</trd>
  <trdgrp lng="pl">
    <trd>hu&sacute;ta&cacute;</trd>
  </trdgrp>
  <trd lng="pt">balan&ccedil;ar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_a;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hojda&tcaron;</trd>,
    <trd>k&yacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">gunga<klr> (n&aring;gon eller n&aring;got)</klr></trd>
</drv>

<drv mrk="balanc1.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="balanc1.0igxi.MOV">
    <dif>
      Movi&gcirc;i alterne leve kaj malleve.
      <ekz>
        <tld/>i&gcirc;i tien kaj reen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        cignoj <tld/>i&gcirc;as sur la akvo
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        lia animo <tld/>i&gcirc;os kiel &scirc;ipeto dum la ventego
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="danc.0i">danci</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>balancejar-se</trd>,
    <trd>gronxar-se</trd>
  </trdgrp>
  <trd lng="en">swing</trd>
  <trd lng="es">balancearse</trd>
  <trdgrp lng="fr">
    <trd>balancer<klr tip="ind"> (ntr.)</klr></trd>,
    <trd>se <ind>balancer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hint&aacute;zik</trd>,
    <trd>leng</trd>
  </trdgrp>
  <trd lng="pl">hu&sacute;ta&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>gunga<klr> (sig sj&auml;lv)</klr></trd>,
    <trd>gungas</trd>
  </trdgrp>
</drv>

<drv mrk="balanc1.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="balanc1.0ilo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Tabulo metita sur &scirc;tipo a&ubreve; subtenita per &scirc;nuroj
      por <ref tip="lst" cel="lud.0ilo" lst="voko:ludiloj">lude</ref>
      <tld/>i&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_e;&c_l;&c_ib;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">gronxador</trd>
  <trdgrp lng="cs">
    <trd>houpa&ccaron;ka</trd>,
    <trd>kyvadlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wippe</trd>, <trd>Schaukel</trd>
  </trdgrp>
  <trd lng="en">balance</trd>
  <trd lng="fr">balan&ccedil;oire</trd>
  <trd lng="hu">hinta</trd>
  <trdgrp lng="nl">
    <trd>schommel</trd>,
    <trd>wip</trd>
  </trdgrp>
  <trd lng="pl">hu&sacute;tawka</trd>
  <trd lng="ru">&c_k;&c_a;&c_ch;&c_je;&c_l;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>hojda&ccaron;ka</trd>,
    <trd>kyvadlo</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>gunga<klr> (subst.)</klr></trd>,
    <trd>gungbr&auml;da</trd>
  </trdgrp>
</drv>
</art>
