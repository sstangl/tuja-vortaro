<drv mrk="hom.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc mrk="hom.cxiu0a.KOMUNE">
    <ref tip="dif" cel="univer1.0a">universala</ref>
  </snc>
  <trd lng="de">gemein</trd>
  <trd
  lng="el">&pi;&alfa;&gamma;&kappa;&omikron_ton;&sigma;&my;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="fr">universel</trd>
  <trd lng="hu">emberi<klr tip="amb"> (egyetemes)</klr></trd>
