<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: knid.xml,v 1.9 2013/09/08 19:10:49 revo Exp $">
<kap>
  <rad>knid</rad>/uloj
</kap>
<drv mrk="knid.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="filum.0o"
      lst="voko:zoologiaj_grupoj">Filumo</ref> 
      de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Cnidaria</trd>),
      kun &ccirc;eloj ellasantaj brulan likva&jcirc;on &ccirc;e tu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Nesseltiere</trd>
  <trd lng="fr">cnidaires</trd>
  <trd lng="hu">csal&aacute;nosok</trd>
  <trdgrp lng="nl">
    <trd>holtedieren</trd>,
    <trd>neteldieren</trd>
  </trdgrp>
<trd lng="ru">&c_k;&c_i;&c_sh;&c_je;&c_ch;&c_n;&c_o;&c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
