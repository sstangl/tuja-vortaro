<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kohort.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kohort</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kohort.0o">
  <kap><tld/>o</kap>
  <snc mrk="kohort.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Dekono de romia
      <ref tip="malprt" cel="legi.0o">legio</ref>:
      <ekz>
        kontra&ubreve; tuta <tld/>o e&ccirc; Herkulo estas malforta
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>o kaj la milestro kaj la oficistoj de la Judoj
        kaptis do Jesuon kaj ligis lin
        <fnt><bib>NT</bib><lok>&Joh; 18:12</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
    <trd lng="bg">&c_k;&c_o;&c_h;&c_o;&c_r;&c_t;&c_a;</trd>
    <trd lng="de">Kohorte</trd>
    <trd lng="en">cohort</trd>
    <trd lng="fr">cohorte<klr tip="ind"> (romaine)</klr></trd>
    <trd lng="it">cohorte</trd>
  </snc>
  <snc mrk="kohort.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Multnombra aro da homoj:
      <ekz>
        kaj tiujn <klr>[reformistojn]</klr> sekvis kaj sekvas
        la <tld/>o de la sen&ccirc;apeligistoj
        <fnt>
        <bib>Voja&gcirc;o</bib>
        <aut>G. Waringhien</aut>
        <vrk>Kaj tamen ni &gcirc;in atingis!</vrk>
        <lok>leciono 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
    <trd lng="de">Haufe</trd>
    <trd lng="en">cohort</trd>
    <trdgrp lng="fr">
      <trd>cohorte<klr tip="ind"> (fig.)</klr></trd>,
      <trd>cort&egrave;ge<klr tip="ind"> (fig.)</klr></trd>,
      <trd>foule</trd>,
      <trd>troupe</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">cohort</trd>
  <trdgrp lng="cs">
    <trd>kohortace</trd>,
    <trd>napomenutí</trd>,
    <trd>pokárání</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cohort</trd>,
    <trd>cohorte</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kohortácia</trd>,
    <trd>napomenutie</trd>,
    <trd>pokarhanie</trd>
  </trdgrp>
</drv>
</art>
