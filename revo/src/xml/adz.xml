<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adz.xml,v 1.22 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>adz</rad>/o
</kap>
<drv mrk="adz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="hak.0ilo">hakilo</ref>,
      kies klingo estas fiksita en la ebeno orta al la tenilo;
      uzata de &ccirc;arpentistoj kaj barelistoj en la okazoj, kiam la
      uzo de <ref cel="topor.0o">toporo</ref> ne estas oportuna.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_l;&c_a;</trd>
  <trd lng="bg">&c_t;&c_je;&c_s;&c_l;&c_a;</trd>
  <trd lng="ca">aixa</trd>
  <trd lng="cs">sekera teslice <klr>(motyčka)</klr></trd>
  <trd lng="de">Dexel</trd>
  <trdgrp lng="en">
    <trd>adz</trd>,
    <trd>adze</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>herminette</trd>,
    <trd>erminette</trd>,
    <trd>asse</trd>,
    <trd>aissette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szekerce</trd>,
    <trd>&aacute;csb&aacute;rd</trd>
  </trdgrp>
  <trd lng="it">ascia</trd>
  <trd lng="nl">dissel</trd>
  <trdgrp lng="pl">
    <trd>toporek</trd>,
    <trd>ciosak</trd>
  </trdgrp>
  <trd lng="pt">enx&oacute;</trd>
  <trd lng="ro">tesl&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sekera <klr>(tesárska)</klr></trd>,
    <trd>tes&aacute;rska <ind>sekera</ind></trd>,
    <trd>tešla</trd>
  </trdgrp>
</drv>
</art>
