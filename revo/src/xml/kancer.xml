<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kancer.xml,v 1.21 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kancer</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kancer.0o">
  <kap><tld/>o</kap>
  <snc mrk="kancer.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref>
      karakterizita de malbonprognoza, solida
      <ref tip="super" cel="tumor.0o.MED">tumoro</ref>,
      kiu emas &ccirc;iam pli disvasti&gcirc;i kaj renaski&gcirc;i post
      fortran&ccirc;o:
      <ekz>
        epitelia, mama, prostata <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de fumantoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;</trd>
  <trd lng="ca">c&agrave;ncer</trd>
  <trd lng="cs">rakovina</trd>
  <trd lng="de">Krebs</trd>
  <trd lng="en">cancer</trd>
  <trd lng="es">c&aacute;ncer</trd>
  <trd lng="fr">cancer<klr tip="ind"> (m&eacute;d.)</klr></trd>
  <trd lng="hu">r&aacute;k</trd>
  <trd lng="id">kanker</trd>
  <trd lng="nl">kanker</trd>
  <trd lng="no">kreft</trd>
  <trdgrp lng="pl">
    <trd>rak</trd>,
    <trd>nowotw&oacute;r</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_k;</trd>
  <trd lng="sk">rakovina</trd>
</drv>
</art>
