<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gajn.xml,v 1.28 2016/02/28 19:10:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gajn</rad>/i
</kap>
<drv mrk="gajn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gajn.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Akiri ion bonan per sia penado:
      <ekz>
        <tld/>i monon, premion, batalon,
        sperton, salajron, la vivrimedojn, sian vivopanon, ies favoron,
        konfidon, &ccirc;ies korojn;
      </ekz>
      <ekz>
        <tld/>i ion el io<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="id">[<ind>oleh</ind>] memperoleh</trd>
    <trd lng="nl">verdienen</trd>
  </snc>
  <snc mrk="gajn.0i.FIG">
    <dif>
      Akiri ion bonan per favoro de la sorto:
      <ekz>
        <tld/>i en loterio<fnt>Z</fnt>,
        en kartludo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i veton;
      </ekz>
      <ekz>
        <tld/>i proceson;
      </ekz>
      <ekz>
        kiu ne riskas, tiu ne <tld/>as<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_j;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="id">[<ind>menang</ind>] memenangkan</trd>
    <trdgrp lng="nl">
      <trd>overwinnen</trd>,
      <trd>winnen</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
     <ref cel="ating.0i">atingi</ref>,
     <ref cel="spez.en0i">enspezi</ref>,
     <ref cel="konker.0i">konkeri</ref>,
     <ref cel="labor.per0i">perlabori</ref>,
     <ref cel="profit.0i">profiti</ref>,
     <ref cel="ricev.0i">ricevi</ref>,
     <ref cel="rikolt.0i">rikolti</ref>
   </refgrp>
  <trd lng="ca">guanyar</trd>
  <trdgrp lng="cs">
    <trd>boj</trd>,
    <trd>hru)</trd>,
    <trd>vyhr&aacute;t</trd>,
    <trd>vyhr&aacute;t (s&aacute;zku</trd>,
    <trd>z&iacute;skat</trd>,
    <trd>z&iacute;skat <klr>(v&yacute;hodu)</klr></trd>
  </trdgrp>
  <trd lng="de">gewinnen</trd>
  <trdgrp lng="en">
    <trd>gain</trd>,
    <trd>win</trd>
  </trdgrp>
  <trd lng="es">ganar</trd>
  <trdgrp lng="fr">
    <trd>gagner<klr tip="ind"> (de l'argent, une bataille, au jeu,
    etc.)</klr></trd>,
    <trd>conqu&eacute;rir</trd>,
    <trd>l'<ind>emporter</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyer</trd>,
    <trd>megnyer</trd>
  </trdgrp>
  <trd lng="pt">ganhar</trd>
  <trd lng="ru">&c_v;&c_y;&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vyhra&tcaron;</trd>,
    <trd>z&iacute;ska&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">kazanmak (para; bir m&uuml;cadeleyi,sava&scedil;i; oyunu kazanmak,
  &ouml;rnek: loto, sport)</trd>
</drv>

<drv mrk="gajn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="gajn.0o.ago">
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Gewinnen</trd>
    <trd lng="hu">nyer&eacute;s</trd>
    <trd lng="nl">het <ind>winnen</ind></trd>
  </snc>
  <snc mrk="gajn.0o.gajnitajxo">
    <dif>
      <tld lit="G"/>ita&jcirc;o.
    </dif>
    <trd lng="de">Gewinn</trd>
    <trd lng="hu">nyerem&eacute;ny</trd>
    <trd lng="nl">winst</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_g;&c_r;&c_y;&c_sh;</trd>
  <trd lng="ca">guany</trd>
  <trdgrp lng="cs">
    <trd>v&yacute;hoda</trd>,
    <trd>v&yacute;hra</trd>,
    <trd>zisk</trd>
  </trdgrp>
  <trd lng="es">ganancia</trd>
  <trdgrp lng="fr">
    <trd>gain</trd>,
    <trd>revenu<klr tip="ind"> (gain)</klr></trd>
  </trdgrp>
  <trd lng="pt">ganho</trd>
  <trd lng="ru">&c_v;&c_y;&c_i;&c_g;&c_r;&c_y;&c_sh;</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;hra</trd>,
    <trd>zisk</trd>
  </trdgrp>
  <trd lng="tr">kazan&ccedil;</trd>
</drv>

<drv mrk="gajn.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ne akiri ion esperitan, ne konservi avanta&gcirc;on, malvenki:
      <ekz>
        mal<tld/>i batalon, vetkuradon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_j;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">perdre</trd>
  <trdgrp lng="cs">
    <trd>prod&ecaron;lat</trd>,
    <trd>prohr&aacute;t</trd>
  </trdgrp>
  <trd lng="de">verlieren</trd>
  <trd lng="es">perder</trd>
  <trd lng="fr">perdre<klr tip="ind"> (de l'argent, une bataille, au
  jeu,
  etc.)</klr></trd>
  <trd lng="hu">vesz&iacute;t</trd>
  <trd lng="nl">verliezen</trd>
  <trd lng="pt">perder</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prehra&tcaron; <klr>(hru)</klr></trd>,
    <trd>strati&tcaron; peniaze v&nbsp;hre</trd>
  </trdgrp>
  <trd lng="tr">kaybetmek (parayi, oyunu : &ouml;rne&gbreve;in loto; sava&scedil;i,
  m&uuml;cadeleyi)</trd>
</drv>
<adm>
  La fakindikoj EKON ne devus ampleksi &ccirc;iujn sencojn. <aut>MB</aut>
</adm>
</art>
