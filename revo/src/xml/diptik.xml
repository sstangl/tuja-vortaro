      <ref cel="tripti.0o.BELA">triptiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_t;&c_y;&c_h;</trd>
  <trd lng="ca">d&iacute;ptic</trd>
  <trdgrp lng="cs">
