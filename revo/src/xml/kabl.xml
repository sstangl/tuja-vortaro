<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kabl.xml,v 1.28 2015/07/04 08:37:36 revo Exp $">
<kap>
    <ofc>5</ofc>
    <rad>kabl</rad>/o
</kap>
<drv mrk="kabl.0o">
    <kap><tld/>o</kap>
    <snc mrk="kabl.0o.sxnurego">
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">MAR</uzo>
      <ref tip="dif" cel="sxnur.0ego">&scirc;nurego</ref>
      <ekz>
        <klr>[mi]</klr> baraktis por malligi la malklaran nodon,
        kiun &scirc;i estis farinta en la <tld/>o
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        tren<tld/>o de balono a&ubreve; &scirc;ipo.
      </ekz>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
        <trd>&c_t;&c_r;&c_o;&c_s;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>corda</trd>,
        <trd>maroma</trd>
      </trdgrp>
      <trdgrp lng="cs">
        <trd>kabel</trd>,
        <trd>lano</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Trosse</trd>,
        <trd>Kabel</trd>
      </trdgrp>
      <trd lng="en">cable</trd>
      <trd lng="es">cable</trd>
      <trdgrp lng="fi">
        <trd>k&ouml;ysi</trd>,
        <trd>vaijeri</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>c&acirc;ble<klr tip="ind"> (faisceau de fils)</klr></trd>,
        <trd>filin</trd>,
        <trd>cordage<klr tip="ind"> (c&acirc;ble)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&aacute;bel</trd>,
        <trd>k&ouml;t&eacute;l</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>tros</trd>,
        <trd>kabel</trd>
      </trdgrp>
      <trd lng="pl">lina</trd>
      <trd lng="pt">cabo <klr tip="amb">(feixe de fios)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
        <trd>&c_t;&c_r;&c_o;&c_s;</trd>
      </trdgrp>
    </snc>

    <snc mrk="kabl.0o.dratofasko">
      <uzo tip="fak">ELE</uzo>
      <uzo tip="fak">TEL</uzo>
      <dif>
        Fasko el pluraj <ref tip="prt"
        cel="drat.0o.fadeno">dratoj</ref>,
        individue tegitaj per izolmaterialo, por konduki elektron
        a&ubreve; lumon; fasko el pluraj tiaj faskoj:
        <ekz>
          elektraj instrumentoj estas konektitaj al amplifiloj per
          <tld/>oj
          <fnt>
          <aut>B. Wennergren</aut>
          <vrk>Roko kaj Popo</vrk><lok>
          <url ref="http://bertilow.com/roko/k.html">Litero
          K</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          transmara telekomunika <tld/>o.
        </ekz>
      </dif>
      <adm>
        Kontroli trd be cs de en es fr hu nl sv ru tr post
        vastigo de la difino ankaux al lumkondukiloj.
        <aut>MB</aut>
      </adm>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
        <trd>&c_d;&c_r;&c_o;&c_t;</trd>,
        <trd>&c_sh;&c_n;&c_u;&c_r;</trd>
      </trdgrp>
      <trdgrp lng="cs">
        <trd>kabel</trd>,
        <trd>lano</trd>
      </trdgrp>
      <trd lng="de">Kabel</trd>
      <trd lng="en">cable</trd>
      <trd lng="es">cable</trd>
      <trdgrp lng="fi">
        <trd>kaapeli</trd>,
      </trdgrp>
      <trd lng="fr">c&acirc;ble<klr tip="ind"> (conducteur)</klr></trd>
      <trd lng="hu">k&aacute;bel</trd>
      <trd lng="nl">kabel</trd>
      <trdgrp lng="pl">
        <trd>kabel</trd>
        <trd>&sacute;wiat&lstroke;ow&oacute;d
        <klr> (kabel z w&lstroke;&oacute;knami
        &sacute;wiat&lstroke;owodowymi)</klr></trd>
      </trdgrp>
      <trd lng="pt">cabo <klr tip="amb">(feixe de fios)</klr></trd>
      <trd lng="sv">kabel</trd>
      <trd lng="ru">&c_k;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>
      <trd lng="tr">kablo</trd>
    </snc>

    <snc mrk="kabl.0o.televidsistemo">
      <uzo tip="fak">TEK</uzo>
      <ref tip="dif" cel="televid.kabla0o"><tld/>a televido</ref>
      <ekz>
        <tld/>okompanio decidis forpreni la oficialan
        turkan televidan stacion (TRT) de la <tld/>o
        <fnt>
        <aut>gvd</aut>
        <vrk>Elkabligita</vrk>
        <lok><url ref="http://www.esperanto.be/fel/mon/006637.html">
        Monato</url></lok>
        </fnt>.
      </ekz>
      <trd lng="ca">televisi&oacute; per cable</trd>
      <trdgrp lng="es">
        <trd>Cable</trd>,
        <trd>Televisi&oacute;n por cable</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>c&acirc;ble<klr tip="ind"> (t&eacute;l&eacute;vision par
        c&acirc;ble)</klr></trd>,
        <trd>t&eacute;l&eacute;vision par c&acirc;ble</trd>
      </trdgrp>
      <trd lng="pl">telewizja kablowa</trd>
    </snc>

    <snc mrk="kabl.0o.mezurunuo">
      <uzo tip="fak">MAR</uzo>
      <uzo tip="stl">ARK</uzo>
      <dif>
        Malnova praktika
        <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
        <ref cel="long.0o.FIZ">longo</ref>, uzata por mezuri mallongajn
        distancojn
        sur la maro, proksimume egala al 200
        <ref cel="metr.0o.unuo">metroj</ref>:
        <ekz>
          <klr>[la trimastulo]</klr> trovi&gcirc;is en la distanco
          de tri &gcirc;is kvar <tld/>oj de la tero
          <fnt><bib>Fab4</bib><lok>Historio el la dunoj</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="fi">kaapelinmitta</trd>
      <trd lng="fr">encablure</trd>
      <trd lng="pl">kabel</trd>
    </snc>
  <trd lng="ca">cable</trd>
  <trd lng="sk">kábel</trd>
</drv>

<drv mrk="kabl.0ero">
    <kap><tld/>ero</kap>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">TEL</uzo>
    <uzo tip="fak">MAR</uzo>
    <snc>
      <dif>
        &Ccirc;iu el la <ref tip="super" cel="drat.0o">dratoj</ref>,
        konsistigantaj <tld/>on.
      </dif>
    </snc>
  <trd lng="ca">fil</trd>
  <trd lng="cs">kabelová žíla</trd>
  <trd lng="en">strand</trd>
  <trdgrp lng="fi">
      <trd>johdin <klr tip="amb"> (kaapelin osana)</klr></trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>fil<klr tip="ind"> (d'un c&acirc;ble)</klr></trd>,
      <trd>brin<klr tip="ind"> (d'un c&acirc;ble)</klr></trd>
    </trdgrp>
  <trd lng="hu">&eacute;r<klr> (k&aacute;bel&eacute;)</klr></trd>
  <trdgrp lng="pl">
      <trd>&zdot;y&lstroke;a<klr> (kablowa)</klr></trd>
      <trd>w&lstroke;&oacute;kno<klr>
(&sacute;wia&lstroke;owodowe)</klr></trd>
      <trd>&sacute;wiat&lstroke;ow&oacute;d
      <klr> (potoczne okre&sacute;lenie zar&oacute;wno kabla jak i
      w&lstroke;&oacute;kna &sacute;wiat&lstroke;owodwego</klr></trd>
    </trdgrp>
  <trd lng="ru">&c_zh;&c_i;&c_l;&c_a;
    <klr>(&c_v;
    &c_m;&c_n;&c_o;&c_g;&c_o;&c_zh;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_m;
    &c_k;&c_a;&c_b;&c_je;&c_l;&c_je;, &c_t;&c_r;&c_o;&c_s;&c_je;)
    </klr></trd>
  <trd lng="sk">káblová žila</trd>
</drv>

<drv mrk="kabl.0eto">
    <kap><tld/>eto</kap>
    <snc>
      <dif>
        Malgranda <tld/>o.
      </dif>
    </snc>
    <adm>
      Senco neklara. Malgranda, maldika,...?
      <aut>MB</aut>
    </adm>
    <trd lng="ca">fil</trd>
    <trd lng="en">wire</trd>
    <trd lng="pl">kabelek</trd>
  <trd lng="sk">káblik</trd>
  <trd lng="cs">kabílek</trd>
</drv>
</art>
