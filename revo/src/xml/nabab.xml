<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nabab.xml,v 1.7 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>nabab</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nabab.0o">
  <kap><tld/>o</kap>
  <snc mrk="nabab.0o.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Malnova ofico kaj titolo de 
      <ref tip="super" cel="gubern1.0estro.POL">guberniestro</ref>
      en Hindio, kaj poste de regnestro:
     <ekz>
       la heroo de <ctl>Malsata &Scirc;tono</ctl> <klr>...</klr> &ccirc;e 
       vesperi&gcirc;o vestas sin kiel tradicia <tld/>o kaj fumas akvopipon
       <fnt>
         <aut>M. Duc Goninaz</aut>,
         <vrk><url
         ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr74/malsata.html">
         Recenzo: Malsata &scirc;tono...</url></vrk>,
         <lok>Nica Literatura Revuo, 1962-03 &gcirc;is 04, numero 7/4, p. 
         151a-154a</lok>
       </fnt>.
     </ekz>
    </dif>
  </snc>
  <snc mrk="nabab.0o.FIG">
    <dif>
      <ref tip="vid" cel="ricx.0ulo">Ri&ccirc;ulo</ref>, luksemulo:
      <ekz>
        tiu Sandy nun ri&ccirc;i&gcirc;is, kiel <tld/>o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sibari.0o">sibarito</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_n;&c_a;&c_b;&c_a;&c_b;</trd>
  <trd lng="ca">nabab</trd>
  <trdgrp lng="cs">
    <trd>bohatec</trd>,
    <trd>nabob</trd>,
    <trd>pyšný nadutec</trd>
  </trdgrp>
  <trd lng="de">Nawab</trd>
  <trd lng="en">nabob</trd>
  <trd lng="fr">nabab</trd>
  <trd lng="it">nababbo</trd>
  <trdgrp lng="sk">
    <trd>boháč</trd>,
    <trd>nabob</trd>,
    <trd>pyšný nadutec</trd>
  </trdgrp>
</drv>
</art>

