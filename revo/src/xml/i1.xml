    <ofc>*</ofc>-<rad>i</rad>
</kap>

<drv mrk="i1.0">
  <kap>-<tld/></kap>
  <snc mrk="i1.0.finajxo">
    <uzo tip="fak">GRA</uzo>
    <dif>
     <ref tip="super" cel="fin.0ajxo.GRA">Fina&jcirc;o</ref> markanta
     la <ref cel="infini2.0o">infinitivon</ref> &ccirc;e <ref
     cel="verb.0o">verbo</ref>:
     <ekz>la&ubreve;d<tld/>, am<tld/><fnt>Z</fnt></ekz>.
     </dif>
  </snc>
  <refgrp tip="hom">
    <ref cel="i.0.sufikso">-<tld/>/<klr> (sufikso)</klr></ref>
  </refgrp>
  <trd lng="fr"><klr>(infinitif de verbes)</klr></trd>
</drv>
</art>

