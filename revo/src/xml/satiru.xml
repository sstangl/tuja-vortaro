<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: satiru.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>satirus</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="satiru.0o">
  <kap><tld/>o</kap>
  <snc mrk="satiru.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Arbara duondio de la greka mitologio:
      <ekz>
        la verkisto figuris lin kapropieda <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>logos</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="satiru.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo voluptema kaj cinika.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_y;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>prostopášník <klr>(přen.)</klr></trd>,
    <trd>satyr</trd>
  </trdgrp>
  <trd lng="de">Satyr</trd>
  <trd lng="en">satyr</trd>
  <trd lng="es">s&aacute;tiro</trd>
  <trd lng="fr">satyre</trd>
  <trd lng="hu">szat&iacute;r</trd>
  <trd lng="it">satiro</trd>
  <trd lng="nl">satyr</trd>
  <trd lng="pl">satyr</trd>
  <trd lng="ru">&c_s;&c_a;&c_t;&c_i;&c_r;</trd>
  <trd lng="sk">satyr</trd>
</drv>
</art>
