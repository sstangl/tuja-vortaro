<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: veft.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>veft</rad>/o
</kap>
<drv mrk="veft.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Fadenoj, kiujn oni pasigas inter la
      <ref tip="vid" cel="varp.0ero">varperojn</ref>.
    </dif>
    <ref tip="malprt" cel="teks.0ajxo.TEKS">teksa&jcirc;o</ref>
  </snc>
  <trd lng="ca">trama</trd>
  <trd lng="cs">osnova</trd>
  <trdgrp lng="de">
    <trd>Schussfaden</trd>,
    <trd>Querfaden</trd>
  </trdgrp>
  <trd lng="en">weft</trd>
  <trd lng="es">trama</trd>
  <trd lng="fr">trame</trd>
  <trd lng="hu">vet&uuml;l&eacute;k</trd>
  <trd lng="nl">inslag</trd>
  <trd lng="pl">w&aogonek;tek<klr> (materia&lstroke;u)</klr></trd>
  <trdgrp lng="pt">
   <trd>trama</trd>,
   <trd>tecido</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_k;<klr tip="ind">
  (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>priečne nite <klr>(text.)</klr></trd>,
    <trd>útok</trd>
  </trdgrp>
</drv>

<drv mrk="veft.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Ccirc;iu unuopa fadeno, kiu interpasante la
      <ref tip="vid" cel="varp.0o">varpon</ref>
      formas la <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">fil de trama</trd>
  <trd lng="cs">útková nit</trd>
  <trd lng="fr">duite</trd>
  <trd lng="hu">vet&uuml;l&eacute;kfonal</trd>
  <trd lng="nl">inslagdraad</trd>
  <trd lng="pl">ni&cacute; <ind>w&aogonek;tku</ind></trd>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">jednotlivá priečna niť</trd>
</drv>
</art>
