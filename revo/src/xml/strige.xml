<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: strige.xml,v 1.9 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>striged</rad>/oj
</kap>
<drv mrk="strige.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de <ref
      tip="malprt"
      cel="strig.0oformaj">strigoformaj
      birdoj</ref> (<trd lng="la">Strigidae</trd>), viandman&gcirc;antaj
      noktobirdoj kun anta&ubreve;endirektitaj okuloj kaj kun plumkovritaj
      piedfingroj.
    </dif>
  </snc>
  <trd lng="ca">estr&iacute;gids</trd>
  <trd lng="cs">puštíkovití</trd>
  <trdgrp lng="en">
    <trd>true owls</trd>,
    <trd>typical owls</trd>
  </trdgrp>
  <trd lng="hu">bagolyf&eacute;l&eacute;k</trd>
  <trd lng="nl">echte <ind>uilen</ind><klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_i;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_s;&c_o;&c_v;&c_y;</ind> <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">sovovité</trd>
</drv>
</art>
