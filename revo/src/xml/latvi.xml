<art mrk="$Id: latvi.xml,v 1.10 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>Latvi</rad>/o
</kap>

<drv mrk="latvi.L0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <ref tip="dif" cel="latv.L0ujo.GEOG">Latvujo</ref>
      <ekz>
        <tld lit="L"/>o estas lando en nordoriento de E&ubreve;ropo.
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Latvio">Latvio</url></lok></fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_L;&c_a;&c_t;&c_v;&c_i;&c_ja;</trd>
  <trd lng="cs">Lotyšsko</trd>
  <trd lng="de">Lettland</trd>
  <trd lng="en">Latvia</trd>
  <trd lng="fr">Lettonie</trd>
  <trd lng="hu">Lettorsz&aacute;g</trd>
  <trd lng="id">Latvia</trd>
  <trd lng="nl">Letland</trd>
  <trd lng="pl">&Lstroke;otwa</trd>
  <trd lng="sk">Lotyšsko</trd>
</drv>
</art>

