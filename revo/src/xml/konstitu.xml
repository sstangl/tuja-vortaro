<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konstitu.xml,v 1.6 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>konstitu</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konstitu.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="konsis.0igi">konsistigi</ref>
    <ekz>
      la imito de la realo <tld/>as la esencon de la teatro
      <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 48a, p.
      360a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>představovat</trd>,
    <trd>tvořit</trd>,
    <trd>ustavit</trd>,
    <trd>založit</trd>
  </trdgrp>
  <trd lng="fr">constituer</trd>
  <trdgrp lng="nl">
    <trd>vormen</trd>,
    <trd><ind>bestaan</ind> uit</trd>,
    <trd>constitueren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>constituir</trd>,
    <trd>compor</trd>,
    <trd>formar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ustanoviť</trd>,
    <trd>založiť</trd>
  </trdgrp>
</drv>
</art>

