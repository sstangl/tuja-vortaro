<vortaro>
  <art mrk="$Id: pjangx.xml,v 1.8 2012/08/04 12:10:11 revo Exp $">
    <kap>
      <rad>Pjan&gcirc;</rad>/o
    </kap>

    <drv mrk="pjangx.P0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Rivero en la
	  <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
	  dislimanta Ta&gcirc;ikujon kaj Afganujon, la maldekstra
	  komponanto de <ref cel="amudarj.A0o">Amudarjo</ref>.
	</dif>
      </snc>
      <trd lng="be">&c_P;&c_ja;&c_n;&c_d;&c_zh;</trd>
      <trd lng="ca">Pjandsh</trd>
      <trd lng="en">Pandj</trd>
      <trd lng="de">Pjandsch</trd>
      <trd lng="fr">Piandj</trd>
      <trd lng="hu">Pjandzs</trd>
      <trd lng="nl">Pjandsj</trd>
      <trd lng="pt">Piandj</trd>
      <trd lng="ru">&c_P;&c_ja;&c_n;&c_d;&c_zh;</trd>
    </drv>

  </art>

