<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tin2.xml,v 1.8 2013/10/30 02:10:14 revo Exp $">
<kap>
  <rad>tin</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tin2.00">
  <kap><tld/>&nbsp;<tld/>,
  <var><kap><tld/>&nbsp;<tld/>&nbsp;<tld/></kap></var></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="tin2.00.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de iom akuta <ref tip="vid" cel="tint.0o">tinto</ref>,
      ekzemple tiu de batata metala&jcirc;o:
      <ekz>
        &leftquot;<tld/>, <tld/>&rightquot; sonoris la sonoriletoj
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        la veturiloj veturis, la azenoj iris...
        <tld/>-<tld/>-<tld/>, &ccirc;ar ili portis sonoriletojn
        <fnt><bib>Fab2</bib>, <lok>la ombro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>drelin-drelin</trd>,
    <trd>ding-dong</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dzy&nacute;-dzy&nacute;</trd>,
    <trd>ding-dong</trd>,
    <trd>brzd&eogonek;k-brzd&eogonek;k</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ding-dong</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_n;&c_mol;-&c_d;&c_i;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

