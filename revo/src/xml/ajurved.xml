<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ajurved.xml,v 1.8 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>ajurved</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="ajurved.0o">
  <kap><tld/>o</kap>
  <snc mrk="ajurved.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Hinddevena tradicia medicino kaj <ref tip="super" 
      cel="art.kurac0o.MED">kuracarto</ref>:
      <ekz>
        mi instruas medicinon al la studentoj de <tld/>o en la <tld/>a 
        kolegio
        <fnt>
          <aut>P. V. Ranganayakulu</aut>,
          <vrk>Leteroj</vrk>,
          <lok><url
          ref="http://www.geocities.com/rezamenhof/TE-NurE.htm">
          Tajvana Esperantisto, 2002-03</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a masa&gcirc;o <klr>...</klr> de la tuta korpo kun varmaj
        herboleoj
        <fnt>
          <aut>Daniel D.</aut>,
          <vrk><url
          ref="http://ddd.delarasse.free.fr/imprimersans.php?id_article=39">
          Tu&scirc;o kaj Masa&gcirc;o</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_ju;&c_r;&c_v;&c_je;&c_d;&c_a;</trd>
  <trd lng="bg">&c_a;&c_ju;&c_r;&c_v;&c_je;&c_d;&c_a;</trd>
  <trd lng="cs">ajurvéda</trd>
  <trd lng="de">Ayurveda</trd>
  <trd lng="fa">&a_A_madda;&f_y;&a_w;&a_r;&a_w;&a_d;&a_A;</trd>
  <trd lng="fr">ayurveda</trd>
  <trd lng="nl">ayurveda</trd>
  <trd lng="pt">aiurveda</trd>
  <trd lng="sk">ajurvéda</trd>
</drv>
</art>

