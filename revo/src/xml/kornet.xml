<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kornet.xml,v 1.14 2013/09/08 18:10:46 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kornet</rad>/o
</kap>
<drv mrk="kornet.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">Blovinstrumento</ref> simila
      al trumpeto kun klavbutonoj.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_n;&c_e;&c_t; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;
    &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
    <trd lng="ca">corneta</trd>
    <trdgrp lng="de">
       <trd>Kornett</trd>,
       <trd>Zink</trd>,
       <trd>Zinke</trd>
    </trdgrp>
    <trd lng="en">cornet</trd>
    <trd lng="es">corneta</trd>
    <trd lng="fr">cornet &agrave; pistons</trd>
    <trd lng="hu">kornett</trd>
    <trd lng="nl">cornet</trd>
    <trd lng="pt">corneta <klr tip="amb">(&ecirc;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_n;&c_je;&c_t;-&c_a;-&c_p;&c_i;&c_s;&c_t;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
