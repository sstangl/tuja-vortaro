<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pat.xml,v 1.21 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pat</rad>/o
</kap>

<drv mrk="pat.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KUI</uzo>
    <dif>
      Malprofunda senkovrila kaserolo kun longa tenilo, servanta por
      friti; fritilo:
      <ekz>
        Li elprenis sangokolbason, &gcirc;in fritis sur pato
      <fnt>
        <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
        <vrk>La Fremdulo</vrk>,
        <lok>S.A.T., 1993, p. 40</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_i;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="br">
    <trd>paelon-lostek</trd>,
    <trd>palarenn</trd>,
    <trd>pillig-lostek</trd>
  </trdgrp>
  <trd lng="ca">paella</trd>
  <trdgrp lng="cs">
    <trd>pekáč</trd>,
    <trd>pánev</trd>
  </trdgrp>
  <trd lng="de">Pfanne</trd>
  <trd lng="el">&tau;&eta;&gamma;&alfa_ton;&ny;&jota;</trd>
  <trd lng="en">pan</trd>
  <trd lng="es">sart&eacute;n</trd>
  <trd lng="fr">po&ecirc;le</trd>
  <trd lng="hu">serpeny&odblac;</trd>
  <trdgrp lng="nl">
    <trd>pan</trd>,
    <trd>braadpan</trd>
  </trdgrp>
  <trd lng="pt">frigideira</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>panvica</trd>,
    <trd>pekáč</trd>
  </trdgrp>
</drv>
</art>
