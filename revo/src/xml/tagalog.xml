<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tagalog.xml,v 1.6 2014/02/28 15:10:20 revo Exp $">
<kap>
  <rad>tagalog</rad>/o <fnt><bib>LanLin</bib></fnt>
</kap>

<drv mrk="tagalog.0o">
  <kap><tld/>o</kap>
  <snc mrk="tagalog.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Ano de popolo el Filipinoj.
      <ekz>
        la <tld lit="T"/>a, la lingvo de la plej potenca etno (la 
        <tld lit="T"/>oj), estas la tut-Filipina komunikilo
        <fnt>
        <aut>Victor Medrano</aut>,
        <vrk><url
        ref="http://ca.geocities.com/vixcafe/esperanto/fil.html">Lingvoj
        en Filipinoj</url></vrk>,
        <lok>2005-11-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">tag&agrave;log<klr> (individu)</klr></trd>
  <trd lng="de">Tagale</trd>
  <trd lng="fr">tagalog<klr> (peuple)</klr></trd>
  <trd lng="hu">tagalog</trd>
  <trd lng="nl">Tagalog<klr tip="ind"> (volk)</klr></trd>
</drv>

<drv mrk="tagalog.0alingvo">
  <kap><tld/>a lingvo</kap>
  <snc mrk="tagalog.0alingvo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Lingvo de la <tld/>oj, bazo de la nacia filipina lingvo.
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_o;&c_g;<klr> (&c_je;&c_z;&c_i;&c_k;)</klr></trd>,
    <trd>&c_t;&c_a;&c_g;&c_a;&c_l;&c_s;&c_k;&c_i; &c_je;&c_z;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">tag&agrave;log<klr> (llengua)</klr></trd>
  <trd lng="de">Tagalog<klr tip="ind"> (Sprache)</klr></trd>
  <trd lng="fr">tagalog<klr> (langue)</klr></trd>
  <trd lng="hu">tagalog<klr tip="ind"> (pl. nyelv)</klr></trd>
  <trd lng="nl">Tagalog<klr tip="ind"> (taal)</klr></trd>
</drv>
</art>

