<vortaro>
<art mrk="$Id: jaguar.xml,v 1.22 2014/05/30 08:10:47 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>jaguar</rad>/o
</kap>
<drv mrk="jaguar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Sudamerika <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> de <ref tip="lst" 
cel="panter.0o.genro" lst="voko:rabobestoj">pantero</ref>
      (<trd lng="la">Panthera onca</trd>), kun brune flava felo kaj 
nigraj makuloj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_g;&c_u;&c_a;&c_r;</trd>
  <trd lng="ca">jaguar</trd>
  <trd lng="de">Jaguar</trd>
  <trd lng="en">jaguar</trd>
  <trd lng="es">jaguar</trd>
  <trdgrp lng="fr">
    <trd>jaguar</trd>
  </trdgrp>
  <trd lng="hu">jagu&aacute;r</trd>
  <trd lng="id">jaguar</trd>
  <trd lng="it">giaguaro</trd>
  <trd lng="nl">jaguar</trd>
  <trd lng="pl">jaguar</trd>
  <trdgrp lng="pt">
   <trd>on&ccedil;a-pintada</trd>,
   <trd>on&ccedil;a</trd>,
   <trd>jaguar</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_g;&c_u;&c_a;&c_r;</trd>
  <trd lng="sk">jagu&aacute;r</trd>
  <trd lng="sv">jaguar</trd>
</drv>
</art>
