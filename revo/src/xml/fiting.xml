<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fiting.xml,v 1.4 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fiting</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fiting.0o">
  <kap><tld/>o</kap>
  <snc mrk="fiting.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Interadaptilo de tuboj, per kiu oni ilin
      plilongigas, angule kunmetas, sekvigas trans
      lar&gcirc;o&scirc;an&gcirc;o...:
      <ekz>
        lerta laboristo, kun <tld/>aj iloj kaj
        mezuriloj, trovus tre malfacile fari
        vaksajn &ccirc;elojn la&ubreve; &gcirc;usta formo
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fitink</trd>,
    <trd>fitinka</trd>,
    <trd>spojník</trd>
  </trdgrp>
  <trd lng="de">Fitting</trd>
  <trd lng="en">fitting</trd>
  <trd lng="fr">raccord de tuyauterie</trd>
  <trd lng="nl">fitting</trd>
  <trd lng="pl">kszta&lstroke;tka</trd>
  <trd lng="ru">&c_F;&c_i;&c_t;&c_i;&c_n;&c_g;</trd>
  <trd lng="sk">spojník</trd>
  <trd lng="uk">&c_F;&c_ib;&c_t;&c_i;&c_n;&c_g;</trd>
</drv>
</art>

