<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sergxe.xml,v 1.17 2013/11/30 21:10:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ser&gcirc;ent</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sergxe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sergxe.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.sub0o">suboficiro</ref>
      de plej malalta rango:
      <ekz>
        post kelkaj minutoj aperis kozaka <tld/>o
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_r;&c_zh;&c_a;&c_n;&c_t;</trd>
  <trd lng="bg">&c_s;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Sergeant</trd>,
    <trd>Wachtmeister</trd>
  </trdgrp>
  <trd lng="en">sergeant</trd>
  <trd lng="fa">&f_g;&a_r;&a_w;&a_h;&a_b;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>sergent<klr tip="ind"> (sous-officier)</klr></trd>,
    <trd>mar&eacute;chal des logis</trd>
  </trdgrp>
  <trd lng="hu">&odblac;rmester</trd>
  <trd lng="nl">sergeant</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_t;</trd>
  <trd lng="es">sargento</trd>
  <trd lng="it">sergente</trd>
</drv>

<drv mrk="sergxe.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <snc mrk="sergxe.cxef0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.sub0o">suboficiro</ref>, kiu rangas
      inter
      <ref cel="sergxe.0o"><tld/>o</ref> kaj
      <ref cel="adjuda.0o">adjudanto</ref>.
    </dif>
  </snc>
  <trdgrp lng="fa">
    <trd>&f_g;&a_r;&a_w;&a_h;&a_b;&a_A;&a_n; &f_y;&f_k;&a_m;</trd>,
    <trd>&a_s;&a_r;&f_g;&a_r;&a_w;&a_h;&a_b;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sergent-chef</trd>,
    <trd>mar&eacute;chal des logis-chef</trd>
  </trdgrp>
  <trd lng="it">sergente maggiore</trd>
</drv>
</art>
