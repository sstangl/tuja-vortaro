<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: borneol.xml,v 1.6 2015/08/21 15:10:15 revo Exp $">
<kap>
  <rad>borneol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="borneol.0o">
  <kap><tld/>o</kap>
  <snc mrk="borneol.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      Alkoholo de <ref tip="vid" cel="kamfor.0o">kamforo</ref> 
      kun la formulo <frm>&c_S;<sub>10</sub>&c_N;<sub>18</sub>&c_O;</frm>:
      <ekz>
        Pacema pretigu pulvoron el perlo, <tld/>o kaj cinabro
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 84a, volumo 3a, p.
        63a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_n;&c_e;&c_o;&c_l;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;&c_n;&c_je;&c_o;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>borneol</trd>,
    <trd>malajsk&yacute; kafr</trd>
  </trdgrp>
  <trd lng="de">Borneol</trd>
  <trd lng="en">borneol</trd>
  <trd lng="fr">born&eacute;ol</trd>
  <trd lng="nl">borneol</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_n;&c_je;&c_o;&c_l;</trd>
  <trd lng="sk">borneol</trd>
</drv>
</art>

