<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arogan.xml,v 1.17 2015/09/17 03:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>arogant</rad>/a
</kap>
<drv mrk="arogan.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ofende fiera, impertinenta:
      <ekz>
        <tld/>a fripono, entrepreno, parolo.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>arrogant</trd>,
      <trd>impudent</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>arrog&aacute;ns</trd>,
      <trd>pimasz</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Kiu arogas al si.
    </dif>
    <refgrp tip="sin">
      <ref cel="respek">malrespekta</ref>,
      <ref cel="fid">tromemfida</ref>,
      <ref cel="modest">malmodesta</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="alt">alte</ref>,
      <ref cel="kracx.0i">kra&ccirc;i</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>arrog&aacute;ns</trd>,
      <trd>k&ouml;vetelz&odblac;d&odblac;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_y;</trd>, 
    <trd>&c_d;&c_z;&c_jo;&c_r;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">zpupn&yacute;</trd>
  <trd lng="en">arrogant</trd>
  <trd lng="es">arrogante</trd>
  <trdgrp lng="id">
    <trd>arogan</trd>,
    <trd>sombong</trd>
  </trdgrp>
  <trd lng="it">arrogante</trd>
  <trd lng="pt">arrogante</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arogantn&yacute;</trd>,
    <trd>bezo&ccaron;iv&yacute;</trd>,
    <trd>drz&yacute;</trd>,
    <trd>spurn&yacute;</trd>
  </trdgrp>
</drv>
</art>
