<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: genealog.xml,v 1.9 2015/10/28 07:10:14 revo Exp $">
<kap>
  <ofc>7</ofc>
  <rad>genealog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="genealog.0o">
  <kap><tld/>o</kap>
  <snc mrk="genealog.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <ref tip="dif" cel="geneal.0isto.SCI">genealogiisto</ref>
    <ekz>
      <tld/>oj esploras la devenon kaj parencecon de familioj
      <fnt>
      <aut>Roland Rotsaert</aut>,
      <vrk>Genealogio</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 2006a, numero 8-9a, p. 33a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_g;&c_je;&c_n;&c_je;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">genealogista</trd>
  <trdgrp lng="cs">
    <trd>genealog</trd>,
    <trd>rodopisec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Genealoge</trd>,
    <trd>Familienforscher</trd>
  </trdgrp>
  <trd lng="en">genealogist</trd>
  <trd lng="fr">g&eacute;n&eacute;alogiste</trd>
  <trd lng="nl">genealoog</trd>
  <trd lng="pl">genealog</trd>
  <trdgrp lng="pt">
    <trd>genealogista</trd>,
    <trd>linhagista</trd>
  </trdgrp>
  <trd lng="sk">geneal&oacute;g</trd>
</drv>
</art>

