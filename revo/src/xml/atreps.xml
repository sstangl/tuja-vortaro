<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: atreps.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>atrepsi</rad>/o
</kap>
<drv mrk="atreps.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> de novnaskitoj, ka&ubreve;zata de
      malordo en la nutrado.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_r;&c_e;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ca">atr&egrave;psia</trd>
  <trd lng="cs">atrepsie</trd>
  <trd lng="en">athrepsia</trd>
  <trd lng="es">atrepsia</trd>
  <trd lng="fr">athrepsie</trd>
  <trdgrp lng="hu">
    <trd>athrepsia</trd>,
    <trd>csecsem&odblac;kori <ind>sorvad&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="id">atrepsia</trd>
  <trd lng="ru">&c_a;&c_t;&c_r;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>atrepsia</trd>,
    <trd>slabosť dojčiat pre poruchu v trávení</trd>
  </trdgrp>
</drv>
</art>
