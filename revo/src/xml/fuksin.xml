<vortaro>
<art mrk="$Id: fuksin.xml,v 1.10 2013/04/09 09:10:10 revo Exp $">
<kap>
  <rad>fuksin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fuksin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Anilina ru&gcirc;a&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_k;&c_s;&c_ib;&c_n;</trd>
  <trd lng="de">Fuchsin</trd>
  <trd lng="fr">fuchsine</trd>
  <trdgrp lng="pl">
    <trd>fuksyna</trd>,
    <trd>rozanilina</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fucsina</trd>,
    <trd>magenta</trd>,
    <trd>rosanilina</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_k;&c_s;&c_i;&c_n;</trd>
</drv>
</art>
