<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rem.xml,v 1.22 2015/10/06 10:10:13 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>rem</rad>/i
</kap>
<drv mrk="rem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Movi &scirc;ipon sur akvo, anta&ubreve;enpu&scirc;ante &gcirc;in per
      speciala plata ligna ilo:
      <ekz>
        en trankvila vetero &ccirc;iu <tld/>as sen dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">veslovat</trd>
  <trd lng="de">rudern</trd>
  <trd lng="en">row</trd>
  <trd lng="fr">ramer</trd>
  <trd lng="hu">evez</trd>
  <trd lng="nl">roeien</trd>
  <trd lng="pl">wios&lstroke;owa&cacute;</trd>
  <trd lng="pt">remar</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_s;&c_t;&c_i; <klr tip="amb">(&c_v;&c_jo;&c_s;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
  <trd lng="sk">veslova&tcaron;</trd>
</drv>

<drv mrk="rem.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Plata ligna ilo kun longa tenilo, uzata por movi &scirc;ipon.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_l;&c_o;</trd>
  <trd lng="cs">veslo</trd>
  <trd lng="de">Ruder</trd>
  <trd lng="en">oar</trd>
  <trdgrp lng="fr">
    <trd>rame<klr tip="ind"> (aviron)</klr></trd>,
    <trd>aviron<klr tip="ind"> (rame)</klr></trd>
  </trdgrp>
  <trd lng="hu">evez&odblac;</trd>
  <trdgrp lng="nl">
    <trd>roeispaan</trd>,
    <trd>riem</trd>,
    <trd>roeiriem</trd>
  </trdgrp>
  <trd lng="pl">wios&lstroke;o</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">veslo</trd>
  <trd lng="vo">gub</trd>
</drv>

<drv mrk="rem.post0ilo">
  <kap>post<tld/>ilo</kap>
  <uzo tip="fak">MAR</uzo>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="rem.0ilo"><tld lit="R"/>ilo</ref>,
      kiun oni uzas per tordaj movoj &ccirc;e la malanta&ubreve;o
      de boato por &gcirc;in anta&ubreve;enigi kvaza&ubreve;
      per helico.
    </dif>
  </snc>
  <trd lng="en">scull</trd>
  <trd lng="fr">godille<klr tip="ind"> (rame)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_o;&c_v;&c_o;&c_je; &c_v;&c_je;&c_s;&c_l;&c_o;</trd>
</drv>

<drv mrk="rem.0pieda">
  <kap><tld/>pieda</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref cel="palm.0opieda" tip="dif">palmopieda</ref>
  </snc>
  <trd lng="en">oar-footed</trd>
  <trd lng="fr">palmip&egrave;de<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">&uacute;sz&oacute;l&aacute;b&uacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_p;&c_a;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ch;&c_a;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_o;&c_n;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
