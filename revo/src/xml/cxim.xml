  <art mrk="$Id: cxim.xml,v 1.5 2015/01/15 21:10:15 revo Exp $">
    <kap><rad>&ccirc;im</rad>/o</kap>

    <drv mrk="cxim.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">BIO</uzo>
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">FIZL</uzo>
          <ref tip="dif" cel="hxim.0o">&Hcirc;imo</ref>
        <rim>
      Vidu <ref cel="cxil.0portmann">la rimarkon &ccirc;e &#171;&ccirc;ilo&#187;</ref> pri
      tio, kial la formo &#171;<ref cel="hxim.0o">&hcirc;imo</ref>&#187; estas
      preferinda.
        </rim>
      </snc>
        <trd lng="be">&c_h;&c_ib;&c_m;&c_u;&c_s;</trd>
        <trd lng="bg">&c_h;&c_i;&c_m;&c_u;&c_s;</trd>
        <trd lng="ca">quim</trd>
        <trdgrp lng="de">
          <trd>Chymus</trd>,
          <trd>Nahrungsbrei</trd>,
          <trd>Speisebrei</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>chyme</trd>,
          <trd>chymus</trd>
        </trdgrp>
        <trd lng="es">quimo</trd>
        <trdgrp lng="fi">
          <trd>ruokasula</trd>
        </trdgrp>
        <trd lng="fr">chyme</trd>
        <trd lng="it">chimo</trd>
        <trdgrp lng="pl">
          <trd>chymus</trd>,
          <trd>mlecz</trd>,
          <trd>tre&sacute;&cacute; &zdot;o&lstroke;&aogonek;dka</trd>,
          <trd>tre&sacute;&cacute; pokarmowa</trd>
        </trdgrp>
        <trd lng="pt">quimo<klr tip="amb"> (fisiologia)</klr></trd>
        <trd lng="ru">&c_h;&c_i;&c_m;&c_u;&c_s;</trd>
    </drv>
  </art>

