<vortaro>
<art mrk="$Id: kitar.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kitar</rad>/o <fnt><bib>EBV</bib></fnt>
</kap>
<drv mrk="kitar.0o">
  <kap><tld/>o</kap>
  <snc mrk="kitar.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="citar.0o">citaro</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_f;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">c&iacute;tara</trd>
  <trd lng="cs">kytara</trd>
  <trdgrp lng="de">
     <trd>Kithara</trd>,
     <trd>altgriechische <ind>Zither</ind></trd>
  </trdgrp>
  <trd lng="es">c&iacute;tara</trd>
  <trd lng="fr">cithare<klr tip="ind"> (lyre antique)</klr></trd>
  <trd lng="hu">kithara</trd>
  <trd lng="nl">kithara</trd>
  <trd lng="pl">kitara</trd>
  <trd lng="pt">c&iacute;tara</trd>
  <trd lng="ru">&c_k;&c_i;&c_f;&c_a;&c_r;&c_a;</trd>
  <trd lng="sk">gitara</trd>
</drv>
</art>

