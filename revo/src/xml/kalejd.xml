<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kalejd.xml,v 1.22 2015/07/04 08:37:36 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>kalejdoskop</rad>/o
</kap>

<drv mrk="kalejd.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">SPO</uzo>
   <snc>
     <dif>
       Optika aparato, enhavanta tri spegulojn kaj multajn
       diverskolorajn
       vitropecetojn, donanta simetriajn bildojn, kiuj 
&scirc;an&gcirc;i&gcirc;as
       je &ccirc;iu moveto de la aparato.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_je;&c_j;&c_d;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="ca">calidoscopi</trd>
  <trdgrp lng="cs">
    <trd>kaleidoskop</trd>,
    <trd>krásnohled</trd>
  </trdgrp>
  <trd lng="de">Kaleidoskop</trd>
  <trd lng="en">kaleidoscope</trd>
  <trd lng="es">caleidoscopio</trd>
  <trd lng="fi">kaleidoskooppi</trd>
  <trd lng="fr">kal&eacute;idoscope</trd>
  <trd lng="hu">kaleidoszk&oacute;p</trd>
  <trd lng="nl">caleidoscoop</trd>
  <trd lng="pl">kalejdoskop</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_je;&c_j;&c_d;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="sk">kaleidoskop</trd>
</drv>

<drv mrk="kalejd.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="kalejd.0i.FIG">
     <uzo tip="stl">FIG</uzo>
     <ref tip="dif" cel="karuse.0i">karuseli</ref>
     <ekz>
       sed kian? &#8208; <tld/>is proponoj
       <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 338a</lok></fnt>.
     </ekz>
   </snc>
   <refgrp tip="vid">
     <ref cel="bunt.0a">bunta</ref>
   </refgrp>
   <trdgrp lng="ca">
     <trd>embrollar-se</trd>,
     <trd>remenar-se</trd>,
     <trd>succeir-se</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>vaihdella (kuin kaleidoskoopissa)</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>se <ind>bousculer</ind><klr tip="ind"> (fig.)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>egym&aacute;st kergetik <klr tip="ind">(&aacute;tv.)</klr></trd>,
     <trd>sz&iacute;nesen <ind>v&aacute;ltakoznak</ind></trd>
   </trdgrp>
   <trd lng="nl">door elkaar <ind>wemelen</ind></trd>
  <trd lng="sk">dívať sa do kaleidoskopu</trd>
  <trd lng="cs">dívat se do kaleidoskopu</trd>
</drv>
</art>
