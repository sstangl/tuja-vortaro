<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaligraf.xml,v 1.8 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kaligraf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kaligraf.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaligraf.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <ref tip="dif" cel="skrib.bel0isto.BELA">belskribisto</ref>
    <ekz>
      Zhong You (151&ndash;230), fama <tld/>o el la Periodo de Tri Regnoj
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 496a,
      noto</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="ca">cal&middot;l&iacute;graf</trd>
  <trdgrp lng="cs">
    <trd>kaligraf</trd>,
    <trd>krasopisec</trd>
  </trdgrp>
  <trd lng="de">Kalligraf</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_sh;&a_n;&a_w;&f_y;&a_s;</trd>,
    <trd>&a_kh;&a_T;&a_A;&a_T;</trd>
  </trdgrp>
  <trd lng="fr">calligraphe</trd>
  <trd lng="nl">kalligraaf</trd>
  <trd lng="pt">cal&iacute;grafo</trd>
  <trd lng="sk">krasopisec</trd>
</drv>
</art>

