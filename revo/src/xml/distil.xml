<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: distil.xml,v 1.23 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>distil</rad>/i
</kap>

<drv mrk="distil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="distil.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Purigi substancon vaporigante per varmo &gcirc;iajn
      vapori&gcirc;emajn
      erojn kaj poste refluecigante la vaporojn per malvarmo:
      <ekz>
        <tld/>i brandon;
      </ekz>
      <ekz>
        solvu en <ind><tld/>ita akvo</ind>
        <trdgrp lng="de">
          <trd>destilliertes Wasser</trd>
        </trdgrp>
        <trd lng="fa">&a_A_madda;&a_b; &a_m;&a_q;&a_T;&a_r;</trd>
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, kronika
        katara konjunktivito, (el la libro de profesoro E.
        Fuchs)</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_t;&c_y;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_s;&c_t;&c_i;&c_l;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>destilovat</trd>,
    <trd>překapat</trd>,
    <trd>překapávat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>destillieren</trd>,
    <trd>brennen</trd>
  </trdgrp>
  <trd lng="en">distill</trd>
  <trd lng="es">destilar</trd>
  <trd lng="fa">&a_t;&a_q;&a_T;&f_y;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">distiller<klr tip="ind"> (soumettre &agrave;
  distillation)</klr></trd>
  <trd lng="nl">distilleren</trd>
  <trd lng="pl">destylowa&cacute;</trd>
  <trd lng="pt">destilar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_t;&c_i;&c_l;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">destilova&tcaron;</trd>
</drv>

<drv mrk="distil.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="distil.0ado.KEM">
    <dif>
      Purigo per <tld/>a vaporigo kaj refluecigo:
      <ekz>
        produkti alkoholon per <ind><tld/>ado</ind>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_je;&c_s;&c_t;&c_i;&c_l;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>destilace</trd>,
    <trd>překapování</trd>,
    <trd>překapávání</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Destillation</trd>,
    <trd>Brennen</trd>
  </trdgrp>
  <trd lng="en">distillation</trd>
  <trd lng="es">destilaci&oacute;n</trd>
  <trd lng="fa">&a_t;&a_q;&a_T;&f_y;&a_r;</trd>
  <trd lng="fr">distillation</trd>
  <trd lng="nl">distillatie</trd>
  <trd lng="pl">destylacja</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_t;&c_i;&c_l;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">destilácia</trd>
</drv>

<drv mrk="distil.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="distil.0ejo.KEM">
    <dif>
      Loko, en kiu oni ordinare <tld/>as, ekzemple por produkti
      alkoholajn
      trinka&jcirc;ojn el grenoj a&ubreve; fruktoj:
      <ekz>
        la registaro permesis al <tld/>ejoj nepagon de imposto, kio
        da&ubreve;ris
        &gcirc;is la enlanda milito
        <fnt>
        <aut>Ono T.</aut>,
        <vrk>raportas pri &leftquot;moonshine&rightquot;...</vrk>,
        <lok>soc.culture.esperanto, 1998-02-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_d;&c_je;&c_s;&c_t;&c_i;&c_l;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_a; &c_i;&c_n;&c_s;&c_t;&c_a;&c_l;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_a; &c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>destilační oddělení továrny</trd>,
    <trd>palírna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brennerei</trd>,
    <trd>Destillerie</trd>
  </trdgrp>
  <trd lng="en">distillery</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_r;&a_kh;&a_A;&a_n;&a_h;&a_hamza_sure; <ind>&a_t;&a_q;&a_T;&f_y;&a_r;</ind></trd>,
    <trd>&f_k;&a_A;&a_r;&a_kh;&a_A;&a_n;&a_h;&a_hamza_sure; <ind>&a_ayn;&a_r;&a_q;&zwnj;&a_s;&a_A;&a_z;&f_y;</ind></trd>
  </trdgrp>
  <trd lng="fr">distillerie</trd>
  <trdgrp lng="nl">
    <trd>distilleerderij</trd>,
    <trd>stokerij</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_n;&c_y;&c_j;
    &c_z;&c_a;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a; &c_d;&c_l;&c_ja;
    <ind>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_k;&c_i;</ind></trd>
  </trdgrp>
  <trd lng="sk">pálenica</trd>
</drv>

<drv mrk="distil.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="distil.0ilo.KEM">
    <dif>
      Aparato por <tld/>i.
    </dif>
  </snc>
  <trd lng="cs">destilační přístroj</trd>
  <trdgrp lng="de">
    <trd>Destillationsapparat</trd>
  </trdgrp>
  <trd lng="en">still<klr tip="amb"> (distilling equipment)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_n;&a_b;&f_y;&a_q;</trd>,
    <trd>&a_d;&a_s;&a_t;&f_g;&a_A;&a_h; <ind>&a_t;&a_q;&a_T;&f_y;&a_r;</ind></trd>
  </trdgrp>
  <trd lng="fr">alambic</trd>
  <trd lng="nl">distilleertoestel</trd>
  <trd lng="pl">destylator</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_n;&c_y;&c_j;
    &c_k;&c_u;&c_b;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_t;&c_i;&c_l;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">destilačný prístroj</trd>
</drv>
</art>
