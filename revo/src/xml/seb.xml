<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
    <art mrk="$Id: seb.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
      <kap>
        <ofc>*</ofc>
        <rad>seb</rad>/o <fnt><bib>PV</bib></fnt>
      </kap>

      <drv mrk="seb.0o">
        <kap><ofc>*</ofc><tld/>o</kap>
        <snc mrk="seb.0o.KOMUNE">
	<dif>
	  Animala <ref tip="super" cel="gras.0o">graso</ref>,
	  tia kian oni fandas por fari kandelojn, sapon ktp:
	  <ekz>
	    kaj li alportu el la pacofero kiel fajroferon al la
	    Eternulo la <tld/>on, kiu kovras la interna&jcirc;ojn, kaj la tutan
            <tld/>on, kiu estas sur la interna&jcirc;oj, kaj amba&ubreve;
	    renojn, kaj la <tld/>on, kiu estas sur ili, kiu estas &ccirc;e la lumbo
	    <fnt><bib>MT</bib>, <lok>&Lev; 3:3&ndash;4.</lok></fnt>;
	  </ekz>
          <ekz>
	    <ind><tld/>a kandelo</ind>.
            <trd lng="bg">&c_l;&c_o;&c_j; &c_s;&c_v;&c_je;&c_shch;</trd>
	    <trd lng="en">tallow candle</trd>
            <trd lng="nl">vetkaars</trd>
	    <trd lng="ru">&c_s;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_v;&c_je;&c_ch;&c_a;</trd>
	  </ekz>
	</dif>
        </snc>
  <trdgrp lng="be">
	  <trd>&c_t;&c_l;&c_u;&c_sh;&c_ch;</trd>,
	  <trd>&c_z;&c_d;&c_o;&c_r;</trd>,
	  <trd>&c_l;&c_o;&c_j;</trd>
        </trdgrp>
  <trd lng="bg">&c_l;&c_o;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>loj</trd>,
    <trd>lůj</trd>,
    <trd>živočišný tuk</trd>
  </trdgrp>
  <trd lng="de">Talg</trd>
  <trd lng="en">tallow</trd>
  <trd lng="es">sebo</trd>
  <trd lng="fr">suif</trd>
  <trd lng="hu">faggy&uacute;</trd>
  <trd lng="it">sego</trd>
  <trdgrp lng="nl">
	  <trd>reuzel</trd>,
	  <trd>talg</trd>,
          <trd>smeer</trd>
        </trdgrp>
  <trdgrp lng="ru">
	<trd>&c_t;&c_o;&c_p;&c_l;&c_jo;&c_n;&c_y;&c_j; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_j; <ind>&c_zh;&c_i;&c_r;</ind></trd>,
	<trd>&c_t;&c_o;&c_p;&c_l;&c_jo;&c_n;&c_o;&c_je; <ind>&c_s;&c_a;&c_l;&c_o;</ind></trd>
	<trd>&c_t;&c_u;&c_k;<klr tip="amb"> (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>
        </trdgrp>
  <trdgrp lng="sk">
    <trd>loj</trd>,
    <trd>živočíšny tuk</trd>
  </trdgrp>
</drv>
    </art>
