<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: longan.xml,v 1.6 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>longan</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="longan.0o">
  <kap><tld/>o</kap>
  <snc mrk="longan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, malgranda ovoforma <ref tip="super"
      cel="drup.0o">drupo</ref> roz- a&ubreve; pupurkolora kun blanka, suka
      karno:
      <ekz>
        <tld/>e granda perlo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 92a, volumo 3a, p.
        178a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">longan</trd>
  <trd lng="de">Linkang-Fr&uuml;chte</trd>
  <trd lng="fr">longane</trd>
  <trd lng="hu">longan</trd>
  <trd lng="sk">longan</trd>
</drv>

<drv mrk="longan.0arbo">
  <kap><tld/>arbo,
  <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="longan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Orientazia kaj oceania arbo
      (<trd lng="la">Dimocarpus longan</trd>)
      kun man&gcirc;eblaj fruktoj:
      <ekz>
        malanta&ubreve; la lernejo fluis rivereto, sur kies
        bordoj kreskis <tld/>-arboj
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Linkang-Baum</trd>
  <trd lng="fr">longanier</trd>
  <trd lng="hu">longanfa</trd>
</drv>
</art>

