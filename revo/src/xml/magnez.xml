<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: magnez.xml,v 1.12 2015/08/21 12:10:26 revo Exp $">
<kap>
  <rad>magnez</rad>/o
</kap>
<drv mrk="magnez.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      MgO. Oksido de magnezio:
      <ekz>
        <tld/>o estas blanka, mola, tre malpeza pulvoro, uzata en
        medicino.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="magnez1.0o">magnezio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_e;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_k;&c_ib;&c_s; &c_m;&c_a;&c_g;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trd lng="de">Magnesia</trd>
  <trdgrp lng="en">
     <trd>magnesia</trd>,
     <trd>magnesium oxide</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_n;&f_y;&a_z;&f_y;</trd>
  <trd lng="fr">magn&eacute;sie</trd>
  <trd lng="hu">magn&eacute;zia</trd>
  <trd lng="nl">magnesia</trd>
  <trd lng="pt">magn&eacute;sia</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s;&c_mol; &c_m;&c_a;&c_g;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
