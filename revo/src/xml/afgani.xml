<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: afgani.xml,v 1.13 2016/03/19 12:10:21 revo Exp $">
<kap>
  <rad>afgani</rad>/o
</kap>

<drv mrk="afgani.A0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <mlg>AF</mlg>
  <snc mrk="afgani.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="afgan.A0ujo.GEOG">Afganujo</ref>:
      <ekz>
        <tld lit="A"/>o estas lando monta kaj seka inter 
        Pakistano kaj Irano en centra Azio
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Afganio">Afganio</url></vrk>,
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_w;&c_g;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_n;</trd>
    <trd lng="bg">&c_A;&c_f;&c_g;&c_a;&c_n;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>Afganistan</trd>,
      <trd>Rep&uacute;blica Isl&agrave;mica de l'Afganistan</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Afghanistan</trd>,
      <trd>Islamische Republik <ind>Afghanistan</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Afghanistan</trd>,
      <trd>Islamic Republic of <ind>Afghanistan</ind></trd>
    </trdgrp>
    <trd lng="fa">&a_A;&a_f;&a_gh;&a_A;&a_n;&a_s;&a_t;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Afghanistan</trd>,
      <trd>R&eacute;publique islamique d'Afghanistan</trd>
    </trdgrp>
    <trd lng="hu">Afganiszt&aacute;n</trd>
    <trd lng="id">Afganistan</trd>
    <trd lng="it">Afganistan</trd>
    <trdgrp lng="pl">
      <trd>Afganistan</trd>,
      <trd>Islamska Republika <ind>Afganistanu</ind></trd>
    </trdgrp>
    <trd lng="tr">Afganistan</trd>
  </snc>
</drv>

<drv mrk="afgani.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-4217">AFN</mlg>
  <snc mrk="afgani.0o.mono">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de <ref tip="vid"
      cel="afgan.A0ujo.GEOG">Afganujo</ref>:
      <ekz>
        1 afganio = 100 <ref tip="sub" cel="pul1.0o.EKON">puloj</ref>
        <fnt>
          <vrk>
            <url ref="http://www.esperanto.be/fel/jar/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>;
      </ekz>
      <ekz>
        por <tld/>o ne ekzistas oficiala deviza kurso
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Afgana_afganio">Afgana
          afganio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_a;&c_w;&c_g;&c_a;&c_n;&c_ib;</trd>
    <trd lng="ca">afgani</trd>
    <trd lng="de">Afghani</trd>
    <trd lng="fa">&a_A;&a_f;&a_gh;&a_A;&a_n;&f_y;<klr tip="ind"> (&a_w;&a_A;&a_H;&a_d;
    &f_p;&a_w;&a_l;)</klr></trd>
    <trd lng="fr">afghani</trd>
    <trd lng="id">afgani</trd>
    <trdgrp lng="pl">
      <trd>afgani</trd>
    </trdgrp>
</drv>
</art>

