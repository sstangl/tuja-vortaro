  <trdgrp lng="hu">
    <trd>baksis</trd>,
    <trd>borraval&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>baksjisj</trd>,
    <trd>fooi</trd>,
    <trd>smeergeld</trd>,
    <trd>drinkgeld</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bakszysz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_k;&c_sh;&c_i;&c_sh;</trd>,
    <trd>&c_ch;&c_a;&c_je;&c_v;&c_y;&c_je;</trd>
