<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: madagas.xml,v 1.11 2014/06/13 09:10:14 revo Exp $">
<kap>
  <rad>madagaskar</rad>/o
</kap>

<drv mrk="madagas.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="madagas.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> en
      la samnoma 
      <ref tip="lst" cel="insul.0o.GEOG"
      lst="voko:insuloj">insulo</ref>
      trovi&gcirc;anta en Hinda Oceano oriente 
      de suda parto de <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Antanarivo.
      Teritorio de Respubliko Madagaskaro.
      ISO-3166 kodo: MG.
    </dif>
    </snc>
    <trd lng="be">&c_M;&c_a;&c_d;&c_a;&c_g;&c_a;&c_s;&c_k;&c_a;&c_r;</trd>
    <trd lng="bg">&c_M;&c_a;&c_d;&c_a;&c_g;&c_a;&c_s;&c_k;&c_a;&c_r;</trd>
    <trd lng="ca">Madagascar</trd>
    <trd lng="de">Madagaskar</trd>
    <trd lng="en">Madagascar</trd>
    <trd lng="fa">&a_m;&a_A;&a_d;&a_A;&f_g;&a_A;&a_s;&f_k;&a_A;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>Madagascar</trd>,
      <trd>R&eacute;publique de Madagascar</trd>
    </trdgrp>
    <trd lng="id">Madagaskar</trd>
    <trd lng="nl">Madagascar</trd>
    <trdgrp lng="pl">
      <trd>Madagaskar</trd>,
      <trd>Republika <ind>Madagaskaru</ind></trd>,
      <trd>Republika <ind>Malgaska</ind><klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="pt">Madag&aacute;scar</trd>
</drv>

</art>

