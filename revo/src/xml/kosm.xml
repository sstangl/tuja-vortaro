<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kosm.xml,v 1.25 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kosm</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="kosm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc mrk="kosm.0o.AST">
    <dif>
      La fizika <ref tip="malprt" cel="univer.0o">universo</ref>,
      precipe
      la astroj kaj la spaco ekster la tero, kontraste al la homa <ref
      tip="vid" cel="mond.0o.tero">mondo</ref>:
      <ekz>
        <ctl>la <tld/>o kaj ni: galaksioj, planedoj kaj vivo en la
        universo</ctl>
        <fnt><bib>Kosmo</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kosmog.0o">kosmogonio</ref>,
      <ref cel="kosmog1.0o">kosmografio</ref>,
      <ref cel="kosmol.0o.SCI">kosmologio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;&c_s;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_s;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_m;&c_o;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kosmos</trd>,
    <trd>vesmír</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Weltall</trd>,
    <trd>All</trd>,
    <trd>Weltraum</trd>,
    <trd>Kosmos</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(outer) <ind>space</ind></trd>,
    <trd>universe</trd>,
    <trd>cosmos</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&f_y;&a_h;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>cosmos</trd>,
    <trd>univers</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vil&aacute;gegyetem</trd>,
    <trd>kozmosz</trd>,
    <trd>vil&aacute;g&udblac;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heelal</trd>,
    <trd>kosmos</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cosmo</trd>,
    <trd>cosmos</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kozmos</trd>,
    <trd>svet</trd>,
    <trd>vesmír</trd>
  </trdgrp>
</drv>

<drv mrk="kosm.0a">
  <kap><tld/>a</kap>
  <snc mrk="kosm.0a.AST">
    <dif>
      En la <tld/>o, al &gcirc;i rilata.
      <ekz>
        <ctl><tld/>a polico</ctl>
        <fnt><vrk>titolo de novelaro eldonita en Santander:
        Grupo Nifo, 2000</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_mol;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">vesmírný</trd>
  <trdgrp lng="de">
    <trd>kosmisch</trd>
    <trd>Weltraum-</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&f_y;&a_h;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">cosmique</trd>
  <trdgrp lng="hu">
    <trd>kozmikus</trd>,
    <trd>&udblac;rbeli</trd>
  </trdgrp>
  <trd lng="nl">kosmisch</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">vesmírny</trd>
</drv>
</art>
