<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: leksik2.xml,v 1.7 2015/09/27 15:10:15 revo Exp $">
<kap>
  <rad>leksik</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="leksik2.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="leksik2.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="vort.0oprovizo">vortoprovizo</ref>
      <ekz>
        mi opinias, ke la <tld/>o Esperanta ne estas konsekvenca
        <fnt>
          <aut>Lee Sau Dan</aut>,
          <vrk>Re: La Mandarena lingvo</vrk>,
          <lok>soc.culture.esperanto, 1996-09-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">l&egrave;xic</trd>
  <trdgrp lng="cs">
    <trd>lexikum</trd>,
    <trd>slovn&iacute; z&aacute;soba</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lexicon</trd>,
    <trd>vocabulary</trd>
  </trdgrp>
  <trd lng="es">l&eacute;xico</trd>
  <trdgrp lng="fr">
    <trd>lexique<klr tip="amb"> (d'une langue)</klr></trd>,
    <trd>vocabulaire</trd>
  </trdgrp>
  <trd lng="nl">woordenschat<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pt">
    <trd>l&eacute;xico</trd>,
    <trd>vocabul&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lexikum</trd>,
    <trd>slovn&aacute; z&aacute;soba</trd>
  </trdgrp>
</drv>
</art>

