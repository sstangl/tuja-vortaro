<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kant.xml,v 1.42 2015/07/04 14:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kant</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kant.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.0i.homo">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  <ref tip="super" cel="muzik.0i">Muziki</ref> per sia
vo&ccirc;o:
	  <ekz>
	    la &scirc;uisto <tld/>is la tutan tagon;
	  </ekz>
    <ekz>
	    <tld/>i arion;
	  </ekz>
    <ekz>
	    ofte <tld/>as la bu&scirc;o kiam ploras la
	    koro<fnt><bib>PrV</bib></fnt>
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="deklam.0i">deklami</ref>.
	</refgrp>
	<trd
lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.birdo">
    <uzo tip="klr">(pri birdoj kaj kelkaj aliaj bestoj)</uzo>
	<dif>
	  Eligi kriojn a&ubreve; sonojn harmonie modulitajn:
	  <ekz>
	    la najtingalo <tld/>as sian amdoloron<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="blek.0i">bleki</ref>,
	  <ref cel="cxirp.0i">&ccirc;irpi</ref>,
	  <ref cel="pep.0i">pepi</ref>.
	</refgrp>
	<trd
lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>parrupar</trd>,
      <trd>refilar</trd>,
      <trd>escatainar</trd>,
      <trd>escotxegar</trd>
    </trdgrp>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.FIG">
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Estigi mildan harmonian murmuron (p.p. a&jcirc;oj):
	  <ekz>
	    la akvo <tld/>as boli&gcirc;ante;
	  </ekz><ekz>
	    la rad&scirc;pinilo <tld/>is en la silenta &ccirc;ambro.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="flustr.0i">flustri</ref>,
	  <ref cel="susur.0i">susuri</ref>
	</refgrp>
	<trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>rondinejar</trd>,
      <trd>murmurar</trd>
    </trdgrp>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.lauxdi">
      <ref tip="dif" cel="kant.pri0i.MUZ">pri<tld/>i</ref>
      <ekz>
	    mi <tld/>os Vian forton<fnt>Z</fnt>;
      </ekz>
      <ekz>
	    mi <tld/>os al Vi per harpo<fnt>Z</fnt>;
      </ekz>
      <ekz>
	    Vergilio <tld/>is Eneon <klr>(abs.)</klr>;
      </ekz>
      <ekz>
	    amo kaj printempo igas la poetojn <tld/>i.
      </ekz>
    <trd lng="be">&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>lloar</trd>,
      <trd>encomiar</trd>
    </trdgrp>
	<trd lng="nl">bezingen</trd>
	<trd lng="de">besingen</trd>
	<trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">besjunga</trd>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_a;</trd>
  <trd lng="ca">cantar</trd>
  <trd lng="cs">zp&iacute;vat</trd>
  <trd lng="en">sing</trd>
  <trd lng="es">cantar</trd>
  <trdgrp lng="fr">
	<trd>chanter</trd>
  </trdgrp>
  <trd lng="hu">&eacute;nekel</trd>
  <trd lng="it">cantare</trd>
  <trd lng="no">synge</trd>
  <trd lng="oc">cantar</trd>
  <trd lng="pl">opiewa&cacute;</trd>
  <trd lng="pt">cantar</trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">spieva&tcaron;</trd>
  <trd lng="vo">kanid&ouml;n</trd>
</drv>

<drv mrk="kant.0o">
  <kap><tld/>o</kap>
  <snc mrk="kant.0o.kantado">
	<ref tip="dif" cel="kant.0ado">kantado</ref>
	<ekz>
	  la malnova <tld/>o jam forgesita en la oreloj al
	  ni resonas<fnt>Z</fnt>;
	</ekz>
	<trdgrp lng="be">
	  <trd>&c_s;&c_mol;&c_p;&c_je;&c_w;</trd>,
	  <trd>&c_s;&c_mol;&c_p;&c_je;&c_v;&c_y;</trd>
	</trdgrp>
    <trd lng="ca">cant</trd>
	<trd lng="hu">&eacute;nekl&eacute;s</trd>
	<trd lng="de">Gesang</trd>
	<trd lng="pl">&sacute;piew</trd>
    <trd lng="sv">s&aring;ng</trd>
  </snc>
  <snc mrk="kant.0o.kantajxo">
	<ref tip="dif" cel="kant.0ajxo">kanta&jcirc;o</ref>
	<ekz>
	  la malnova <tld/>o jam forgesita en la oreloj al
	  ni resonas<fnt>Z</fnt>;
	</ekz><ekz>
	  la libro de la <tld/>oj de Heine;
	</ekz><ekz>
	  la Alta <tld/>o <klr>(ama poemo el la biblio)</klr>..
	</ekz>
	<trd lng="be">&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
    <trd lng="ca">can&ccedil;&oacute;</trd>
	<trd lng="pl">pie&sacute;&nacute;</trd>
    <trd lng="sv">s&aring;ng</trd>
  </snc>
  <snc mrk="kant.0o.cxapitro">
	<uzo>POE</uzo>
	<dif>
	  <ref tip="sub" cel="cxapit.0o">&Ccirc;apitro</ref> de
	  <ref tip="malprt" cel="poem.0o.POE">poemo<sncref/></ref>:
	  <ekz>
	    Duli&hcirc;io estas insulo kiu aperas en la unua <tld/>o de
	    Odiseado de Homero, kaj plue en la na&ubreve;a <tld/>o
	    <fnt><bib>Viki</bib>,
	      <lok><url ref="http://eo.wikipedia.org/wiki/Duli%C4%A5io"
		  >Duli&hcirc;io</url></lok></fnt>.
	  </ekz>
	</dif>
    <trd lng="ca">cant</trd>
	<trd lng="pl">pie&sacute;&nacute;</trd>
  </snc>
  <trd lng="cs">p&iacute;se&ncaron;</trd>
  <trd lng="de">Lied</trd>
  <trd lng="es">canto</trd>
  <trdgrp lng="fr">
	<trd>chanson</trd>,
	<trd>chant</trd>
  </trdgrp>
  <trd lng="hu">&eacute;nek</trd>
  <trd lng="it">canto</trd>
  <trd lng="nl">lied</trd>
  <trd lng="no">sang</trd>
  <trd lng="pt">canto</trd>
  <trdgrp lng="ru">
	<trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
	<trd>&c_p;&c_je;&c_s;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pesni&ccaron;ka</trd>,
    <trd>piese&ncaron;</trd>
  </trdgrp>
  <trd lng="vo">kanid</trd>
</drv>

<drv mrk="kant.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="kant.0ado.MUZ">
    <dif>
      Homvo&ccirc;a muzikado;
      <ref tip="sin" cel="kant.0o.kantado">kanto<sncref/></ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
	<trd>&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>,
	<trd>&c_s;&c_mol;&c_p;&c_je;&c_w;</trd>,
	<trd>&c_s;&c_mol;&c_p;&c_je;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">cant</trd>
  <trdgrp lng="cs">
    <trd>p&iacute;se&ncaron;</trd>,
    <trd>zp&iacute;v&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gesang</trd>
  <trd lng="es">canto</trd>
  <trdgrp lng="fr">
    <trd>chanson</trd>,
    <trd>chant</trd>
  </trdgrp>
  <trd lng="hu">&eacute;nekl&eacute;s</trd>
  <trd lng="it">canto</trd>
  <trd lng="nl">lied</trd>
  <trd lng="no">sang</trd>
  <trd lng="pl">&sacute;piew</trd>
  <trd lng="pt">canto</trd>
  <trdgrp lng="ru">
	<trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
	<trd>&c_p;&c_je;&c_s;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">spev</trd>
  <trd lng="sv">s&aring;ng</trd>
</drv>

<drv mrk="kant.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kant.0ajxo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref>
      kun paroloj eldirataj la&ubreve; melodio;
      <ref tip="sin" cel="kant.0o.kantajxo">kanto<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="ca">can&ccedil;&oacute;</trd>
  <trd lng="cs">p&iacute;se&ncaron;</trd>
  <trdgrp lng="es">
	<trd>canci&oacute;n</trd>,
	<trd>canto</trd>
  </trdgrp>
  <trdgrp lng="fr">
	<trd>chanson</trd>,
	<trd>chant</trd>
  </trdgrp>
  <trd lng="hu">&eacute;neksz&aacute;m</trd>
  <trdgrp lng="it">
    <trd>canto</trd>, <trd>canzone</trd>
  </trdgrp>
  <trd lng="nl">lied</trd>
  <trd lng="no">sang</trd>
  <trdgrp lng="pt">
	<trd>canto</trd>,
	<trd>can&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="pl">
	<trd>pie&sacute;&nacute;</trd>,
	<trd>piosenka</trd>
  </trdgrp>
  <trdgrp lng="ru">
	<trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
	<trd>&c_p;&c_je;&c_s;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Lied</trd>
  <trd lng="sv">s&aring;ng</trd>
</drv>

<drv mrk="kant.0eti">
  <kap><tld/>eti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.0eti.MUZ">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  <tld lit="K"/>i duonvo&ccirc;e, malla&ubreve;te,
	  ne klare prononcante la silabojn.
	</dif>
  </snc>
  <refgrp tip="vid">
	<ref cel="zum.0i">zumi</ref>
  </refgrp>
  <trd lng="be">&c_n;&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>cantussejar</trd>,
    <trd>taral&middot;lejar</trd>
  </trdgrp>
  <trd lng="nl">neuri&euml;n</trd>
  <trd lng="no">nynne</trd>
  <trdgrp lng="fr">
	<trd>fredonner</trd>
  </trdgrp>
  <trd lng="es">tararear</trd>
  <trd lng="hu">d&uacute;dol</trd>
  <trd lng="it">canticchiare</trd>
  <trd lng="pl">pod&sacute;piewywa&cacute;</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">nynna</trd>
  <trd lng="sk">pospevova&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>notovat</trd>,
    <trd>pop&ecaron;vovat</trd>
  </trdgrp>
</drv>

<drv mrk="kant.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kant.0isto.profesio">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  <ref tip="super" cel="muzik.0isto">Muzikisto</ref>
	  kiu vo&ccirc;e plenumas melodiojn.
	</dif>
	<trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>cantant</trd>,
      <trd>cantor</trd>
    </trdgrp>
	<trd lng="hu">&eacute;nekes</trd>
	<trd lng="nl">zanger</trd>
    <trd lng="no">sanger</trd>
	<trd lng="de">S&auml;nger</trd>
	<trdgrp lng="pl">
	  <trd>&sacute;piewak</trd>,
	  <trd>pie&sacute;niarz</trd>,
	  <trd>piosenkarz</trd>
	</trdgrp>
    <trd lng="sv">s&aring;ngare</trd>
  </snc>
  <snc mrk="kant.0isto.poeto">
	<dif>
	  Poeto.
	</dif>
	<trd lng="be">&c_p;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_r;</trd>
    <trdgrp lng="ca">
      <trd>bard</trd>,
      <trd>poeta</trd>
    </trdgrp>
	<trdgrp lng="pl">
	  <trd>pie&sacute;niarz</trd>,
	  <trd>bard</trd>
	</trdgrp>
  </snc>
  <trd lng="cs">zp&ecaron;v&aacute;k</trd>
  <trdgrp lng="fr">
	<trd>chanteur</trd>
  </trdgrp>
  <trd lng="hu">dalnok</trd>
  <trdgrp lng="it">
    <trd>cantante</trd>, <trd>cantore</trd>
  </trdgrp>
  <trd lng="pt">cantor</trd>
  <trd lng="ru">&c_p;&c_je;&c_v;&c_je;&c_c;</trd>
  <trd lng="sk">spev&aacute;k</trd>
</drv>

<drv mrk="kant.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
	<dif>
	  <tld lit="K"/>i anta&ubreve; publiko:
	  <ekz>
	    Mirjam anta&ubreve;<tld/>is al ili
	    <fnt><bib>MT</bib><lok>&Eli; 15:21</lok></fnt>.
	  </ekz>
	</dif>
  </snc>
  <trd lng="ca">cantar en p&uacute;blic</trd>
  <trd lng="fr">chanter<klr tip="ind"> (devant un
public)</klr></trd>
  <trd lng="de">vorsingen</trd>
</drv>

<drv mrk="kant.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
	<dif>
	  Komenci <tld/>i.
	</dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cantar</trd>,
    <trd>entonar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t se do zp&ecaron;vu</trd>,
    <trd>zazp&iacute;vat</trd>
  </trdgrp>
  <trd lng="fr">entonner<klr tip="ind"> (un chant)</klr></trd>
  <trd lng="it"><ind>intonare</ind> un canto</trd>
  <trd lng="pl">za&sacute;piewa&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">da&tcaron; sa do spevu</trd>
</drv>

<drv mrk="kant.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.pri0i.MUZ">
	<dif>
	  <ref tip="super" cel="glor.0i">Glori</ref> per kanto
	  a&ubreve; per versoj; versfaradi honore al iu a&ubreve; io;
    <ref tip="sin" cel="kant.0i.lauxdi">kanti<sncref/></ref>
	</dif>
    <refgrp tip="vid">
      <ref cel="lauxd.0i">la&ubreve;di</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>lloar</trd>,
    <trd>fer cants</trd>
  </trdgrp>
  <trd lng="cs">op&ecaron;vovat</trd>
  <trd lng="de">besingen</trd>
  <trdgrp lng="fr">
	<trd>chanter<klr> (louer)</klr></trd>
  </trdgrp>
  <trd lng="hu">meg&eacute;nekel</trd>
  <trdgrp lng="it">
    <trd>cantare</trd>, <trd>decantare</trd>
  </trdgrp>
  <trd lng="nl">bezingen</trd>
  <trd lng="pl">opiewa&cacute;</trd>
  <trd lng="pt">cantar</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">ospevova&tcaron;</trd>
  <trd lng="sv">besjunga</trd>
</drv>

<drv mrk="kant.re0ajxo">
  <kap>re<tld/>a&jcirc;o</kap>
  <snc mrk="kant.re0ajxo.MUZ">
	<uzo tip="fak">MUZ</uzo>
	<dif>
	  Parto de <ref tip="malprt"
cel="kant.0ajxo"><tld/>a&jcirc;o</ref>,
	  kiun oni re<tld/>as post &ccirc;iu strofo.
	</dif>
  </snc>
  <trdgrp lng="be">
	<trd>&c_p;&c_r;&c_y;&c_p;&c_je;&c_w;</trd>,
	<trd>&c_r;&c_e;&c_f;&c_r;&c_e;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tornada</trd>,
    <trd>recoble</trd>
  </trdgrp>
  <trdgrp lng="fr">
	<trd>refrain</trd>
  </trdgrp>
  <trd lng="hu">refr&eacute;n</trd>
  <trd lng="it">ritornello</trd>
  <trd lng="nl">refrein</trd>
  <trd lng="no">refregn</trd>
  <trd lng="pl">refren</trd>
  <trdgrp lng="pt">
	<trd>refr&atilde;o</trd>,
	<trd>estribilho</trd>
  </trdgrp>
  <trdgrp lng="ru">
	<trd>&c_p;&c_r;&c_i;&c_p;&c_je;&c_v;</trd>,
	<trd>&c_r;&c_je;&c_f;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">refr&auml;ng</trd>
</drv>

<drv mrk="kant.am0o">
  <kap>am<tld/>o</kap>
  <snc mrk="kant.am0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="K"/>o esprimanta sentojn de amantoj:
      <ekz>
        am<tld/>oj kortu&scirc;as min
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="https://glosbe.com/it/eo/canzone">
          canzone</url></vrk>,
          <lok>glosbe.com, [vidita en 2014]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">milostn&aacute; p&iacute;se&ncaron;</trd>
  <trd lng="fr">chanson d'amour</trd>
  <trd lng="sk">&lcaron;&uacute;bostn&aacute; piese&ncaron;</trd>
  <trd lng="zh">&#24773;&#27468; [<ind>q&iacute;ng g&#275;</ind>]</trd>
</drv>

<drv mrk="kant.glor0o">
  <kap>glor<tld/>o,
  <var><kap>la&ubreve;d<tld/>o</kap></var></kap>
  <snc mrk="kant.glor0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="K"/>o kiu la&ubreve;das, glorigas iun a&ubreve; ion:
      <ekz>
        oni komponis belegan glor<tld/>on kaj vespere 
        <klr>...</klr> la koruso grupi&gcirc;is kaj ekkantis
        <fnt>
          <aut>I. Franko, trad. V. Pajuk</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tl-ukr01.htm">
          La farbita vulpo</url></vrk>,
          <lok>La Ondo de Esperanto, 2006 (8&ndash;9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">chvalozp&ecaron;v</trd>
  <trdgrp lng="fr">
    <trd>chant de louange</trd>,
    <trd>gloria</trd>
  </trdgrp>
  <trd lng="sk">chv&aacute;lospev</trd>
</drv>

<drv mrk="kant.lul0o">
  <kap>lul<tld/>o</kap>
  <snc mrk="kant.lul0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Murmura <tld/>o por dormigi infanon, kvietigi doloron.
    </dif>    
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_y;&c_h;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">luskellerez</trd>
  <trdgrp lng="ca">
    <trd>can&ccedil;&oacute; de <ind>bressol</ind></trd>,
    <trd>non-non</trd>
  </trdgrp>
  <trd lng="cs">ukol&eacute;bavka</trd>
  <trd lng="de">Wiegenlied</trd>
  <trd lng="es">nana</trd>
  <trd lng="fr">berceuse</trd>
  <trdgrp lng="hu">
    <trd>b&ouml;lcs&odblac;dal</trd>,
    <trd>altat&oacute;dal</trd>
  </trdgrp>
  <trd lng="it">ninna nanna</trd>
  <trd lng="nl">wiegelied</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_y;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; <klr>(&c_p;&c_je;&c_s;&c_n;&c_ja;)</klr></trd>
  <trd lng="sk">usp&aacute;vanka</trd>
</drv>

<drv mrk="kant.marsx0o">
  <kap>mar&scirc;<tld/>o</kap>
  <snc mrk="kant.marsx0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ritma <tld/>o, uzata dum takta irado, mar&scirc;ado:
      <ekz>
        ni kantu <klr>...</klr> imponan mar&scirc;<tld/>on
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">marche<klr> (chanson)</klr></trd>
</drv>

<drv mrk="kant.mod0o">
  <kap>mod<tld/>o<fnt><bib>HejmVort</bib></fnt>,
  <var><kap>furor<tld/>o<fnt><bib>HejmVort</bib></fnt></kap></var></kap>
  <snc mrk="kant.mod0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Dumtempe tre populara <tld/>o;
      <ref tip="sin" cel="sxlagr.0o">&scirc;lagro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
	<trd>&c_h;&c_ib;&c_t;</trd>,
	<trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>&egrave;xit</trd>,
    <trd>can&ccedil;&oacute; de moda</trd>
  </trdgrp>
  <trd lng="en">hit<klr tip="ind"> (song)</klr></trd>
  <trdgrp lng="de">
	<trd>Schlager</trd>,
	<trd>Hit</trd>
  </trdgrp>
  <trdgrp lng="es">
	<trd>&eacute;xito<klr tip="ind"> (canci&oacute;n)</klr></trd>,
	<trd>hit</trd>
  </trdgrp>
  <trd lng="fi">hitti</trd>
  <trdgrp lng="fr">
	<trd>chanson &agrave; succ&egrave;s</trd>,
	<trd>hit<klr tip="ind"> (chanson)</klr></trd>,
	<trd>succ&egrave;s<klr tip="ind"> (chanson)</klr></trd>,
	<trd>tube<klr tip="ind"> (chanson)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
	<trd>sl&aacute;ger</trd>,
	<trd>sikersz&aacute;m</trd>,
	<trd>hit</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><ind>canzone</ind> di successo</trd>, <trd>hit</trd>
  </trdgrp>
  <trdgrp lng="nl">
	<trd>schlager</trd>,
	<trd>hit</trd>
  </trdgrp>
  <trdgrp lng="pl">
	<trd>przeb&oacute;j</trd>,
	<trd>hit</trd>,
	<trd>szlagier</trd>
  </trdgrp>
  <trdgrp lng="pt">
	<trd>top</trd>,
	<trd>sucesso da temporada</trd>
  </trdgrp>
  <trdgrp lng="sv">
	<trd>schlager</trd>,
	<trd>hit</trd>
  </trdgrp>
  <trdgrp lng="ru">
	<trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
	<trd>&c_h;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kant.mok0o">
  <kap>mok<tld/>o</kap>
  <snc mrk="kant.mok0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="K"/>o moka al iu:
      <ekz>
        iu petolulo verkis pri tiu familio longan 
        mok<tld/>on
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">posm&ecaron;&scaron;n&aacute; p&iacute;sni&ccaron;ka</trd>
  <trd lng="fr">chanson satirique</trd>
  <trd lng="sk">posme&scaron;n&aacute; pesni&ccaron;ka</trd>
</drv>

<drv mrk="kant.venko0o">
  <kap>venko<tld/>o,
  <var><kap>triumfo<tld/>o</kap></var></kap>
  <snc mrk="kant.venko0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="K"/>o esprimanta la &gcirc;ojon de venkanto:
      <ekz>
        Nun silentu, vantaj venko<tld/>oj, /
        Nun parolu faktoj kaj praktiko! 
        <fnt>
          <aut>R. Schwartz</aut>,
          <vrk><url
          ref="http://www.steloj.de/esperanto/butiko/horodelprogreso.html">
          La Horo de l' Progreso</url></vrk>,
          <lok>La Strnaga Butiko, 1931</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&iacute;se&ncaron; v&iacute;t&ecaron;zstv&iacute;</trd>
  <trdgrp lng="fr">
    <trd>chant de victoire</trd>,
    <trd>p&eacute;an</trd>
  </trdgrp>
  <trd lng="sk">piese&ncaron; v&iacute;&tcaron;azstva</trd>
</drv>
