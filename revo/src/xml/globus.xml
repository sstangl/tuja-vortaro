<vortaro>
  <art mrk="$Id: globus.xml,v 1.13 2015/07/04 08:37:35 revo Exp $">
    <kap>
      <rad>globus</rad>/o<fnt>Z</fnt>
    </kap>
    <drv mrk="globus.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
        <dif>
          Sfera mondmapo, malgrandigita modelo de la terglobo,
          ofte muntita tiel ke &gcirc;i estas turnebla sur sia
          apogilo.
        </dif>
      </snc>
  <trd lng="be">&c_g;&c_l;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="bg">&c_g;&c_l;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="ca">globus terraq&uuml;i</trd>
  <trd lng="cs">glóbus</trd>
  <trd lng="de">Globus</trd>
  <trd lng="en">globe</trd>
  <trd lng="es">globo</trd>
  <trd lng="fa">&f_k;&a_r;&a_h;&a_hamza_sure; &a_j;&a_gh;&a_r;&a_A;&a_f;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">globe<klr> (terrestre)</klr></trd>
  <trd lng="hu">f&ouml;ldg&ouml;mb<klr tip="ind"> (t&eacute;rk&eacute;p)</klr></trd>
  <trd lng="nl">wereldbol</trd>
  <trd lng="pt">globo terrestre</trd>
  <trd lng="ru">&c_g;&c_l;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="sk">glóbus</trd>
</drv>
  </art>

