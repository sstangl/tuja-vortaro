<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suspensi.xml,v 1.10 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>suspensi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suspensi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="sxveb.0o.KOMUNE">&scirc;vebo <sncref/></ref>
    <ekz>
      la poleno <klr>...</klr> kaptas mineralajn molekulojn
      <klr>...</klr>, kiuj estas en <tld/>o en la aero
      <fnt>
      <aut>Robert Roze</aut>,
      <vrk>Vivisekcio de polenero</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 1997a, numero 11a, p. 12a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_s;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_p;&c_e;&c_n;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">suspensi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>pérování</trd>,
    <trd>zavěšení</trd>,
    <trd>závěs</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flottement<klr> (chimie)</klr></trd>,
    <trd>suspension<klr> (chimie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szuszpenzi&oacute;</trd>,
    <trd>lebeg&eacute;s<klr> (k&eacute;miai)</klr></trd>
  </trdgrp>
  <trd lng="it">sospensione<klr> (ch.)</klr></trd>
  <trd lng="nl">suspensie</trd>
  <trd lng="no">suspensjon</trd>
  <trdgrp lng="pl">
    <trd>zawieszenie</trd>,
    <trd>zwis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_v;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_p;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pérovanie</trd>,
    <trd>zavesenie</trd>,
    <trd>záves</trd>
  </trdgrp>
</drv>
</art>

