<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekund.xml,v 1.28 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sekund</rad>/o
</kap>
<drv mrk="sekund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="sekund.0o.tempo">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">FIZ</uzo>
    <mlg kod="SI">s</mlg>
    <dif>
      Baza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:da&ubreve;roj">mezurunuo</ref> de
      <ref cel="temp.0o.FIZ">tempo</ref> en la Internacia Sistemo de
      unuoj:
      <ekz>
        unu <tld/>o egalas al 31 556 925,9747-ono de iu tropika jaro;
      </ekz>
      <ekz>
        <tld/>o estas sesdekono de <ref
        cel="minut.0o.tempo">minuto</ref>.
      </ekz>
    </dif>
    <trd lng="de">Sekunde</trd>
    <trd lng="hu">m&aacute;sodperc</trd>
    <trdgrp lng="id">
      <trd>detik</trd>,
      <trd>sekon</trd>
    </trdgrp>
    <adm>
      Restas por difini ie, kio estas tropika jaro... <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="sekund.0o.angulo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="angul.0o.MAT">angulo</ref>, egala al 3600-ono
      de <ref tip="malprt" cel="grad.0o.unuo">grado</ref>.
    </dif>
    <trd lng="de">Bogensekunde</trd>
    <trd lng="hu">sz&ouml;gm&aacute;sodperc</trd>
    <trd lng="id">detik</trd>
    <trd lng="pl">sekunda</trd>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
  <trd lng="bg">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
  <trd lng="ca">segon</trd>
  <trdgrp lng="cs">
    <trd>sekunda</trd>,
    <trd>vte&rcaron;ina</trd>,
    <trd>vteřina</trd>
  </trdgrp>
  <trd lng="en">second</trd>
  <trd lng="es">segundo</trd>
  <trd lng="fr">seconde<klr tip="ind"> (unit&eacute; de temps,
  d'angle)</klr></trd>
  <trd lng="it">secondo</trd>
  <trd lng="nl">seconde</trd>
  <trd lng="pl">sekunda</trd>
  <trd lng="ro">secund&abreve;</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">sekunda</trd>
  <trd lng="zh">秒 [<ind>miǎo</ind>]</trd>
</drv>
</art>
