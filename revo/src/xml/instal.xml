  <trdgrp lng="hu">
    <trd>felsz&aacute;mol <klr tip="amb">(l&eacute;tes&iacute;tm&eacute;nyt)></klr></trd>,
    <trd>leszerel</trd>,
    <trd>elt&aacute;vol&iacute;t</trd>
  </trdgrp>
  <trd lng="pt">desinstalar</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_je;&c_i;&c_n;&c_s;&c_t;&c_a;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odinštalovať</trd>
</drv>

<drv mrk="instal.mal0ilo">
  <kap>mal<tld/>ilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Programo por forigi iujn <tld/>itajn pecojn de programaro.
    </dif>
  </snc>
  <trd lng="cs">odinstalační program</trd>
  <trd lng="de">Deinstallationsprogramm</trd>
  <trd lng="en">uninstaller</trd>
  <trdgrp lng="es">
    <trd>programa de desinstalaci&oacute;n</trd>,
    <trd>desinstalador</trd>
