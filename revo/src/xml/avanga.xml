<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: avanga.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>avangard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="avanga.0o">
  <kap><tld/>o</kap>
  <snc mrk="avanga.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ta&ccirc;mento, kiu esploras la terenon anta&ubreve; iranta armeo;
      <ref tip="sin" cel="gvardi.antaux0o.MIL">anta&ubreve;gvardio</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Vortrupp</trd>,
      <trd>Vorausabteilung</trd>,
      <trd>Sp&auml;her</trd>,
      <trd>Sp&auml;habteilung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>scout party</trd>,
      <trd>vanguard</trd>
    </trdgrp>
    <trd lng="hu">el&odblac;v&eacute;d</trd>
  </snc>
  <snc mrk="avanga.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Anta&ubreve;irantoj, &ccirc;efe en la arta a&ubreve; te&hcirc;nika kampoj:
      <ekz>
        li apartenas al la <tld/>o de kinoarto;
      </ekz>
      <ekz>
        <tld/>a partio;
      </ekz>
      <ekz>
        <tld/>aj literaturistoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Avantgarde</trd>,
      <trd>Vortrupp</trd>,
      <trd>Trendsetter</trd>
    </trdgrp>
    <trd lng="en">avant-garde</trd>
    <trdgrp lng="hu">
      <trd>&eacute;lcsapat</trd>,
      <trd>avantg&aacute;rd</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
  <trd lng="ca">avantguarda</trd>
  <trdgrp lng="cs">
    <trd>přední voj</trd>,
    <trd>předvoj</trd>,
    <trd>vedoucí skupina</trd>
  </trdgrp>
  <trd lng="es">vanguardia</trd>
  <trd lng="fr">avant-garde</trd>
  <trd lng="it">avanguardia</trd>
  <trd lng="ru">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>1.predvoj</trd>,
    <trd>2.skupina priekopníkov</trd>,
    <trd>avantgarda</trd>
  </trdgrp>
</drv>
</art>
