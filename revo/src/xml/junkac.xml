<vortaro>
<art mrk="$Id: junkac.xml,v 1.12 2015/10/09 14:10:20 revo Exp $">
<kap>
  <rad>junkac</rad>/oj
</kap>
<drv mrk="junkac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Juncaceae</trd>), herbsimilaj, kun rondaj,
      rektaj kaj flekseblaj trunketoj.
    </dif>
  </snc>
  <trd lng="de">Binsengew&auml;chse</trd>
  <trd lng="es">junc&aacute;ceas</trd>
  <trdgrp lng="en">
    <trd>Juncaceae</trd>,
    <trd>rush family</trd>, <trd>rushes</trd>
  </trdgrp>
  <trd lng="fr">juncac&eacute;es</trd>
  <trd lng="hu">szitty&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="it">giuncacee</trd>
  <trd lng="nl">russenfamilie</trd>
  <trd lng="pl">sitowate</trd>
  <trd lng="pt">junc&aacute;ceas</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; 
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
