<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stefan.xml,v 1.11 2015/11/03 06:10:15 revo Exp $">
<kap>
  <ofc>*</ofc><rad>Stefan</rad>/o
</kap>
<drv mrk="stefan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        mia frato diris al <tld/>o, ke li amas lin pli, ol sin mem
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>     
  <trd lng="br">Stefan</trd>
  <trdgrp lng="en">
    <trd>Stephen</trd>,
    <trd>Steven</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&Eacute;tienne</trd>,
    <trd>St&eacute;phane</trd>
  </trdgrp>
  <trd lng="hu">Istv&aacute;n</trd>
  <trd lng="it">Stefano</trd>
  <trd lng="pl">Stefan</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_t;&c_je;&c_p;&c_a;&c_n;</trd>,
    <trd>&c_S;&c_t;&c_je;&c_f;&c_a;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_E;&c_t;&c_mol;&c_je;&c_n;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="stefan.in0o">
  <kap><tld/>ino</kap>
  <snc>
  <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena, kun la
      signifo de "la kronitino".
    </dif>
  </snc>
  <trd lng="en">Stephanie</trd>
  <trd lng="fr">St&eacute;phanie</trd>
  <trd lng="pl">Stefania</trd>
</drv>
</art>

