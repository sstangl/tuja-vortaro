<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: primus.xml,v 1.10 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>primus</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="primus.0o">
  <kap><tld/>o</kap>
  <snc mrk="primus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Eta, portebla <ref tip="super"
      cel="masxin.kuir0o">kuirma&scirc;ino</ref>,
      pliofte kun propra rezervo de bruli&gcirc;a&jcirc;o:
      <ekz>
        mi metos la tekru&ccirc;on sur la <tld/>on kaj ni
        preparos la teon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_u;&c_s;</trd>
  <trd lng="cs">petrolejový vařič</trd>
  <trd lng="fa">&f_p;&a_r;&f_y;&a_m;&a_w;&a_s;</trd>
  <trd lng="fr">r&eacute;chaud</trd>
  <trdgrp lng="hu">
    <trd>gyorsf&odblac;z&odblac;</trd>,
    <trd>g&aacute;zrezs&oacute;</trd>,
    <trd>spirituszf&odblac;z&odblac;</trd>,
    <trd>primusz</trd>
  </trdgrp>
  <trd lng="nl">komfoor</trd>
  <trdgrp lng="pl">
    <trd>prymus</trd>,
    <trd>kuchenka turystyczna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fogareiro</trd>,
    <trd>r&eacute;chaud</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_m;&c_u;&c_s;</trd>
  <trd lng="sk">petrolejový varič</trd>
</drv>
</art>

