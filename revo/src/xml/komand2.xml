<kap>
  <rad>komandor</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="komand2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tiu, kiu havas, en <ref cel="orden.0o.honora">ordeno</ref>, 
      rangon superan je tiu de
      <ref cel="oficir.0o.en_ordeno">oficiro</ref>:
      <ekz>
        <tld/>o de la Honora legio, de la Malta ordeno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kavali.0o.en_ordeno">kavaliro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_o;&c_r;</trd>
  <trd lng="cs">komandér řádu</trd>
  <trd lng="en">commander</trd>
  <trd lng="es">comandante</trd>
  <trd lng="fr">commandeur</trd>
  <trd lng="nl">commandeur</trd>
  <trd lng="pt">comendador</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_o;&c_r;</trd>
  <trd lng="sk">komandér rádu</trd>
</drv>
</art>
