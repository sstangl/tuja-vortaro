<vortaro>
<art mrk="$Id: aventu1.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>aventurin</rad>/o
</kap>
<drv mrk="aventu1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxton.juvel0o">Juvel&scirc;tono</ref> el kvarco
      entenanta malgrandajn orbrilajn pecetojn:
      <ekz>mi a&ubreve;dis, ke <klr>...</klr> oni nomas specon de brile
      glimanta gemo <tld/>o
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
      373a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_n;&c_t;&c_u;&c_r;&c_y;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>aventurin</trd>,
    <trd>leskovec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aventurin</trd>,
    <trd>Glimmerquarz</trd>
  </trdgrp>
  <trd lng="en">aventurine</trd>
  <trd lng="fr">aventurine</trd>
  <trdgrp lng="it">
    <trd>avventurina<klr> (min.)</klr></trd>,
    <trd>venturina</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_a;&c_n;&c_t;&c_ju;&c_r;&c_i;&c_n;</trd>
  <trd lng="sk">aventurínové sklo</trd>
</drv>
</art>
