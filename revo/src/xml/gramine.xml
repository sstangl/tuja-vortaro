  <trd lng="ca">gram&iacute;nies</trd>
  <trdgrp lng="cs">
    <trd>traviny</trd>,
    <trd>trávy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gramineen</trd>,
    <trd>Gr&auml;ser</trd>
