  (fam.)</klr></trd>
  <trd lng="pt">gram&iacute;neas</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>traviny</trd>,
    <trd>trávy</trd>
  </trdgrp>
</drv>
</art>
