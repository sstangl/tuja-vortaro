<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kabuk.xml,v 1.12 2015/07/04 08:37:36 revo Exp $">
<kap>
   <rad>kabuk</rad>/o
</kap>
<drv mrk="kabuk.0o">
   <kap><tld/>o</kap> <fnt><bib>EncJap</bib></fnt>
   <uzo tip="fak">TEA</uzo>
   <snc>
     <dif>
       Japana tradicia <ref tip="super"
       cel="teatr.0o.belarto">teatro</ref>
       ludata de viraj roluloj kun konvenciaj &scirc;minka&jcirc;oj kaj
       pozoj, kun akompanaj &ccirc;anto kaj <ref tip="vid"
       cel="sxamisen.0o">&scirc;amisena</ref> muziko.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_u;&c_k;&c_ib; 
<klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib; 
&c_t;&c_e;&c_a;&c_t;&c_r;)</klr></trd>
  <trd lng="ca">kabuki</trd>
  <trd lng="cs">kabuki</trd>
  <trd lng="de">Kabuki-Theater</trd>
  <trd lng="fi">kabuki(-teatteri)</trd>
  <trdgrp lng="fr">
     <trd>kabuki</trd>
   </trdgrp>
  <trd lng="he"><klr tip="amb">(&tav;&yod;&alef;&tet;&resh;&vav;&fnun;) 
</klr>&qof;&bet;&vav;&qof;&yod;</trd>
  <trd lng="hu">kabuki <klr tip="amb">(sz&iacute;nh&aacute;z)</klr></trd>
  <trd lng="nl">kabuki</trd>
  <trdgrp lng="pt">
     <trd>cab&uacute;qui</trd>,
     <trd>kabuki</trd>
   </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_b;&c_u;&c_k;&c_i;</trd>
  <trd lng="sk">kabuki</trd>
</drv>
</art>
