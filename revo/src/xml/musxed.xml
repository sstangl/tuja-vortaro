<vortaro>
<art mrk="$Id: musxed.xml,v 1.8 2015/10/15 12:10:20 revo Exp $">
<kap>
  <rad>mu&scirc;ed</rad>/oj
</kap>
<drv mrk="musxed.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Muscidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_h;&c_ib; &c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Echte <ind>Fliegen</ind></trd>
  <trd lng="en">Muscidae <klr>(includes house flies)</klr></trd>
  <trd lng="hu">igazi <ind>legyek</ind></trd>
  <trd lng="nl">echte <ind>vliegen</ind></trd>
  <trd lng="pt">d&iacute;pteros <klr tip="amb">(zool.)</klr></trd>
  <trd lng="ru"><ind>&c_m;&c_u;&c_h;&c_i;</ind> &c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
