<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bandag.xml,v 1.17 2013/08/18 18:10:11 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>banda&gcirc;</rad>/o
</kap>
<drv mrk="bandag.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Longa tola strio por &ccirc;irka&ubreve;volvi a&ubreve; senmovigi
       vunditan korpoparton:
       <ekz>
         hernia <tld/>o.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="zon.0o">zono</ref>,
       <ref cel="bend.0o">bendo</ref>,
       <ref cel="lig">&ccirc;irka&ubreve;liga&jcirc;o</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
    <trd>&c_b;&c_ib;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>
  </trdgrp>
   <trdgrp lng="de"><trd>Verband</trd>, <trd>Binde</trd></trdgrp>
   <trd lng="en">bandage</trd>
   <trdgrp lng="es">
     <trd>venda</trd>,
     <trd>vendaje</trd>
   </trdgrp>
   <trdgrp lng="fa">
     <trd>&a_n;&a_w;&a_A;&a_r; &a_z;&a_kh;&a_m;&zwnj;&a_b;&a_n;&a_d;&f_y;</trd>,
     <trd>&a_t;&a_n;&a_z;&f_y;&a_b;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>bande<klr tip="ind"> (bandage)</klr></trd>,
     <trd>bandage</trd></trdgrp>
   <trdgrp lng="hu">
     <trd>k&ouml;t&eacute;s<klr tip="ind"> (seben)</klr></trd>,
     <trd>k&ouml;tszer</trd></trdgrp>
    <trdgrp lng="it">
     <trd>benda<klr> (bendaggio)</klr></trd>,
     <trd>fascia<klr> (bendaggio)</klr></trd>,
     <trd>bendaggio<klr> (fascia, benda)</klr></trd></trdgrp>
   <trdgrp lng="nl">
    <trd>verband</trd>,
    <trd>bandage</trd>
  </trdgrp>
  <trd lng="pl">banda&zdot;</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
     <trd>&c_b;&c_i;&c_n;&c_t;</trd>
   </trdgrp>
  <trd lng="sk">obv&auml;z</trd>
</drv>
<drv mrk="bandag.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Garni per <tld/>o:
       <ekz>
         <tld/>i vundojn
         <fnt><bib>NT</bib>, <lok>Luko 10:34</lok></fnt>;
       </ekz>
       <ekz>
         <tld/>i vunditon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>i la piedon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         Sara aperis kun <tld/>ita kapo
         <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 10</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="vind.0i">vindi</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
   <trd lng="de">verbinden</trd>
   <trd lng="en">bandage</trd>
   <trd lng="es">vendar</trd>
   <trd lng="fa">&a_b;&a_A;&a_n;&a_d;&f_p;&f_y;&f_ch;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
   <trd lng="fr">bander<klr tip="ind"> (avec une bande)</klr></trd>
   <trd lng="hu">bek&ouml;t&ouml;z<klr tip="ind"> (sebet)</klr></trd>
   <trdgrp lng="it">
     <trd>bendare</trd>,
     <trd>fasciare<klr> (bendare)</klr></trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>verbinden<klr tip="ind"> (omzwachtelen)</klr></trd>,
     <trd>omzwachtelen</trd>,
     <trd>omwikkelen</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>banda&zdot;owa&cacute;</trd>,
     <trd>obanda&zdot;owywa&cacute;</trd>,
     <trd>obanda&zdot;owa&cacute;</trd>,
     <trd>zabanda&zdot;owywa&cacute;</trd>,
     <trd>zabanda&zdot;owa&cacute;</trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
   <trd lng="sk">obviaza&tcaron;</trd>
</drv>
</art>
