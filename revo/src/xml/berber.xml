<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: berber.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>berberis</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="berber.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>acoj 
      (<trd lng="la">Berberis</trd>),
      arbusto kun ru&gcirc;aj, agrable acidetaj beretoj.
    </dif>
    <adm>
      Aran&gcirc;i la difinon alie, ke eblus doni tradukojn
      alilingvajn por "~acoj". (MB)
    </adm>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_y;&c_s;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l; &c_t;&c_r;&c_malmol;&c_n;</trd>
  <trd lng="cs">dřišťál obecný <klr>(dráč)</klr></trd>
  <trdgrp lng="de">
    <trd>Sauerdorn</trd>,
    <trd>Berberitze</trd>
  </trdgrp>
  <trd lng="en">berberis</trd>
  <trd lng="fa">&a_z;&a_r;&a_sh;&f_k;</trd>
  <trd lng="fr">&eacute;pine-vinette</trd>
  <trd lng="hu">borbolya</trd>
  <trd lng="pl">berberys</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">dráč <klr>(ker)</klr></trd>
  <trd lng="sv">berberis</trd>
</drv>
</art>
