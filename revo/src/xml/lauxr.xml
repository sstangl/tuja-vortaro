<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: lauxr.xml,v 1.26 2013/08/27 10:10:24 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>la&ubreve;r</rad>/o
    </kap>

    <drv mrk="lauxr.0o">
      <kap>
        <ofc>*</ofc><tld/>o
      </kap>
      <snc mrk="lauxr.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <dif>
          <ref tip="lst" cel="genr.0o.BIO"
          lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
          tip="malprt"
          cel="lauxrac.0oj"><tld/>acoj</ref>
          (<trd lng="la">Laurus</trd>), kreskanta en mediteranea
          regiono, &ccirc;iamverda arbo kun ledaj folioj kaj blunigraj 
          beroj, en la antikva tempo uzata por kroni venkintojn.
        </dif>
      </snc>
      <trdgrp lng="br">
        <trd>loreenn</trd>,
        <trd>gwezenn-lore</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>g&egrave;nere Laurus</trd>,
        <trd>llorer</trd>
      </trdgrp>
      <trd lng="de">Lorbeerbaum</trd>
      <trdgrp lng="en">
        <trd>laurel</trd>,
        <trd>bay tree</trd>
      </trdgrp>
      <trd lng="es">laurel</trd>
      <trd lng="fr">laurier</trd>
      <trd lng="hu">bab&eacute;rfa</trd>
      <trd lng="nl">laurier</trd>
      <trd lng="pl">laur</trd>
      <trd lng="pt">louro<klr tip="amb"> (subst., bot.)</klr></trd>
      <trd lng="be">&c_l;&c_a;&c_w;&c_r;</trd>
      <trd lng="ru">&c_l;&c_a;&c_v;&c_r;</trd>
    </drv>

    <drv mrk="lauxr.0oj">
      <kap>
        <tld/>oj
      </kap>
      <snc mrk="lauxr.0oj.BOT">
        <uzo tip="fak">BOT</uzo>
        <dif>
          Plurnombro de &leftquot;<tld/>o&rightquot;.
        </dif>
        <trdgrp lng="br">
          <trd>lore</trd>,
          <trd>gwez-lore</trd>
        </trdgrp>
        <trd lng="nl">lauweren</trd>
        <trd lng="pl">laury</trd>
      </snc>
      <snc mrk="lauxr.0oj.gloro">
        <uzo tip="stl">FIG</uzo>
        <ref tip="dif" cel="lauxr.0okrono"><tld/>krono</ref>
        <ekz>
          la <tld/>oj de la gloro;
        </ekz><ekz>
          <tld/>e kroni triumfinton;
        </ekz><ekz>
          rikolti <tld/>ojn <klr>(multe sukcesi)</klr>.
        </ekz>
        <trd lng="de">Lorbeeren <klr tip="ind">(Sieg, Ruhm)</klr></trd>
        <trd lng="en">laurels</trd>
        <trd lng="hu">bab&eacute;r<klr
            tip="ind">(dics&odblac;s&eacute;g)</klr></trd>
        <trd lng="nl">lauwerkrans</trd>
        <trd lng="pl">laury</trd>
        <trdgrp lng="pt">
          <trd>laureal</trd>,
          <trd>laurel</trd>
        </trdgrp>
      </snc>
      <trd lng="fr">lauriers</trd>
      <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_y;</trd>
    </drv>

    <drv mrk="lauxr.0okrono">
      <kap>
        <tld/>okrono
      </kap>
      <snc>
        <dif>
          <ref tip="super" cel="kron.0o.plektita">Krono<sncref/></ref>
          el <tld/>aj bran&ccirc;etoj, kutima honorsigno &ccirc;e la
          antikvaj
          grekoj kaj romianoj; metafore, simbolo de venko, sukceso,
          merito:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            kion ajn fifamigan oni alie povis diri pri
            <ctl>Jorike</ctl>, pro unu afero &gcirc;i meritis
            <tld/>okronon: &gcirc;i estis eminenta instrumajstro
            <fnt>
              <aut>B. Traven, tr. Hans-Georg Kaiser</aut>:
              <vrk>La Mortula &scirc;ipo</vrk>.
              <lok>
              <url
              ref="http://home.t-online.de/home/kaiser-altenburg/u/B.Traven/m31-35.htm">
              &Ccirc;ap. 31a</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">kurunenn lore</trd>
      <trdgrp lng="ca">
        <trd>corona de llorer</trd>,
        <trd>laur&egrave;ola</trd>
      </trdgrp>
      <trd lng="nl">lauwerkrans</trd>
      <trd lng="pl">wieniec <ind>laurowy</ind></trd>
      <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_o;&c_v;&c_y;&c_j;
      &c_v;&c_je;&c_n;&c_o;&c_k;</trd>
    </drv>

  </art>
