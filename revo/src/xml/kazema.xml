<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kazema.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kazemat</rad>/o
</kap>
<drv mrk="kazema.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Arka&jcirc;a subtera &ccirc;ambro en citadelo a&ubreve; fortika&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_e;&c_m;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>kasemata</trd>,
    <trd>kasematy</trd>,
    <trd>kazemata</trd>,
    <trd>kazematy</trd>
  </trdgrp>
  <trd lng="de">Kasematte</trd>
  <trd lng="en">casemate</trd>
  <trd lng="fr">casemate</trd>
  <trd lng="hu">kazamata</trd>
  <trd lng="it">casamatta</trd>
  <trd lng="nl">kazemat</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_je;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">kazematy</trd>
</drv>
</art>
