<vortaro>
<art mrk="$Id: melolo.xml,v 1.16 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>melolont</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="melolo.0o">
  <kap><tld/>o</kap>
  <snc mrk="melolo.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="skarab.maj0o.ZOO">majskarabo</ref>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_u;&c_sh;&c_ch;</trd>
  <trdgrp lng="ca">
    <trd>escarabat de maig</trd>,
    <trd>escarabat de Sant Joan</trd>
  </trdgrp>
  <trd lng="cs">chroust</trd>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="en">may bug</trd>
  <trdgrp lng="fr">
    <trd>hanneton</trd>,
    <trd>ver blanc<klr tip="ind"> (insecte)</klr></trd>
  </trdgrp>
  <trd lng="hu">cserebog&aacute;r</trd>
  <trd lng="nl">meikever</trd>
  <trd lng="pl">chrab&aogonek;szcz majowy</trd>
  <trdgrp lng="pt">
    <trd>melolonta</trd>,
    <trd>besouro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_zh;&c_u;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_h;&c_r;&c_u;&c_shch;</trd>
  </trdgrp>
  <trd lng="sk">chr&uacute;st</trd>
</drv>
</art>
