<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volut.xml,v 1.4 2015/07/04 08:37:46 revo Exp $">
<kap>
    <rad>volut</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="volut.0o">
  <kap><tld/>o</kap>
  <snc mrk="volut.0o.ornamo">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Spiralforma ornama&jcirc;o, ekz. &ccirc;e konstrua&jcirc;oj, muzikinstrumentoj,
      litertipoj:
      <ekz>
        la bildoj sur la muroj montris <tld/>ajn
        ornama&jcirc;ojn a&ubreve; skribsignojn de fremdspeca alfabeto
        <fnt>
          <aut>Micheal Ende, trad. Wolfram Diestel</aut>,
          <vrk>La Sen&ccirc;esa Rakonto</vrk>,
          <lok>&ccirc;apitro 16</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>voluta</trd>,
    <trd>závit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schn&ouml;rkel</trd>,
    <trd>Volute</trd>
  </trdgrp>
  <trd lng="fr">volute</trd>
  <trdgrp lng="sk">
    <trd>voluta</trd>,
    <trd>závit</trd>
  </trdgrp>
</drv>
</art>

