<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: male.xml,v 1.11 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>male</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="male.0o">
  <kap><tld/>o</kap>
  <snc mrk="male.0o.ilo">
    <dif>
      <ref tip="super" cel="martel.0o">Martelo</ref> kun kapo
      amba&ubreve;flanke
      samforma kaj samuza, ofte ligna:
      <ekz>
        <tld/>o enbatas la ringojn de barelo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 7a, p.
        432a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">ma&ccedil;a</trd>
  <trdgrp lng="cs">
    <trd>dřevěná palička</trd>,
    <trd>tlouk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kn&uuml;pfel</trd>,
    <trd>Klipfel</trd>,
    <trd>Kl&uuml;pfel</trd>,
    <trd>Kl&ouml;pfel</trd>,
    <trd>Schlegel</trd>,
    <trd>Holzhammer</trd>
  </trdgrp>
  <trd lng="es">mazo</trd>
  <trd lng="fr">maillet</trd>
  <trdgrp lng="hu">
    <trd>fakalap&aacute;cs</trd>,
    <trd>k&eacute;tfej&udblac; <ind>kalap&aacute;cs</ind></trd>
  </trdgrp>
  <trd lng="nl">houten <ind>hamer</ind></trd>
  <trdgrp lng="pt">
    <trd>ma&ccedil;o</trd>,
    <trd>malho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dvojhlavé drevené kladivo</trd>,
    <trd>pobiják</trd>,
    <trd>pálka <klr>( šport.)</klr></trd>
  </trdgrp>
</drv>
</art>
