<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koalic.xml,v 1.11 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>koalici</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koalic.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="koalic.0i.KOMUNE">
    <dif>
      Fari momentan atakan interligon kontra&ubreve; komuna malamiko:
      <ekz>
        la regnoj, kiuj <tld/>is kontra&ubreve; Napoleono.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ju;</ind></trd>,
    <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a; &c_w; <ind>&c_k;&c_a;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="cs">být v koalici</trd>
  <trdgrp lng="de">
    <trd>koalieren</trd>,
    <trd>sich <ind> verb&uuml;nden</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>coaliser</ind></trd>,
    <trd>se <ind>liguer</ind></trd>
  </trdgrp>
  <trd lng="nl">een <ind>coalitie</ind> sluiten</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_v; &c_k;&c_o;&c_a;&c_l;&c_i;&c_c;&c_i;&c_ju;</trd>
  <trd lng="sk">byť v koalícií</trd>
</drv>

<drv mrk="koalic.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="koalic.0o.KOMUNE">
    <dif>
      Portempa interligo,
      <ref tip="vid" cel="kartel.0o">kartelo</ref>:
      <ekz>
        <tld/>o de diversaj partioj kontra&ubreve; la ministraro.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">koalice stran</trd>
  <trd lng="fr">coalition</trd>
  <trd lng="sk">koalícia</trd>
</drv>
</art>
