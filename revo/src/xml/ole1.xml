<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ole1.xml,v 1.21 2015/10/27 11:10:30 revo Exp $">
<kap>
  <rad>ole</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ole1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ole1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbo, kies plej grava specio estas la
      <ref tip="sub" cel="oliv.0arbo">olivarbo</ref>;
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Olea</trd>).
    </dif>
  </snc>
  <rim mrk="ole1.0o.RIM_HOM">
    Fundamente <ref tip="hom" cel="ole.0o">oleo</ref> estas
    grasa substanco simila al tiu, kiun oni premas el fruktoj
    de olivarbo.
  </rim>
  <trd lng="be">&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">g&egrave;nere Olea</trd>
  <trd lng="de">&Ouml;lbaum</trd>
  <trd lng="en">Olea</trd>
  <trd lng="fr">olivier<klr> (genre)</klr></trd>
  <trd lng="hu">olajfa</trd>
  <trd lng="nl">olieboom</trd>
  <trd lng="pl">oliwka</trd>
  <trd lng="pt">&oacute;lea</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nafta</trd>,
    <trd>olej</trd>
  </trdgrp>
</drv>
</art>
