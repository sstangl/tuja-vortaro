<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ortogr.xml,v 1.20 2015/07/04 08:37:40 revo Exp $">
<kap><ofc>1</ofc> <rad>ortografi</rad>/o</kap>
<drv mrk="ortogr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Maniero &gcirc;uste skribi la vortojn de iu lingvo:
      <ekz>
        racia, fonetika, etimologia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grafis.0o">grafismo</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Rechtschreibung</trd>,
     <trd>Orthografie</trd>
    </trdgrp>
    <trd lng="fr">graphie</trd>
  </snc>
  <snc>
    <dif>
      Maniero skribi la vortojn:
      <ekz>
        la fina le&gcirc;o por nia <tld/>o estos <ctl>por unu sono unu
        litero</ctl><fnt><bib>LR</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Schreibung</trd>,
     <trd>Schreibweise</trd>
    </trdgrp>
  <ref tip="vid" cel="alfabe.0o">alfabeto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_p;&c_ib;&c_s;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">ortografia</trd>
  <trdgrp lng="cs">
    <trd>ortografie</trd>,
    <trd>pravopis</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&theta;&omikron;&gamma;&rho;&alfa;&phi;&jota_ton;&alfa;</trd>
  <trd lng="en">orthography</trd>
  <trd lng="fr">orthographe</trd>
  <trd lng="hu">helyes&iacute;r&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>orthografie</trd>,
    <trd>spelling</trd>
  </trdgrp>
  <trd lng="pt">ortografia</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_r;&c_f;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">pravopis</trd>
  <trd lng="vo">lotogaf</trd>
</drv>
</art>
