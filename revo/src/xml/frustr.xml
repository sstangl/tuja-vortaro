<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frustr.xml,v 1.5 2014/04/14 12:10:22 revo Exp $">
<kap>
  <ofc>9</ofc><rad>frustr</rad>/i <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="frustr.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <snc mrk="frustr.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="cxagre.0i.per_kontrauxstaro">&ccirc;agreni</ref>
    <ekz>
      unu afero aparte <tld/>is min dum la pasintaj monatoj: Ne plu
      venis
      novaj ideoj al mi
      <fnt>
      <aut>D. J. Kunar</aut>,
      <vrk><url ref="http://kunar.livejournal.com/109967.html">&Ccirc;e
      kafeja
      tablo...</url></vrk>,
      <lok>[blogo], 2005-09-12</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="en">frustrate</trd>
  <trd lng="fr">frustrer</trd>
  <trdgrp lng="pt">
    <trd>frustrar</trd>,
    <trd>decepcionar</trd>
  </trdgrp>
</drv>

<drv mrk="frustr.0o">
  <kap><tld/>o</kap>
  <snc mrk="frustr.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="cxagre.0o.KOMUNE">&ccirc;agreno</ref>
    <ekz>
      naivaj revoj pri balda&ubreve;a &leftquot;fina venko&rightquot; povas naski <tld/>ojn
      <fnt>
      <vrk><url ref="http://www.rano.org/voss.html">Proklamo de
      Voss</url></vrk>,
      <bib>Esperanto</bib>,
      <lok>1991-10</lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="en">frustration</trd>
  <trd lng="fr">frustration</trd>
  <trd lng="pt">frustra&ccedil;&atilde;o</trd>
</drv>
</art>

