<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eskud.xml,v 1.13 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>eskud</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="eskud.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref>
      de diversaj landoj
      kaj epokoj (&Ccirc;ilio, Francujo, Portugalujo):
      <ekz>
        li trovis en kunnodita naztuko amaseton da oraj
        <tld/>oj<fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro
        23a, p. 165a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_ju;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_u;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_u;&c_d;&c_o;</trd>
  <trd lng="ca">escut</trd>
  <trd lng="cs">stará francouzská stříbrná mince</trd>
  <trd lng="de">Escudo</trd>
  <trd lng="en">escudo</trd>
  <trd lng="es">escudo</trd>
  <trdgrp lng="fr">
    <trd>&eacute;cu<klr> (monnaie)</klr></trd>,
    <trd>escudo</trd>
  </trdgrp>
  <trd lng="id">escudo</trd>
  <trd lng="pl">escudo</trd>
  <trd lng="pt">escudo</trd>
  <trd lng="sk">stará francúzska strieborná minca</trd>
</drv>
</art>
