<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boks.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
  <kap>
  <ofc>1</ofc>
  <rad>boks</rad>/i
  </kap>
  <drv mrk="boks.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="fak">SPO</uzo>
    <snc mrk="boks.0i.lukti">
      <dif>
      Pugnolukti. 
      </dif>
      <trd lng="de">boxen</trd>
      <trd lng="en">box</trd>
      <trd lng="es">boxear</trd>
      <trd lng="fr">boxer</trd>
      <trd lng="hu">bokszol</trd>
      <trdgrp lng="it">
         <trd>boxare</trd>,
         <trd>pugilare</trd></trdgrp>
      <trd lng="pl">boksowa&cacute;</trd>
      <trd lng="ru">
&c_b;&c_o;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="sk">boxova&tcaron;</trd>
      <trd lng="sv">boxas</trd>
    </snc>

    <snc mrk="boks.0i.okupigxi">
    <dif>
      Okupi&gcirc;i pri <tld/>o kiel sportisto.
    </dif>
      <trdgrp lng="it">
         <trd>tirare di <ind>boxe</ind></trd>,
         <trd>praticare <ind>pugilato</ind></trd></trdgrp>
      <trd lng="pl">uprawia&cacute; <ind>boks</ind></trd>
    </snc>
  <trd lng="ca">boxar</trd>
  <trd lng="cs">boxovat</trd>
</drv>

  <drv mrk="boks.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">SPO</uzo>
    <snc>
      <dif>
        Arto, sporto de pugnolukto.
      </dif>
      <trd lng="be">&c_b;&c_o;&c_k;&c_s;</trd>
      <trd lng="ca">boxa</trd>
      <trdgrp lng="de">
         <trd>Boxen</trd>,
         <trd>Faustkampf</trd></trdgrp>
      <trdgrp lng="en">
        <trd>boxing</trd>,
        <trd>pugilism</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>boxeo</trd>,
        <trd>box</trd>,
        <trd>pugilismo</trd>
      </trdgrp>
      <trd lng="fr">boxe <klr tip="ind">(sport)</klr></trd>
      <trdgrp lng="hu">
         <trd>&ouml;k&ouml;lv&iacute;v&aacute;s</trd>,
         <trd>boksz</trd></trdgrp>
      <trdgrp lng="it">
         <trd>boxe</trd>,
         <trd>pugilato</trd></trdgrp>
      <trd lng="pl">boks</trd>
      <trd lng="ru">&c_b;&c_o;&c_k;&c_s;</trd>
      <trd lng="sk">box</trd>
      <trd lng="sv">boxning</trd>
    </snc>
  <trd lng="cs">box</trd>
  <trd lng="sk">box <klr>(oddelené miesto v reštaurácií)</klr></trd>
</drv>
</art>
