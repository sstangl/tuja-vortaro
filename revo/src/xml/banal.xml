<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: banal.xml,v 1.22 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>banal</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="banal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ordinare, ofte uzata de &ccirc;iu ajn; seninteresa pro
      senoriginaleco:
      <ekz>
        <tld/>aj la&ubreve;doj, <tld/>a esprimo, vera&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;oj de la &ccirc;iutaga vivo;
      </ekz>
      <ekz>
        mi <klr>...</klr> faros &scirc;ajne <tld/>ajn demandojn, kaj
        paroligos lin
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gurd.0i">gurdita</ref>,
      <ref cel="stereo3.0a">stereotipa</ref>,
      <ref cel="sxablo">&scirc;ablona</ref>,
      <ref cel="trivia.0a.EVI">triviala</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>banal</trd>,
    <trd>insuls</trd>,
    <trd>com&uacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>banální</trd>,
    <trd>nevkusný</trd>,
    <trd>otřepaný</trd>,
    <trd>všední</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>banal</trd>,
    <trd>allt&auml;glich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banal</trd>,
    <trd>trite</trd>,
    <trd>commonplace</trd>
  </trdgrp>
  <trd lng="es">banal</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&zwnj;&f_p;&a_A; &a_A;&a_f;&a_t;&a_A;&a_d;&a_h;</trd>,
    <trd>&a_m;&a_b;&a_t;&a_dh;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">banal</trd>
  <trdgrp lng="hu">
    <trd>ban&aacute;lis</trd>,
    <trd>elcs&eacute;pelt</trd>
  </trdgrp>
  <trd lng="nl">banaal</trd>
  <trd lng="no">banal</trd>
  <trdgrp lng="pl">
    <trd>banalny</trd>,
    <trd>oklepany</trd>,
    <trd>pospolity</trd>,
    <trd>trywialny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_v;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_b;&c_i;&c_t;&c_y;&c_j;<klr tip="ind">
    (&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_z;&c_a;&c_t;&c_a;&c_s;&c_k;&c_a;&c_n;&c_n;&c_y;&c_j;<klr
    tip="ind">
    (&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ban&aacute;lny</trd>,
    <trd>otrepaný</trd>,
    <trd>všedný</trd>
  </trdgrp>
</drv>

<drv mrk="banal.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="banal.mal0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="ordina.ekster0a">Eksterordinara</ref>,
      <ref tip="vid" cel="sensac1.0a">sensacia</ref>:
      <ekz>
        preska&ubreve; &ccirc;iupa&gcirc;e li prezentas fascinajn, originalajn kaj 
        mal<tld/>ajn bildojn, metaforojn, asociadojn
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk><url
          ref="http://esperanto.net/literaturo/roman/libr/unumanrec.html">
          Plezura sed &scirc;topita</url></vrk>,
          <lok>[2004]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>extraordinari</trd>,
    <trd>ins&ograve;lit</trd>,
    <trd>original</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>original<klr> (particulier)</klr></trd>,
    <trd>qui sort de l'ordinaire</trd>
  </trdgrp>
</drv>
</art>
