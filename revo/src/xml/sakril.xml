<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sakril.xml,v 1.20 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sakrilegi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sakril.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Malrespekta ago kontra&ubreve; sanktigita persono, objekto a&ubreve; loko.
    </dif>
    <refgrp tip="vid">
      <ref cel="blasfe.0i">blasfemi</ref>,
      <ref cel="profan.0i">profani</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_sh;&c_ch;&c_u;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_l;&c_ju;&c_z;&c_n;&c_je;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;&c_t;&c_a;&c_c;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rouhání</trd>,
    <trd>svatokrádež</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Entweihung</trd>,
    <trd>Frevel</trd>,
    <trd>L&auml;sterung</trd>,
    <trd>Sch&auml;ndung</trd>,
    <trd>Sakrileg</trd>
  </trdgrp>
  <trd lng="en">sacrilege</trd>
  <trd lng="es">sacrilegio</trd>
  <trd lng="fr">sacril&egrave;ge<klr> (acte)</klr></trd>
  <trd lng="hu">szents&eacute;gt&ouml;r&eacute;s</trd>
  <trd lng="it">sacrilegio</trd>
  <trd lng="nl">heiligschennis</trd>
  <trdgrp lng="pt">
   <trd>sacril&eacute;gio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_shch;&c_u;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_t;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">svätokrádež</trd>
</drv>

<drv mrk="sakril.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malrespekta, atenca al sankta&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>rouhačský</trd>,
    <trd>svatokrádežný</trd>
  </trdgrp>
  <trd lng="en">sacrilegious</trd>
  <trd lng="es">sacr&iacute;lego</trd>
  <trd lng="fr">sacril&egrave;ge<klr> (adj.)</klr></trd>
  <trd lng="it">sacrilego<klr> (agg.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_shch;&c_u;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_t;&c_a;&c_t;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rúhačský</trd>,
    <trd>svätokrádežný</trd>
  </trdgrp>
</drv>

<drv mrk="sakril.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>páchat svatokrádež</trd>,
    <trd>rouhat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entweihen</trd>,
    <trd>freveln</trd>,
    <trd>l&auml;stern</trd>,
    <trd>sch&auml;nden</trd>,
    <trd><klr>ein </klr>Sakrileg <ind>begehen</ind></trd>
  </trdgrp>
  <trd lng="en">commit sacrilege</trd>
  <trd lng="es">hacer un sacrilegio</trd>
  <trdgrp lng="fr">
    <trd>profaner</trd>,
    <trd>commettre un <ind>sacril&egrave;ge</ind></trd>
  </trdgrp>
  <trd lng="hu">szents&eacute;gt&ouml;r&eacute;st k&ouml;vet el</trd>
  <trd lng="it">commettere un <ind>sacrilegio</ind></trd>
  <trd lng="nl">heiligschennis plegen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_shch;&c_u;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_t;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dopustiť sa svätokrádeže</trd>,
    <trd>rúhať sa</trd>
  </trdgrp>
</drv>

<drv mrk="sakril.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Atencinto kontra&ubreve; sankta&jcirc;o.
    </dif>
  </snc>
  <trd lng="en">sacrilegist</trd>
  <trdgrp lng="fr">
    <trd>profanateur</trd>,
    <trd>sacril&egrave;ge<klr> (pers.)</klr></trd>
  </trdgrp>
  <trd lng="it">sacrilego<klr> (sost.)</klr></trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_t;&c_a;&c_t;&c_je;&c_c;</trd>
</drv>
</art>
