<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxamot.xml,v 1.2 2013/02/04 11:10:10 revo Exp $">
<kap>
  <rad>&scirc;amot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxamot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxamot.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Pli a&ubreve; malpli subtila disprema&jcirc;o de kuirita argilo,
      kiun oni miksas kun kruda argilo anta&ubreve; kuiro
      por ricevi materialon pli solidan, varmimunan ktp;
      fajrargilo:
      <ekz>
        la koaksofornoj simile al altforno estas
        elmasonitaj per varmegorezista <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Koaksigejo">
          koaksigejo</url>, 2012-12-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_m;&c_o;&c_t;</trd>
  <trd lng="en">grog<klr> (clay)</klr></trd>
  <trd lng="fr">chamotte</trd>
  <trd lng="ru">&c_sh;&c_a;&c_m;&c_o;&c_t;</trd>
</drv>
</art>

