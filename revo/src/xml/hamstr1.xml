<kap>
   <rad>hamstred</rad>/oj
</kap>
<drv mrk="hamstr1.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO"
       lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
       <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Cricetidae</trd>).
     </dif>
   </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_ja;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cric&egrave;tids</trd>
  <trd lng="cs">křečkovití</trd>
  <trd lng="de">W&uuml;hler</trd>
  <trd lng="fr">cric&eacute;tid&eacute;s</trd>
  <trd lng="hu">h&ouml;rcs&ouml;gf&eacute;l&eacute;k</trd>
  <trd lng="it">criceti</trd>
  <trd lng="nl">hamsters en woelmuizen<klr tip="ind">
   (fam.)</klr></trd>
  <trdgrp lng="pt">
    <trd>cricet&iacute;deos</trd>,
    <trd>mur&iacute;deos</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_h;&c_o;&c_m;&c_ja;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>,
     <trd>&c_h;&c_o;&c_m;&c_ja;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
   </trdgrp>
  <trd lng="sk">škrečkovité</trd>
</drv>
