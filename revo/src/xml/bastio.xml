<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bastio.xml,v 1.21 2015/10/02 02:10:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bastion</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bastio.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bastio.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fortika&jcirc;o el tero a&ubreve; masona&jcirc;o elstaranta el
      remparo.
    </dif>
    <trd lng="br">beg-moger</trd>
    <trd lng="ca">basti&oacute;</trd>
    <trdgrp lng="de">
      <trd>Bollwerk</trd>,
      <trd>Bastion</trd>,
      <trd>Bastei</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_s;&c_t;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="hu">b&aacute;stya</trd>
    <trd lng="pl">bastion</trd>
  </snc>
  <snc mrk="bastio.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Defendilo, &scirc;irmilo: 
      <ekz>
        la Hungaroj longtempe funkciis kiel <tld/>o 
        de la Kristanismo kontra&ubreve; la Turkoj.
      </ekz>
    </dif>
    <trd lng="ca">baluard</trd>
    <trdgrp lng="de">
      <trd>Bollwerk</trd>,
      <trd>Bastion</trd>,
      <trd>Schutzwall</trd>,
    </trdgrp>
    <trd lng="hu">v&eacute;d&odblac;b&aacute;stya</trd>
    <trdgrp lng="pl">
      <trd>bastion</trd>,
      <trd>ostoja</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_s;&c_t;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_t;&c_y;&c_jo;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>bastion</trd>,
    <trd>ba&scaron;ta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bastion</trd>
  </trdgrp>
  <trd lng="en">bastion</trd>
  <trd lng="es">basti&oacute;n</trd>
  <trd lng="fr">bastion</trd>
  <trd lng="it">bastione</trd>
  <trd lng="pt">basti&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>ba&scaron;ta</trd>,
    <trd>ve&zcaron;a <klr>(&scaron;ach.)</klr></trd>
  </trdgrp>
</drv>
</art>
