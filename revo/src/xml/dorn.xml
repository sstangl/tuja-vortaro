<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dorn.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>dorn</rad>/o
</kap>
<drv mrk="dorn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Natura pikilo de vegeta&jcirc;oj:
      <ekz>
        neniu rozo sen <tld/>o.
      </ekz>
    </dif>
    <trd lng="es">espina</trd>
    <trdgrp lng="fr">
      <trd>&eacute;pine</trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pine)</klr></trd>
    </trdgrp> 
    <trdgrp lng="pl">
      <trd>kolec</trd>,
      <trd>cier&nacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_i;&c_p;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Pikilo simila al <tld/>o:
      <ekz>
        <ind>buko<tld/>o</ind>.
        <trd lng="fr">ardillon</trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>aiguillon</trd>,
      <trd>pointe<klr tip="ind"> (aiguillon)</klr></trd>
    </trdgrp> 
    <trd lng="pl">szpilka</trd>
    <trd lng="ru">&c_sh;&c_p;&c_je;&c_n;&c_jo;&c_k;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpa&jcirc;o, malplezuro. 
    </dif>
    <trdgrp lng="fr">
      <trd>cactus<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>os<klr tip="ind"> (difficult&eacute;)</klr></trd>,
      <trd>&eacute;cueil<klr tip="ind"> (difficult&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="pl">zagwozdka</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_n;&c_o;&c_z;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_v;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">espina</trd>
  <trdgrp lng="cs">
    <trd>osten</trd>,
    <trd>trn</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dorn</trd>,
    <trd>Stachel</trd>
  </trdgrp>
  <trd lng="nl">doorn</trd>
  <trdgrp lng="pt">
    <trd>espinho</trd>,
    <trd>estrepe</trd>,
    <trd>pico</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>osteň</trd>,
    <trd>pichliač</trd>,
    <trd>tŕň</trd>
  </trdgrp>
</drv>
<drv mrk="dorn.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Plena je <tld/>oj:
      <ekz>
        kiel <tld/>a kano en la mano de ebriulo, tiel
        estas sentenco en la bu&scirc;o de malsa&gcirc;uloj
        <fnt><bib>MT</bib>, <lok>&Sen; 26:9</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;pineux<klr tip="ind"> (piquant)</klr></trd>,
      <trd>piquant<klr tip="ind"> (&eacute;pineux)</klr></trd>
    </trdgrp> 
    <trdgrp lng="nl">
      <trd>doornig</trd>,
      <trd>stekelig</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ciernisty</trd>,
      <trd>kolczasty</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_i;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena je malhelpa&jcirc;oj kaj malplezuroj. 
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;licat<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>embarrassant<klr tip="ind"> (&eacute;pineux)</klr></trd>,
      <trd>&eacute;pineux<klr tip="ind"> (difficile)</klr></trd>
    </trdgrp> 
    <trd lng="nl">stekelig</trd>
    <trd lng="pl">naje&zdot;ony <klr
    tip="amb">trudno&sacute;ciami</klr></trd>
  </snc>
  <trd lng="ca">espin&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>ostnatý</trd>,
    <trd>ostnitý</trd>,
    <trd>pichlavý s trny</trd>
  </trdgrp>
  <trd lng="de">dornig</trd>
  <trd lng="es">espinoso</trd>
  <trd lng="sk">tŕnitý</trd>
</drv>
<drv mrk="dorn.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld lit="D"/>arbetaro.  
        (<refgrp tip="vid">
          <ref cel="vepr.0o">vepro</ref>
        </refgrp>)
     </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>esbarzer</trd>,
    <trd>bardissa</trd>,
    <trd>romeguera</trd>
  </trdgrp>
  <trd lng="cs">trnisko</trd>
  <trd lng="fr">&eacute;pinaie</trd>
  <trdgrp lng="nl">
    <trd>doornbos</trd>,
    <trd>braambos</trd>
  </trdgrp>
  <trd lng="sk">tŕnité krovie</trd>
</drv>
</art>
