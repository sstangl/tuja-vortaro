<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: filip.xml,v 1.13 2015/02/26 21:10:17 revo Exp $">
<kap>
  <rad>Filip</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="filip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        &Ccirc;u mi estas kun vi tiel longan tempon, kaj vi min ne konas
        <tld/>o
        <fnt><bib>NT</bib>, <lok>&Mat; 14:9</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="br">Fulup</trd>
  <trd lng="de">Philipp</trd>
  <trd lng="fr">Philippe</trd>
  <trd lng="hu">F&uuml;l&ouml;p</trd>
  <trd lng="it">Filippo</trd>
  <trdgrp lng="nl">
    <trd>Filip</trd>,
    <trd>Filips</trd>
  </trdgrp>
  <trd lng="pl">Filip</trd>
  <trdgrp lng="pt">
    <trd>Filipe</trd>,
    <trd>Felipe</trd>
  </trdgrp>
  <trd lng="ru">&c_F;&c_i;&c_l;&c_i;&c_p;&c_p;</trd>
</drv>
</art>
