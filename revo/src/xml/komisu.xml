<art mrk="$Id: komisu.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>komisur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="komisu.0o">
  <kap><tld/>o</kap>
  <snc mrk="komisu.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Punkto, kie du similaj partoj kuni&gcirc;as:
      <ekz>
      <ind>lipa <tld/>o</ind>,
      <trd lng="fa">&f_g;&a_w;&a_sh;&a_h;&a_hamza_sure; &a_l;&a_b;</trd>
      <trd lng="nl">mondhoek</trd>
      </ekz>
      <ekz>
      <ind>palpebra <tld/>o</ind>.
      <trd lng="fa">&f_g;&a_w;&a_sh;&a_h;&a_hamza_sure; &f_ch;&a_sh;&a_m;</trd>
      <trd lng="fr">canthus</trd>
      <trd lng="it"><ind>canto</ind> oculare</trd>
      <trd lng="nl">ooghoek</trd>
      </ekz>
    </dif>
    <trd lng="fr">commissure<klr tip="ind"> (anat.)</klr></trd>
    <trdgrp lng="hu">
      <trd>testzug</trd>,
      <trd>testsz&ouml;glet</trd>
    </trdgrp>
    <trd lng="pl">spoid&lstroke;o</trd>
  </snc>
  <snc mrk="komisu.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kuni&gcirc;a punkto de du karpeloj.  
    </dif>
    <trd lng="hu">term&odblac;lev&eacute;lzug</trd>
  </snc>
  <trd lng="cs">komisura</trd>
  <trd lng="fa">&f_p;&f_y;&a_w;&a_n;&a_d;&f_g;&a_A;&a_h;</trd>
  <trd lng="it">commessura</trd>
  <trdgrp lng="nl">
    <trd>naad<klr tip="ind"> (commissuur)</klr></trd>,
    <trd>commissuur</trd>
  </trdgrp>
  <trd lng="pt">comissura</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">komis√∫ra</trd>
</drv>
</art>
