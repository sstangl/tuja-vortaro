<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: rus.xml,v 1.30 2015/07/04 08:37:43 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>rus</rad>/o
    </kap>

    <drv mrk="rus.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Ano de orientslava 
          <ref tip="lst" cel="etn.0o" lst="voko:popolanoj">etno</ref>:
          <ekz>
            Du maristoj <tld/>oj gaje saltis malsupren kaj preska&ubreve; 
            renversis la kompatindan knabon
            <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_je;&c_c;<klr> (&c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je;
      &c_n;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s; &c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_n;&c_a;&c_k;</trd>
  <trd lng="ca">rus<klr> (subst.)</klr></trd>
  <trd lng="cs">Rus</trd>
  <trd lng="de">Russe</trd>
  <trd lng="en">Russian</trd>
  <trd lng="fr">Russe</trd>
  <trd lng="hu">orosz</trd>
  <trd lng="nl">Rus</trd>
  <trdgrp lng="pl">
        <trd>Rosjanin</trd>,
        <trd>Rusek<klr> (pogardliwie lub pot.)</klr></trd>,
        <trd>Ruski<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
  <trd lng="pt">russo</trd>
  <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr>
      (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">Rus</trd>
  <trd lng="vo">lusel</trd>
</drv>

    <drv mrk="rus.in0o">
      <kap><tld/>ino</kap>
      <snc>
        <dif>
          Ino el la <tld/>a etno:
          <ekz>
            ekbrilis por ni la karaktero de l' <tld/>oj kaj
            <tld/>inoj en &gcirc;iaj la plej bonaj ecoj
            <fnt>
              <aut>Valdemar Langlet</aut>:
              <vrk>Voja&gcirc;impresoj</vrk>. <lok>1895</lok>
            </fnt>;
          </ekz><ekz>
            la mastrino estis <tld/>ino, virino kvindekjara, pure
            vestita, kun inteligenta dol&ccirc;a viza&gcirc;o kaj
            serioza parolmaniero
            <fnt>
              <aut>I. Turgenjev, tr. Kabe</aut>:
              <vrk>Patroj kaj filoj</vrk>.
              <lok>Inko Tyreso 2001. &#8208; &Ccirc;ap. 8a.</lok>
            </fnt>;
          </ekz>
        </dif>
      </snc>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;&c_n;&c_ja;</trd>
  <trd lng="ca">russa<klr> (subst.)</klr></trd>
  <trd lng="cs">Ruska</trd>
  <trd lng="de">Russin</trd>
  <trdgrp lng="pl">
        <trd>Rosjanka</trd>,
        <trd>Ruska<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_a;&c_ja;<klr>
      (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">Ruska</trd>
</drv>

    <drv mrk="rus.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>oj a&ubreve; ilia etno kaj lando:
          <ekz>
            la <tld/>a lingvo.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">rus<klr> (adj.)</klr></trd>
  <trd lng="cs">ruský</trd>
  <trd lng="de">russisch</trd>
  <trd lng="en">Russian</trd>
  <trd lng="fr">russe</trd>
  <trd lng="hu">orosz</trd>
  <trd lng="nl">Russisch</trd>
  <trdgrp lng="pl">
        <trd>rosyjski</trd>,
        <trd>ruski<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j; 
        <klr>(&c_p;&c_r;&c_i;&c_l;.)</klr></trd>
  <trd lng="sk">ruský</trd>
  <trdgrp lng="vo">
        <trd>lusik</trd>,
        <trd>lus&auml;nik</trd>
      </trdgrp>
  <trd lng="zh">&#20420;<klr> [&eacute;]</klr></trd>
</drv>

    <drv mrk="rus.R0ujo">
      <kap><tld lit="R"/>ujo, <var><kap><tld
      lit="R"/>lando</kap></var></kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Plurnacia &scirc;tato, situanta en orienta
          <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj
          norda <ref tip="malprt" cel="azi.A0o">Azio</ref>,
          kies historia kerno estas la <tld/>a nacio:
          <ekz>
            La &ccirc;efurbo de <tld lit="R"/>ujo &gcirc;is 1712 kaj
            ekde 1918 estas <ref tip="prt" cel="moskv.0o">Moskvo</ref>;
            de 1712 &gcirc;is 1918 la &ccirc;efurbo de <tld lit="R"/>ujo
            estis <ref tip="prt" cel="peterburg.0o">Peterburgo</ref>.
          </ekz>
        </dif>
        <rim>
        La nomo estas gentobaza<fnt>AdE</fnt>, oni uzas la vorton <tld
        lit="R"/>lando pro internacieco a&ubreve; Esperanta tradicio.
        </rim>
      </snc>
      <refgrp tip="sin">
        <ref cel="rusi.0o">Rusio</ref>,
        <ref cel="rus.R0aFederacio">Ruslanda Federacio</ref>
      </refgrp>
      <bld lok="../bld/Rusujo.png">
        Mapo de <tld lit="R"/>ujo
      </bld>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_ja;</trd>
      <trd lng="bg">&c_R;&c_u;&c_s;&c_i;&c_ja;</trd>
      <trdgrp lng="ca">
        <trd>R&uacute;ssia</trd>,
        <trd>Federaci&oacute; Russa</trd>
      </trdgrp>
      <trd lng="de">Russland</trd>
      <trd lng="en">Russia</trd>
      <trd lng="fr">Russie</trd>
      <trd lng="hu">Oroszorsz&aacute;g</trd>   
      <trd lng="nl">Rusland</trd>
      <trd lng="pl">Rosja</trd>
      <trd lng="pt">R&uacute;ssia</trd>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_ja;</trd>
      <trd lng="vo">Lus&auml;n</trd>
      <trdgrp lng="zh">
        <trd>&#20420;&#22269;</trd>,
        <trd><klr>tr. </klr>&#20420;&#22283;<klr> 
            [&Eacute;gu&oacute;]</klr></trd>,
        <trd>&#20420;&#32599;&#26031;<klr> 
            [&Eacute;lu&oacute;s&#299;]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="rus.R0aFederacio">
      <kap><tld lit="R"/>landa Federacio</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Dua oficiala nomo de <tld lit="R"/>ujo.
        </dif>
      </snc>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja; 
        &c_F;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
      <trd lng="ca">Federaci&oacute; Russa</trd>
      <trd lng="de">Russische F&ouml;deration</trd>
      <trd lng="en">Russian Federation</trd>
      <trd lng="fr">F&eacute;d&eacute;ration de Russie</trd>
      <trd lng="hu">Orosz F&ouml;der&aacute;ci&oacute;</trd>   
      <trd lng="nl">Russische Federatie</trd>
      <trd lng="pl">Federacja <ind>Rosyjska</ind></trd>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; 
        &c_F;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="rus.R0aImperio">
      <kap><tld lit="R"/>landa Imperio</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Oficiala nomo de <tld lit="R"/>ujo ekde 1721 &gcirc;is 1917.
        </dif>
      </snc>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja; 
        &c_Ib;&c_m;&c_p;&c_e;&c_r;&c_y;&c_ja;</trd>
      <trd lng="ca">Imperi Rus</trd>
      <trd lng="de">Russisches Reich</trd>
      <trd lng="en">Russian Empire</trd>
      <trd lng="fr">Empire de Russie</trd>
      <trd lng="hu">Orosz Birodalom</trd>   
      <trd lng="nl">Russische Rijk</trd>
      <trdgrp lng="pl">
        <trd>Imperium <ind>Rosyjskie</ind></trd>,
        <trd>Wielka <ind>Rosja</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; 
        &c_I;&c_m;&c_p;&c_je;&c_r;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="rus.0ujano">
      <kap><tld/>ujano, <var><kap><tld/>landano</kap></var></kap>
      <snc>
        <dif>
          Civitano de <tld lit="R"/>ujo sendepende de etno:
          <ekz>
            germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas 
            <tld/>ujanoj,
            kvankam ili ne estas <tld/>oj
            <fnt><bib>F</bib>, <lok>Ekzercaro, &para;37.</lok></fnt>;
          </ekz><ekz>
            mia gepatra lingvo estas la <tld/>a, sed nun mi parolas pli
            pole, kaj mi nomas min ne &leftquot;<tld/>o&rightquot; sed
            &leftquot;<tld/>ujano&rightquot;
            <fnt>
              <aut>&Z;</aut>: <vrk>
              [Letero de 1901-03-08 al Th. Thorsteinsson, Kopenhago
              </vrk>,
              <lok>
              <url ref="http://www.math.uu.se/~kiselman/zamenhof.pdf">
              citite la&ubreve; Zamenhof 1929:523</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_je;&c_c;
      <klr>(&c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je;
    &c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;)
      </klr></trd>
  <trd lng="ca">rus<klr> (subst.)</klr></trd>
  <trd lng="cs">ruský občan</trd>
  <trd lng="de">russischer Staatsb&uuml;rger</trd>
  <trd lng="en">Russian</trd>
  <trd lng="hu">oroszorsz&aacute;gi <klr tip="ind">(lakos)</klr>
      </trd>
  <trd lng="nl">Russisch staatsburger</trd>
  <trd lng="pl">Rosjanin</trd>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">ruský občan</trd>
  <trd lng="vo">lus&auml;nel</trd>
  <trdgrp lng="zh">
        <trd>&#20420;&#22269;&#20154;</trd>,
        <trd><klr>tr. </klr>&#20420;&#22283;&#20154;<klr> 
            [&eacute;gu&oacute;r&eacute;n]</klr></trd>
      </trdgrp>
</drv>

    <drv mrk="rus.0uja">
      <kap><tld/>uja, <var><kap><tld/>landa</kap></var></kap>
      <snc>
        <dif>
          Rilatanta a&ubreve; apartenanta al <tld lit="R"/>ujo:
          <ekz>
           gazeto &leftquot;<tld lit="R"/>landa Esperantisto&rightquot;.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">rus<klr> (adj.)</klr></trd>
  <trd lng="cs">ruský <klr>(týkající se Ruska)</klr></trd>
  <trd lng="de">Russl&auml;nd-</trd>
  <trd lng="en">Russian</trd>
  <trd lng="hu">oroszorsz&aacute;gi</trd>
  <trd lng="nl">Russisch</trd>
  <trd lng="pl">rosyjski</trd>
  <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
      </trd>
  <trd lng="sk">ruský</trd>
  <trd lng="vo">lus&auml;nik</trd>
</drv>
  </art>
