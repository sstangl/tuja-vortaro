<kap>
  <rad>&jcirc;inz</rad>/o  <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="jxinz.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxinz.0o.gxinzo">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="gxinz.0o.TEKS">&gcirc;inzo</ref>
    <ekz>
      ni denove vidas ilin &ccirc;iujn, la dancistojn kaj kantistojn kaj
      iliajn
      inojn, en trivitaj <tld/>oj, kun violkoloraj pluvmanteloj
      <fnt><aut>Cezar</aut>, <vrk><url
      ref="http://www.cezarkulturo.de/197985.html">Cezar-prozpoemoj
      05</url></vrk>, <lok>7, Adia&ubreve;, rusoj!</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n;&c_s;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_d;&c_malmol;&c_n;&c_k;&c_i;</trd>,
    <trd>&c_d;&c_zh;&c_i;&c_n;&c_s;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jeans</trd>,
    <trd>Nietenhose</trd>
  </trdgrp>
  <trd lng="cs">d&zcaron;in(s)y</trd>
  <trd lng="en">jeans</trd>
  <trdgrp lng="es">
    <trd>(pantalones) <ind>vaqueros</ind></trd>,
    <trd>jeans</trd>,
    <trd>pantal&oacute;n de <ind>mezclilla</ind></trd>
  </trdgrp>
  <trd lng="fr">jeans</trd>
  <trd lng="hu">farmernadr&aacute;g</trd>
  <trd lng="it">blue-jeans</trd>
  <trd lng="nl">jeans</trd>
  <trd lng="pl">d&zdot;insy</trd>
  <trd lng="pt">jeans</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;&c_s;&c_y;</trd>
</drv>
