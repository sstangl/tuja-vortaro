<kap>
  <rad>turp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="turp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turp.0a.KOMUNE">
    <ref tip="dif" cel="bel.mal0a.KOMUNE">malbelega</ref>
      <ekz>
        belon kaj <tld/>on, &ccirc;ielon kaj inferon
        vi &ccirc;erpis nure el spirito mia
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="cs">o&scaron;kliv&yacute;</trd>
  <trd lng="en">ugly</trd>
  <trd lng="fr">hideux</trd>
  <trd lng="sk">&scaron;kared&yacute;</trd>
</drv>
</art>

