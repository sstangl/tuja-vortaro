<vortaro>
<art mrk="$Id: maroke.xml,v 1.14 2012/07/30 10:10:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>maroken</rad>/o
</kap>
<drv mrk="maroke.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Grajneca kolorita kaproledo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_k;&c_je;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_f;'&c_ja;&c_n;</trd>
  </trdgrp>        
  <trd lng="br">gavrgen</trd>
  <trd lng="ca">tafilet</trd>
  <trdgrp lng="de">
      <trd>Maroquin</trd>,
      <trd>Saffianleder</trd>
  </trdgrp>
  <trd lng="en">Moroccan <ind>leather</ind></trd>  
  <trdgrp lng="es">
      <trd>tafilete</trd>,
      <trd>cuero marroqu&iacute;</trd>
  </trdgrp>
  <trd lng="fr">maroquin</trd>
  <trdgrp lng="hu">
      <trd>marokk&oacute;b&odblac;r</trd>,
      <trd>szatty&aacute;nb&odblac;r</trd>
      <trd>maroken</trd>
  </trdgrp>  
  <trd lng="nl">marokijnleer</trd>
  <trd lng="pt">marroquim</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_f;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_o;&c_k;&c_i;&c_n;</trd>
  </trdgrp>
</drv>
</art>
