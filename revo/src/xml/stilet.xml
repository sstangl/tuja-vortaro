<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stilet.xml,v 1.13 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stilet</rad>/o
</kap>
<drv mrk="stilet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Malgranda ponardo kun klingo tre akrapinta.
    </dif>
    <refgrp tip="sin">
      <ref cel="ponard.0o">ponardo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_y;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>graphium</trd>,
    <trd>stilus</trd>
  </trdgrp>
  <trd lng="de">Stilett</trd>
  <trd lng="en">stiletto</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_sh;&a_n;&a_h;</trd>,
    <trd>&a_kh;&a_n;&a_j;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>stil&eacute;t</trd>,
    <trd>gyilok</trd></trdgrp>
  <trd lng="it">stiletto</trd>
  <trd lng="nl">stilet</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">krátka ostrá dýka</trd>
</drv>
</art>
