<kap><rad>nitrid</rad>/o</kap>
<drv mrk="nitrid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Kombina&jcirc;o de <ref cel="azot.0o">azoto</ref> kun alia elemento,
      kutime metalo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_y;&c_d;</trd>
  <trd lng="cs">nitrid</trd>
  <trd lng="de">Nitrid</trd>
  <trd lng="en">nitrite</trd>
  <trd lng="fa">&a_n;&f_y;&a_t;&a_r;&f_y;&a_t;</trd>
  <trd lng="fr">nitrure</trd>
  <trd lng="hu">nitrid</trd>
  <trd lng="nl">nitride</trd>
  <trd lng="pt">nitreto <klr tip="amb">(&ecirc;)</klr></trd>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_i;&c_d;</trd>
  <trd lng="sk">nitrid</trd>
  <trd lng="tr">nitr&uuml;r</trd>
  <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_i;&c_d;</trd>
</drv>
</art>

