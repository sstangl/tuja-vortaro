<vortaro>
<art mrk="$Id: eklamp.xml,v 1.17 2015/12/31 17:10:19 revo Exp $">
<kap>
  <rad>eklampsi</rad>/o
</kap>
<drv mrk="eklamp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> de virino ofta dum la lasta
      tempo de &scirc;ia gravedeco
      a&ubreve; tuj
      poste kaj karakterizata per konvulsiaj, epilepsiaj spasmoj kaj per
      albuminurio.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_l;&c_ja;&c_m;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ca">ecl&agrave;mpsia</trd>
  <trdgrp lng="cs">
    <trd>d&rcaron;. bo&zcaron;ec</trd>,
    <trd>eklampsie</trd>,
    <trd>k&rcaron;e&ccaron;e t&ecaron;hotn&eacute;</trd>,
    <trd>psotn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Eklampsie</trd>
  <trd lng="en">eclampsia</trd>
  <trd lng="es">eclampsia</trd>
  <trd lng="fr">&eacute;clampsie</trd>
  <trd lng="hu">eklampszia</trd>
  <trd lng="id">eklampsia</trd>
  <trdgrp lng="pl">
    <trd>rzucawka<klr> ci&aogonek;&zdot;owa</klr></trd>,
    <trd>eklampsja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>eclampsia</trd>,
    <trd>eclampse</trd>,
    <trd>ecl&acirc;mpsia</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_l;&c_a;&c_m;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">eklampsia</trd>
</drv>
</art>
