<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korela.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>korelativ</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="korela.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Reciproke responda, interresponda:
      <ekz>
        <tld/>aj fenomenoj;
      </ekz>
      <ekz>
        <uzo tip="fak">GRA</uzo>
        tabelo de <tld/>aj vortoj;
      </ekz>
      <ekz>
        <uzo tip="fak">GRA</uzo>
        <ctl>tia</ctl> estas <tld/>a al <ctl>tia</ctl>, <ctl>tiom</ctl>
        al
        <ctl>kiom</ctl> ktp.
      </ekz>
    </dif>
    <ref tip="vid" cel="tabel.0vorto">tabelvorto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_a;&c_d;&c_n;&c_o;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korelativní</trd>,
    <trd>korelátní</trd>,
    <trd>souvztažný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bez&uuml;glich</trd>,
    <trd>wechselseitig</trd>,
    <trd>in <ind>Wechselbeziehung</ind> stehend</trd>,
    <trd>korrelativ</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>correlative</trd>,
    <trd>correlated</trd>
  </trdgrp>
  <trd lng="es">correlativo</trd>
  <trd lng="fr">corr&eacute;latif</trd>
  <trdgrp lng="hu">
    <trd>korrelat&iacute;v</trd>,
    <trd>viszonylagos</trd>,
    <trd>k&ouml;lcs&ouml;n&ouml;sen <ind>&ouml;sszef&uuml;gg&odblac;</ind></trd>,
    <trd>egym&aacute;ssal <ind>k&ouml;lcs&ouml;nhat&aacute;sban</ind> l&eacute;v&odblac;</trd>
  </trdgrp>
  <trd lng="nl">correlatief<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;zale&zdot;ny</trd>,
    <trd>korelatywny</trd>,
    <trd>korelacyjny</trd>
  </trdgrp>
  <trd lng="pt">correlativo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_je;&c_l;&c_ja;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korelatívny</trd>,
    <trd>korelačný</trd>,
    <trd>súvzťažný</trd>
  </trdgrp>
</drv>

<drv mrk="korela.0eco">
  <kap><tld/>eco</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Eco de a&jcirc;oj <tld/>aj:
      <ekz>
        <tld/>eco de du fenomenoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>korelace</trd>,
    <trd>souvztažnost</trd>
  </trdgrp>
  <trd lng="de">Korrelativit&auml;t</trd>
  <trd lng="en">correlation</trd>
  <trd lng="fr">corr&eacute;lation</trd>
  <trdgrp lng="hu">
    <trd>korrel&aacute;ci&oacute;</trd>,
    <trd>viszonylagoss&aacute;g</trd>,
    <trd>k&ouml;lcs&ouml;n&ouml;s <ind>&ouml;sszef&uuml;gg&eacute;s</ind></trd>,
    <trd>k&ouml;lcs&ouml;nhat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afhankelijkheid</trd>,
    <trd>samenhang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;zale&zdot;no&sacute;&cacute;</trd>,
    <trd>korelacja</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korelácia</trd>,
    <trd>súvzťažnosť</trd>
  </trdgrp>
</drv>
</art>
