<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: foliol.xml,v 1.11 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>foliol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="foliol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="foliol.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="foli.0ero.BOT">foliero</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>lísteček složeného listu</trd>,
    <trd>šupina obalová</trd>
  </trdgrp>
  <trd lng="sk">lístoček <klr>(zložitých kvetov)</klr></trd>
</drv>
</art>
