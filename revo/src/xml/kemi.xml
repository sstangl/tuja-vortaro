<kap>
  <ofc>8</ofc>
  <rad>kemi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kemi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <ref tip="dif" cel="hxemi.0o">&hcirc;emio</ref>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="br">kimiezh</trd>
  <trd lng="ca">qu&iacute;mica</trd>
  <trd lng="de">Chemie</trd>
  <trd lng="en">chemistry</trd>
  <trd lng="es">qu&iacute;mica</trd>
  <trd lng="fa">&a_sh;&f_y;&a_m;&f_y;</trd>
  <trd lng="fi">kemia</trd>
  <trd lng="fr">chimie</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;mia</trd>,
    <trd>vegytan</trd>
  </trdgrp>
  <trd lng="it">chimica</trd>
  <trd lng="nl">chemie</trd>
  <trd lng="no">kjemi</trd>
  <trd lng="pl">chemia</trd>
  <trd lng="pt">qu&iacute;mica <klr tip="amb">(ci&ecirc;ncia)</klr></trd>
  <trd lng="ru">&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">ch&eacute;mia</trd>
  <trd lng="sv">kemi</trd>
  <trd lng="vo">kiem</trd>
  <trdgrp lng="cs">
    <trd>chemie</trd>,
    <trd>luƒçba</trd>
  </trdgrp>
</drv>

<drv mrk="kemi.elektro0o">
  <kap>elektro<tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <ref tip="dif" cel="hxemi.elektro0o">elektro&hcirc;emio</ref>
  <ekz>
    per elektro<tld/>a procedo la emisiado reali&gcirc;as &ccirc;e ordinara
    temperaturo    
    <fnt>
      <aut>-, trad. J. Palu</aut>,
      <vrk><url
      ref="http://www.fw.hu/eventoj/steb/kemio/hkk7612.htm">
      Kion kapablas elektrono?</url></vrk>,
      <lok>Homo kaj Kosmo, numero 1976/1-1, p. 7a-8a</lok>
    </fnt>.
  </ekz>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
  <trd lng="de">Elektrochemie</trd>
  <trd lng="en">electrochemistry</trd>
  <trd lng="es">electroqu&iacute;mica</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_q;&a_A;&a_sh;&f_y;&a_m;&f_y;</trd>,
    <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_sh;&f_y;&a_m;&f_y;</trd>
  </trdgrp>
  <trd lng="fi">s&auml;hk&ouml;kemia</trd>
  <trd lng="fr">&eacute;lectrochimie</trd>
  <trd lng="hu">elektrok&eacute;mia</trd>
  <trd lng="it">elettrochimica</trd>
  <trd lng="nl">elektrochemie</trd>
  <trd lng="no">elektrokjemi</trd>
  <trd lng="pl">elektrochemia</trd>
  <trd lng="pt">eletroqu&iacute;mica</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>
