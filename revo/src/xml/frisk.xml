<art mrk="$Id: frisk.xml,v 1.5 2015/10/14 08:10:15 revo Exp $">
<kap>
  <rad>frisk</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="frisk.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="frisk.0a.KOMUNE">
    <ref tip="dif" cel="varm.mal0eta.KOMUNE">malvarmeta</ref>
      <ekz>
        <tld/>a posttagmezo de Aprilo
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>osv&ecaron;&zcaron;uj&iacute;c&iacute;</trd>,
    <trd>p&rcaron;&iacute;jem.chladiv&yacute;</trd>,
    <trd>studen&yacute;</trd>,
    <trd>vla&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">chilly</trd>
  <trd lng="fr">frisquet</trd>
  <trdgrp lng="pl">
    <trd>ch&lstroke;odny</trd>,
    <trd>rze&sacute;ki</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chladiv&yacute;</trd>,
    <trd>osvie&zcaron;uj&uacute;ci</trd>
  </trdgrp>
</drv>
</art>

