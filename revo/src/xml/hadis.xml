<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: hadis.xml,v 1.10 2013/03/26 10:10:14 revo Exp $">
    <kap>
      <rad>hadis</rad>/o
    </kap>

    <drv mrk="hadis.0o">
      <kap><tld/>o</kap>
      <snc mrk="hadis.0o.ISL">
        <uzo tip="fak">ISL</uzo>
        <dif>
          Raporto pri diroj kaj faroj de la profeto Mohamedo,
          parto de la <ref tip="malprt" cel="suna.0o.ISL">sunao</ref>:
          <ekz>
            sur la randoj de la tombo&scirc;tono videblas <klr>...</klr>
            instruaj  
            sentencoj pri la agoj kaj eldiroj de profeto Mohameto
            (<tld/>oj)
            <fnt>
              <aut>A. Ionesov</aut>,
              <vrk>Perlo &ccirc;e la Silka Vojo</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2006a, numero 8-9a, p. 20a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_h;&c_a;&c_d;&c_i;&c_s;</trd>
      <trd lng="ca">hadit</trd>
      <trd lng="de">Hadith</trd>
      <trd lng="en">hadith</trd>
      <trd lng="fa">&a_H;&a_d;&f_y;&a_th;</trd>
      <trd lng="fr">hadith</trd>
      <trd lng="hu">hadisz</trd>
      <trdgrp lng="id">
        <trd>hadis</trd>,
        <trd>hadits</trd>
      </trdgrp>
      <trd lng="it">hadith</trd>
      <trd lng="pl">hadis</trd>
      <trd lng="pt">hadith</trd>
      <trd lng="ru">&c_h;&c_a;&c_d;&c_i;&c_s;</trd>
      <trd lng="tr">hadis</trd>
    </drv>
  </art>

