  </trdgrp>
  <trdgrp lng="pl">
    <trd>z&lstroke;apa&cacute;</trd>,
    <trd>schwyta&cacute;</trd>,
    <trd>z&lstroke;owi&cacute;</trd>,
    <trd>dopa&sacute;&cacute;</trd>,
    <trd>pojma&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_j;&c_m;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chmatnúť</trd>,
    <trd>chytiť</trd>,
    <trd>uchopiť</trd>,
    <trd>zachytiť</trd>
  </trdgrp>
</drv>

<drv mrk="kapt.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kapt.en0i.KOMUNE">
    <dif>
      <tld lit="K"/>i en <tld/>ilon:
      <ekz>
        liaj propraj malbonfaroj en<tld/>os la malpiulon,
        kaj la &scirc;nuroj de lia peko lin tenos
        <fnt><bib>MT</bib>, <lok>&Sen; 5:22</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        da sciencoj li en<tld/>is senmezuran amasegon
        <fnt><bib>Revizoro</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>atrapar</trd>,
    <trd>entrampar</trd>,
    <trd>contenir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einfangen</trd>,
    <trd>umgarnen</trd>
  </trdgrp>
  <trd lng="es">atrapar</trd>
  <trdgrp lng="fr">
    <trd>prendre au pi&egrave;ge</trd>,
    <trd>avaler<klr tip="ind"> (des connaissances)</klr></trd>,
    <trd>ingurgiter<klr tip="ind"> (des connaissances)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>befog</trd>,
    <trd>csapd&aacute;ba ejt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;apa&cacute;<klr>
    (w sid&lstroke;a, matni&eogonek;, itp.)</klr></trd>,
    <trd>chwyta&cacute;<klr>
    (w sid&lstroke;a, matni&eogonek;, itp.)</klr></trd>,
    <trd>&lstroke;owi&cacute;<klr>
    (w sid&lstroke;a, matni&eogonek;, itp.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kapt.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="kapt.for0i.KOMUNE">
    <dif>
      Malproksimen konduki kiel <tld/>iton:
      <ekz>
        la Eternulo revenigis la for<tld/>itojn al Cion
        <fnt><bib>MT</bib>, <lok>&Psa; 126:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">raptar</trd>
  <trdgrp lng="cs">
    <trd>uloupit</trd>,
    <trd>unést</trd>
  </trdgrp>
  <trd lng="de">entf&uuml;hren</trd>
  <trdgrp lng="fr">
    <trd>enlever<klr tip="ind"> (qn)</klr></trd>,
    <trd>ravir<klr tip="ind"> (enlever)</klr></trd>
  </trdgrp>
  <trd lng="nl">ontvoeren</trd>
  <trdgrp lng="pl">
    <trd>porywa&cacute;</trd>,
    <trd>uprowadza&cacute;</trd>,
    <trd>rozchwytywa&cacute;</trd>
  </trdgrp>
  <trd lng="sk">uniesť</trd>
  <trd lng="tr">ka&ccedil;&inodot;rmak</trd>
</drv>

<drv mrk="kapt.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kapt.re0i.KOMUNE">
    <dif>
      <tld lit="K"/>i ion, kion oni lasis forgliti:
      <ekz>
        li re<tld/>is sian falantan ganton;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per forto de la volo &scirc;i re<tld/>is la forglitantan
        memkonscion<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;
    <klr>(&c_z;&c_n;&c_o;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>recuperar</trd>,
    <trd>recobrar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wieder <ind>einfangen</ind></trd>,
    <trd>zur&uuml;ckgewinnen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attraper de nouveau</trd>,
    <trd>appr&eacute;hender de nouveau</trd>,
    <trd>capter de nouveau</trd>,
    <trd>capturer de nouveau</trd>,
    <trd>prendre de nouveau</trd>,
    <trd>rattraper<klr tip="ind"> (attraper de nouveau)</klr></trd>,
    <trd>reprendre<klr tip="ind"> (saisir de nouveau)</klr></trd>,
    <trd>ressaisir<klr tip="ind"> (saisir de nouveau)</klr></trd>,
    <trd>saisir de nouveau</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszaszerez</trd>,
    <trd>visszanyer</trd>,
    <trd>&uacute;jra elkap</trd>,
    <trd>elcs&iacute;p</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;apa&cacute;<klr> (ponownie)</klr></trd>,
    <trd>chwyta&cacute;<klr> (ponownie)</klr></trd>,
    <trd>&lstroke;owi&cacute;<klr> (ponownie)</klr></trd>,
    <trd>pojma&cacute;<klr> (ponownie)</klr></trd>,
    <trd>odzyskiwa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>yeniden yakalamak</trd>,
    <trd>yeniden ele ge&ccedil;irmek</trd>
  </trdgrp>
</drv>

<drv mrk="kapt.atento0a">
  <kap>atento<tld/>a</kap>
  <snc mrk="kapt.atento0a.KOMUNE">
    <dif>
      Kiu ka&ubreve;zas fortan atenton, instigas al scivolo,
      <ref tip="vid" cel="kurioz.0a">kurioza</ref>,
      <ref tip="vid" cel="fascin.0a.FIG">fascina</ref>:
      <ekz>
        tiom atento<tld/>a kaj aventurplena estas la menciita 
        voja&gcirc;priskribo
        <fnt>
          <aut>Releganto</aut>,
          <vrk>Vamberi...</vrk>,
          <lok><url ref="http://valhalla.ulver.com/f129/t5779.html">
          Valhalla - Esperanto</url>, 2006-11-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>encisador</trd>,
    <trd>fascinant</trd>,
    <trd>atractiu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attrayant</trd>,
    <trd>captivant</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>intryguj&aogonek;cy</trd>,
    <trd>zajmuj&aogonek;cy</trd>,
    <trd>przyci&aogonek;gaj&aogonek;cy uwag&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="kapt.milit0ito">
  <kap>milit<tld/>ito</kap>
  <snc mrk="kapt.milit0ito.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Batalinto, kiun malamikoj kaptis kaj tenas mallibera:
      <ekz>
        klopodi informi la familiojn de milit<tld/>itoj pri iliaj filoj
        en la kontra&ubreve;a lando
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 21a, p. 56a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_v;&c_a;&c_je;&c_n;&c_n;&c_a;&c_p;&c_a;&c_l;&c_o;&c_n;&c_n;&c_y;
  </trd>
  <trd lng="ca">presoner de guerra</trd>
  <trd lng="cs">válečný zajatec</trd>
  <trd lng="de">Kriegsgefangener</trd>
  <trd lng="en">prisoner of war</trd>
  <trd lng="fr">prisonnier de guerre</trd>
  <trd lng="hu">hadifogoly</trd>
  <trd lng="pl">jeniec<klr> wojenny</klr></trd>
  <trd lng="ru">
  &c_v;&c_o;&c_je;&c_n;&c_n;&c_o;&c_p;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;
  </trd>
  <trd lng="sk">vojnový zajatec</trd>
</drv>
