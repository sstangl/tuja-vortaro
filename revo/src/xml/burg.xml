<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: burg.xml,v 1.6 2015/09/21 13:10:15 revo Exp $">
<kap>
  <rad>burg</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="burg.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="burg.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fortika
      <ref tip="super" cel="urb.0o.ARKI">urbo</ref>,
      &scirc;irmata de murego:
      <ekz>
        la mezepoka <tld/>o de dukoj de Saint-Omer
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>burg</trd>,
    <trd>fortified city</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&ecaron;ste&ccaron;ko</trd>,
    <trd>opevn&ecaron;n&yacute; hrad</trd>
  </trdgrp>
  <trd lng="fr">place forte</trd>
  <trd lng="nl">burcht</trd>
  <trd lng="no">borg</trd>
  <trd lng="pl">twierdza</trd>
  <trd lng="sk">hradn&yacute; ka&scaron;tie&lcaron;</trd>
</drv>
</art>

