<vortaro>
<art mrk="$Id: magnez2.xml,v 1.11 2014/01/09 10:10:18 revo Exp $">
<kap>
  <rad>magnezit</rad>/o
</kap>
<drv mrk="magnez2.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MIN</uzo>
    <dif>
      MgCO<sub>3</sub>. Natura magnezia karbonato.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_e;&c_z;&c_ib;&c_t;</trd>
  <trd lng="ca">magnesita</trd>
  <trd lng="fr">magn&eacute;site</trd>
  <trd lng="hu">magnezit</trd>
  <trdgrp lng="nl">
    <trd>magneziet</trd>,
    <trd>magnesiumcarbonaat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>magnezyt</trd>,
    <trd>w&eogonek;glan magnezu</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>magnesita</trd>,
    <trd>giobertita</trd>,
    <trd>espato-de-magn&eacute;sio</trd>    
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_t;</trd>
</drv>
</art>
