<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: front.xml,v 1.24 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>front</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="front.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="front.0o.1">
    <dif>
      Anta&ubreve;a, la&ubreve;lar&gcirc;a, kvaza&ubreve;frunta parto de objekto:
      <ekz>
        <uzo tip="fak">ARKI</uzo>
        <ind><tld/>o de konstrua&jcirc;o</ind>, de palaco;
        <trd lng="hu">homlokzat</trd>
      </ekz>
      <ekz>
        <ind><tld/>a</ind> fenestro;
        <trd lng="fr">frontal</trd>
        <trd lng="nl">frontaal</trd>
        <trd lng="hu">homlokzati ablak</trd>
      </ekz>
      <ekz>
        <ind><tld/>artikolo</ind> de gazeto
        <klr>(tiu &ccirc;e la plej anta&ubreve;a loko; &ccirc;efartikolo.)</klr>
        <trd lng="nl">hoofdartikel</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fasad.0o">fasado</ref>,
      <ref cel="fronto.0o">frontono</ref>;
      ne konfuzu kun:
      <ref cel="frunt.0o">frunto</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>front</trd>,
      <trd>prz&oacute;d</trd>,
      <trd>czo&lstroke;o</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>fronta</trd>,
    <trd>průčelí</trd>,
    <trd>přední část</trd>,
    <trd>čelo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Front</trd>,
    <trd>Vorderseite</trd>,
    <trd>Stirnseite</trd>
  </trdgrp>
  <trd lng="en">front</trd>
  <trd lng="es">frente</trd>
  <trdgrp lng="fr">
    <trd>front<klr tip="ind"> (partie ant&eacute;rieure)</klr></trd>,
    <trd>avant<klr tip="ind"> (ligne de front)</klr></trd>,
    <trd>ligne de front</trd>,
    <trd>bloc<klr tip="ind"> (parti politique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>homlokr&eacute;sz</trd>,
    <trd>eleje</trd>,
    <trd>front</trd>
  </trdgrp><trdgrp lng="hu">
      <trd>arcvonal</trd>,
      <trd>front</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>front</trd>,
    <trd>voorzijde</trd>,
    <trd>voorkant</trd>
  </trdgrp><trd lng="nl">front</trd>
  <trd lng="pl">front</trd>
  <trdgrp lng="pt">
    <trd>frente</trd>,
    <trd>vanguarda</trd>,
    <trd>fachada</trd>,
    <trd>fronte</trd>,
    <trd>frontisp&iacute;cio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">čelná strana</trd>
  <trdgrp lng="sv">
    <trd>front</trd>,
    <trd>framsida</trd>
  </trdgrp>
</drv>

<drv mrk="front.0e">
  <kap><tld/>e</kap>
  <snc mrk="front.0e.KOMUNE">
    <dif>
      Sur la <tld/>a flanko.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>na čele</trd>,
    <trd>čelně</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>an der <ind>Front</ind></trd>,
    <trd>an der <ind>Vorderseite</ind></trd>,
    <trd>stirnseitig</trd>
  </trdgrp>
  <trd lng="en">in front</trd>
  <trd lng="es">enfrente</trd>
  <trdgrp lng="fr">
    <trd>de <ind>front</ind></trd>,
    <trd>frontalement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>az <ind>elej&eacute;n</ind></trd>,
    <trd>az <ind>&eacute;l&eacute;n</ind></trd>
  </trdgrp>
  <trd lng="nl">aan de <ind>voorkant</ind></trd>
  <trdgrp lng="pl">
    <trd>z <ind>przodu</ind></trd>,
    <trd>na <ind>froncie</ind></trd>,
    <trd>na <ind>czele</ind></trd>
  </trdgrp>
  <trd lng="sk">na čelnej strane</trd>
  <trdgrp lng="sv">
    <trd>p&aring; <ind>fronten</ind></trd>,
    <trd>p&aring; <ind>framsidan</ind></trd>
  </trdgrp>
</drv>

<drv mrk="front.0al0e">
  <kap><tld/>al<tld/>e</kap>
  <snc mrk="front.0al0e.KOMUNE">
    <dif>
      Duope, sin <tld/>ante, rigardante unu al la alia:
      <ekz>
        ili staris <tld/>-al-<tld/>e
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u &scirc;i mortu tra-fike?</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="dors.0al0e.KOMUNE">dorsaldorse</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="flank.0al0e.KOMUNE">flankalflanke</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>na čele</trd>,
    <trd>čelně</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affront&eacute;<klr> (adv.)</klr></trd>,
    <trd>face &agrave; face<klr> (adv.)</klr></trd>
  </trdgrp>
  <trd lng="sk">na čelnej strane</trd>
</drv>

<drv mrk="front.0e.al">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="front.0e.al.kie">
    <dif>
      <ref tip="super" cel="kontraux.0.kie">Kontra&ubreve;</ref>, kun sia
      <tld/>o, fasado rigardanta al:
      <ekz>
        <tld/>e al mi, sur la hamaka deklivo, mi apogis la kajeran
        taglibron
        <fnt><aut>W. Schad</aut>, <vrk>Orinoko-delto: Indianoj,
        mar&ccirc;oj, arbarego</vrk>, <bib>Monato</bib>,
        <lok>2000:11, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>frontal</trd>,
      <trd>frontseitig</trd>,
      <trd>stirnseitig</trd>
    </trdgrp>
    <trd lng="en">facing</trd>
    <trd lng="fr">en face de</trd>
    <trdgrp lng="hu">
      <trd>szemben</trd>,
      <trd>&aacute;tellenben</trd>,
      <trd>vizav&iacute;</trd>
    </trdgrp>
    <trd lng="nl">tegenover</trd>
    <trdgrp lng="pl">
      <trd>naprzeciwko</trd>,
      <trd>twarz&aogonek; <klr>do</klr></trd>,
      <trd>frontem <klr>do</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_i;&c_c;&c_o;&c_m; &c_k;</trd>
  </snc>
  <snc mrk="front.0e.al.rilate">
    <dif>
      <ref tip="super" cel="kontraux.0.al">Kontra&ubreve;</ref>, reagante al
      la
      &ccirc;eesto de:
      <ekz>
        la konduto de la homo de Neandertalo <tld/>e al la morto
        <fnt><aut>R. Roze</aut>, <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>, <lok>1997:11, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>naprzeciwko</trd>,
      <trd>przeciw</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">čelem k</trd>
  <trd lng="fr">face &agrave;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d; &c_l;&c_i;&c_c;&c_o;&c_m;<klr tip="amb">
  (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">čelom k</trd>
</drv>

<drv mrk="front.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="front.0i.KOMUNE">
    <dif>
      Sin turni prezentante <tld/>on kontra&ubreve; io a&ubreve; iu por batali:
      <ekz>
        <tld/>i kontra&ubreve; malamikon, dan&gcirc;eron, malbon&scirc;ancon;
      </ekz>
      <ekz>
        al kiu direkto via domo <tld/>as?
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://tatoeba.org/epo/sentences/show/483188">
          Frazo n-ro 483188</url></vrk>,
          <lok>Tatoeba, 2010</lok>
        </fnt>
      </ekz>
      <ekz>
        FIFA <klr>...</klr> <tld/>as akuzojn de koruptado
        <fnt>
          <aut>-</aut>,
          <vrk>Sindikataj nova&jcirc;o</vrk>,
          <lok><url
          ref="http://www.slea.se/informilo_1_2014_PDFversion.pdf">
          SLEA Informilo, 2014-03 (1)</url>,
          </lok>
        </fnt> 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>být naproti</trd>,
    <trd>čelit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Front machen (gegen)</trd>,
    <trd>die <ind>Stirn bieten</ind></trd>
  </trdgrp>
  <trd lng="en">face toward</trd>
  <trd lng="es"><ind>enfrentar</ind>se</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>face</ind> (&agrave;)</trd>,
    <trd>&ecirc;tre <ind>tourn&eacute;</ind> (vers)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szemben <ind>&aacute;ll</ind></trd>,
    <trd>szembefordul</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>hoofd</ind> bieden aan</trd>,
    <trd>met de <ind>voorzijde</ind> staan naar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sta&cacute; <ind>naprzeciw</ind></trd>,
    <trd>patrze&cacute; <klr>(w kierunku czego&sacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_l;&c_i;&c_c;&c_o;&c_m; &c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>byť naproti</trd>,
    <trd>postaviť sa zoči voči</trd>
  </trdgrp>
</drv>

<drv mrk="front.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="front.al0i.KOMUNE">
    <dif>
      <tld lit="F"/>i kontra&ubreve;:
      <ekz>
        al<tld/>i dan&gcirc;eron, malsanon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">čelit</trd>
  <trdgrp lng="de">
    <trd>gegen&uuml;berstehen</trd>,
    <trd>konfrontiert sein (mit)</trd>
  </trdgrp>
  <trd lng="en">confront</trd>
  <trdgrp lng="es">
    <trd>enfrentar</trd>,
    <trd>afrontar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affronter</trd>,
    <trd>faire <ind>face</ind> (&agrave;)</trd>,
    <trd>se <ind>mesurer</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szembesz&aacute;ll</trd>,
    <trd>felveszi a harcot</trd>,
    <trd>szembes&uuml;l</trd>
  </trdgrp>
  <trd lng="nl">confronteren met</trd>
  <trdgrp lng="pl">
    <trd>stawa&cacute; <ind>naprzeciw</ind></trd>,
    <trd>stawia&cacute; <ind>czo&lstroke;a</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">čeliť</trd>
  <trd lng="sv">konfrontera</trd>
</drv>
</art>
