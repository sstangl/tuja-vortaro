<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ran.xml,v 1.28 2015/07/04 08:37:42 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ran</rad>/o
</kap>
<drv mrk="ran.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="ran.0o.komune">
     <uzo tip="stl">KOMUNE</uzo>
     <dif>
       Iu el <tld/>edoj; <ref tip="lst" cel="amfibi.0o"
       lst="voko:amfibioj">amfibio</ref>, senvostulo kun horizontalaj
       pupiloj kaj dentoj.
     </dif>
     <ref tip="vid" cel="buf.0o">bufo</ref>
   </snc>
   <snc mrk="ran.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
       tip="malprt"
       cel="raned.0oj"><tld/>edoj</ref>
       (<trd lng="la">Rana</trd>).
     </dif>
     <trd lng="pl">&zdot;aba</trd>
   </snc>
   <bld lok="../bld/ran2.jpg">
     <ind>akvo<tld/>o</ind>
     <trd lng="hu">kecskeb&eacute;ka</trd>
     <trd lng="la">Rana esculenta</trd>
     <trd lng="es">Rana de agua</trd>
   </bld>
   <trd lng="be">&c_zh;&c_a;&c_b;&c_a;</trd>
   <trd lng="de">Frosch</trd>
   <trd lng="en">frog</trd>
   <trd lng="es">rana</trd>
   <trd lng="fr">grenouille</trd>
   <trd lng="he">&tsadi;&pe;&resh;&dalet;&ayin;</trd>
   <trd lng="hu">b&eacute;ka</trd>
   <trd lng="id">katak</trd>
   <trdgrp lng="nl">
     <trd>kikker</trd>,
     <trd>kikvors</trd>
   </trdgrp>
   <trd lng="pt">r&atilde;</trd>
   <trd lng="ru">&c_l;&c_ja;&c_g;&c_u;&c_sh;&c_k;&c_a;</trd>
   <trd lng="sv">groda</trd>
  <trd lng="sk">žaba</trd>
  <trdgrp lng="cs">
    <trd>skokan</trd>,
    <trd>žába</trd>
  </trdgrp>
</drv>
</art>
