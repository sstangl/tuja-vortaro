<vortaro>
<art mrk="$Id: preskr1.xml,v 1.11 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>preskript</rad>/o
</kap>
<drv mrk="preskr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Rajtperdo per tempopaso.
    </dif>
    <refgrp tip="vid">
      <ref cel="uzukap.0o">uzukapo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">promlčení</trd>
  <trd lng="de">Verj&auml;hrung</trd>
  <trd lng="hu">el&eacute;v&uuml;l&eacute;s</trd>
  <trd lng="nl">verjaring</trd>
  <trd lng="pt">prescri&ccedil;&atilde;o <klr>(caducidade)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_r;&c_o;&c_k; &c_d;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">premlčanie</trd>
</drv>

<drv mrk="preskr1.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i ne plu postulebla pro tempopaso.
    </dif>
  </snc>
  <trd lng="de">verj&auml;hren</trd>
  <trd lng="hu">el&eacute;v&uuml;l</trd>
  <trd lng="nl">verjaren</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_k;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_p;&c_o; &c_d;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
</drv>
</art>
