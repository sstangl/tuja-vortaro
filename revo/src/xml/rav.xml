<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rav.xml,v 1.17 2015/07/04 08:37:42 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>rav</rad>/i
</kap>
<drv mrk="rav.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       &Ccirc;armegi estigante entuziasman admiron:
       <ekz>nin &ccirc;iam <tld/>as, kion ni ne havas
<fnt><bib>PrV</bib></fnt>;</ekz>
       <ekz>
         tiuj arkitekturaj juveloj <tld/>as la okulojn;
       </ekz>
       <ekz>
         li estis tute <tld/>ita de la gracia apero, de tia afabla
         akcepto;
       </ekz>
       <ekz>
         <tld/>(ant)a junulino, pejza&gcirc;o<fnt>Z</fnt>,
       </ekz>
       <ekz>
         muziko;
       </ekz>
       <ekz>
         <tld/>e danci;
       </ekz>
       <ekz>
         lia viza&gcirc;o esprimis <tld/>itecon<fnt>Z</fnt>,
       </ekz>
       <ekz>
         <tld/>i&gcirc;on
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="ekstaz.0o">ekstazo</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_u; 
<ind>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
   </trdgrp>
  <trd lng="ca">encantar</trd>
  <trdgrp lng="cs">
    <trd>nadchnout</trd>,
    <trd>okouzlit</trd>,
    <trd>uchvátit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>bezaubern</trd>,
     <trd>entz&uuml;cken</trd>,
     <trd>hinrei&szlig;en</trd>,
     <trd>mitrei&szlig;en</trd>
   </trdgrp>
  <trd lng="en">delight</trd>
  <trd lng="es">encantar</trd>
  <trdgrp lng="fr">
     <trd>ravir</trd>,
     <trd>enchanter</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>elb&udblac;v&ouml;l</trd>,
    <trd>elb&aacute;jol</trd>
  </trdgrp>
  <trd lng="nl">verrukken</trd>
  <trdgrp lng="pl">
    <trd>zachwyca&cacute;</trd>,
    <trd>zachwyci&cacute;</trd>,
    <trd>oczarowywa&cacute;</trd>,
    <trd>oczarowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>encantar</trd>,
    <trd>extasiar</trd>,    
    <trd>arrebatar</trd>,
    <trd>embevecer</trd>,
    <trd>enlear</trd>,
    <trd>maravilhar</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_v;&c_o;&c_s;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; 
&c_v;&c_o;&c_s;&c_t;&c_o;&c_r;&c_g;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>nadchnúť</trd>,
    <trd>očariť</trd>
  </trdgrp>
</drv>
</art>
