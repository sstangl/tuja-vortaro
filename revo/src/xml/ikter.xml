<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ikter.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
    <kap>
      <rad>ikter</rad>/o
    </kap>

    <drv mrk="ikter.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">MED</uzo>
      <snc mrk="ikter.0o.flavigxo">
        <ref tip="dif" cel="flav.0igxo.MED">Flavi&gcirc;o<sncref/></ref>
      </snc>
      <snc mrk="ikter.0o.malsano">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="san.flavmal0o">Flavmalsano</ref>
        <ekz>
          simpla, infekta, katara, obstrukca <tld/>o.
        </ekz>
        <trd lng="de">Gelbsucht</trd>
      </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="ca">icter&iacute;cia</trd>
  <trdgrp lng="cs">
    <trd>ikterus</trd>,
    <trd>žloutenka</trd>
  </trdgrp>
  <trd lng="en">jaundice</trd>
  <trd lng="es">ictericia</trd>
  <trdgrp lng="fr">
        <trd>ict&egrave;re</trd>,
        <trd>jaunisse</trd>
      </trdgrp>
  <trd lng="hu">s&aacute;rgas&aacute;g</trd>
  <trd lng="nl">geelzucht</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;taczka</trd>
  <trd lng="pt">icter&iacute;cia</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="sk">žltačka</trd>
  <trd lng="sv">gulsot</trd>
</drv>
  </art>
