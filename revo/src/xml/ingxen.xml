<art mrk="$Id: ingxen.xml,v 1.14 2009/08/08 16:32:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>in&gcirc;enier</rad>/o
</kap>
<drv mrk="ingxen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu per helpo de sciencaj metodoj kondukas kaj direktas
      teknikajn laborojn (konstruadon de ma&scirc;inoj, vojoj, pontoj ktp,
      a&ubreve; ekspluatadon de terbieno):
      <ekz><tld/>o pri agronomio, minejoj, metalurgio;</ekz>
      <ekz><tld/>o en fabriko de &hcirc;emia&jcirc;oj.</ekz>
    </dif>
  </snc>
  <trd lng="cs">in&zcaron;en&yacute;r</trd>
  <trd lng="de">Ingenieur</trd>
  <trd lng="en">engineer</trd>
  <trd lng="es">ingeniero</trd>
  <trd lng="fr">ing&eacute;nieur</trd>
  <trd lng="hu">m&eacute;rn&ouml;k</trd>
  <trd lng="nl">ingenieur</trd>
  <trd lng="pt">engenheiro</trd>
  <trd lng="ru">&c_i;&c_n;&c_zh;&c_je;&c_n;&c_je;&c_r;</trd>
  <trd lng="sv">ingenj&ouml;r</trd>
</drv>
</art>
