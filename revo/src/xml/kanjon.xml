<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanjon.xml,v 1.19 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kanjon</rad>/o
</kap>

<drv mrk="kanjon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanjon.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta kaj profunda <ref tip="super" cel="val.0o.GEOG">valo</ref>,
      kies bordoj estas krutegaj:
      <ekz>
        li a&ubreve;dacis deklari, ke la <tld/>o de Kolorado estas pli bela ol
        la
        valo de la Tajo en Hispanio
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="ravin.0o">ravino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="ca">congost, engorjat</trd>
  <trd lng="cs">kaňon</trd>
  <trd lng="de">Canyon</trd>
  <trd lng="en">canyon</trd>
  <trd lng="es">ca&ntilde;on <klr>(geog.)</klr></trd>
  <trdgrp lng="fr">
    <trd>canyon</trd>,
    <trd>ca&ntilde;on</trd>,
    <trd>gorges</trd>
  </trdgrp>
  <trd lng="hu">kanyon</trd>
  <trdgrp lng="nl">
    <trd>canyon</trd>,
    <trd>ca&ntilde;on</trd>,
    <trd>ravijn</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">kaňon</trd>
</drv>
</art>
