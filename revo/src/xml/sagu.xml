<vortaro>
<art mrk="$Id: sagu.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sagu</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sagu.0o">
  <kap><tld/>o</kap>
  <snc mrk="sagu.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Nutra grio eltirita el medolo de iuj palmarboj.
    </dif>
    <refgrp tip="vid">
      <ref cel="tapiok.0o.KUI">tapioko</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">sag&uacute;</trd>
  <trd lng="cs">ságo</trd>
  <trdgrp lng="de">
    <trd>Sago</trd>,
    <trd>Palmst&auml;rke</trd>
  </trdgrp>
  <trd lng="en">sago</trd>
  <trd lng="es">sag&uacute;</trd>
  <trd lng="fr">sagou</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;g&oacute;</trd>,
    <trd>sz&aacute;g&oacute;liszt</trd>
  </trdgrp>
  <trd lng="nl">sago</trd>
  <trd lng="pl">sago</trd>
  <trdgrp lng="pt">
   <trd>sagu</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_g;&c_o;</trd>
  <trd lng="sk">ságo</trd>
</drv>
</art>
