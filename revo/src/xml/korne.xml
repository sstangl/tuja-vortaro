<art mrk="$Id: korne.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>korne</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korne.0o">
  <kap><tld/>o</kap>
  <snc mrk="korne.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Travidebla anta&ubreve;a membrano de okulo:
      <ekz>
        &ccirc;e iuj krustuloj <klr>...</klr> estas duobla <tld/>o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>
  <trd lng="ca">c&ograve;rnia</trd>
  <trdgrp lng="cs">
    <trd>kornea</trd>,
    <trd>oční rohovka</trd>
  </trdgrp>
  <trd lng="de">Hornhaut</trd>
  <trd lng="en">cornea</trd>
  <trd lng="es">c&oacute;rnea</trd>
  <trd lng="fr">corn&eacute;e<klr> (anatomie)</klr></trd>
  <trd lng="he">&qof;&resh;&nun;&yod;&tav;</trd>
  <trd lng="hu">szaruh&aacute;rtya</trd>
  <trd lng="it">cornea</trd>
  <trdgrp lng="nl">
    <trd>hoornvlies</trd>,
    <trd>cornea</trd>
  </trdgrp>
  <trd lng="pl">rog&oacute;wka</trd>
  <trd lng="pt">c&oacute;rnea</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_g;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kornea</trd>,
    <trd>očná rohovka</trd>
  </trdgrp>
</drv>
</art>
