<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: arbust.xml,v 1.22 2015/07/04 08:37:30 revo Exp $">
    <kap>
      <ofc>6</ofc>
      <rad>arbust</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="arbust.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
        <dif>
          Ligna, <ref tip="super" cel="plant.dauxra0o">da&ubreve;ra
          planto</ref>,
          disbran&ccirc;i&gcirc;anta &ccirc;e la bazo, sed
          pli malgranda ol <ref tip="vid" cel="arbed.0o">arbedo</ref>:
          <ekz>
            kaprifolio, berberiso, rubuso, eriko estas <tld/>oj;
          </ekz><ekz>
            kaj ili venos kaj sidi&gcirc;os &ccirc;iuj en la dezertigitaj valoj
            kaj en la fendoj de la rokoj kaj sur &ccirc;iuj <tld/>oj kaj
            sur &ccirc;iuj pik<ref tip="super"
            cel="arb.0etajxo">arbeta&jcirc;o</ref>j
            <fnt><bib>MT</bib>, <lok>&Jes; 7:19</lok></fnt>;
          </ekz><ekz>
            se ne tiam eliros fajro el la dorn<tld/>o kaj forbruligos
            la cedrojn de Lebanon
            <fnt><bib>MT</bib>, <lok>&Jug; 9:15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_k;&c_u;&c_s;&c_t;</trd>, 
        <trd>&c_k;&c_u;&c_s;&c_t;&c_o;&c_w;&c_je;</trd>, 
        <trd>&c_h;&c_m;&c_y;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
      </trdgrp>
  <trd lng="cs">ke≈ô</trd>
  <trdgrp lng="de">
        <trd><klr tip="ind">(kleiner)</klr> Strauch</trd>, 
        <trd>Zwergstrauch <klr>(rekta)</klr></trd>,
        <trd>Spalierstrauch <klr>(ku&scirc;anta)</klr></trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>bush</trd>,
        <trd>shrub</trd>,
        <trd>shrubbery</trd>
      </trdgrp>
  <trd lng="es">arbusto</trd>
  <trd lng="fr">arbuste</trd>
  <trdgrp lng="hu">
        <trd>bokor</trd>,
        <trd>cserje</trd>
      </trdgrp>
  <trd lng="nl">struik</trd>
  <trdgrp lng="pl">
      <trd>krzew</trd>,
      <trd>krzak</trd>
      </trdgrp>
  <trd lng="pt">arbusto</trd>
  <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_s;&c_t;</trd>,
        <trd>&c_k;&c_u;&c_s;&c_t;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trd lng="sk">poloker</trd>
</drv>
