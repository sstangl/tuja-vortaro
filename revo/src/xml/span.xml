<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: span.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>span</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="span.0o">
  <kap><tld/>o</kap>
  <snc mrk="span.0o.KOMUNE">
    <dif>
      Spaco inter du &ccirc;irka&ubreve;prena&jcirc;oj.
    </dif>
    <subsnc mrk="span.0o.MEZ">
      <dif>
        Plej granda diso inter dika kaj eta fingro, uzata kiel <ref
        tip="lst" cel="unu.mezur0o"
        lst="voko:longoj">mezurunuo</ref> 
        de longo:
        <ekz>
          mi vidis min tiel proksima al la &ccirc;ielo, ke mi apena&ubreve;
          distancis
          de &gcirc;i unu <tld/>on kaj duonon
          <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 41a, p.
          622a</lok></fnt>
        </ekz>
      </dif>
      <trd lng="fa">&a_w;&a_j;&a_b;</trd>
      <trd lng="hu">arasz</trd> 
      <trd lng="id">jengkal</trd>
      <trdgrp lng="nl">
        <trd>span<klr tip="ind"> (oude lengtemaat)</klr></trd>,
        <trd>handbreedte</trd>
      </trdgrp>     
    </subsnc>
    <subsnc mrk="span.0o.ARKI">
      <uzo tip="fak">ARKI</uzo>
      <dif>
        Diso inter du subportaj kolonoj de arka&jcirc;o.
      </dif>
      <trdgrp lng="hu">
        <trd>feszt&aacute;v</trd>,
        <trd>oszlopk&ouml;z</trd>
        <trd>&iacute;vny&iacute;l&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>spanwijdte</trd>,
        <trd>vlucht<klr tip="ind"> (spanwijdte)</klr></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="cs">rozpětí <klr>(oblouku)</klr></trd>
  <trd lng="de">Spanne</trd>
  <trdgrp lng="fr">
    <trd>empan</trd>,
    <trd>port&eacute;e<klr> (d'une arche)</klr></trd>
  </trdgrp>
  <trd lng="sk">rozpätie oblúka <klr>(mostového)</klr></trd>
</drv>
</art>
