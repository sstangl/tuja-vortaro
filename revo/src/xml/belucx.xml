<kap>
  <rad>belu&ccirc;</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="belucx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de iran-lingva popolo vivanta en Pakistano, sudorienta Irano,
      Afganujo, unui&gcirc;inta en la 18a jarcento.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_u;&c_d;&c_zh;</trd>
  <trd lng="de">Belutsche</trd>
  <trd lng="fr">Baloutche<klr> (subst.)</klr></trd>
  <trd lng="pt">Bal&uacute;chi<klr> (subst.)</klr></trd>
  <trd lng="hu">beludzs</trd>
  <trd lng="nl">Baloetsj</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_u;&c_d;&c_zh;</trd>
</drv>

<drv mrk="belucx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_u;&c_d;&c_zh;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">belutschisch</trd>
  <trd lng="fr">baloutche<klr> (adj.)</klr></trd>
  <trd lng="pt">bal&uacute;chi<klr> (adj.)</klr></trd>
  <trd lng="hu">beludzs</trd>
  <trd lng="nl">Baloetsjisch</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_u;&c_d;&c_zh;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

