<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kapok.xml,v 1.14 2015/07/04 08:37:36 revo Exp $">
<kap>
<rad>kapok</rad>/o
</kap>
<drv mrk="kapok.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plantharoj el la interna frukt&scirc;elo de la 
      <ref tip="malprt" cel="kapok.0ujo"><tld/>ujo</ref>,
      uzataj por remburi savringojn, na&gcirc;zonojn, 
      kusenojn, matracojn, ktp.
    </dif>
  </snc>
  <trd lng="ca">capoc</trd>
  <trd lng="cs">kapok</trd>
  <trd lng="de">Kapok</trd>
  <trd lng="en">kapok</trd>
  <trd lng="fr">kapok</trd>
  <trd lng="hu">kapok<klr tip="ind"> (sz&aacute;las anyag)</klr></trd>
  <trd lng="nl">kapok</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_k;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_p;&c_u;&c_h;)</klr></trd>
  <trd lng="sk">kapok</trd>
</drv>
<drv mrk="kapok.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="bombak1.0oj">bombakacoj</ref>
      (<trd lng="la">Ceiba</trd>), azia arbo.
    </dif>
  </snc>
  <trd lng="ca">capoquer</trd>
  <trd lng="de">Kapokbaum</trd>
  <trd lng="en">kapok tree</trd>
  <trd lng="fr">kapokier</trd>
  <trdgrp lng="hu">
    <trd>kapok(fa)</trd>,
    <trd>pamutfa</trd></trdgrp>
  <trd lng="nl">kapokboom</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_k;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>

