<vortaro>
<art mrk="$Id: telepa.xml,v 1.14 2014/01/05 09:10:18 revo Exp $">
<kap>
  <ofc>9</ofc><rad>telepati</rad>/o
</kap>
<drv mrk="telepa.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">PSI</uzo>
    <dif>
      Metapsikia fenomeno, konsistanta en senpera komuniki&gcirc;o de du
      mensoj, je pli malpli granda distanco, per aliaj rimedoj ol la
      ordinaraj sensa&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="de">Telepathie</trd>
  <trd lng="en">telepathy</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_r;&a_A_madda;&f_g;&a_A;&a_h;&f_y;</trd>,
    <trd>&a_t;&a_kasra;&a_l;&a_h;&zwnj;&f_p;&a_A;&a_t;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;l&eacute;pathie</trd>
  <trdgrp lng="hu">
    <trd>gondolat&aacute;tvitel</trd>,
    <trd>telep&aacute;tia</trd></trdgrp>
  <trd lng="nl">telepathie</trd>
  <trd lng="pl">telepatia</trd>
  <trdgrp lng="pt">
    <trd>telepatia</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trd lng="sk">telepatia</trd>
</drv>
</art>
