<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxtom2.xml,v 1.10 2011/12/29 17:10:14 revo Exp $">
<kap>
<rad>a&ubreve;tomorfi</rad>/o
</kap>
<drv mrk="auxtom2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bijekc.0a">Bijekcia</ref>
      <ref tip="super" cel="endomo.0o">endomorfio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_r;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="en">automorphism</trd>
  <trd lng="de">Automorphismus</trd>
  <trd lng="fr">automorphisme</trd>
  <trd lng="hu">automorfizmus</trd>
  <trd lng="it">automorfismo</trd>
  <trd lng="pl">automorfizm</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="auxtom2.interna0o">
  <kap>interna <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(en <ref cel="grup.0o.MAT">grupo</ref>)</klr>
      <ref tip="super" cel="auxtom2.0o"><tld lit="A"/>o</ref> de la tipo
      
      <frm><k>f</k>(<k>x</k>) =
      <k>a</k>.<k>x</k>.<k>a</k><sup>-1</sup></frm>
      por iu <frm><k>a</k></frm>.
    </dif>
    <ref tip="vid" cel="konjug.0ita.subgrupo">konjugita.</ref>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_y; <ind>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_r;&c_f;&c_ib;&c_z;&c_m;</ind></trd>
  <trd lng="en">inner <ind>automorphism</ind></trd>
  <trd lng="de">innerer <ind>Automorphismus</ind></trd>
  <trd lng="fr"><ind>automorphisme</ind> int&eacute;rieur</trd>
  <trd lng="hu">bels&odblac; <ind>automorfizmus</ind></trd>
  <trd lng="it"><ind>automorfismo</ind> interno</trd>
  <trd lng="pl">automorfizm wewn&eogonek;trzny</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j; <ind>&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</ind></trd>
</drv>
</art>
