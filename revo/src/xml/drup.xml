<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: drup.xml,v 1.14 2015/07/04 08:37:32 revo Exp $">
<kap>
<rad>drup</rad>/o
</kap>
<drv mrk="drup.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.ferm0o">Fermfrukto</ref>, kies
      perikarpo tavoli&gcirc;as al interna malmola kerno kaj
      ekstera suke karna (pruno, &ccirc;erizo) a&ubreve; ledeca
      (juglando, migdalo) parto.
    </dif>
    <ref tip="sin" cel="kern.0ofrukto">kernofrukto</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>drupa</trd>,
    <trd>peckovice</trd>
  </trdgrp>
  <trd lng="de">Steinfrucht</trd>
  <trdgrp lng="en">
    <trd>drupe</trd>,
    <trd>stone fruit</trd>
  </trdgrp>
  <trd lng="fr">drupe</trd>
  <trd lng="hu">csonth&eacute;jas gy&uuml;m&ouml;lcs</trd>
  <trd lng="pl">pestkowiec</trd>
  <trd lng="pt">drupa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j;
&c_p;&c_l;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">kôstkovité ovocie</trd>
</drv>
</art>
