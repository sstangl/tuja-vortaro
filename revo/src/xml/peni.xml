<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: peni.xml,v 1.7 2013/09/22 19:12:02 revo Exp $">
<kap>
  <rad>peni</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="peni.0o">
  <kap><tld/>o</kap>
  <snc mrk="peni.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o.EKON" lst="voko:monunuoj">Mona
      subunuo</ref> 
      egala al centono de kanada <ref tip="malprt" 
      cel="dolar.0o.EKON">dolaro</ref>, brituja <ref tip="malprt" 
      cel="pund.0o.EKON">pundo</ref>, finlanda <ref tip="malprt" 
      cel="mark2.0o.EKON">marko</ref>...; tiomvalora <ref tip="super" 
      cel="mon.0ero.EKON">monero</ref>:
      <ekz>
        se Dio lasus nin trovi sur strato sepdek kvin <tld/>ojn, anka&ubreve; 
        ni a&ccirc;etus printempajn paltojn
        <fnt>
          <aut>T. Pakkala, trad. A. Karkiainen</aut>,
          <vrk>Mensogulinoj</vrk>,
          <bib>LOdE</bib>,
          <lok>2001:3 (77)</lok>
        </fnt>.
      </ekz>
      <ref tip="sin" cel="penc.0o">penco</ref>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_n;&c_i;</trd>
  <trd lng="de">Penny</trd>
  <trd lng="en">penny</trd>
  <trd lng="fa">&f_p;&a_n;&f_y;</trd>
  <trd lng="fr">penny</trd>
  <trd lng="nl">penny</trd>
  <trd lng="pl">pens</trd>
  <trd lng="pt">p&ecirc;ni</trd>
</drv>
</art>

