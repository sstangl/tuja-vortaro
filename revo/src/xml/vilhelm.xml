<kap>
  <ofc>*</ofc>
  <rad>Vilhelm</rad>/o
</kap>
<drv mrk="vilhelm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        Erneston, <tld/>on <klr>...</klr> iliaj gepatroj nomas Erne&ccirc;jo
        <klr>...</klr>, Vilhel&ccirc;jo
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">William</trd>
  <trd lng="be">&c_V;&c_ib;&c_l;&c_mol;&c_g;&c_je;&c_l;&c_mol;&c_m;</trd>  
  <trd lng="bg">&c_U;&c_i;&c_l;&c_ja;&c_m;</trd>
  <trdgrp lng="br">
    <trd>Gwilherm</trd>,
    <trd>Lom</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Guillem</trd>,
    <trd>Wilhelm</trd>
  </trdgrp>
  <trd lng="de">Wilhelm</trd>
  <trd lng="fr">Guillaume</trd>
  <trd lng="hu">Vilmos</trd>
  <trd lng="it">Guglielmo</trd>
  <trd lng="nl">Willem</trd>
  <trd lng="pl">Wilhelm</trd>
  <trdgrp lng="ru">
    <trd>&c_V;&c_i;&c_l;&c_mol;&c_g;&c_je;&c_l;&c_mol;&c_m;</trd>,
    <trd>&c_V;&c_i;&c_l;&c_mol;&c_ja;&c_m;</trd>
  </trdgrp>
</drv>
</art>

