<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolibr.xml,v 1.19 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolibr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolibr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolibr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tre malgranda birdo kun arkoforma beko kaj brile multkoloraj 
      plumoj, vivanta en tropika Ameriko, el familio tro&hcirc;iledoj (<trd 
      lng="la">Trochilidae</trd>).
    </dif>
    <refgrp tip="sin">
      <ref cel="bird.musx0o.ZOO">mu&scirc;birdo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_y;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="ca">colibr&iacute;</trd>
  <trd lng="cs">kolibřík</trd>
  <trd lng="de">Kolibri</trd>
  <trd lng="en">humming bird</trd>
  <trd lng="es">colibr&iacute;</trd>
  <trd lng="fa">&a_m;&f_g;&a_s;&zwnj;&a_m;&a_r;&a_gh;</trd>
  <trdgrp lng="fr">
    <trd>colibri</trd>,
    <trd>oiseau-mouche</trd>
  </trdgrp>
  <trd lng="hu">kolibri</trd>
  <trd lng="nl">kolibri</trd>
  <trd lng="pl">koliber</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="sk">kolibrík</trd>
  <trd lng="tr">sinekku&scedil;u</trd>
</drv>
</art>
