<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lamen.xml,v 1.17 2015/04/20 18:10:15 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>lamen</rad>/o
</kap>
<drv mrk="lamen.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Mallar&gcirc;a, tre maldika, plejofte metala plato:
      <ekz>
        &scirc;tala, ligna <tld/>o;
      </ekz>
      <ekz>
        balen<tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="balen.0ajxo">balena&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_t;&c_a; <klr tip="amb">(&c_t;&c_o;&c_n;&c_k;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_s;&c_t; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_jo;&c_v;&c_y; &c_ib; &c_d;&c_a;
    &c_g;.&c_p;.)</klr></trd>,
    <trd>&c_a;&c_r;&c_k;&c_u;&c_sh; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_jo;&c_v;&c_y; &c_ib; &c_d;&c_a;
    &c_g;.&c_p;.)</klr></trd>
  </trdgrp>
  <trd lng="ca">l&agrave;mina</trd>
  <trdgrp lng="de">
     <trd>Pl&auml;ttchen</trd>,
     <trd>Sch&uuml;ppchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lamina</trd>,
    <trd>plate</trd>,
    <trd>layer</trd>
  </trdgrp>
  <trd lng="es">l&aacute;mina</trd>
  <trd lng="fr">lamelle</trd>  
  <trd lng="hu">lemez</trd>
  <trdgrp lng="pt">
    <trd>l&acirc;mina</trd>,
    <trd>chapa</trd>,
    <trd>placa</trd>
  </trdgrp>
  <trd lng="nl">lamel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_s;&c_t; <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;, &c_f;&c_a;&c_n;&c_je;&c_r;&c_n;&c_y;&c_j; &c_i;
    &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lamen.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Platigi en <tld/>ojn a&ubreve; foliojn.
    </dif>
  </snc>
  <trd lng="ca">laminar<klr> (v.)</klr></trd>
  <trdgrp lng="de">
     <trd><klr>in </klr>d&uuml;nne Pl&auml;ttchen <ind>aufspalten</ind></trd>,
     <trd><klr>in </klr>d&uuml;nne Schichten <ind>aufspalten</ind></trd>,
     <trd>Folien auswalzen</trd>,
     <trd>Folien schlagen</trd>
  </trdgrp>
  <trd lng="en">laminate</trd>
  <trd lng="es">laminar</trd>
  <trdgrp lng="hu">
     <trd>lap&iacute;t</trd>,
     <trd>lemezz&eacute; alak&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lamineren</trd>,
    <trd>pletten</trd>
  </trdgrp>
  <trd lng="pt">laminar</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v; &c_t;&c_o;&c_n;&c_k;&c_i;&c_j;
  &c_l;&c_i;&c_s;&c_t;)</klr></trd>
</drv>
</art>
