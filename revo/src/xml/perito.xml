   <ofc>*</ofc>
   <rad>peritone</rad>/o
</kap>
<drv mrk="perito.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ANA</uzo>
   <snc>
     <dif>
       Membrano entenanta la intestojn.
     </dif>
     <trd lng="en">peritoneum</trd>
   </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_je;&c_m;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
     <trd>rouedenn ar bouzello&ugrave;</trd>,
     <trd>rouedenn-gof</trd>
   </trdgrp>
  <trd lng="ca">peritoneu</trd>
  <trdgrp lng="cs">
    <trd>peritoneum</trd>,
    <trd>pobřišnice</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Bauchfell</trd>,
     <trd>Peritoneum</trd>
   </trdgrp>
  <trd lng="es">peritoneo</trd>
  <trdgrp lng="fa">
     <trd>&a_S;&a_f;&a_A;&a_q;</trd>,
     <trd>&f_p;&a_r;&a_d;&a_h;&a_hamza_sure; &a_S;&a_f;&a_A;&a_q;</trd>
   </trdgrp>
  <trd lng="fr">p&eacute;ritoine</trd>
  <trdgrp lng="hu">
     <trd>hash&aacute;rtya</trd>,
     <trd>peritoneum</trd>
   </trdgrp>
  <trd lng="it">peritoneo</trd>
  <trd lng="la">peritoneum</trd>
  <trd lng="nl">buikvlies</trd>
  <trd lng="pl">otrzewna</trd>
  <trd lng="pt">perit&ocirc;nio</trd>
  <trd lng="ru">&c_b;&c_r;&c_ju;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">pobrušnica</trd>
</drv>
