<vortaro>
<art mrk="$Id: cxam.xml,v 1.17 2012/07/30 12:10:46 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>&ccirc;am</rad>/o
  </kap>
  <drv mrk="cxam.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <uzo tip="fak">ZOO</uzo>
    <snc>
      <dif>
        Montara rema&ccirc;besto el fam. antilopoj kies ha&ubreve;to
        servas por fari 
        delikatan ledon.
      </dif>
      <refgrp tip="vid">
      <ref cel="sxam.0o">&scirc;amo</ref>.
      </refgrp>

      <trd lng="be">&c_s;&c_a;&c_r;&c_n;&c_a;</trd>
      <trdgrp lng="ca">
        <trd>isard</trd>,
        <trd>camussa</trd>
      </trdgrp>
      <trdgrp lng="de">
         <trd>Gams</trd>,
         <trd>G&auml;mse</trd></trdgrp>
      <trd lng="en">chamois</trd>
      <trd lng="fr">chamois</trd>
      <trd lng="he">&yod;&ayin;&lamed;</trd>
      <trd lng="hu">zerge</trd>
      <trd lng="pl">kozica<klr> g&oacute;rska</klr></trd>
      <trd lng="pt">camur&ccedil;a</trd>
      <trd lng="ru">&c_s;&c_je;&c_r;&c_n;&c_a;</trd>
      <trd lng="it">camoscio</trd>
    </snc>
  </drv>
</art>

