<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fornel.xml,v 1.7 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fornel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fornel.0o">
  <kap><tld/>o</kap>
  <snc mrk="fornel.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kuiraparato enhavanta <ref tip="prt" cel="bak.0ujo.KUI">bakujon
      </ref> kaj kuirplatojn a&ubreve; flamingojn:
      <ekz>
        via patrino tu&scirc;is la <tld/>on, kaj nun &scirc;i havas 
        vundon sur la  mano
        <fnt>
          <aut>R. Fischer</aut>,
          <vrk><url ref="http://www.esperanto.de/lehrbuch/">
          Esperanto im Dialog</url></vrk>,
          <lok>2007-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_ch;&c_k;&c_a; <klr> (&c_g;&c_o;&c_t;&c_v;&c_a;&c_r;&c_s;&c_k;&c_a;)</klr></trd>
  <trd lng="cs">kuchyňský sporák</trd>
  <trd lng="de">Herd<klr tip="amb"> (K&uuml;che)</klr></trd>
  <trd lng="en">stove</trd>
  <trd lng="fr">cuisini&egrave;re<klr> (&eacute;lectrom&eacute;nager)
  </klr></trd>
  <trd lng="nl">fornuis</trd>
  <trd lng="pl">kuchenka</trd>
  <trd lng="sk">sporák</trd>
</drv>

<drv mrk="fornel.elektro0o">
  <kap>elektro<tld/>o, <var><kap>elektra <tld/>o</kap></var></kap>
  <snc mrk="fornel.elektro0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="fornel.0o.KUI"><tld lit="F"/>o</ref> 
      funkcianta per elektro:
      <ekz>
        nelavitaj teleroj kaj tasegoj sur la lavtablo, seki&gcirc;intaj 
        kaseroloj sur la elektra <tld/>o, uzitaj forkoj kaj kuleroj. 
        Kial ili ne purigis anta&ubreve; ol ekferii?
        <fnt>
          <aut>Saliko</aut>,
          <vrk>En fremda lando</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2006a, numero 3a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Elektroherd</trd>
  <trd lng="en">electric <ind>stove</ind></trd>
  <trd lng="fr">cuisini&egrave;re &eacute;lectrique</trd>
  <trd lng="nl">elektrisch <ind>fornuis</ind></trd>
  <trd lng="pl"><ind>kuchenka</ind> elektryczna</trd>
</drv>

<drv mrk="fornel.gas0o">
  <kap>gas<tld/>o, <var><kap>gasa <tld/>o</kap></var></kap>
  <snc mrk="fornel.gas0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="fornel.0o.KUI"><tld lit="F"/>o</ref> 
      funkcianta per brulgaso:
      <ekz>
        por prepari la man&gcirc;on la unuaj levintoj uzis senprokraste
        la malmultajn gas<tld/>ojn, la aliaj devis alvici&gcirc;i
        <fnt>
          <aut>R. Frey</aut>,
          <vrk><url 
          ref="http://www.esperanto-geneve-regions.info/Roberto3.html">
          La veraj rakontoj de Roberto, 3</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">plynový sporák</trd>
  <trd lng="de">Gasherd</trd>
  <trd lng="en">gas <ind>stove</ind></trd>
  <trd lng="fr">cuisini&egrave;re &agrave; gaz</trd>
  <trd lng="nl">gasfornuis</trd>
  <trd lng="pl"><ind>kuchenka</ind> gazowa</trd>
  <trd lng="sk">plynový sporák</trd>
</drv>

<drv mrk="fornel.karbo0o">
  <kap>karbo<tld/>o, <var><kap>karba <tld/>o</kap></var></kap>
  <snc mrk="fornel.karbo0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="fornel.0o.KUI"><tld lit="F"/>o</ref> 
      funkcianta per bruligo de karbo.
    </dif>
  </snc>
  <trd lng="de">Kohleherd</trd>
  <trd lng="en">charcoal <ind>stove</ind></trd>
  <trd lng="fr">fourneau</trd>
  <trd lng="nl">kolenfornuis</trd>
  <trd lng="pl"><ind>kuchenka</ind> w&eogonek;glowa</trd>
</drv>
</art>

