<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ruz.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ruz</rad>/a
</kap>
<drv mrk="ruz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Artifike kaj lerte trompa:
      <ekz>
        <tld/>a homo;
      </ekz>
      <ekz>
        <tld/>a ago;
      </ekz>
      <ekz>
        e&ccirc; vulpo plej <tld/>a fine estas kaptata
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>miena.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="subtil.0a">subtila</ref>,
      <ref cel="sagac.0a">sagaca</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_t;&c_r;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>chytrácký</trd>,
    <trd>lstivý</trd>,
    <trd>lstný</trd>,
    <trd>prohnaný</trd>,
    <trd>vychytral&yacute;</trd>,
    <trd>úlisný</trd>,
    <trd>úskočný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schlau</trd>,
    <trd>listig</trd>
  </trdgrp>
  <trd lng="en">cunning</trd>
  <trd lng="fr">rus&eacute;</trd>
  <trdgrp lng="he">
    <trd>&ayin;&resh;&mem;&vav;&mem;&yod;</trd>,
    <trd>&ayin;&resh;&vav;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ravasz</trd>,
    <trd>cseles</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slim</trd>,
    <trd>sluw</trd>,
    <trd>listig</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>astucioso</trd>,
    <trd>astuto</trd>,    
    <trd>ardiloso</trd>,    
    <trd>fin&oacute;rio</trd>,    
    <trd>ladino</trd>,    
    <trd>manhoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_t;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_u;&c_k;&c_a;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chytrý</trd>,
    <trd>prefíkaný</trd>,
    <trd>úlisný</trd>,
    <trd>ľstivý</trd>
  </trdgrp>
</drv>

<drv mrk="ruz.0o0ajxo">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc>
    <dif>
      <tld lit="R"/>a rimedo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_y;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>List</trd>,
    <trd>listige Tat</trd>
  </trdgrp>
  <trd lng="fr">ruse</trd>
  <trdgrp lng="he">
    <trd>&tav;&kaf;&samekh;&yod;&samekh;</trd>,
    <trd>&tav;&het;&bet;&vav;&lamed;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>furfang</trd>,
    <trd>csel</trd>,
    <trd>ravaszs&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slimmigheid</trd>,
    <trd>list</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_t;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="ruz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_u;&c_d;&c_r;&c_a;&c_g;&c_je;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>být lstivý</trd>,
    <trd>být vychytralý</trd>,
    <trd>chytračit</trd>
  </trdgrp>
  <trd lng="fr">ruser</trd>
  <trd lng="hu">ravaszkodik</trd>
  <trdgrp lng="nl">
    <trd>slim zijn</trd>,
    <trd>listig zijn</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_i;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>byť ľstivý</trd>,
    <trd>chytrý</trd>,
    <trd>úlisný</trd>
  </trdgrp>
</drv>

<drv mrk="ruz.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Superi per <tld/>o:
      <ekz>
        nun tiu &ccirc;i azeno lin super<tld/>as<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_h;&c_ib;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="cs">přelstít</trd>
  <trd lng="de">&uuml;berlisten</trd>
  <trd lng="fr">rouler<klr tip="ind"> (quelqu'un)</klr></trd>
  <trd lng="he">&lamed;&he;<ind>&ayin;&resh;&yod;&fmem;</ind> &ayin;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;szed</trd>,
    <trd>t&uacute;lj&aacute;r az esz&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>te <ind>slim</ind> af zijn</trd>,
    <trd>verschalken</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_h;&c_i;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">preľstiť</trd>
</drv>
</art>
