<kap>
   <rad>sistr</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="sistr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MUZ</uzo>
   <snc>
     <dif>
       Antikva egipta muzikilo konsistanta el metalaj vergetoj sonantaj
       per
       skui&gcirc;o.
     </dif>
   </snc>
  <trd lng="ca">sistre</trd>
  <trdgrp lng="cs">
    <trd>sistr</trd>,
    <trd>sistrum</trd>
  </trdgrp>
  <trd lng="de">Sistrum</trd>
  <trd lng="es">sistro</trd>
  <trd lng="fr">sistre</trd>
  <trd lng="hu">szisztrum</trd>
  <trd lng="nl">sistrum</trd>
  <trd lng="pl">sistrum</trd>
  <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">sistrum</trd>
</drv>
</art>
