<kap>
  <ofc>1</ofc>
  <rad>teologi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="teolog1.0o">
  <kap><tld/>o</kap>
  <snc mrk="teolog1.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Metoda studo pri Dio kaj diaj aferoj:
      <ekz>
        la <tld/>o <klr>...</klr> ne estas io alia ol
        filozofia redakto kaj komentado de la revelacio
         <fnt>
           <aut>V. O&scaron;lak</aut>,
           <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
           <lok>2003</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">teologia</trd>
  <trd lng="cs">teologie</trd>
  <trd lng="de">Theologie</trd>
  <trd lng="en">theology</trd>
  <trd lng="fr">th&eacute;ologie</trd>
  <trdgrp lng="hu">
    <trd>hittudom&aacute;ny</trd>,
    <trd>teol&oacute;gia</trd>
  </trdgrp>
  <trd lng="nl">teologie</trd>
  <trd lng="pl">teologia</trd>
  <trd lng="pt">teologia</trd>
  <trd lng="ru">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>bohoslovie</trd>,
    <trd>teol√≥gia</trd>
  </trdgrp>
</drv>

<drv mrk="teolog1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="teolog1.0isto.REL">
    <dif>
      Specialisto pri <tld/>o;
      <ref tip="sin" cel="teolog.0o.REL">teologo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">te&ograve;leg</trd>
  <trd lng="de">Theolog</trd>
  <trd lng="en">theologist</trd>
  <trd lng="fr">th&eacute;ologien</trd>
  <trdgrp lng="hu">
    <trd>hittud&oacute;s</trd>,
    <trd>teol&oacute;gus</trd>
  </trdgrp>
  <trd lng="nl">teoloog</trd>
  <trd lng="pl">teolog</trd>
  <trd lng="ru">&c_t;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
