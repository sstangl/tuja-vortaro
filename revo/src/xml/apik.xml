<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apik.xml,v 1.12 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>apik</rad>/a
</kap>
<drv mrk="apik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Krute vertikala:
      <ekz>
        <tld/>a roko;
      </ekz>
      <ekz>
        la &scirc;ipo alfundi&gcirc;is <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_e;&c_r;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prudký</trd>,
    <trd>příkrý</trd>,
    <trd>strmý</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>steil</trd>,
     <trd>fast <ind>senkrecht</ind></trd>,
     <trd>nahezu senkrecht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tilted</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>pic</ind></trd>,
    <trd>escarp&eacute;</trd>
  </trdgrp>
  <trd lng="hu">meredek</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_t;&c_o;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_t;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>príkry</trd>,
    <trd>strm&yacute;</trd>,
    <trd>zvislý</trd>
  </trdgrp>
</drv>
</art>
