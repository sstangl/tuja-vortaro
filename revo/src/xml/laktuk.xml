<vortaro>
<art mrk="$Id: laktuk.xml,v 1.19 2009/09/23 17:25:57 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>laktuk</rad>/o
</kap>
<drv mrk="laktuk.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Lactuca</trd>),
      legoma vegeta&jcirc;o uzata kiel salato:
        <ekz>
	Borges &scirc;topis sin per <tld/>o kaj bovoviando
        <fnt>
          <aut>Machado de Assis, trad. Paulo Viana</aut>
          <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno,
          jaro 2005a, p. 42a</lok>
        </fnt>.
	</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_t;&c_u;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_a;&c_t;-&c_l;&c_a;&c_t;&c_u;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_u;&c_l;&c_ja;</trd>
  <trd lng="br">letuzenn</trd>
  <trdgrp lng="de">
    <trd>Lattich</trd>,
    <trd>Salat</trd>
  </trdgrp>
  <trd lng="ca">enciam</trd>
  <trd lng="el">&my;&alfa;&rho;&omikron;&ypsilon_ton;&lambda;&jota;</trd>
  <trd lng="en">lettuce</trd>
  <trd lng="es">lechuga</trd>
  <trd lng="fr">laitue</trd>
  <trd lng="hu">sal&aacute;ta<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  <trdgrp lng="nl">
    <trd>sla</trd>,
    <trd>krulsla</trd>
  </trdgrp>
  <trd lng="pl">sa&lstroke;ata</trd>
  <trd lng="pt">alface</trd>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_a;&c_t;-&c_l;&c_a;&c_t;&c_u;&c_k;</trd>
</drv>
</art>
