<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korvet.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>korvet</rad>/o
</kap>
<drv mrk="korvet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Trimasta milita <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>,
      mezgranda inter <ref cel="brig.0o">brigo</ref> kaj
      <ref cel="fregat.0o.MIL">fregato</ref>:
      <ekz>
        dum mallonga tempo, mi komandis la <tld/>on George Washington &ccirc;e
        Suda Ameriko
        <fnt>
        <aut>Edward Everett Hale, trad. E. L. Robinson, trad. Don
        Harlow</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html">La
        viro senpatria</url></vrk>,
        <lok>1907 kaj 1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_v;&c_e;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_v;&c_je;&c_t;&c_a;</trd>
  <trd lng="ca">corbeta</trd>
  <trdgrp lng="cs">
    <trd>kavka</trd>,
    <trd>kavka obecná</trd>
  </trdgrp>
  <trd lng="de">Korvette</trd>
  <trd lng="en">corvette</trd>
  <trd lng="es">corbeta</trd>
  <trdgrp lng="fr">
    <trd>corvette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>korvett</trd>,
    <trd>k&iacute;s&eacute;r&odblac; nasz&aacute;d</trd>,
  </trdgrp>
  <trd lng="it">corvetta</trd>
  <trd lng="nl">korvet</trd>
  <trd lng="pt">corveta <klr tip="amb">(&ecirc;)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_v;&c_je;&c_t;</trd>
  <trd lng="sk">ľahká vojenská plachetnica</trd>
</drv>
</art>
