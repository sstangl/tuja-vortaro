<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oleac.xml,v 1.13 2015/10/27 11:10:31 revo Exp $">
<kap>
  <rad>oleac</rad>/oj
</kap>
<drv mrk="oleac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      lignaj plantoj (<trd lng="de">Oleaceae</trd>),
      al kiu apartenas ekzemple la genroj:
      <refgrp tip="sub">
        <ref cel="frakse.0o">frakseno</ref>,
        <ref cel="jasmen.0o">jasmeno</ref>,
        <ref cel="ligust.0o">ligustro</ref>,
        <ref cel="ole1.0o">oleo</ref> kaj
        <ref cel="siring.0o">siringo</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">ole&agrave;cies</trd>
  <trd lng="cs">olivovn&iacute;kovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">&Ouml;lbaumgew&auml;chse</trd>
  <trd lng="el">&epsilon;&lambda;&alfa;&jota;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="en">Oleaceae</trd>
  <trd lng="fa">&a_z;&f_y;&a_t;&a_w;&a_n;&f_y;&a_A;&a_n;</trd>
  <trd lng="hu">olajfaf&eacute;l&eacute;k</trd>
  <trd lng="nl">olijffamilie</trd>
  <trd lng="pl">oliwkowate</trd>
  <trd lng="pt">ole&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">olivovn&iacute;kovit&eacute;</trd>
</drv>
</art>
