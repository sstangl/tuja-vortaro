    <trd>byk</trd>,
    <trd>buhaj</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>touro</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_k;</trd>
