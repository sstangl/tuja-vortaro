<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxkci.xml,v 1.18 2013/09/02 09:10:10 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;kci</rad>/o
</kap>
<drv mrk="auxkci.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxkci.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Procedo de <ref tip="super" cel="vend.0o.EKON">vendo</ref> per
      publika konkuro, kie la pliproponanto ricevas la a&ccirc;etebla&jcirc;on:
      <ekz>
        <tld/>a vendo, propono;
      </ekz>
      <ekz>
        la bienon de mia patro oni <klr>...</klr> vendis per <tld/>o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_k;&c_c;&c_y;&c_jo;&c_n;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_malmol;&c_r;&c_g;</trd>,
    <trd>&c_a;&c_u;&c_k;&c_c;&c_i;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Auktion</trd>,
    <trd>Versteigerung</trd>
  </trdgrp>
  <trd lng="en">auction</trd>
  <trd lng="fr">vente aux <ind>ench&egrave;res</ind></trd>
  <trd lng="es">subasta</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rver&eacute;s</trd>,
    <trd>aukci&oacute;</trd>
  </trdgrp>
  <trd lng="it">asta</trd>
  <trdgrp lng="nl">
    <trd>veiling</trd>,
    <trd>vendu</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_u;&c_k;&c_c;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">dra&zcaron;ba</trd>
  <trd lng="sv">auktion</trd>
</drv>

<drv mrk="auxkci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxkci.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="vend.0i.EKON">Vendi</ref> per <tld/>o:
      <ekz>
        oni <tld/>is la leterojn de iu usona poeto
        <fnt>
        <aut>E. Grimley Evans</aut>,
        <vrk>Re: afi&scirc;ado de retmesa&gcirc;oj</vrk>,
        <lok>soc.culture.esperanto, 2000-08-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_z; <ind>&c_a;&c_w;&c_k;&c_c;&c_y;&c_jo;&c_n;&c_u;</ind></trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_d;&c_a;&c_v;&c_a;&c_m; &c_n;&c_a; &c_t;&c_malmol;&c_r;&c_g;</trd>
  <trdgrp lng="de">
    <trd>verauktionieren</trd>,
    <trd>versteigern (lassen)</trd>
  </trdgrp>
  <trd lng="en">auction (off)</trd>
  <trd lng="es">subastar</trd>
  <trd lng="fr">vendre aux <ind>ench&egrave;res</ind></trd>
  <trd lng="hu">&aacute;rverez</trd>
  <trdgrp lng="it">
    <trd>vendere all'<ind>asta</ind></trd>,
    <trd>mettere all'<ind>asta</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_s; &c_a;&c_u;&c_k;&c_c;&c_i;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_s; &c_m;&c_o;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>veilen</trd>,
    <trd>per <ind>opbod</ind> verkopen</trd>
  </trdgrp>
  <trd lng="sk">dra&zcaron;i&tcaron;</trd>
  <trd lng="sv">auktionera</trd>
</drv>

<drv mrk="auxkci.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxkci.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Komisiito por <tld/>e vendi.
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_u;&c_k;&c_c;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Auktionator</trd>,
    <trd>Versteigerer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>auctioneer</trd>
  </trdgrp>
  <trd lng="fr">commissaire-priseur</trd>
  <trd lng="it"><ind>battitore</ind> d'asta</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rverez&odblac;<klr tip="amb"> (tisztvisel&odblac;)</klr></trd>,
    <trd>kiki&aacute;lt&odblac;</trd>
  </trdgrp>
  <trd lng="es">subastar</trd>
  <trd lng="nl">veiligmeester</trd>
</drv>
</art>
