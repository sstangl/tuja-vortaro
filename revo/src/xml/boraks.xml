<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boraks.xml,v 1.14 2015/10/06 02:10:37 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>boraks</rad>/o
  </kap>
  <drv mrk="boraks.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <uzo tip="fak">KEM</uzo>
    <snc>
      <dif>
        Natria salo de boro. Na<sub>2</sub>B<sub>4</sub>O<sub>7</sub>. 
      </dif>
      <trd lng="bg">&c_b;&c_o;&c_r;&c_a;&c_k;&c_s;</trd>
      <trd lng="ca">b&ograve;rax</trd>
      <trdgrp lng="de">
        <trd>Borax</trd>,
        <trd>Tinkal</trd>,
        <trd>Natriumborat</trd>
      </trdgrp>
      <trd lng="en">borax</trd>
      <trd lng="es">b&oacute;rax</trd>
      <trd lng="fr">borax</trd>
      <trd lng="nl">borax</trd>
      <trd lng="pl">boraks</trd>
      <trd lng="pt">b&oacute;rax</trd>
      <trd lng="ru">&c_b;&c_u;&c_r;&c_a;</trd>
    </snc>
  <trd lng="cs">borax</trd>
  <trd lng="sk">b&oacute;rax</trd>
</drv>
</art>
