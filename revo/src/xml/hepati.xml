<vortaro>
<art mrk="$Id: hepati.xml,v 1.16 2015/04/14 20:10:16 revo Exp $">
<kap>
  <rad>hepatik</rad>/o
</kap>
<drv mrk="hepati.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Hepatica</trd>), plurjara herbo kun plej ofte bluaj
      floroj kaj trilobaj folioj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="de">Leberbl&uuml;mchen</trd>
  <trd lng="fr">h&eacute;patique<klr tip="ind"> (subst., bot.)</klr></trd>
  <trdgrp lng="hu">
    <trd>m&aacute;jvir&aacute;g</trd>,
    <trd>m&aacute;jf&udblac;</trd>
  </trdgrp>
  <trd lng="nl">leverbloempje</trd>
  <trd lng="no">bl&aring;veis</trd>
  <trd lng="pl">przylaszczka</trd>
  <trd lng="pt">hep&aacute;tica</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_jo;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
