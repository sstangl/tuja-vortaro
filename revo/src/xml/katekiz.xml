<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katekiz.xml,v 1.6 2011/12/05 14:10:25 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>katekiz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katekiz.0i">
  <kap><ofc>3</ofc><tld/>i</kap>
  <snc mrk="katekiz.0i.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0i">kate&hcirc;izi</ref>
    <ekz>
      oni <tld/>is nin en la grandega, tri-nava katedralo
      <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
      <lok>1987</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_h;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">catequitzar</trd>
  <trd lng="en">catechize</trd>
  <trdgrp lng="fr">
    <trd>cat&eacute;chiser</trd>,
    <trd>faire le <ind>cat&eacute;chisme</ind></trd>
  </trdgrp>
  <trd lng="nl">catechiseren</trd>
</drv>

<drv mrk="katekiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="katekiz.0o.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0o">kate&hcirc;izo</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>catequesi</trd>,
    <trd>catecisme</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cat&eacute;ch&egrave;se</trd>,
    <trd>cat&eacute;chisme<klr tip="ind"> (enseignement)</klr></trd>
  </trdgrp>
  <trd lng="nl">catechese</trd>
</drv>

<drv mrk="katekiz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="katekiz.0isto.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0isto">kate&hcirc;izisto</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">catequista</trd>
  <trd lng="de">Katechist</trd>
  <trd lng="en">catechist</trd>
  <trd lng="fr">cat&eacute;chiste</trd>
  <trdgrp lng="hu">
    <trd>hitoktat&oacute;</trd>,
    <trd>katek&eacute;ta</trd>
  </trdgrp>
  <trd lng="nl">catecheet</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_o;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>

