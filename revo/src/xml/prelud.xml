<kap>
  <ofc>1</ofc>
  <rad>prelud</rad>/o
</kap>

<drv mrk="prelud.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konduk.en0o">Enkonduko</ref> de
      unuinstrumenta
      <ref tip="malprt" cel="kompon.0ajxo">kompona&jcirc;o</ref>,
      ekzerca por la muzikanto kaj anonca pri la stilo de la tuto,
      eventuale memstare ludebla.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="uvertu.0o">uverturo</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_r;&c_e;&c_l;&c_ju;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">preludi</trd>
  <trdgrp lng="de">
    <trd>Pr&auml;ludium</trd>,
    <trd>Vorspiel</trd>
  </trdgrp>
  <trd lng="en">prelude</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;lude</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>prel&uacute;dium</trd>,
    <trd>el&odblac;j&aacute;t&eacute;k</trd>
  </trdgrp>
  <trd lng="it">preludio<klr> (mus.)</klr></trd>
  <trdgrp lng="nl">
    <trd>prelude</trd>,
    <trd>voorspel</trd>
  </trdgrp>
  <trd lng="pl">preludium</trd>
  <trd lng="pt">prel&uacute;dio</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_l;&c_ju;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>preludium</trd>,
    <trd>f&ouml;rspel</trd>
  </trdgrp>
  <trd lng="sk">predohra</trd>
  <trdgrp lng="cs">
    <trd>preludium</trd>,
    <trd>p≈ôedehra</trd>
  </trdgrp>
</drv>
</art>
