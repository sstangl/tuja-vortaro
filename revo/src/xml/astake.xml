<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: astake.xml,v 1.9 2013/08/31 16:10:23 revo Exp $">
<kap>
  <rad>astaked</rad>/oj
</kap>
<drv mrk="astake.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de
      <ref tip="malprt" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref>
      (<trd lng="la">Astacidae</trd>).
    </dif>
    <ref tip="prt" cel="kankr.0o">kankro</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_ch;&c_n;&c_y;&c_ja; &c_r;&c_a;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="ca">
    <trd>ast&agrave;cids</trd>,
    <trd>crancs de riu</trd>
  </trdgrp>
  <trd lng="de">Flusskrebse</trd>
  <trd lng="hu">folyami <ind>r&aacute;kok</ind> <klr
  tip="amb">(csal&aacute;d)</klr></trd>
  <trd lng="it"><ind>gamberi</ind> d'acqua dolce</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_n;&c_y;&c_je; <ind>&c_r;&c_a;&c_k;&c_i;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

