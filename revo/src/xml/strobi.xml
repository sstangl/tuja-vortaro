<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: strobi.xml,v 1.12 2015/04/25 22:10:14 revo Exp $">
<kap>
  <rad>strobil</rad>/o
</kap>
<drv mrk="strobi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>
      spikforma kun ligneca akso kaj folioj:
      <ekz>
        <tld/>o de abio, pino, alno.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_sh;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="de">Zapfen</trd>
  <trd lng="fr">c&ocirc;ne</trd>
  <trd lng="hu">toboz</trd>
  <trdgrp lng="nl">
    <trd>dennenappel</trd>,
    <trd>sparappel</trd>
  </trdgrp>
  <trd lng="pl">szyszka</trd>
  <trd lng="ru">&c_sh;&c_i;&c_sh;&c_k;&c_a; <klr
  tip="amb">(&c_je;&c_l;&c_o;&c_v;&c_a;&c_ja;,
  &c_s;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_i;
  &c_t;.&c_p;.)</klr></trd>
</drv>
</art>
