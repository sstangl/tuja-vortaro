<vortaro>
<art mrk="$Id: orangx1.xml,v 1.11 2010/09/28 16:30:32 revo Exp $">
<kap>
  <rad>oran&gcirc;eri</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="orangx1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HOR</uzo>
  <snc>
    <dif>
      Varmigebla <ref tip="super" cel="forc.0ejo">forcejo</ref>, en kiu
      oni konservas en malvarmaj landoj oran&gcirc;arbojn kaj aliajn
      delikatajn
      arbojn dum la vintro:
      <ekz>
        &scirc;i havis <klr>...</klr> belegan &gcirc;ardenon kun <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        80a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_n;&c_zh;&c_a;&c_r;&c_e;&c_ja;</trd>,
    <trd>&c_c;&c_ja;&c_p;&c_l;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Orangerie</trd>
  <trd lng="en">orangery</trd>
  <trdgrp lng="fr">
    <trd>orangerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;lmah&aacute;z</trd>,
    <trd>orangerie</trd>
  </trdgrp>
  <trd lng="nl">orangerie</trd>
  <trd lng="pt">estufa</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_a;&c_n;&c_zh;&c_je;&c_r;&c_je;&c_ja;</trd>,
    <trd>&c_t;&c_je;&c_p;&c_l;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
