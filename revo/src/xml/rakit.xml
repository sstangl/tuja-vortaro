<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rakit.xml,v 1.6 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>rakit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rakit.0o">
  <kap><tld/>o</kap>
  <snc mrk="rakit.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsufi&ccirc;a solidi&gcirc;o de ostoj en tre juna a&gcirc;o
      ka&ubreve;zanta misformi&gcirc;ojn de spino, de gamboj:
      <ekz>
        <tld/>o <klr>...</klr> estas ka&ubreve;zata de manko de la
        akcesora nutrofaktoro vitamino D
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="bg">&c_r;&c_a;&c_h;&c_i;&c_t;</trd>
  <trd lng="ca">raquitisme</trd>
  <trdgrp lng="cs">
    <trd>křivice</trd>,
    <trd>rachitida</trd>
  </trdgrp>
  <trdgrp lng="da">
    <trd>engelsk syge</trd>,
    <trd>rakitis</trd>
  </trdgrp>
  <trd lng="de">Rachitis</trd>
  <trd lng="en">rickets</trd>
  <trd lng="es">raquitismo</trd>
  <trd lng="fr">rachitisme</trd>
  <trd lng="hr">rahitis</trd>
  <trd lng="id">rakitis</trd>
  <trd lng="it">rachitismo</trd>
  <trd lng="nl">rachitis</trd>
  <trd lng="pl">krzywica</trd>
  <trd lng="ru">&c_r;&c_a;&c_h;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>krivica</trd>,
    <trd>rachitída</trd>
  </trdgrp>
</drv>
</art>

