<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: batu1.xml,v 1.8 2015/10/04 14:10:15 revo Exp $">
<kap>
  <rad>batu</rad>/i
</kap>
<drv mrk="batu1.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="cxas.0i.bestojn">&Ccirc;asi</ref> bestojn,
      pelante
      ilin al kaptoloko per bruo a&ubreve; per aliaj timigoj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>fer una batuda</trd>,
    <trd>batre el bosc <klr>(per aixecar la ca&ccedil;a)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lovit na honu</trd>,
    <trd>&uacute;&ccaron;astnit se honu</trd>
  </trdgrp>
  <trd lng="en">to haze</trd>
  <trd lng="es">hacer una <ind>batida</ind><klr> (caza)</klr></trd>
  <trd lng="fr">faire une <ind>battue</ind><klr> (chasse)</klr></trd>
  <trdgrp lng="pl">
    <trd>nagania&cacute;</trd>,
    <trd>polowa&cacute; z nagonk&aogonek;</trd>
  </trdgrp>
  <trd lng="sk">z&uacute;&ccaron;astni&tcaron; sa honu</trd>
</drv>

<drv mrk="batu1.0o">
  <kap><tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="cxas.0o">&Ccirc;aso</ref>, en kiu pelistoj
      eltimigas bestojn, por ke ili trafu kaptilojn a&ubreve; pafistojn:
      <ekz>
        okazis la <tld/>o, kaj la reto pleni&gcirc;is per diversaj
        birdetoj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 58a, p.
        723a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>batuda</trd>,
    <trd>cacera</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hon</trd>,
    <trd>hromadn&yacute; lov</trd>,
    <trd>le&ccaron;</trd>
  </trdgrp>
  <trd lng="es">batida<klr> (caza)</klr></trd>
  <trd lng="fr">battue<klr> (chasse)</klr></trd>
  <trdgrp lng="pl">
    <trd>nagonka</trd>,
    <trd>polowanie z nagonk&aogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_v;&c_a;<klr>
  (&c_o;&c_h;&c_o;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>hon</trd>,
    <trd>hromadn&yacute; lov</trd>
  </trdgrp>
</drv>
</art>

