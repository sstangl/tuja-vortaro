<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ulm.xml,v 1.30 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ulm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ulm.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el mezvarmaj landoj de la norda terglobduono, 
      kies fibreca, elasta ligno estas uzata por fari objektojn, 
      kies strukturo a&ubreve; uzado postulas fortikecon, <ref tip="lst"
      cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el 
      familio <ref tip="malprt" cel="ulmac.0oj.BOT"><tld/>acoj</ref>
      (<trd lng="la">Ulmus</trd>). 
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_z;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-oulm</trd>,
    <trd>oulmenn</trd>
  </trdgrp>
  <trd lng="ca">om</trd>
  <trd lng="cs">jilm</trd>
  <trd lng="de">Ulme</trd>
  <trd lng="el">&phi;&tau;&epsilon;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="en">elm</trd>
  <trd lng="es">olmo</trd>
  <trd lng="fa">&a_n;&a_A;&a_r;&a_w;&a_n;</trd>
  <trd lng="fi">jalava</trd>
  <trd lng="fr">orme</trd>
  <trd lng="he">&bet;&vav;&qof;&yod;&tsadi;&he;</trd>
  <trd lng="hu">szil(fa)</trd>
  <trd lng="it">olmo</trd>
  <trdgrp lng="nl">
    <trd>olm</trd>,
    <trd>iep</trd>,
    <trd>iepenboom</trd>
  </trdgrp>
  <trd lng="pl">wi&aogonek;z</trd>
  <trdgrp lng="pt">
    <trd>olmo</trd>,
    <trd>ulmo </trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_ja;&c_z;</trd>
  <trd lng="sk">brest</trd>
  <trd lng="sv">alm</trd>
</drv>
</art>
