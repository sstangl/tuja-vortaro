<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: purpur.xml,v 1.18 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>purpur</rad>/o
</kap>
<drv mrk="purpur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Violru&gcirc;a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>:
      <ekz>li surmetis vulpofelan jakon, tegitan per melongene <tld/>a
      veluro
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p.
      123a</lok></fnt></ekz>
    </dif>
    <trd lng="en">purple</trd>
    <trd lng="fa">&a_A;&a_r;&a_gh;&a_w;&a_A;&a_n;&f_y;</trd>
  </snc>
  <snc>
    <dif>
      &Scirc;tofo el tiu koloro:
      <ekz>
        plej delikata tolo kaj <tld/>o estas &scirc;iaj vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la re&gcirc;a, kardinala <tld/>o.
      </ekz>
    </dif>
    <trd lng="fa">&f_p;&a_A;&a_r;&f_ch;&a_h;&a_hamza_sure; <ind>&a_A;&a_r;&a_gh;&a_w;&a_A;&a_n;&f_y;</ind></trd>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_r;&c_p;&c_u;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>porpra</trd>,
    <trd>p&uacute;rpura</trd>
  </trdgrp>
  <trd lng="cs">purpur</trd>
  <trd lng="de">Purpur</trd>
  <trdgrp lng="fr">
    <trd>pourpre</trd>
  </trdgrp>
  <trd lng="hu">b&iacute;bor</trd>
  <trd lng="nl">purper</trd>
  <trdgrp lng="pt">
   <trd>p&uacute;rpura</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_r;&c_p;&c_u;&c_r;</trd>
  <trd lng="sk">purpur</trd>
</drv>
</art>
