<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: omagx.xml,v 1.14 2014/05/08 18:10:37 revo Exp $">
<kap>
  <ofc>9</ofc><rad>oma&gcirc;</rad>/o
</kap>
<drv mrk="omagx.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <dif>
      Respekta promeso de fidela submeti&gcirc;o farita de vasalo al
      fe&ubreve;destro.
    </dif>
    <trdgrp lng="de">
      <trd>Lehenseid</trd>,
      <trd>Lehnseid</trd>,
      <trd>Vasalleneid</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>h&oacute;dol&aacute;s</trd>,
      <trd>beh&oacute;dol&aacute;s</trd>,
      <trd>h&udblac;b&eacute;resk&udblac;</trd>
    </trdgrp>
    <trd lng="nl">eed van trouw</trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Respekta a&ubreve; &gcirc;entila donaco de io al iu; dedi&ccirc;donaco.
    </dif>
    <trdgrp lng="de">
      <trd>Huldigung</trd>,
      <trd>Ehrerbietung</trd>,
      <trd>Ehrfurchtsbezeugung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszteletnyilv&aacute;n&iacute;t&aacute;s</trd>,
      <trd>tisztelettel ad&oacute;z&aacute;s</trd>,
      <trd>f&odblac;hajt&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">eerbetoon</trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_o;&c_d;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="ca">homenatge</trd>
  <trd lng="en">homage</trd>
  <trd lng="fr">hommage</trd>
  <trdgrp lng="pt">
    <trd>homenagem</trd>,
    <trd>preito</trd>,
    <trd>oferta</trd>    
   </trdgrp>
</drv>

<drv mrk="omagx.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari honoron al iu per io; respekte oferprezenti:
      <ekz>
        <tld/>i libron al iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">homenatjar</trd>
  <trdgrp lng="de">
      <trd>huldigen</trd>,
      <trd>Ehre <ind>erweisen</ind></trd>,
      <trd>Ehrfurcht <ind>bezeugen</ind></trd>
  </trdgrp>
  <trd lng="en">pay homage to</trd>
  <trd lng="fr">rendre hommage</trd>
  <trdgrp lng="hu">
    <trd>h&oacute;dol</trd>,
    <trd>tisztelettel ad&oacute;zik</trd>,
    <trd>fejet hajt</trd>
  </trdgrp>
  <trd lng="nl">eren</trd>
  <trd lng="pt">homenagear</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_d;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
