<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kompli1.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kompliment</rad>/o
</kap>

<drv mrk="kompli1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Gratulaj flataj vortoj adresataj al iu:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        adresi al iu <tld/>on pro lia sukceso;
      </ekz>
      <ekz>
        diri <tld/>ojn al virino;
      </ekz>
      <ekz>
        okaza <tld/>o ne iras al testamento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kompliment</trd>,
      <trd>Schmeichelei</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Gcirc;entilaj ceremoniaj formuloj:
      <ekz>
        vi afable tion diras pro <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        manieroj kaj <tld/>oj apartenas al la akcepta
        saluto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;flichkeitsbezeugung</trd>,
      <trd>Huldigung</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="ca">compliment</trd>
  <trdgrp lng="cs">
    <trd>kompliment</trd>,
    <trd>lichotka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kompliment</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>compliment<klr tip="ind"> (f&eacute;licitation,
    civilit&eacute;)</klr></trd>,
    <trd>civilit&eacute;<klr tip="ind"> (parole de civilit&eacute;)</klr></trd>,
    <trd>congratulation</trd>,
    <trd>f&eacute;licitation</trd>,
    <trd>louange</trd>
  </trdgrp>
  <trd lng="nl">compliment</trd>
  <trdgrp lng="pt">
    <trd>cumprimento</trd>,
    <trd>v&ecirc;nia</trd>,
    <trd>lisonja</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kompliment</trd>,
    <trd>lichôtka</trd>,
    <trd>poklona</trd>
  </trdgrp>
</drv>

<drv mrk="kompli1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari al iu <tld/>on:
      <ekz>
        <tld/>i iun pri lia kura&gcirc;o;
      </ekz>
      <ekz>
        <tld/>u lin komisie de mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas <tld/>ema kaj flatema.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">complimentar</trd>
  <trdgrp lng="cs">
    <trd>dvořit se</trd>,
    <trd>klanět se</trd>,
    <trd>lichotit <klr>(někomu)</klr></trd>,
    <trd>skládat poklony</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komplimente machen</trd>,
    <trd>schmeicheln</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>complimenter</trd>,
    <trd>faire un <ind>compliment</ind> (&agrave;)</trd>,
    <trd>congratuler</trd>,
    <trd>f&eacute;liciter</trd>,
    <trd>louer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complimenteren</trd>,
    <trd>een <ind>compliment</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lichotiť</trd>,
    <trd>poklonkovať</trd>
  </trdgrp>
</drv>
</art>
