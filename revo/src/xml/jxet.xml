<drv mrk="jxet.0lanco">
  <kap><tld/>lanco</kap>
  <snc>
    <dif>
      <ref tip="super" cel="arm.0ilo">Armilo</ref>, simila je lanco,
      kiun oni <tld/>as de malproksime.
    </dif>
    <trd lng="en">spear</trd>
    <trd lng="fr">javelot</trd>
    <trdgrp lng="hu">
      <trd>l&aacute;ndzsa</trd>,
      <trd>haj&iacute;t&oacute;d&aacute;rda</trd>
    </trdgrp>
    <trd lng="id">lembing</trd>
    <trd lng="nl">speer</trd>
    <trdgrp lng="pl">
      <trd>oszczep</trd>,
      <trd>w&lstroke;&oacute;cznia</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_p;&c_mol;&c_jo;</trd>
    <trd lng="it">giavellotto</trd>
  </snc>
</drv>

<drv mrk="jxet.sxton0ilo">
  <kap>&scirc;ton<tld/>ilo</kap>
  <snc>
    <dif>
      Aparato por <tld/>i malproksimen &scirc;tonetojn.
      <ekz>
        Ne forpelos &gcirc;in sago; &Scirc;tonoj el &scirc;ton<tld/>ilo
        fari&gcirc;as pajleroj anta&ubreve; &gcirc;i.
        <fnt><bib>MT</bib>, <lok>&Ijo; 41:28</lok></fnt>;
      </ekz>
    </dif>
    <ref tip="sin" cel="katapult.0o">katapulto, snc.2</ref>
    <trdgrp lng="en">
      <trd>sling-shot</trd>
    </trdgrp>
    <trd lng="fr">lance-pierres</trd>
    <trd lng="id">katapel</trd>
    <trd lng="nl">katapult</trd>
    <trd lng="pl">proca</trd>
    <trdgrp lng="pt">
      <trd>catapulta</trd>,
      <trd>funda</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_shch;&c_a;</trd>
    <trd lng="it">fionda</trd>
  </snc>
</drv>
