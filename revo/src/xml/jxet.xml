      <tld lit="&Jcirc;"/>i por forigi:
      <ekz>
        for<tld/>i malpura&jcirc;on, malbonan pomon;
      </ekz><ekz>
        for<tld/>i konsilon<fnt>Z</fnt>,
      </ekz><ekz>
        instruon<fnt>Z</fnt>,
      </ekz><ekz>
        la mal&gcirc;ojon<fnt>Z</fnt>,
      </ekz><ekz>
        la pre&gcirc;on de iu<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        mi for<tld/>is <klr>(rezignis)</klr> mian unuan projekton
        <fnt>Z</fnt>,
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iujn utopiojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
