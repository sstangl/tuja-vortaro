  </trdgrp>
  <trdgrp lng="hu">
    <trd>eldob</trd>,
    <trd>elvet</trd>,
    <trd>elsz&oacute;r</trd>,
    <trd>elhaj&iacute;t</trd>
  </trdgrp>
  <trd lng="id">[<ind>buang</ind>] membuang</trd>
  <trd lng="nl">wegwerpen</trd>
  <trd lng="pl">wyrzuca&cacute;</trd>
  <trd lng="pt">jogar fora</trd>
  <trd lng="ru">&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="it">
    <trd>buttare via</trd>,
    <trd>proiettare</trd>,
    <trd>scartare</trd>
  </trdgrp>
</drv>

