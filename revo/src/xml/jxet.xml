<drv mrk="jxet.dis0o">
  <kap>dis<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="jxet.dis0o.MAT">
    <fnt><bib>KompLeks</bib>
    <lok><url ref="&KompLeks;/JX.html#JxETO">TTT</url></lok>
    </fnt>
    <dif>
      <tld lit="&Jcirc;"/>o<sncref ref="jxet.0o.MAT"/> atribuanta
      malsamajn (t.e. <em>disajn</em>) bildojn al malsamaj
      (t.e. <em>disaj</em>) elementoj
      de la <ref cel="ar.fonto0o">fonta aro</ref>.  
    </dif>
    <rim num="1">
       <tld lit="&Jcirc;"/>on<sncref ref="jxet.0o.MAT"/> kiu povas diversajn
       argumentojn <tld/>i en unu saman bildon (kunigi tiujn
       argumentojn), oni povas nomi <ctl>kun<tld/>o</ctl>.
    </rim>
    <rim num="2">
      Vd &ccirc;e <ref tip="sin" cel="enjekc.0o">enjekcio</ref>
      klarigon, kial tiu termino estas misgvida.
    </rim>
    <trd lng="de">Injektion</trd>
    <trd lng="en">injection</trd>
    <trdgrp lng="fr">
      <trd>injection</trd>,
      <trd>application injective</trd>
    </trdgrp>
    <trd lng="hu">injekci&oacute;</trd>
    <trdgrp lng="pl">
      <trd>funkcja r&oacute;&zdot;nowarto&sacute;ciowa</trd>,
      <trd>injekcja</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_i;&c_n;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="jxet.dis0a">
  <kap>dis<tld/>a</kap>
  <snc mrk="jxet.dis0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      (p.p. <tld/>o<sncref ref="jxet.0o.MAT"/>) Havanta la ecojn
      de dis<tld/>o:
      <ekz>
        <ref cel="homomo.0o">homomorfio</ref> inter vektoraj
        spacoj estas dis<tld/>aa se kaj nur se &gcirc;ia
        <ref cel="kern.0o.homomorfio">kerno</ref>
        redukti&gcirc;as al <frm>{0}</frm>.
      </ekz>
    </dif> 
    <ref tip="sin" cel="enjekc.0a">enjekcia</ref>
    <trd lng="en">injective</trd>
    <trd lng="de">injektiv</trd>
    <trd lng="fr">injectif</trd>
    <trd lng="hu">injekt&iacute;v</trd>
    <trdgrp lng="pl">
      <trd>r&oacute;&zdot;nowarto&sacute;ciowy</trd>,
      <trd>injektywny</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_n;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>

