<vortaro>
<art mrk="$Id: grapl.xml,v 1.14 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>grapl</rad>/o
</kap>
<drv mrk="grapl.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Speco de malgranda ankro kun multaj brakoj.  
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_t;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="ca">ruix&oacute;</trd>
  <trd lng="cs">malá čtyř až šestiramenná kotva</trd>
  <trdgrp lng="de">
     <trd>Dregganker</trd>,
     <trd>Draggen</trd>,
     <trd>Suchanker</trd></trdgrp>
  <trdgrp lng="en">
    <trd>grapnel</trd>,
    <trd>grappling iron</trd>
  </trdgrp>
  <trd lng="fr">grappin</trd>
  <trdgrp lng="hu">
    <trd>kutat&oacute;horgony</trd>,
    <trd>kap&oacute;cs&aacute;klya</trd>
  </trdgrp>
  <trd lng="nl">dreg</trd>
  <trd lng="pt">fateixa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_k;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnohoramenná kotva</trd>
</drv>
</art>
