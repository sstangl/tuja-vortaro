<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pagure.xml,v 1.10 2015/09/15 14:10:16 revo Exp $">
<kap><rad>pagured</rad>/oj</kap>
<drv mrk="pagure.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref>
      (<trd lng="la">Paguridae</trd>), vivantaj en
      malplenaj konkoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Paguridae</trd>,
    <trd>hermit crabs</trd>
  </trdgrp>
  <trd lng="be">&c_r;&c_a;&c_k;&c_ib;-&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">pag&uacute;rids</trd>
  <trd lng="de">Meereseinsiedlerkrebse</trd>
  <trd lng="hu">remeter&aacute;kok <klr tip="amb">(csal&aacute;d)</klr></trd>
  <trd lng="nl">heremietkreeften</trd>
  <trdgrp lng="pt">
   <trd>pagur&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_i;-&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
