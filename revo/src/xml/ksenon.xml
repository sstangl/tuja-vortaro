<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ksenon.xml,v 1.17 2013/09/17 18:10:12 revo Exp $">
<kap>
  <rad>ksenon</rad>/o
</kap>
<drv mrk="ksenon.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Xe</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
     lst="voko:&hcirc;emiaj_elementoj" val="54">elemento</ref> kun
     atomnumero 54, atompezo 131,30;
     <ref tip="super" cel="gas.nobla0o">nobla gaso</ref>, kies sola
     konata kombina&jcirc;o estas ksenontetrafluorido.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_s;&c_e;&c_n;&c_o;&c_n;</trd>
  <trd lng="bg">&c_k;&c_s;&c_je;&c_n;&c_o;&c_n;</trd>
  <trd lng="br">ksenon</trd>
  <trd lng="de">Xenon</trd>
  <trd lng="en">xenon</trd>
  <trd lng="es">xen&oacute;n</trd>
  <trd lng="fr">x&eacute;non</trd>
  <trd lng="hu">xenon</trd>
  <trd lng="id">xenon</trd>
  <trd lng="it">xenon</trd>
  <trd lng="la">Xenon</trd>
  <trd lng="nl">xenon</trd>
  <trd lng="pl">ksenon</trd>
  <trd lng="pt">x&eacute;non</trd>
  <trd lng="ru">&c_k;&c_s;&c_je;&c_n;&c_o;&c_n;</trd>
  <trd lng="sv">xenon</trd>
</drv>
</art>

