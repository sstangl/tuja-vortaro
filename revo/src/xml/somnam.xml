<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: somnam.xml,v 1.16 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>somnambul</rad>/o
</kap>
<drv mrk="somnam.0o">
  <kap><tld/>o</kap>
  <snc mrk="somnam.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Dormiranto, homo senkonscie kaj senmemore iranta, parolanta kaj 
      aganta  dum dormo, kvaza&ubreve; li estus maldorma kaj konscia.
    </dif>
    <refgrp tip="vid">
      <ref cel="lunati.0o">lunatiko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ju;&c_n;&c_a;&c_t;&c_y;&c_k;</trd>, 
    <trd>&c_s;&c_a;&c_m;&c_n;&c_a;&c_m;&c_b;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>náměsíčník</trd>,
    <trd>somnambul</trd>
  </trdgrp>
  <trd lng="de">Schlafwandler</trd>
  <trd lng="en">sleepwalker</trd>
  <trd lng="es">son&aacute;mbulo</trd>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_b;&f_g;&a_r;&a_d;</trd>
  <trd lng="fr">somnambule<klr> (subst.)</klr></trd>
  <trd lng="hu">alvaj&aacute;r&oacute;</trd>
  <trd lng="nl">slaapwandelaar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_m;&c_n;&c_a;&c_m;&c_b;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_n;&c_a;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lunatik</trd>,
    <trd>námesačník</trd>
  </trdgrp>
</drv>

<drv mrk="somnam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="somnam.0i.MED">
    <dif>
      Iri, agi kiel <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dum unu semajno ili <tld/>is preska&ubreve; senvorte
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">sleepwalk</trd>
  <trd lng="fr">agir en <ind>somnambule</ind></trd>
</drv>
</art>
