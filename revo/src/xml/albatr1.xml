<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: albatr1.xml,v 1.8 2014/11/04 11:10:20 revo Exp $">
<kap>
  <rad>albatred</rad>/oj
</kap>
<drv mrk="albatr1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="procel.0oformaj">
      procelarioformaj birdoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_t;&c_r;&c_o;&c_s;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Albatrosse <klr tip="ind">(Familie)</klr></trd>
  <trdgrp lng="en">
     <trd>albatrosses</trd>,
     <trd>procellariformes</trd>
  </trdgrp>
  <trd lng="fr">diom&eacute;d&eacute;id&eacute;s</trd>
  <trd lng="hu">albatroszok <klr tip="amb">(csal&aacute;d)</klr></trd>
  <trd lng="nl">albatros</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_t;&c_r;&c_o;&c_s;&c_y; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

