  </snc>
  <trdgrp lng="en">
    <trd>Polygonaceae</trd>,
    <trd>buckwheat family</trd>, <trd>smartweed family</trd>,
    <trd>knotweed family</trd>
  </trdgrp>
  <trd lng="ca">poligon&agrave;cies</trd>
  <trd lng="de">Kn&ouml;terichgew&auml;chse</trd>
  <trd lng="fr">polygonac&eacute;es</trd>
  <trd lng="hu">keser&udblac;f&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="nl">veelknopigen</trd>
  <trd lng="pl">rdestowate</trd>
  <trd lng="pt">poligon&aacute;ceas</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_ch;&c_i;&c_sh;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
