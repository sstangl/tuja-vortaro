<kap>
  <rad>poligonac</rad>/oj
</kap>
<drv mrk="poligo1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="poligo1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      vegeta&jcirc;oj (<trd lng="la">Polygonaceae</trd>).
