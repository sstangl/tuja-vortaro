<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: limed.xml,v 1.5 2015/09/30 11:10:14 revo Exp $">
<kap>
  <rad>limed</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="limed.0o">
  <kap><tld/>o</kap>
  <snc mrk="limed.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de 
      <tld/>arbo, flava kiel citrono sed pli ronda kaj pli dol&ccirc;a:
      <ekz>
        li unue prezentis al klerikoj a&ubreve; pastroj
        kelkajn ne tre altvalorajn donacetojn:
        la&ubreve; la sezono, laktukon el Murcio, paron
        da <tld/>oj, oran&gcirc;oj, persikoj a&ubreve; piroj
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_i;&c_p;&c_a;</trd>
  <trd lng="cs">limetta</trd>
  <trd lng="de">Limette</trd>
  <trd lng="en">sweet lime</trd>
  <trdgrp lng="fr">
    <trd>citron doux</trd>,
    <trd>limette</trd>
  </trdgrp>
  <trd lng="nl">limoen</trd>
  <trd lng="pl">cytryna rzymska</trd>
  <trd lng="sk">limeta</trd>
</drv>

<drv mrk="limed.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="limed.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta <ref tip="super" cel="citrus.0o">citruso</ref>
      dorna, kun malrektaj bran&ccirc;oj kaj malgrandaj folioj 
      (<trd lng="la">Citrus limetta</trd>).
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>limettier doux</trd>,
    <trd>limonette</trd>
  </trdgrp>
  <trd lng="pl">cytryna rzymska</trd>
</drv>
</art>

