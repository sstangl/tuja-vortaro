<vortaro>
<art mrk="$Id: vila.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vila</rad>/o
</kap>
<drv mrk="vila.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Somerdomo, kampodomo:
      <ekz>
        ni vidis ke multaj vila&gcirc;anoj konstruis jam belajn <tld/>ojn,
        meblitajn kaj provizitajn per akvo kaj elektra energio, kiel en
        la
        urbo<fnt><aut>Bardhyl Selimi</aut>: <vrk>Voja&gcirc;impresoj, somero
        2000</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_i;&c_l;&c_a;</trd>
  <trd lng="ca">vil&middot;la</trd>
  <trd lng="cs">vila</trd>
  <trd lng="de">Villa</trd>
  <trd lng="el">&beta;&jota_ton;&lambda;&lambda;&alfa;</trd>
  <trdgrp lng="en">
    <trd>villa</trd>,
    <trd>cottage</trd>,
    <trd>country cottage</trd>,
    <trd>seaside cottage</trd>,
    <trd>summer residence</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_w;&f_y;&a_l;&a_A;</trd>,
    <trd>&a_kh;&a_A;&a_n;&a_h;&a_hamza_sure; <ind>&f_y;&f_y;&a_l;&a_A;&a_q;&f_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;sidence secondaire</trd>,
    <trd>villa</trd>
  </trdgrp>
  <trd lng="hu">villa <klr tip="ind">(h&aacute;z)</klr></trd>
  <trdgrp lng="nl">
    <trd>villa</trd>,
    <trd>landhuis</trd>
  </trdgrp>
  <trd lng="pt">vila</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_l;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vila</trd>,
    <trd>vilka</trd>
  </trdgrp>
</drv>
</art>
