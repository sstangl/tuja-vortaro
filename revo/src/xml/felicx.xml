<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: felicx.xml,v 1.22 2015/11/26 18:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>feli&ccirc;</rad>/a
</kap>
<drv mrk="felicx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="felicx.0a.kontenta">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Sentanta animan plenkontentecon:
      <ekz>
        <tld/>a homo;
      </ekz>
      <ekz>
        mi estas <tld/>a vin renkonti;
      </ekz>
      <ekz>
        havi <tld/>an mienon<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxoj.0a">&gcirc;oja</ref>,
      <ref cel="beat.0a">beata</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="cxiel1.0o">&ccirc;ielo</ref>.
    </refgrp>
    <trd lng="en">happy</trd>
  </snc>
  <snc mrk="felicx.0a.sukcesanta">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Favorata de la sorto; sukcesanta, bon&scirc;anca:
      <ekz>
        <tld/>a en siaj entreprenoj;
      </ekz>
      <ekz>
        <tld/>a je monludo;
      </ekz>
      <ekz>
        al <ind><tld/>ulo</ind> e&ccirc; koko donas ovojn.
        <trdgrp lng="fr">
          <trd>chanceux<klr tip="ind"> (subst.)</klr></trd>
        </trdgrp>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0i">ridi</ref>.
    </refgrp>
    <trd lng="de">Gl&uuml;ck habend</trd>
    <trdgrp lng="en">
      <trd>fortunate</trd>,
      <trd>lucky</trd>, <trd>favored</trd>, <trd>happy</trd>
    </trdgrp>
  </snc>
  <snc mrk="felicx.0a.kontentiga">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Plenkontentiganta la animon:
      <ekz>
        <tld/>a vivo;
      </ekz>
      <ekz>
        deziri al iu <tld/>an feston;
      </ekz>
      <ekz>
        <tld/>a karaktero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>,
      <ref cel="rav">rava</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="paradi1.0o">paradizo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>froh</trd>,
      <trd>erfolgreich</trd>
    </trdgrp>
    <trd lng="en">happy</trd>
  </snc>
  <snc mrk="felicx.0a.favora">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Favora, sukcesa, bonokaza:
      <ekz>
        <tld/>a renkonto, situacio, cirkonstanco;
      </ekz>
      <ekz>
        <tld/>a aku&scirc;o;
      </ekz>
      <ekz>
        la operacio havis <tld/>an sekvon;
      </ekz>
      <ekz>
        alkonduki aferon al <tld/>a fino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kelkaj <tld/>aj ideoj kura&gcirc;igis min<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prospe">prospera</ref>.
    </refgrp>
    <trd lng="ca">feli&ccedil;</trd>
    <trdgrp lng="de">
      <trd>froh</trd>,
      <trd>zufrieden</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>fortunate</trd>,
      <trd>happy</trd>, <trd>lucky</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="cs">&scaron;&tcaron;astn&yacute;</trd>
  <trd lng="de">gl&uuml;cklich</trd>
  <trd lng="es">feliz</trd>
  <trdgrp lng="fr">
    <trd>heureux</trd>,
    <trd>avantageux</trd>,
    <trd>chanceux<klr tip="ind"> (adj.)</klr></trd>,
    <trd>content</trd>,
    <trd>opportun</trd>,
    <trd>propice</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldog</trd>,
    <trd>szerencs&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">gelukkig</trd>
  <trd lng="oc">a&uuml;r&oacute;s</trd>
  <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="pt">feliz</trd>
</drv>

<drv mrk="felicx.0o">
  <kap><tld/>o</kap>
  <snc mrk="felicx.0o.kontento">
    <dif>
      Sento, stato de plenkontenteco:
      <ekz>
        ne estas sur la tero senmanka <tld/>o;
      </ekz>
      <ekz>
        nemiksita <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de koro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la naski&gcirc;o de filo estis por li granda <tld/>o;
      </ekz>
      <ekz>
        ebria de <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi sopiras je mia perdita <tld/>o
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 29</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">happiness</trd>
    <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_mol;&c_je;</trd>
  </snc>
  <snc mrk="felicx.0o.bonsxanco">
    <dif>
      Bon&scirc;anco:
      <ekz>
        la <tld/>o lin akompanas;
      </ekz>
      <ekz>
        havi <tld/>on &ccirc;e virinoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne veku mal<tld/>on, kiam &gcirc;i dormas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vezik.0o">veziko</ref>.
    </refgrp>
    <trd lng="en">luck</trd>
    <trd lng="ru">&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </snc>
  <snc mrk="felicx.0o.okazajxo">
    <dif>
      <tld lit="F"/>a okaza&jcirc;o:
      <ekz>
        <tld/>o fierigas, <ind>mal<tld/>o</ind> sa&gcirc;igas;
        <trdgrp lng="fr">
          <trd>malheur</trd>,
          <trd>d&eacute;veine</trd>,
          <trd>malchance</trd>
        </trdgrp>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        ne &ccirc;iu havas egalan <tld/>on<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>o malofte venas sole<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        erareco en la nacia traduko de vorto ne prezentas grandan
        mal<tld/>on
        <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>fortune</trd>,
      <trd>luck</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_ch;&c_a;&c_s;&c_t;&c_mol;&c_je;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="es">felicidad</trd>
  <trdgrp lng="fr">
    <trd>bonheur</trd>,
    <trd>chance</trd>,
    <trd>veine<klr tip="ind"> (chance)</klr></trd>,
    <trd>satisfaction</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldogs&aacute;g</trd>,
    <trd>szerencse</trd>
  </trdgrp>
  <trd lng="nl">geluk</trd>
  <trd lng="pt">felicidade</trd>
</drv>

<drv mrk="felicx.0e">
  <kap><tld/>e</kap>
  <snc mrk="felicx.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e vivi;
      </ekz>
      <ekz>
        <tld/>e solvi la problemojn;
      </ekz>
      <ekz>
        la na&gcirc;anto <tld/>e atingis la bordon.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;</trd>
    <trd lng="en">happily</trd>
    <trd lng="ru">&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="felicx.0e.bonsxance">
    <dif>
      Bon&scirc;ance, malbeda&ubreve;rinde, maldoma&gcirc;e:
      <ekz>
        <tld/>e tiu periodo da&ubreve;ris ne longe<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e la entrepreno ne sukcesis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e la akcidento ne estis grava;
      </ekz>
      <ekz>
        <tld/>e estas, ke...
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_d;&c_a;&c_l;&c_a;</trd>,
      <trd>&c_n;&c_a; <ind>&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>luckily</trd>,
      <trd>fortunately</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_a;&c_ch;&c_n;&c_o;</trd>,
      <trd>&c_k; &c_s;&c_ch;&c_a;&c_s;&c_t;&c_mol;&c_ju;</trd>,
      <trd>&c_p;&c_o; &c_s;&c_ch;&c_a;&c_s;&c_t;&c_mol;&c_ju;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>afortunadament</trd>,
    <trd>per <ind>sort</ind></trd>
  </trdgrp>
  <trd lng="es">felizmente</trd>
  <trdgrp lng="fr">
    <trd>heureusement</trd>,
    <trd>avec <ind>bonheur</ind></trd>,
    <trd>d'<ind>heureuse</ind> mani&egrave;re</trd>,
    <trd>par <ind>bonheur</ind></trd>,
    <trd>par <ind>chance</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldogan</trd>,
    <trd>szerencs&eacute;sen</trd>
  </trdgrp>
  <trd lng="nl">gelukkig</trd>
  <trd lng="oc">a&uuml;rosament</trd>
  <trd lng="pt">felizmente</trd>
</drv>

<drv mrk="felicx.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>a:
      <ekz>
        lia edzi&gcirc;o, liaj infanoj, liaj sukcesoj
        <ind>tute <tld/>igi</ind>s lin.
        <trd lng="fr">combler<klr tip="ind"> (rendre pleinement
        heureux)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>gl&uuml;cklich</ind> machen</trd>,
    <trd>begl&uuml;cken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to make happy</trd>,
    <trd>please</trd>, <trd>gladden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rendre <ind>heureux</ind></trd>,
    <trd>satisfaire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldog&iacute;t</trd>,
    <trd>boldogg&aacute; tesz</trd>
  </trdgrp>
  <trd lng="nl">gelukkig maken</trd>
  <trd lng="ru">&c_o;&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="felicx.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Ekstrema <tld/>o:
      <ekz>
        la &ccirc;iela <tld/>ego de la dioj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>bliss</trd>,
    <trd>felicity</trd>
  </trdgrp>
  <trd lng="fr">f&eacute;licit&eacute;</trd>
  <trd lng="hu">&ouml;r&ouml;mm&aacute;mor</trd>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_zh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="felicx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <ref tip="ant" cel="felicx.0a"><tld/>a</ref>
    <refgrp tip="vid">
      <ref cel="mizer.0a">mizera</ref>,
      <ref cel="kompat.0inda">kompatinda</ref>,
      <ref cel="venen.0i.FIG">venenita</ref>,
      <ref cel="cxagre.0a">&ccirc;agrena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_sh;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>unhappy</trd>,
    <trd>sad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>malheureux</trd>,
    <trd>infortun&eacute;</trd>,
    <trd>f&acirc;cheux</trd>,
    <trd>malencontreux</trd>,
    <trd>malchanceux</trd>,
    <trd>regrettable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldogtalan</trd>,
    <trd>szerencs&eacute;tlen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_ch;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="de">ungl&uuml;cklich</trd>
  <trd lng="nl">ongelukkig</trd>
  <trd lng="pt">infeliz</trd>
</drv>

<drv mrk="felicx.mal0e">
  <kap>mal<tld/>e</kap>
  <snc>
    <dif>
      En mal<tld/>a maniero:
      <ekz>
        mal<tld/>e estas, ke vi rompis al vi la brakon;
      </ekz>
      <ekz>
        vorto mal<tld/>e kreita<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bedaux.0inde">beda&ubreve;rinde</ref>,
      <ref cel="domagx.0e">doma&gcirc;e</ref>,
      <ref cel="sxanc">malbon&scirc;ance</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a; <ind>&c_n;&c_ja;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;</ind></trd>,
    <trd>&c_n;&c_je;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">ungl&uuml;cklich</trd>
  <trdgrp lng="en">
    <trd>unfortunately</trd>,
    <trd>unluckily</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>malheureusement</trd>,
    <trd>malencontreusement</trd>,
    <trd>par <ind>malheur</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>boldogtalanul</trd>,
    <trd>szerencs&eacute;tlen&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongelukkig</trd>,
    <trd>jammer genoeg</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k; &c_n;&c_je;&c_s;&c_ch;&c_a;&c_s;&c_t;&c_mol;&c_ju;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_ch;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="pt">infelizmente</trd>
</drv>
</art>
