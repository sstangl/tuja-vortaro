<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: astr.xml,v 1.24 2015/10/13 09:10:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>astr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="astr.0o">
  <kap><tld/>o</kap>
  <snc mrk="astr.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      &Ccirc;iela korpo:
      <refgrp tip="sub">
        <ref cel="stel.0o.AST">stelo</ref>,
        <ref cel="planed.0o">planedo</ref>,
        <ref cel="komet.0o">kometo</ref>
      </refgrp>.
    </dif>
    <trd lng="be">&c_n;&c_ja;&c_b;&c_je;&c_s;&c_n;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    <trd lng="ca">astre</trd>
    <trd lng="de">Himmelsk&ouml;rper</trd>
    <trd lng="en">celestial <ind>body</ind></trd>
    <trd lng="fa">&a_j;&a_s;&a_m; &a_s;&a_m;&a_A;&a_w;&f_y;</trd>
    <trd lng="fr">astre</trd>
    <trd lng="hu">&eacute;gitest</trd>
    <trd lng="it">astro</trd>
    <trd lng="nl">hemellichaam</trd>
    <trd lng="oc">astre</trd>
    <trd lng="pl">cia&lstroke;o niebieskie</trd>
    <trd lng="ru">&c_n;&c_je;&c_b;&c_je;&c_s;&c_n;&c_o;&c_je; &c_t;&c_je;&c_l;&c_o;</trd>
    <trd lng="sk">nebesk&eacute; <ind>teleso</ind></trd>
  </snc>
  <!--  
  mi forkomentas cxi tiun duan sencon.
  ne estas kohere ke la senco "stelo" evitindas,
  dum la unua senco estas difinita kiel supernocio de "stelo".
  &jcirc;v  
  <snc mrk="astr.0o.stelo">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="stel.0o">stelo</ref>
    <trd lng="be">&c_z;&c_o;&c_r;&c_k;&c_a;</trd>
    <trd lng="de">Stern</trd>
    <trd lng="en">star</trd>
    <trd lng="fa">&a_s;&a_t;&a_A;&a_r;&a_h;</trd>
    <trd lng="hu">csillag</trd>
    <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
    <trd lng="sk">hviezda</trd>
  </snc>
  -->
  <trd lng="es">astro</trd>
  <trd lng="sk">nebesk&eacute; teleso</trd>
  <trd lng="cs">nebesk&eacute; t&ecaron;leso <klr>(hv&ecaron;zda| planeta|
  kometa)</klr></trd>
</drv>

<drv mrk="astr.nebul0o">
  <kap>nebul<tld/>o</kap>
  <snc mrk="astr.nebul0o.AST">
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="nebulo.0o.AST">nebulozo</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="br">nivlenn<klr tip="ind"> (steredon.)</klr></trd>
  <trd lng="ca">nebulosa</trd>     
  <trd lng="de">Sternennebel</trd>
  <trd lng="el">&ny;&epsilon;&phi;&epsilon_ton;&lambda;&omega;&my;&alfa;</trd>
  <trd lng="en">nebula</trd>
  <trd lng="fr">n&eacute;buleuse<klr tip="ind"> (astr.)</klr></trd>
  <trd lng="he">&ayin;&resh;&pe;&yod;&lamed;&yod;&tav;</trd>
  <trd lng="hu">csillagk&ouml;d</trd>
  <trd lng="it">nebulosa</trd>
  <trd lng="es">nebulosa</trd>
  <trd lng="nl">nevelvlek</trd>
  <trd lng="oc">nebulosa</trd>
  <trd lng="pl">mg&lstroke;awica</trd>
  <trd lng="pt">nebulosa</trd>
  <trd lng="ru">&c_t;&c_u;&c_m;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">nebulosa</trd>
</drv>

</art>
