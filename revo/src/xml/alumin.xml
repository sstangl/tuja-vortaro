<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alumin.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>alumin</rad>/o
</kap>
<drv mrk="alumin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Al<sub>2</sub>O<sub>3</sub>. Aluminia oksido.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_ib;&c_n;&c_a;&c_z;&c_jo;&c_m;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_ib;&c_d; &c_a;&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hlínovka</trd>,
    <trd>kysličník hlinitý</trd>,
    <trd>oxid hlinitý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tonerde</trd>,
    <trd>Aluminiumoxyd</trd>
  </trdgrp>
  <trd lng="en">alumina</trd>
  <trd lng="es">al&uacute;mina</trd>
  <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_m;&f_y;&a_n;</trd>
  <trd lng="fr">alumine</trd>
  <trdgrp lng="it">
    <trd>allumina</trd>,
    <trd>ossido <klr>ceramico </klr>di <ind>alluminio</ind></trd>
  </trdgrp>
  <trd lng="nl">toonaarde</trd>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_n;&c_o;&c_z;&c_jo;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>alumíniumoxid</trd>,
    <trd>kysli&ccaron;n&iacute;k hlinit&yacute;</trd>,
    <trd>oxid hlinitý</trd>
  </trdgrp>
</drv>
</art>
