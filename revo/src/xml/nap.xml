<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nap.xml,v 1.24 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nap</rad>/o
</kap>
<drv mrk="nap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="brasik.0o">brasiko</ref>
      (<trd lng="la">Brassica napus</trd>),
      kies tubera radiko estas man&gcirc;ebla.
    </dif>
    <ref tip="sub" cel="kolz.0o">kolzo</ref>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_malmol;&c_l;&c_t;&c_a; <ind>&c_r;&c_ja;&c_p;&c_a;</ind></trd>,
    <trd>&c_b;&c_r;&c_ju;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="br">irvinenn</trd>
  <trd lng="ca">nap</trd>
  <trd lng="cs">řepka olejka</trd>
  <trd lng="de">Kohlr&uuml;be</trd>
  <trd lng="el">&gamma;&omikron;&gamma;&gamma;&ypsilon_ton;&lambda;&jota;</trd>
  <trdgrp lng="en">
    <trd>rape</trd>,
    <trd>rapeseed</trd>,
    <trd>canola</trd>
  </trdgrp>
  <trd lng="es">nabo</trd>
  <trd lng="fa">&a_sh;&a_l;&a_gh;&a_m;</trd>
  <trd lng="fr">navet</trd>
  <trd lng="hu">kar&oacute;r&eacute;pa</trd>
  <trd lng="nl">koolraap</trd>
  <trd lng="pt">nabo</trd>
  <trd lng="ru">&c_b;&c_r;&c_ju;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">repka olejná</trd>
  <trd lng="sv">k&aring;lrot</trd>
</drv>
</art>
