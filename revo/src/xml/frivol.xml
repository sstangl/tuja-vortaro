<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frivol.xml,v 1.20 2015/07/04 08:37:34 revo Exp $">
<kap><ofc>1</ofc>
<rad>frivol</rad>/a</kap>
<drv mrk="frivol.0a">
  <kap><tld/>a</kap>
  <snc mrk="frivol.0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malserioza, bagatela, vanta:
      <ekz>
        <tld/>aj aferoj, pensoj;
      </ekz>
      <ekz>
        <tld/>a konduto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vent.0a">venta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>nichtig</trd>,
      <trd>geringf&uuml;gig</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_b;&f_y;&zwnj;&a_A;&a_h;&a_m;&f_y;&a_t;</trd>,
      <trd>&a_b;&f_y;&a_h;&a_w;&a_d;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>komolytalan</trd>,
      <trd>l&eacute;ha</trd>
    </trdgrp>
    <trd lng="nl">onbelangrijk</trd>
  <trdgrp lng="pt">
    <trd>fr&iacute;volo</trd>,
    <trd>f&uacute;til</trd>,
    <trd>leviano</trd>
  </trdgrp>
  </snc>
  <snc mrk="frivol.0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inklina al malseriozaj, vantaj aferoj:
      <ekz>
        <tld/>a virino.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anim.facil0a">facilanima</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>frivol</trd>,
      <trd>leichtsinnig</trd>,
      <trd>lose</trd>,
      <trd>locker</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&a_T;&a_H;&f_y;</trd>,
      <trd>&a_s;&a_b;&f_k;&zwnj;&a_s;&a_r;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;nnyelm&udblac;</trd>,
      <trd>l&eacute;ha</trd>,
      <trd>frivol</trd>
    </trdgrp>
    <trdgrp lng="nl">
    <trd>frivool</trd>,
    <trd>lichtzinnig</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fr&iacute;volo</trd>,
    <trd>f&uacute;til</trd>,
    <trd>leviano</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_r;&c_y;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_a;&c_d;&c_u;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">fr&iacute;vol</trd>
  <trdgrp lng="cs">
    <trd>lehkovážný</trd>,
    <trd>marnivý</trd>,
    <trd>nicotný</trd>,
    <trd>rozpustilý</trd>,
    <trd>uličnický</trd>
  </trdgrp>
  <trd lng="en">frivolous</trd>
  <trd lng="es">fr&iacute;volo</trd>
  <trdgrp lng="fr">
    <trd>frivole</trd>,
    <trd>futile</trd>,
    <trd>insignifiant</trd>,
    <trd>superficiel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>frywolny</trd>,
    <trd>niepowa&zdot;ny</trd>,
    <trd>b&lstroke;ahy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>frivolný</trd>,
    <trd>necudný</trd>
  </trdgrp>
</drv>
</art>
