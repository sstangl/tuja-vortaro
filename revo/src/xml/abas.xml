<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: abas.xml,v 1.14 2015/02/26 20:11:28 revo Exp $">
    <kap>
      <rad>Abas</rad>/o
    </kap>

    <drv mrk="abas.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
	<dif>
	  Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> arabdevena (la&ubreve;vorte
<ctl>severa</ctl>,
	  <ctl>malserena</ctl>), uzata precipe en la islama mondo.
	</dif>
      </snc>
      <trd lng="be">&c_A;&c_b;&c_a;&c_s;</trd>
      <trd lng="ca">'Abbas</trd>
      <trd lng="de">Abbas</trd>
      <trd lng="en">Abbas</trd>
      <trd lng="es">Abb&aacute;s</trd>
      <trd lng="fa">&a_ayn;&a_b;&a_A;&a_s;</trd>
      <trd lng="fr">Abbas</trd>
      <trd lng="hu">Abbasz</trd>
      <trd lng="it">Abbas</trd>
      <trd lng="pl">Abbas</trd>
      <trd lng="pt">Abas</trd>
      <trd lng="ru">&c_A;&c_b;&c_b;&c_a;&c_s;</trd>
    </drv>

    <drv mrk="abas.0ido">
      <kap><tld lit="a"/>ido</kap>
      <uzo tip="fak">HIS</uzo>
      <snc>
	<dif>
	  Ano de la dua dinastio da <ref
cel="hxalif.0o">&hcirc;alifoj</ref>
	  (749&ndash;1298), rektaj posteuloj de <tld/>o la onklo de
	  <ref cel="muhama.0o.profeto">Muhamado</ref>.
	</dif>
      </snc>
      <trd lng="be">&c_a;&c_b;&c_a;&c_s;&c_ib;&c_d;</trd>
      <trd lng="ca">abb&agrave;ssida</trd>
      <trd lng="de">Abbaside</trd>
      <trd lng="en">Abbasid</trd>
      <trdgrp lng="es">
	<trd>abas&iacute;</trd>,
	<trd>abasida</trd>
      </trdgrp>
      <trdgrp lng="fa">
        <trd>&a_ayn;&a_b;&a_A;&a_s;&f_y;</trd>,
        <trd>&a_b;&a_n;&f_y;&zwnj;&a_ayn;&a_b;&a_A;&a_s;</trd>
      </trdgrp>
      <trd lng="fr">Abbasside<klr tip="ind"> (subst.)</klr></trd>
      <trd lng="hu">Abbasszida</trd>
      <trd lng="it">Abbasside</trd>
      <trd lng="pl">Abbasyda</trd>
      <trd lng="pt">ab&aacute;ssida</trd>
      <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_s;&c_i;&c_d;</trd>
    </drv>
  </art>
