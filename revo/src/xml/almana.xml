<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: almana.xml,v 1.20 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>almanak</rad>/o
</kap>
<drv mrk="almana.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Libro enhavanta kalendaron, kun astronomiaj sciigoj, priveteraj 
      anta&ubreve;diroj kaj diversaj informoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_a;&c_h;</trd>
  <trd lng="bg">&c_a;&c_l;&c_m;&c_a;&c_n;&c_a;&c_h;</trd>
  <trd lng="ca">almanac</trd>
  <trdgrp lng="cs">
    <trd>almanach</trd>,
    <trd>ročenka</trd>,
    <trd>sborník</trd>,
    <trd>čítanka</trd>
  </trdgrp>
  <trd lng="da">almanak</trd>
  <trdgrp lng="de">
    <trd>Almanach</trd>,
    <trd>Jahrbuch</trd>
  </trdgrp>
  <trd lng="en">almanac</trd>
  <trd lng="es">almanaque</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_q;&a_w;&f_y;&a_m; &a_n;&a_j;&a_w;&a_m;&f_y;</trd>
    <trd>&a_s;&a_A;&a_l;&a_n;&a_m;&a_A;</trd>
    <trd>&a_s;&a_A;&a_l;&a_n;&a_A;&a_m;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">almanach</trd>
  <trdgrp lng="hu">
    <trd>almanach</trd>,
    <trd>&eacute;vk&ouml;nyv</trd>
  </trdgrp>
  <trd lng="id">almanak</trd>
  <trd lng="nl">almanak</trd>
  <trd lng="pl">almanach</trd>
  <trd lng="pt">almanaque</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_a;&c_h;</trd>
  <trdgrp lng="sk">
    <trd>almanach</trd>,
    <trd>ročenka</trd>,
    <trd>zborník</trd>
  </trdgrp>
  <trd lng="sv">almanacka</trd>
</drv>
</art>
