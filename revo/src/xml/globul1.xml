<kap><rad>globulariac</rad>/oj</kap>
<drv mrk="globul1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Globulariaceae</trd>).
    </dif>
  </snc>
  <trd lng="fr">globulariac&eacute;es</trd>
  <trd lng="de">Kugelblumengew&auml;chse</trd>
  <trd lng="hu">gub&oacute;vir&aacute;gf&eacute;l&eacute;k</trd>
  <trd lng="nl">kogelbloemfamilie</trd>
  <trd lng="pt">globulari&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_r;&c_o;&c_v;&c_n;&c_i;&c_c;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_l;&c_o;&c_b;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>

