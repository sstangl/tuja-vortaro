<vortaro>
<art mrk="$Id: salmed.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>salmed</rad>/oj
</kap>
<drv mrk="salmed.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="salm.0oformaj">salmoformaj
      fi&scirc;oj</ref> (<trd lng="la">Salmonidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_a;&c_s;&c_jo;&c_v;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">salm&ograve;nids</trd>
  <trd lng="cs">lososovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Lachse</trd>
  <trd lng="fr">salmonid&eacute;s</trd>
  <trd lng="hu">lazacf&eacute;l&eacute;k</trd>
  <trd lng="it">salmonidi</trd>
  <trd lng="nl">echte <ind>zalmachtigen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_s;&c_o;&c_s;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_l;&c_o;&c_s;&c_o;&c_s;&c_jo;&c_v;&c_y;&c_je; <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">lososovité</trd>
</drv>
</art>
