<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vulkan1.xml,v 1.17 2015/10/30 06:10:21 revo Exp $">
<kap>
  <rad>vulkaniz</rad>/i
</kap>
<drv mrk="vulkan1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Kombini ka&ubreve;&ccirc;ukon per sulfuro, por ke &gcirc;i fari&gcirc;u pli firma, pli
      elasta
      kaj indiferenta je varmo a&ubreve; malvarmo.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">vulcanitzar</trd>
  <trd lng="cs">vulkanizovat</trd>
  <trd lng="de">vulkanisieren</trd>
  <trd lng="en">vulcanize</trd>
  <trd lng="es">vulcanizar</trd>
  <trd lng="fa">&a_w;&a_damma;&a_l;&f_k;&a_A;&a_n;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">vulcaniser</trd>
  <trd lng="hu">vulkaniz&aacute;l</trd>
  <trd lng="nl">vulcaniseren</trd>
  <trd lng="pl">wulkanizowa&cacute;</trd>
  <trd lng="pt">vulcanizar</trd>
  <trd lng="ru">&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vulkanizova&tcaron;</trd>,
    <trd>zu&scaron;&lcaron;ach&tcaron;ova&tcaron; kau&ccaron;uk s&iacute;rou</trd>
  </trdgrp>
</drv>
</art>
