<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kauxcx.xml,v 1.15 2014/09/14 10:10:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ka&ubreve;&ccirc;uk</rad>/o
</kap>

<drv mrk="kauxcx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Elasta gumo, konsistanta el preparita suko de kelkaj amerikaj
      a&ubreve; afrikaj arboj a&ubreve; lianoj:
      <ekz>
        sulfur<tld/>o
        <klr>(<tld/>o prilaborita per sulfuro, kaj pli malmola ol
        la kruda)</klr>;
      </ekz>
      <ekz><tld/>a mantelo.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gutape.0o">gutaperko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_w;&c_ch;&c_u;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_u;&c_ch;&c_u;&c_k;</trd>
  <trd lng="ca">cautx&uacute;</trd>
  <trd lng="de">Kautschuk</trd>
  <trd lng="en">rubber</trd>
  <trd lng="es">caucho</trd>
  <trd lng="fa">&f_k;&a_A;&a_y_hamza;&a_w;&f_ch;&a_w;</trd>
  <trd lng="fr">caoutchouc</trd>
  <trdgrp lng="hu">
    <trd>gumi</trd>,
    <trd>kaucsuk</trd>
  </trdgrp>
  <trd lng="nl">rubber</trd>
  <trd lng="pl">kauczuk</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_u;&c_ch;&c_u;&c_k;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
