<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nil.xml,v 1.13 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>Nil</rad>/o
</kap>
<drv mrk="nil.N0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Grandega nordafrika <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">rivero</ref> ekfluanta en Etiopujo kaj Sudano
      kaj
      enfluanta en <ref tip="vid"
      cel="mediterane.M0o">Mediteraneon</ref> en
      Egiptujo. Apud <ref tip="vid" cel="amazon2.A0o">Amazono</ref> la
      plej longa rivero en la mondo.
    </dif>
    <trd lng="be">&c_N;&c_ib;&c_l;</trd>
    <trd lng="bg">&c_N;&c_i;&c_l;</trd>
    <trd lng="de">Nil</trd>
    <trd lng="en">Nile</trd>
    <trd lng="hu">N&iacute;lus</trd>
    <trd lng="lat">Nilus</trd>
    <trd lng="mk">&c_N;&c_i;&c_l;</trd>
    <trd lng="nl">Nijl</trd>
    <trd lng="pl">Nil</trd>
    <trd lng="pt">Nilo</trd>
    <trd lng="ru">&c_N;&c_i;&c_l;</trd>
  </snc>
  <trd lng="cs">Nil</trd>
  <trd lng="sk">Níl</trd>
</drv>

<drv mrk="nil.Blanka0o">
  <kap>Blanka <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Sudana alfluo al Nilo.
    </dif>
    <trd lng="be">&c_B;&c_je;&c_l;&c_y; <ind>&c_N;&c_ib;&c_l;</ind></trd>
    <trd lng="bg">&c_B;&c_je;&c_l;&c_i; <ind>&c_N;&c_i;&c_l;</ind></trd>
    <trd lng="de">Wei&szlig;er <ind>Nil</ind></trd>
    <trd lng="en">White <ind>Nile</ind></trd>
    <trd lng="hu">Feh&eacute;r-N&iacute;lus</trd>
    <trd lng="mk">&c_B;&c_je;&c_l;&c_i; <ind>&c_N;&c_i;&c_l;</ind></trd>
    <trd lng="nl">Witte <ind>Nijl</ind></trd>
    <trd lng="pl">Bia&lstroke;y <ind>Nil</ind></trd>
    <trd lng="pt">Nilo Branco</trd>
    <trd lng="ru">&c_B;&c_je;&c_l;&c_y;&c_j; <ind>&c_N;&c_i;&c_l;</ind>
    </trd>
  </snc>
  <trd lng="cs">Bílý Nil</trd>
  <trd lng="sk">Biely Níl</trd>
</drv>

<drv mrk="nil.Blua0o">
  <kap>Blua <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Etiopuja alfluo al Nilo.
    </dif>
    <trd lng="be">&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y; <ind>&c_N;&c_ib;&c_l;</ind></trd>
    <trd lng="bg">&c_S;&c_i;&c_n;&c_i; <ind>&c_N;&c_i;&c_l;</ind></trd>
    <trd lng="de">Blauer <ind>Nil</ind></trd>
    <trd lng="en">Blue <ind>Nile</ind></trd>
    <trd lng="hu">K&eacute;k-N&iacute;lus</trd>
    <trd lng="mk">&c_S;&c_i;&c_n;&c_i; <ind>&c_N;&c_i;&c_l;</ind></trd>
    <trd lng="nl">Blauwe <ind>Nijl</ind></trd>
    <trd lng="pl">B&lstroke;&eogonek;kitny <ind>Nil</ind></trd>
    <trd lng="pt">Nilo Azul</trd>
    <trd lng="ru">&c_G;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j; <ind>&c_N;&c_i;&c_l;</ind></trd>
  </snc>
</drv>
</art>
