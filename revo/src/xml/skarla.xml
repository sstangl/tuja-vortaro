<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skarla.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skarlat</rad>/o
</kap>
<drv mrk="skarla.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sange ru&gcirc;a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_a;-&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_v;&c_o;&c_v;&c_y; &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nach</trd>,
    <trd>šarlat</trd>,
    <trd>šarlatová červeň</trd>
  </trdgrp>
  <trd lng="de">Scharlachrot</trd>
  <trd lng="en">scarlet</trd>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">skarl&aacute;tv&ouml;r&ouml;s sz&iacute;n</trd>
  <trd lng="nl">scharlaken</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_r;&c_ja;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">šarlátová červeň</trd>
</drv>

<drv mrk="skarla.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sange <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref>:
      <ekz>
        &leftquot;kiakolora la skarpo?&rightquot; demandis la knabino.
        &leftquot;Skarlata.&rightquot;<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 35a, p. 498a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_a;-&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_v;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">šarlatový</trd>
  <trd lng="en">scarlet</trd>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">skarl&aacute;tv&ouml;r&ouml;s</trd>
  <trd lng="sk">šarlátový</trd>
</drv>
</art>
