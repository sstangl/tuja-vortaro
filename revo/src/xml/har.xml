    <trd>riasnica</trd>
  </trdgrp>
</drv>

<drv mrk="har.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="har.sen0a.ANA">
    <dif>
      Ne havanta <tld/>ojn, perdinta la <tld/>ojn:
      <ekz>
        la kruela patro elektis por &scirc;i malbelegan, maljunan,
        sen<tld/>an baronon
        <fnt><bib>FK</bib>, <lok>Arturo (V. Devjatnin)</lok></fnt>.
      </ekz>
      <ekz>
        sen<tld/>a kapo.
      </ekz>
    </dif>
    <ref tip="sin" cel="kalv.0a">kalva</ref>
  </snc>
  <trd lng="br">moal</trd>
  <trdgrp lng="cs">
    <trd>bezvlasý</trd>,
    <trd>holohlavý</trd>,
    <trd>lysý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>haarlos</trd>,
    <trd>glatzk&ouml;pfig</trd>
  </trdgrp>
  <trd lng="en">bald</trd>
  <trd lng="es">calvo</trd>
  <trdgrp lng="fr">
    <trd>chauve</trd>,
    <trd>glabre</trd>
  </trdgrp>
  <trd lng="hu">kopasz</trd>
  <trdgrp lng="id">
    <trd>botak</trd>,
    <trd>gundul</trd>
  </trdgrp>
  <trd lng="it">calvo</trd>
  <trd lng="nl">kaal</trd>
  <trd lng="pl">&lstroke;ysy</trd>
  <trdgrp lng="pt">
    <trd>careca<klr tip="ind"> (adj.)</klr></trd>,
