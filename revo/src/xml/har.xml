</drv>

<drv mrk="har.lip0aro">
  <kap>lip<tld/>aro,
  <var><kap><ofc>*</ofc>lip<tld/>oj</kap></var></kap>
  <snc mrk="har.lip0aro.FRIZ">
    <dif>
      Tuto de la <tld/>oj kreskantaj &ccirc;e la supera lipo:
      <ekz>
        liaj lip<tld/>oj estas pli grizaj, ol liaj vangharoj
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        densa lip<tld/>aro
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li bremsis 
        sufi&ccirc;e?</vrk>, <lok>1979</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_y;</trd>
  <trd lng="br">mourro&ugrave;</trd>
  <trd lng="cs">knír</trd>
  <trdgrp lng="de">
    <trd>Oberlippenbarthaare</trd>,
    <trd>Schnauzer</trd>,
    <trd>Bart<klr>(haare)</klr></trd>
  </trdgrp>
  <trd lng="el">&my;&omikron;&ypsilon;&sigma;&tau;&alfa_ton;&kappa;&jota;</trd>
  <trd lng="en">moustache</trd>
  <trd lng="es">bigote</trd>
  <trd lng="fr">moustache</trd>
  <trd lng="he">&shin;&pe;&fmem;</trd>
  <trd lng="hu">bajusz</trd>
  <trd lng="id">kumis</trd>
  <trd lng="it">baffi</trd>
  <trd lng="nl">snor</trd>
  <trd lng="pl">w&aogonek;sy</trd>
  <trd lng="pt">bigode</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;</trd>
  <trd lng="sk">fúzy</trd>
  <trd lng="sv">mustasch</trd>
</drv>

<drv mrk="har.okul0o">
  <kap><ofc>*</ofc>okul<tld/>o</kap>
  <snc mrk="har.okul0o.ANA">
    <dif>
      <tld lit="H"/>o &ccirc;e la rando de palpebroj:
      <ekz>
        li <klr>...</klr> vidis larmojn, kiuj tremis inter &scirc;iaj
        okul<tld/>oj
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_i;&c_g;&c_l;&c_a;</trd>
  <trd lng="br">malgudenno&ugrave;</trd>
  <trdgrp lng="cs">
    <trd>brva</trd>,
    <trd>řasa</trd>
  </trdgrp>
  <trd lng="de">Wimper</trd>
  <trd lng="en">eyebrow</trd>
  <trd lng="es">ceja</trd>
  <trd lng="fr">cil</trd>
  <trd lng="hu">szempilla</trd>
  <trd lng="it">ciglia<klr tip="amb"> (di palpebre)</klr></trd>
  <trd lng="nl">wimper</trd>
  <trd lng="pl">rz&eogonek;sy</trd>
  <trdgrp lng="pt">
    <trd>c&iacute;lio</trd>,
    <trd>pestana</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_n;&c_i;&c_c;&c_y;</trd>
  <trdgrp lng="sk">
