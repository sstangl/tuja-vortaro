  <trd lng="cs">hříva <klr>(lva| koně)</klr></trd>
  <trd lng="de">M&auml;hne</trd>
  <trd lng="en">mane</trd>
  <trdgrp lng="es">
    <trd>melena</trd>,
    <trd>crin</trd>
  </trdgrp>
  <trd lng="fr">crini&egrave;re</trd>
  <trd lng="hu">s&ouml;r&eacute;ny</trd>
  <trd lng="it">criniera</trd>
  <trd lng="nl">manen</trd>
