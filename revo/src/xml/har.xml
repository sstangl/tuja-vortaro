</kap>

<drv mrk="har.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="har.0o.mamulo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="filame.0o">Filamento</ref> kreskanta el
      la
      ha&ubreve;to de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      kaj pli-malpli dense kovranta ilian korpon:
      <ekz>
        <tld/>o estas tre maldika
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        tondi <tld/>ojn;
      </ekz><ekz>
        kombi al si la <tld/>ojn;
      </ekz>
      <ekz>
        la <tld/>oj de negroj similas stupon;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>disfendi, dissplitigi harojn</ind>
        <klr>(subtila&ccirc;e diskuti)</klr>;
        <trd lng="br">spazha&ntilde; laou</trd>
        <trd lng="de">Haare <ind>spalten</ind></trd>
        <trd lng="fr">couper les <ind>cheveux</ind> en quatre</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_v; <ind>&c_m;&c_je;&c_l;&c_o;&c_ch;&c_a;&c_h;</ind></trd>,
          <trd>&c_b;&c_u;&c_k;&c_v;&c_o;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_a;&c_n;&c_u;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        lia vivo estas sur <tld/>eto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pendas de, el <tld/>eto<fnt>Z</fnt>
        <klr>(estas en ekstrema dan&gcirc;ero)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        li estas <ind><tld/>plena</ind> kiel urso.
        <trd lng="br">blevek</trd>
        <trd lng="fr">poilu</trd>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>hajsz&aacute;l</trd>,
      <trd>sz&odblac;rsz&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>pelo</trd>,
      <trd>capello</trd>
    </trdgrp>
    <refgrp tip="vid">
      <ref cel="bukl.0o">buklo</ref>,
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="fibr.0o">fibro</ref>,
      <ref cel="peruk.0o">peruko</ref>
    </refgrp>
    <trd lng="pt">cabelo</trd>
    <trd lng="sv">h&aring;rstr&aring;</trd>
  </snc>
  <snc mrk="har.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Fibra elkreska&jcirc;o el la epidermo de
      <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>.
    </dif>
    <trd lng="hu">sz&odblac;r</trd>
    <trd lng="it">tricoma</trd>
    <trd lng="pt">p&ecirc;lo<klr tip="amb"> (de vegetal)</klr></trd>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_malmol;&c_m;</trd>
  <trd lng="br">blevenn</trd>
  <trdgrp lng="cs">
    <trd>chlup</trd>,
    <trd>vlas</trd>,
    <trd>vlasy</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trd lng="en">hair</trd>
  <trdgrp lng="es">
    <trd>cabello</trd>,
    <trd>pelo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cheveu</trd>,
    <trd>poil</trd>
  </trdgrp>
  <trd lng="id">rambut</trd>
  <trd lng="nl">haar</trd>
  <trd lng="pl">w&lstroke;os</trd>
  <trdgrp lng="pt">
    <trd>cabelo</trd>,
    <trd>p&ecirc;lo</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>chlp</trd>,
    <trd>vlas</trd>
  </trdgrp>
  <trd lng="tp">linja</trd>
</drv>

<drv mrk="har.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <snc mrk="har.0aro.FRIZ">
    <dif>
      Tuto de la kapaj <tld/>oj de iu homo,
      de la <tld/>oj de iu besto:
      <ekz>
        lia korve nigra <tld/>aro
        <fnt><bib>Kiso</bib>, <lok>poetoj kaj homoj</lok></fnt>;
      </ekz><ekz>
        blonda, griza <tld/>aro.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>blev</trd>,
      <trd>blevenn<klr> [bestoj]</klr>
        <klr tip="ind"> (hollad blev ar bronneged)</klr></trd>,
      <trd>pennad blev</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hair <klr tip="amb">(person)</klr></trd>,
      <trd>coat <klr tip="amb">(animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cabellera</trd>,
      <trd>pelaje<klr tip="ind"> (animal)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>chevelure</trd>,
      <trd>cheveux</trd>,
      <trd>pelage<klr tip="ind"> (d'un animal)</klr></trd>
    </trdgrp>
    <trd lng="hu">haj</trd>
    <trdgrp lng="nl">
      <trd>haardos</trd>,
      <trd>haar</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>chioma</trd>, 
      <trd>capelli</trd>,
      <trd>capigliatura</trd>, 
      <trd>manto<klr> (di animali)</klr></trd>, 
      <trd>pelame<klr> (di animali)</klr></trd>
    </trdgrp>
    <trd lng="pl">ow&lstroke;osienie</trd>
    <trd lng="ru">&c_sh;&c_je;&c_v;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>
    <trd lng="sv">h&aring;r</trd>
  </snc>
  <snc mrk="har.0aro.peruko">
    <uzo tip="stl">ARK</uzo>
    <fnt>Z</fnt>
    <ref tip="dif" cel="peruk.0o">peruko</ref>
    <trd lng="br">perukenn</trd>
    <trd lng="de">Per&uuml;cke</trd>
    <trd lng="es">peluca</trd>
    <trd lng="fr">perruque</trd>
    <trd lng="it">parrucca</trd>
    <trd lng="pl">peruka</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_k;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kštice</trd>,
    <trd>srst</trd>,
    <trd>vlasy</trd>
  </trdgrp>
  <trd lng="de">Haar</trd>
  <trdgrp lng="hu">
    <trd>par&oacute;ka</trd>,
    <trd>vend&eacute;ghaj</trd>
  </trdgrp>
  <trd lng="pt">cabeleira</trd>
  <trdgrp lng="sk">
    <trd>srsť</trd>,
    <trd>vlasy</trd>,
    <trd>štica</trd>
  </trdgrp>
</drv>

<drv mrk="har.berenicaH0aro">
  <kap>Berenica <tld lit="H"/>aro
    <fnt><vrk><url ref="http://at.komputilo.org">Astronomia
          Terminaro</url></vrk></fnt>
  </kap>
  <mlg>Com</mlg>
  <snc mrk="har.berenicaH0aro.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-duonsfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Coma Berenices</trd>).
    </dif>
  </snc>
  <trd lng="de">Haar der Berenike</trd>
  <trd lng="en">Coma Berenices</trd>
  <trd lng="es">Cabellera de Berenices</trd>
  <trd lng="it">Chioma di Berenice</trd>
  <trd lng="pl">Warkocz Bereniki</trd>
  <trd lng="ru">&c_V;&c_o;&c_l;&c_o;c&c_y; &c_V;&c_je;&c_r;&c_o;&c_n;&c_i;&c_k;&c_i;</trd>
</drv>

<drv mrk="har.0ego">
  <kap><ofc>*</ofc><tld/>ego</kap>
  <snc mrk="har.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dika <tld/>o de iuj bestoj:
      <ekz>
        persistemaj &ccirc;evaloj kun longaj <tld/>egoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>egoj de apro.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">reun</trd>
  <trdgrp lng="cs">
    <trd>štětina</trd>,
    <trd>žíně</trd>
  </trdgrp>
  <trd lng="de">Borste</trd>
  <trd lng="en">bristle</trd>
  <trd lng="es">cerda</trd>
  <trdgrp lng="fr">
    <trd>crin</trd>,
    <trd>soie<klr tip="ind"> (poil)</klr></trd>
  </trdgrp>
  <trd lng="hu">s&ouml;rte</trd>
  <trdgrp lng="it">
    <trd>setola</trd>, 
    <trd>crine</trd>
  </trdgrp>
  <trd lng="nl">borstel</trd>
  <trd lng="pl">sier&sacute;&cacute;</trd>
  <trd lng="pt">crina</trd>
  <trd lng="ru">&c_shch;&c_je;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">štetina</trd>
</drv>

<drv mrk="har.0plektajxo">
  <kap><tld/>plekta&jcirc;o</kap>
  <snc mrk="har.0plektajxo.FRIZ">
    <dif>
      Kelkaj (plej ofte, tri) &scirc;nure kunplektitaj tufoj da
      kap<tld/>oj:
      <ekz>
        kiam li vidis min, li &ccirc;iam tiris
        miajn longajn ru&gcirc;ajn <tld/>plekta&jcirc;ojn kaj &scirc;ajnigis, ke li
        bruligis sian manon
        <fnt>
          <aut>A. L&ouml;wenstein</aut>, <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        junulinoj en Kroatio amase aran&gcirc;as por si <tld/>plekta&jcirc;ojn
        kiel Janica
        <fnt>
          <aut>Z. Ti&scaron;ljar</aut>,
          <vrk>Janica Kosteli&cacute; - skia miraklo</vrk>,
          <bib>Monato</bib>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;<klr tip="amb"> (&c_k;&c_o;&c_s;&c_a;)</klr></trd>
  <trd lng="br">plezhenn<klr tip="ind"> (blev)</klr></trd>
  <trd lng="de">Zopf</trd>
  <trd lng="en">plait</trd>
  <trd lng="es">trenza</trd>
  <trd lng="fr">tresse<klr tip="ind"> (de cheveux)</klr></trd>
  <trdgrp lng="hu">
    <trd>copf</trd>,
    <trd>hajfonat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>treccia<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd>codino<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd><ind>coda</ind> di cavallo<klr tip="amb">
        (acconciatura)</klr></trd>
  </trdgrp>
  <trd lng="nl">vlecht</trd>
  <trd lng="pl">warkocz</trd>
  <trd lng="pt">tran&ccedil;a<klr tip="amb"> (de cabelo)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a; <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>

<drv mrk="har.0ligo">
  <kap><ofc>*</ofc> <tld/>ligo</kap>
  <uzo tip="stl">ARK</uzo>
  <ref tip="dif" cel="har.0plektajxo"><tld/>plekta&jcirc;o</ref>
  <ekz>
    &scirc;ia korpo estis admirinde gracia; &scirc;ia <tld/>ligo de ora koloro
    kaj peza kiel oro, falis &gcirc;is la genuoj, sed neniu povus &scirc;in nomi
    belulino
    <fnt><bib>PatrojFiloj</bib>, <lok>&Ccirc;ap. 7a.</lok></fnt>;
  </ekz>
  <ekz>
    dika plekta&jcirc;o de nigraj <tld/>ligoj
    <fnt><bib>Marta</bib></fnt>.
  </ekz>
  <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;<klr tip="amb">
      (&c_k;&c_o;&c_s;&c_a;)</klr></trd>
  <trd lng="br">plezhenn<klr tip="ind"> (blev)</klr></trd>
  <trd lng="de">Zopf</trd>
  <trd lng="en">plait</trd>
  <trd lng="es">trenza</trd>
  <trd lng="fr">tresse<klr tip="ind"> (de cheveux)</klr></trd>
  <trdgrp lng="hu">
    <trd>copf</trd>,
    <trd>hajfonat</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>treccia<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd>codino<klr tip="amb"> (acconciatura)</klr></trd>, 
    <trd><ind>coda</ind> di cavallo<klr tip="amb">
        (acconciatura)</klr></trd>
  </trdgrp>
  <trd lng="nl">vlecht</trd>
  <trd lng="pl">warkocz</trd>
  <trd lng="pt">tran&ccedil;a<klr tip="amb"> (de cabelo)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a; <klr>(&c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
</drv>

