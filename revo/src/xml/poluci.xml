<kap>
  <rad>poluci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="poluci.0o">
  <kap><tld/>o</kap>
  <snc mrk="poluci.0o.vir">
  <uzo tip="fak">MED</uzo>
    <dif>
      Nevola elfluo de spermo sen koitado, precipe dum son&gcirc;ado.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_ja;</trd>
    <trd lng="bg">&c_p;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
    <trd lng="de">Pollution</trd>
    <trd lng="en">nocturnal emission</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_H;&a_t;&a_l;&a_A;&a_m;</trd>,
      <trd>&a_A;&a_n;&a_z;&a_A;&a_l; &a_sh;&a_b;&a_A;&a_n;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pollution nocturne</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;jszakai mag&ouml;ml&eacute;s</trd>,
      <trd>poll&uacute;ci&oacute;</trd>,
    </trdgrp>
    <trd lng="it">polluzione notturna</trd>
    <trd lng="pt">polu&ccedil;&atilde;o</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="poluci.0o.medi">
    <uzo tip="fak">EKOL</uzo>
      <ref tip="dif" cel="polu.0ado">poluado</ref>
      <ekz>
        la publiko ne sufi&ccirc;e konsideras la eksterajn kostojn de
        transportilo: <tld/>on, bruon, klimat&scirc;an&gcirc;on kaj
        akcidentojn<fnt><aut>Roland Rotsaert</aut>: <vrk>Medio-indulgo
        de
        la &ccirc;efaj ekonomiaj sektoroj</vrk>,
<bib>Monato</bib><lok>jaro
        2001a, numero 3a, p. 18a</lok></fnt>.
      </ekz>
    <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="bg">&c_z;&c_a;&c_m;&c_malmol;&c_r;&c_s;&c_ja;&c_v;&c_a;&c_n;&c_je;</trd>
    <trd lng="de">Umweltverschmutzung</trd>
    <trd lng="en">pollution</trd>
    <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_d;&f_g;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>pollution</trd>
    </trdgrp>
    <trd lng="hu">k&oacute;rnyezetszennyez&eacute;s</trd>
    <trd lng="it">inquinamento</trd>
  </snc>
  <trd lng="cs">znečištění životního prostředí</trd>
  <trd lng="nl">pollutie</trd>
  <trd lng="pt">polui&ccedil;&atilde;o</trd>
  <trd lng="sk">znečistenie životného prostredia</trd>
</drv>
</art>
