<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lavend.xml,v 1.23 2015/07/04 08:37:38 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>lavend</rad>/o
</kap>
<drv mrk="lavend.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Lavandula</trd>),
      el kies unu specio <trd lng="la">Lavandula angustifolia</trd>
      oni produktas bonodoran oleon; 
      duon<ref tip="lst" cel="arbust.0o" 
      lst="voko:lamioparencoj">arbusto</ref>,
      foje arbusto a&ubreve; sta&ubreve;do,
      kun nesimetriaj kvinlobaj intense bluaj a&ubreve; violkoloraj floroj,
      kies mallongaj stiluso kaj stamenoj ka&scirc;i&gcirc;as en la
      korola tubeto; la
      fruktoj disfalas en unusemajn &ccirc;elojn &ccirc;e maturi&gcirc;o:
      <ekz>
        beleta malgranda flor&gcirc;ardeno kie oni sentis la odoron 
        de rozoj kaj <tld/>o
        <fnt><bib>Fab4</bib>, <lok>Sterkskarabo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_v;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_v;&c_a;&c_n;&c_d;&c_u;&c_l;&c_a;</trd>
  <trd lng="cs">levandule</trd>
  <trd lng="de">Lavendel</trd>
  <trd lng="en">lavander</trd>
  <trd lng="fr">lavande</trd>
  <trd lng="hu">levendula</trd>
  <trd lng="nl">lavendel</trd>
  <trdgrp lng="pt">
    <trd>lavanda <klr tip="amb">(bot.)</klr></trd>,
    <trd>alfazema <klr tip="amb">(bot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">levanduÄ¾a</trd>
  <trd lng="sv">lavendel</trd>
</drv>
</art>
