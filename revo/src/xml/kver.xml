<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kver.xml,v 1.14 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kver</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kver.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kver.0i.SON">
    <uzo>(pri turtoj kaj kolomboj)</uzo>
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleki</ref>
      per milda murmuro.
    </dif>
    <refgrp tip="vid">
      <ref cel="kurre.0.ZOO">kurre</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>girren</trd>
    </trdgrp>
  </snc>
  <snc mrk="kver.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ame interparoli.
    </dif>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>turteln</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>cukrovat <klr>(hrdlička)</klr></trd>,
    <trd>vrkat</trd>
  </trdgrp>
  <trd lng="en">coo</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trd lng="nl">kirren</trd>
  <trdgrp lng="pt">
    <trd>arrulhar</trd>,
    <trd>gorjear</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hrkútať</trd>
</drv>
</art>
