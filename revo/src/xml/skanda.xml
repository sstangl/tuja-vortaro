<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skanda.xml,v 1.16 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>skandal</rad>/o
</kap>
<drv mrk="skanda.0o">
  <kap><tld/>o</kap>
  <snc mrk="skanda.0o.PSI">
    <dif>
      Indigno ka&ubreve;zata de malbonekzemplaj agoj a&ubreve; paroloj:
      <ekz>
        <tld/>a konduto<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="skanda.0o.fifamo">
    <dif>
      Beda&ubreve;rinda kaj dan&gcirc;era diskoniteco de iu malbona ekzemplo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>ostuda</trd>,
    <trd>pohoršení</trd>,
    <trd>skandál</trd>,
    <trd>škandál <klr>(hovor.)</klr></trd>
  </trdgrp>
  <trd lng="de">Skandal</trd>
  <trd lng="en">scandal</trd>
  <trdgrp lng="fr">
    <trd>scandale</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>botr&aacute;ny</trd>,
    <trd>skandalum</trd>
  </trdgrp>
  <trd lng="nl">schandaal</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>pohoršenie</trd>,
    <trd>výtržnosť</trd>,
    <trd>škandál</trd>
  </trdgrp>
</drv>

<drv mrk="skanda.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on al iu.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    &c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>budit pohoršení</trd>,
    <trd>ostouzet</trd>,
    <trd>skandalizovat</trd>,
    <trd>způsobit ostudu</trd>,
    <trd>způsobit pohoršení</trd>,
    <trd>způsobit skandál</trd>
  </trdgrp>
  <trd lng="de"><klr>einen </klr>Skandal verursachen</trd>
  <trd lng="en">scandalize</trd>
  <trdgrp lng="fr">
    <trd>scandaliser</trd>
  </trdgrp>
  <trd lng="hu">botr&aacute;nyt okoz</trd>
  <trd lng="nl">te <ind>schande</ind> maken</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <ind>&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;</ind> <klr>(&c_p;&c_je;&c_r;&c_je;&c_d;
    &c_k;&c_je;&c_m;-&c_l;.,
    &c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>budiť pohoršenie</trd>,
    <trd>robiť výtržnosť</trd>
  </trdgrp>
</drv>
</art>
