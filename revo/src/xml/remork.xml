<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: remork.xml,v 1.5 2015/11/26 08:10:15 revo Exp $">
<kap>
  <rad>remork</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="remork.0o">
  <kap><tld/>o</kap>
  <snc mrk="remork.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <ref tip="dif" cel="vetur.post0ilo">postveturilo</ref>
    <ekz>
      la tieaj kulturistoj rikoltas tritikon plenigante
      <tld/>ojn
      <fnt>
        <aut>J.-J. Moro</aut>,
        <vrk>Periplo tra la Baltaj Landoj</vrk>,
        <lok><url ref="http://eomikelo.free.fr/lukoj/luko23.pdf">
        Turan&gcirc;ia Luko, 2011:3 (23)</url></lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="ca">remolc</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;v&ecaron;sn&eacute; vozidlo <klr>(za autem a j.)</klr></trd>,
    <trd>vlek</trd>,
    <trd>vle&ccaron;n&yacute; &ccaron;lun</trd>
  </trdgrp>
  <trd lng="fr">remorque</trd>
  <trdgrp lng="hu">
    <trd>ut&aacute;nfut&oacute;</trd>,
    <trd>p&oacute;tkocsi</trd>
  </trdgrp>
  <trd lng="pl">przyczepa</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;ves <klr>(automobilov&yacute;, bicyklov&yacute;...)</klr></trd>,
    <trd>vle&ccaron;n&yacute; &ccaron;ln</trd>
  </trdgrp>
  <trd lng="sv">sl&auml;p</trd>
  <trd lng="tr">r&ouml;mork</trd>
</drv>

<drv mrk="remork.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="remork.0i.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Tiradi kiel postveturilon:
      <ekz>
        e&ccirc; tre junaj infanoj povas partopreni
        side en trenveturiloj bicikle <tld/>ataj
        de siaj gepatroj
        <fnt>
          <aut>Johano</aut>,
          <vrk>Radio Verda</vrk>, 
          <lok><url ref="">RV102</url>, 2007-05-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">remolcar</trd>
  <trdgrp lng="cs">
    <trd>remorkovat</trd>,
    <trd>tla&ccaron;it <klr>(lo&dcaron;, p&rcaron;&iacute;v&ecaron;s)</klr></trd>,
    <trd>t&aacute;hnout</trd>,
    <trd>vl&eacute;ci</trd>
  </trdgrp>
  <trd lng="fr">remorquer</trd>
  <trdgrp lng="sk">
    <trd>remorkova&tcaron;</trd>,
    <trd>tla&ccaron;i&tcaron; <klr>(lo&dcaron;, pr&iacute;ves)</klr></trd>,
    <trd>vliec&tcaron;</trd>,
    <trd>&tcaron;aha&tcaron;</trd>
  </trdgrp>
</drv>
</art>

