<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: luks.xml,v 1.21 2015/07/04 08:37:38 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>luks</rad>/o
</kap>
<drv mrk="luks.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Elmontrema kaj multekosta pompa brileco en vivmaniero:
       <ekz>
         <tld/>o en vestoj, pri man&gcirc;oj;
       </ekz>
       <ekz>
         re&gcirc;a <tld/>o.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="komfor">komforto</ref>,
       <ref cel="pomp.0o">pompo</ref>,
       <ref cel="ricx">ri&ccirc;eco</ref>,
       <ref cel="abund.0eco">abundeco</ref>.
     </refgrp>
     <refgrp tip="vid">
       <ref cel="velur.0o">veluro</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_l;&c_ju;&c_k;&c_s;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_a;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_h;&c_a;&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>luxus</trd>,
    <trd>přepych</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Pracht</trd>,
      <trd>Luxus</trd>,
      <trd>Aufwand</trd>,
      <trd>Verschwendung</trd>
   </trdgrp>
  <trd
lng="el">&pi;&omikron;&lambda;&ypsilon;&tau;&epsilon_ton;&lambda;&epsilon;&jota;&alfa;</trd>
  <trd lng="en">luxury</trd>
  <trd lng="es">lujo</trd>
  <trdgrp
lng="fr"><trd>luxe</trd><trd>faste</trd><trd>somptuosit&eacute;</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;ny&udblac;z&eacute;s</trd>,
    <trd>luxus</trd>
  </trdgrp>
  <trd lng="nl">luxe</trd>
  <trd lng="pt">luxo</trd>
  <trdgrp lng="ru">
     <trd>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_mol;</trd>,
     <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>luxus</trd>,
    <trd>nádhera</trd>,
    <trd>prepych</trd>
  </trdgrp>
  <trd lng="sv">lyx</trd>
</drv>

<drv mrk="luks.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Elmontranta <tld/>on:
       <ekz>
         <tld/>a la vesto, sed malplena
         la po&scirc;o (<ref tip="vid" cel="murmur">murmuro</ref>)
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         pli bona bran&ccirc;o sennuksa ol ka&gcirc;o plej
         <tld/>a
         (<ref tip="vid" cel="liber.0o">libero</ref>)
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>a fenestro, meblaro, lo&gcirc;ejo;
       </ekz>
       <ekz>
         mi ne povas permesi al mi tian <tld/>an malavarecon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         floroj en plej <tld/>a florado<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kontra&ubreve;<tld/>a le&gcirc;o;
       </ekz>
       <ekz>
         vivi lar&gcirc;e kaj <tld/>e
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ref tip="sin" cel="flu.super0a">superflua</ref>.
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_l;&c_ju;&c_k;&c_s;&c_a;&c_v;&c_y;</trd>,
     <trd>&c_l;&c_ju;&c_k;&c_s;&c_u;&c_s;&c_o;&c_v;&c_y;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>luxusní</trd>,
    <trd>přepychový</trd>,
    <trd>skvostný</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>pr&auml;chtig</trd>,
      <trd>&uuml;ppig</trd>,
      <trd>luxuri&ouml;s</trd>,
      <trd>aufw&auml;ndig</trd>,
      <trd>verschwenderisch</trd>
   </trdgrp>
  <trd
lng="el">&pi;&omikron;&lambda;&ypsilon;&tau;&epsilon;&lambda;&eta_ton;&sigma_fina;</trd>
  <trd lng="es">lujoso</trd>
  <trdgrp lng="fr"><trd>luxueux</trd><trd>fastueux</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;ny&udblac;z&odblac;</trd>,
    <trd>luxus-</trd>
  </trdgrp>
  <trd lng="nl">luxueus</trd>
  <trd lng="pt">luxuoso</trd>
  <trdgrp lng="ru">
     <trd>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_k;&c_l;&c_a;&c_s;&c_s;&c_a;
&leftquot;&c_l;&c_ju;&c_k;&c_s;&rightquot;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>luxusný</trd>,
    <trd>prepychový</trd>,
    <trd>skvostný</trd>
  </trdgrp>
  <trd lng="sv">lyxig</trd>
</drv>

<drv mrk="luks.sen0ane0a">
   <kap>sen<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
   <snc>
     <dif>
       Simpla, modesta:
       <ekz>
         &ccirc;ambro sen<tld/>a sed komforta.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
   </trdgrp>
   <trdgrp lng="de">
      <trd>bescheiden</trd>,
      <trd>einfach</trd>
   </trdgrp>
   <trdgrp lng="hu">
      <trd>f&eacute;ny&udblac;z&eacute;st&odblac;l mentes</trd>,
      <trd>szer&eacute;ny</trd>
   </trdgrp>
   <trd lng="nl">sober</trd>
   <trd lng="fr">simple</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j;</trd>,
     <trd>&c_b;&c_je;&c_z; <ind>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_i;</ind></trd>
   </trdgrp>
</drv>

<drv mrk="luks.mal0a">
   <kap>mal<tld/>a</kap>
   <snc>
     <dif>
       Havanta malri&ccirc;an aspekton.
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_je;&c_d;&c_n;&c_y; <klr>(&c_n;&c_a;
&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jednoduchý</trd>,
    <trd>skromný</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>karg</trd>,
      <trd>&auml;u&szlig;erst bescheiden</trd>
   </trdgrp>
  <trdgrp lng="fr">
      <trd>pauvre</trd>,
      <trd>indigent</trd>
   </trdgrp>
  <trdgrp lng="hu">
      <trd>szeg&eacute;nyes</trd>,
      <trd>nyomor&uacute;s&aacute;gos</trd>
   </trdgrp>
  <trd lng="nl">armelijk</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_n;&c_y;&c_j; &c_n;&c_a; &c_v;&c_i;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>jednoduchý</trd>,
    <trd>skromný</trd>
  </trdgrp>
</drv>
</art>
