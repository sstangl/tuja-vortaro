<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: foin.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>foin</rad>/o
</kap>
<drv mrk="foin.0o">
  <kap><tld/>o</kap>
  <snc mrk="foin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
       lst="voko:zoologiaj_specioj">Specio</ref> de <ref tip="super" 
       cel="martes.0o.ZOO_genro">marteso</ref>
      (<trd lng="la">Martes foina</trd>),
      malgranda <ref tip="lst" cel="best.rabo0o" 
      lst="voko:rabobestoj">rabobesto</ref> el E&ubreve;ropo kaj Norda Azio,
      kun
      duparta blanka brustmakulo, konata pri tio, ke &gcirc;i &ccirc;asas anka&ubreve;
      kortbirdojn
      kaj dismordas gumajn partojn de a&ubreve;toj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;</ind></trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">fagina</trd>
  <trdgrp lng="cs">
    <trd>kuna domácí</trd>,
    <trd>kuna skaln&iacute;</trd>
  </trdgrp>
  <trd lng="da">husm&aring;r</trd>
  <trd lng="de">Steinmarder</trd>
  <trdgrp lng="en">
    <trd>beech <ind>marten</ind></trd>,
    <trd>stone <ind>marten</ind></trd>
  </trdgrp>
  <trd lng="fr">fouine</trd>
  <trd lng="hu">nyest</trd>
  <trd lng="it">faina</trd>
  <trd lng="nl">steenmarter</trd>
  <trdgrp lng="pl">
    <trd>kuna domowa</trd>,
    <trd>kamionka <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fuinha</trd>,
    <trd>fuinho</trd>,
    <trd>gardunho</trd>,
    <trd>gardunho</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">kuna skalná</trd>
  <trd lng="sv">stenm&aring;rd</trd>
</drv>
</art>
