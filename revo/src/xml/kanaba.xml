<vortaro>
<art mrk="$Id: kanaba.xml,v 1.8 2013/08/25 16:10:37 revo Exp $">
<kap>
  <rad>kanabac</rad>/oj
</kap>
<drv mrk="kanaba.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cannabaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hanfgew&auml;chse</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&a_d;&a_A;&a_n;&f_g;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>cannabac&eacute;es</trd>,
    <trd>cannabinac&eacute;es</trd>
  </trdgrp>
  <trd lng="hu">kenderf&eacute;l&eacute;k</trd>
  <trd lng="nl">hennepfamilie</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_jo;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
