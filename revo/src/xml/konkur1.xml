<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkur1.xml,v 1.15 2014/04/14 12:10:25 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>konkurenc</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konkur1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Komerca konkurado:
      <ekz>
        la <tld/>o trudas &gcirc;ustan prezon al la varoj;
      </ekz>
      <ekz>
        <tld/>i <klr>(x)</klr> iun, kun iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">compet&egrave;ncia<klr> (econ.)</klr></trd>
  <trdgrp lng="de">
     <trd>Konkurrenz</trd>,
     <trd>Wettbewerb</trd>
  </trdgrp>
  <trd lng="en">commercial <ind>competition</ind></trd>
  <trd lng="es">competencia</trd>
  <trd lng="fr">concurrence</trd>
  <trd lng="nl">concurrentie</trd>
  <trd lng="pt">concorr&ecirc;ncia</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
