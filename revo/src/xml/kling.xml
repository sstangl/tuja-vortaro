<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kling.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kling</rad>/o
</kap>
<drv mrk="kling.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Metala plata parto de instrumento a&ubreve; armilo, destinita por
      tran&ccirc;i a&ubreve; piki:
      <ekz><tld/>o de tran&ccirc;ilo, glavo, &ccirc;izilo, hakilo.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_z;&c_o;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">fulla <klr>(tallant)</klr></trd>
  <trdgrp lng="cs">
    <trd>ostří</trd>,
    <trd>čepel</trd>
  </trdgrp>
  <trd lng="de">Klinge</trd>
  <trdgrp lng="en">
    <trd>blade</trd>,
    <trd>cutting edge</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuchilla</trd>,
    <trd>hoja<klr> (cortante)</klr></trd>
  </trdgrp>
  <trd lng="fr">lame</trd>
  <trd lng="nl">lemmet</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_z;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ostrie</trd>,
    <trd>čepeľ</trd>
  </trdgrp>
</drv>
</art>
