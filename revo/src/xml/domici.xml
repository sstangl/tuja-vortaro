<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: domici.xml,v 1.13 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>domicil</rad>/o
</kap>
<drv mrk="domici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Loko, kie oni havas siajn &ccirc;efaferojn &ccirc;u okupon k kiu
      estas konsiderata
      kiel kutima la&ubreve;le&gcirc;a restadejo.  
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_je;&c_s;&c_c;&c_a;
      &c_zh;&c_y;&c_h;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_ib;&c_c;&c_y;&c_l;&c_ib;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Wohnsitz</trd>,
       <trd>Domizil</trd>
    </trdgrp>
    <trd lng="es">domicilio</trd>
    <trd lng="fr">domicile</trd>
    <trd lng="hu">&aacute;lland&oacute; <ind>lakhely</ind></trd>
    <trdgrp lng="pl">
      <trd>domicyl</trd>,
      <trd>zameldowanie</trd>
    </trdgrp>
    <trd lng="pt">domic&iacute;lio</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bydliště</trd>,
    <trd>domicil <klr>(mezinárodní nebo směnečné)</klr></trd>
  </trdgrp>
  <trd lng="sk">bydlisko</trd>
</drv>
</art>
