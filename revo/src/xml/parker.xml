<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parker.xml,v 1.18 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>parker</rad>/e
</kap>
<drv mrk="parker.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      Per la memoro, memore:
      <ekz>
        <tld/>e scii, reciti, lerni.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_n;&c_a;&c_i;&c_z;&c_u;&c_s;&c_t;</trd>
  <trdgrp lng="br">
    <trd>dindan <ind>e&ntilde;vor</ind></trd>,
    <trd>dre <ind>e&ntilde;vor</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nazpaměť</trd>,
    <trd>zpam&ecaron;ti</trd>,
    <trd>zpaměti</trd>
  </trdgrp>
  <trd lng="de">auswendig</trd>
  <trdgrp lng="en">
    <trd>from <ind>memory</ind></trd>,
    <trd>by <ind>heart</ind></trd>
  </trdgrp>
  <trd lng="es">de <ind>memoria</ind></trd>
  <trd lng="fr">par <ind>c&oelig;ur</ind></trd>
  <trdgrp lng="hu">
    <trd>k&iacute;v&uuml;lr&odblac;l</trd>,
    <trd>fejb&odblac;l</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>a <ind>memoria</ind></trd>,
    <trd>mnemonicamente</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>uit het <ind>hoofd</ind></trd>,
    <trd>van <ind>buiten</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>de cor</trd>,
    <trd>de mem&oacute;ria</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_i;&c_z;&c_u;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">naspamäť</trd>
</drv>
</art>
