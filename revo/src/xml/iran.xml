<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: iran.xml,v 1.11 2014/06/06 16:10:20 revo Exp $">
<kap>
  <rad>iran</rad>/o
</kap>

<drv mrk="iran.I0o">
  <kap>
    <tld lit="I"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="iran.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>IR</mlg>
    <dif>
      &Scirc;tato en la sudokcidenta Azio kun &ccirc;efurbo Tehrano.
      Oficiala nomo: <em>Islama Respubliko de Irano</em>.
      &Gcirc;i limas kun <ref tip="vid"
      cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="armen.A0ujo.GEOG">Armenujo</ref>,
      <ref tip="vid" cel="azerbajg.A0o.GEOG">Azerbaj&gcirc;ano</ref>,
      <ref tip="vid" cel="irak.I0o.GEOG">Irako</ref>,
      <ref tip="vid" cel="pakistan.P0o.GEOG">Pakistano</ref>,
      <ref tip="vid" cel="turkmen.T0ujo.GEOG">Turkmenujo</ref>,
      <ref tip="vid" cel="turk.T0ujo.GEOG">Turkujo</ref>
      kaj kun Araba Maro kaj <ref tip="vid"
      cel="pers.P0aGolfo.GEOG">Persa Golfo</ref>.
      &Gcirc;is 1935 &gcirc;i estis nomata <ref tip="sin"
      cel="pers.P0ujo">Persujo</ref>.
    </dif>
    <trd lng="be">&c_Ib;&c_r;&c_a;&c_n;</trd>
    <trd lng="bg">&c_I;&c_r;&c_a;&c_n;</trd>
    <trd lng="de">Iran</trd>
    <trd lng="en">Iran</trd>
    <trd lng="fa">&a_A;&f_y;&a_r;&a_A;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>Iran</trd>,
      <trd>R&eacute;publique islamique d'Iran</trd>
    </trdgrp>
    <trd lng="id">Iran</trd>
    <trd lng="it">Iran</trd>
    <trdgrp lng="pl">
      <trd>Iran</trd>,
      <trd>Islamska Republika <ind>Iranu</ind></trd>
    </trdgrp>
    <trd lng="pt">Ir&atilde;</trd>
  </snc>
</drv>

</art>

