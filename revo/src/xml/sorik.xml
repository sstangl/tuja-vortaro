<vortaro>
<art mrk="$Id: sorik.xml,v 1.25 2015/07/04 08:37:43 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>sorik</rad>/o
</kap>
<drv mrk="sorik.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="sorik.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
      Malgranda
       <ref tip="lst" cel="mangx.insekto0ulo" 
       lst="voko:insektoman&gcirc;uloj">insektoman&gcirc;ulo</ref>, 
       musosimila kun longa nazo;
       <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio 
       <ref tip="malprt" cel="sorike.0oj"><tld/>edoj</ref>
       (<trd lng="la">Sorex</trd>).
     </dif>
   </snc>
<bld lok="../bld/sorik.jpg">
<tld/>o </bld>
   <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_r;&c_y;&c_j;&c_k;&c_a;</trd>
   <trd lng="bg">&c_z;&c_je;&c_m;&c_je;&c_r;&c_o;&c_v;&c_k;&c_o;&c_v;&c_i;</trd>
   <trd lng="de">Spitzmaus</trd>
   <trd lng="en">shrew</trd>
   <trd lng="es">musara&ntilde;a</trd>
   <trd lng="fr">musaraigne</trd>
   <trd lng="id">celurut</trd>
   <trd lng="it">toporagno</trd>
   <trd lng="hu">cick&aacute;ny</trd>
   <trd lng="nl">spitsmuis</trd>
   <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_r;&c_o;&c_j;&c_k;&c_a;</trd>
  <trd lng="sk">piskor</trd>
  <trd lng="cs">rejsek</trd>
</drv>
</art>
