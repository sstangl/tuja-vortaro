<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id: lakt.xml,v 1.33 2015/07/04 08:37:38 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>lakt</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>
 
    <drv mrk="lakt.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="lakt.0o.mambesto">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Blanka, dol&ccirc;a flua&jcirc;o, produktata de specialaj
          glandoj de la inaj mambestoj por la nutrado de la idoj:
          <ekz>
            kaprina <tld/>o;
          </ekz>
          <ekz>
            <tld/>a dieto;
          </ekz>
          <ekz>
            havi ankora&ubreve; la <tld/>on sur la lipoj<fnt>Z</fnt>
            <klr>(esti juna kaj sensperta)</klr>;
          </ekz>
          <ekz>
            nenia peno nek provo donus <tld/>on de
            bovo<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="pl">mleko</trd>
      </snc>
      <snc mrk="lakt.0o.similanta">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Nomo de kelkaj flua&jcirc;oj similantaj <tld/>on:
          <ekz>
            migdala, kokosa <tld/>o.</ekz>
        </dif>
        <trd lng="pl">mleczko</trd>
      </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_k;&c_o;</trd>
  <trd lng="bg">&c_m;&c_l;&c_ja;&c_k;&c_o;</trd>
  <trd lng="br">laezh</trd>
  <trd lng="ca">llet</trd>
  <trd lng="cs">mléko</trd>
  <trd lng="de">Milch</trd>
  <trd lng="el">&gamma;&alfa_ton;&lambda;&alfa;</trd>
  <trd lng="en">milk</trd>
  <trd lng="es">leche</trd>
  <trd lng="fr">lait</trd>
  <trd lng="hu">tej</trd>
  <trd lng="id">susu</trd>
  <trd lng="nl">melk</trd>
  <trd lng="pt">leite</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_k;&c_o;</trd>
  <trd lng="sk">mlieko</trd>
  <trd lng="sv">mj&ouml;lk</trd>
  <trd lng="tr">s&uuml;t</trd>
  <trd lng="vo">milig</trd>
</drv>
 
    <drv mrk="lakt.0eca">
      <kap><tld/>eca, <var><kap><tld/>a</kap></var></kap>
      <snc mrk="lakt.0eca.apartenanta">
        <dif>
          Apartenanta al <tld/>o.
        </dif>
        <trd lng="de">Milch-</trd>
        <trdgrp lng="hu">
          <trd>tej-</trd>,
          <trd>tejes</trd>
        </trdgrp>
        <trd lng="fr">lactique</trd>
        <trdgrp lng="pl">
          <trd>mleczny</trd>,
          <trd>mlekowy</trd>
        </trdgrp>
      </snc>
      <snc mrk="lakt.0eca.simila">
        <dif>
          Simila al <tld/>o:
          <ekz>
            la &ccirc;ielo fari&gcirc;is flavru&gcirc;a,
            fine lakta<fnt>B</fnt>
            <klr>(<tld/>okolora)</klr>.
          </ekz>
        </dif>
        <trd
        lng="el">&gamma;&alfa;&lambda;&alfa;&kappa;&tau;&omega_ton;&delta;&eta;&sigma_fina;</trd>
        <trd lng="de">milchig</trd>
        <trd lng="en">milky</trd>
        <trd lng="fr">laiteux</trd>
        <trd lng="hu">tejszer&udblac;</trd>     
        <trd lng="nl">melkachtig</trd>
        <trd lng="pl">mleczny</trd>
        <trd lng="pt">leitoso</trd>
        <trd lng="tr">s&uuml;t gibi</trd>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y;</trd>
      <trd lng="br">laezhek</trd>
      <trdgrp lng="ca">
        <trd>llet&oacute;s</trd>,
        <trd>lacti</trd>,
        <trd>l&agrave;ctic</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>l&aacute;cteo</trd>,
        <trd>l&aacute;ctea</trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </drv>
 
    <drv mrk="lakt.0avojo">
      <kap><tld lit="L"/>a Vojo</kap>
      <snc mrk="lakt.0avojo.AST">
        <uzo tip="fak">AST</uzo>
        <dif>
          Nia <ref tip="super"
          cel="galaksi.0o.stelamaso">galaksio</ref>,
          amaso de &ccirc;irka&ubreve; 100 miliardoj da <ref
          tip="prt" cel="stel.0o">steloj</ref>, enhavanta la
          <ref tip="prt" cel="sun.0sistemo">sunsistemon</ref>,
          kun formo de ovala plata kuko, kun diametro de
          &ccirc;irka&ubreve; 100000 lumjaroj kaj diko de
          &ccirc;irka&ubreve; 10000 je la
          centro; de sur Tero &gcirc;i aspektas kiel lar&gcirc;a
          blanketa strio videbla nokte en &ccirc;ielo,
          konsistanta el malproksimegaj sennombraj steloj.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_M;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y;
        &c_Sh;&c_l;&c_ja;&c_h;</trd>,
        <trd>&c_P;&c_t;&c_u;&c_sh;&c_y;&c_n;&c_y;
        &c_Sh;&c_l;&c_ja;&c_h;</trd>,
        <trd>&c_S;&c_t;&c_a;&c_n;</trd>
      </trdgrp>
      <trd lng="bg">&c_m;&c_l;&c_je;&c_ch;&c_je;&c_n;
      &c_p;&c_malmol;&c_t;</trd>
      <trd lng="br">Hent Sant-Jakez</trd>
      <trdgrp lng="ca">
        <trd>la Via L&agrave;ctia</trd>,
        <trd>Cam&iacute; de Sant Jaume</trd>
      </trdgrp>
      <trd lng="cs">Ml&eacute;&ccaron;n&aacute; dr&aacute;ha</trd>
      <trd lng="de">Milchstra&szlig;e</trd>
      <trd lng="en">Milky Way</trd>
      <trd lng="es">V&iacute;a L&aacute;ctea</trd>
      <trd lng="fr">Voie Lact&eacute;e</trd>
      <trd lng="hu">tej&uacute;t</trd>   
      <trd lng="id">Bima Sakti (galaksi)</trd>
      <trd lng="it">Via Lattea</trd>
      <trd lng="la">Lacteus orbis</trd>
      <trd lng="nl">Melkweg</trd>
      <trd lng="pl">droga mleczna</trd>
      <trd lng="pt">Via L&aacute;ctea</trd>
      <trd lng="ru">&c_M;&c_l;&c_je;&c_ch;&c_n;&c_y;&c_j;
      &c_P;&c_u;&c_t;&c_mol;</trd>
      <trd lng="tr">Samanyolu</trd>
      <trd lng="sv">Vintergatan</trd>
    </drv>
 
    <drv mrk="lakt.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="lakt.0i.FIZL">
        <uzo tip="fak">FIZL</uzo>
        <dif>
          <ref tip="super" cel="sekrec.0i.FIZL">Sekrecii</ref> <tld/>on:
          <ekz>
            <tld/>anta bovino<fnt>B</fnt>.</ekz>
        </dif>
      </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;
      &c_m;&c_a;&c_l;&c_a;&c_k;&c_o;</trd>
  <trd lng="br">rei&ntilde; <ind>laezh</ind></trd>
  <trdgrp lng="ca">
        <trd>lactar</trd>,
        <trd>alletar</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>dojit</trd>,
    <trd>dávat mléko</trd>
  </trdgrp>
  <trd lng="de">Milch geben</trd>
  <trd lng="en">lactate</trd>
  <trd lng="fr">donner du lait</trd>
  <trd lng="hu">tejel</trd>
  <trdgrp lng="pl">
        <trd>mleczny</trd>,
        <trd>mlekodajny</trd>
      </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;
      &c_m;&c_o;&c_l;&c_o;&c_k;&c_o;<klr> (&c_o;
      &c_k;&c_o;&c_r;&c_o;&c_v;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>dojiť</trd>,
    <trd>dávať mlieko</trd>
  </trdgrp>
</drv>
 
    <drv mrk="lakt.0umo">
      <kap><tld/>umo</kap>
      <snc mrk="lakt.0umo.CXAS">
        <dif>
          Semo de viraj fi&scirc;oj, kiu fruktigas la ovojn:
        </dif>
        <ekz>
        mi tute konsentas kun vi, ke <tld/>o kaj <tld/>umo havas inter
        si preska&ubreve; nenion komunan<fnt><bib>LR</bib></fnt>.
        </ekz>
        <refgrp tip="vid">
          <ref cel="fraj.0o">frajo</ref>,
          <ref cel="kaviar.0o">kaviaro</ref>
        </refgrp>
      </snc>
  <trd lng="br">laezhenn</trd>
  <trdgrp lng="ca">
        <trd>llet</trd>,
        <trd>l&iacute;quid seminal dels peixos</trd>
      </trdgrp>
  <trd lng="cs">rybí mlíčí</trd>
  <trdgrp lng="de">
        <trd>Fischmilch</trd>,
        <trd>Milch <klr>(vom Fisch)</klr></trd>
      </trdgrp>
  <trd lng="en">milt</trd>
  <trd lng="es">lecha</trd>
  <trd lng="fr">laitance</trd>
  <trd lng="hu">haltej</trd>
  <trd lng="nl">hom</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_k;&c_i;</trd>
  <trd lng="sk">mliečie <klr>(rybacie sperma)</klr></trd>
  <trd lng="tr">s&uuml;t</trd>
</drv>
  </art>
