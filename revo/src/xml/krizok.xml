<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krizok.xml,v 1.11 2015/08/22 15:10:24 revo Exp $">
<kap>
  <rad>krizokal</rad>/o
</kap>
<drv mrk="krizok.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Alojo el kupro, zinko, stano kaj plumbo, kiu imitas oron.
    </dif>
  </snc>
  <trd lng="de">Goldkupfer</trd>
  <trd lng="en">pinchbeck</trd>
  <trd lng="fr">chrysocale</trd>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>hamisarany</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pinsbek</trd>,
    <trd>semilor</trd>
  </trdgrp>
  <trd lng="pl">tombak</trd>
  <trd lng="pt">crisocola</trd>
  <trd lng="ru">&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>
</drv>
</art>
