<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: uzur.xml,v 1.19 2015/07/04 08:37:45 revo Exp $">
<kap>
   <ofc>4</ofc>
   <rad>uzur</rad>/o
</kap>
<drv mrk="uzur.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Procentego.
     </dif>
     <refgrp tip="vid">
       <ref cel="ung.0ego">ungego</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_v;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>opotřebování</trd>,
    <trd>uzura</trd>,
    <trd>částečné zničení</trd>
  </trdgrp>
  <trd lng="de">Wucher</trd>
  <trd lng="en">usury</trd>
  <trd lng="es">usura</trd>
  <trd lng="fa">&a_r;&a_kasra;&a_b;&a_A;</trd>
  <trd lng="fi">koronkiskonta</trd>
  <trd lng="fr">usure <klr>(finances)</klr></trd>
  <trd lng="hu">uzsora <klr>(kamat)</klr></trd>
  <trd lng="it">usura</trd>
  <trd lng="nl">woekerwinst</trd>
  <trdgrp lng="pt">
    <trd>usura</trd>
   </trdgrp>
  <trd 
lng="ru">&c_r;&c_o;&c_s;&c_t;&c_o;&c_v;&c_shch;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>opotrebovanie</trd>,
    <trd>uzúra</trd>
  </trdgrp>
</drv>
</art>
