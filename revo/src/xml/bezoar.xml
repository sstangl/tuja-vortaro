<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bezoar.xml,v 1.9 2012/07/20 08:10:26 revo Exp $">
<kap>
  <rad>bezoar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bezoar.0o">
  <kap><tld/>o</kap>
  <snc mrk="bezoar.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="konkrement.0o.FIZL">Konkremento</ref>,
      kunglua&jcirc;o el haroj a&ubreve; vegeta&jcirc;oj, kiu
      formi&gcirc;as en la digestaj organoj precipe de 
      rema&ccirc;uloj, iam uzita kiel kontra&ubreve;veneno:
      <ekz>
        &ccirc;u vi revenas kun kargo da perloj, cinamo kaj <tld/>oj?
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">betzoar</trd>
  <trd lng="de">Bezoar</trd>
  <trd lng="en">bezoar</trd>
  <trdgrp lng="es">
    <trd>bezoar</trd>,
    <trd>bezoard</trd>
  </trdgrp>
  <trd lng="fr">bezoard</trd>
  <trd lng="hu">bezo&aacute;r</trd>
  <trd lng="it">bezoario</trd>
  <trdgrp lng="pl">
    <trd>bezoar</trd>,
    <trd>kamie&nacute; jelitowy</trd>,
    <trd>strutka</trd>
  </trdgrp>
  <trd lng="pt">bezoar</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_o;&c_a;&c_r;</trd>
</drv>
</art>

