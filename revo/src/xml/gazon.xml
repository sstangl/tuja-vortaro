<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gazon.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gazon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="gazon.0o">
  <kap><tld/>o</kap>
  <snc mrk="gazon.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Zorge kultivata <ref tip="super" cel="herb.0ejo">herbejo</ref>
      precipe ornama:
      <ekz>
        kiom da tempo ili bezonos por fal&ccirc;i la <tld/>on
        <fnt>
        <aut>Jomo Ipfelkofer</aut>,
        <vrk>Komparo de instrumetodoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 6a, p. 9a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
  <trd lng="bg">&c_t;&c_r;&c_je;&c_v;&c_n;&c_a; &c_p;&c_l;&c_o;&c_shch;</trd>
  <trd lng="ca">gespa</trd>
  <trd lng="cs">trávník pěstěný</trd>
  <trd lng="de">Rasen</trd>
  <trd lng="en">lawn</trd>
  <trd lng="es">c&eacute;sped</trd>
  <trdgrp lng="fr">
    <trd>gazon<klr> (pelouse)</klr></trd>,
    <trd>pelouse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;zsit</trd>,
    <trd>gyep</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gazon</trd>,
    <trd>grasperk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trawnik</trd>,
    <trd>gazon</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gramado</trd>, 
   <trd>relvado</trd>, 
   <trd>relvagem</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_u;&c_zh;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">trávnik</trd>
  <trd lng="sv">gr&auml;smatta</trd>
  <trd lng="tr">&ccedil;imen</trd>
</drv>
</art>
