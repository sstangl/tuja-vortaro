<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaviar.xml,v 1.21 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kaviar</rad>/o
</kap>
<drv mrk="kaviar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kaviar.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Franda&jcirc;o farita el salita 
      <ref tip="super" cel="fraj.0o">frajo</ref> de  
      <ref tip="malprt" cel="huz.0o">huzoj</ref> kaj 
      kelkaj acipenseroj: <refgrp tip="malprt">
        <ref cel="sterle.0o">sterledo</ref>,
        <ref cel="sevrug.0o">sevrugo</ref> kaj
        <ref tip="vid" cel="sturg.0o">sturgo <klr>(protektita de
        1982)</klr></ref>
      </refgrp>:
      <ekz>
        por du salitaj kukumoj aparte kaj duonporcio da <tld/>o unu
        rublo
        dudek-kvin kopekoj
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_r;&c_a; <klr>(&c_je;&c_zh;&c_a;)</klr></trd>
  <trd lng="br">kaviar</trd>
  <trd lng="ca">caviar</trd>
  <trd lng="cs">kaviár</trd>
  <trd lng="de">Kaviar</trd>
  <trd lng="en">caviar</trd>
  <trd lng="es">caviar</trd>
  <trd lng="fr">caviar</trd>
  <trd lng="hu">kavi&aacute;r</trd>
  <trd lng="nl">kaviaar</trd>
  <trd lng="pl">kawior</trd>
  <trd lng="ru">&c_i;&c_k;&c_r;&c_a; <klr>(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>
  <trd lng="sk">kaviár</trd>
  <trd lng="sv">kaviar</trd>
  <trd lng="tr">kavyar</trd>
</drv>
</art>
