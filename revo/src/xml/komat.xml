<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komat.xml,v 1.13 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>komat</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="komat.0o">
  <kap><tld/>o</kap>
  <snc mrk="komat.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsan</ref>eca peza dormo kun malesto de
      sentokapablo kaj de
      memvola movkapablo:
      <ekz>
        diabeta, duondelira <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="letarg.0o">letargio</ref>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_a;</trd>
  <trd lng="ca">coma</trd>
  <trdgrp lng="cs">
    <trd>hluboké bezvědomí</trd>,
    <trd>kóma</trd>
  </trdgrp>
  <trd lng="de">Koma</trd>
  <trd lng="en">coma</trd>
  <trd lng="es">coma</trd>
  <trd lng="fr">coma</trd>
  <trd lng="id">koma</trd>
  <trd lng="nl">coma</trd>
  <trdgrp lng="pl">
    <trd>koma</trd>,
    <trd>&sacute;pi&aogonek;czka</trd>
  </trdgrp>
  <trd lng="pt">coma</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hlboké bezvedomie</trd>,
    <trd>kóma</trd>
  </trdgrp>
</drv>

<drv mrk="komat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        <tld/>a stato.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;</trd>
  <trd lng="ca">comat&oacute;s</trd>
  <trd lng="de">komat&ouml;s</trd>
  <trd lng="en">comatose</trd>
  <trd lng="es">comatoso</trd>
  <trd lng="fr">comateux</trd>
  <trd lng="nl">comateus</trd>
  <trd lng="pt">comatoso</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
