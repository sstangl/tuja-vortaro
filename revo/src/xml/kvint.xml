<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvint.xml,v 1.17 2015/09/23 07:10:18 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kvint</rad>/o
</kap>
<drv mrk="kvint.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="kvint.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el kvin sinsekvaj tonoj de gamo:
      <ekz>
        en Islando <klr>...</klr> konservi&gcirc;is <klr>...</klr> duvo&ccirc;a
        kanto,
        tielnomata <tld/>-kanto, &ccirc;ar plejparte la interspaco inter la
        du
        vo&ccirc;oj estas <tld/>oj
        <fnt><aut>Stefan Sigurdsson</aut>, <vrk>fluge super la islanda
        kulturo</vrk>, <lok>norda prismo, 1956, numero 3a, p.
        104-106a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kvint.0o.de.toniko">
    <dif>
      Kvina tono de gamo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_ib;&c_n;&c_t;&c_a;</trd>
  <trd lng="bg">&c_k;&c_v;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="cs">kvinta</trd>
  <trd lng="de">Quinte</trd>
  <trd lng="en">fifth</trd>
  <trdgrp lng="fr">
    <trd>quinte<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">kvint</trd>
  <trd lng="nl">quintool</trd>
  <trd lng="pl">kwinta</trd>
  <trd lng="pt">quinta <klr>(m&uacute;s.)</klr></trd>
  <trd lng="ru">&c_k;&c_v;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">kvinta</trd>
</drv>
</art>
