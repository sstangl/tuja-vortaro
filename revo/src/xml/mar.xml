<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mar.xml,v 1.52 2015/07/04 08:37:39 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>mar</rad>/o
   </kap>

    <drv mrk="mar.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="mar.0o.parto">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Iel apartigita parto de la
          <ref tip="malprt" cel="ocean.0o">oceano</ref>.
       </dif>
        <lstref lst="voko:maroj">maroj</lstref>
        <trd lng="id">laut</trd>
     </snc>
      <snc mrk="mar.0.oceano">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="stl">EVI</uzo>
        <dif>
          <ref tip="dif" cel="ocean.0o">oceano</ref>
       </dif>
        <trd lng="ar">&a_b;&a_fatha;&a_H;&a_sukun;&a_r;&a_dammatan;
        &a_j;
          &a_b;&a_kasra;&a_H;&a_fatha_A;&a_r;&a_dammatan;</trd>
     </snc>
     <snc mrk="mar.0.lago">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Iom granda sala lago (precipe en kelkaj propraj nomoj):
          <ekz>
            Kaspia <tld/>o,
         </ekz><ekz>
            Malviva <tld/>o.
         </ekz>
       </dif>
     </snc>
     <snc mrk="mar.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Grandega kvanto:
          <ekz>
            <ind><tld/>o da sango</ind><fnt>Z</fnt>;
            <trd lng="hu">tengernyi v&eacute;r</trd>
            <trd lng="ru">&c_m;&c_o;&c_r;&c_je; &c_k;&c_r;&c_o;&c_v;&c_i;</trd>
         </ekz><ekz>
            <tld/>o da da mizeroj
            <fnt><aut>Zamenhof</aut>: <vrk>Hamleto</vrk></fnt>;
         </ekz><ekz>
            la urbo disvolvis anta&ubreve; li ondi&gcirc;antan <tld/>on da
            tegmentoj;
         </ekz><ekz>
            li englutis <tld/>on da teo<fnt>B</fnt>;
         </ekz><ekz>
            inter diro kaj faro estas <tld/>o
            <klr>(granda diferenco)</klr><fnt>Z</fnt>
            <refgrp tip="vid">vd. 
              <ref cel="abism.0o">abismo</ref>,
              <ref cel="mont.0o">monto</ref>
           </refgrp>.
         </ekz>
       </dif>
        <trd lng="id"><ind>laut</ind>an</trd>
     </snc>
  <trd lng="be">&c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="br">mor</trd>
  <trd lng="ca">mar</trd>
  <trd lng="cs">moře</trd>
  <trd lng="de">Meer</trd>
  <trd lng="en">sea</trd>
  <trd lng="es">mar</trd>
  <trd lng="fr">mer</trd>
  <trd lng="hu">tenger</trd>
  <trd lng="it">mare</trd>
  <trd lng="ku">derya</trd>
  <trd lng="nl">zee</trd>
  <trd lng="pl">morze</trd>
  <trd lng="pt">mar</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">more</trd>
  <trd lng="sv">hav</trd>
  <trd lng="tr">deniz</trd>
</drv>

    <drv mrk="mar.en0ejo">
      <kap>en<tld/>ejo</kap>
      <snc mrk="mar.en0ejo.loko">
        <dif>
          Loko kie riveroj fluas en maron.
       </dif>
     </snc>
      <ref tip="sin" cel="flu.en0ejo">enfluejo</ref>
      <trd lng="en">mouth <klr tip="ind">(river)</klr></trd>
      <trd lng="id">muara</trd>
      <trd lng="it">foce</trd>
      <trd lng="pl">uj&sacute;cie <klr>(rzeki)</klr></trd>
   </drv>

    <drv mrk="mar.trans0a">
      <kap>trans<tld/>a</kap>
      <snc mrk="mar.trans0a.estas">
        <dif>
          Kiu estas trans <tld/>o, precipe alilande kaj malproksime:
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind><mll tip="kom">en landoj trans<tld/>aj</mll>
              estas oraj arbaroj</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="hu">az &Oacute;perenci&aacute;s-tengeren t&uacute;l
              aranyalma
              terem</trd>
            <trd lng="nl">in <ind>overzeese gebieden</ind></trd>
            <trd lng="ru"><mll tip="kom"><ind>&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;
                  &c_b;&c_u;&c_b;&c_n;&c_y;</ind></mll>
              &c_z;&c_a; &c_g;&c_o;&c_r;&c_a;&c_m;&c_i;</trd>
         </ekz>
       </dif>
     </snc>
      <snc mrk="mar.trans0a.venas">
        <dif>
          Kiu devenas el trans <tld/>o:
          <ekz>
            <ind>&ccirc;io trans<tld/>a estas &ccirc;arma kaj kara</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="hu">a messze mindig sz&eacute;p</trd>
         </ekz>
       </dif>
     </snc>
      <trd lng="br">tramor</trd>
      <trd lng="ca">ultramar&iacute;</trd>
      <trd lng="es">ultramarino</trd>
      <trd lng="hu">tengeren t&uacute;li</trd>
      <trd lng="pl">zamorski</trd>
      <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
   </drv>

    <drv mrk="mar.alt0o">
      <kap>alt<tld/>o</kap>
      <snc mrk="mar.alt0o.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Tiu fazo de <ref tip="malprt" cel="flus.0o">fluso</ref>,
          kiam la akvo atingas la plej altan nivelon:
          <ekz>
            estas en ili ondado &#8208; kaj anka&ubreve; malfluso kaj
            alt<tld/>o
            &#8208; kaj en iliaj pensmanieroj kaj en iliaj sentoj
            <fnt>
              <aut>H. Ibsen, trad. Odd Tangerud</aut>,
              <vrk><url ref="http://www.jec.no/esp/ene/FRUEN.htm"
                        >La sinjorino el la maro</url></vrk>
           </fnt>;
         </ekz><ekz>
            mi reiris al la nordorienta marbordo, formetis la
            surtuton, &scirc;uojn kaj &scirc;trumpojn, kaj eniris la maron
            je
            duonhoro anta&ubreve; alt<tld/>o
            <fnt>
              <aut>J. Swift, tr. M. Inman</aut>:
              <vrk>Gulivero en Liliputo</vrk>.
              <lok>ISBN 91-7303-178-X. &Ccirc;ap. 5a, p. 43a</lok>
           </fnt>.
         </ekz>
       </dif>
     </snc>
  <trd lng="br">uhelvor</trd>
  <trd lng="ca">marea alta</trd>
  <trd lng="cs">příliv</trd>
  <trd lng="de">Hochwasser</trd>
  <trd lng="en">high tide</trd>
  <trd lng="es">marea alta, pleamar</trd>
  <trd lng="fr">mar&eacute;e haute</trd>
  <trd lng="hu">dag&aacute;ly</trd>
  <trd lng="id">(laut) <ind>pasang</ind></trd>
  <trd lng="nl">hoge <ind>tij</ind></trd>
  <trd lng="pl">przyp&lstroke;yw</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_a;&c_ja;
        &c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">príliv</trd>
</drv>

    <drv mrk="mar.malalt0o">
      <kap>malalt<tld/>o</kap>
      <snc mrk="mar.malalt0o.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Tiu fazo de <ref tip="malprt" cel="flus.mal0o">malfluso</ref>,
          kiam la akvo atingas la plej malaltan nivelon:
          <ekz>
            la interspaco inter la alt<tld/>o kaj la malalt<tld/>o
            estas la <em>amplekso</em> de la tajdo
            <fnt>
              <aut>M. Rollet de l'Isle k.a.</aut>:
              <vrk>Provo de marista terminaro</vrk>.
              <lok><url
 ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html"
                      >Paris: Hachette, 1908</url></lok>
           </fnt>.
         </ekz>
       </dif>
     </snc>
      <trd lng="br">izelvor</trd>
      <trd lng="ca">marea baixa</trd>
      <trd lng="de">Niedrigwasser</trd>
      <trd lng="en">low tide</trd>
      <trd lng="es">marea baja, bajamar</trd>
      <trd lng="fr">mar&eacute;e basse</trd>
      <trd lng="hu">ap&aacute;ly</trd>
      <trd lng="id">(laut) <ind>surut</ind></trd>
      <trd lng="nl">lage <ind>tij</ind></trd>
      <trd lng="pl">odp&lstroke;yw</trd>
      <trd lng="ru">&c_m;&c_a;&c_l;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;</trd>
   </drv>

    <drv mrk="mar.ArabaM0o">
      <kap>Araba <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld
          lit="M"/>o</ref>
          en
          <ref tip="malprt" cel="hind.H0aOceano">Hinda Oceano</ref>
          inter <ref tip="vid" cel="arab.A0aDuoninsulo">Araba
            Duoninsulo</ref> kaj
          <ref tip="vid" cel="hind.H0ujo">Hindujo</ref>.
       </dif>
     </snc>
      <trd lng="bg">&c_A;&c_r;&c_a;&c_b;&c_s;&c_k;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="de">Arabisches Meer</trd>
      <trd lng="en">Arabian Sea</trd>
      <trd lng="es">Mar ar&aacute;bigo</trd>
      <trd lng="fr">Mer d'Arabie</trd>
      <trd lng="he">&he;&yod;&fmem; &he;&ayin;&resh;&bet;&yod;</trd>
      <trd lng="hu">Arab-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Arab</trd>
      <trd lng="nl">Arabische Zee</trd>
      <trd lng="pl">Morze <ind>Arabskie</ind></trd>
      <trd lng="ru">&c_A;&c_r;&c_a;&c_v;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je;
        &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.AzovaM0o">
      <kap>Azova <tld lit="M"/>o <fnt><bib>PIV1</bib></fnt></kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Interna <ref tip="lst" cel="mar.0o"
          lst="voko:maroj"><tld/>o</ref>
          Orient-<ref cel="euxrop1.0a">E&ubreve;ropa</ref>,
          inter <ref cel="rus.R0ujo">Rusujo</ref> kaj Ukrainujo,
          norde de la <ref cel="mar.NigraM0o">Nigra <tld
          lit="M"/>o</ref>.
       </dif>
     </snc>
      <trd lng="be">
        &c_A;&c_z;&c_o;&c_w;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="de">Asowsches <ind>Meer</ind></trd>
      <trd lng="en">Sea of <ind>Azov</ind></trd>
      <trd lng="fr">Mer d'Azov</trd>
      <trd lng="hu">Azovi-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Azov</trd>
      <trd lng="nl">Zee van <ind>Azov</ind></trd>
      <trd lng="pl">Morze <ind>Azowskie</ind></trd>
      <trd lng="ru">
        &c_A;&c_z;&c_o;&c_v;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.BaltaM0o">
      <kap>Balta <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="malprt" cel="atlant.A0o">Atlantika</ref> 
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
          norde en E&ubreve;ropo, inter Pollando kaj Svedujo:
          <ekz>
            apud la Balta <tld lit="M"/>o &ccirc;e la Danaj golfoj...
            <fnt><bib>Fab1</bib>, <lok>Fluganta kofro</lok></fnt>.
         </ekz>
       </dif>
     </snc>
  <trd lng="be">&c_B;&c_a;&c_l;&c_t;&c_y;&c_j;&c_s;&c_k;&c_a;&c_je;
        &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="cs">Baltické moře</trd>
  <trd lng="de">Ostsee</trd>
  <trd lng="en">Baltic Sea</trd>
  <trd lng="es">Mar <ind>B&aacute;ltico</ind></trd>
  <trd lng="fr">Mer <ind>Baltique</ind></trd>
  <trd lng="id"><ind>Laut</ind> Baltik</trd>
  <trd lng="nl">Oostzee</trd>
  <trdgrp lng="pl">
        <trd>Morze <ind>Ba&lstroke;tyckie</ind></trd>,
        <trd>Ba&lstroke;tyk</trd>
     </trdgrp>
  <trd lng="ru">&c_B;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je;
        &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">Baltské more</trd>
  <trd lng="tr">Balt&inodot;k Denizi</trd>
</drv>

    <drv mrk="mar.BeringaM0o">
      <kap>Beringa <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          La plej norda
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
          <ref tip="malprt" cel="pacifik.P0o">Pacifika</ref>.
       </dif>
       <ref tip="vid" cel="bering.B0o">Beringo</ref>
     </snc>
      <trd lng="be">&c_B;&c_e;&c_r;&c_y;&c_n;&c_g;&c_a;&c_v;&c_a; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="de">Beringsee</trd>
      <trd lng="en">Bering Sea</trd>
      <trd lng="fr">Mer de Bering</trd>
      <trd lng="hu">Bering-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Bering</trd>
      <trd lng="nl">Beringzee</trd>
      <trd lng="pl">Morze <ind>Beringa</ind></trd>
      <trd lng="ru">&c_B;&c_je;&c_r;&c_i;&c_n;&c_g;&c_o;&c_v;o &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.CXukcxaM0o">
      <kap>&Ccirc;uk&ccirc;a <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Periferia
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
          en la <ref tip="malprt" cel="ocean.ArktaO0o">Arkta
          Oceano</ref>, lavanta la bordojn
          de la <ref cel="cxukcx.CX0aDuoninsulo"><tld lit="&Ccirc;"/>a
            Duoninsulo</ref> kaj <ref cel="alask.A0o">Alasko</ref>,
          komuniki&gcirc;anta kun la
          <ref cel="mar.BeringaM0o">Beringa Maro</ref> per la
          <ref cel="kol.BeringaMar0o">Beringa Markolo</ref>.
       </dif>
        <ref tip="vid" cel="cxukcx.0o">&ccirc;uk&ccirc;o</ref>
     </snc>
      <trd lng="be">&c_Ch;&c_u;&c_k;&c_o;&c_c;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="de">Tschuktschensee</trd>
      <trd lng="en">Chukchee Sea</trd>
      <trd lng="fr">Mer des Tchouktches</trd>
      <trd lng="hu">Csukcs-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Chukchee</trd>
      <trd lng="pl">Morze <ind>Czukockie</ind></trd>
      <trd lng="ru">&c_Ch;&c_u;&c_k;&c_o;&c_t;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.FlavaM0o">
      <kap>Flava <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="malprt" cel="pacifik.P0o">Pacifika</ref>
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
          inter la kontinenta <ref cel="cxin.CX0ujo">&Ccirc;inujo</ref> kaj
          la
          Korea duoninsulo:
          <ekz>
            Oriente de Bohai-markolo estas la Flava <tld lit="M"/>o
            <fnt><bib>EncCxi</bib>
              <lok>&ccirc;ap. &#171;Maroj kaj Marbordo&#187;, p. 34a</lok></fnt>.
         </ekz>
       </dif>
     </snc>
      <trd lng="de">Gelbes Meer</trd>
      <trd lng="en">Yellow Sea</trd>
      <trdgrp lng="fr">
        <trd>Mer Jaune</trd>
     </trdgrp>
      <trd lng="hu">S&aacute;rga-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Kuning</trd>
      <trd lng="nl">Gele Zee</trd>
      <trd lng="pl">Morze <ind>&Zdot;&oacute;&lstroke;te</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_Zh;&c_jo;&c_l;&c_t;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>,
        <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
     </trdgrp>
   </drv>

    <drv mrk="mar.KaraaM0o">
      <kap>Karaa <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld
          lit="M"/>o</ref>
          en la <ref tip="malprt" cel="ocean.ArktaO0o">Arkta
          oceano</ref>,
          norde de Uralo kaj de la limo inter
          <ref cel="siberi.S0o">Siberio</ref>
          kaj <ref cel="euxrop1.E0o">E&ubreve;ropo</ref>.
       </dif>
        <ref tip="vid" cel="kara.K0o">Karao</ref>
     </snc>
      <trd lng="de">Karasee</trd>
      <trd lng="fr">Mer de <ind>Kara</ind></trd>
      <trd lng="hu">Kara-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Kara</trd>
      <trd lng="nl">Karische Zee</trd>
      <trd lng="pl">Morze <ind>Karskie</ind></trd>
      <trd lng="ru">&c_K;&c_a;&c_r;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
      <rim>
        PIV ne konas tiun <tld/>on (nek la rivereton Karao); aliaj
        vortaroj
        kaj la Po&scirc;atlaso proponas <ctl>Kara <tld/>o</ctl>, kio sonas iom
        komike kaj lasas onin imagi, kion signifas la adjektivo
        <ctl>Kara</ctl>.
     </rim>
   </drv>

    <drv mrk="mar.LaptevaM0o">
      <kap>Lapteva <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld
          lit="M"/>o</ref>
          &ccirc;e la <ref tip="malprt" cel="siberi.S0o">Siberia</ref> bordo
          de la <ref tip="malprt" cel="ocean.ArktaO0o">Arkta
          Oceano</ref>,
          inter la <ref cel="mar.KaraaM0o">Karaa <tld lit="M"/>o</ref>
          kaj
          la <ref cel="mar.OrientsiberiaM0o">Orientsiberia Maro</ref>.
       </dif>
        <ref tip="vid" cel="laptev.L0o">Laptevo</ref>
     </snc>
      <trd lng="be">&c_m;&c_o;&c_r;&c_a; &c_L;&c_a;&c_p;&c_c;&c_je;&c_v;&c_y;&c_h;</trd>
      <trd lng="de">Laptev See</trd>
      <trd lng="en">Laptev Sea</trd>
      <trd lng="es">Mar de Laptev</trd>
      <trd lng="hu">Laptyev-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Laptev</trd>
      <trd lng="nl">Laptevzee</trd>
      <trd lng="pl">Morze <ind>&Lstroke;aptiew&oacute;w</ind></trd>
      <trd lng="pt">mar de <ind>Laptev</ind></trd>
      <trd lng="ru">&c_m;&c_o;&c_r;&c_je; &c_L;&c_a;&c_p;&c_t;&c_je;&c_v;&c_y;&c_h;</trd>
   </drv>

    <drv mrk="mar.MalvivaM0o">
      <kap>Malviva <tld lit="M"/>o <fnt><bib>FK</bib></fnt></kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Sala lago inter Israelo kaj Jordanio, nutrata de la rivero
          Jordano:
       <ekz>
         malgra&ubreve; tiaj malfavoraj kondi&ccirc;oj en la sal-broma akvo de la
         Malviva <tld lit="M"/>o ekzistas vivaj kreita&jcirc;oj
	 <fnt>
           <bib>FK</bib> <lok>5. El la vivo kaj sciencoj</lok>.
	 </fnt>;
       </ekz><ekz>
         unufoje <klr>mi</klr> min banis en la Malviva <tld lit="M"/>o
         kaj pasigis unu sabaton en Jerusalemo
         <fnt>
           <aut>S. Pokrovskij</aut>:
           <vrk>Pilgrimo en la Sanktan Landon</vrk>.
           <bib>LOdE</bib>, <lok>n-ro 61, 1999:11</lok>.
         </fnt>.
       </ekz>
       </dif>
     </snc>
     <trd lng="de">Totes Meer</trd>
     <trd lng="en">Dead Sea</trd>
     <trd lng="fr">Mer Morte</trd>
     <trd lng="pl">Morze Martwe</trd>
     <trd lng="ru">&c_M;&c_jo;&c_r;&c_t;&c_v;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.MezM0o">
      <kap>Mez<tld/>o</kap>
      <uzo tip="stl">RAR</uzo>
      <snc mrk="mar.MezM0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <ref tip="dif" cel="mediterane.M0o.GEOG">Mediteraneo</ref>
        <ekz>
          Nolan iam renkontis Burr-on en unu el niaj &scirc;ipoj, kiam grupo
          da usonanoj vizitis &gcirc;in sur la
          Mez<ref tip="lst" cel="mar.0o" lst="voko:maroj"><tld/>o</ref>
          <fnt>
            <aut>Edward Everett Hale, trad. E. L. Robinson, trad. Don
              Harlow</aut>,
            <vrk><url
 ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html"
                    >La viro senpatria</url></vrk>,
            <lok>1907 kaj 1994</lok>
         </fnt>;
       </ekz><ekz>
          boato de Mez<tld/>a speco
          <fnt><bib>Mortul&Scirc;ip</bib>, <lok>aldonoj, malgranda marista
              terminaro</lok></fnt>.
       </ekz>
     </snc>
      <trd lng="be">&c_M;&c_ib;&c_zh;&c_z;&c_je;&c_m;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="br">Mor <ind>Kreizdouar</ind></trd>
      <trd lng="de">Mittelmeer</trd>
      <trd lng="en">Mediterranean Sea</trd>
      <trd lng="fr">Mer <ind>M&eacute;diterran&eacute;e</ind></trd>
      <trd lng="hu">F&ouml;ldk&ouml;zi-tenger</trd>
      <trdgrp lng="id">
        <trd><ind>Laut</ind> Mediterania</trd>,
        <trd><ind>Laut</ind> Tengah</trd>
     </trdgrp>
      <trd lng="nl">Middellandse Zee</trd>
      <trd lng="pl">Morze <ind>&Sacute;r&oacute;dziemne</ind></trd>
      <trd lng="ru">&c_S;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="sk">Stredozemn&eacute; more</trd>
   </drv>

    <drv mrk="mar.NigraM0o">
      <kap>Nigra <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Interna <ref tip="lst" cel="mar.0o"
          lst="voko:maroj"><tld/>o</ref>
          <ref tip="malprt" cel="atlant.A0o">Atlantika</ref>
          inter E&ubreve;ropo kaj Azio, &ccirc;e la bordoj de
          <ref cel="rus.R0ujo">Rusujo</ref>, Ukrainujo, Rumanujo,
          <ref cel="bulgar.B0ujo">Bulgarujo</ref>,
          <ref cel="turk.T0ujo">Turkujo</ref> kaj
          <ref cel="kartvel.K0ujo">Kartvelujo</ref>.
       </dif>
     </snc>
      <adm>
        Mi dubas, &ccirc;u vere "atlantika".
        <aut>MB</aut>
     </adm>
      <trd lng="be">&c_Ch;&c_o;&c_r;&c_n;&c_a;&c_je;
        &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="br">Mor Du</trd>
      <trd lng="bg">&c_Ch;&c_je;&c_r;&c_n;&c_o; &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="de">Schwarzes Meer</trd>
      <trd lng="en">Black Sea</trd>
      <trd lng="fr">Mer Noire</trd>
      <trd lng="id"><ind>Laut</ind> Hitam</trd>
      <trd lng="ja">&#40658;&#28023;
        [<ind>&#12371;&#12387;&#12363;&#12356;</ind>]</trd>
      <trd lng="nl">Zwarte Zee</trd>
      <trd lng="pl">Morze <ind>Czarne</ind></trd>
      <trd lng="pt">Mar Negro</trd>
      <trd lng="ru">&c_Ch;&c_jo;&c_r;&c_n;&c_o;&c_je;
        &c_m;&c_o;&c_r;&c_je;</trd>
      <trd lng="tr">Karadeniz</trd>
   </drv>

    <drv mrk="mar.NordaM0o">
      <kap>Norda <tld lit="M"/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Parto de la <ref tip="malprt" cel="atlant.A0o">Atlantiko</ref>
          limanta al nordokcidenta
          <ref tip="vid" cel="euxrop1.E0o">E&ubreve;ropo</ref>
          kaj orienta bordo de
          <ref tip="vid" cel="brit.B0ujo">Britujo</ref>.
       </dif>
     </snc>
      <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
      <trd lng="br">Mor an Hanternoz</trd>
      <trd lng="de">Nordsee</trd>
      <trd lng="fr">Mer du Nord</trd>
      <trd lng="hu">&Eacute;szaki-tenger</trd>
      <trd lng="id"><ind>Laut</ind> Utara</trd>
      <trd lng="nl">Noordzee</trd>
      <trd lng="pl">Morze <ind>P&oacute;&lstroke;nocne</ind></trd>
      <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
   </drv>

    <drv mrk="mar.OhxockaM0o">
      <kap>O&hcirc;ocka <tld lit="M"/>o <fnt><bib>PIV1</bib></fnt></kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
