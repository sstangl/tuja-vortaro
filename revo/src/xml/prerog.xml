<vortaro>
<art mrk="$Id: prerog.xml,v 1.15 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>prerogativ</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="prerog.0o">
  <kap><tld/>o</kap>
  <snc mrk="prerog.0o.JUR">
    <dif>
      Ekskluziva <ref tip="vid" cel="privil.0o">privilegio</ref>:
      <ekz>
        la posteno jam ne estas <tld/>o de Terzini,
        kaj Iqbal bonvenigas la &scirc;ancon for&scirc;teli &gcirc;in
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>tk-stafeto, 1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_e;&c_r;&c_a;&c_g;&c_a;&c_t;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_n;&c_a;&c_je; &c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prerogativ</trd>,
    <trd>přednost</trd>,
    <trd>přednostní právo</trd>,
    <trd>výsada <klr>(na př. od krále)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pr&auml;rogativ</trd>,
    <trd>Pr&auml;rogative</trd>,
    <trd>Vorrecht</trd>,
    <trd>Privileg</trd>
  </trdgrp>
  <trd lng="en">prerogative</trd>
  <trd lng="fr">pr&eacute;rogative</trd>
  <trdgrp lng="hu">
    <trd>prerogat&iacute;va</trd>,
    <trd>el&odblac;jog</trd>,
    <trd>kiv&aacute;lts&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>prerogatief</trd>,
    <trd>voorrecht</trd>,
    <trd>privilege</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prerrogativa</trd>,
    <trd>regalia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_r;&c_o;&c_g;&c_a;&c_t;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_r;&c_a;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prednosť</trd>,
    <trd>výsada</trd>
  </trdgrp>
</drv>
</art>
