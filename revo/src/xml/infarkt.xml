<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: infarkt.xml,v 1.10 2015/11/20 07:10:17 revo Exp $">
<kap>
  <rad>infarkt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="infarkt.0o">
  <kap><tld/>o</kap>
  <snc mrk="infarkt.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loka, amasa morti&gcirc;o de &ccirc;eloj pro
      nesufi&ccirc;a alveno de sango:
      <ekz>
        la loka malutila&jcirc;o konsistas en ia alia malsano de la okulo, 
        <klr>...</klr> kiel ekzemple <klr>...</klr> <tld/>oj en
        la glandetoj Meibomaj
        <fnt>
          <bib>FK</bib>
          <lok>Kronika Katara Konjunktivito</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="ca">infart</trd>
  <trd lng="cs">infarkt</trd>
  <trd lng="de">Infarkt</trd>
  <trd lng="en">infarct</trd>
  <trd lng="es">infarto</trd>
  <trd lng="fr">infarctus</trd>
  <trd lng="id">infarktus</trd>
  <trd lng="pl">zawa&lstroke;</trd>
  <trd lng="sk">infarkt</trd>
</drv>
</art>

