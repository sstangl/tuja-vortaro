<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elegi.xml,v 1.18 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>elegi</rad>/o
</kap>
<drv mrk="elegi.0o">
  <kap><tld/>o</kap>
  <snc mrk="elegi.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Malgranda lirika <ref tip="super" cel="poem.0o.POE">poemo</ref>,
      kun melankolia a&ubreve; sopira karaktero.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">elegia</trd>
  <trdgrp lng="cs">
    <trd>duma</trd>,
    <trd>dumka</trd>,
    <trd>elegie</trd>,
    <trd>žalozpěv</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Elegie</trd>,
    <trd>Klagelied</trd>
  </trdgrp>
  <trd lng="en">elegy</trd>
  <trd lng="es">eleg&iacute;a</trd>
  <trd lng="fr">&eacute;l&eacute;gie</trd>
  <trd lng="hu">el&eacute;gia</trd>
  <trd lng="pl">elegia</trd>
  <trd lng="pt">elegia</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>elégia</trd>,
    <trd>žalospev</trd>
  </trdgrp>
</drv>

<drv mrk="elegi.0a">
  <kap><tld/>a</kap>
  <snc mrk="elegi.0a.POE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a poezio.
      </ekz>
    </dif>
    <trd lng="de">elegisch</trd>
    <trd
    lng="ru">&c_e;&c_l;&c_je;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="elegi.0a.FIG">
    <dif>
      Plorema kiel <tld/>o:
      <ekz>
        <tld/>a tono.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schwerm&uuml;tig</trd>,
      <trd>wehm&uuml;tig</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_n;&c_y;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">eleg&iacute;ac</trd>
  <trdgrp lng="cs">
    <trd>elegický</trd>,
    <trd>smutný</trd>,
    <trd>tklivý</trd>,
    <trd>žalostivý</trd>
  </trdgrp>
  <trd lng="en">elegaic</trd>
  <trd lng="fr">&eacute;l&eacute;giaque</trd>
  <trd lng="hu">el&eacute;gikus</trd>
  <trdgrp lng="sk">
    <trd>elegický</trd>,
    <trd>smutný</trd>,
    <trd>trúchlivý</trd>,
    <trd>žiaľny</trd>
  </trdgrp>
</drv>
</art>
