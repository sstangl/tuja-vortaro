<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akolit.xml,v 1.10 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akolit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="akolit.0o">
  <kap><tld/>o</kap>
  <snc mrk="akolit.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="help.0anto.KOMUNE">Helpanto</ref>
      en diservoj kaj ritoj:
      <ekz>
        tiam Plastro ordonis al sia <tld/>o infuzi bonan, fortan teon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
        680a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">ac&ograve;lit</trd>
  <trdgrp lng="cs">
    <trd>akolyt</trd>,
    <trd>akolyta</trd>
  </trdgrp>
  <trd lng="en">acolyte</trd>
  <trd lng="es">ac&oacute;lito</trd>
  <trd lng="fr">acolyte<klr> (rel.)</klr></trd>
  <trdgrp lng="nl">
    <trd>acoliet</trd>,
    <trd>misdienaar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_l;&c_u;&c_zh;&c_n;&c_i;&c_k;<klr tip="ind"> (&c_c;&c_je;&c_r;&c_k;.)</klr></trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_k;&c_a;<klr tip="amb"> (&c_c;&c_je;&c_r;&c_k;.)</klr></trd>,
    <trd>&c_a;&c_k;&c_o;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">akolyt</trd>
</drv>
</art>

