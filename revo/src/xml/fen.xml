<art mrk="$Id: fen.xml,v 1.16 2015/09/22 10:10:17 revo Exp $">
<kap>
  <rad>fen</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Varma kaj seka <ref tip="super" cel="vent.0o.MET">vento</ref>
      blovanta de montoj al malalta&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>foehn</trd>,
    <trd>f&ouml;hn</trd>
  </trdgrp>
  <trd lng="be">&c_s;&c_u;&c_h;&c_a;&c_v;&c_je;&c_j;</trd>
  <trd lng="bg">&c_f;&c_mol;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>f&eacute;n</trd>,
    <trd>vysou&scaron;e&ccaron; vlas&uring;</trd>
  </trdgrp>
  <trd lng="de">F&ouml;hn</trd>
  <trdgrp lng="fr">
    <trd>f&oelig;hn</trd>,
    <trd>f&ouml;hn</trd>
  </trdgrp>
  <trd lng="hu">f&odblac;n</trd>
  <trd lng="nl">f&ouml;hn</trd>
  <trd lng="pl">fen</trd>
  <trd lng="pt">foehn</trd>
  <trd lng="ru">&c_s;&c_u;&c_h;&c_o;&c_v;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>f&eacute;n</trd>,
    <trd>tepl&yacute; alpsk&yacute; vietor</trd>
  </trdgrp>
</drv>
</art>
