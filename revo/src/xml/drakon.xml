<kap>
  <rad>drakon</rad>/a
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="drakon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Severega, kiel la le&gcirc;oj de Drakono.  
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;&c_a;&c_w;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">draconi&agrave;</trd>
  <trdgrp lng="cs">
    <trd>drakonický</trd>,
    <trd>drakónský</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>drakonisch</trd>,
     <trd>hart</trd>,
     <trd>streng</trd>
  </trdgrp>
  <trd lng="en">draconian</trd>
  <trd lng="es">draconiano</trd>
  <trd lng="fr">draconien</trd>
  <trd lng="pl">drako&nacute;ski</trd>
  <trd lng="pt">draconiano</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_k;&c_o;&c_n;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;
    <klr>(&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;, &c_zh;&c_je;&c_s;&c_t;&c_o;&c_k;&c_i;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>drakonický</trd>,
    <trd>krutý</trd>
  </trdgrp>
</drv>
</art>
