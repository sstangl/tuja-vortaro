<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: antropozof.xml,v 1.2 2013/02/25 20:10:09 revo Exp $">
<kap>
  <rad>antropozof</rad>/o
</kap>
<drv mrk="antropozof.0o">
  <kap><tld/>o</kap>
  <snc mrk="antropozof.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="vid" cel="antropozofi.0o.FIL">Antropozofi</ref>ano:
      <ekz>
        Klaesi estis <klr>...</klr> aktiva <tld/>o
        kaj malavara mecenato de idealismaj movadoj
        <fnt>
          <aut>A. K&uuml;nzli</aut>,
          <vrk><url
          ref="http://www.plansprachen.ch/files/33_ANTOL_22-26.pdf">
          Antologio XXII</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Anthroposoph</trd>
  <trd lng="en">anthroposophist</trd>
  <trd lng="fr">anthroposophe</trd>
</drv>
</art>

