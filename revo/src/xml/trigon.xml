<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trigon.xml,v 1.19 2012/08/14 13:10:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>trigonometri</rad>/o
</kap>
<drv mrk="trigon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 55</lok></fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      kiu studas la rilatojn inter anguloj kaj lateroj de triangulo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_g;&c_a;&c_n;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">trigonometria</trd>
  <trdgrp lng="de">
    <trd>Trigonometrie</trd>,
    <trd>Dreiecksmessung</trd>
  </trdgrp>
  <trd lng="en">trigonometry</trd>
  <trd lng="fr">trigonom&eacute;trie</trd>
  <trdgrp lng="hu">
    <trd>trigonometria</trd>,
    <trd>h&aacute;romsz&ouml;gtan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trigonometrie</trd>,
    <trd>driehoeksmeting</trd>
  </trdgrp>
  <trd lng="pl">trygonometria</trd>
  <trd lng="pt">trigonometria</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="trigon.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="trigon.0a.gxenerale">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ind><tld/>a egala&jcirc;o</ind>,
        <trd lng="ca">igualtat <ind>trigonom&egrave;tric</ind>a</trd>
      </ekz>
      <ekz>
        ekvacio,
      </ekz>
      <ekz>
        serio;
      </ekz>
      <ekz>
        <ind><tld/>a prezento</ind> de <ref
        cel="komple.0o.MAT">komplekso</ref>
        <klr>(sub formo <frm>&rho;(cos&theta;+<k>i</k>.sin&theta;)</frm>)</klr>.
        <trd lng="ca">representaci&oacute; <ind>trigonom&egrave;tric</ind>a</trd>
        <trd lng="de">trigonometrische <ind>Form</ind></trd>
        <trdgrp lng="en">
          <trd>polar <ind>form</ind></trd>,
          <trd>trigonometric <ind>representation</ind></trd>
        </trdgrp>
        <trd lng="fr">forme trigonom&eacute;trique</trd>
        <trd lng="hu">trigonometrikus alak</trd>        
        <trd lng="pl">posta&cacute; trygonometryczna</trd>
        <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_f;&c_o;&c_r;&c_m;&c_a;</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkl.trigonometria0o"><tld/>a cirklo</ref>,
      <ref cel="funkci.trigonometria0o"><tld/>a funkcio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_g;&c_a;&c_n;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">trigonom&egrave;tric</trd>
  <trd lng="de">trigonometrisch</trd>
  <trd lng="en">trigonometric</trd>
  <trd lng="fr">trigonom&eacute;trique</trd>
  <trd lng="hu">trigonometrikus</trd>  
  <trd lng="pl">trygonometryczny</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
