<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hibisk.xml,v 1.16 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hibisk</rad>/o
</kap>
<drv mrk="hibisk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Hibiscus</trd>), rimarkinda pro siaj grandaj,
      brilkoloraj floroj.
    </dif>
  </snc>
  <bld lok="../bld/hibisk.jpg">
    floro de <tld/>o
  </bld>
  <trd lng="be">&c_g;&c_ib;&c_b;&c_ib;&c_s;&c_k;&c_u;&c_s;</trd>
  <trd lng="de">Hibiskus</trd>
  <trd lng="fr">hibiscus</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;lyvacserje</trd>,
    <trd>hibiszkusz</trd>
  </trdgrp>
  <trd lng="it">ibisco</trd>
  <trd lng="nl">hibiscus</trd>
  <trdgrp lng="pl">
    <trd>hibiskus</trd>, <trd>ketmia</trd>
  </trdgrp>
  <trd lng="pt">hibisco</trd>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_i;&c_s;&c_k;&c_u;&c_s;</trd>
  <trd lng="sk">ibištek</trd>
  <trd lng="cs">ibišek</trd>
</drv>
</art>
