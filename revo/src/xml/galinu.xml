<vortaro>
<art mrk="$Id: galinu.xml,v 1.15 2015/06/29 16:29:39 revo Exp $">
<kap>
  <rad>galinul</rad>/o
</kap>
<drv mrk="galinu.0o">
  <kap><tld/>o</kap>
  <snc mrk="galinu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="raled.0oj">raledoj</ref>
      (<trd lng="la">Gallinula</trd>).
    </dif>
  </snc>
  <trd lng="ca"><ind>polla</ind> d'aigua</trd>
  <trd lng="cs">sl&iacute;pka zelenonoh&aacute;</trd>
  <trdgrp lng="de">
    <trd>Teichralle</trd>,
    <trd>Teichhuhn</trd>,
    <trd>Bl&auml;sshuhn</trd>
  </trdgrp>
  <trd lng="en">moorhen</trd>
  <trd lng="fr">poule d'eau</trd>
  <trd lng="hu">v&iacute;zity&uacute;k</trd>
  <trd lng="nl">waterhoen</trd>
  <trd lng="pl">kurka wodna</trd>
  <trdgrp lng="pt">
    <trd>frango-d'&aacute;gua</trd>,
    <trd>galinhola</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_y;&c_sh;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">vodná sliepočka</trd>
</drv>
</art>
