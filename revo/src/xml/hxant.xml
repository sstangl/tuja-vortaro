<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id: hxant.xml,v 1.8 2013/02/04 12:10:17 revo Exp $">
     <kap><rad>&hcirc;ant</rad>/o
       <fnt><bib>PIV2</bib>
         <lok>(sub &leftquot;ugra&rightquot;, sen aparta artikolo)</lok>
       </fnt>
     </kap>

     <drv mrk="hxant.0o">
       <kap><tld/>o</kap>
       <snc>
  <dif>
           Ugra gento el &ccirc;. 20 000 homoj en la okcidenta Siberio.
  </dif>
       </snc>
       <trd lng="be">&c_h;&c_a;&c_n;&c_t;&c_y;</trd>
       <trdgrp lng="de">
         <trd>Chanti</trd>
       </trdgrp>
       <trdgrp lng="en">
         <trd>Khanty</trd>, <trd>Ostiak</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>hanti</trd>
       </trdgrp>
       <trdgrp lng="hu">
         <trd>hanti</trd>,
         <trd>osztj&aacute;k</trd>
       </trdgrp>
       <trdgrp lng="it">
         <trd>Khanti</trd>, <trd>Ostiachi</trd>
       </trdgrp>
       <trd lng="nl">Chanti</trd>
       <trdgrp lng="pl">
         <trd>Chantowie</trd>,
         <trd>Ostiacy</trd>
       </trdgrp>
       <trd lng="ru">&c_h;&c_a;&c_n;&c_t;&c_y;</trd>
     </drv>

   </art>

