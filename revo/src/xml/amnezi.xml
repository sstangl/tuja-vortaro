<kap>
  <rad>amnezi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="amnezi.0o">
  <kap><tld/>o</kap>
  <snc mrk="amnezi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Parta a&ubreve; kompleta perdo de la memoro:
      <ekz>
        la juda barbisto, <tld/>a, restas en kuracejo dum jaroj
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_n;&c_e;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_t;&c_a; &c_p;&c_a;&c_m;&c_ja;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">amnézie</trd>
  <trdgrp lng="de">
      <trd>Ged&auml;chtnisverlust</trd>,
      <trd>Amnesie</trd>
  </trdgrp>
  <trd lng="en">amnesia</trd>
  <trd lng="es">amnesia</trd>
  <trd lng="fa">&a_f;&a_r;&a_A;&a_m;&a_w;&a_sh;&f_y;</trd>
  <trd lng="fr">amn&eacute;sie</trd>
  <trd lng="nl">amnesie</trd>
  <trd lng="ru">&c_a;&c_m;&c_n;&c_je;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>amn&eacute;zia</trd>,
    <trd>chorobná zábudlivosť</trd>
  </trdgrp>
</drv>
</art>
