<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: adiant.xml,v 1.11 2013/04/12 10:10:15 revo Exp $">
<kap>
  <rad>adiant</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adiant.0o">
  <kap><tld/>o</kap>
  <snc mrk="adiant.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Filiko pro delikate dentetaj folioj anka&ubreve; kromnomata 
      <ctl>harfiliko</ctl>, genro el familio pteridacoj (<trd 
      lng="la">Adiantum</trd>):
      <ekz>
        &scirc;i alkondukis siajn kaprinojn <klr>...</klr> &scirc;i 
        plezuris vidi ilin man&gcirc;antajn la <tld/>ojn
        <fnt>
          <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>, 
          <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
          Pa&ubreve;lo kaj Virginio</url></vrk>,
          <lok>1905</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi rikoltos florojn, <tld/>ojn belajn,
        en la verdaj kampoj, de vi alnomataj
        la &Gcirc;arden' de Di'
        <fnt>
          <aut>P. C. Pinheiro, J. de Aquino, trad. I. Magalh&atilde;es
          </aut>,
          <vrk><url 
          ref="http://www.kke.org.br/esperanto/muzikaro/vojagxo.php">
          Voja&gcirc;o</url></vrk>,
          <lok>[vidita en 2008-01]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_y;&c_ja;&c_n;&c_t;&c_u;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>adiant</trd>,
    <trd>falzia</trd>
  </trdgrp>
  <trd lng="de">Frauenhaarfarn</trd>
  <trd lng="fa">&f_p;&a_r;&a_s;&f_y;&a_A;&a_w;&a_sh;</trd>
  <trd lng="fr">capillaire<klr> (subst., foug&egrave;re)</klr></trd>
  <trd lng="hu">v&eacute;nuszfodorka</trd>
  <trd lng="it">adianto<klr> (felce)</klr></trd>
  <trdgrp lng="nl">
    <trd>vrouwenhaar<klr tip="ind"> (plant)</klr></trd>,
    <trd>venushaar</trd>
  </trdgrp>
  <trd lng="pl">adiantum</trd>
  <trd lng="pt">avenca</trd>
  <trd lng="sv">adiantumsl&auml;ktet</trd>
</drv>
</art>

