<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kronom1.xml,v 1.10 2014/12/22 11:10:20 revo Exp $">
<kap>
  <rad>kronometri</rad>/o
</kap>
<drv mrk="kronom1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
    <dif>
      Parto de la fiziko, pritraktanta la mezuron de la tempo.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_n;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Chronometrie</trd>,
     <trd>Zeitmessung</trd>
  </trdgrp>
  <trd lng="en">chronometry</trd>
  <trd lng="fr">chronom&eacute;trie</trd>
  <trdgrp lng="hu">
     <trd>kronometria</trd>,
     <trd>id&odblac;m&eacute;r&eacute;stan</trd>,
     <trd>tudom&aacute;nyos <ind>id&odblac;m&eacute;r&eacute;s</ind></trd>
  </trdgrp> 
  <trd lng="pt">cronometria</trd> 
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="kronom1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_n;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>chronometrisch</trd>,
     <trd>zeitlich (eingeordnet)</trd>
  </trdgrp>
  <trd lng="en">chronometric</trd>
  <trd lng="fr">chronom&eacute;trique</trd>
  <trdgrp lng="hu">
     <trd>kronometriai</trd>,
     <trd>id&odblac;m&eacute;r&eacute;stani</trd>
  </trdgrp>  
  <trd lng="ru">&c_h;&c_r;&c_o;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
