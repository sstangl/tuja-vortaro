    <ref tip="prt" cel="mev.0o">mevo</ref>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_j;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">l&agrave;rids</trd>
  <trd lng="de">M&ouml;wen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">sir&aacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="nl">meeuwen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">lar&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_j;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_ch;&c_a;&c_j;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>
