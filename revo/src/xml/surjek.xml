<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: surjek.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>surjekci</rad>/o
</kap>
<drv mrk="surjek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Tia <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>, ke &ccirc;iu
      elemento de la celo-aro estas <ref
      cel="bild.0o.elemento">bildo</ref> 
      de iu elemento el la fonto-aro.
    </dif>
    <ref tip="sin" cel="jxet.sur0o">sur&jcirc;eto.</ref>
  </snc>
  <trd lng="be">&c_s;&c_ju;&c_r;'&c_je;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>surjekce</trd>,
    <trd>surjektivní zobrazení</trd>,
    <trd>zobrazení na...</trd>
  </trdgrp>
  <trd lng="de">Surjektion</trd>
  <trdgrp lng="en">
    <trd>surjection</trd>,
    <trd>onto <ind>function</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_sh;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_n;&f_g;&a_A;&a_sh;&a_t; &f_p;&a_w;&a_sh;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>surjection</trd>,
    <trd>application surjective</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;k&eacute;pez&eacute;s</trd>,
    <trd>sz&uuml;rjekci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>suriezione</trd>,
    <trd>funzione <ind>suriettiva</ind></trd>
  </trdgrp>
  <trd lng="nl">surjectie</trd>
  <trdgrp lng="pl">
    <trd>surjekcja</trd>,
    <trd>funkcja surjektywna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sobreje&ccedil;&atilde;o</trd>,
    <trd>aplica&ccedil;&atilde;o sobrejetiva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je; &leftquot;&c_n;&c_a;&rightquot;</trd>,
    <trd>&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">zobrazenie na...</trd>
</drv>

<drv mrk="surjek.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib><lok>epimorfio</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        la bildigo, kiu &jcirc;etas reelon al &gcirc;ia kvadrato ne estas
        <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ju;&c_r;'&c_je;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="en">surjective</trd>
  <trd lng="de">surjektiv</trd>
  <trd lng="fa">&f_p;&a_w;&a_sh;&a_A;</trd>
  <trd lng="fr">surjectif</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;k&eacute;pez&odblac;</trd>,
    <trd>sz&uuml;rjekt&iacute;v</trd>
  </trdgrp>
  <trd lng="it">suriettivo</trd>
  <trd lng="nl">surjectief</trd>
  <trd lng="pl">surjektywny</trd>
  <trd lng="pt">sobrejetivo</trd>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
