<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tanacet.xml,v 1.4 2015/07/04 08:37:44 revo Exp $">
    <kap>
      <rad>tanacet</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="tanacet.0o">
      <kap><tld/>o</kap>
      <snc mrk="tanacet.0o.SNC">
      <uzo tip="fak">BOT</uzo>
        <dif>
          Vegetala genro (<trd lng="la">Tanacetum</trd> el
          <ref tip="super" cel="astera.0oj">asteracoj</ref>),
          parenca al krizantemo, de unu- a&ubreve; plur-jaraj aromaj herboj
          kun plume dividaj folioj kaj kun plejparte flavaj kapituloj;
          &ccirc;irka&ubreve; 150 specioj el la Norda duonglobo.
        </dif>
      </snc>
  <trdgrp lng="cs">
    <trd>vrati&ccaron;</trd>,
    <trd>řimbaba obecná</trd>
  </trdgrp>
  <trd lng="de">Wucherblumen</trd>
  <trd lng="fr">tanaisie</trd>
  <trd lng="pl">wrotycz</trd>
  <trd lng="ru">&c_p;&c_i;&c_zh;&c_m;&c_a;</trd>
  <trd lng="sk">vrati&ccaron;</trd>
</drv>
  </art>

