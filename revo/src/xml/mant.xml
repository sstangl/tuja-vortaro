<art mrk="$Id: mant.xml,v 1.11 2015/04/10 20:10:20 revo Exp $">
<kap>
  <rad>mant</rad>/o
</kap>
<drv mrk="mant.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Mantis</trd>), kiu atendas &ccirc;asa&jcirc;on kun
      levitaj anta&ubreve;aj kruroj, kvaza&ubreve; pre&gcirc;ante.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pregad&eacute;u</trd>,
    <trd>g&egrave;nere Mantis</trd>
  </trdgrp>
  <trd lng="de">Gottesanbeterin</trd>
  <trd lng="en">mantis</trd>
  <trd lng="hu">im&aacute;dkoz&oacute; <ind>s&aacute;ska</ind></trd>
  <trd lng="nl">bidsprinkhaan</trd>
  <trd lng="pl">modliszka</trd>
  <trdgrp lng="pt">
    <trd>louva-a-deus</trd>,
    <trd>louva-deus</trd>,
    <trd>beata <klr tip="amb">(subst., zool.)</klr></trd>,
    <trd>bendito <klr tip="amb">(subst., zool.)</klr></trd>,
    <trd>bota-mesa</trd>,
    <trd>cavalinho-de-deus</trd>,
    <trd>emboici</trd>,    
    <trd>louvinha-a-deus</trd>,    
    <trd>m&atilde;e-de-cobra</trd>,    
    <trd>manta</trd>,    
    <trd>pai-de-cobra</trd>,    
    <trd>p&otilde;e-mesa</trd>,    
    <trd>profeta</trd>
  </trdgrp>
  <trd lng="ro">mantodea</trd>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_o;&c_m;&c_o;&c_l; <klr
  tip="amb">(&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_o;&c_je;)</klr></trd>
</drv>
</art>
