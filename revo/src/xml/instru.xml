    <trd>&c_v;&c_je;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Lehre</trd>
  <trdgrp lng="es">
    <trd>ense&ntilde;anza</trd>,
    <trd>lecci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enseignement<klr tip="ind"> (ce qu'on
    enseigne)</klr></trd>,
    <trd>le&ccedil;on<klr tip="ind"> (enseignement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tanult ismeret</trd>,
    <trd>tanuls&aacute;g</trd>
  </trdgrp>
  <trd lng="id">[<ind>ajar</ind>] pelajaran</trd>
  <trd lng="nl">onderwezen <ind>kennis</ind></trd>
  <trdgrp lng="pl">
    <trd>nauka</trd>,
    <trd>pouczenie</trd>,
    <trd>instrukta&zdot;</trd>,
    <trd>kszta&lstroke;cenie</trd>,
    <trd>wskaz&oacute;wka</trd>,
    <trd>nauczka<klr> (przen.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>uma <ind>li&ccedil;&atilde;o</ind></trd>,
    <trd>um <ind>ensinamento</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_r;&c_o;&c_k;
      <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>,
    <trd>&c_p;&c_o;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="instru.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="instru.0ejo.lernejo">
    <uzo tip="fak">PED</uzo>
    <ref cel="lern.0ejo" tip="dif">lernejo</ref>
  </snc>
  <trd lng="cs">učebna</trd>
  <trdgrp lng="de">
    <trd>Schule</trd>,
    <trd>Unterrichtsraum</trd>,
    <trd>Seminarraum</trd>
  </trdgrp>
  <trd lng="en">school</trd>
  <trdgrp lng="es">
    <trd>colegio</trd>,
    <trd>escuela</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;cole</trd>,
    <trd>&eacute;tablissement scolaire</trd>
  </trdgrp>
  <trdgrp lng="hu">
