<vortaro>
<art mrk="$Id: guf.xml,v 1.16 2015/07/04 08:37:35 revo Exp $">
<kap><ofc>*</ofc>
<rad>guf</rad>/o</kap>
<drv mrk="guf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio de
      <ref tip="malprt" cel="strige.0oj">strigedoj</ref>
      (<trd lng="la">Bubo</trd>), granda noktobirdo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_g;&c_a;&c_ch;</trd>
  <trd lng="ca">duc</trd>
  <trdgrp lng="cs">
    <trd>v&yacute;r velk&yacute;</trd>,
    <trd>výr velký</trd>
  </trdgrp>
  <trd lng="de">Uhu</trd>
  <trd lng="en">eagle-owl</trd>
  <trd lng="fr">hibou<klr tip="ind"> (oiseau)</klr></trd>
  <trd lng="hu">uhu</trd>
  <trdgrp lng="nl">
    <trd>oehoe</trd>,
    <trd>grote <ind>uil</ind></trd>,
    <trd>ooruil</trd>
  </trdgrp>
  <trd lng="pl">puchacz</trd>
  <trd lng="pt">bufo</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">výr veľký</trd>
</drv>
</art>
