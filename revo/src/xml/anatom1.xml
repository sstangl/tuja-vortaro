<art mrk="$Id: anatom1.xml,v 1.6 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anatom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anatom1.0o">
  <kap><tld/>o</kap>
  <snc mrk="anatom1.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="anatom.0isto.ANA">anatomiisto</ref>
    <ekz>
      la <tld/>o amindumis la junan kaj grasi&gcirc;intan edzinon de la
      futbalju&gcirc;isto
      <fnt>
      <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
      <vrk><url ref="http://www.freeweb.hu/verkoj/fredlamalpura.htm">
      Fred la malpura intervenas</url></vrk>,
      <lok>2002</lok>
      </fnt>;
    </ekz>
    <ekz>
      en 1962 &ccirc;ina <tld/>o pri fi&scirc;o konstatis, ke dol&ccirc;akva kaj mara
      ru&gcirc;brunaj rajoj apartenas al la sama speco
      <fnt>
      <aut>&Ccirc;ina Radio Internacia</aut>,
      <vrk><url ref="http://es.chinabroadcast.cn">
      Ru&gcirc;bruna rajo</url></vrk>,
      <lok>2005-10-18</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>anatom</trd>,
    <trd>pitevní chirurg</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_A;&a_l;&a_b;&a_d;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">anatomiste</trd>
  <trd lng="it">anatomista</trd>
  <trd lng="sk">anatóm</trd>
</drv>
</art>

