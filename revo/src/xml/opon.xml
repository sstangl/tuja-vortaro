<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: opon.xml,v 1.22 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>opon</rad>/i
  <fnt>B</fnt>
</kap>
<drv mrk="opon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="opozic.0i">Opozicii</ref>:
      <ekz>
        tamen <tld/>is unu vo&ccirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        anta&ubreve; kiam la patro povis <tld/>i, li elkuris sur la
        terason<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili tute ne <tld/>is, ke li partoprenu en la soleno<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio, kion diris mia <tld/>anto, estas pura vero<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="star.kontraux0i">kontra&ubreve;stari</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="ca">oposar</trd>
  <trdgrp lng="cs">
    <trd>být v opozici</trd>,
    <trd>namítat</trd>,
    <trd>odporovat</trd>,
    <trd>oponovat</trd>
  </trdgrp>
  <trd lng="de">opponieren</trd>
  <trd lng="en">oppose</trd>
  <trd lng="es">oponer</trd>
  <trd lng="fr">opposer<klr> (s')</klr></trd>
  <trdgrp lng="hu">
    <trd>m&aacute;s v&eacute;lem&eacute;nyen van</trd>,
    <trd>szembehelyezkedik</trd>,
    <trd>ellene sz&oacute;l</trd>,
    <trd>szembehelyezkedik</trd>,
    <trd>oppon&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">opponeren</trd>
  <trd lng="pt">opor-se</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_p;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_n;&c_i;&c_k;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nesúhlasiť</trd>,
    <trd>oponovať</trd>,
    <trd>protirečiť</trd>
  </trdgrp>
</drv>
<drv mrk="opon.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
     Tiu, kiu oponas, ekz-e la alia ludanto en <ref tip="vid"
     cel="sxak.0ludo">&scirc;akludo</ref>.
    </dif>
  </snc>
  <trd lng="cs">oponent</trd>
  <trdgrp lng="de">
        <trd>Opponent</trd>,
        <trd>Gegner</trd>,
        <trd>Gegenspieler</trd>
  </trdgrp>
  <trd lng="en">opponent</trd>
  <trd lng="es">opositor</trd>
  <trdgrp lng="fr">
    <trd>adversaire</trd>,
    <trd>opposant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellenf&eacute;l</trd>,
    <trd>opponens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opponent</trd>,
    <trd>tegenstander</trd>
  </trdgrp>
  <trd lng="pt">advers&aacute;rio</trd>
  <trd lng="sk">oponent</trd>
</drv>

<drv mrk="opon.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
     Tiu, kiu oponas, ekz-e politike a&ubreve; ideologie:
      <ekz>
        oponuloj akuzis Holzmann je degne aristokrata stilo
        <fnt><aut>Trevor Steele</aut>,
  <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
  <lok>p. 91</lok>
  </fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="cs">oponent</trd>
  <trdgrp lng="de">
        <trd>Opponent</trd>,
        <trd>Gegner</trd>,
        <trd>Gegenspieler</trd>
  </trdgrp>
  <trd lng="en">opponent</trd>
  <trd lng="es">opositor</trd>
  <trdgrp lng="fr">
    <trd>adversaire</trd>,
    <trd>opposant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellenf&eacute;l</trd>,
    <trd>opponens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opponent</trd>,
    <trd>tegenstander</trd>
  </trdgrp>
  <trd lng="pt">advers&aacute;rio</trd>
  <trd lng="sk">oponent</trd>
</drv>
</art>
