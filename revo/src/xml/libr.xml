    <trd lng="bg">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_o;</trd>
    <trd lng="ca">llibret <klr tip="amb">&ograve;pera</klr></trd>
    <trd lng="en">libretto</trd>
    <trd lng="de">Libretto</trd>
    <trd lng="es">libretto<klr tip="amb"> &oacute;pera</klr></trd>
    <trdgrp lng="hu">
      <trd>sz&ouml;vegk&ouml;nyv</trd>,
      <trd>librett&oacute;</trd>
    </trdgrp>
    <trd lng="nl">libretto</trd>
    <trd lng="pl">libretto</trd>
    <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
  </snc>
  <snc mrk="libr.0eto.MUZ.teksto">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Tiuj paroloj mem.
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_b;&c_r;&c_e;&c_t;&c_a;</trd>
    <trd lng="de">Libretto</trd>
    <trd lng="en">libretto</trd>
    <trdgrp lng="hu">
      <trd>sz&ouml;vegk&ouml;nyv</trd>,
      <trd>librett&oacute;</trd>
    </trdgrp>
    <trd lng="nl">libretto</trd>
    <trd lng="pl">libretto</trd>
    <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
  </snc>
  <trd lng="ca">llibret</trd>
  <trd lng="cs">libreto <klr>(opery)</klr></trd>
  <trd lng="es">libretto <klr>(opera)</klr></trd>
  <trdgrp lng="fr">
    <trd>libretto</trd>,
    <trd>livret</trd>
  </trdgrp>
  <trd lng="sk">libreto</trd>
</drv>

<drv mrk="libr.aboco0o">
  <kap>aboco<tld/>o</kap>
  <snc mrk="libr.aboco0o.TIP">
    <ref tip="dif" cel="alfabe.0umo">alfabetumo</ref>
    <ekz>
      estis iam homo, kiu verkis iom da novaj versoj por la
      aboco<tld/>o, po du vicoj al &ccirc;iu litero
      <fnt><bib>Fab4</bib>, <lok>abocolibro</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_m;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">beceroles</trd>
  <trd lng="cs">slabik&aacute;&rcaron;</trd>
  <trdgrp lng="es">
    <trd>abecedario</trd>,
    <trd>ABC<klr tip="ind"> (abecedario)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ab&eacute;c&eacute;daire</trd>,
    <trd>ABC<klr tip="ind"> (ab&eacute;c&eacute;daire)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;b&eacute;c&eacute;sk&ouml;nyv</trd>,
    <trd>&aacute;b&eacute;c&eacute;<klr tip="ind"> (k&ouml;nyv)</klr></trd>
  </trdgrp>
  <trd lng="pl">elementarz</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_k;&c_v;&c_a;&c_r;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;labik&aacute;r</trd>
</drv>

<drv mrk="libr.bit0o">
  <kap>bit<tld/>o
    <fnt>
    <bib>KompLeks</bib>
    <lok><url ref="&KompLeks;/B.html#BITLIBRO">bitlibro</url></lok>
    </fnt>
  </kap>
  <snc mrk="libr.bit0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="L"/>o en formo de cifereca dosiero, legebla per
      komputilo (precipe, per po&scirc;komputilo a&ubreve; speciala komputila
      <tld/>legilo) uzante ta&ubreve;gan programon:
      <ekz>
        en Usono jam 3% de la <tld/>ovendoj estas bit<tld/>oj, kaj
        14% de venditaj bit<tld/>oj estas amromanoj
        <fnt>
          <aut>E. Grimley Evans</aut>,
          <vrk><url
          ref="http://www.esperanto.be/fel/publika/010280p.php">
          La nova Kindlo de Amazon</url></vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_n;&c_ib;&c_g;&c_a;</ind></trd>
  <trd lng="ca">llibre <klr>electr&ograve;nic</klr></trd>
  <trd lng="en">e-book</trd>
  <trdgrp lng="es">
    <trd>libro electr&oacute;nico</trd>
  </trdgrp>
  <trd lng="fr">lire &eacute;lectronique</trd>
  <trdgrp lng="pl">
    <trd>ksi&aogonek;&zdot;ka elektroniczna</trd>,
    <trd>e-book</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_n;&c_i;&c_g;&c_a;</trd>
</drv>

<drv mrk="libr.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="libr.blanka0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      En kelkaj landoj, iom granda
      <ref tip="vid" cel="raport.0o">raporto</ref> pri
      socia a&ubreve; te&hcirc;nika demando, kun esplorrezultoj kaj
      rekomendoj:
      <ekz>
        publiki&gcirc;os la blanka <tld/>o pri la &ccirc;ina
        lo&gcirc;antara nutra&jcirc;o kaj la sana stato
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk>&Ccirc;inio unuafoje faris enketadon...</vrk>,
          <lok>2003-12-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_ja; &c_k;&c_n;&c_ib;&c_g;&c_a;</trd>
  <trd lng="ca">Llibre <ind>Blanc</ind></trd>
  <trd lng="en">white paper</trd>
  <trd lng="es">libro <ind>blanco</ind></trd>
  <trd lng="fr">livre blanc</trd>
  <trd lng="hu">feh&eacute;r k&ouml;nyv</trd>
  <trd lng="pl">bia&lstroke;a <ind>ksi&eogonek;ga</ind></trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_a;&c_ja; &c_k;&c_n;&c_i;&c_g;&c_a;</trd>
</drv>

<drv mrk="libr.depon0eto">
  <kap>depon<tld/>eto</kap>
  <snc mrk="libr.depon0eto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="L"/>eto registranta bankajn deponojn:
      <ekz>
        la kontanta mono en la mono ja estas multe pli 
        certa ol en la malgranda depon<tld/>eto
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">livret de d&eacute;p&ocirc;t</trd>
</drv>

<drv mrk="libr.fest0o">
  <kap>fest<tld/>o</kap>
  <snc mrk="libr.fest0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Kolekto da artikoloj, eseoj a&ubreve; alispecaj tekstoj speciale
      verkitaj kaj eldonitaj por honori personon:
      <ekz>
        tiu &ccirc;i artikolo estis verkita por fest<tld/>o honore al
        Ivo Lapenna okaze de ties 75-jara naski&gcirc;taga festo
        <fnt>
          <aut>P. Neergaard</aut>,
          <vrk>La sencovasteco de kelkaj vortoj en Plena
            Ilustrita Vortaro de Esperanto</vrk>,
          <lok>Esperantologio, 1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="artiko.0aro">artikolaro</ref>
  </snc>
  <trd lng="ca">llibre-homenatge</trd>
  <trd lng="en">festschrift</trd>
  <trdgrp lng="en">
    <trd>festschrift</trd>,
    <trd>commemorative publication</trd>
  </trdgrp>
  <trd lng="es">publicaci&oacute;n conmemorativa</trd>
  <trd lng="fr">m&eacute;langes<klr> (recueil d'essais)</klr></trd>
  <trd lng="hu">eml&eacute;kk&ouml;tet</trd>
  <trd lng="nl">feestbundel</trd>
  <trd lng="pl">ksi&eogonek;ga okazyjna</trd>
  <trd lng="ru">&c_ju;&c_b;&c_i;&c_l;&c_je;&c_j;&c_n;&c_y;&c_j; &c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k; &c_s;&c_t;&c_a;&c_t;&c_je;&c_j;</trd>
</drv>

<drv mrk="libr.fraz0o">
  <kap>fraz<tld/>o</kap>
  <snc mrk="libr.fraz0o.TUR">
    <dif>
      <tld lit="L"/>o instruanta bazajn frazojn en fremda lingvo,
      ordinare por uzo de voja&gcirc;antoj:
      <ekz>
        esperanta fraz<tld/>o de l' turisto
        <fnt>
          <aut>P. Fruictier, M. Merckens, T. Cart</aut>,
          <vrk>Esperanta Frazlibro de l' turisto</vrk>,
          <lok>1904</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">konverza&ccaron;n&iacute; p&rcaron;&iacute;ru&ccaron;ka</trd>
  <trd lng="fr">guide de conversation</trd>
  <trd lng="sk">konverza&ccaron;n&aacute; pr&iacute;ru&ccaron;ka</trd>
</drv>

<drv mrk="libr.gast0o">
  <kap>gast<tld/>o</kap>
  <snc mrk="libr.gast0o.TIP">
    <dif>
      <tld lit="L"/>o <sncref ref="libr.0o.notilo"/>, en kiu
      la gastoj a&ubreve; vizitantoj povas lasi sian subskribon
      a&ubreve; komenton:
      <ekz>
        mi devas skribi mian nomon en la gast<tld/>on
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
            9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a; &c_v;&c_o;&c_d;&c_g;&c_u;&c_k;&c_a;&c_w;</trd>
  <trd lng="ca">llibre de visites</trd>
  <trd lng="cs">kniha host&uring;</trd>
  <trd lng="de">G&auml;stebuch</trd>
  <trd lng="el">&beta;&jota;&beta;&lambda;&jota_ton;&omikron; &epsilon;&pi;&jota;&sigma;&kappa;&epsilon;&pi;&tau;&omega_ton;&ny;</trd>
  <trd lng="en">guestbook</trd>
  <trd lng="es">libro de visitas</trd>
  <trd lng="fr">livre d'or</trd>
  <trd lng="hu">vend&eacute;gk&ouml;nyv</trd>
  <trd lng="id"><ind>buku</ind> tamu</trd>
  <trd lng="pl">ksi&eogonek;ga go&sacute;ci</trd>
  <trd lng="pt">livro de visitantes</trd>
  <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a; &c_o;&c_t;&c_z;&c_y;&c_v;&c_o;&c_v;</trd>
  <trd lng="sk">kniha host&iacute;</trd>
  <trd lng="sv">g&auml;stbok</trd>
</drv>

<drv mrk="libr.gvid0o">
  <kap>gvid<tld/>o</kap>
  <snc mrk="libr.gvid0o.TUR">
    <dif>
      <tld lit="L"/>o por gvidi voja&gcirc;antojn:
      <ekz>
        malfermita gvid<tld/>o &ccirc;e la pa&gcirc;o de la plej bela urbo
        <fnt>
          <aut>S. &Scirc;timec</aut>,
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">guia <klr>(d'una ciutat, un museu)</klr></trd>
  <trd lng="cs">pr&uring;vodce <klr>(kniha)</klr></trd>
  <trdgrp lng="de">
    <trd><klr tip="ind">Reise-</klr>F&uuml;hrer</trd>,
    <trd>Reisef&uuml;hrer</trd>
  </trdgrp>
  <trd lng="en">guidebook</trd>
  <trd lng="es">gu&iacute;a <klr tip="ind"> (libro)</klr></trd>
  <trd lng="fr">guide<klr tip="ind"> (livre)</klr></trd>
  <trd lng="hu">&uacute;tikalauz</trd>
  <trd lng="id"><ind>buku</ind> panduan</trd>
  <trdgrp lng="nl">
    <trd>reisgids<klr tip="ind"> (boek)</klr></trd>,
    <trd>gids<klr tip="ind"> (boek)</klr></trd>
