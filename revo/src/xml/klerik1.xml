<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klerik1.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klerikal</rad>/a
  <fnt>B</fnt>
</kap>
<drv mrk="klerik1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Favora al la <ref cel="klerik.0aro">klerikaro</ref>
      kaj al &gcirc;ia superregado:
      <ekz>
        <tld/>a partio;
      </ekz>
      <ekz>
        <ind>kontra&ubreve;<tld/>a</ind> agitado.
        <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_k;&c_l;&c_je;&c_r;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trd lng="ca">anticlerical</trd>
        <trd lng="fr">anticl&eacute;rical</trd>
        <trd lng="hu">antiklerik&aacute;lis</trd>
        <trd lng="nl">antiklerikaal</trd>
        <trdgrp lng="de">
          <trd>antiklerikal</trd>,
          <trd>kirchenfeindlich</trd>
        </trdgrp>
        <trd
        lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_r;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">clerical</trd>
  <trd lng="cs">klerikální</trd>
  <trdgrp lng="de">
    <trd>klerikal</trd>,
    <trd>geistlich</trd>,
    <trd>kirchenfreundlich</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;rical<klr tip="ind"> (favorable au
  cl&eacute;ricalisme)</klr></trd>
  <trd lng="hu">klerik&aacute;lis</trd>
  <trd lng="nl">klerikaal</trd>
  <trd
  lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">klerikálny</trd>
</drv>

<drv mrk="klerik1.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politiko favora al la klerikaro.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_r;&c_y;&c_k;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">clericalisme</trd>
  <trdgrp lng="de">
    <trd>Klerikalismus</trd>,
    <trd>Kirchenfreundlichkeit</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;ricalisme</trd>
  <trd lng="hu">klerikalizmus</trd>  
  <trd lng="nl">clericalisme</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
