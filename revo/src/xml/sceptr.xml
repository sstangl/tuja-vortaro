<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sceptr.xml,v 1.21 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sceptr</rad>/o
</kap>
<drv mrk="sceptr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Komanda bastono, signo de re&gcirc;a povo.
    </dif>
  </snc>
  <snc>
    <dif>
      Re&gcirc;a povo.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Superregado.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_ib;&c_p;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_zh;&c_je;&c_z;&c_malmol;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>přen. vláda</trd>,
    <trd>sceptrum</trd>,
    <trd>žezlo</trd>
  </trdgrp>
  <trd lng="de">Zepter</trd>
  <trdgrp lng="en">
    <trd>scepter</trd>,
    <trd>sceptre</trd>
  </trdgrp>
  <trd lng="es">cetro</trd>
  <trd lng="fr">sceptre</trd>
  <trd lng="he">&shin;&resh;&bet;&yod;&tet;</trd>
  <trdgrp lng="hu">
    <trd>jogar</trd>,
    <trd>kir&aacute;lyi p&aacute;lca</trd>
  </trdgrp>
  <trd lng="it">scettro</trd>
  <trd lng="nl">scepter</trd>
  <trd lng="pl">ber&lstroke;o</trd>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_p;&c_je;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kráľovská berla; pren. kráľovská moc</trd>,
    <trd>vláda</trd>,
    <trd>žezlo</trd>
  </trdgrp>
</drv>
</art>
