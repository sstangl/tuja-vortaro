<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekonom1.xml,v 1.32 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ekonomi</rad>/o
</kap>
<drv mrk="ekonom1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekonom1.0o.situacio">
    <tezrad fak="EKON"/>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Situacio de institucio a&ubreve; regno la&ubreve; vidpunkto de
      ri&ccirc;eco kaj materia prospereco:
      <ekz>
        <tld/>a konkurado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a krizo.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</trd>
    <trd lng="bg">&c_i;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Wirtschaft</trd>,
      <trd>&Ouml;konomie</trd>
    </trdgrp>
    <trd lng="hu">gazd&aacute;lkod&aacute;s</trd>
    <trd lng="pl">ekonomia</trd>
    <trd lng="ru">&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_k;&c_a;</trd>
    <trd lng="sv">ekonomi</trd>
  </snc>
  <snc mrk="ekonom1.0o.SCI">
    <ref tip="dif" cel="ekonom2.0o">ekonomiko</ref>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_ja;
      &c_t;&c_e;&c_o;&c_r;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Wirtschaftswissenschaft</trd>
    <trdgrp lng="fr">
      <trd>&eacute;conomie<klr> (science &eacute;conomique)</klr></trd>
    </trdgrp>
    <trd lng="nl">economie</trd>
    <trd lng="hu">gazdas&aacute;gtan</trd>
    <trd lng="pl">ekonomia</trd>
    <trd lng="ru">&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_k;&c_a;
    <klr>(&c_n;&c_a;&c_u;&c_k;&c_a;)</klr></trd>
    <trd lng="sv">ekonomi</trd>
  </snc>
  <snc mrk="ekonom1.0o.mastrumado">
    <dif>
      Sistema kaj &scirc;parema ordo en mastrumado a&ubreve;
      administrado de
      domo, bieno, institucio, regno:
      <ekz>
        hejma, socia, monda, agrikultura <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
      &c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Wirtschaften</trd>,
      <trd>Wirtschaft</trd>,
      <trd>Wirtschaftlichkeit</trd>,
      <trd>Haushalten</trd>,
      <trd>&Ouml;konomie</trd>
    </trdgrp>
    <trd lng="hu">gazdas&aacute;g</trd>
    <trdgrp lng="pl">
      <trd>ekonomia</trd>,
      <trd>gosopodarka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_o;&c_je;
      &c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
    <trd lng="sv">ekonomi</trd>
  </snc>
  <snc mrk="ekonom1.0o.sxparado">
    <dif>
      Atingo de celo per plej simplaj kaj plej efikaj rimedoj;
      &scirc;parado:
      <ekz>
        la scienco estas <tld/>o pri pensado;
      </ekz>
      <ekz>
        la reguleco de la lingvo enkondukas grandegan <tld/>on en la
        nombro de la vortoj, kiujn oni bezonas lerni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni uzu niajn fortojn <tld/>e kaj sa&gcirc;e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Wirtschaftlichkeit</trd>,
      <trd>&Ouml;konomie</trd>
    </trdgrp>
    <trd lng="hu">gazdas&aacute;goss&aacute;g</trd>
    <trd lng="pl">ekonomia</trd>
    <trd lng="ru">&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="ca">economia</trd>
  <trdgrp lng="cs">
    <trd>ekonomie</trd>,
    <trd>hospodaření</trd>,
    <trd>hospodárnost</trd>,
    <trd>hospodářství</trd>
  </trdgrp>
  <trd lng="en">economy</trd>
  <trd lng="es">econom&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>&eacute;conomie</trd>
  </trdgrp>
  <trd lng="nl">economie</trd>
  <trd lng="no">&oslash;konomi</trd>
  <trd lng="pt">economia</trd>
  <trd lng="sk">hospodárska veda</trd>
</drv>

<drv mrk="ekonom1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al la <tld/>o.
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">econ&ograve;mic</trd>
  <trdgrp lng="cs">
    <trd>ekonomický</trd>,
    <trd>hospodárný</trd>,
    <trd>hospodářský</trd>,
    <trd>úsporný</trd>,
    <trd>šetrný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirtschaftlich</trd>,
    <trd>&ouml;konomisch</trd>
  </trdgrp>
  <trd lng="en">economic</trd>
  <trd lng="es">econ&oacute;mico</trd>
  <trd lng="fr">&eacute;conomique</trd>
  <trd lng="hu">gazdas&aacute;gi</trd>
  <trd lng="nl">economisch</trd>
  <trd lng="no">&oslash;konomisk</trd>
  <trd lng="pl">ekonomiczny</trd>
  <trd lng="ru">&c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hospodárny</trd>,
    <trd>hospodársky</trd>,
    <trd>úsporný</trd>
  </trdgrp>
</drv>

<drv mrk="ekonom1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">economista</trd>
  <trd lng="cs">ekonom</trd>
  <trd lng="en">economist</trd>
  <trd lng="fr">&eacute;conomiste</trd>
  <trd lng="nl">econoom</trd>
  <trd lng="no">&oslash;konom</trd>
  <trd lng="sk">ekonóm</trd>
</drv>

<drv mrk="ekonom1.merkat0o">
  <kap>merkat<tld/>o</kap>
  <snc mrk="ekonom1.merkat0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="E"/>o, en kiu regas precipe merkataj proponoj kaj
      postuloj, kaj kompaniaj decidoj:
      <ekz>
        merkat<tld/>o altnivele konkurkapabla
        <fnt>
        <aut>T. Lemaheux, trad. P. Levi</aut>,
        <vrk><url
        ref="http://satesperanto.free.fr/satkulturo/article.php3?id_article=153">Dek
        motivoj por rifuzi...</url></vrk>,
        <lok>L'Humanit&eacute;, 2004-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_n;&c_k;&c_a;&c_v;&c_a;&c_ja;
  <ind>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</ind></trd>
  <trd lng="ca"><ind>economia</ind> de mercat</trd>
  <trdgrp lng="cs">
    <trd>tržní ekonomie</trd>,
    <trd>tržní hospodářství</trd>
  </trdgrp>
  <trd lng="fr">&eacute;conomie de march&eacute;</trd>
  <trd lng="nl">markteconomie</trd>
  <trd lng="no">markeds&oslash;konomi</trd>
  <trd lng="pl">gospodarka rynkowa</trd>
  <trd lng="sk">tržné hospodárstvo</trd>
</drv>

<drv mrk="ekonom1.plan0o">
  <kap>plan<tld/>o</kap>
  <snc mrk="ekonom1.plan0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="E"/>o, en kiu precipe la &scirc;tato planas kaj direktas
      la
      produktadon kaj &gcirc;ian evoluon:
      <ekz>
        La diferenco inter plan<tld/>o kaj merkatekonomio fakte ja ne
        konsistas en tio, ke la unua planas, kaj la dua ne, sed la
        maniero
        de la planado
        <fnt>
        <aut>M. Behr k. a.</aut>,
        <vrk><url
        ref="http://www.fw.hu/eventoj/steb/direktado_kaj_ekonomio/smerkat.htm">Enkonduko
        en la socialan merkat-ekonomion</url></vrk>,
        <lok><url ref="http://www.fw.hu/eventoj/steb/">Steb</url>,
        1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_ja;
  <ind>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</ind></trd>
  <trd lng="ca"><ind>economia</ind> planificada</trd>
  <trd lng="fr">&eacute;conomie planifi&eacute;e</trd>
  <trd lng="nl">planeconomie</trd>
  <trd lng="no">plan&oslash;konomi</trd>
  <trd lng="pl">gospodarka planowa</trd>
</drv>

<drv mrk="ekonom1.0a.OEKE">
  <kap>Organiza&jcirc;o por <tld lit="E"/>aj Kunlaboro kaj Evoluigo
  <fnt><bib>ManPol</bib></fnt></kap>
  <mlg>OEKE</mlg>
  <snc mrk="ekonom1.0a.OEKE.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speciala <ref tip="lst"
      cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj">organiza&jcirc;o</ref>
      de <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj
      Nacioj</ref>, kiu celas plani, kunordigi kaj profundigi <tld/>ajn
      kunlaboron kaj evoluigon:
      <ekz>
        la listo de landoj, kiuj disponigas favorajn impostojn, estas
        kunmetita de Organiza&jcirc;o por <tld lit="E"/>aj Kunlaboro kaj
        Evoluigo (OEKE). Kriterio estis aliro al la imposta konkurenco,
        kiu en tiuj landoj, la&ubreve; opinio de OEKE, estas maljusta
        kaj
        nelojala.
        <fnt>
        <aut>Evgeni GEORGIEV</aut>,
        <vrk><url ref="&Monato;/007128.html">&Ccirc;u venos fino de la
        impostaj paradizoj?</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;
    &c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_g;&c_a;
    &c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;
    &c_ib;
    &c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_c;&c_mol;&c_c;&c_ja;</trd>,
    <trd>&c_A;&c_E;&c_S;&c_R;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; de Cooperaci&oacute; i Desenvolupament Econ&ograve;mic</trd>,
    <trd>OCDE</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Organisation f&uuml;r Wirtschaftliche Zusammenarbeit</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Organisation for <ind>Economic</ind> Co-operation and
    Development</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation de coop&eacute;ration et de d&eacute;veloppement
    <ind>&eacute;conomique</ind></trd>,
    <trd>OCDE</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Gazdas&aacute;gi Egy&uuml;ttm&udblac;k&ouml;d&eacute;si
    &eacute;s
    Fejleszt&eacute;si Szervezet</trd>,
    <trd>OECD</trd>
  </trdgrp><trdgrp lng="nl">
    <trd>Organisatie voor Economische Samenwerking en
    Ontwikkeling</trd>,
    <trd>OESO</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>Organisasjon for &Oslash;konomisk Samarbeid og Utvikling</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Organizacja Wsp&oacute;&lstroke;pracy <ind>Gospodarczej</ind> i
    Rozwoju</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o para a Coopera&ccedil;&atilde;o
    <ind>Econ&ocirc;mica</ind> e o
    Desenvolvimento</trd>,
    <trd>OCED</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    &c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_g;&c_o;
    &c_s;&c_o;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;
    &c_i; &c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_O;&c_E;&c_S;&c_R;</trd>
  </trdgrp>
</drv>
</art>
