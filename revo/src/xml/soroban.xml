<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: soroban.xml,v 1.5 2015/08/22 15:10:25 revo Exp $">
<kap>
<rad>soroban</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="soroban.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Japana <ref tip="super" cel="kalkul.0ilo">kalkulilo</ref>:
    </dif>
    <ekz>
    <tld/>o kun globetoj povantaj repreni &ccirc;iuj sian originan lokon
en duonsekundo
    <fnt>"El Popola &Ccirc;inio", 8/1994, <lok>"Kolektanto de abakoj",
p.50</lok></fnt>
    </ekz>
    <ref tip="vid" cel="abak.0o.kalkulilo">abako</ref>
  </snc>
  <trd lng="cs">japonsk&eacute; po&ccaron;&iacute;tadlo</trd>
  <trdgrp lng="de">
    <trd>Abakus</trd>,
    <trd>Soroban</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>soroban</trd>,
    <trd>abacus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soroban</trd>,
    <trd>boulier</trd>
  </trdgrp>
  <trd lng="sk">japonsk&eacute; po&ccaron;&iacute;tadlo</trd>
</drv>
</art>

