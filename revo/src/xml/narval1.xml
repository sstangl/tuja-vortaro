<kap>
  <rad>narvaled</rad>/oj
</kap>
<drv mrk="narval1.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Monodontidae</trd>).
    </dif>
  </snc>
  <trd lng="ca">monod&ograve;ntids</trd>
  <trd lng="cs">narvalovití <klr>(čeleď)</klr></trd>
  <trd lng="de">Gr&uuml;ndelwale</trd>
  <trd lng="fr">monodontidae</trd>
  <trd lng="hu">narv&aacute;lok<klr tip="ind"> (csal&aacute;d)</klr></trd>
  <trd lng="nl">grondeldolfijnen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">monodont&iacute;deos</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_u;&c_h;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">narvalovití <klr>(čeľaď)</klr></trd>
</drv>
</art>
