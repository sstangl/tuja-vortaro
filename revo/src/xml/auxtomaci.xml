<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxtomaci.xml,v 1.4 2005/06/22 16:32:10 revo Exp $">
<kap>
  <rad>a&ubreve;tomaci</rad>/o
</kap>
<drv mrk="auxtomaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      A&ubreve;tomata kunordigo de sinsekvaj stadioj de la produktado,
      ekz-e a&ubreve;tomata provizado kaj regado de il-ma&scirc;inoj en fabriko.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Automatisierung</trd>,
    <trd>Automation</trd>,
    <trd>Mechanisierung</trd>
  </trdgrp>
  <trd lng="en">automation</trd>
  <trdgrp lng="es">
    <trd>automaci&oacute;n</trd>,
    <trd>automatizaci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">automation</trd>
  <trd lng="hu">automatiz&aacute;l&aacute;s</trd>
  <trd lng="it">automazione</trd>
  <trdgrp lng="nl">
    <trd>automatisering</trd>,
    <trd>automatie</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_a;&c_t;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">automation</trd>
</drv>
</art>
