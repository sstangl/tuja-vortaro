<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: huz.xml,v 1.21 2013/10/07 15:11:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>huz</rad>/o
</kap>
<drv mrk="huz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref> parenca
      al 
      <ref cel="acipenser.0o">acipensero</ref>, sed kun lar&gcirc;a duoncirkla
      anta&ubreve;en malfermi&gcirc;anta bu&scirc;o; <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="acipen1.0oj">acipenseredoj</ref>
      (<trd lng="la">Huso</trd>), kies pli granda,
      &gcirc;is ses metrojn longa, specio vivas en Nigra kaj Kaspia Maroj kaj 
      alfluantaj riveroj kaj estas kaptata pro
      <ref tip="prt" cel="kaviar.0o">kaviaro</ref>. 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_u;&c_g;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Hausen<klr tip="amb"> (Fisch)</klr></trd>,
    <trd>Belugast&ouml;r</trd>
  </trdgrp>
  <trd lng="el">&omikron;&xi;&ypsilon_ton;&rho;&rho;&ypsilon;&gamma;&chi;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>beluga</trd>,
    <trd>European sturgeon</trd>,
    <trd>white sturgeon</trd>,
    <trd>hausen</trd>
  </trdgrp>
  <trd lng="es">esturi&oacute;n</trd>
  <trd lng="fr">esturgeon b&eacute;louga</trd>
  <trd lng="hu">viza</trd>
  <trd lng="it">storione<klr> (genere "huso")</klr></trd>
  <trdgrp lng="nl">
    <trd>huso</trd>, <trd>steur</trd>
  </trdgrp>
  <trd lng="pl">bie&lstroke;uga</trd>
  <trd lng="pt">esturj&atilde;o</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_u;&c_g;&c_a;
<klr>(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
</drv>
</art>
