  <trd lng="cs">Volha</trd>
  <trd lng="de">Wolga</trd>
  <trd lng="en">Volga</trd>
  <trd lng="es">Volga</trd>
  <trd lng="fa">&a_w;&a_l;&f_g;&a_A;</trd>
  <trd lng="fr">Volga</trd>
  <trd lng="hu">Volga</trd>
  <trd lng="nl">Wolga</trd>
  <trd lng="pl">Wo&lstroke;ga</trd>
  <trdgrp lng="pt">
       <trd>Volga</trd>
    </trdgrp>
  <trd lng="ru">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="sk">Volga</trd>
</drv>
