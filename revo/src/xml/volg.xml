      <rad>Volg</rad>/o
    </kap>
    <drv mrk="volg.V0o">
      <kap><tld/>o</kap>
      <fnt><bib>ZR</bib></fnt>
      <snc>
      <uzo tip="fak">GEOG</uzo>
	<dif>
          Rivero en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
          la plej longa en
          <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
          enfluanta <ref cel="kaspi.K0o">Kaspion</ref>.
	</dif>
      </snc>
  <trd lng="be">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="bg">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
