<vortaro>
<art mrk="$Id: log1.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>log</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="log1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Instrumento, konsistanta el &scirc;nuro kun plombo, kiun oni mergas en
      maron por mezuri la rapidon de &scirc;ipo.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="log.0o">logo</ref>.
    </rim>
  </snc>
  <trdgrp lng="cs">
    <trd>vnada</trd>,
    <trd>ženské vnady</trd>
  </trdgrp>
  <trd lng="de">Log</trd>
  <trd lng="en">log</trd>
  <trdgrp lng="es">
    <trd>barquilla</trd>
  </trdgrp>
  <trd lng="fr">loch</trd>
  <trdgrp lng="hu">
    <trd>sebess&eacute;gm&eacute;r&odblac; <klr
tip="amb">(haj&oacute;z&aacute;s)</klr></trd>,
    <trd>log</trd>
  </trdgrp>
  <trd lng="nl">log</trd>
  <trd lng="pt">barquinha</trd>
  <trd lng="ru">&c_l;&c_a;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>vnada</trd>,
    <trd>ženské vnady</trd>
  </trdgrp>
</drv>
</art>
