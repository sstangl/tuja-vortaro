<vortaro>
<art mrk="$Id: sauxrk.xml,v 1.14 2013/08/26 17:10:17 revo Exp $">
<kap>
  <rad>sa&ubreve;rkra&ubreve;t</rad>/o
</kap>
<drv mrk="sauxrk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentinta lamenigita brasiko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_ja; 
       <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>, 
    <trd>&c_k;&c_v;&c_a;&c_sh;&c_a;&c_n;&c_a;&c_ja; 
       <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd> 
  </trdgrp>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_o; &c_z;&c_je;&c_l;&c_je;</trd>
  <trd lng="ca">xucrut</trd>
  <trd lng="de">Sauerkraut</trd>
  <trd lng="en">sauerkraut</trd>
  <trd lng="fr">choucroute</trd>
  <trd lng="he">&kaf;&resh;&vav;&bet; &kaf;&bet;&vav;&shin;</trd>
  <trd lng="hu">savany&uacute; <ind>k&aacute;poszta</ind></trd>
  <trd lng="it">crauti</trd>
  <trd lng="nl">zuurkool</trd>
  <trdgrp lng="pl">
    <trd>kiszona <ind>kapusta</ind></trd>,
    <trd>kwaszona <ind>kapusta</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_s;&c_l;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>,
    <trd>&c_k;&c_v;&c_a;&c_sh;&c_je;&c_n;&c_a;&c_ja; <ind>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</ind></trd>
  </trdgrp>
</drv>
</art>
