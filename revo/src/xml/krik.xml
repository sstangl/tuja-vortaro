<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krik.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>krik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krik.0o">
  <kap><tld/>o</kap>
  <snc mrk="krik.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de <ref tip="super" cel="lev.0ilo.aparato">levilo</ref>,
      havanta dentostangon suprenlevatan de dentradeto, movata per
      kranko kaj uzata por levi pezajn &scirc;ar&gcirc;ojn:
      <ekz>
        <tld/>o de pumpilo;
      </ekz>
      <ekz>
        <ind>veturil<tld/>o</ind>;
        <trd lng="hu">kocsiemel&odblac;</trd>
      </ekz>
      <ekz>
        nitinga <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="dent.0ostango">dentostango</ref>,
      <ref cel="krank.0o">kranko</ref>,
      <ref cel="kapsta.0o">kapstano</ref>,
      <ref cel="vindas.0o">vindaso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>hever</trd>,
    <trd>zved√°k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Winde</trd>,
    <trd>Zahnstangenwinde</trd>,
    <trd>Wagenheber</trd>
  </trdgrp>
  <trd lng="en">jack</trd>
  <trd lng="fr">cric</trd>
  <trd lng="hu">emel&odblac;</trd>
  <trd lng="nl">krik</trd>
  <trd lng="pl">lewar</trd>
  <trd lng="pt">macaco <klr tip="amb">(de autom&oacute;vel)</klr></trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">hever <klr>(s ozub. kolesom)</klr></trd>
</drv>
</art>
