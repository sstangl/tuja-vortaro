<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fiask.xml,v 1.19 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>fiask</rad>/o
</kap>
<drv mrk="fiask.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plena malsukceso:
      <ekz>renkonti <tld/>on<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_ja;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>frac&agrave;s</trd>,
    <trd>desengany</trd>,
    <trd>fiasco</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fiasko</trd>,
    <trd>neúspěch</trd>,
    <trd>zmar</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fiasko</trd>,
     <trd>Misserfolg</trd>,
     <trd>Pleite</trd></trdgrp>
  <trd lng="el">&phi;&jota;&alfa_ton;&sigma;&kappa;&omikron;</trd>
  <trd lng="en">fiasco</trd>
  <trdgrp lng="es">
    <trd>fiasco</trd>,
    <trd>fracaso</trd>,
    <trd>decepci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;chec</trd>,
    <trd>fiasco</trd>,
    <trd>insucc&egrave;s</trd>,
    <trd>ratage</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>kudarc</trd>,
    <trd>fiask&oacute;</trd></trdgrp>
  <trd lng="nl">fiasco</trd>
  <trdgrp lng="pt">
    <trd>fiasco</trd>,
    <trd>malogro</trd>,
    <trd>fracasso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_a;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">nezdar</trd>
</drv>
<drv mrk="fiask.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plene malsukcesi. 
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.0i">fali</ref>,
      <ref cel="bankro.0i">bankroti</ref>,
      <ref cel="abort.0i">aborti</ref>,
      <ref cel="tomb">entombi&gcirc;i</ref>,
      <ref cel="ruin">ruini&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol; <ind>&c_f;&c_ib;&c_ja;&c_s;&c_k;&c_a;</ind></trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol; <ind>&c_n;&c_ja;&c_w;&c_d;&c_a;&c_ch;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fracassar</trd>,
    <trd>fallir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blamovat se</trd>,
    <trd>neuspět</trd>,
    <trd>propadnout ve škole</trd>,
    <trd>utrpět fiasko</trd>,
    <trd>ztroskotat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>fehlschlagen</trd>,
     <trd>Schiffbruch erleiden</trd>,
     <trd>scheitern</trd></trdgrp>
  <trd lng="en">fail utterly</trd>
  <trd lng="es">fracasar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;chouer</trd>,
    <trd>rater<klr tip="ind"> (intr., &eacute;chouer)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>kudarcot vall</trd>,
    <trd>cs&uuml;t&ouml;rt&ouml;k&ouml;t mond</trd></trdgrp>
  <trd lng="nl">falen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; &c_f;&c_i;&c_a;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; &c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nezdar</trd>,
    <trd>utrpieť fiasko</trd>,
    <trd>zlyhať</trd>
  </trdgrp>
</drv>
</art>
