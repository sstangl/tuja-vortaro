<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gamel.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>gamel</rad>/o
</kap>
<drv mrk="gamel.0o">
  <kap><tld/>o</kap>
  <snc mrk="gamel.0o.MIL">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Soldata a&ubreve; &scirc;ipana man&gcirc;ovazo ordinare metala:
      <ekz>
        unutone tintas <tld/>o, kro&ccirc;ita al dorsosako
        <fnt>
        <aut>Vladimir Glazunov</aut>,
        <vrk><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
        &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
        <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ja;&c_l;&c_o;&c_k; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_zh;&c_a;&c_w;&c_n;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="ca">carmanyola</trd>
  <trdgrp lng="cs">
    <trd>ešus</trd>,
    <trd>jídel. miska</trd>,
    <trd>společná mísa</trd>,
    <trd>šálek</trd>
  </trdgrp>
  <trd lng="de">Essgeschirr</trd>
  <trdgrp lng="en">
    <trd>billy can</trd>,
    <trd>dixie</trd>,
    <trd>pannikin</trd>,
    <trd>mess tin</trd>
  </trdgrp>
  <trd lng="fr">gamelle</trd>
  <trd lng="hu">csajka</trd>
  <trdgrp lng="it">
    <trd>gamella</trd>,
    <trd>gavetta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gamel</trd>,
    <trd>etensbak</trd>,
    <trd>eetketeltje</trd>
  </trdgrp>
  <trd lng="pt">marmita</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;<klr> (&c_p;&c_o;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_s;&c_u;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>ešus</trd>,
    <trd>vojenská šálka</trd>
  </trdgrp>
</drv>
</art>
