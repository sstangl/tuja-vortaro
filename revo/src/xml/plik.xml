<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plik.xml,v 1.16 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plik</rad>/o
</kap>
<drv mrk="plik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plik.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> de kraniha&ubreve;to, karakterizata per
      troa grasa sekrecio.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_w;&c_t;&c_u;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>koldoun</trd>,
    <trd>zplstění vlasů</trd>
  </trdgrp>
  <trd lng="de">Weichselzopf</trd>
  <trd lng="es">plica <klr>(med.)</klr></trd>
  <trdgrp lng="fr">
    <trd>plique<klr tip="amb"> polonaise</klr></trd>,
    <trd>trichoma</trd>
  </trdgrp>
  <trd lng="hu">lengyelf&uuml;rt</trd>
  <trd lng="nl">Poolse <ind>vlecht</ind></trd>
  <trd lng="pl">ko&lstroke;tun</trd>
  <trdgrp lng="pt">
    <trd>plica</trd>,
    <trd>tricoma</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_t;&c_u;&c_n;</trd>
  <trd lng="sk">mazotok lebečnej kože</trd>
</drv>
</art>
