<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kral.xml,v 1.7 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kral</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="kral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Granda bruto<ref tip="super" cel="kort.0o">korto</ref>
      &ccirc;irka&ubreve;ata de
      palisoj a&ubreve; konstrua&jcirc;oj:
      <ekz>
        tiam ili decidis iri en la <tld/>on
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 17a, p.
        121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_a;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kraal</trd>,
    <trd>kral</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gehege</trd>,
    <trd>Einpferchung</trd>
  </trdgrp>
  <trd lng="en">corral</trd>
  <trd lng="es">corral</trd>
  <trd lng="fr">corral</trd>
  <trdgrp lng="hu">
    <trd>cser&eacute;ny</trd>,
    <trd>kar&aacute;m</trd>
  </trdgrp>
  <trd lng="nl">kraal<klr tip="amb"> (vee)</klr></trd>
  <trd lng="pt">curral</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_a;&#769;&c_l;&c_mol;</trd>
  <trd lng="sk">kraal</trd>
</drv>
</art>

