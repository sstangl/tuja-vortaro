<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: babiloni.xml,v 1.14 2011/10/15 16:10:14 revo Exp $">
<kap>
  <rad>Babiloni</rad>/o
</kap>

<drv mrk="babiloni.B0o">
  <kap><tld/>o <fnt>PIV</fnt></kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Antikva regno en
      <ref tip="malprt" cel="mezopotami.M0o">Mezopotamio</ref>,
      kaj kies &ccirc;efurbo estis
      <ref tip="prt" cel="babilon.B0o">Babilono</ref>:
      <ekz>
        la tuta <ref cel="sumer.S0ujo">Sumerujo</ref> i&gcirc;is
        parto de la suda <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="babel.B0o">Babelo</ref>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">Babil&ograve;nia</trd>
  <trd lng="de">Babylonien</trd>
  <trd lng="en">Babylonia</trd>
  <trd lng="es">Babilonia</trd>
  <trd lng="fa">&a_b;&a_A;&a_b;&a_kasra;&a_l;<klr tip="ind"> (&f_k;&a_sh;&a_w;&a_r;)</klr></trd>
  <trd lng="fr">Babylonie</trd>
  <trd lng="hu">Babil&oacute;nia</trd>
  <trd lng="it">Babilonia</trd>
  <trd lng="nl">Babyloni&euml;</trd>
  <trd lng="pl">Babilonia</trd>
  <trd lng="ru">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sv">Babylonien</trd>
</drv>

</art>

