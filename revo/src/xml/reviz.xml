<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: reviz.xml,v 1.12 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>reviz</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="reviz.0i">
  <kap><tld/>i</kap>
  <snc>
    <ref tip="dif" cel="revizi.0i">revizii</ref>
    <ekz>
      alveturis oficisto kun la ordono <tld/>i la tutan gubernion
      <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 1a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_v;&c_ib;&c_z;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ja;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kontrolovat</trd>,
    <trd>prověřovat</trd>,
    <trd>přezkoumat</trd>,
    <trd>revidovat</trd>
  </trdgrp>
  <trd lng="de">revidieren</trd>
  <trdgrp lng="fr">
    <trd>contr&ocirc;ler</trd>,
    <trd>inspecter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;tvizsg&aacute;l</trd>,
    <trd>fel&uuml;lvizsg&aacute;l</trd>,
    <trd>revide&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">reviseren</trd>
  <trd lng="pt">revisar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_v;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kontrolovať</trd>,
    <trd>preskúmať</trd>,
    <trd>preverovať</trd>,
    <trd>revidovať</trd>
  </trdgrp>
</drv>
</art>
