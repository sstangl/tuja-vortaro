<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lampir.xml,v 1.22 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lampir</rad>/o
</kap>
<drv mrk="lampir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="lampir1.0oj"><tld/>edoj</ref> (<trd lng="la">Lampyris</trd>),
      kies ino havas &ccirc;e abdomeno du organojn per kiuj &gcirc;i povas
      produkti
      gla&ubreve;kan lumeton:
      <ekz>
        <ctl>studi &ccirc;e la ne&gcirc;a helo a&ubreve; <tld/>a brilo</ctl>
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, p. 592a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cuca de llum</trd>,
    <trd>lluerna</trd>,
    <trd>llumeneta</trd>
  </trdgrp>
  <trd lng="cs">světluška</trd>
  <trd lng="de">Gl&uuml;hw&uuml;rmchen</trd>
  <trd lng="el">&pi;&ypsilon;&gamma;&omikron;&lambda;&alfa;&my;&pi;&jota_ton;&delta;&alfa;</trd>
  <trdgrp lng="en">
    <trd>glow-worm</trd>,
    <trd>firefly</trd>
  </trdgrp>
  <trd lng="es">luci&eacute;rnaga</trd>
  <trd lng="fr">vers luisant</trd>
  <trd lng="hu">szentj&aacute;nosbog&aacute;r</trd>
  <trd lng="nl">glimworm</trd>
  <trdgrp lng="pt">
    <trd>vaga-lume</trd>,
    <trd>pirilampo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">svätojánska muška</trd>
</drv>
</art>
