<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krizol.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>krizolit</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krizol.0o">
  <kap><tld/>o</kap>
  <snc mrk="krizol.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta &scirc;tono, plej perfekta kristalo de
      <ref tip="super" cel="peridot.0o.MIN">peridoto</ref>,
      bele verdeta, travidebla.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_y;&c_z;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">cris&ograve;lit</trd>
  <trd lng="cs">chryzolit</trd>
  <trd lng="de">Chrysolith</trd>
  <trdgrp lng="en">
    <trd>chrysolite</trd>,
    <trd>olivine</trd>,
    <trd>peridot</trd>
  </trdgrp>
  <trd lng="fr">chrysolite</trd>
  <trd lng="hu">krizolit</trd>
  <trd lng="nl">chrysoliet</trd>
  <trd lng="pt">cris&oacute;lita</trd>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_z;&c_o;&c_l;&c_i;&c_t;</trd>
  <trd lng="sk">chryzolit</trd>
</drv>
</art>

