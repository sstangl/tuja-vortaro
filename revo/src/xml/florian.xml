<art mrk="$Id: florian.xml,v 1.8 2015/02/26 20:11:16 revo Exp $">
<kap>
  <rad>Florian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="florian.0o">
  <kap><tld/>o</kap>
  <snc mrk="florian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        sinjoro <tld/>o ekskuis la kapon, dirante: &leftquot;La hundoj 
        el&scirc;iris sin el mia mano...&rightquot;
        <fnt>
          <aut>J. Arbes, trad. J. Gr&ncaron;a</aut>,
          <vrk>Moderna Magdaleno</vrk>,
          <lok>
<url ref="http://www.gutenberg.org/etext/18178">Rakontoj</url>, 
          eld. F. Boh&aacute;&ccaron;ek, 1908</lok> 
        </fnt>;
      </ekz>
      <ekz>
        en malluma angulo &ccirc;e tablo sidis super paperrula&jcirc;o <tld/>o 
        Basjacki
        <fnt>
          <aut>U. Karatkevi&ccirc;, trad.  Z. Lapcionak</aut>,
          <vrk>
<url ref="http://zmila.at.tut.by/kaeh/Enhavo.html">Kristo 
          alteri&gcirc;is en Harodno</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_F;&c_l;&c_o;&c_r;&c_i;&c_a;&c_n;</trd>
  <trd lng="de">Florian</trd>
  <trd lng="fr">Florian</trd>
  <trdgrp lng="hu">
   <trd>Fl&oacute;ris</trd>,
   <trd>Fl&oacute;ri&aacute;n</trd>
  </trdgrp>
  <trd lng="it">Floriano</trd>
  <trdgrp lng="pl">
    <trd>Florian</trd>,
    <trd>Florek <klr>(zdrob.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Floriano</trd>
</drv>
</art>

