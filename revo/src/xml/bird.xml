<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bird.xml,v 1.53 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bird</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bird.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bird.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la klaso <ref tip="malprt" cel="bird.0oj"><tld/>oj</ref>:
      <ekz>
        &gcirc;i estas ankora&ubreve; <tld/>o sur la tegmento
        <klr>(neefektivigita revo)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
    <trdgrp lng="br">
      <trd>evn</trd>,
      <trd>labous</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ocell</trd>,
      <trd>pardal</trd>
    </trdgrp>
    <trd lng="en">bird</trd>
    <trd lng="es">p&aacute;jaro</trd>
    <trd lng="fr">oiseau</trd>
    <trd lng="it">uccello</trd>
    <trdgrp lng="he">
      <trd>&tsadi;&yod;&pe;&vav;&resh;</trd>,
      <trd>&ayin;&vav;&fpe;</trd>
    </trdgrp>
    <trd lng="hu">mad&aacute;r</trd>
    <trd lng="pl">ptak</trd>
    <trdgrp lng="pt">
      <trd>ave</trd>,
      <trd>p&aacute;ssaro</trd>
    </trdgrp>
    <trd lng="tr">ku&scedil;</trd>
  </snc>
  <snc mrk="bird.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ridinde afekta homo:
      <ekz>
        bela birdo
        <klr>(dando)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">gandin</trd>
    <trd lng="hu">piperk&odblac;c</trd>
    <trdgrp lng="pl">
      <trd>ptak</trd>,
      <trd>ptaszek</trd>,
      <trd>gogu&sacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="cs">pták</trd>
  <trd lng="de">Vogel</trd>
  <trd lng="id">burung</trd>
  <trd lng="nl">vogel</trd>
  <trd lng="no">fugl</trd>
  <trd lng="oc">auc&egrave;l</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">vták</trd>
  <trd lng="tp">waso</trd>
  <trd lng="zh">鸟 [<ind>niǎo</ind>]</trd>
</drv>

<drv mrk="bird.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="bird.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kovritaj de <ref tip="prt" cel="plum.0o">plumoj</ref>, kun
      du piedoj, <ref tip="prt" cel="flug.0ilo">flugiloj</ref>,
      naskantaj
      <ref cel="ov.0o">ovojn</ref>, el <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">klaso</ref> de <ref tip="malprt"
      cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Aves</trd>), solaj plu vivantaj
      <ref tip="malprt" cel="dinosa.0oj">dinosa&ubreve;roj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_ib; 
<klr>(&c_k;&c_l;&c_ja;&c_s;&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;)</klr></trd>
  <trd lng="de">V&ouml;gel</trd>
  <trd lng="en">birds</trd>
  <trd lng="es">aves</trd>
  <trd lng="fr">oiseaux</trd>
  <trd lng="hu">madarak<klr tip="amb"> (oszt&aacute;lya)</klr></trd>
  <trd lng="no">fugler</trd>
  <trd lng="pl">ptaki</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> 
(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="tr">ku&scedil;lar</trd>
</drv>

<drv mrk="bird.korto0o">
  <kap>korto<tld/>o</kap>
  <snc mrk="bird.korto0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref cel="bird.0o" tip="super"><tld lit="B"/>o</ref> bredata en 
      <ref tip="vid" cel="kort.birdo0o.ARKI">
      <tld/>okorto</ref> pro siaj viando a&ubreve;
      <ref cel="ov.0o">ovoj</ref>:
      <ekz>
        <ref tip="sub" cel="anas.0o">anaso</ref>,
        <ref tip="sub" cel="kok.0o">koko</ref> kaj 
        <ref tip="sub" cel="meleag.0o">meleagro</ref> estas
        korto<tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; 
<ind>&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</ind></trd>
  <trd lng="de">Gefl&uuml;gel</trd>
  <trdgrp lng="en">
    <trd>farm bird</trd>,
    <trd>domestic fowl</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oiseau de basse-cour</trd>,
    <trd>volaille</trd>
  </trdgrp>
  <trd lng="hu">baromfi</trd>
  <trd lng="pl">dr&oacute;b</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ja;&c_ja; 
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="bird.musx0o">
  <kap>mu&scirc;<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bird.musx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="kolibr.0o.ZOO">kolibro</ref>
    <ekz>
      Nel estis ravita precipe pro la vido de paradizaj mu&scirc;<tld/>etoj
      <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url 
      ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
      praarbaro</url></vrk>, <lok>1978</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_y;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="ca">colibr&iacute;</trd>
  <trd lng="de">Kolibri</trd>
  <trd lng="en">humming bird</trd>
  <trd lng="es">colibr&iacute;</trd>
  <trdgrp lng="fr">
    <trd>colibri</trd>,
    <trd>oiseau-mouche</trd>
  </trdgrp>
  <trd lng="hu">kolibri</trd>
  <trd lng="nl">kolibrie</trd>
  <trd lng="pl">koliber</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="tr">sinekku&scedil;u</trd>
</drv>

<drv mrk="bird.nagxo0o">
  <kap>na&gcirc;o<tld/>o, <var><kap>na&gcirc;<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="bird.nagxo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref cel="bird.0o" tip="super"><tld lit="B"/>o</ref> havanta
      na&gcirc;okapablon:
      <ekz>
        <ref tip="sub" cel="anas.0o">anaso</ref>, 
        <ref tip="sub" cel="cign.0o">cigno</ref>, 
        <ref tip="sub" cel="mev.0o">mevo</ref> estas na&gcirc;o<tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_p;&c_l;&c_a;&c_w;&c_n;&c_a;&c_ja; 
&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="br">evn palvezek</trd>
  <trd lng="de">Schwimmvogel</trd>
  <trd lng="fr">oiseau d'eau</trd>
  <trd lng="hu">&uacute;sz&oacute;mad&aacute;r</trd>
  <trd lng="nl">zwemvogel</trd>
  <trd 
lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_shch;&c_a;&c_ja; 
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">su ku&scedil;u</trd>
</drv>

<drv mrk="bird.rabo0oj">
  <kap>rabo<tld/>oj, <var><kap>rab<tld/>oj</kap></var></kap>
  <snc mrk="bird.rabo0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="bird.0oj"><tld lit="B"/>oj</ref> kun akra
      beko kaj ungegoj, tage &ccirc;asantaj el la ordoj 
      <trd lng="la">Accipitriformes</trd> kaj <trd lng="la">
      Falconiformes</trd>, foje anka&ubreve; la nokte &ccirc;asantaj el la 
ordo <trd
      lng="la">Strigiformes</trd>.
    </dif>
  </snc>
  <trd 
lng="be">&c_s;&c_o;&c_k;&c_a;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
    <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr>
  </trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_a; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a; 
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>evned-preizh</trd>,
    <trd>laboused-preizh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Taggreife</trd>,
    <trd>Greifv&ouml;gel</trd>,
    <trd>Raubv&ouml;gel</trd>
  </trdgrp>
  <trd lng="en">birds of prey</trd>
  <trdgrp lng="es">
    <trd>rapaces</trd>,
    <trd>aves de presa</trd>,
    <trd>rapaz <klr tip="amb">(ave)</klr></trd>
  </trdgrp> 
  <trdgrp lng="fr">
    <trd>oiseaux de proie</trd>,
    <trd>rapaces<klr> (oiseaux)</klr></trd>
  </trdgrp>
  <trd lng="he">&ayin;&vav;&fpe; &dalet;&vav;&resh;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>s&oacute;lyomalak&uacute;ak</trd>,
    <trd>ragad&oacute;z&oacute;madarak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_y;&c_je; &c_p;&c_t;&c_i;&c_c;&c_y;<klr 
tip="ind">
    (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_je; 
&c_h;&c_i;&c_shch;&c_n;&c_y;&c_je; &c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb">
    (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="pl"><ind>ptaki</ind> drapie&zdot;ne</trd>
</drv>

<drv mrk="bird.vad0oj">
  <kap>vad<tld/>oj, <var><kap>vado<tld/>oj</kap></var></kap>
  <snc mrk="bird.vad0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="B"/>oj kun plejofte 
      longaj kruroj kaj bekoj, kiuj ofte
      vadas en akvo, <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">ordo</ref> 
      el la klaso <ref tip="malprt"
      cel="bird.0oj"><tld/>oj</ref> (<trd lng="la">Ciconiiformes</trd>):
      <ekz>
        la longaj piedfingroj de vad<tld/>oj estas formitaj
        por piediri sur mar&ccirc;oj kaj flosantaj plantoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_b;&c_u;&c_s;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;<klr 
tip="amb">
  (&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="br">hirc'hareged</trd>
  <trdgrp lng="cs">
    <trd>brodiví</trd>,
    <trd>brodivý pták</trd>
  </trdgrp>
  <trd lng="de">Schreitv&ouml;gel</trd>
  <trd lng="en">wading <ind>birds</ind></trd>
  <trd lng="fr">&eacute;chassiers</trd>
  <trdgrp lng="hu">
    <trd>g&oacute;lyaalak&uacute;ak</trd>,
    <trd>g&aacute;zl&oacute;madarak</trd>
  </trdgrp>
  <trd lng="pl"><ind>ptaki</ind> w&eogonek;drowne</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_i;&c_s;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
    <klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_g;&c_o;&c_l;&c_je;&c_n;&c_a;&c_s;&c_t;&c_y;&c_je;
    <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="sk">brodivé vtáky</trd>
  <trd lng="tr">uzun bacakl&inodot; ku&scedil;lar</trd>
</drv>
</art>
