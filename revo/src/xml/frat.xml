<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frat.xml,v 1.43 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frat</rad>/o
</kap>

<drv mrk="frat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frat.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Virseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>
      de samaj gepatroj, rilate al samdevenaj idoj:
      <ekz>
        miaj <tld/>oj havis hodia&ubreve; gastojn
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mensogo kaj &scirc;telo estas du <tld/>oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>fiv&eacute;r</trd>,
      <trd>fi&uacute;testv&eacute;r</trd>
    </trdgrp>
    <trd lng="nl">broer</trd>
  <trdgrp lng="pt">
    <trd>irm&atilde;o</trd>,
    <trd>mano</trd>
  </trdgrp>
    <trd lng="de">Bruder</trd>
  </snc>
  <snc mrk="frat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Persono, kiun oni amas kiel <tld/>on, kaj kiu reciprokas tian
amon:
      <ekz>
        amiko fari&gcirc;as <tld/>o en mizero
        <fnt><bib>MT</bib>, <lok>&Sen; 17:17</lok></fnt>;
      </ekz>
      <ekz>
        <ind>milit<tld/>o</ind>.
        <trd lng="fr">fr&egrave;re d'armes</trd>
        <trd lng="hu">fegyvert&aacute;rs</trd>
        <trd lng="nl">wapenbroeder</trd>
        <trd lng="de">Waffenbruder</trd>
      </ekz>
    </dif>
    <trd lng="hu">testv&eacute;r</trd>
    <trd lng="nl">broeder</trd>
  <trdgrp lng="pt">
    <trd>irm&atilde;o</trd>,
    <trd>mano</trd>
  </trdgrp>
    <trd lng="de">Bruder</trd>
  </snc>
  <snc mrk="frat.0o.religia">
    <dif>
      Titolo de ano en kelkaj religiaj ordenoj kaj en kelkaj societoj.
    </dif>
    <trdgrp lng="hu">
      <trd>bar&aacute;t</trd>,
      <trd>testv&eacute;r</trd>
    </trdgrp>
    <trd lng="nl">broeder</trd>
    <trd lng="de">Bruder</trd>
  <trdgrp lng="pt">
    <trd>irm&atilde;o</trd>,
    <trd>frei</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>bratr</trd>,
    <trd>fráter</trd>,
    <trd>konvrš</trd>,
    <trd>mnich laik</trd>
  </trdgrp>
  <trd lng="de">Bruder</trd>
  <trd lng="en">brother</trd>
  <trd lng="es">hermano</trd>
  <trd lng="fr">fr&egrave;re</trd>
  <trd lng="he">&alef;&het;</trd>
  <trd lng="pl">brat</trd>
  <trd lng="pt">irm&atilde;o</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">brat <klr>(v spoločenstve)</klr></trd>
  <trd lng="sv">bror</trd>
  <trd lng="tr">erkek karde&scedil;</trd>
  <trd lng="vo">blod</trd>
</drv>

<drv mrk="frat.0a">
  <kap><tld/>a</kap>
  <snc mrk="frat.0a.KOMUNE">
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a amo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_t;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">bratrský</trd>
  <trd lng="de">br&uuml;derlich</trd>
  <trd lng="es">fraterno</trd>
  <trd lng="fr">fraternel</trd>
  <trd lng="hu">testv&eacute;ri</trd>
  <trd lng="nl">broederlijk</trd>
  <trd lng="pl">braterski</trd>
  <trdgrp lng="pt">
    <trd>fraterno</trd>,
    <trd>fraternal</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">bratský</trd>
</drv>

<drv mrk="frat.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc mrk="frat.0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Inseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>
      de samaj gepatroj, rilate al samdevenaj idoj:
      <ekz>
        mia <tld/>ino estas tre bela knabino
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schwester</trd>
    <trdgrp lng="hu">
      <trd>l&aacute;nytestv&eacute;r</trd>,
      <trd>n&odblac;testv&eacute;r</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>irm&atilde;</trd>,
    <trd>mana</trd>
  </trdgrp>
  </snc>
  <snc mrk="frat.0ino.religia">
    <dif>
      Titolo de anino en kelkaj religiaj ordenoj kaj en kelkaj societoj.
    </dif>
    <trd lng="de">Schwester</trd>
    <trd lng="hu">n&odblac;v&eacute;r</trd>
    <trd lng="pt">irm&atilde;</trd>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="bg">&c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="cs">sestra</trd>
  <trd lng="de">Schwester</trd>
  <trd lng="en">sister</trd>
  <trd lng="es">hermana</trd>
  <trd lng="fr">s&oelig;ur</trd>
  <trd lng="he">&alef;&het;&vav;&tav;</trd>
  <trd lng="nl">zuster</trd>
  <trd lng="pl">siostra</trd>
  <trd lng="ru">&c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="sk">sestra</trd>
  <trd lng="sv">syster</trd>
  <trd lng="tr">k&inodot;z karde&scedil;</trd>
  <trd lng="vo">jiblod</trd>
</drv>

<drv mrk="frat.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="frat.0eco.GEN">
    <dif>
      Parenceco inter <tld/>oj.
    </dif>
    <trd lng="pl">braterstwo</trd>
  </snc>
  <snc mrk="frat.0eco.sento">
    <dif>
      Ama, helpema, kvaza&ubreve;<tld/>a sento al iu, a&ubreve;
      &gcirc;enerale al la tuta homaro; homamo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="cs">bratrství</trd>
  <trd lng="de">Br&uuml;derlichkeit</trd>
  <trd lng="es">fraternidad</trd>
  <trd lng="fr">fraternit&eacute;</trd>
  <trd lng="he">&alef;&het;&vav;&vav;&he;</trd>
  <trd lng="hu">testv&eacute;ris&eacute;g</trd>
  <trd lng="nl">broederschap</trd>
  <trdgrp lng="pl">
    <trd>braterstwo</trd>,
    <trd>bruderszaft</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">fraternita</trd>
  <trd lng="tr">karde&scedil;lik</trd>
</drv>

<drv mrk="frat.bo0o">
  <kap><ofc>*</ofc>bo<tld/>o</kap>
  <snc mrk="frat.bo0o.edzinafrato">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="frat.0o" lst="voko:parencoj"><tld
      lit="F"/>o</ref> de la edzino rilate al la edzo:
      <ekz>
        &scirc;ia <tld/>o estas mia bo<tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_v;&c_a;&c_g;&c_je;&c_r;</trd>,
      <trd>&c_sh;&c_u;&c_r;&c_y;&c_n;</trd>
    </trdgrp>
    <trd lng="es">cu&ntilde;ado</trd>
    <trd lng="pl">szwagier</trd>
    <trd lng="ru">&c_sh;&c_u;&c_r;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="frat.bo0o.edzafrato">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o de la edzo rilate al la edzino.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_je;&c_v;&c_je;&c_r;</trd>
    <trd lng="es">cu&ntilde;ado</trd>
    <trd lng="ru">&c_d;&c_je;&c_v;&c_je;&c_r;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.fratinaedzo">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzo de <tld/>ino rilate al &scirc;iaj ge<tld/>oj.
    </dif>
    <trd lng="be">&c_z;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="es">cu&ntilde;ado</trd>
    <trd lng="ru">&c_z;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.bofratinaedzo">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzo de bo<tld/>ino:
    </dif>
    <subsnc mrk="frat.bo0o.edzinfratinaedzo">
      <dif>
        La edzo de <tld/>ino de la edzino:
        <ekz>
          inter si la edzoj de fratinoj estas bo<tld/>oj.
        </ekz>
      </dif>
      <trd lng="ru">&c_s;&c_v;&c_o;&c_ja;&c_k;</trd>
    </subsnc>
    <subsnc mrk="frat.bo0o.edzofratinaedzo">
      <dif>
        La edzo de <tld/>ino de la edzo.
      </dif>
      <trd lng="ru">&c_z;&c_ja;&c_t;&c_mol;</trd>
    </subsnc>
    <trd lng="es">concu&ntilde;o</trd>
  </snc>
  <trd lng="cs">švagr</trd>
  <trd lng="de">Schwager</trd>
  <trd lng="en">brother-in-law</trd>
  <trd lng="fr">beau-fr&egrave;re</trd>
  <trd lng="hu">s&oacute;gor</trd>
  <trd lng="nl">zwager</trd>
  <trd lng="pl">szwagier</trd>
  <trd lng="sk">švagor</trd>
</drv>

<drv mrk="frat.bo0ino">
  <kap><ofc>*</ofc>bo<tld/>ino</kap>
  <snc mrk="frat.bo0ino.edzinafratino">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="frat.0ino" lst="voko:parencoj"><tld
      lit="F"/>ino</ref> de la edzino rilate al la edzo:
      <ekz>
        &ccirc;iuj parencoj de mia edzino estas miaj boparencoj, sekve
        <klr>...</klr> &scirc;ia <tld/>ino estas mia bo<tld/>ino
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_v;&c_a;&c_g;&c_je;&c_r;&c_k;&c_a;</trd>
    <trd lng="es">cu&ntilde;ada</trd>
    <trd lng="ru">&c_s;&c_v;&c_o;&c_ja;&c_ch;&c_je;&c_n;&c_i;&c_c;&c_a;
    </trd>
    <trd lng="de">Schw&auml;gerin</trd>
  </snc>
  <snc mrk="frat.bo0ino.edzafratino">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>ino de la edzo rilate al la edzino.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_l;&c_v;&c_ib;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="es">cu&ntilde;ada</trd>
    <trd lng="de">Schw&auml;gerin</trd>
    <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="frat.bo0ino.frataedzino">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzino de <tld/>o rilate al liaj ge<tld/>oj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_ja;</trd>,
      <trd>&c_n;&c_ja;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="es">cu&ntilde;ada</trd>
    <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="frat.bo0ino.bofrataedzino">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzino de
      <ref cel="frat.bo0o.edzinafrato">bo<tld/>o<sncref/></ref>:
      <ekz>
        inter si la edzinoj de <tld/>oj estas bo<tld/>inoj.
      </ekz>
    </dif>
    <trd lng="be">&c_ja;&c_t;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
    <trd lng="es">concu&ntilde;a</trd>
    <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
    <trd lng="de">Schw&auml;gerin</trd>
  </snc>
  <trd lng="cs">švagrová</trd>
  <trd lng="de">Schw&auml;gerin</trd>
  <trd lng="en">sister-in-law</trd>
  <trd lng="fr">belle-s&oelig;ur</trd>
  <trd lng="hu">s&oacute;gorn&odblac;</trd>
  <trd lng="nl">schoonzuster</trd>
  <trd lng="pl">szwagierka</trd>
  <trd lng="sk">švagriná</trd>
</drv>

<drv mrk="frat.boge0oj">
  <kap><ofc>*</ofc>boge<tld/>oj</kap>
  <snc mrk="frat.boge0oj.edzingefratoj">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="frat.ge0oj"
      lst="voko:parencoj">Ge<tld/>oj</ref> de la edzino rilate al la
      edzo:
      <ekz>
        mia <tld/>o kaj <tld/>ino <klr>...</klr> estas la boge<tld/>oj
        de mia edzino
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frat.boge0oj.edzogefratoj">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Ge<tld/>oj de la edzo rilate al la edzino.
    </dif>
  </snc>
  <trd lng="cs">sourozenci manželky</trd>
  <trd lng="fr">beaux-fr&egrave;res et belles-s&oelig;rs</trd>
  <trd lng="hu">s&oacute;gorok &eacute;s s&oacute;gorn&odblac;k</trd>
  <trd lng="pl">szwagrostwo</trd>
  <trd lng="sk">súrodenci manželky</trd>
  <trd lng="tr">&uuml;vey karde&scedil;ler</trd>
</drv>

<drv mrk="frat.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="frat.duon0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Virseksa<ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>
      nur de sama patro a&ubreve; nur de sama patrino,
      rilate al la sampatraj a&ubreve; sampatrinaj idoj.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_o;&c_d;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t;
  </trd>
  <trd lng="cs">nevlastní bratr</trd>
  <trd lng="de">Halbbruder</trd>
  <trd lng="es">medio <ind>hermano</ind></trd>
  <trd lng="fr">demi-fr&egrave;re</trd>
  <trd lng="he">&alef;&het; &het;&vav;&resh;&gimel;</trd>
  <trd lng="hu">f&eacute;ltestv&eacute;r</trd>
  <trd lng="nl">halfbroer</trd>
  <trdgrp lng="pl">
    <trd>przyrodni <ind>brat</ind></trd>,
    <trd>przyszywany <ind>brat</ind><klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;
  </trd>
  <trd lng="sk">nevlastný brat</trd>
  <trd lng="sv">halvbror</trd>
</drv>

<drv mrk="frat.ge0oj">
  <kap><ofc>*</ofc>ge<tld/>oj</kap>
  <snc mrk="frat.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Amba&ubreve;seksaj <ref tip="lst" cel="id.0o"
      lst="voko:parencoj">idoj</ref> de samaj gepatroj:
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj)
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bratři a sestry</trd>,
    <trd>sourozenci obojího pohlaví</trd>
  </trdgrp>
  <trd lng="en">siblings</trd>
  <trd lng="es">hermanos y hermanas</trd>
  <trd lng="fr">fr&egrave;res et s&oelig;urs</trd>
  <trd lng="hu">testv&eacute;rek <klr tip="amb">(fiv&eacute;r/ek
  &eacute;s n&odblac;v&eacute;r/ek)</klr></trd>
  <trd lng="pl">rodze&nacute;stwo</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_t; &c_i; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;
    </trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol;&c_ja; &c_i;
    &c_s;&c_jo;&c_s;&c_t;&c_r;&c_y;
    </trd>
  </trdgrp>
  <trd lng="sk">bratia a sestry</trd>
  <trd lng="tr">karde&scedil;ler</trd>
</drv>

<drv mrk="frat.inter0igxi">
  <kap>inter<tld/>i&gcirc;i</kap>
  <snc mrk="frat.inter0igxi.KOMUNE">
    <dif>
      Interrilati kvaza&ubreve;<tld/>e, kun <tld/>aj sentoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_b;&c_r;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">fraterniser</trd>
  <trd lng="es">fraternizar</trd>
  <trd lng="hu">&ouml;sszebar&aacute;tkozik</trd>
  <trd lng="nl">verbroederen</trd>
  <trd lng="pl">brata&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_r;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    </trd>
  </trdgrp>
</drv>

<drv mrk="frat.lakto0o">
  <kap>lakto<tld/>o</kap>
  <snc mrk="frat.lakto0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Ido mamnutrita de fremda patrino, rilate al &scirc;iaj propraj 
      idoj:
