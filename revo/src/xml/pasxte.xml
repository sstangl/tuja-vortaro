<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pasxte.xml,v 1.14 2005/04/28 16:35:19 revo Exp $">
<kap>
<ofc>4</ofc>
<rad>pa&scirc;tel</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="pasxte.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasxte.0o.ilo">
    <dif>
      Krajono farita el pulvorigita kolorilo kaj gumo.
    </dif>
    <trd lng="de">Pastellstift</trd>
  </snc>
  <snc mrk="pasxte.0o.ajxo">
    <ref tip="dif" cel="pasxte.0ajxo">pa&scirc;tela&jcirc;o</ref>
    <trd lng="de">Pastellgem&auml;lde</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="ca">pastel</trd>
  <trd lng="en">pastel</trd>
  <trd lng="es">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trd lng="hu">pasztell</trd>
  <trd lng="nl">pastel</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="pasxte.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Bildo farita per pa&scirc;teloj.
    </dif>
    <refgrp tip="sin">
      <ref cel="pasxte.0o.ajxo">pa&scirc;telo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="de">Pastellgem&auml;lde</trd>
  <trd lng="en">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pasztell</trd>,
    <trd>kr&eacute;tarajz</trd>
  </trdgrp>
  <trd lng="nl">pastel</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
