<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kerosen.xml,v 1.11 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kerosen</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kerosen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kerosen.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="petrol.0o">Petrola</ref> frakcio, akirata
      per distilado de <ref tip="malprt" cel="naft.0o">nafto</ref>
      &ccirc;e la temperaturoj inter 151&ring;C kaj 273&ring;C; uzata por lumigado
      (en petrollampoj), sed nun precipe kiel la &ccirc;efa brula&jcirc;o de la
      <ref tip="vid" cel="reakci.0o.TEK">reakciaj</ref> motoroj:
      <ekz>
        brilis <klr>...</klr> granda <tld/>a lampo kaj kvar palacaj lanternoj
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 13a, p. 146a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kerosen</trd>,
    <trd>lampový petrolej</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brenn&ouml;l</trd>,
    <trd>Petroleum</trd>,
    <trd>Kerosin</trd>
  </trdgrp>
  <trd lng="en">kerosene</trd>
  <trd lng="fr">k&eacute;ros&egrave;ne</trd>
  <trd lng="hu">kerozin</trd>
  <trd lng="nl">kerosine</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_o;&c_s;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>kerosén</trd>,
    <trd>lampový olej</trd>
  </trdgrp>
</drv>
</art>
