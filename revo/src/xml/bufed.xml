<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bufed.xml,v 1.22 2015/10/06 12:10:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bufed</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bufed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bufed.0o.MEBL">
    <dif>
      Meblo provizita per man&gcirc;a&jcirc;oj disponeblaj por invititoj
      a&ubreve; klientoj &ccirc;e teatro, balo, stacidomo.
    </dif>   
    <trd lng="br">taoliad veuzio&ugrave;</trd>
    <trdgrp lng="de">
      <trd>B&uuml;ffet</trd>,
      <trd>Anrichte</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>b&uuml;f&eacute;</trd>,
      <trd>t&aacute;lal&oacute;asztal</trd>
    </trdgrp>
    <trd lng="nl">buffet</trd>
    <trdgrp lng="it">
      <trd>buff&egrave;</trd>,
      <trd>buffet</trd>,
      <trd>banco<klr> (di bar)</klr></trd>,
      <trd>bancone<klr> (di bar)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bufet</trd>,
      <trd>lada</trd>,
      <trd>bar</trd>,
      <trd>kredens</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>aparador</trd>,
      <trd>bif&ecirc;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bufed.0o.ejo">
    <dif>
      Ejo, kie staras tiu instala&jcirc;o.  
    </dif>
    <trd lng="nl">buffet</trd>
    <trdgrp lng="pl">
      <trd>bufet</trd>,
      <trd>bar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_f;&c_e;&c_t;</trd>
  <trd lng="ca">bufet</trd>
  <trdgrp lng="cs">
    <trd>bufet</trd>,
    <trd>st&uring;l s ob&ccaron;erstven&iacute;m</trd>
  </trdgrp>
  <trd lng="en">buffet</trd>
  <trd lng="es">buffet</trd>
  <trd lng="fr">buffet</trd>
  <trd lng="hu">b&uuml;f&eacute;</trd>
  <trdgrp lng="it">
    <trd>buvette</trd>,
    <trd>buff&egrave;<klr> (luogo dove...)</klr></trd>,
    <trd>buffet<klr> (luogo dove...)</klr></trd>, 
    <trd>caff&egrave;<klr tip="amb"> (luogo di ristoro)</klr></trd>,
    <trd>bar</trd>
  </trdgrp>
  <trd lng="ro">bufet</trd>
  <trd lng="ru">&c_b;&c_u;&c_f;&c_je;&c_t;</trd>
  <trd lng="sk">bufet</trd>
  <trd lng="sv">buff&eacute;</trd>
</drv>
</art>
