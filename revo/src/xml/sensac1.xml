<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sensac1.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sensaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sensac1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sensac1.0o.baz">
    <dif>
      Forta impreso sur publikon:
      <ekz>
        estis <tld/>o, kiam, post prilukto de grandegaj malfacila&jcirc;oj,
        en
        la E&ubreve;ropa Centro de Nuklea Esploro CERN, &Gcirc;enevo, en 1995 oni
        sukcesis kunmeti kompletan antiatomon
        <fnt><aut>OH</aut>, <vrk>&Ccirc;u
        antimaterio en la universo?</vrk>, <bib>Monato</bib>, <lok>jaro
        2001a, numero 2a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="impon.0eco">imponeco</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_e;&c_n;&c_s;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="de">Sensation</trd>
    <trdgrp lng="en">
      <trd>sensation</trd>,
      <trd>impression</trd>,
      <trd>commotion</trd>,
      <trd>interest</trd>,
      <trd>furor</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>sensation<klr> (effet spectaculaire)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szenz&aacute;ci&oacute;</trd>,
      <trd>felt&udblac;n&eacute;s</trd>
    </trdgrp>
    <trd lng="it">sensazione<klr> (effetto spettacolare)</klr></trd>
    <trd lng="nl">sensatie</trd>
    <trdgrp lng="pl">
      <trd>sensacja</trd>,
      <trd>wra&zdot;enie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_je;&c_n;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="sensac1.0o.fil">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Spirita impreso ricevita per iu el la kvin sentumoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="sensac.0o">sensaco</ref>
    </refgrp>
    <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="en">
      <trd>perception</trd>,
      <trd>sensation</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>perception</trd>,
      <trd>sensation</trd>
    </trdgrp>
    <trd lng="hu">&eacute;rzet</trd>
    <trdgrp lng="it">
      <trd>percezione</trd>,
      <trd>sensazione<klr> (percezione)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>sensacja</trd>,
      <trd>wra&zdot;enie</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>senzace</trd>,
    <trd>senzační věc</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Empfindung</trd>,
    <trd>Sinneswahrnehmung</trd>
  </trdgrp>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  <trdgrp lng="sk">
    <trd>senzačná vec</trd>,
    <trd>senzácia</trd>
  </trdgrp>
</drv>

<drv mrk="sensac1.0a">
  <kap><tld/>a</kap>
  <snc mrk="sensac1.0a.KOMUNE">
    <dif>
      Tre impresa, fama a&ubreve; fami&gcirc;anta &ccirc;e multaj homoj:
      <ekz>
        la ordinara homo legos <ctl>Re&gcirc;on Edipon</ctl>
        ne pro tio, ke &gcirc;i estas antikva kaj iom historia
        teksto, sed, &gcirc;uste male, pro tio, ke &gcirc;i estas
        relative <tld/>a kaj moderna intrigo
        <fnt>
          <aut>K. Pi&ccirc;</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr65/grekaj.html">
          Du grekaj dramoj</url></vrk>,
          <lok>Nica Literatura Revuo, 1961-05 &gcirc;is 06,
          numero 6/5, p. 195-200a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">senzační</trd>
  <trdgrp lng="fr">
    <trd>sensationnel</trd>,
    <trd>spectaculaire</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sensazionale</trd>,
    <trd>spettacolare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sensacyjny</trd>,
    <trd>spektakularny</trd>
  </trdgrp>
  <trd lng="sk">senzačný</trd>
</drv>
</art>
