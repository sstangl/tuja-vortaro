<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: filogenez.xml,v 1.6 2015/10/05 14:10:20 revo Exp $">
<kap>
  <rad>filogenez</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="filogenez.0o">
  <kap><tld/>o</kap>
  <snc mrk="filogenez.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Esti&gcirc;o de specio a&ubreve; pli vasta taksono
      kaj teorio pri tiu esti&gcirc;o;
      <ref tip="sin" cel="filogeni.0o.BIO">filogenio</ref>:
      <ekz>
        en <tld/>o de la birdoj estas <klr>...</klr> multe da neklara&jcirc;oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Portalo:Birdoj">
          Portalo:Birdoj</url>, 2010-08-17</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ontogenez.0o.BIO">ontogenezo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>filog&egrave;nesi <klr>(proc&eacute;s)</klr></trd>,
    <trd>filog&egrave;nia <klr>(ci&egrave;ncia)</klr></trd>
  </trdgrp>
  <trd lng="cs">fylogeneze</trd>
  <trdgrp lng="en">
    <trd>phylogeny</trd>,
    <trd>phylogenesis</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>phylog&eacute;n&egrave;se</trd>,
    <trd>phylog&eacute;nie</trd>
  </trdgrp>
  <trd lng="hu">filogenezis</trd>
  <trd lng="sk">fylogen&eacute;za</trd>
</drv>
</art>

