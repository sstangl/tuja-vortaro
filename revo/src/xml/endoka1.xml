<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: endoka1.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>endokarp</rad>/o
</kap>
<drv mrk="endoka1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Plej interna parto de la <ref tip="malprt"
      cel="perika1.0o">perikarpo</ref>, membrano, kiu envolvas la
      grajnojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="epikar.0o">epikarpo</ref>,
      <ref cel="mezoka.0o">mezokarpo</ref>
    </refgrp>
  </snc>
  <trd lng="ca">endocarpi</trd>
  <trd lng="cs">oplodí vnitřní</trd>
  <trdgrp lng="de">
    <trd>Endokarp<klr> [fak]</klr></trd>,
    <trd>innenerste Schicht der Fruchtwand</trd>
  </trdgrp>
  <trd lng="en">endocarp</trd>
  <trd lng="es">endocarpio</trd>
  <trd lng="fr">endocarpe</trd>
  <trd lng="hu">bels&odblac; <ind>term&eacute;sfal</ind></trd>
  <trd lng="pl">&lstroke;uska <ind>nasienna</ind></trd>
  <trd lng="pt">endocarpo</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_k;&c_a;&c_r;&c_p;&c_i;&c_j;</trd>
  <trd lng="sk">oplodie vnútorné</trd>
</drv>
</art>
