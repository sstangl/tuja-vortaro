<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: svat.xml,v 1.20 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>svat</rad>/i
</kap>
<drv mrk="svat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="svat.0i.mem">
    <dif>
      Sin proponi al iu kiel edzon a&ubreve; edzinon, peti por si mem la manon
      de
      iu:
      <ekz>
        li intencas <tld/>i la filinon de la kolonelo;
      </ekz>
      <ekz>
        <tld/>koketi junulinon.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_v;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="it">proporsi<klr> (di matrimonio)</klr></trd>
    <trd lng="nl">een <ind>huwelijkaanzoek</ind> doen</trd>
    <trd lng="pl">o&sacute;wiadcza&cacute; si&eogonek;</trd>
    <trd lng="ru">&c_s;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="svat.0i.peri">
    <dif>
      Proponi iun al alia persono kiel edzon a&ubreve; edzinon; peti kiel
      peranto
      de iu alia la manon de iu.
    </dif>
    <trd lng="be">&c_s;&c_v;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="nl">koppelen<klr tip="ind"> (v.mensen)</klr></trd>
    <trd lng="it">combinare<klr> (matrimonio)</klr></trd>
    <trd lng="pl">swata&cacute;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>dohazovat</trd>,
    <trd>zprostředkovat manželství</trd>,
    <trd>zprostředkovávat svatbu</trd>
  </trdgrp>
  <trd lng="en">match</trd>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">demander en <ind>mariage</ind></trd>
  <trd lng="hu">megk&eacute;ri a kez&eacute;t <klr tip="amb">(maga &eacute;s m&aacute;s
  sz&aacute;m&aacute;ra)</klr></trd>
  <trd lng="it">chiedere in <ind>matrimonio</ind></trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dohadzovať</trd>,
    <trd>nahovárať <klr>(na manželstvo)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="svat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Propono pri edzi&gcirc;o a&ubreve; edzini&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>námluvy</trd>,
    <trd>sňatkové zprostředkování</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y;</trd>
  <trd lng="it">offerta di <ind>matrimonio</ind></trd>
  <trd lng="pl">swaty</trd>
  <trdgrp lng="sk">
    <trd>nahovárky</trd>,
    <trd>sobášne sprostredkovanie</trd>
  </trdgrp>
</drv>

<drv mrk="svat.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu <tld/>is virinon.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_t;</trd>
  <trd lng="cs">nápadník</trd>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;</trd>
  <trd lng="fr">pr&eacute;tendant<klr> (au mariage)</klr></trd>
  <trd lng="hu">le&aacute;nyk&eacute;r&odblac;</trd>
  <trd lng="it">pretendente<klr> (matrimonio)</klr></trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_t;</trd>
  <trd lng="sk">nápadník</trd>
</drv>

<drv mrk="svat.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies profesio a&ubreve; okupo estas proponi aliulojn por edzi&gcirc;o:
      <ekz>
        la oficiala <ind><tld/>istino</ind> venis kun horoskopaj kartoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 72a, p. 530a</lok></fnt>
      <trdgrp lng="it">
        <trd>mezzana</trd>, 
        <trd>ruffiana</trd>
       </trdgrp>
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_h;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dohazovač</trd>,
    <trd>družba</trd>,
    <trd>zprostředkovatel manželství</trd>,
    <trd>zprostředkovatel sňatků</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heiratsvermittler</trd>,
    <trd>Kuppler</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_l;&a_A;&a_l; &a_A;&a_z;&a_d;&a_w;&a_A;&a_j;</trd>
  <trdgrp lng="fr">
    <trd>marieur</trd>,
    <trd>marieuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&aacute;zass&aacute;gszerz&odblac;</trd>,
    <trd>h&aacute;zass&aacute;gk&ouml;zvet&iacute;t&odblac;</trd>,
    <trd>t&aacute;rskeres&odblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
        <trd>mezzano</trd>, 
        <trd>ruffiano</trd>
       </trdgrp>
  <trdgrp lng="nl">
    <trd>huwelijksmakelaar</trd>,
    <trd>koppelaar</trd>
  </trdgrp>
  <trd lng="pl">swat</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_h;&c_a;</trd>
  <trd lng="sk">pytač <klr>(kto pýta za ženu pre druhého)</klr></trd>
</drv>

<drv mrk="svat.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="svat.0i.mem"><tld/>i <sncref/></ref>
      <ekz>
        <tld/>i&gcirc;i pri iu fra&ubreve;lino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        venis nova <tld/>i&gcirc;anto je &scirc;i<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dohazovat</trd>,
    <trd>zprostředkovat manželství</trd>,
    <trd>zprostředkovávat svatbu</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">demander en <ind>mariage</ind></trd>
  <trd lng="hu">megk&eacute;r&odblac; a kez&eacute;t <klr tip="amb">(maga
  sz&aacute;m&aacute;ra)</klr></trd>
  <trd lng="it">proporsi<klr> (matrimonio)</klr></trd>
  <trd lng="pl">o&sacute;wiadcza&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>dohadzovať</trd>,
    <trd>nahovárať <klr>(na manželstvo)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="svat.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Formala sinpropono kiel edzo a&ubreve; edzino:
      <ekz>
        li fine eldiris al &scirc;i sian <tld/>i&gcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        centoj da re&gcirc;idinoj akceptus lian <tld/>i&gcirc;on kun
        &gcirc;ojo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">demande en <ind>mariage</ind></trd>
  <trd lng="hu">le&aacute;nyk&eacute;r&eacute;s</trd>
  <trd lng="it">proposta<klr> di matrimonio</klr></trd>
  <trd lng="pl">o&sacute;wiadczyny</trd>
</drv>
</art>
