<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: papag.xml,v 1.24 2015/07/04 08:37:41 revo Exp $">
<kap><ofc>*</ofc><rad>papag</rad>/o</kap>
<drv mrk="papag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="papag.0o.komune">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <tld/>edoj.
    </dif>
  </snc>
  <snc mrk="papag.0o.zoo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="papage.0oj"><tld/>edoj</ref>
      (<trd lng="la">Psittacus</trd>), tropika grimpobirdo,
      kapabla imiti la homan vo&ccirc;on kaj &ccirc;iuspecajn kriojn.
    </dif>
  </snc>
  <snc mrk="papag.0o.ripetulo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo, kiu senkomprene ripetas instruon:
      <ekz>
        <tld/>e ripeti gurditan lecionon;
      </ekz>
      <ekz>
        lernejo ne devas esti <tld/>ejo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_g;&c_a;&c_j;</trd>
  <trd lng="br">peroked</trd>
  <trd lng="ca">lloro</trd>
  <trd lng="cs">papoušek</trd>
  <trd lng="de">Papagei</trd>
  <trd lng="fr">perroquet</trd>
  <trd lng="hu">papag&aacute;ly</trd>
  <trd lng="nl">papegaai</trd>
  <trd lng="pl">papuga</trd>
  <trd lng="pt">papagaio</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_g;&c_a;&c_j;</trd>
  <trd lng="sk">papagáj</trd>
  <trd lng="tr">papa&gbreve;an</trd>
</drv>

<drv mrk="papag.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Psittaciformes</trd>).
      Grimpantaj, tropikaj birdoj, kun brilaj diverskoloraj plumoj
      kaj hoka beko.
    </dif>
  </snc>
  <trd lng="ca">psitaciformes</trd>
  <trd lng="de">Papageienv&ouml;gel</trd>
  <trd lng="hu">papag&aacute;lyalak&uacute;ak</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_g;&c_a;&c_i; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
