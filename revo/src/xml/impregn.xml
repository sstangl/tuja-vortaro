<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: impregn.xml,v 1.6 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>impregn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="impregn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="impregn.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="sorb.0igi.KOMUNE">Sorbigi</ref> 
      substancon en...;
      <ref tip="vid" cel="balzam.0i.PAR">balzami</ref>:
      <ekz>
        per la pretigita infuza&jcirc;o ni bone <tld/>u la vatopecon
        <fnt>
          <aut>A. P&aacute;sztor</aut>,
          <vrk>Viza&gcirc;a refre&scirc;igila&jcirc;o</vrk>,
          <lok><url ref="http://kiralylajos.uw.hu/ujsag/NORDHUNGARIA
          INFORMO 202007-01.pdf">
          Nordhungaria Informo, 2007-01</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fatala aromo <tld/>is la aeron
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>impregnovat</trd>,
    <trd>napouštět</trd>
  </trdgrp>
  <trd lng="fr">impr&eacute;gner</trd>
  <trdgrp lng="pl">
    <trd>impregnowa&cacute;</trd>,
    <trd>przesyca&cacute;</trd>,
    <trd>przepaja&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>impregnovať</trd>,
    <trd>moriť</trd>
  </trdgrp>
</drv>
</art>

