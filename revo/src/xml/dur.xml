<art mrk="$Id: dur.xml,v 1.13 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>dur</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="dur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Malnova <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>
      de Hispanujo kaj Portugalujo. Ar&gcirc;enta monero.
    </dif>
  </snc>
  <trd lng="ca">duro</trd>
  <trd lng="cs">tvrdost</trd>
  <trd lng="de">Duro</trd>
  <trd lng="es">duro <klr tip="amb">(moneda)</klr></trd>
  <trd lng="fr">douro</trd>
  <trd lng="hu">duro <klr tip="amb">(p&eacute;nz)</klr></trd>
  <trdgrp lng="pt">
    <trd>duro</trd>,
    <trd>peso duro</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_r;&c_o; <klr tip="amb">(&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;)</klr></trd>
  <trd lng="sk">tvrdos≈•</trd>
</drv>
</art>
