<vortaro>
<art mrk="$Id: surpli.xml,v 1.14 2015/07/04 08:37:44 revo Exp $">
<kap>
   <rad>surplis</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="surpli.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">TEKS</uzo>
   <uzo tip="fak">KRI</uzo>
   <snc mrk="surpli.0o.TEKS">
     <dif>
       Lina &hcirc;orvesto por ekleziulo.
     </dif>
    <refgrp tip="vid">
      <ref cel="dalmat.0o">dalmatiko</ref>
      <ref cel="sutan.0o.TEKS">sutano</ref>
    </refgrp>     
   </snc>
  <trd lng="be">&c_s;&c_t;&c_y;&c_h;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>komže</trd>,
    <trd>rocheta</trd>
  </trdgrp>
  <trd lng="de">Chorhemd</trd>
  <trd lng="en">surplice</trd>
  <trd lng="fr">surplis</trd>
  <trdgrp lng="hu">
     <trd>karing</trd>,
     <trd>miseing</trd>
   </trdgrp>
  <trd lng="it">cotta<klr> (rel.)</klr></trd>
  <trd lng="nl">koorhemd</trd>
  <trd lng="pl">kom&zdot;a</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_h;&c_a;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kamža</trd>,
    <trd>rocheta</trd>
  </trdgrp>
</drv>
</art>
