<vortaro>
<art mrk="$Id: tarantas.xml,v 1.7 2012/08/13 16:10:31 revo Exp $">
<kap>
  <rad>tarantas</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="tarantas.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rusa kampara <ref tip="super" cel="kalesx.0o">kale&scirc;o</ref>:
      <ekz>
        sur la vojo sin montris <tld/>o, al kiu estis jungitaj tri
        &ccirc;evaloj
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 1a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_a;&c_s;</trd>
  <trd lng="ca">carruatge rus</trd>
  <trd lng="fr">tarantass</trd>
  <trd lng="hu">tarant&aacute;sz</trd>
  <trdgrp lng="pt">
   <trd>tarantas</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_a;&c_s;</trd>
</drv>
</art>

