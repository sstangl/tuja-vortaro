<vortaro>
<art mrk="$Id: tenon.xml,v 1.15 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tenon</rad>/o
</kap>
<drv mrk="tenon.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">KON</uzo>
    <dif>
      Elstaro de ligna a&ubreve; metala peco, enigata en
      <ref tip="vid" cel="mortez.0o">mortezon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_p;</trd>
  <trd lng="bg">&c_shch;&c_i;&c_p;</trd>
  <trd lng="cs">čep</trd>
  <trd lng="de">Zapfen</trd>
  <trd lng="en">tenon</trd>
  <trd lng="fr">tenon</trd>
  <trd lng="hu">csap</trd>
  <trdgrp lng="nl">
    <trd>pin</trd>,
    <trd>tap</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trzpie&nacute;</trd>,
    <trd>czop</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>macho</trd>,
    <trd>espiga<klr> de malhete</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_p;</trd>
  <trd lng="sk">čap</trd>
</drv>
</art>
