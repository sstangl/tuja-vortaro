<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marten.xml,v 1.10 2015/02/22 21:10:19 revo Exp $">
<kap>
  <rad>Marten</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="marten.0o">
  <kap><tld/>o</kap>
  <snc mrk="marten.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        al li venas la kura&gcirc;a maristo <tld/>o Alonzo Pinzon
        <fnt>
          <bib>FK</bib>,
          <lok>4, rakontoj, nokto, (de Eube el Odeso)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_M;&c_a;&c_r;&c_t;&c_i;&c_n;</trd>
  <trd lng="ca">Mart&iacute;</trd>
  <trd lng="de">Martin</trd>
  <trd lng="fr">Martin</trd>
  <trd lng="it">Martino</trd>
  <trd lng="nl">Maarten</trd>
  <trd lng="no">Morten</trd>
  <trd lng="pl">Marcin</trd>
  <trdgrp lng="pt">
    <trd>Martinho</trd>,
    <trd>Martim</trd>,
    <trd>Martins</trd>,
    <trd>Martiniano</trd>,
    <trd>Martino</trd>    
  </trdgrp>
</drv>
</art>

