<art mrk="$Id: diatez.xml,v 1.13 2015/07/04 08:37:32 revo Exp $">
  <kap>
    <rad>diatez</rad>/o
  </kap>
  <drv mrk="diatez.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
        Emo de la organismo al ofta samspeca malsani&gcirc;o.
      </dif>
    </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_t;&c_e;&c_z;</trd>
  <trd lng="ca">di&agrave;tesi</trd>
  <trdgrp lng="cs">
    <trd>diatéze</trd>,
    <trd>slovesný rod</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Diathese</trd>,
       <trd>Neigung</trd>,
       <trd>Anlage</trd>
    </trdgrp>
  <trd lng="es">di&aacute;tesis</trd>
  <trd lng="fr">diath&egrave;se<klr tip="ind">
    (m&eacute;d.)</klr></trd>
  <trdgrp lng="hu">
       <trd>hajlam</trd>,
       <trd>alkat<klr tip="ind"> (diat&eacute;zis)</klr></trd>,
       <trd>diat&eacute;zis</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>diateza</trd>,
      <trd>skaza<klr tip="amb"> (konstytucjonalna)</klr></trd>,
      <trd>konstytucja patologiczna</trd>,
      <trd>sk&lstroke;onno&sacute;&cacute;<klr> do
      zachorowa&nacute;</klr></trd>,
      <trd>podatno&sacute;&cacute;<klr> na zachorowania</klr></trd>
    </trdgrp>
  <trd lng="pt">di&aacute;tese</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_t;&c_je;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>diatéza</trd>,
    <trd>náchylnosť k chorobe</trd>
  </trdgrp>
</drv>
</art>
