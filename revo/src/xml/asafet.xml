<vortaro>
<art mrk="$Id: asafet.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>asafetid</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="asafet.0o">
  <kap><tld/>o</kap>
  <snc mrk="asafet.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malbonodora rezino, devenanta el <ref cel="ferol.0o">ferolo</ref>
      kaj uzata kiel nerva kuracilo:
      <ekz>
        lia helpanto <klr>...</klr> kirlis super fajro magiajn 
        dekoktojn el pe&ccirc;o, <klr>...</klr> ononiso kaj <tld/>o
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url
        ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve;
        fabeloj</url></vrk>,
        <lok>Granda doktora fabelo, [2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">asaf&egrave;tida</trd>
  <trd lng="cs">훾ertovo lejno</trd>
  <trdgrp lng="de">
    <trd>Asant</trd>,
    <trd>Stinkasant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>asa f&oelig;tida</trd>,
    <trd>assa foetida</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>asa f&eacute;tida</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ase f&eacute;tide</trd>,
    <trd>asa-f&oelig;tida</trd>
  </trdgrp>
  <trd lng="hu">p&aacute;lcak&oacute;r&oacute;gyanta</trd>
  <trdgrp lng="it">
    <trd>assafetida</trd>,
    <trd>assa fetida</trd>,
    <trd><ind>sterco</ind> del diavolo</trd>
  </trdgrp>
  <trd lng="nl">duivelsdrek</trd>
  <trd lng="ru">&c_a;&c_s;&c_a;&c_f;&c_je;&c_t;&c_i;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>훾ertovo lajno</trd>,
    <trd>탑ivica z 훾ertovho lajna</trd>
  </trdgrp>
</drv>
</art>
