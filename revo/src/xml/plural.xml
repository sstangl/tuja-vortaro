<art mrk="$Id: plural.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>plural</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt> 
</kap>

<drv mrk="plural.0o">
  <kap><tld/>o</kap>
  <snc mrk="plural.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="nombr.multe0o.GRA">multenombro</ref>
    <ekz>
      en esperanto la substantivan kaj adjektivan <tld/>on markas j;
    </ekz>
    <ekz>
      se vi prenos ian artikolon esperantan, prezentitan
      de niaj kontra&ubreve;uloj por senkreditigi Esperanton,
      vi preska&ubreve; &ccirc;iam trovos nur unu aferon: grandan 
      amasigon de la <tld/>a fini&gcirc;o <ctl>j</ctl>
      <fnt>
        <aut>&Z;</aut>,
        <vrk>
          Parolado anta&ubreve; la Sesa Kongreso Esperantista,
          (Washington, 1910-08-15).
        </vrk>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_n;&c_o;&c_zh;&c_n;&c_y; &c_l;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>množné číslo</trd>,
    <trd>plurál</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mehrzahl</trd>,
    <trd>Plural</trd>
  </trdgrp>
  <trd lng="en">plural</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_m;&a_ayn;<klr tip="ind"> (&a_d;&a_s;&a_t;&a_w;&a_r; &a_z;&a_b;&a_A;&a_n;)</klr></trd>,
    <trd>&a_S;&f_y;&a_gh;&a_h;&a_hamza_sure; &a_j;&a_m;&a_ayn;</trd>
  </trdgrp>
  <trd lng="fr">pluriel<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&ouml;bbes sz&aacute;m</trd>,
    <trd>pluralis</trd>
  </trdgrp>
  <trd lng="nl">meervoud</trd>
  <trd lng="pt">plural</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">množné číslo</trd>
  <trd lng="sv">plural</trd>
</drv>
</art>
