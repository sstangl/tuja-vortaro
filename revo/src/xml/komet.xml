  <trd lng="it">cometa</trd>
  <trd lng="ja">&#24407;&#26143;
[<ind>&#12377;&#12356;&#12379;&#12356;</ind>]
[<ind>&#12411;&#12358;&#12365;&#12412;&#12375;</ind>]</trd>
