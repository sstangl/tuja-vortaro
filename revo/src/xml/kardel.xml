<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kardel.xml,v 1.21 2015/12/08 07:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kardel</rad>/o
</kap>
<drv mrk="kardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kardel.0o.KOMUNE">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <fnt>*</fnt>
    <dif>
      Birdo el la samnoma specio
      (<tld/>o<sncref ref="kardel.0o.specio"/>), kun ru&gcirc;a
      viza&gcirc;o kaj kun nigraj kaj flavaj flugiloj:
      <ekz>
        la najtingalo kantas belege, kaj la <tld/>o &gcirc;in imitas
        <fnt>
          <aut>Tr. A. GRABOWSKI</aut>:
          <vrk>La deveno de la virino</vrk>.
          <lok>2. Fabeloj kaj legendoj</lok>. <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        Same birdist' rigardas kaptilon, logante
        <tld lit="K"/>ojn kaj samtempe paserojn kaptante
        <fnt>
          <aut>A. Mickiewicz, tr. A. Grabowski</aut>:
          <vrk>Sinjoro Tadeo</vrk>. <lok>Libro 3&#170;</lok>.
        </fnt>;
      </ekz><ekz>
        ar&gcirc;enta voko de <tld/>oj
        <fnt>
          <aut>N. Hohlov</aut>:
          <vrk>&Ccirc;e miaj plumvestitaj amikoj</vrk>.
          <lok>Literatura Mondo, Nov. 1922</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_o;&c_l;</trd>     
    <trd lng="br">pabor<klr tip="ind"> (evn)</klr></trd>
    <trd lng="ca">cadernera</trd>
    <trd lng="cs">stehl&iacute;k obecn&yacute;</trd>
    <trdgrp lng="de">
      <trd>Distelfink</trd>,
      <trd>Stieglitz</trd>
    </trdgrp>
    <trd lng="en">goldfinch</trd>
    <trd lng="fr">chardonneret</trd>
    <trd lng="hu">tengelic</trd>
    <trdgrp lng="nl">
      <trd>distelvink</trd>,
      <trd>putter</trd>
    </trdgrp>
    <trd lng="pl">szczygie&lstroke;</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_o;&c_l;</trd>
  </snc>

  <snc mrk="kardel.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">SCI</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      Specio (<trd lng="la">Carduelis carduelis</trd>) kies tipa kaj
      nomdona reprezentanto estas
      <tld/>o<sncref ref="kardel.0o.KOMUNE"/>.
    </dif>
    <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_o;&c_l;</trd>     
    <trd lng="br">pabor<klr tip="ind"> (evn)</klr></trd>
    <trd lng="ca">cadernera</trd>
    <trd lng="cs">stehl&iacute;k obecn&yacute;</trd>
    <trdgrp lng="de">
      <trd>Distelfink</trd>,
      <trd>Stieglitz</trd>
    </trdgrp>
    <trd lng="en">goldfinch</trd>
    <trd lng="fr">chardonneret</trd>
    <trd lng="hu">tengelic</trd>
    <trdgrp lng="nl">
      <trd>distelvink</trd>,
      <trd>putter</trd>
    </trdgrp>
    <trd lng="pl">szczygie&lstroke;</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_o;&c_l;</trd>
  </snc>

  <snc mrk="kardel.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="fak">SCI</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Carduelis</trd>).
    </dif>
    <trdgrp lng="ca">
      <trd>cadernera</trd>,
      <trd>lluer</trd>,
      <trd>passerell</trd>,
      <trd>verdum</trd>
    </trdgrp>
    <trd lng="de">Zeisige</trd>
    <trd lng="en">Carduelis</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_l;&c_y;<klr tip="amb"> (&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <trd lng="sk">stehl&iacute;k</trd>
</drv>
</art>
