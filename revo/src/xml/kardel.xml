<art mrk="$Id: kardel.xml,v 1.20 2015/06/29 16:33:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kardel</rad>/o
</kap>
<drv mrk="kardel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kardel.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Carduelis</trd>).
    </dif>
    <trdgrp lng="ca">
      <trd>cadernera</trd>,
      <trd>lluer</trd>,
      <trd>passerell</trd>,
      <trd>verdum</trd>
    </trdgrp>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_o;&c_l;<klr tip="amb"> (&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <snc mrk="kardel.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <fnt>*</fnt>
    <dif>
      Specio de tiu genro (<trd lng="la">Carduelis carduelis</trd>).
    </dif>
    <trd lng="be">&c_sh;&c_ch;&c_y;&c_g;&c_o;&c_l;</trd>     
    <trd lng="br">pabor<klr tip="ind"> (evn)</klr></trd>
    <trd lng="ca">cadernera</trd>
    <trd lng="cs">stehl&iacute;k obecn&yacute;</trd>
    <trdgrp lng="de">
      <trd>Distelfink</trd>,
      <trd>Stieglitz</trd>
    </trdgrp>
    <trd lng="en">goldfinch</trd>
    <trd lng="fr">chardonneret</trd>
    <trd lng="hu">tengelic</trd>
    <trdgrp lng="nl">
      <trd>distelvink</trd>,
      <trd>putter</trd>
    </trdgrp>
    <trd lng="pl">szczygie&lstroke;</trd>
    <trd lng="ru">&c_shch;&c_je;&c_g;&c_o;&c_l;</trd>
  </snc>
  <trd lng="sk">stehl√≠k</trd>
</drv>
</art>
