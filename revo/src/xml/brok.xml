<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brok.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>brok</rad>/i
</kap>
<drv mrk="brok.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="brod.0teksi">brodteksi.</ref>
  </snc>
  <trd lng="ca">fer un brocat</trd>
  <trdgrp lng="cs">
    <trd>stříbrem nebo zlatem</trd>,
    <trd>vetkávat obrazce hedvábím</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&odblac; <klr tip="ind">(brok&aacute;tot)</klr></trd>,
    <trd>&aacute;tsz&odblac; <klr>(selyemmel)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tka&cacute;</trd>,
    <trd>przetyka&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_t;&c_mol; <klr>(&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m;, &c_s;&c_je;&c_r;&c_je;&c_b;&c_r;&c_o;&c_m; &c_i;&c_l;&c_i;
  &c_sh;&c_jo;&c_l;&c_k;&c_o;&c_m;)</klr></trd>
  <trd lng="sk">pretkať</trd>
</drv>

<drv mrk="brok.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o <tld/>ita per ar&gcirc;entaj a&ubreve; oraj fadenoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Brokatstoff</trd>,
    <trd>Brokat</trd>
  </trdgrp>
  <trd lng="ca">brocat</trd>
  <trd lng="fr">brocart</trd>
  <trd lng="hu">brok&aacute;t</trd>
  <trd lng="pl">brokat</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_ch;&c_a;</trd>
</drv>
</art>
