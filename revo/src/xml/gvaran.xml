<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gvaran.xml,v 1.5 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gvaran</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gvaran.0o">
  <kap><tld/>o</kap>
  <snc mrk="gvaran.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda ru&gcirc;a <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de arbo <em>Paullinia cupana</em> kreskanta en Brazilo,
      man&gcirc;ata post sekigo kaj precipe uzata en vigligaj trinka&jcirc;oj
      pro kafeino kaj aliaj substancoj:
      <ekz>
        troveblas anka&ubreve; karambolfrukto, asaio kaj <tld/>o,
        kiu donis sian nomon al la plej populara sodakvo
        de la lando
        <fnt>
          <aut>C. Pinardon. R. Sanz</aut>,
          <vrk>Du Bicikloj je la Horizonto</vrk>,
          <lok><url
          ref="http://www.horizonto.net/rakontoj/34-brazilo.html">
          Brazilo</url>, 2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>guarana</trd>,
    <trd>guaranová pasta</trd>
  </trdgrp>
  <trd lng="en">guarana</trd>
  <trd lng="fr">guarana</trd>
  <trd lng="pl">guarana</trd>
  <trd lng="sk">guaranová pasta</trd>
</drv>

<drv mrk="gvaran.0arbo">
  <kap><tld/>ujo</kap>
  <snc mrk="gvaran.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Rampa&jcirc;o de tropika Sudameriko
      kun man&gcirc;eblaj fruktoj, konataj pro granda enhavo de kofeino
      el familio sapindacoj.
    </dif>
  </snc>
  <trdgrp lng="la">
    <trd>Paullinia cupana</trd>,
    <trd>Paullinia guarana</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>guarana</trd>,
    <trd>cierniopl&aogonek;t</trd>,
    <trd>osm&eogonek;ta</trd>
  </trdgrp>
</drv>

</art>

