<art mrk="$Id: reputa.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>reputaci</rad>/o
</kap>
<drv mrk="reputa.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Opinio de la publiko pri iu:
      <ekz>
        per la akcidento anka&ubreve; grave makuli&gcirc;is la belga
        <tld/>o<fnt><aut>Guido Van Damme</aut>: <vrk>Pli bone unu birdo
        en
        la mano</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 11a, p.
        19a</lok></fnt>..
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam.0o">famo</ref>,
      <ref cel="renom.0o">renomo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_p;&c_u;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">reputaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>jméno</trd>,
    <trd>proslulost</trd>,
    <trd>reputace</trd>,
    <trd>vážnost</trd>,
    <trd>úcta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Reputation</trd>,
    <trd>Ruf</trd>,
    <trd>&ouml;ffentlicher <ind>Ruf</ind></trd>,
    <trd>Ansehen</trd>,
    <trd>&ouml;ffentliches <ind>Ansehen</ind></trd>
  </trdgrp>
  <trd lng="en">reputation</trd>
  <trd lng="es">reputaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>image publique</trd>,
    <trd>r&eacute;putation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&eacute;v</trd>,
    <trd>tekint&eacute;ly</trd>,
    <trd>reput&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">reputatie</trd>
  <trd lng="pt">reputa&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>dobrá povesť</trd>,
    <trd>dobré meno</trd>
  </trdgrp>
</drv>
</art>
