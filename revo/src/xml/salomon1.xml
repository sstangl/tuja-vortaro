<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: salomon1.xml,v 1.6 2014/06/18 20:10:13 revo Exp $">
<kap>
  <rad>Salomon</rad>/oj
</kap>

<drv mrk="salomon1.S0oj">
  <kap>
    <tld lit="S"/>oj
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="salomon1.S0oj.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insularo en la sudorienta <ref tip="malprt"
      cel="oceani.O0o">Oceanio</ref>.
      Teritorio de Respubliko Salomonoj
      kaj <ref tip="vid"
      cel="papu.P0oNovGvineo.sxtato">Papuo-Nov-Gvineo</ref>.
    </dif>
    <trd lng="bg">&c_S;&c_o;&c_l;&c_o;&c_m;&c_o;&c_n;&c_o;&c_v;&c_i; &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;</trd>
    <trd lng="de">Salomon-Inseln</trd>
    <trd lng="en">Solomon Islands<klr tip="amb">
    (archipelago)</klr></trd>
    <trd lng="fr">&icirc;les <ind>Salomon</ind></trd>
    <trdgrp lng="pl">
      <trd><klr>archipelag </klr>Wysp Salomona</trd>
    </trdgrp>
  </snc>
  <snc mrk="salomon1.0oj.sxtato">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la orienta parto de la samnoma insularo kun
      &ccirc;efurbo Honiaro.
      Respubliko Salomonoj.
      ISO-3166 kodo: SB.
    </dif>
    <trd lng="bg">&c_S;&c_o;&c_l;&c_o;&c_m;&c_o;&c_n;&c_o;&c_v;&c_i; &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;<klr tip="amb">
    (&c_d;&c_malmol;&c_r;&c_zh;&c_a;&c_v;&c_a;)</klr></trd>
    <trd lng="de">Salomonen</trd>
    <trd lng="en">Solomon Islands</trd>
    <trd lng="fr">Salomon</trd>
    <trd lng="it">Isole <ind>Salomone</ind></trd>
    <trdgrp lng="pl">
      <trd>Wyspy Salomona</trd>
    </trdgrp>
    <trd lng="pt">Ilhas Salom&atilde;o</trd>
  </snc>
</drv>

</art>

