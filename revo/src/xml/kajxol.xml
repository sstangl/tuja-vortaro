<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kajxol.xml,v 1.13 2015/05/22 17:10:15 revo Exp $">
<kap>
   <rad>ka&jcirc;ol</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kajxol.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       <ref tip="super" cel="kares.0i">Karesi</ref>, ame prizorgi, peni
       por
       pla&ccirc;i al iu, ofte por ricevi ian favoron:
       <ekz>
         <tld/>i infanon;
       </ekz>
       <ekz>
         ili <klr>[...]</klr> tiom <tld/>as al la ekonomoj
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, volumo 2a, p.
         502a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="cxarm.0i">&ccirc;armi</ref>,
       <ref cel="flat.0i">flati</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
     <trd>&c_d;&c_a;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="bg">&c_l;&c_a;&c_s;&c_k;&c_a;&c_ja;</trd>
   <trd lng="en">cajole</trd>
   <trdgrp lng="fi">
     <trd>mairitella (suostumaan)</trd>,
     <trd>suostutella (hell&auml;sti)</trd>,
     <trd>mielistell&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>cajoler</trd>,
     <trd>c&acirc;liner</trd>,
     <trd>dorloter</trd>,
     <trd>enj&ocirc;ler</trd>
   </trdgrp>
   <trdgrp lng="es">
    <trd>mimar (esp)</trd>,
    <trd>apapachar (amer)</trd>,
    <trd>engatusar</trd>
   </trdgrp> 
   <trdgrp lng="hu">
     <trd>babusgat</trd>,
     <trd>cir&oacute;gat</trd>,
     <trd>d&eacute;delget</trd>,
     <trd>gazsul&aacute;l</trd>,
     <trd>h&iacute;zelkedik</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>vleien</trd>,
     <trd>zoete <ind>broodjes</ind> bakken</trd>
   </trdgrp>
   <trd lng="pt">bajular</trd>
   <trdgrp lng="ru">
     <trd>&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_u;&c_g;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_u;&c_g;&c_o;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="tr">
     <trd>ok&scedil;amak</trd>,
     <trd>sevmek</trd>,
     <trd>tavlamak</trd>,
     <trd>piyazlamak</trd>
   </trdgrp>
</drv>

<drv mrk="kajxol.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       <tld lit="K"/>anta:
       <ekz>
       <tld/>a konduto, gestoj, vo&ccirc;o:
       </ekz>
       <ekz>
         sed kial li min bezonas? &ndash; <tld/>e demandis
         Margarita
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 197a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fi">
     <trd>hellytt&auml;v&auml;</trd>,
     <trd>mairea</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>cajoleur<klr tip="ind"> (adj.)</klr></trd>,
     <trd>enjoleur<klr tip="ind"> (adj.)</klr></trd>,
     <trd>c&acirc;lin<klr tip="ind"> (adj.)</klr></trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>zalamero<klr tip="ind"> (adj.)</klr></trd>,
     <trd>adulador<klr tip="ind"> (adj.)</klr></trd>,
     <trd>mimoso</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>babusgatva</trd>,
     <trd>d&eacute;delgetve</trd>,
     <trd>h&iacute;zelg&odblac;n</trd>
   </trdgrp>
   <trd lng="nl">vleiend</trd>
   <trd lng="ru">&c_l;&c_mol;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>
   <trdgrp lng="tr">
     <trd>pohpoh&ccedil;u</trd>,
     <trd>seven</trd>
   </trdgrp>
</drv>
</art>
