<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lik.xml,v 1.19 2015/09/21 08:10:17 revo Exp $">
<kap>
  <rad>lik</rad>/i
</kap>
<drv mrk="lik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Tralasi likva&jcirc;on, p.p. pri ujo, kies vando fendeti&gcirc;is pro
      akcidento, eluzado ktp:
      <ekz>
        <tld/>as kaldrono,
      </ekz>
      <ekz>
        tubo,
      </ekz>
      <ekz>
        vazo;
      </ekz>
      <ekz>
        Dunjo, malfermu! &Ccirc;u de vi <tld/>as la akvo?
        <fnt><bib>MkM</bib>, <lok></lok></fnt>.
      </ekz>
    </dif>
    <adm>
      Ofte renkonteblas uzo de "liki" p.p. likva&jcirc;o ("likas akvo" en
      la ekzemplo de Pokrovskij), do "liki" tiuokaze havas ankora&ubreve;
      unu sencon, sinoniman al "traliki&gcirc;i". &Ccirc;u indas aldoni tiun
      sencon, a&ubreve; tio estas nur ofta eraro?
      <aut>JF</aut>
    </adm>
    <trdgrp lng="hu">
      <trd>sziv&aacute;rog</trd>,
      <trd>ereszt</trd>,
      <trd>folyat</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ciekn&aogonek;&cacute;</trd>,
      <trd>przecieka&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAR</uzo>
    <dif>
      Enlasi akvon, p.p. pri &scirc;ipo, kies kareno estis iel difektita.
    </dif>
    <trd lng="hu">l&eacute;ket kapott</trd>
    <trd lng="pl">przecieka&cacute;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      Ellasi la elektran kurenton al la tero, pro difekti&gcirc;o de la
      kondukiloj.
    </dif>
    <trd lng="hu">sziv&aacute;rog</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_ch;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;
<ind>&c_v;&c_a;&c_d;&c_u;</ind></trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;
<ind>&c_c;&c_je;&c_ch;&c_u;</ind></trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;
<ind>&c_u;&c_c;&c_je;&c_ch;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nab&iacute;rat vodu</trd>,
    <trd>propou&scaron;t&ecaron;t kapalinu</trd>,
    <trd>prosakovat</trd>,
    <trd>t&eacute;ci</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>lecken</trd>,
     <trd>leck sein</trd>
  </trdgrp>
  <trd lng="en">leak</trd>
  <trdgrp lng="es">
     <trd>rezumar</trd>,
     <trd>fugar</trd>
  </trdgrp>
  <trd lng="fr">fuire</trd>
  <trd lng="nl">lekken</trd>
  <trd lng="pt">vazar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_t;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_u;&c_t;&c_je;&c_ch;&c_k;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>netesni&tcaron;</trd>,
    <trd>prep&uacute;&scaron;&tcaron;a&tcaron; kvapalinu</trd>,
    <trd>presakova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">l&auml;cka</trd>
</drv>

<drv mrk="lik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Difektita loko, je kiu <tld/>as la responda objekto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_ch;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_b;&c_o;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prasklina</trd>,
    <trd>skulina</trd>,
    <trd>&scaron;t&ecaron;rbina</trd>
  </trdgrp>
  <trd lng="de">Leck</trd>
  <trd lng="en">leak</trd>
  <trd lng="es">fuga</trd>
  <trd lng="fr">fuite</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;k</trd>,
    <trd>sziv&aacute;rg&oacute; hely</trd>
  </trdgrp>
  <trd lng="nl">lek</trd>
  <trdgrp lng="pl">
    <trd>przeciek</trd>,
    <trd>wyciek</trd>,
    <trd>przebicie <klr>(elektr.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_ch;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>1.netesnos&tcaron;</trd>,
    <trd>2.izola&ccaron;n&yacute; defekt <klr>(el. tech.)</klr></trd>,
    <trd>puklina</trd>
  </trdgrp>
  <trd lng="sv">l&auml;cka</trd>
</drv>

<drv mrk="lik.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Stato de io <tld/>anta.
    </dif>
  </snc>
  <trd lng="en">leaking</trd>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_ch;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_jo;&c_k;</trd>,
    <trd>&c_u;&c_c;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>te&ccaron;en&iacute;</trd>,
    <trd>unik&aacute;n&iacute; kapaliny</trd>
  </trdgrp>
  <trd lng="de">Lecken</trd>
  <trdgrp lng="hu">
    <trd>sziv&aacute;rg&aacute;s</trd>,
    <trd>ereszt&eacute;s</trd>,
    <trd>folyat&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">het <ind>lekken</ind></trd>
  <trdgrp lng="pl">
    <trd>przeciek</trd>,
    <trd>wyciek</trd>,
    <trd>przebicie <klr>(elektr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_t;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>presakovanie</trd>,
    <trd>unikanie <klr>(pre netesnos&tcaron; )</klr></trd>
  </trdgrp>
  <trd lng="sv">l&auml;ckande</trd>
</drv>

<drv mrk="lik.0imuna">
  <kap><tld/>imuna</kap>
  <snc>
    <dif>
      Ne penetrebla de la akvo, p.p. &scirc;ipvando ks.
      <ekz>
        Ju pli firma estas la tegmento, des pli <tld/>imuna &gcirc;i estas.
        <fnt>
        <aut>Zheng Zhenjie</aut>:
        <vrk>Vojagho al Maiji-monto</vrk>.
        <lok>&Ccirc;ina Interreta Informa Centro</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en"><trd>leakage-immune</trd>,
  <trd>leak-resistant</trd></trdgrp>
  <trd lng="be">&c_v;&c_o;&c_d;&c_a;&c_n;&c_je;&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>nepropustn&yacute;</trd>,
    <trd>vodot&ecaron;sn&yacute;</trd>
  </trdgrp>
  <trd lng="de">dicht</trd>
  <trd lng="hu">sziv&aacute;rg&aacute;smentes</trd>
  <trd lng="nl">lekvrij</trd>
  <trdgrp lng="pl">
    <trd>nieprzeciekaj&aogonek;cy</trd>,
    <trd>nieprzepuszcaj&aogonek;cy</trd>,
    <trd>szczelny</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_n;&c_je;&c_p;&c_r;&c_o;&c_n;&c_i;&c_c;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nepriepustn&yacute;</trd>,
    <trd>vodotesn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">t&auml;t</trd>
</drv>

<drv mrk="lik.tra0igxi">
  <kap>tra<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Trairi tra <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sekreto iel tra<tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en"><trd>to leak out</trd>, <trd>to leak
  through</trd></trdgrp>
  <trdgrp lng="de">
     <trd>austreten</trd>,
     <trd>durchlecken</trd>
  </trdgrp>
  <trd lng="fr">fuire</trd>
  <trd lng="hu">kisziv&aacute;rog</trd>
  <trd lng="nl">doorlekken</trd>
  <trdgrp lng="pl">
    <trd>przecieka&cacute;</trd>,
    <trd>wycieka&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">l&auml;cka<klr> (igenom)</klr></trd>
</drv>

<drv mrk="lik.memor0ado">
  <kap>memor<tld/>ado</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Programista eraro, manifesti&gcirc;anta en perdo de loko en komputila
      memoro pro multobla disponigo de dinamika memorareo por datumoj
      sen posta liberigo de &gcirc;i, pro kio la disponebla memoro
el&ccirc;erpi&gcirc;as
      kaj la programo paneas:
      <ekz>
        La&ubreve; la planoj oni komencos serioze koncentri&gcirc;i je
plirapidigo
        de la programo (kaj forigo de memor<tld/>ado) nur en M19 a&ubreve;
        M20.
        <fnt>
        <aut>Bertilo Wennergren</aut>,
        <lok>soc.culture.esperanto,
        Message-ID: 8uusm3$2ve8d$1@ID-4825.news.dfncis.de</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_c;&c_je;&c_ch;&c_k;&c_a;
<ind>&c_p;&c_a;&c_m;&c_ja;&c_c;&c_ib;</ind></trd>
  <trd lng="de">Speicherleck</trd>
  <trd lng="en">memory leak</trd>
  <trd lng="fr">fuite de m&eacute;moire</trd>
  <trd lng="hu">mem&oacute;riafogy&aacute;s</trd>
  <trd lng="nl">geheugenlek</trd>
  <trd lng="pl">wyciek pami&eogonek;ci</trd>
  <trd lng="ru">&c_u;&c_t;&c_je;&c_ch;&c_k;&c_a;
&c_p;&c_a;&c_m;&c_ja;&c_t;&c_i;</trd>
  <trd lng="sv">minnesl&auml;cka</trd>
</drv>
</art>
