<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">  
<vortaro>
<art mrk="$Id: pin.xml,v 1.31 2015/07/04 08:37:41 revo Exp $">
<kap><ofc>*</ofc><rad>pin</rad>/o</kap>
<drv mrk="pin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;iamverda arbo <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref> kun longaj pingloformaj
      folioj
      de du specoj: unuj sidantaj spirale rekte &ccirc;e la bran&ccirc;oj,
      aliaj en faskoj duope &gcirc;is kvinope, malofte sesope;
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="pinac.0oj">pinacoj</ref>
      (<trd
      lng="la">Pinus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_o;&c_r;</trd>
  <trd lng="ca">pi</trd>
  <trdgrp lng="cs">
    <trd>borovice</trd>,
    <trd>borovice sosna</trd>
  </trdgrp>
  <trd lng="da">fyr</trd>
  <trd lng="de">Kiefer</trd>
  <trd lng="en">pine</trd>
  <trd lng="es">pino</trd>
  <trd lng="fa">&f_k;&a_A;&a_j;</trd>
  <trd lng="fr">pin</trd>
  <trd lng="hu">feny&odblac;</trd>
  <trd lng="id">pinus</trd>
  <trd lng="mk">&c_b;&c_o;&c_r;</trd>
  <trdgrp lng="nl">
    <trd>pijnboom</trd>, <trd>den</trd>
  </trdgrp>
  <trd lng="pl">sosna</trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_n;&c_a;</trd>
  <trd lng="sk">borovica</trd>
  <trd lng="sr">&c_b;&c_o;&c_r;</trd>
  <trd lng="tr">&ccedil;am</trd>
  <trd lng="uk">&c_s;&c_o;&c_s;&c_n;&c_a;</trd>
</drv>
</art>
