<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: len.xml,v 1.12 2015/07/04 08:37:38 revo Exp $">
    <kap>
      <rad>Len</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="len.L0o">
      <kap><tld/>o</kap>
      <snc>
  <uzo tip="fak">GEOG</uzo>
  <dif>
    Riverego en la Orienta <ref tip="malprt"
    cel="siberi.S0o">Siberio</ref>, 4400km longa, fluanta de la
    Bajkala monto&ccirc;eno (&ccirc;e la okcidenta bordo de <ref
    cel="bajkal.B0o">Bajkalo</ref>) en la
    <ref cel="mar.LaptevaM0o">Laptevan Maron</ref>.
  </dif>
      </snc>
  <trd lng="be">&c_L;&c_je;&c_n;&c_a;</trd>
  <trd lng="bg">&c_L;&c_je;&c_n;&c_a;</trd>
  <trd lng="cs">Lena</trd>
  <trd lng="de">Lena</trd>
  <trd lng="en">Lena</trd>
  <trd lng="fr">L&eacute;na</trd>
  <trd lng="hu">L&eacute;na</trd>
  <trd lng="nl">Lena<klr tip="ind"> (rivier)</klr></trd>
  <trd lng="pt">Lena<klr> (geogr.)</klr></trd>
  <trd lng="ru">&c_L;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">Lena</trd>
</drv>

  </art>

