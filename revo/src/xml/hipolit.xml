<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hipolit.xml,v 1.8 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>Hipolit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hipolit.0o">
  <kap><tld/>o</kap>
  <snc mrk="hipolit.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        en la komenco de tiu &ccirc;i centjaro <klr>...</klr> la franco
        <klr>...</klr> <tld/>o Michon alkondukis tiun &ccirc;i arton
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Hip&ograve;lit</trd>
  <trd lng="cs">Hippolytos</trd>
  <trd lng="de">Hippolyt</trd>
  <trd lng="fr">Hippolyte</trd>
  <trd lng="it">Ippolito</trd>
  <trd lng="pt">Hip&oacute;lito</trd>
  <trd lng="sk">Hippolytos</trd>
</drv>
</art>

