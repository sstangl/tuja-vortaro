<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: zibel.xml,v 1.25 2015/06/28 10:33:41 revo Exp $">
   <kap>
   <ofc>*</ofc>
   <rad>zibel</rad>/o
   <fnt><bib>PV</bib></fnt>
   </kap>
<drv mrk="zibel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="zibel.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Malgranda <ref tip="lst" cel="best.rabo0o" 
       lst="voko:rabobestoj">rabobesto</ref> kun svelta korpo kaj
       mallongaj
       kruroj, &ccirc;asata pro la tre valora pelto;
       <ref tip="lst" cel="speci.0o.BIO"
       lst="voko:zoologiaj_specioj">specio</ref> de <ref tip="super"
       cel="martes.0o">marteso</ref>
       (<trd lng="la">Martes zibellina</trd>):
      <ekz>
        dum dudek-kvar vintroj mi &ccirc;asadis <tld/>ojn kaj mustelojn en la 
        arbaroj de Siberujo
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>       
     </dif>
   </snc>
  <trd lng="be">&c_s;&c_o;&c_b;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">mart gibel&iacute;</trd>
  <trd lng="cs">sobol</trd>
  <trd lng="de">Zobel</trd>
  <trd lng="el">&sigma;&alfa;&my;&omikron;&ypsilon_ton;&rho;&jota;</trd>
  <trdgrp lng="en">
     <trd>sable</trd>,
     <trd>marten</trd>
   </trdgrp>
  <trd lng="es">marta cebellina</trd>
  <trd lng="fr">zibeline</trd>
  <trd lng="hu">coboly</trd>
  <trd lng="it">zibelino</trd>
  <trd lng="nl">sabeldier</trd>
  <trd lng="pl">sob&oacute;l</trd>
  <trdgrp lng="pt">
     <trd>zibelina</trd>,
     <trd>marta-zibelina</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_b;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">soboÄ¾</trd>
</drv>
</art>
