<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grapol.xml,v 1.15 2015/09/21 14:10:20 revo Exp $">
<kap>
  <rad>grapol</rad>/o
</kap>
<drv mrk="grapol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, kie &ccirc;e
      meza akso trovi&gcirc;as la floroj (a&ubreve; fruktoj) sur tigetoj:
      <ekz>
        <tld/>o de ribo, hiacinto;
      </ekz>
      <ekz>
        <tld/>o da vinberoj;
      </ekz>
      <ekz>
        <ind>umbela <tld/>o</ind>
        <klr>(la floroj havas diverslongajn tigetojn kaj trovi&gcirc;as
        en unu ebeno)</klr>;
        <trd lng="de">Doldentraube</trd>
        <trd lng="hu">erny&odblac;s vir&aacute;gzat</trd>
      </ekz>
      <ekz>
        <ind>kunmetita <tld/>o</ind>
        <klr>(&ccirc;e meza akso trovi&gcirc;as <tld/>etoj)</klr>.
        <trd lng="de">Doppeltraube</trd>
      </ekz>       
    </dif>
    <ref tip="vid" cel="ber.0aro">beraro</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>ra&iuml;m</trd>,
    <trd>infloresc&egrave;ncia</trd>
  </trdgrp>
  <trd lng="cs">hrozen kv&ecaron;t&uring; nebo ovoce</trd>
  <trd lng="de">Traube</trd>
  <trd lng="en">cluster</trd>
  <trd lng="fr">grappe</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;rt</trd>,
    <trd>f&uuml;rtvir&aacute;gzat</trd>
  </trdgrp>
  <trd lng="nl">tros<klr tip="ind"> (fruit of vruchten)</klr></trd>
  <trdgrp lng="pt">
   <trd>cacho</trd>, 
   <trd>penca<klr tip="amb"> (de flores ou frutas)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_o;&c_z;&c_d;&c_mol;</trd>,
    <trd>&c_k;&c_i;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>metlina</trd>,
    <trd>strapec</trd>
  </trdgrp>
  <trd lng="tr">salk&inodot;m</trd>
</drv>
</art>

