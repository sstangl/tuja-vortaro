<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konsek.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>konsekr</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konsek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konsek.0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Ekfunkciigi en sankta rolo.
    </dif>
    <subsnc mrk="konsek.0i.iun">
      <dif>
        Solene enoficigi per religiaj ritoj pastron, re&gcirc;on... 
      </dif>
      <trdgrp lng="en">
        <trd>ordain <klr>(p. p. pastro)</klr></trd>,
        <trd>coronate <klr>(p. p. re&gcirc;o)</klr></trd>
      </trdgrp>
      <trdgrp lng="de">
         <trd>weihen</trd>,
         <trd>konsekrieren</trd>
      </trdgrp>
      <trd lng="hu">szentel</trd>
      <trd lng="nl">inwijden</trd>
      <trd lng="ru">&c_p;&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_v; &c_s;&c_a;&c_n;)</klr></trd>
    </subsnc>
    <subsnc mrk="konsek.0i.ion">
      <dif>
        Destini a&jcirc;ojn por dia servado per religiaj ritoj:
        <ekz>
          ni konstruis la pre&gcirc;ejon, kaj &gcirc;uste
          post unu jaro mi &gcirc;in <tld/>is
          <fnt>
            <aut>L. Vai&ccaron;iulionis</aut>,
            <vrk><url
            ref="http://barelo.blogspot.com/2011/12/monsinjoro-lionginas-vaiciulionis-mi.html">
            Mi spertis neniajn spiritajn interrompojn...</url></vrk>,
            <lok>2011-12-31</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
         <trd>einsegnen</trd>,
         <trd>einweihen</trd>,
         <trd>konsekrieren</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>consecrate</trd>,
        <trd>decicate</trd>,
        <trd>devote</trd>,
        <trd>hallow</trd>,
        <trd>sanctify</trd>
      </trdgrp>
      <trd lng="hu">felszentel</trd>
      <trd lng="nl">consacreren</trd>
      <trdgrp lng="pt">
        <trd>consagrar</trd>,
        <trd>sagrar</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>konsekrovat</trd>,
    <trd>posvěcovat</trd>,
    <trd>světit</trd>,
    <trd>zasvěcovat</trd>
  </trdgrp>
  <trd lng="es">consagrar</trd>
  <trdgrp lng="fr">
    <trd>consacrer</trd>,
    <trd>ordonner<klr> (rel. consacrer)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>posväcovať</trd>,
    <trd>svätiť</trd>
  </trdgrp>
</drv>
</art>
