<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vak.xml,v 1.20 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vak.0i.KOMUNE">
    <uzo tip="klr">(pri loko, ofico ktp)</uzo>
    <dif>
      Esti ne okupata, esti
      libera, disponebla:
      <ekz>
        <ind><tld/>anta se&gcirc;o</ind>;
        <trd lng="hu">&uuml;res sz&eacute;k</trd>
      </ekz>
      <ekz>
        li kondukis &scirc;in al <tld/>anta spaco anta&ubreve; la statuo;
      </ekz>
      <ekz>
        la prezidanteco <tld/>as;
      </ekz>
      <ekz>
        <tld/>anteco de lo&gcirc;ejo, de posteno.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_s;&c_v;&c_a;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;</ind>
      <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_n;&c_je;&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
      <ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="ca">vacar</trd>
  <trdgrp lng="cs">
    <trd>být prázdný</trd>,
    <trd>uvolněný <klr>(místo, úřad)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>frei sein</trd>,
    <trd>unbesetzt sein</trd>,
    <trd>leer sein</trd>,
    <trd>vakant sein</trd>
  </trdgrp>
  <trd lng="en">be vacant</trd>
  <trdgrp lng="es">
    <trd>estar vac&iacute;o</trd>,
    <trd>estar libre</trd>
  </trdgrp>
  <trd lng="fr">vacant (&ecirc;tre)</trd>
  <trdgrp lng="hu">
    <trd>&uuml;res</trd>,
    <trd>szabad <klr tip="ind">(nincs elfoglalva)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vrij zijn</trd>,
    <trd>vacant zijn</trd>
  </trdgrp>
  <trd lng="pt">estar vago</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_n;&c_je;
&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_v;&c_a;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_s;&c_v;&c_o;&c_b;&c_o;&c_d;&c_n;&c_y;&c_m;</ind> <klr tip="amb">(&c_o;
&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>byť neobsadený</trd>,
    <trd>prázdny</trd>,
    <trd>uvoľnený</trd>
  </trdgrp>
</drv>

<drv mrk="vak.0a">
  <kap><tld/>a</kap>
  <snc mrk="vak.0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="liber.0a.vaka">Libera<sncref/></ref>,
      <ref tip="vid" cel="dispon.0ebla">disponebla</ref>:
      <ekz>
        ne estas <tld/>a loko en nia oficejo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>vacant</trd>,
    <trd>lliure</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neobsazený</trd>,
    <trd>prázdný</trd>,
    <trd>uprázdněný</trd>,
    <trd>uvolněný</trd>,
    <trd>volný</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inoccup&eacute;</trd>,
    <trd>vacant</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neobsadený</trd>,
    <trd>prázdny</trd>,
    <trd>uprázdnený</trd>,
    <trd>voľný</trd>
  </trdgrp>
</drv>
</art>
