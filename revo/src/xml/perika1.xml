<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: perika1.xml,v 1.16 2011/12/03 23:10:22 revo Exp $">
<kap>
  <rad>perikarp</rad>/o
</kap>
<drv mrk="perika1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;io de la <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>,
      kio envolvas unuopan a&ubreve; plurajn grajnojn:
      <ekz>
        <tld/>o konsistas el epikarpo, sarkokarpo a&ubreve; mezokarpo kaj
        endokarpo.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="grajn.0umo">grajnumo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_y;&c_k;&c_a;&c_r;&c_p;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_p;&c_l;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">pericarpi</trd>
  <trdgrp lng="de">
    <trd>Fruchtschale</trd>,
    <trd>Pericarp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pericarp</trd>,
    <trd>seed case</trd>,
    <trd>seed vessel</trd>
  </trdgrp>
  <trd lng="es">pericarpio</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;ricarpe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>term&eacute;sfal</trd>,
    <trd>perikarpium</trd>
  </trdgrp>
  <trd lng="la">pericarpium</trd>
  <trd lng="nl">pericarpium</trd>
  <trdgrp lng="pt">
    <trd>pericarpo</trd>,
    <trd>peric&aacute;rpio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_k;&c_a;&c_r;&c_p;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_l;&c_o;&c_p;&c_l;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
