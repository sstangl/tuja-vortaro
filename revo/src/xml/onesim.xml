<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: onesim.xml,v 1.2 2015/02/26 19:11:23 revo Exp $">
<kap>
  <rad>Onesim</rad>/o
</kap>
<drv mrk="onesim.0o">
  <kap><tld/>o</kap>
  <snc mrk="onesim.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        li konsolu viajn korojn, kune kun <tld/>o,
        la fidela kaj amata frato
        <fnt><bib>NT</bib>, <lok>&Kol; 4:8-9</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o informis min
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">On&eacute;sime</trd>
</drv>
</art>

