<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sofism.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sofism</rad>/o
</kap>
<drv mrk="sofism.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;ajne &gcirc;usta sed fakte erariga rezono.
    </dif>
    <refgrp tip="sin">
      <ref cel="paralo.0o">paralogismo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">sofisma</trd>
  <trdgrp lng="cs">
    <trd>logický klam</trd>,
    <trd>matoucí argumentace</trd>,
    <trd>sofizma</trd>,
    <trd>úskok</trd>
  </trdgrp>
  <trd lng="de">Sophismus</trd>
  <trd lng="es">sofisma</trd>
  <trd lng="fr">sophisme</trd>
  <trdgrp lng="hu">
    <trd>szofizmus</trd>,
    <trd>sz&odblac;rsz&aacute;lhasogat&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">sofisme</trd>
  <trd lng="pl">sofizmat</trd>
  <trd lng="ru">&c_s;&c_o;&c_f;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>logický klam</trd>,
    <trd>mätúca argumentácia</trd>,
    <trd>úskok</trd>
  </trdgrp>
</drv>
</art>
