<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pacifik.xml,v 1.17 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Pacifik</rad>/o
</kap>
<drv mrk="pacifik.P0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      inter Ameriko, Azio kaj Oceanio:
      <ekz>
        vulkanoj el la eksplodema tipo trovi&gcirc;as la&ubreve;longe de
        zono &ccirc;irka&ubreve;
        <tld/>o<fnt><aut>Evgeni Georgiev</aut>: <vrk>Vulkanoj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_ib;&c_h;&c_ib; <ind>&c_a;&c_k;&c_ib;&c_ja;&c_n;</ind></trd>
  <trd lng="bg">&c_t;&c_i;&c_h;&c_i; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>Pacifický oceán</trd>,
    <trd>Tichý oceán</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stiller <ind>Ozean</ind></trd>,
    <trd>Pazifik</trd>
  </trdgrp>
  <trd lng="en">Pacific Ocean</trd>
  <trd lng="es">oc&eacute;ano <ind>Pac&iacute;fico</ind></trd>
  <trdgrp lng="fr">
    <trd>oc&eacute;an <ind>pacifique</ind></trd>
  </trdgrp>
  <trd lng="hu">Csendes-&oacute;ce&aacute;n</trd>
  <trd lng="nl">Stille <ind>Oceaan</ind></trd>
  <trdgrp lng="pl">
    <trd>Pacyfik</trd>,
    <trd>Ocean Spokojny</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>oceano Pac&iacute;fico</trd>
  </trdgrp>
  <trd lng="ru">&c_T;&c_i;&c_h;&c_i;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  <trd lng="sk">Tichý oceán</trd>
</drv>
</art>
