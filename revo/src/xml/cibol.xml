<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cibol.xml,v 1.6 2009/07/03 16:30:36 revo Exp $">
<kap>
  <rad>cibol</rad>/o
</kap>
<drv mrk="cibol.0o">
  <kap><tld/>o</kap>
  <snc mrk="cibol.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      (<trd lng="la">Allium fistulosum</trd>) pli granda ol
      &scirc;enoprazo, kun pluraj subspecioj, kies foliojn oni uzas kiel <ref
      tip="super" cel="spic.0o">spico</ref>, a&ubreve; kuire kiel legomo.
      <ekz>
        neimageble dikaj <tld/>vendistinoj kaj kamparanoj, venintaj &ccirc;i
        tien por vendi brasikon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schnittzwiebel</trd>,
    <trd>Winterzwiebel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&ouml;veshagyma</trd>,
    <trd>t&eacute;li sarjad&eacute;k<ind>hagyma</ind></trd>
  </trdgrp>  
  <trdgrp lng="fr">
    <trd>ciboule</trd>,
    <trd>cive</trd>
  </trdgrp>
  <trd lng="nl">grof <ind>bieslook</ind></trd>
  <trdgrp lng="pl">
    <trd>siedmiolatka</trd>,
    <trd>cebula siedmiolatka</trd>,
    <trd>czosnek d&eogonek;ty</trd>
  </trdgrp>
  <trd lng="pt">cebolinha</trd>
</drv>
</art>

