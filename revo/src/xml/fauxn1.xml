<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fauxn1.xml,v 1.1 2012/08/25 18:10:18 revo Exp $">
<kap>
  <rad>fa&ubreve;n</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fauxn1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fauxn1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="best.0aro.ZOO">bestaro</ref>
    <ekz>
      ni tro rapide veturas <klr>...</klr>
      por vidi la <tld/>on de tiu &ccirc;i &gcirc;angalo
      <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
      <lok>1982</lok></fnt>.
    </ekz>
  </snc>
  <snc mrk="fauxn1.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
       Kataloga verko priskribanta la bestaron de regiono.
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="fauxn.0o">Fa&ubreve;no</ref> anka&ubreve;
    estas nomo de kaprovira figuro.
  </rim>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_w;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">fauna</trd>
  <trdgrp lng="de">
   <trd>Fauna</trd>,
   <trd>Tierwelt</trd></trdgrp>
  <trd lng="en">fauna</trd>
  <trd lng="es">fauna</trd>
  <trd lng="fr">faune<klr tip="ind"> (f&eacute;m.)</klr></trd>
  <trd lng="hu">fauna</trd>
  <trdgrp lng="nl">
    <trd>fauna</trd>,
    <trd>dierenwereld</trd>
  </trdgrp>
  <trd lng="pl">fauna</trd>
  <trd lng="pt">fauna</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_j; &c_m;&c_i;&c_r;</trd>
  </trdgrp>
</drv>
</art>

