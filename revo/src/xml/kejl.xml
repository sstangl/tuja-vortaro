<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kejl.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kejl</rad>/o
</kap>

<drv mrk="kejl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ligna a&ubreve; metala peco, &gcirc;enerale kojnoforma, uzata anstata&ubreve; najlo,
      por perforte &scirc;topi truon a&ubreve; rigide kunfiksi du partopecojn:
      <ekz>
        al&gcirc;ustiga, duobla, kalkana, konkava, kvadrata, ronda <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_v;&c_a;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_y;&c_p;</trd>
  </trdgrp>
  <trd lng="br">ibil</trd>
  <trdgrp lng="cs">
    <trd>klín</trd>,
    <trd>kolík</trd>
  </trdgrp>
  <trd lng="de">Keil</trd>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheville<klr tip="ind"> (de fixation)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;k</trd>,
    <trd>csap</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spie</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klin <klr>(spájací, upevňovací)</klr></trd>,
    <trd>klin <klr>(spájací| upevňovací)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kejl.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Ligi a&ubreve; fiksi per <tld/>o.
    </dif>
  </snc>
  <trd lng="br">ibilia&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>klínovat</trd>,
    <trd>zaklínovat</trd>
  </trdgrp>
  <trd lng="de">verkeilen</trd>
  <trdgrp lng="en">
    <trd>peg</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="fr">cheviller</trd>
  <trdgrp lng="hu">
    <trd>&ouml;ssze&eacute;kel</trd>,
    <trd>&ouml;csapol</trd>
  </trdgrp>
  <trd lng="nl">vastpinnen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_r;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_k;&c_o;&c_m;</ind></trd>
  <trdgrp lng="sk">
    <trd>spájať</trd>,
    <trd>upevňovať pomocou klinov</trd>
  </trdgrp>
</drv>
</art>
