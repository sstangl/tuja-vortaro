<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lic.xml,v 1.12 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lic</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="lic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Esti permesata:
      <ekz>
        nupto ne <tld/>as okaze de la imperia funebro
        <fnt><bib>RugxDom</bib>, <lok>capitro 68a, p. 453a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_a;&c_z;&c_v;&c_o;&c_l;&c_je;&c_n;&c_y;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>být dovoleno</trd>,
    <trd>smět</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>legal sein</trd>,
    <trd>erlaubt sein</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estar permitido, -a</trd>,
    <trd>ser l&iacute;cito, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>l&eacute;gal</ind></trd>,
    <trd>&ecirc;tre <ind>permis</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szabad <klr tip="ind">(vmit csin&aacute;lni)</klr></trd>,
    <trd>megengedett <klr tip="ind">(vmit csin&aacute;lni)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>legaal</ind> zijn<klr tip="ind"> (zeldz.)</klr></trd>,
    <trd><ind>toegelaten</ind> zijn<klr tip="ind"> (zeldz.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ser <ind>permitido</ind></trd>,
    <trd>ser <ind>l&iacute;cito</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_je;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_jo;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>byť dovolené</trd>,
    <trd>byť možné</trd>,
    <trd>smieť sa</trd>
  </trdgrp>
</drv>
</art>
