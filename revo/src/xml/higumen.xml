<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: higumen.xml,v 1.4 2009/09/10 16:32:30 revo Exp $">
<kap><rad>higumen</rad>/o</kap>
<drv mrk="higumen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="igumen.0o">Igumeno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
  <trd lng="hu">ap&aacute;t <klr tip="amb">(pravoszl&aacute;v)</klr></trd>
  <trdgrp lng="it">
    <trd>egumeno</trd>, 
    <trd>abate</trd>
  </trdgrp> 
  <trd lng="pt">superior de um mosteiro</trd> 
  <trd lng="ru">&c_i;&c_g;&c_u;&c_m;&c_je;&c_n;</trd>
</drv>
<drv mrk="higumen.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="igumen.0ino">Igumenino</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="hu">ap&aacute;tn&odblac; <klr tip="amb">(pravoszl&aacute;v)</klr></trd>    
  <trdgrp lng="it">
    <trd>egumenia</trd>, 
    <trd>badessa</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_ja;</trd>
</drv>
</art>

