<art mrk="$Id: agarik.xml,v 1.29 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>agarik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="agarik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fung.lamen0o">Lamenfungo</ref>.
    </dif>
    <trd lng="de">Lamellenpilz</trd>
    <trd lng="hu">lemezesgomba</trd>
    <trd
lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_y;&c_j;
&c_g;&c_r;&c_i;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="agarik1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Agaricus</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Egerling</trd>,
      <trd>Champignon</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csiperke(gomba)</trd>,
      <trd>sampinyon(gomba)</trd></trdgrp>
    <trd lng="ru">&c_sh;&c_a;&c_m;&c_p;&c_i;&c_n;&c_mol;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>pečárka</trd>,
    <trd>žampion</trd>
  </trdgrp>
  <trd lng="en">agaricus</trd>
  <trd lng="fr">agaric</trd>
  <trdgrp lng="it">
      <trd>agarico</trd>,
      <trd>champignon</trd>
    </trdgrp>
  <trd lng="nl">champignon</trd>
  <trd lng="pt">ag&aacute;rico</trd>
  <trdgrp lng="sk">
    <trd>&scaron;ampi&ncaron;&oacute;n</trd>,
    <trd>pečiarka</trd>,
    <trd>šampiňón</trd>
  </trdgrp>
  <trd lng="sv">champinjon</trd>
  <trd lng="tr">&ccedil;ay&inodot;rmantar&inodot;</trd>
</drv>
</art>
