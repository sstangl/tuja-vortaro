<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sanato.xml,v 1.15 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sanatori</rad>/o
</kap>
<drv mrk="sanato.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="san.mal0ulejo">Malsanulejo</ref>,
      en kiu oni kuracas malsanulojn a&ubreve; flegas malfortikulojn
      per longda&ubreve;ra severa higieno kaj per naturaj faktoroj pli ol per
      medikamentoj.  
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_a;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>sanatorium</trd>,
    <trd>sanatoř <klr>(zast.)</klr></trd>
  </trdgrp>
  <trd lng="de">Sanatorium</trd>
  <trd lng="en">sanatorium</trd>
  <trd lng="es">sanatorio</trd>
  <trd lng="fr">sanatorium</trd>
  <trd lng="hu">szanat&oacute;rium</trd>
  <trd lng="it">sanatorio</trd>
  <trd lng="nl">sanatorium</trd>
  <trdgrp lng="pt">
   <trd>sanat&oacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_n;&c_a;&c_t;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">sanatórium</trd>
</drv>
</art>
