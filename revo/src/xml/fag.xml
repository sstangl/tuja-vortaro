<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fag.xml,v 1.29 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fag</rad>/o
</kap>

<drv mrk="fag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fagac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Fagus</trd>), arbo kun triangulaj nuksoj,
      kies ligno estas multe uzata en &ccirc;arpentado.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_k;</trd>
  <trd lng="bg">&c_b;&c_u;&c_k;</trd>
  <trdgrp lng="br">
    <trd>faouenn</trd>,
    <trd>gwezenn-faou</trd>
  </trdgrp>
  <trd lng="ca">faig</trd>
  <trd lng="cs">buk</trd>
  <trd lng="de">Buche</trd>
  <trd lng="en">beech</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_A;&a_sh;</trd>,
    <trd>&a_r;&a_A;&a_sh;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&ecirc;tre</trd>,
    <trd>fau</trd>,
    <trd>fou<klr tip="ind"> (h&ecirc;tre)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>b&uuml;kk</trd>,
    <trd>b&uuml;kkfa</trd></trdgrp>
  <trd lng="nl">beuk</trd>
  <trd lng="pl">buk</trd>
  <trd lng="pt">faia</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;</trd>
  <trd lng="sk">buk</trd>
  <trd lng="sv">bok</trd>
  <trd lng="tr">g&uuml;rgen</trd>
</drv>

</art>
