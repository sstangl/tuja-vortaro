<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nepoti.xml,v 1.16 2015/07/04 08:37:40 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>nepotism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nepoti.0o">
   <kap><tld/>o</kap>
   <snc mrk="nepoti.0o.POL">
     <uzo tip="fak">POL</uzo>
     <dif>
       Troa uzo de sia povo a&ubreve; influo por havigi postenojn 
       a&ubreve; favorojn al sia parencaro:
       <ekz>
         li estas idioto, produkto de nura <tld/>o, kiel multaj aliaj 
         en ilia administrantaro
         <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
         <lok>tk-stafeto, 1982</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>nepotizmus</trd>,
    <trd>příbuzenská protekce</trd>,
    <trd>strýčkování</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Nepotismus</trd>,
     <trd>Vetternwirtschaft</trd>
   </trdgrp>
  <trd lng="en">nepotism</trd>
  <trd lng="es">nepotismo</trd>
  <trd lng="fa">&a_t;&a_b;&a_A;&a_r;&f_g;&a_m;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">n&eacute;potisme</trd>
  <trd lng="hu">nepotizmus</trd>
  <trd lng="nl">nepotisme</trd>
  <trd lng="pt">nepotismo</trd>
  <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_p;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>,
     <trd>&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_k;&c_u;&c_m;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>nepotizmus</trd>,
    <trd>rodinkárstvo</trd>
  </trdgrp>
</drv>
</art>
