<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skolop1.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
<rad>skolopendr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="skolop1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="skolop1.0edoj">
      <tld/>
      edoj</ref>
      (<trd lng="la">Scolopendra</trd>), sudamerikaj miriapodoj,
      ringoformaj, venenaj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_ja;&c_p;&c_e;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="ca">escolopendra</trd>
  <trd lng="cs">stonoha</trd>
  <trdgrp lng="de">
    <trd>Bandassel</trd>,
    <trd>Skolopender</trd>
  </trdgrp>
  <trd lng="hu">szkolopendra</trd>
  <trd lng="nl">duizendpoot</trd>
  <trd lng="oc">escolopendra</trd>
  <trd lng="pl">skolopendra</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_o;&c_p;&c_je;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="sk">stonožka <klr>(jedovatá)</klr></trd>
</drv>

<drv mrk="skolop1.0edoj">
  <kap>
  <tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la klaso
      <ref tip="super" cel="miriap.0oj">miriapodoj</ref>
      (<trd lng="la">Scolopendridae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_ja;&c_p;&c_e;&c_n;&c_d;&c_r;&c_y;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr>
  </trd>
  <trd lng="hu">szkolopendraflk</trd>
  <trdgrp lng="ca">
    <trd>centcames</trd>,
    <trd>centpeus</trd>
  </trdgrp>
  <trd lng="pl">jednoparce</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_o;&c_p;&c_je;&c_n;&c_d;&c_r;&c_y;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr>
  </trd>
</drv>
</art>
