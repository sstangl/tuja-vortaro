<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mikrot.xml,v 1.18 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mikrot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mikrot.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrot.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="mus.kampo0o.ZOO"
      lst="voko:ron&gcirc;uloj">Kampomuso</ref>
      kun malgrandegaj oreloj; <ref tip="lst"
      cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el la familio <ref tip="malprt" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Microtus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_w;&c_k;&c_a; <klr>(&c_m;&c_y;&c_sh;)</klr></trd>
  <trd lng="ca">talp&oacute;</trd>
  <trdgrp lng="cs">
    <trd>hraboš</trd>,
    <trd>hrabošík</trd>
  </trdgrp>
  <trd lng="de">W&uuml;hlmaus</trd>
  <trd lng="en">grass <ind>vole</ind></trd>
  <trd lng="fr">campagnol<klr> (microtus)</klr></trd>
  <trd lng="hu">pocok</trd>
  <trd lng="nl">woelmuis</trd>
  <trdgrp lng="pt">
   <trd>rato silvestre</trd>,
   <trd>microto</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">hraboš</trd>
</drv>
</art>
