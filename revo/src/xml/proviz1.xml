<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: proviz1.xml,v 1.17 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>provizor</rad>/a
</kap>
<drv mrk="proviz1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konsiderata kiel ne longe da&ubreve;ronta, sed 
anstata&ubreve;igota per definitiva;
      atenda:
      <ekz>
        <tld/>a komitato, regularo, ponto;
      </ekz>
      <ekz>
        <tld/>e fiksi la konsiston de l' komisiono.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>dočasný</trd>,
    <trd>provizorní</trd>,
    <trd>prozatímní</trd>,
    <trd>přechodný</trd>
  </trdgrp>
  <trd lng="de">provisorisch</trd>
  <trdgrp lng="en">
    <trd>provisional</trd>,
    <trd>temporary</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>provisional</trd>
  </trdgrp>
  <trd lng="fr">provisoire</trd>
  <trdgrp lng="hu">
    <trd>ideiglenes</trd>,
    <trd>&aacute;tmeneti</trd>,
    <trd>id&odblac;leges</trd>
    <trd>proviz&oacute;rikus</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>provisional</trd>,
    <trd>sementara</trd>,
    <trd>temporer</trd>,
    <trd>tentatif</trd>
  </trdgrp>
  <trd lng="nl">voorlopig</trd>
  <trdgrp lng="pl">
    <trd>prowizoryczny</trd>,
    <trd>przej&sacute;ciowy</trd>,
    <trd>tymczasowy</trd>
  </trdgrp>
  <trd lng="ro">temporar</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dočasný</trd>,
    <trd>predbežný</trd>,
    <trd>provizórny</trd>
  </trdgrp>
  <trd lng="sv">provisorisk</trd>
</drv>
</art>
