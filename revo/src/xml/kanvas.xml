<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanvas.xml,v 1.18 2015/09/26 14:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kanvas</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanvas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanvas.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Lina a&ubreve; kanaba tolo, tre lar&gcirc;ma&scirc;e kaj retece teksita, sur kiu
      oni faras broda&jcirc;on a&ubreve; punton:
      <ekz>
        &scirc;i rekomencis labori kaj klinis la kapon tre 
        proksime al la <tld/>o
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url
          ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_a;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_k;&c_a;&c_n;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">llen&ccedil;</trd>
    <trdgrp lng="de">
      <trd>Kanevas</trd>,
      <trd>Stramin</trd>
    </trdgrp>
    <trd lng="en">canvas</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_r;&a_b;&a_A;&a_s;</trd>,
      <trd>&a_m;&a_t;&a_q;&a_A;&a_l;</trd>,
      <trd>&f_p;&a_A;&a_r;&f_ch;&a_h;&a_hamza_sure; <ind>&f_k;&a_A;&a_n;&a_w;&a_A;&a_d;&a_w;&a_z;&f_y;</ind></trd>
    </trdgrp>
    <trd lng="fr">canevas<klr tip="ind"> (toile)</klr></trd>
    <trd lng="hu">kanav&aacute;sz</trd>
    <trd lng="nl">canvas</trd>
    <trd lng="pl">kanwa</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_h;&c_o;&c_l;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kanvas.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Skizo,
      <ref tip="vid" cel="skem.0o.TEK">skemo</ref>,
      &gcirc;eneralaj trajtoj:
      <ekz>
        jen la <tld/>o, la&ubreve; kiu ni povus labori.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_n;&c_v;&c_a;</trd>
    <trd lng="ca">esb&oacute;s</trd>
    <trdgrp lng="de">
      <trd>Schema</trd>,
      <trd>Entwurf</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>canvas</trd>,
      <trd>setting</trd>, <trd>scope</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_T;&a_r;&a_H;</trd>,
      <trd>&a_z;&a_m;&f_y;&a_n;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>canevas<klr tip="ind"> (plan)</klr></trd>,
      <trd>&eacute;bauche<klr tip="ind"> (d'un ouvrage)</klr></trd>,
      <trd>esquisse</trd>,
      <trd>plan<klr tip="ind"> (d'un ouvrage)</klr></trd>,
      <trd>synopsis</trd>,
      <trd>trame<klr tip="ind"> (d'un ouvrage)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>minta</trd>,
      <trd>sablon</trd>
    </trdgrp>
    <trd lng="nl">stramien</trd>
    <trd lng="pl">kanwa</trd>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_v;&c_a;</trd>
</drv>
</art>
