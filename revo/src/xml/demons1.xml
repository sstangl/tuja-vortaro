  <fnt><bib>PV</bib></fnt>
  <snc mrk="demons1.0o.dedukto">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="demons.0o">demonstro.</ref>
  </snc>
