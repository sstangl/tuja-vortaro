  <trd
  lng="be">&c_d;&c_e;&c_m;&c_a;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Demonstration</trd>
  <trd lng="es">demostraci&oacute;n</trd>
  <trd lng="hu">bizony&iacute;t&aacute;s</trd>
  <trd lng="pt">demonstra&ccedil;&atilde;o</trd>
  <trd
  lng="ru">&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">názorná ukážka</trd>
  <trdgrp lng="cs">
    <trd>demonstrace <klr>(např. síly)</klr></trd>,
    <trd>názorná ukázka</trd>
  </trdgrp>
</drv>
