    <dif>
      Perfekta elpruvo.
    </dif>
    <trdgrp lng="en">
      <trd>demonstration</trd>,
      <trd>proof</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;monstration<klr tip="ind">
    (argumentation)</klr></trd>
    <trd lng="pl">demonstracja<klr> (pokaz)</klr></trd>
