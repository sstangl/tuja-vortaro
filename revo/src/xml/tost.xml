<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tost.xml,v 1.20 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>tost</rad>/o
</kap>
<drv mrk="tost.0o">
  <kap><tld/>o</kap>
  <snc mrk="tost.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Dumfestena propono pri trinko por honoro a&ubreve; sukceso de iu
      a&ubreve; io:
      <ekz>
        sen <tld/>oj, sen paroladoj pasis la tagman&gcirc;o, per kiu
        fini&gcirc;is 
        la soleno
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 7a</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_s;&c_t; <klr>(&c_z;&c_a;
  &c_s;&c_t;&c_a;&c_l;&c_o;&c_m;)</klr></trd>
  <trd lng="bg">&c_t;&c_o;&c_s;&c_t;</trd>
  <trd lng="ca">brindis</trd>
  <trd lng="cs">přípitek</trd>
  <trdgrp lng="de">
    <trd>Trinkspruch</trd>,
    <trd>Tost</trd>
  </trdgrp>
  <trd lng="en">toast<klr tip="amb"> (speech during drink)</klr></trd>
  <trd lng="es">brindis</trd>
  <trd lng="fr">toast</trd>
  <trdgrp lng="hu">
    <trd>poh&aacute;rk&ouml;sz&ouml;nt&odblac;</trd>,
    <trd>&aacute;ldom&aacute;s</trd>,
    <trd>t&oacute;szt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toast</trd>,
    <trd>dronk</trd>,
    <trd>heildronk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>toast</trd>,
    <trd>strzemienny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toste</trd>,
    <trd>brinde</trd>,
    <trd>sa&uacute;de</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_s;&c_t; <klr tip="amb">(&c_z;&c_a;
  &c_s;&c_t;&c_o;&c_l;&c_o;&c_m;)</klr></trd>
  <trdgrp lng="sk">
    <trd>prípitok</trd>,
    <trd>zdravica</trd>
  </trdgrp>
</drv>

<drv mrk="tost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eldiri <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trd lng="bg">&c_v;&c_d;&c_i;&c_g;&c_n;&c_a; <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trd lng="ca">brindar</trd>
  <trd lng="cs">říkat přípitek</trd>
  <trd lng="de">einen <ind>Tost</ind> ausbringen</trd>
  <trd lng="en">toast<klr tip="amb"> (speech during drink)</klr></trd>
  <trd lng="es">brindar</trd>
  <trdgrp lng="fr">
     <trd>trinquer</trd>,
     <trd>porter un toast</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>poh&aacute;rk&ouml;sz&ouml;nt&odblac;t mond</trd>,
    <trd>&aacute;ldom&aacute;st mond</trd>,
    <trd>t&oacute;sztot mond</trd>
  </trdgrp>
  <trd lng="nl">een <ind>toast</ind> uitbrengen</trd>
  <trdgrp lng="pl">
    <trd>wznosi&cacute; <ind>toast</ind></trd>,
    <trd>przypija&cacute;<klr> do kogo&sacute;</klr></trd>
  </trdgrp>
  <trd lng="pt">brindar</trd>
  <trd
  lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_g;&c_l;&c_a;&c_s;&c_i;&c_t;&c_mol;
  <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trdgrp lng="sk">
    <trd>povedať prípitok</trd>,
    <trd>zdravicu</trd>
  </trdgrp>
</drv>
</art>
