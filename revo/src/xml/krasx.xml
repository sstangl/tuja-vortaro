<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krasx.xml,v 1.12 2015/02/15 11:10:23 revo Exp $">
<kap>
  <rad>kra&scirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="krasx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krasx.0i.KOMUNE">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="fal.0i.mallevigxi">Fal</ref>egi kaj
      frakasi&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_c;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a;
    &c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;)</klr></trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_r;&c_p;&c_je;&c_c;&c_mol; <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>estavellar-se</trd>,
    <trd>caure a terra</trd>,
    <trd>desplomar-se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>crasher</ind></trd>,
    <trd>s'<ind>&eacute;craser</ind><klr> (&agrave; terre)</klr></trd>
  </trdgrp>
  <trd lng="hu">lezuhan <klr tip="ind">(rep&uuml;l&odblac;g&eacute;p)</klr></trd>
  <trdgrp lng="it">
    <trd>schiantarsi</trd>,
    <trd>fracassarsi</trd>,
    <trd>sfracellarsi</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>espatifar-se</trd>,
    <trd>esborrachar-se</trd>
  </trdgrp>
  <trd lng="nl">neerstorten</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_o;
    &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_je;)</klr></trd>,
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; <ind>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="krasx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="krasx.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de io fale frakasi&gcirc;anta:
      <ekz>
        &scirc;i kokete retiris sian manon. <tld lit="K"/>! La taso
        <klr>...</klr> plon&gcirc;is sur la plankon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 80a, volumo 2a, p.
        671a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">patapam!</trd>
  <trdgrp lng="fr">
    <trd>badaboum</trd>,
    <trd>patatras</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>puff!</trd>,
    <trd>bumm!</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>badabum</trd>,
    <trd>patatrac</trd>,
    <trd>crash</trd>
  </trdgrp>
  <trd lng="nl">boem!</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_m;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;!</trd>,
    <trd>&c_t;&c_r;&c_a;&c_h;-&c_t;&c_a;&c_r;&c_a;&c_r;&c_a;&c_h;!</trd>
  </trdgrp>
</drv>

<drv mrk="krasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="krasx.0o.KOMUNE">
    <dif>
      Brua, impona <ref tip="super" cel="fal.0o">fal</ref>ego, 
      subita ruini&gcirc;o:
      <ekz>
        misfaroj <klr>...</klr>, kiuj ka&ubreve;zis la
        <ind>borso<tld/>o</ind>n en 1987
        <fnt>
        <aut>D. E. Rogers</aut>,
        <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
        <bib>Monato</bib>,
        <lok>1997:2, p. 14a</lok>
        </fnt>.
        <trd lng="ca">crac borsari</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
  <trd lng="ca">desplomament</trd>
  <trdgrp lng="fr">
    <trd>crash <klr>(falego)</klr></trd>
    <trd>krach <klr>(ruini&gcirc;o)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>krach</trd>,
    <trd>zuhan&aacute;s</trd>,
    <trd>puffan&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>schianto</trd>,
    <trd>fracassamento</trd>,
    <trd>sfracellamento</trd>,
    <trd>disastro<klr> aereo</klr></trd>,
    <trd>crack<klr> (econ.)</klr></trd>,
    <trd>patatrac</trd>
  </trdgrp>
  <trd lng="nl">crash</trd>
  <trd lng="pt">queda ou desastre com avi&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
</drv>
</art>

