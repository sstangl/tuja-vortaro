<kap>
  <rad>himeni</rad>/o
</kap>
<drv mrk="himeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Membrano kovranta la receptaklon de la fungoj kaj kiu portas la
      sporojn.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>blána houby</trd>,
    <trd>hymenium</trd>,
    <trd>rouško houby</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hymenium</trd>,
    <trd>Sporenlager</trd>
  </trdgrp>
  <trd lng="fr">hym&eacute;nium</trd>
  <trdgrp lng="hu">
    <trd>term&odblac;r&eacute;teg<klr tip="ind"> (gomba)</klr></trd>,
    <trd>hymenium</trd>
  </trdgrp>
  <trd lng="nl">hymenium</trd>
  <trdgrp lng="pl">
    <trd>hymenium</trd>, <trd>ob&lstroke;ocznia</trd>
  </trdgrp>
  <trd lng="pt">him&ecirc;nio</trd>
  <trdgrp lng="sk">
    <trd>blana huby</trd>,
    <trd>rúško huby</trd>
  </trdgrp>
</drv>
</art>
