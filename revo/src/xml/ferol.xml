<art mrk="$Id: ferol.xml,v 1.15 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>ferol</rad>/o
</kap>
<drv mrk="ferol.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="apiac.0oj">apiacoj</ref>
       (<trd lng="la">Ferula</trd>), vivanta precipe en mediteranea
       regiono.
       La radiko de iuj specioj enhavas <ref
       cel="asafet.0o">asafetidon</ref>,
       malbonodoran guman rezinon.
     </dif>
   </snc>
  <trd lng="be">&c_f;&c_e;&c_r;&c_u;&c_l;&c_a;</trd>
  <trd lng="cs">metlovka</trd>
  <trdgrp lng="de">
     <trd>Asant</trd>,
     <trd>Teufelsdreck</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>stinking gum</trd>,
     <trd>devil's dung</trd>,
     <trd>giant <ind>fennel</ind></trd>
   </trdgrp>
  <trd lng="fr">f&eacute;rule</trd>
  <trd lng="hu">&ouml;rd&ouml;ggy&ouml;k&eacute;r</trd>
  <trdgrp lng="nl">
     <trd>godenvoedsel</trd>,
     <trd>duivelsdrek</trd>
   </trdgrp>
  <trd lng="pt">f&eacute;rula</trd>
  <trd lng="ru">&c_f;&c_je;&c_r;&c_u;&c_l;&c_a;</trd>
  <trd lng="sk">ƒçertovo lajno</trd>
</drv>
</art>
