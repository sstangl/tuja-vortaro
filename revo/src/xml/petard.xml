<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: petard.xml,v 1.16 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>petard</rad>/o
</kap>
<drv mrk="petard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malforta eksplodilo, konsistanta el pulvo entenata en kartona tubo
      kaj uzata kiel signalo a&ubreve; amuza&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_t;&c_a;&c_r;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>petarda</trd>,
    <trd>signální raketa</trd>
  </trdgrp>
  <trd lng="de">Feuerwerksk&ouml;rper</trd>
  <trdgrp lng="en">
    <trd>petard</trd>,
    <trd>detonator</trd>,
    <trd>firecracker</trd>
  </trdgrp>
  <trd lng="es">petardo</trd>
  <trd lng="fr">p&eacute;tard</trd>
  <trd lng="hu">pet&aacute;rda</trd>
  <trd lng="nl">voetzoeker</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_t;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_r;&c_y;&c_v;&c_n;&c_a;&c_ja; &c_sh;&c_a;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>alebo zábavná náložka</trd>,
    <trd>signalizačná</trd>
  </trdgrp>
</drv>
</art>
