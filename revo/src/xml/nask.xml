    <drv mrk="nask.du0ito">
      <kap>du<tld/>ito</kap>
      <snc mrk="nask.du0ito.GEN">
        <uzo tip="fak">GEN</uzo>
        <fnt><bib>PV</bib><lok>&gcirc;emelo</lok></fnt>
        <dif>
          &Ccirc;iu el la du idoj, <tld/>itaj &ccirc;e la sama aku&scirc;o:
          <ekz>
            via du<tld/>ito, malfeli&ccirc;a luanto de la bienoj de la
            kronprinco
            estas ruinigita
            <fnt><bib>Far1</bib><lok>&ccirc;ap. 13</lok></fnt>;
          </ekz><ekz>
            same vi falis en kampoj Rutulaj, Larido kaj Timbro, vi
            du<tld/>itoj
            de Da&ubreve;ko, kaj tiel similaj, ke ne vin malsimiligis gepatroj
            <fnt><aut>H. Vallienne</aut><vrk>&Ccirc;u li?</vrk>
              <lok>Postparolo</lok></fnt>;
          </ekz><ekz>
            unu el la du du<tld/>itoj havis laman piedeton
            <klr>[...]</klr> kaj tiu
            alia havis sur la piedo ses fingrojn
            <fnt><aut>J. Ha&scaron;ek trad V. V&aacute;&ncaron;a</aut>
              <vrk>La brava soldato &Scirc;vejk</vrk>
              <lok><url
 ref="http://i-espero.info/files/elibroj/eo%20-%20hasek,%20jaroslav%20-%20la%20brava%20soldato%20svejk%204.pdf"
 >http://i-espero.info/files/elibroj/eo%20-%20hasek,%20jaroslav%20-%20la%20brava%20soldato%20svejk%204.pdf</url></lok></fnt>;
          </ekz>
          kaj analoge:
          <ekz>
            <ind>tri<tld/>ito</ind>,
            <trd lng="fr">tripl&eacute;<klr tip="ind"> (enfant)</klr></trd>
          </ekz><ekz>
            <ind>kvar<tld/>ito</ind>,
            <trd lng="fr">quadrupl&eacute;<klr tip="ind"> (enfant)</klr></trd>
          </ekz><ekz>
            <ind>kvin<tld/>ito</ind>,
            <trd lng="fr">quintupl&eacute;<klr tip="ind"> (enfant)</klr></trd>
          </ekz><ekz>
            <ind>ses<tld/>ito</ind>...
            <trd lng="fr">sextupl&eacute;<klr tip="ind"> (enfant)</klr></trd>
          </ekz>
        </dif>
        <ref tip="sin" cel="gxemel.0o.dunaskito">&gcirc;emelo</ref>
      </snc>
  <trd lng="cs">dvojče</trd>
  <trdgrp lng="es">
        <trd>gemelo</trd>,
        <trd>mellizo</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>jumeau<klr tip="ind"> (subst., enfant)</klr></trd>,
        <trd>besson</trd>
      </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_j;&c_n;&c_ja;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">dvojča</trd>
</drv>

