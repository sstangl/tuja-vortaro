    <drv mrk="nask.de0akun0ita">
      <kap>de<tld/>a, <var><kap>kun<tld/>ita</kap></var></kap>
      <snc>
        <dif>
          Ekzistanta jam de la <tld/>i&gcirc;o:
          <ekz>
            de<tld/>a surdeco;
          </ekz><ekz>
            lia de<tld/>i&gcirc;a bona humoro
            <fnt><bib>Fab4</bib>, <lok>pupludisto</lok></fnt>;
          </ekz><ekz>
            mi havas de<tld/>an pasion spiti
            <fnt><bib>TermKurs</bib></fnt>.
          </ekz>
        </dif>
        <trd
          lng="be">&c_p;&c_r;&c_y;&c_r;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_y;</trd>
