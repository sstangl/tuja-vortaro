          <trd>&c_p;&c_r;&c_i;&c_r;&c_o;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Asimilita jam en la frua infana&gcirc;o de la medio, precipe de la
          familio:
          <ekz>
            oni uzas tamen <ctl>gepatra lingvo</ctl> anka&ubreve;
            kiel &gcirc;eneralan esprimon por de<tld/>a lingvo,
            &ccirc;ar tian lingvon oni plej ofte lernas de amba&ubreve; gepatroj
            <fnt>PMEG</fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
        <trd lng="hu">sz&uuml;let&eacute;si</trd>
        <trdgrp lng="ru">
          <trd>&c_r;&c_o;&c_d;&c_n;&c_o;&c_j;</trd>,
          <trd>&c_p;&c_r;&c_i;&c_r;&c_o;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <trd lng="br">enganet</trd>
      <trd lng="de">angeboren</trd>
      <trdgrp lng="es">
        <trd>cong&eacute;nito</trd>,
        <trd>connatural</trd>,
        <trd>de nacimiento</trd>
      </trdgrp>
