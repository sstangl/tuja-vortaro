<vortaro>
  <art mrk="$Id: trip.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>trip</rad>/o
    </kap>

    <drv mrk="trip.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
      <uzo tip="fak">KUI</uzo>
        <dif>
          Man&gcirc;eblaj internaj organoj de la bestoj, kiujn oni opinias
          malpli valoraj ol <ref cel="viand.0o">viando</ref>; ekz-e
          <refgrp tip="prt">
            <ref cel="intest.0o">intestoj</ref>,
            <ref cel="stomak.0o">stomako</ref>,
            <ref cel="hepat.0o">hepato</ref>,
            <ref cel="kor.0o">koro</ref>,
            <ref cel="lang.0o">lango</ref>
          </refgrp>:
          <ekz>
            &leftquot;Poste mi iris a&ccirc;eti la kolombidojn <klr>...</klr>
            kaj mi rostis ilin, kaj por vi mi pretigis la supon
            el la kolombaj <tld/>oj&rightquot;
            <fnt>
              <aut>Karel &Ccaron;apek, tr. Josef Vondrou&scaron;ek</aut>:
              <vrk>Marta kaj Maria</vrk>,
              <lok>Libro de apokrifoj</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_t;&c_r;&c_y;&c_b;&c_u;&c_h;&c_ib;</trd>,
        <trd>&c_v;&c_a;&c_n;&c_t;&c_r;&c_o;&c_b;&c_y;</trd>
      </trdgrp>
  <trd lng="ca">budells</trd>
  <trdgrp lng="cs">
    <trd>drůbky</trd>,
    <trd>držky</trd>,
    <trd>vnitřnosti</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Eingeweide</trd>,
        <trd>Kaldaunen</trd>,
        <trd>Kutteln</trd>
      </trdgrp>
  <trd lng="en">tripe</trd>
  <trdgrp lng="fa">
        <trd>&a_sh;&f_k;&a_n;&a_b;&a_h;</trd>,
        <trd>&a_s;&f_y;&a_r;&a_A;&a_b;&f_y;</trd>,
        <trd>&a_d;&a_l; &a_w; &a_r;&a_w;&a_d;&a_h;</trd>
      </trdgrp>
  <trd lng="fr">tripes</trd>
  <trdgrp lng="hu">
        <trd>zsigerek</trd>,
        <trd>bels&odblac;s&eacute;g</trd>,
        <trd>pacal</trd>
      </trdgrp>
  <trd lng="nl">pens</trd>
  <trd lng="pl">podroby</trd>
  <trd lng="pt">tripa</trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_r;&c_o;&c_h;&c_a;</trd>,
        <trd>&c_t;&c_r;&c_je;&c_b;&c_u;&c_h;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>droby</trd>,
    <trd>jedlé črevá zvierat</trd>,
    <trd>vnútornosti</trd>
  </trdgrp>
</drv>
  </art>
