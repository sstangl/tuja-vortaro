<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sordid.xml,v 1.4 2015/09/16 14:10:20 revo Exp $">
<kap>
  <rad>sordid</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sordid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sordid.0a.KOMUNE">
    <ref tip="dif" cel="pur.mal0a.KOMUNE">malpurega</ref>
    <ekz>
      magistrato malkonstruis la <tld/>ajn apartamentojn 
      kaj konstruigis pli bonajn domarojn
      <fnt>
        <aut>G. Macaoidh</aut>,
        <vrk><url ref="http://www.esperanto.be/fel/2006/008699.php">
        Kara antikva malpura Dublino</url></vrk>,
        <bib>Monato</bib>,
        <lok>2006:07, p. 13a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="en">
    <trd>filthy</trd>,
    <trd>sordid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neomalen&yacute;</trd>,
    <trd>podl&yacute;</trd>,
    <trd>sprost&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
  <trd lng="fr">sordide</trd>
  <trdgrp lng="sk">
    <trd>podl&yacute;</trd>,
    <trd>uf&uacute;&lcaron;an&yacute;</trd>,
    <trd>&scaron;pinav&yacute;</trd>
  </trdgrp>
</drv>
</art>

