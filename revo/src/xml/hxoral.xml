<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id: hxoral.xml,v 1.11 2013/03/22 14:10:14 revo Exp $">
     <kap><rad>&hcirc;oral</rad>/o</kap>

     <drv mrk="hxoral.0o">
       <kap><tld/>o</kap>
       <snc>
         <uzo tip="fak">KRI</uzo>
         <uzo tip="fak">MUZ</uzo>
         <dif>
           En la okcidenta kristana diservo, precipe &ccirc;e la 
           luteranoj kaj romkatolikoj, malrapida
           <ref tip="super" cel="hxor.0ajxo">&hcirc;ora&jcirc;o</ref> kiun
           kunkantas &ccirc;iuj &ccirc;eestantoj:
           <ekz>
             la <tld/>oj de J. S. Ba&hcirc;o;
           </ekz>
           <ekz>
             <uzo tip="fak">KAT</uzo>
             la gregoriaj <tld/>oj;
           </ekz><ekz>
             &scirc;ajne mi a&ubreve;dis akordojn de solena <tld/>o
             &scirc;vebi tra la aero
             <fnt>
               <aut>E.T.A. Hoffmann, tr. Wolfgang Kirschstein</aut>:
               <vrk>Konsilisto Krespel</vrk>.
               <lok>Kaliningrad: Sezonoj, 2001.
         &#8208; (Serio &leftquot;Germana literaturo&rightquot;, vol.1a)</lok>
             </fnt>.
           </ekz>
         </dif>
       </snc>
       <trd lng="be">&c_h;&c_a;&c_r;&c_e;&c_ja;</trd>
       <trd lng="bg">&c_h;&c_o;&c_r;&c_a;&c_l;</trd>
       <trd lng="ca">coral</trd>
       <trd lng="de">Choral</trd>
       <trd lng="en">choral</trd>
       <trd lng="fi">koraali</trd>
       <trdgrp lng="hu">
         <trd>kor&aacute;l</trd>,
         <trd>egyh&aacute;zi <ind>&eacute;nek</ind></trd>
       </trdgrp>
       <trd lng="pl">chora&lstroke;</trd>
       <trd lng="pt">coral <klr tip="amb">(m&uacute;s.)</klr></trd>
       <trd lng="ru">&c_h;&c_o;&c_r;&c_a;&c_l;</trd>
       <trdgrp lng="it">
         <trd>canto<klr> (corale, religioso)</klr></trd>, 
         <trd>corale<klr> (sost.)</klr></trd>
       </trdgrp>
       <trd lng="fr">chorale</trd>
     </drv>

   </art>

