<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxu.xml,v 1.42 2015/05/25 02:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;u</rad>/o
</kap>

<drv mrk="sxu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxu.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.pied0o">Piedvesto</ref> kun solida
      plandumo, ta&ubreve;ga por uzo eksterhejma:
      <ekz>
        <ind>&ccirc;e botisto la <tld/>o estas &ccirc;iam kun truo</ind>
        <trd lng="fr">c'est le cordonnier le plus mal
        chauss&ecirc;</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>esti sub la <tld/>o de</ind> sia edzino
        <trd lng="fr">&ecirc;tre &agrave; la botte de</trd>
        <klr>(humile kaj sklave obei)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>trovi &gcirc;ustmezuran <tld/>on</ind>
        <klr>(trovi ion a&ubreve; iun konforman al siaj deziroj)</klr>
        <trd lng="fr">trouver chaussure &agrave; son pied</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_H;&a_kasra;&a_dh;&a_fatha_A;&a_hamza;&a_dammatan; &a_j;
  &a_A_hamza_sure;&a_fatha;&a_H;&a_sukun;&a_dh;&a_kasra_y;&a_fatha;&a_t_marbuta;&a_dammatan;
</trd>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_f;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_u;&c_v;&c_k;&c_a;</trd>
  <trd lng="ca">sabata</trd>
  <trdgrp lng="cs">
    <trd>polobotka</trd>,
    <trd>st&rcaron;ev&iacute;c</trd>
  </trdgrp>
  <trd lng="de">Schuh</trd>
  <trd lng="el">
&pi;&alfa;&pi;&omikron;&ypsilon_ton;&tau;&sigma;&jota;</trd>
  <trd lng="en">shoe</trd>
  <trd lng="es">zapato</trd>
  <trdgrp lng="fr">
    <trd>soulier</trd>,
    <trd>chaussure</trd>
  </trdgrp>
  <trd lng="he">&nun;&ayin;&lamed;</trd>
  <trd lng="hu">cip&odblac;</trd>
  <trd lng="id">sepatu</trd>
  <trd lng="it">scarpa</trd>
  <trd lng="nl">schoen</trd>
  <trd lng="oc">sabata</trd>
  <trdgrp lng="pl">
    <trd>but</trd>,
    <trd>p&oacute;&lstroke;but</trd>,
    <trd>trzewik</trd>
  </trdgrp>
  <trd lng="pt">sapato</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_t;&c_i;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_f;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">sko</trd>
  <trd lng="tr">ayakkab&inodot;</trd>
  <trd lng="vo">juk</trd>
</drv>

<drv mrk="sxu.danc0o">
  <kap>danc<tld/>o</kap>
  <snc mrk="sxu.danc0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      speciale farita por danco:
      <ekz>
        &leftquot;vidu, kiaj belegaj danc<tld/>oj!&rightquot;
        <fnt><bib>Fab2</bib>, <lok>ru&gcirc;aj &scirc;uoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tanzschuh</trd>
  <trdgrp lng="en">
    <trd>dance shoe</trd>,
    <trd>ballet shoe</trd>,,
    <trd>pointe shoe</trd>
    <trd>ghillie</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ballerine</trd>,
    <trd>chaussure de dance</trd>
  </trdgrp>
  <trd lng="it">scarpa<klr> da ballo</klr></trd>
  <trdgrp lng="pl">
    <trd>buty do ta&nacute;ca</trd>,
    <trd>baleriny</trd>,
    <trd>baletki<klr> (buty do baletu)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxu.glit0o">
  <kap>glit<tld/>o</kap>
  <snc mrk="sxu.glit0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun suba lameno por glitado
      sur glacio.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de gel)</klr></trd>
  <trd lng="de">Schlittschuh</trd>
  <trd lng="en">ice skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de hielo)</klr></trd>
  <trd lng="fr">patin &agrave; glace</trd>
  <trd lng="he">&mem;&het;&lamed;&yod;&qof;&yod;&yod;&fmem;<klr>
  (&yod;&het;&yod;&dalet;)</klr></trd>
  <trd lng="hu">korcsolya</trd>
  <trd lng="it">pattino<klr> da ghiaccio</klr></trd>
  <trdgrp lng="nl">
    <trd>ijsschaats</trd>,
    <trd>schaats</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;y&zdot;wa<klr> z butem</klr></trd>
  <trd lng="pt">patim</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">skridsko</trd>
</drv>

<drv mrk="sxu.ligno0o">
  <kap>ligno<tld/>o, <var><kap>ligna <tld/>o</kap></var></kap>
  <snc mrk="sxu.ligno0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      farita tute
      el ligno:
      <ekz>
        li estis lerta ligno<tld/>isto
        <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda
        kristino</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galosx.0o">galo&scirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>esclop</trd>,
    <trd>galotxa</trd>
  </trdgrp>
  <trd lng="de">Holzschuh</trd>
  <trdgrp lng="en">
    <trd>sabot</trd>,
    <trd>wooden shoe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>zueco <klr>(de madera)</klr></trd>,
    <trd>zapato de madera</trd>
  </trdgrp>
  <trd lng="fr">sabot</trd>
  <trdgrp lng="he">
    <trd>&qof;&bet;&qof;&bet;</trd>,
    <trd>&nun;&ayin;&lamed; &ayin;&ftsadi;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>facip&odblac;</trd>,
    <trd>klumpa</trd>
  </trdgrp>
  <trd lng="it">zoccolo</trd>
  <trd lng="nl">klomp</trd>
  <trdgrp lng="pl">
    <trd>kierpc<klr> (reg.)</klr></trd>,
    <trd>mokasyny</trd>
  </trdgrp>
</drv>

<drv mrk="sxu.negx0o">
  <kap>ne&gcirc;<tld/>o</kap>
  <snc mrk="sxu.negx0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
    Rakedforma ilo por promeni sur ne&gcirc;o.
    </dif>
  <trd lng="en">snowshoe</trd>
  <trd lng="es">raqueta de nieve</trd>
   <trdgrp lng="it">
    <trd>ciaspola</trd>,
    <trd><ind>racchetta</ind> da neve</trd>
  </trdgrp>
    </snc>
</drv>
    
<drv mrk="sxu.rad0o">
  <kap>rad<tld/>o</kap>
  <snc mrk="sxu.rad0o.rulsxuo">
    <uzo tip="fak">SPO</uzo>
    <ref tip="dif" cel="sxu.rul0o.SPO">rul<tld/>o</ref>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;
  &c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de rodes)</klr></trd>
  <trd lng="de">Rollschuh</trd>
  <trd lng="en">roller skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de ruedas)</klr></trd>
 <trdgrp lng="fr">
    <trd>patin &agrave; roulettes</trd>,
    <trd>roller</trd>
  </trdgrp>
  <trd lng="he">&gimel;&lamed;&gimel;&yod;&lamed;&yod;&tav;</trd>
  <trd lng="hu">g&ouml;rkorcsolya</trd>
  <trd lng="it">pattino<klr> a rotelle</klr></trd>
  <trd lng="nl">rolschaats</trd>
  <trd lng="pl">wrotka<klr> z butem</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j; &c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">rullskridsko</trd>
</drv>

<drv mrk="sxu.rul0o">
  <kap>rul<tld/>o</kap>
  <snc mrk="sxu.rul0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun radetoj, por glitado sur
      glata grundo:
      <ekz>
        uzo de rultabuloj, skutiloj kaj rul<tld/>oj estas malpermesata
        en la &gcirc;ardenaro
        <fnt>
        <aut>A&ubreve;stralia Nacia Botanika &Gcirc;ardenaro, trad. D. Broadribb
        k.a.</aut>,
        <vrk>
<url
ref="http://www.anbg.gov.au/anbg/esperanto/index.html">Bonvenon...</url>
</vrk>,
        <lok>2002-06-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;
  &c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de rodes)</klr></trd>
  <trd lng="de">Rollschuh</trd>
  <trd lng="en">roller skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de ruedas)</klr></trd>
  <trdgrp lng="fr">
    <trd>patin &agrave; roulettes</trd>,
    <trd>roller</trd>
  </trdgrp>
  <trd lng="he">&gimel;&lamed;&gimel;&yod;&lamed;&yod;&tav;</trd>
  <trd lng="hu">g&ouml;rkorcsolya</trd>
  <trd lng="it">pattino<klr> a rotelle</klr></trd>
  <trd lng="nl">rolschaats</trd>
  <trd lng="pl">wrotka<klr> z butem</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;
  &c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">rullskridsko</trd>
</drv>

<drv mrk="sxu.tol0o">
  <kap>tol<tld/>o</kap>
  <snc mrk="sxu.tol0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      tute
      a&ubreve; grandparte el tolo:
      <ekz>
        La viroj kun falditaj pantalonoj, &ccirc;emizoj de konataj
        markoj,
        <klr>...</klr> tol<tld/>oj anka&ubreve; de konataj markoj
        <fnt>
        <aut>-</aut>,
        <vrk>
<url ref="http://www.vilaweb.com/www/especials/cronica?p_idcmp=570421">
Mogao-grotoj</url></vrk>,
        <lok>vilaweb, [vidita je 2006-01-31]</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradiciaj tol<tld/>oj kaj vasta pantalono montras lin tipa
        pekinano
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk>
<url
ref="http://es1.chinabroadcast.cn/1/2004/01/07/1@2474.htm">Triciklisto
        de Pekino</url></vrk>,
        <lok>2004-01-07</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>sneaker<klr tip="amb"> (shoe)</klr></trd>,
    <trd>canvas shoe</trd>,
    <trd>athletic shoe</trd>,
    <trd>running shoe</trd>,
    <trd>runner<klr tip="amb"> (shoe)</klr></trd>,
   <trd>tennis shoe</trd>,
    <trd>boat shoe</trd>,
    <trd>gym shoe</trd>,
    <trd>deck shoe</trd>,
    <trd>bowling shoe</trd>
  </trdgrp>
 <trdgrp lng="fr">
    <trd>basket<klr tip="amb"> (chaussure)</klr></trd>,
    <trd>chaussure de toile</trd>,
    <trd>tennis<klr tip="amb"> (chaussure)</klr></trd>
  </trdgrp>
  <trd lng="it">scarpa<klr> di tela</klr></trd>
  <trdgrp lng="pl">
    <trd>tenis&oacute;wek</trd>,
    <trd>&lstroke;ape&cacute;</trd>,
    <trd>walonka</trd>
  </trdgrp>
  </snc>
</drv>
</art>
