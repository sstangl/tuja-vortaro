<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karuse.xml,v 1.22 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karusel</rad>/o
</kap>
<drv mrk="karuse.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="karuse.0o.rajda">
    <dif>
      Festa turniro, kie kavaliroj a&ubreve; rajdistoj turni&gcirc;e kaj
      artisme manovras.
    </dif>
    <trd lng="ca">carrusel</trd>
    <trdgrp lng="de">
      <trd>Karussellreiten</trd>,
      <trd>Ritterspiel mit Ringstechen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>carrousel</trd>
    </trdgrp>
    <trd lng="en">carousel</trd>
    <trd lng="es">carrusel</trd>
    <trdgrp lng="hu">
      <trd>lovasj&aacute;t&eacute;k</trd>,
      <trd>karusszel</trd>
    </trdgrp>
    <trd lng="nl">caroussel</trd>
    <trd lng="pl">karuzel</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_r;&c_u;&c_s;&c_je;&c_l;&c_mol;<klr tip="ind"> 
      (&c_k;&c_o;&c_n;&c_n;&c_o;&c_je; 
      &c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je;)</klr></trd>,
      <trd>&c_k;&c_o;&c_n;&c_n;&c_o;&c_je; 
      &c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="karuse.0o.infana">
    <dif>
      Porinfana amuzilo, konsistanta el lignaj rajdbestoj kaj 
      se&gcirc;oj, metitaj sur granda ronda plata&jcirc;o, kiu 
      turni&gcirc;as &ccirc;irka&ubreve; sia akso.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_u;&c_s;&c_e;&c_l;&c_mol;</trd>
    <trd lng="br">kazeg-vezevenn</trd>
    <trd lng="ca">cavallets</trd>
    <trdgrp lng="de">
      <trd>Karussell</trd>,
      <trd>Ringelspiel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>carousel</trd>,
      <trd>merry-go-round</trd>
    </trdgrp>
    <trd lng="fr">man&egrave;ge<klr tip="ind"> (forain)</klr></trd>
    <trdgrp lng="hu">
      <trd>k&ouml;rhinta</trd>,
      <trd>ringlispil</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>caroussel</trd>,
      <trd>draaimolen</trd>,
      <trd>paardenmolen</trd>
    </trdgrp>
    <trd lng="pl">karuzela</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_u;&c_s;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trd lng="cs">cyklická obsluha <klr>(několika procesů procesorem)</klr></trd>
  <trd lng="sk">cyklická obsluha <klr>(niekoľkých procesov procesorom)</klr></trd>
</drv>

<drv mrk="karuse.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">FIG</uzo>
  <snc mrk="karuse.0i.FIG">
    <dif>
      Alternadi, ade kaj senpa&ubreve;ze kirli&gcirc;i kaj 
      &scirc;an&gcirc;i&gcirc;i:
      <ekz>
        dubo <tld/>is en lia kapo
        <fnt>
          <bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 251a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kalejd.0i">kalejdoskopi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="svarm.0i">svarmi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>embrollar-se</trd>,
    <trd>remenar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>točit se</trd>,
    <trd>vézt se na kolotoči</trd>,
    <trd>vířit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>bousculer</ind><klr tip="ind"> (fig.)</klr></trd>,
    <trd>s'<ind>embrouiller</ind><klr tip="ind"> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">ronddraaien</trd>
  <trdgrp lng="pl">
    <trd>kr&eogonek;ci&cacute;</trd>,
    <trd>miesza&cacute;</trd>,
    <trd>m&aogonek;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_s;&c_t;&c_r;&c_o; 
    <ind>&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>,
    <trd>&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_v; 
    <ind>&c_k;&c_r;&c_u;&c_g;&c_o;&c_v;&c_je;&c_r;&c_t;&c_i;</ind></trd>
  </trdgrp>
  <trd lng="sk">zúčastniť sa na jazdeckom turnaji</trd>
</drv>
</art>
