<vortaro>
<art mrk="$Id: kompen.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kompendi</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="kompen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompen.0o.BELE">
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref> resumanta
      la tuton de scienco a&ubreve; doktrino:
      <ekz>
        la <ctl>Dida&hcirc;eo</ctl> <klr>...</klr> estas, por tiel diri, 
        <tld/>o pri eklezia vivo
        <fnt><aut>trad. G. Berveling</aut>, <vrk><url 
        ref="http://www.esperanto.gr/lit/els/did.pdf">la dida&hcirc;eo</url>, 
        enkonduko. - IABO, 1979</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="epitom.0o">epitomo</ref>,
    <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trd lng="ca">compendi</trd>
  <trdgrp lng="de">
    <trd>Kompendium</trd>,
    <trd>Abriss</trd>,
    <trd>Handbuch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compendium</trd>,
    <trd><klr>concise but comprehensive </klr>abridgment</trd>
  </trdgrp>
  <trd lng="es">compendio</trd>
  <trdgrp lng="fr">
    <trd>compendium</trd>,
    <trd>abr&eacute;g&eacute;</trd>,
    <trd>aide-m&eacute;moire</trd>,
    <trd>&eacute;pitom&eacute;</trd>,
    <trd>introduction<klr tip="ind"> (ouvrage)</klr></trd>,
    <trd>m&eacute;mento</trd>,
    <trd>vade-mecum</trd>
  </trdgrp>
  <trd lng="nl">compendium</trd>
  <trdgrp lng="pt">
    <trd>comp&ecirc;ndio</trd>,
    <trd>resumo</trd>,
    <trd>ep&iacute;tome</trd>,
    <trd>manual</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>manuál</trd>,
    <trd>príručka</trd>,
    <trd>rukoväť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kompendium</trd>,
    <trd>příručka</trd>
  </trdgrp>
</drv>
</art>
