<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volb.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>volb</rad>/o
</kap>
<drv mrk="volb.0o">
  <kap><tld/>o</kap>
  <snc mrk="volb.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Arka&jcirc;o el vicoj de &scirc;tonoj, kiuj konsistigas murojn kaj supre
      kuni&gcirc;as
      en kurba <ref tip="super" cel="plafon.0o.ARKI">plafono</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la blua &ccirc;ielo <tld/>i&gcirc;is super ni;
      </ekz>
      <ekz>
        ponto kun rondaj arkaj <tld/>oj
        <fnt><bib>Kiso</bib>, <lok>la brulanta urbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_s;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">volta</trd>
  <trd lng="cs">klenba</trd>
  <trd lng="de">Gew&ouml;lbe</trd>
  <trdgrp lng="el">
    <trd>&theta;&omikron_ton;&lambda;&omikron;&sigma_fina;</trd>,
    <trd>&tau;&rho;&omikron;&ypsilon_ton;&lambda;&lambda;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vault</trd>,
    <trd>arched roof</trd>
  </trdgrp>
  <trd lng="es">b&oacute;veda</trd>
  <trdgrp lng="fr">
    <trd>voute</trd>
  </trdgrp>
  <trd lng="hu">boltozat</trd>
  <trd lng="nl">gewelf</trd>
  <trd lng="pt">ab&oacute;bada</trd>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_d; <klr tip="amb">(&c_a;&c_r;&c_h;&c_i;&c_t;.)</klr></trd>
  <trd lng="sk">klenba</trd>
</drv>
</art>
