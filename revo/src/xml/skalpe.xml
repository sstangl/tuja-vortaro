<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skalpe.xml,v 1.16 2015/09/30 12:10:15 revo Exp $">
<kap>
  <rad>skalpel</rad>/o
</kap>
<drv mrk="skalpe.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Malgranda tran&ccirc;ilo por sekci.
    </dif>
    <ref tip="vid" cel="bistur.0o">bisturio</ref>
  </snc>
  <trd lng="lat">scalprum</trd>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_k;&c_a;&c_l;&c_p;&c_je;&c_l;</trd>
  <trd lng="ca">escalpel</trd>
  <trd lng="cs">skalpel</trd>
  <trd lng="de">Skalpell</trd>
  <trd lng="en">scalpel</trd>
  <trd lng="fr">scalpel</trd>
  <trdgrp lng="hu">
    <trd>szike</trd>,
    <trd>bonck&eacute;s</trd>
  </trdgrp>
  <trd lng="it">scalpello<klr> (coltello chirurgico)</klr></trd>
  <trd lng="la">scalprum</trd>
  <trdgrp lng="nl">
    <trd>scalpel</trd>,
    <trd>ontleedmes</trd>
  </trdgrp>
  <trd lng="pl">skalpel</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>opera&ccaron;n&yacute; n&ocirc;&zcaron;</trd>,
    <trd>skalpel</trd>
  </trdgrp>
</drv>
</art>
