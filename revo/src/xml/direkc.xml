<art mrk="$Id: direkc.xml,v 1.16 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>direkci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="direkc.0o">
  <kap><tld/>o</kap>
  <snc mrk="direkc.0o.estraro">
    <dif>
      Direktantaro, estraro:
      <ekz>
        <tld/>o de la zoo ne &scirc;atis vidi, kiam gardistoj instruis
        siajn 
        bestojn por prezentadoj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 9a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Direktion</trd>,
      <trd>Leitung</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dyrekcja</trd>,
      <trd>szefostwo<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="direkc.0o.estrejo">
    <dif>
      Ejo okupata de direktantaro.
    </dif>
    <trdgrp lng="de">
      <trd>Direktionsr&auml;ume</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dyrekcja</trd>,
      <trd>gabinet <ind>dyrektora</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_r;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>direkce</trd>,
    <trd>vedení podniku</trd>,
    <trd>ředitelství</trd>
  </trdgrp>
  <trd lng="en">directorate</trd>
  <trd lng="es">direcci&oacute;n</trd>
  <trd lng="fr">direction<klr tip="ind"> (services du
    directeur)</klr></trd>
  <trd lng="hu">igazgat&oacute;s&aacute;g<klr tip="ind">
  (&eacute;p&uuml;let is)</klr></trd>
  <trdgrp lng="pt">
    <trd>diretoria</trd>,
    <trd>dire&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_r;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">riaditeľstvo</trd>
</drv>
</art>
