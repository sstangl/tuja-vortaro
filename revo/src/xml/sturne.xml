<kap>
  <rad>sturned</rad>/oj
</kap>
<drv mrk="sturne.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sturnidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">est&uacute;rnids</trd>
  <trd lng="cs">špačkovití</trd>
  <trd lng="de">Stare<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">sturnid&eacute;s</trd>
  <trd lng="hu">sereg&eacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="nl">spreeuwen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_v;&c_o;&c_r;&c_c;&c_y;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_v;&c_o;&c_r;&c_c;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">škorcovité</trd>
</drv>
</art>
