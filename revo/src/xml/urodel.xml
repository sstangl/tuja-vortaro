<kap>
   <rad>urodel</rad>/oj
</kap>
<drv mrk="urodel.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Ordo el la klaso <ref tip="super" cel="amfibi.0oj">amfibioj</ref>
       (<trd lng="la">Urodela</trd>), havantaj voston.
     </dif>
   </snc>
   <trd lng="be">&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_t;&c_y;&c_ja;<klr tip="amb"> (&c_a;&c_t;&c_r;&c_a;&c_d;
   &c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;&c_h;)</klr></trd>
   <trd lng="bg">&c_o;&c_p;&c_a;&c_sh;&c_a;&c_t;&c_i; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_i;</trd>
   <trd lng="ca">urodels</trd>
   <trd lng="de">Schwanzlurche</trd>
   <trd lng="en">salamanders</trd>
   <trd lng="es">urodelos</trd>
   <trd lng="fi">pyrst&ouml;sammakot</trd>
   <trd lng="fr">urod&egrave;les</trd>
   <trd lng="hu">farkos <ind>k&eacute;t&eacute;lt&udblac;ek</ind></trd>
   <trdgrp lng="it">
   <trd>urodeli</trd>, 
   <trd>caudati</trd>
   </trdgrp>
   <trd lng="nl">salamanders<klr tip="ind"> (fam.)</klr></trd>
   <trd lng="pl">p&lstroke;azy ogoniaste</trd>
   <trdgrp lng="pt">
    <trd>urodelos</trd>
   </trdgrp>
   <trd lng="ru">&c_h;&c_v;&c_o;&c_s;&c_t;&c_a;&c_t;&c_y;&c_je; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;<klr
   tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
   <trd lng="sv">stj&auml;rtgroddjur</trd>
</drv>
</art>
