<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elegan.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>elegant</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="elegan.0a">
  <kap><tld/>a</kap>
  <snc mrk="elegan.0a.KOMUNE">
    <dif>
      Gustoplene distinginda:
      <ekz>
        <tld/>a homo, vesto, mantelo stilo, verkisto;
      </ekz>
      <ekz>
        <tld/>a (simpla kaj klara) demonstracio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pimp.0a">pimpa</ref>,
      <ref cel="sxik.0a">&scirc;ika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>refinat</trd>,
    <trd>elegant<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elegantní</trd>,
    <trd>úpravný <klr>(čl.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>elegant</trd>,
    <trd>vornehm</trd>
  </trdgrp>
  <trd lng="en">elegant</trd>
  <trd lng="es">elegante</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>eleg&aacute;ns</trd>,
    <trd>v&aacute;laszt&eacute;kos</trd>,
    <trd>csinos</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elegancki</trd>,
    <trd>wytworny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>elegante</trd>,
    <trd>gracioso</trd>,
    <trd>fino</trd>,
    <trd>gentil</trd>,
    <trd>airoso</trd>,
    <trd>garboso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elegantný</trd>,
    <trd>vkusný</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="elegan.0eco.KOMUNE">
    <dif>
      <tld lit="E"/>a aspekto, kiu &ccirc;armas:
      <ekz>
        en matematika pruvo <klr>...</klr> <tld/>eco estas 
        la minimuma kvanto da manipuloj por atingi la 
        solvon kun maksimuma klareco
        <fnt>
          <aut>S. Cobb</aut>,
          <vrk>Elegantaj Solvoj</vrk>,
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>elegance</trd>,
    <trd>uhlazenost</trd>
  </trdgrp>
  <trd lng="fr">&eacute;l&eacute;gance</trd>
  <trdgrp lng="sk">
    <trd>elegancia</trd>,
    <trd>vyberaný vkus</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="elegan.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, pla&ccirc;a:
      <ekz>
        ili provis <klr>...</klr> sin pli <tld/>igi, 
        razigante siajn vilajn mentonojn
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rendre &eacute;l&eacute;gant</trd>
</drv>

<drv mrk="elegan.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="elegan.0ulo.MOR">
    <dif>
      Homo <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>dandi</trd>,
    <trd>elegant<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="cs">elegán</trd>
  <trdgrp lng="de">
    <trd>eleganter Herr</trd>,
    <trd>elegante Dame</trd>,
    <trd>Person von Geschmack</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>elegant person</trd>,
  </trdgrp>
  <trd lng="es">catr&iacute;n</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr> (subst.)</klr></trd>
  <trd lng="hu">eleg&aacute;ns ember</trd>
  <trdgrp lng="pl">
    <trd>elegant</trd>,
    <trd>fircyk</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="sk">elegán</trd>
</drv>

<drv mrk="elegan.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="elegan.mal0a.KOMUNE">
    <dif>
      Ne <tld/>a,
      <ref tip="vid" cel="plump.0a">plumpa</ref>:
      <ekz>
        &scirc;tal&ccirc;apaj botetoj, kiajn portas konstruistoj, estas 
        kutime grandaj, fortikaj, mal<tld/>aj piedvesta&jcirc;oj
        <fnt>
          <aut>I&ccirc;o</aut>,
          <vrk><url
          ref="http://th.lernu.net/komunikado/forumo/temo.php?t=5236">
          Botetegoj?</url></vrk>,
          <lok>lernu.net, 2009-10-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nevkusný</trd>
  <trd lng="fr">in&eacute;l&eacute;gant</trd>
  <trd lng="sk">nevkusný</trd>
</drv>
</art>
