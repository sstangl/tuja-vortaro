<kap>
  <rad>astrolab</rad>/o
</kap>
<drv mrk="astrol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Instrumento por determini horon a&ubreve; pozicion de la observanto
      per mezuro de la angula pozicio de astro super la horizonto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_b;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_a;&c_b;&c_i;&c_ja;</trd>
  <trd lng="cs">astroláb</trd>
  <trd lng="de">Astrolabium</trd>
  <trd lng="en">astrolabe</trd>
  <trd lng="es">astrolabio</trd>
  <trd lng="fa">&a_A;&a_s;&a_T;&a_r;&a_l;&a_A;&a_b;</trd>
  <trd lng="fr">astrolabe</trd>
  <trd lng="hu">asztrol&aacute;bium</trd>
  <trd lng="it">astrolabio</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_ja;&c_b;&c_i;&c_ja;</trd>
  <trd lng="sk">hvezdársky uhlomer</trd>
</drv>
</art>
