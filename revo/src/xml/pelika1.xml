<kap>
  <rad>pelikaned</rad>/oj
</kap>
<drv mrk="pelika1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="pelika.0oformaj">pelikanoformaj
      birdoj</ref> (<trd lng="la">Pelecanidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">pelikánovití</trd>
  <trd lng="de">Pelikane<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">g&ouml;d&eacute;nyf&eacute;l&eacute;k</trd>
  <trd lng="nl">pelikanen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">pelikanowate</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">pelikánovití</trd>
</drv>
</art>
