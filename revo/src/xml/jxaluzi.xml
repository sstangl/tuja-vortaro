<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jxaluzi.xml,v 1.14 2015/05/14 10:10:15 revo Exp $">
<kap>
  <rad>&jcirc;aluzi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="jxaluzi.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxaluzi.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestra a&jcirc;ura&jcirc;o &scirc;irma kontra&ubreve; eksteraj
      varmo, lumo kaj envido:
      <ekz>
        la fenestroj <klr>...</klr> similis al truoj, kaj krome ilin
        kovris tre densaj kaj dikaj <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 40a, p.
        301a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kurten.lat0o.ARKI">latkurteno</ref>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>
  <trd lng="bg">&c_shch;&c_o;&c_r;&c_a;</trd>
  <trd lng="ca">persiana</trd>
  <trd lng="de">Jalousie</trd>
  <trd lng="es">persiana</trd>
  <trd lng="fa">&f_k;&a_r;&f_k;&a_r;&a_h;</trd>
  <trd lng="fr">jalousie<klr tip="ind"> (de fen&ecirc;tre)</klr></trd>
  <trd lng="nl">jaloezie</trd>
  <trd lng="pl">&zdot;aluzja</trd>
  <trdgrp lng="pt">
    <trd>gelosia</trd>,
    <trd>r&oacute;tula <klr tip="amb">(arq.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_i;</trd>
  <trdgrp lng="it">
  <trd>gelosia<klr> (da finestra)</klr></trd>, 
  <trd>persiana<klr> (da finestra)</klr></trd>
  </trdgrp>
</drv>
</art>
