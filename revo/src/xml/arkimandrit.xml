<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: arkimandrit.xml,v 1.7 2014/10/09 17:10:27 revo Exp $">
    <kap>
      <rad>arkimandrit</rad>/o
    </kap>

    <drv mrk="arkimandrit.0o">
      <kap><tld/>o <fnt><bib>PIV1</bib></fnt></kap>
      <snc mrk="arkimandrit.0o.orto">
      <uzo tip="fak">KRI</uzo>
    	<dif>
          En ortodoksismo,
          <ref tip="super" cel="monahx.0ejestro">mona&hcirc;ejestro</ref>
    	  de grava mona&hcirc;ejo a&ubreve; estro de pluraj mona&hcirc;ejoj; rango pli
          alta ol <ref cel="igumen.0o">igumeno</ref>.
    	</dif>
	<rim>
          Kp <ref cel="provincial.0o">provincialo</ref> en katolikismo.
        </rim>
      </snc>
      <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y;&c_t;</trd>
      <trdgrp lng="de">
         <trd>Klosteroberhaupt<klr> (in der griechischen
         Kirche)</klr></trd>,
         <trd>Archimandrit</trd>
      </trdgrp>
      <trd lng="en">archimandrite</trd>
      <trd lng="fr">archimandrite</trd>
      <trdgrp lng="hu">
         <trd>archimandrita</trd>,
         <trd>f&odblac;ap&aacute;t<klr> (pravoszl&aacute;v)</klr></trd>
      </trdgrp>
      <trd lng="it">archimandrita</trd>
      <trd lng="grc">&alfa_psili;&rho;&chi;&jota;&my;&alfa;&ny;&delta;&rho;&jota_acute;&tau;&eta;&sigma_fina;</trd>
      <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_m;&c_a;&c_n;&c_d;&c_r;&c_i;&c_t;</trd>
    </drv>
  </art>

