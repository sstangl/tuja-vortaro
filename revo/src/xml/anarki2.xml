<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anarki2.xml,v 1.7 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>anarki</rad>/o
</kap>
<drv mrk="anarki2.0o">
  <kap><tld/>o</kap>
  <snc mrk="anarki2.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Socia stato de absoluta individua libereco, senregeco, senestreco.
    </dif>
    <trd lng="sv">anarki</trd>
    <trd lng="hu">anarchia</trd>    
  </snc>
  <snc mrk="anarki2.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Senordego.
    </dif>
    <trdgrp lng="de">
      <trd>Unordnung</trd>,
      <trd>Durcheinander</trd>,
      <trd>Chaos</trd>
    </trdgrp>
    <trd lng="es">anarqu&iacute;a</trd>
    <trdgrp lng="sv">
      <trd>anarki</trd>,
      <trd>lagl&ouml;shet</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>felfordul&aacute;s</trd>,
      <trd>z&udblac;rzavar</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bez&lstroke;ad</trd>,
      <trd>chaos</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
  <trd lng="cs">anarchie</trd>
  <trdgrp lng="de">
      <trd>Gesetzlosigkeit</trd>,
      <trd>Anarchie</trd>
  </trdgrp>
  <trd lng="en">anarchy</trd>
  <trd lng="fr">anarchie</trd>
  <trd lng="nl">anarchie</trd>
  <trd lng="pl">anarchia</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>anarchia</trd>,
    <trd>bezvládie</trd>
  </trdgrp>
  <trd lng="sv">anarki</trd>
</drv>

<drv mrk="anarki2.0a">
  <kap><tld/>a</kap>
  <snc mrk="anarki2.0a.POL">
    <dif>
      Konforma al <tld/>o, senestra.
    </dif>
    <trd lng="en">anarchist</trd>
    <trd lng="fr">anarchiste<klr tip="amb"> (adj.)</klr></trd>
    <trd lng="hu">anarchikus</trd>
    <trd lng="pl">anarchistyczny</trd> 
    <trd lng="sk">anarchistick&yacute;</trd>
  </snc>
  <snc mrk="anarki2.0a.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Senregula, senordega, &hcirc;aosa:
      <ekz>
        <tld/>a vivmaniero.
      </ekz>
    </dif>
    <trd lng="de">chaotisch</trd>
    <trdgrp lng="en">
      <trd>anarchic</trd>,
      <trd>chaotic</trd>
    </trdgrp>
    <trd lng="fr">anarchique</trd>
    <trdgrp lng="hu">
      <trd>z&udblac;rzavaros</trd>,
      <trd>kaotikus</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bez&lstroke;adny</trd>,
      <trd>chaotyczny</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="cs">anarchický</trd>
  <trdgrp lng="de">
      <trd>gesetzlos</trd>,
      <trd>anarchisch</trd>
  </trdgrp>
  <trd lng="es">anarquista</trd>
  <trd lng="nl">anarchistisch</trd>
  <trd lng="pl">anarchiczny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">anarchický</trd>
  <trd lng="sv">anarkistisk</trd>
</drv>

<drv mrk="anarki2.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="anarki2.0ismo.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      Socia kaj politika doktrino, kiu celas      absolutan individuan
      liberecon kaj forigon de &ccirc;ia &scirc;tata      regado; senregeco,
      senestreco.
    </dif>
    <refgrp tip="vid">
      <ref cel="indivi.0ismo">individuismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">anarchizmus</trd>
  <trdgrp lng="de">
      <trd>Gesetzlosigkeit</trd>,
      <trd>Anarchie</trd>
  </trdgrp>
  <trd lng="en">anarchism</trd>
  <trd lng="fr">anarchisme</trd>
  <trd lng="hu">anarchizmus</trd>
  <trd lng="nl">anarchisme</trd>
  <trd lng="pl">anarchizm</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">anarchizmus</trd>
  <trd lng="sv">anarkism</trd>
</drv>

<drv mrk="anarki2.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="anarki2.0isto.POL">
    <dif>
      Adepto de <tld/>ismo
    </dif>
    <refgrp tip="vid">
      <ref cel="indivi.0isto">individuisto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="cs">anarchista</trd>
  <trd lng="de">Anarchist</trd>
  <trd lng="en">anarchist</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="fr">anarchiste<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">anarchista</trd>
  <trd lng="nl">anarchist</trd>
  <trd lng="pl">anarchista</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">anarchista</trd>
  <trd lng="sv">anarkist</trd>
</drv>

<drv mrk="anarki2.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="anarki2.0ulo.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Homo kies konduto kaj vivmaniero estas senregula, senordega.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>anarchista</trd>,
    <trd>bouřlivák</trd>,
    <trd>nedisciplinovaný člověk</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Anarch</trd>,
      <trd>Chaot</trd>
  </trdgrp>
  <trd lng="en">anarchist</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="fr">anarchiste<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">z&udblac;r&ouml;s alak</trd>
  <trdgrp lng="nl">
    <trd>chaoot</trd>,
    <trd>warhoofd</trd>
  </trdgrp>
  <trd lng="pl">anarchista</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">anarchista</trd>
  <trd lng="sv">anarkist</trd>
</drv>
</art>

