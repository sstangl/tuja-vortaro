<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nauxz.xml,v 1.19 2009/02/26 17:30:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>na&ubreve;z</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nauxz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="nauxz.0i.FIZL">
    <dif>
      Ka&ubreve;zi vomemon:
      <ekz>
        tiu putra viando <tld/>as min;
      </ekz>
      <ekz>
        <tld/>i al iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&eacute;mely&iacute;t</trd>
  </snc>
  <snc mrk="nauxz.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&ubreve;zi senton de mallogo, abomeneto, tedego:
      <ekz>
        <tld/>i &ccirc;iujn per siaj hipokrita&jcirc;oj;
      </ekz>
      <ekz>
        mi ne volas vin <tld/>i;
      </ekz>
      <ekz>
        la tro ofta pritraktado de tiu demando efikas <tld/>e;
      </ekz>
      <ekz>
        <tld/>e ripetata fabelo<fnt>Z</fnt>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>undor&iacute;t</trd>,
      <trd>visszatasz&iacute;t</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;
<ind>&c_n;&c_u;&c_d;&c_o;&c_t;&c_u;</ind></trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;
<ind>&c_m;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ekeln</trd>,
    <trd>anekeln</trd>,
    <trd>anwidern</trd>
  </trdgrp>
  <trd lng="en">nauseate</trd>
  <trdgrp lng="es">
    <trd>producir <ind>n&aacute;useas</ind></trd>,
    <trd>nausear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;gouter</trd>,
    <trd>&eacute;c&oelig;urer</trd>,
    <trd>donner la <ind>naus&eacute;e</ind></trd>
  </trdgrp>
  <trd lng="nl">doen <ind>walgen</ind></trd>
  <trd lng="pt">nausear</trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
<ind>&c_t;&c_o;&c_sh;&c_n;&c_o;&c_t;&c_u;</ind></trd>
  <trd lng="sv">&auml;ckla</trd>
  <trd lng="tp">jaki</trd>
</drv>

<drv mrk="nauxz.0o">
  <kap><tld/>o</kap>
  <snc mrk="nauxz.0o.KOMUNE">
    <dif>
      Sento de vomemo a&ubreve; abomeneto:
      <ekz>
        senti <tld/>on al, pri io.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_u;&c_d;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ekel</trd>,
    <trd>&Uuml;belkeit</trd>
  </trdgrp>
  <trd lng="en">nausea</trd>
  <trd lng="es">n&aacute;useas</trd>
  <trdgrp lng="fr">
    <trd>&eacute;c&oelig;urement</trd>,
    <trd>naus&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&aacute;nyinger</trd>,
    <trd>undor</trd>
  </trdgrp>
  <trd lng="nl">walging</trd>
  <trd lng="pt">n&aacute;usea</trd>
  <trd lng="ru">&c_t;&c_o;&c_sh;&c_n;&c_o;&c_t;&c_a;</trd>
  <trd lng="sv">&auml;ckel</trd>
</drv>
</art>
