<vortaro>
<art mrk="$Id: alvus.xml,v 1.14 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>alvus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alvus.0o">
  <kap><tld/>o</kap>
  <snc mrk="alvus.0o.CXAS">
    <dif>
      Fi&scirc;idaro, fi&scirc;idetoj por provizi riverojn kaj lagojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>supozeble vi mem ne estas tute malsperta
        <tld/>o pri komputiloj
        <fnt>
          <aut>S. Dereviannykh</aut>,
          <vrk>&Ccirc;u vi tradukas france?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 4a, p. 18a</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_w;&c_k;&c_ib; <klr tip="amb">(&c_r;&c_y;&c_b;&c_y;)</klr></trd>
  <trdgrp lng="cs">
    <trd>násada rybího potěru</trd>,
    <trd>rybka</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>restocking fish</trd>
  </trdgrp>
  <trd lng="fr">alevins</trd>
  <trd lng="hu">halivad&eacute;k</trd>
  <trd lng="nl">pootvis</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_mol;&c_k;&c_i;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;sada <klr>(ryb&aacute;rska)</klr></trd>,
    <trd>násada</trd>,
    <trd>rybka</trd>
  </trdgrp>
</drv>
</art>
