<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekslib.xml,v 1.14 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>ekslibris</rad>/o
</kap>
<drv mrk="ekslib.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Surskribo a&ubreve; bildo, kiun iu surmetas sur la libroj de sia
      biblioteko,
      por montri, ke ili estas lia propra&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_l;&c_ib;&c_b;&c_r;&c_y;&c_s;</trd>
  <trd lng="ca">ex-libris</trd>
  <trd lng="cs">exlibris</trd>
  <trd lng="de">Exlibris</trd>
  <trd lng="en">ex libris</trd>
  <trd lng="es">ex libris</trd>
  <trd lng="fr">ex-libris</trd>
  <trd lng="hu">ex libris</trd>
  <trdgrp lng="pl">
    <trd>ekslibris</trd>,
    <trd>ex-libris</trd>
  </trdgrp>
  <trd lng="pt">ex-libris</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_l;&c_i;&c_b;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">exlibris</trd>
</drv>
</art>
