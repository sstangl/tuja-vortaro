<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kresp.xml,v 1.18 2010/10/30 16:30:27 revo Exp $">
<kap>
  <rad>kresp</rad>/o
</kap>
<drv mrk="kresp.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Maldika pasta&jcirc;o farita el faruno miksita kun lakto kaj ovoj,
      kiun oni fritas en pato kaj man&gcirc;as kun sukerpulvoro;
      <ref tip="sin" cel="kuk.pat0o">patkuko</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_a;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>        
  <trd lng="br">krampouezhenn</trd>
  <trdgrp lng="de">
    <trd>Pfannkuchen</trd>
    <trd>Cr&ecirc;pe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crepe</trd>,
    <trd>pancake</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;pe</trd>
  <trd lng="hu">palacsinta</trd>
  <trdgrp lng="nl">
    <trd>pannenkoek</trd>,
    <trd>flensje</trd>
  </trdgrp>
  <trd lng="pl">nale&sacute;nik</trd>
  <trdgrp lng="pt">
    <trd>filh&oacute;</trd>,
    <trd>coscor&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_o;&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>pannkaka</trd>,
    <trd>cr&ecirc;pe</trd>
  </trdgrp>
</drv>
</art>
