<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lem.xml,v 1.13 2015/07/04 08:37:38 revo Exp $">
  <kap>
    <rad>lem</rad>/o
  </kap>
<drv mrk="lem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Aserto, demonstrita dum la pruvo de pli grava
      <ref cel="teorem.0o">teoremo</ref> kaj servanta al tiu pruvo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_m;&c_a;</trd>
  <trd lng="ca">lema</trd>
  <trdgrp lng="cs">
    <trd>lemma</trd>,
    <trd>pomocná věta</trd>,
    <trd>poučka</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Lemma</trd>,
     <trd>Satz</trd>
  </trdgrp>
  <trd lng="el">&lambda;&eta_ton;&my;&my;&alfa;</trd>
  <trd lng="en">lemma</trd>
  <trd lng="es">lema</trd>
  <trd lng="fa">&a_l;&a_m;</trd>
  <trd lng="fr">lemme</trd>
  <trdgrp lng="hu">
    <trd>lemma</trd>,
    <trd>seg&eacute;dt&eacute;tel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lemma</trd>,
    <trd>hulpstelling</trd>
  </trdgrp>
  <trd lng="pl">lemat</trd>
  <trd lng="pt">lema</trd>
  <trd lng="ru">&c_l;&c_je;&c_m;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>lema</trd>,
    <trd>pomocná veta</trd>,
    <trd>poučka</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>lemma</trd>,
    <trd>hj&auml;lpsats</trd>
  </trdgrp>
</drv>
</art>

