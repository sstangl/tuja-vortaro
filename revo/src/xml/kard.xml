<vortaro>
<art mrk="$Id: kard.xml,v 1.22 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kard.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kard.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="genr.0o.BIO">Genro</ref> el familio 
      <ref tip="malprt" cel="astera.0oj">asteracoj</ref> 
      (<trd lng="la">Carduus</trd>):
      <ekz>
        ni grimpis sur la klifojn kaj pluiris la&ubreve; pado plena je dornoj
        kaj <tld/>oj
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url
          ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
          rakontoj</url>. - 2005</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">thistle</trd>
    <trd lng="fr">chardon</trd>
    <trd lng="nl">distel</trd>
  </snc>
  <snc mrk="kard.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Similaj herboj kun pikilhavaj folioj kaj kapituloj:
      <refgrp tip="vid">
        <ref cel="dipsak.0o">dipsako</ref>,
        <ref cel="artisx.0o">arti&scirc;oko</ref>,
        <ref cel="karlin.0o">karlino</ref>
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_t;&c_a;&c_p;&c_a;&c_l;&c_o;&c_h;</trd>
  <trd lng="br">askolenn</trd>
  <trd lng="ca">card</trd>
  <trd lng="cs">bodl√°k</trd>
  <trd lng="de">Distel</trd>
  <trd lng="hu">bog&aacute;ncs</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_h;</trd>
  <trd lng="sk">bodliak</trd>
</drv>
</art>
