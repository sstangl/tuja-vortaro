<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diapaz.xml,v 1.15 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>diapazon</rad>/o
</kap>
<drv mrk="diapaz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Ilo, per kiu oni a&ubreve;digas sen&scirc;an&gcirc;an noton,
      la&ubreve; kiu oni agordas la muzikilojn.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_t;&c_o;&c_n;</trd>
    <trd lng="de">Stimmgabel</trd>
    <trd lng="es">diapas&oacute;n</trd>
    <trd lng="fr">diapason</trd>
    <trd lng="hu">hangvilla</trd>
    <trd lng="it">diapason</trd>
    <trdgrp lng="pl">
      <trd>kamerton</trd>,
      <trd>stroik</trd>
    </trdgrp>
    <trd lng="pt">diapas&atilde;o</trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_t;&c_o;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>diapazon</trd>,
    <trd>hlasové rozpětí</trd>,
    <trd>tónové rozpětí</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pásmo</trd>,
    <trd>rozpätie</trd>
  </trdgrp>
</drv>
</art>
