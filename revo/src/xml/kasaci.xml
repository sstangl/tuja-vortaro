<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kasaci.xml,v 1.9 2004/12/05 17:33:26 revo Exp $">
<kap>
  <rad>kasaci</rad>/i
</kap>
<drv mrk="kasaci.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <klr>(tribunalan ju&gcirc;on, dekreton, proceduron, verdikton)</klr>
      Le&gcirc;e nuligi:
      <ekz>
        la <tld/>a kortumo <tld/>is la verdikton de la unua instanco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>kassieren</trd>,
     <trd>aufheben</trd>,
     <trd>f&uuml;r nichtig erkl&auml;ren</trd>,
     <trd>f&uuml;r ung&uuml;ltig erkl&auml;ren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>overturn</trd>,
    <trd>quash</trd>,
    <trd>vacate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>casser<klr tip="ind"> (un jugement)</klr></trd>,
    <trd>annuler<klr tip="ind"> (en cassation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez</trd>,
     <trd>hat&aacute;lytalan&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>casseren</trd>,
    <trd>nietig verklaren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_d;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kasaci.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Le&gcirc;a nuligo al tribunala ju&gcirc;o, dekreto, proceduro, verdikto.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Kassation</trd>,
     <trd>Aufhebung</trd>,
     <trd>Nichtigkeitserkl&auml;rung</trd>,
     <trd>Ung&uuml;ltigmachung</trd>
  </trdgrp>
  <trd lng="fr">cassation<klr tip="ind"> (droit)</klr></trd>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t&eacute;s <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t&aacute;s</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez&eacute;s</trd>,
     <trd>hat&aacute;lytalan&iacute;t&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">cassatie</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
