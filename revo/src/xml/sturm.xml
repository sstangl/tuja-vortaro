<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sturm.xml,v 1.10 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>sturm</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sturm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Impetege ataki:
      <ekz>
        <uzo tip="stl">FIG</uzo>anka&ubreve; &ccirc;iuj malsanaj kaj mortantaj
        elektorajtaj irakanoj heroe <tld/>is al urnoj por elekti sian
        heroon<fnt><aut>Stefan Maul</aut>: <vrk>Du mil du</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 12a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>arremetre</trd>,
    <trd>assaltar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vzít útokem</trd>,
    <trd>útočit ztečí</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>storm</trd>,
    <trd>assault</trd>,
    <trd>charge</trd>,
    <trd>attack</trd>
  </trdgrp>
  <trd lng="fr">assaillir</trd>
  <trdgrp lng="hu">
    <trd>rohamoz</trd>,
    <trd>megrohamoz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>assaltare</trd>,
    <trd>caricare<klr> (assalire)</klr></trd>,
    <trd>attaccare<klr> (assalire)</klr></trd>,
    <trd>investire<klr> (assalire)</klr></trd>
  </trdgrp>
  <trd lng="nl">bestormen</trd>
  <trd lng="pt">assaltar</trd>
  <trd lng="sk">vziať útokom</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sturm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ordinare amasa, impeta atako.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>assalt</trd>,
    <trd>assalt</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prudký útok</trd>,
    <trd>zteč</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>storm</trd>,
    <trd>assault</trd>,
    <trd>charge</trd>,
    <trd>attack</trd>
  </trdgrp>
  <trd lng="fr">assaut</trd>
  <trd lng="hu">roham</trd>
  <trdgrp lng="it">
    <trd>assalto</trd>,
    <trd>carica<klr> (assalto)</klr></trd>,
    <trd>attacco<klr> (assalto)</klr></trd>
  </trdgrp>
  <trd lng="nl">bestorming</trd>
  <trd lng="pt">assalto</trd>
  <trd lng="sk">útok</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
</drv>
</art>

