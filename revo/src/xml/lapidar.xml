<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lapidar.xml,v 1.3 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lapidar</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lapidar.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lapidar.0a.BELA">
    <dif>
      &Scirc;tongravurita:
      <ekz>
        kopioj de <tld/>aj surskriboj
        <fnt>
          <aut>Zhang Shuang, Wang Lihua</aut>,
          <vrk><url
          ref="http://www.espero.com.cn/se/txt/2011-09/23/content_392593.htm">
          Ekspozicio &leftquot;Lanting-Pavilono&rightquot;...</url></vrk>,
          <lok>espero.com.cn, 2011-09-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lapidar.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmultvorta,
      <ref tip="vid" cel="konciz.0a.KOMUNE">konciza</ref>,
      kiel estas kutime &scirc;tongravura&jcirc;oj:
      <ekz>
        li <tld/>e informas
        <fnt>
          <aut>H.-G. Kaiser</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/cezar/la_cxielira_komando.html">
          La &ccirc;ielira komando</url></vrk>,
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lapidární</trd>,
    <trd>stručně výstižný</trd>
  </trdgrp>
  <trd lng="sk">stručne výstižný</trd>
</drv>
</art>

