<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ribek.xml,v 1.11 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>ribek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ribek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva mediteranea kaj e&ubreve;ropa <ref tip="lst"
      cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref> kun tri ar&ccirc;e
      sonigataj
      kordoj:
      <ekz>
        li scipovas legi kaj skribi kaj ludas belege la
        <tld/>on<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 11a, p.
        83a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">rabec</trd>
  <trd lng="cs">trojstrunné prahousle</trd>
  <trd lng="de">Rebeck</trd>
  <trd lng="en">rebec</trd>
  <trd lng="fr">rebec</trd>
  <trd lng="hu">rebec</trd>
  <trdgrp lng="pl">
    <trd>rebec</trd>,
    <trd>mandola</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>rabeca</trd>,
    <trd>rebeque</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_b;&c_je;&c_k;</trd>
  <trd lng="sk">trojstrunové prahusle</trd>
</drv>
</art>

