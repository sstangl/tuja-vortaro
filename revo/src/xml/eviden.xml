<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eviden.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>evident</rad>/a
</kap>

<drv mrk="eviden.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tiel klara k senduba ke &gcirc;i ne bezonas pruvon:
      <ekz>la vero ne estas tiam <tld/>a por la spirito;</ekz>
      <ekz><tld/>a eraro.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cert.0a">certa</ref>,
      <ref cel="vid">videbla</ref>,
      <ref cel="frap">okulfrapa</ref>,
      <ref cel="palp.0ebla">palpebla</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>evidentní</trd>,
    <trd>jasný</trd>,
    <trd>nepochybný</trd>,
    <trd>očividný</trd>,
    <trd>zjevný</trd>,
    <trd>zřejmý</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>offensichtlich</trd>,
     <trd>offenkundig</trd>,
     <trd>offenbar</trd>,
     <trd>augenscheinlich</trd>,
     <trd>einleuchtend</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>evident</trd>,
     <trd>clear</trd>,
     <trd>obvious</trd>,
     <trd>unmistakable</trd>
  </trdgrp>
  <trd lng="es">evidente</trd>
  <trd lng="fr">&eacute;vident</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;</trd>,
    <trd>evidens</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ewidentny</trd>,
    <trd>oczywisty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>evidente</trd>,
    <trd>&oacute;bvio</trd>,
    <trd>inequ&iacute;voco</trd>,
    <trd>patente</trd>,
    <trd>manifesto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zjavný</trd>,
    <trd>zrejmý</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>tydlig</trd>,
    <trd>uppenbar</trd>,
    <trd>sj&auml;lvklar</trd>
  </trdgrp>
</drv>

<drv mrk="eviden.0eco">
  <kap><tld/>eco</kap>
    <snc>
    <dif>
    Klareco, kiun prezentas la vero al la spirito estigante la certecon:
    <ekz>la <tld/>eco de la geometriaj demonstracioj.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>evidentnost</trd>,
    <trd>nepochybnost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Offensichtlichkeit</trd>,
     <trd>Augenscheinlichkeit</trd>,
     <trd>Evidenz<klr> [sci]</klr></trd>
  </trdgrp>
  <trd lng="en">obviousness</trd>
  <trd lng="fr">&eacute;vidence</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;s&aacute;g</trd>,
    <trd>evidencia</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ewidentno&sacute;c</trd>,
    <trd>oczywisto&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepochybnosť</trd>,
    <trd>zrejmosť</trd>
  </trdgrp>
</drv>

<drv mrk="eviden.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
    Tute sendube.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>evidentně</trd>,
    <trd>jasně</trd>,
    <trd>nasnadě</trd>,
    <trd>nasnadě</trd>,
    <trd>nepochybně</trd>,
    <trd>očividně</trd>,
    <trd>zřejmě</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>offensichtlich</trd>,
     <trd>offenkundig</trd>,
     <trd>offenbar</trd>,
     <trd>augenscheinlich</trd>,
     <trd>einleuchtend</trd>
  </trdgrp>
  <trd lng="en">obviously</trd>
  <trd lng="es">evidentemente</trd>
  <trd lng="fr">&eacute;videmment</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;an</trd>,
    <trd>k&eacute;ts&eacute;gtelen&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ewidentnie</trd>,
    <trd>oczywi&sacute;cie</trd>,
    <trd>pewnie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>navidomoči</trd>,
    <trd>nepochybne</trd>,
    <trd>zrejme</trd>
  </trdgrp>
  <trd lng="sv">tydligen</trd>
</drv>
</art>
