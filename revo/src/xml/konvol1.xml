<vortaro>
<art mrk="$Id: konvol1.xml,v 1.8 2015/10/29 07:10:14 revo Exp $">
<kap>
  <rad>konvolvulac</rad>/oj
</kap>
<drv mrk="konvol1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Convolvulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Windengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Convolvulaceae</trd>,
    <trd>morning glory family</trd>
  </trdgrp>
  <trd lng="hu">szul&aacute;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">windefamilie</trd>
  <trd lng="pl">powojowate</trd>
  <trd lng="pt">convolvul&aacute;ceas</trd>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>
</drv>
</art>

