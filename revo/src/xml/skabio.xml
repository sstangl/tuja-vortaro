<vortaro>
<art mrk="$Id: skabio.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>skabioz</rad>/o
</kap>
<drv mrk="skabio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="dipsak1.0oj">dipsakacoj</ref>
      (<trd lng="la">Scabiosa</trd>), ornama planto.
    </dif>
  </snc>
  <trd lng="ca">g&egrave;nere Scabiosa</trd>
  <trdgrp lng="cs">
    <trd>chrastavec</trd>,
    <trd>hlaváč</trd>,
    <trd>skabióza</trd>
  </trdgrp>
  <trd lng="de">Skabiose</trd>
  <trd lng="hu">&ouml;rd&ouml;gszem</trd>
  <trd lng="nl">scabiosa</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_b;&c_i;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">hlaváč</trd>
</drv>
</art>
