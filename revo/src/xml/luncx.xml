<vortaro>
<art mrk="$Id: luncx.xml,v 1.12 2011/06/13 22:10:11 revo Exp $">
<kap>
  <rad>lun&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="luncx.0o">
  <kap><tld/>o</kap>
  <snc mrk="luncx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malpeza, rapida
      <ref tip="super" cel="mangx.0o.regula">tagman&gcirc;o</ref>:
      <ekz>
        la tuta klubo revenis en la 
        &gcirc;ardenon nur post <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 485a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_n;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Mittagessen</trd>,
    <trd>Imbiss</trd>
  </trdgrp>
  <trd lng="es">merienda</trd>
  <trdgrp lng="fr">
    <trd>casse-croute</trd>,
    <trd>collation</trd>,
    <trd>en-cas<klr> (casse-croute)</klr></trd>,
    <trd>lunch</trd>
  </trdgrp>
  <trd lng="hu">uzsonna</trd>
  <trd lng="nl">lunch</trd>
  <trdgrp lng="pt">
    <trd>lanche</trd>,
    <trd>merenda</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_n;&c_ch;</trd>,
    <trd>&c_v;&c_t;&c_o;&c_r;&c_o;&c_j; &c_z;&c_a;&c_v;&c_t;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
</drv>
</art>
