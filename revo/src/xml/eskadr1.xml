<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eskadr1.xml,v 1.21 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>eskadron</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eskadr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="eskadr1.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="skadr.0o.MIL">skadro</ref>
    <ekz>
      kial ni tie &ccirc;i estas kvaza&ubreve; tuta <tld/>o
      <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 1a</lok></fnt>?
    </ekz>
  </snc>
  <!-- formon evitindan prefere ne traduku -->
  <trdgrp lng="sk">
    <trd>eskadra</trd>,
    <trd>eskadróna</trd>,
    <trd>jazdná rota</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>eskadra</trd>,
    <trd>eskadrona</trd>,
    <trd>jízdní rota</trd>,
    <trd>skadrona</trd>
  </trdgrp>
</drv>
</art>
