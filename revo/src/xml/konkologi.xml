<vortaro>
<art mrk="$Id: konkologi.xml,v 1.3 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>konkologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konkologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkologi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Konkoscienco; parto de la 
      <ref tip="malprt" cel="zoolog1.0o.SCI">zoologio</ref>,
      kiu studas konkhavajn moluskojn:
      <ekz>
        privataj kolektoj <klr>...</klr> ludis gravan rolon
        en iuj sciencaj fakoj kiaj entomologio a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Kurioza%C4%B5kabineto">
          Kurioza&jcirc;kabineto</url>, 22012-10-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nauka o ulitách</trd>
  <trd lng="fr">conchyliologie</trd>
  <trd lng="sk">náuka o lastúrach a ulitách</trd>
</drv>
</art>

