<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: torak.xml,v 1.19 2015/10/06 18:10:17 revo Exp $">
<kap>
  <rad>torak</rad>/o
</kap>
<drv mrk="torak.0o">
  <kap><tld/>o</kap>
  <snc mrk="torak.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="korp.0o.ANA">korpo</ref> 
      de <ref tip="vid" cel="verteb.0uloj">vertebruloj</ref> inter 
      la <ref tip="vid" cel="kol.0o.korpo">kolo</ref> 
      kaj la <ref tip="vid" cel="abdome.0o">abdomeno</ref>, 
      entenanta la <ref tip="vid" cel="kor.0o.ANA">koron</ref>, 
      <ref tip="vid" cel="pulm.0o">pulmojn</ref>, trakeon k ezofagon.
    </dif>
  <trd lng="be">&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja;
&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Thorax</trd>,
    <trd>Brustkorb</trd>
  </trdgrp>
  <trd lng="en">thorax</trd>
  <trd lng="es">t&oacute;rax</trd>
  <trd lng="fr">thorax</trd>
  <trd lng="hu">mellkas</trd>
  <trd lng="it">torace</trd>
  <trd lng="la">thorax</trd>  
  <trdgrp lng="nl">
    <trd>thorax</trd>,
    <trd>borstholte</trd>
  </trdgrp>
  <trd lng="pl">klatka piersiowa</trd>
  <trd lng="pt">t&oacute;rax</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja;
&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="torak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La dua <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>parto 
      de <ref tip="vid" cel="insekt.0oj">insektoj</ref>, 
      portanta la krurojn kaj la flugilojn.
    </dif>
    <trd lng="pl">tu&lstroke;&oacute;w</trd>
  </snc>
  <trd lng="ca">t&ograve;rax</trd>
  <trdgrp lng="cs">
    <trd>hrudn&iacute; ko&scaron;</trd>,
    <trd>hrudn&iacute;k</trd>,
    <trd>torax</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrudn&yacute; k&ocirc;&scaron;</trd>,
    <trd>torax</trd>
  </trdgrp>
</drv>

<drv mrk="torak.kap0o">
  <kap>kap<tld/>o</kap>
  <snc mrk="torak.kap0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Anta&ubreve;a parto de <ref tip="malprt"
cel="korp.0o.ANA">korpo</ref>, 
      kuniganta kapon kaj 
      <ref tip="vid" cel="torak.0o.ZOO">torakon</ref> kiel tuton, 
      &ccirc;e <ref tip="vid" cel="arane.0uloj">araneuloj</ref>, 
      <ref tip="vid" cel="krust.0uloj">krustuloj</ref>:  
      <ekz>
        sur la kap<tld/>o trovi&gcirc;as plejofte ok okuloj
        <fnt>
          <aut>E. Nieuwenhuys, trad. L. De Cooman</aut>,
          <vrk><url
ref="http://www.xs4all.nl/~ednieuw/Spiders/InfoNed/Araneo_korpo.html">
          Araneo</url></vrk>,
          <lok>1999-03</lok>
        </fnt>.
      </ekz>
    </dif>
  <rim>La&ubreve; <fnt><bib>PIV2</bib></fnt> SIN: cefalo<tld/>o.</rim>
  </snc>
  <trd lng="ca">cefalot&ograve;rax</trd>
  <trd lng="en">cephalothorax</trd>
  <trd lng="fr">c&eacute;phalothorax</trd>
  <trdgrp lng="it">
    <trd>cefalotorace</trd>,
    <trd>capotorace<klr> (nei crostacei)</klr></trd>,
    <trd>prosoma<klr> (negli aracnidi)</klr></trd>
  </trdgrp>
  <trd lng="pl">g&lstroke;owotu&lstroke;&oacute;w</trd>
</drv>
</art>
