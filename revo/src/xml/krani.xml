<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krani.xml,v 1.24 2015/07/04 08:37:38 revo Exp $">
<kap><ofc>*</ofc>
<rad>krani</rad>/o</kap>
<drv mrk="krani.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krani.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Osta skatolo, parto de la <ref tip="malprt"
      cel="skelet.0o.ANA">skeleto</ref>, kiu entenas kaj
      &scirc;irmas la cerbon de la vertebruloj:
      <ekz>
        a&ubreve;tomobilo krevigis al li la <tld/>on;
      </ekz>
      <ekz>
        kapoj diferencas, <tld/>oj egalas
        <klr>(subkomprenu: post morto)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        long<tld/>ulo, rond<tld/>ulo.
      </ekz>
    </dif>
    <trd lng="de">Sch&auml;del</trd>
    <trdgrp lng="en">
      <trd>skull</trd>,
      <trd>brain pan</trd>,
      <trd>cranium</trd>,
    </trdgrp>
    <trd lng="es">cr&aacute;neo</trd>
    <trd lng="nl">schedel</trd>
  </snc>
  <snc mrk="krani.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kapo:
      <ekz>
        en &ccirc;iu <tld/>o regas aparta opinio<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        granda <tld/>o, sed interne nenio<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kopf</trd>
    <trdgrp lng="en">
      <trd>head</trd>,
      <trd>pate</trd>
    </trdgrp>
    <trd lng="nl">hoofd</trd>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_a;&c_p;</trd>
  <trd lng="cs">lebka</trd>
  <trd lng="fr">cr&acirc;ne</trd>
  <trd lng="hu">koponya</trd>
  <trd lng="id">tengkorak</trd>
  <trd lng="it">cranio</trd>
  <trd lng="pl">czaszka</trd>
  <trd lng="pt">cr&acirc;nio</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;</trd>
  <trd lng="sk">lebka</trd>
</drv>

<drv mrk="krani.post0o">
  <kap>post<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <ref tip="dif" cel="okcipi.0o">okcipito</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_y;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Hinterhaupt</trd>
  <trd lng="es">occipital</trd>
  <trdgrp lng="hu">
    <trd>nyakszirt</trd>,
    <trd>tark&oacute;</trd>
  </trdgrp>
  <trd lng="it">occipite</trd>
  <trd lng="nl">achterhoofd</trd>
  <trd lng="pl">potylica</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_k;</trd>
</drv>
</art>
