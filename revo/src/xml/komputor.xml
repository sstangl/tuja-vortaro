<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komputor.xml,v 1.7 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>komputor</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="komputor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
  <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="komput.0ilo">komputilo</ref>
    <ekz>
      inter la pli special-celaj oficejaj kaj personaj
      <tld/>oj povas ekzisti grandaj diferencoj
      <fnt>
        <aut>R. Eichholz</aut>,
        <vrk><url
        ref="http://www.familienforschung-pabst.de/EspBiographien/PP-pdf/11paroletoj.pdf">
        Dekunu Paroladetoj pri Pekoteko</url></vrk>,
        <lok>1993</lok>
      </fnt>.
    </ekz>    
  </snc>
  <trdgrp lng="cs">
    <trd>komputer</trd>,
    <trd>počítač</trd>
  </trdgrp>
  <trd lng="fr">ordinateur</trd>
  <trd lng="nl">computer<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pt">computador</trd>
  <trdgrp lng="sk">
    <trd>komputer</trd>,
    <trd>počítač</trd>
  </trdgrp>
</drv>
</art>

