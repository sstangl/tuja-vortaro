<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suker.xml,v 1.30 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>suker</rad>/o
</kap>

<drv mrk="suker.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="suker.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Blanka dol&ccirc;a substanco, konsistanta el karbono, hidrogeno
      kaj oksigeno, kaj elprenita el diversaj vegeta&jcirc;oj, precipe el
      <tld/>kano kaj el beto:
      <ekz>
        li donis al mi teon kun <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        la pli vaste konataj <tld/>oj, glukozo kaj fruktozo, havas
        en &ccirc;iu molekulo ses karbon-atomojn
        <fnt>
        <aut>Ste&ccirc;jo Norvell</aut>,
        <vrk>Riboj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 11a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_a;&c_r;</trd>
  <trd lng="bg">&c_z;&c_a;&c_h;&c_a;&c_r;</trd>
  <trd lng="cs">cukr</trd>
  <trd lng="de">Zucker</trd>
  <trd lng="el">&zeta;&alfa_ton;&chi;&alfa;&rho;&eta;</trd>
  <trd lng="en">sugar</trd>
  <trd lng="es">az&uacute;car</trd>
  <trd lng="fr">sucre</trd>
  <trd lng="hu">cukor</trd>
  <trd lng="id">gula</trd>
  <trd lng="it">zucchero</trd>
  <trd lng="nl">suiker</trd>
  <trd lng="ru">&c_s;&c_a;&c_h;&c_a;&c_r;</trd>
  <trd lng="sk">cukor</trd>
  <trd lng="sv">socker</trd>
  <trd lng="tr">&scedil;eker</trd>
</drv>

<drv mrk="suker.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dol&ccirc;igi per <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">sladit cukrem</trd>
  <trd lng="de">zuckern</trd>
  <trd lng="es">azucarar</trd>
  <trdgrp lng="fr">
    <trd>sucrer</trd>
  </trdgrp>
  <trd lng="hu">cukroz</trd>
  <trd lng="id">[<ind>gula</ind>] menggulai</trd>
  <trd lng="it">zuccherare</trd>
  <trd lng="nl">zoeten</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_t;&c_mol; <ind>&c_s;&c_a;&c_h;&c_a;&c_r;</ind><klr> (&c_v;&c_o;
  &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trd lng="sk">sladiť cukrom</trd>
  <trd lng="sv">sockra</trd>
</drv>

<drv mrk="suker.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="suker.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="S"/>ita franda&jcirc;o:
      <ekz>
        sur la tablo staris diversaj <tld/>a&jcirc;oj
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        Mi havas 80 jarojn kaj spertis kiel, &ccirc;e ni, la boligitaj
        <tld/>a&jcirc;oj,
        biskvitoj, ktp. ofte estis sen volva&jcirc;oj kaj liveritaj al
        a&ccirc;etantoj
        en simplaj papersakoj, kiam mi estis infano
        <fnt>
        <aut>Henry D. Palmer</aut>,
        <vrk>Papera mal&scirc;paro</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 12a, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">S&uuml;&szlig;igkeit</trd>
  <trdgrp lng="es">
    <trd>dulce</trd>,
    <trd>confite</trd>,
    <trd>chucher&iacute;a</trd>,
    <trd>golosina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sucrerie</trd>
  </trdgrp>
  <trd lng="hu">&eacute;dess&eacute;g</trd>
  <trdgrp lng="it">
    <trd>zuccherino</trd>,
    <trd>dolce<klr> (sost.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>suikerwaar</trd>,
    <trd>snoepgoed</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>,
    <trd>&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="suker.0ajxejo">
  <kap><tld/>a&jcirc;ejo</kap>
  <snc mrk="suker.0ajxejo.KOM">
    <dif>
      Butiko, kie oni vendas <tld/>a&jcirc;ojn:
      <ekz>
        tiajn <klr>[bombonojn]</klr> ili anta&ubreve;e vidis nur en la
        <tld/>a&jcirc;ejoj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Konditorei</trd>
  <trdgrp lng="es">
    <trd>confiter&iacute;a</trd>,
    <trd>dulcer&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiserie</trd>
  </trdgrp>
  <trd lng="hu">cukr&aacute;szda</trd>
  <trd lng="it">confetteria</trd>
  <trd lng="nl">snoepwinkel</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
</drv>

<drv mrk="suker.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="suker.0ujo.KOMUNE">
    <dif>
      Vazo a&ubreve; alia ujo por konservi a&ubreve; prezenti <tld/>on:
      <ekz>
        estas <klr>...</klr> pulvora <tld/>o, en <tld/>ujoj
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">cukřenka</trd>
  <trd lng="es">azucarero <klr>(recipiente)</klr></trd>
  <trd lng="fr">sucrier</trd>
  <trd lng="it">zuccheriera</trd>
  <trd lng="sk">cukornička</trd>
</drv>

<drv mrk="suker.lakto0o">
  <kap>lakto<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="laktoz.0o">laktozo</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>laktóza</trd>,
    <trd>mléčný cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Milchzucker</trd>,
    <trd>Laktose</trd>
  </trdgrp>
  <trd lng="es">lactosa</trd>
  <trd lng="fr">lactose</trd>
  <trdgrp lng="hu">
    <trd>tejcukor</trd>,
    <trd>lakt&oacute;z</trd>
  </trdgrp>
  <trd lng="id">laktosa</trd>
  <trd lng="it">lattosio</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_a;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mliečny cukor</trd>
</drv>

<drv mrk="suker.pulvor0o">
  <kap>pulvor<tld/>o</kap>
  <snc>
    <dif>
      Pulvorigita sukero.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>moučkový cukr</trd>,
    <trd>práškový cukr</trd>
  </trdgrp>
  <trd lng="de">Puderzucker</trd>
  <trd lng="en">powdered sugar</trd>
  <trdgrp lng="es">
    <trd>az&uacute;car glas</trd>,
    <trd>az&uacute;car glas&eacute;</trd>
  </trdgrp>
  <trd lng="fr">az&uacute;car glas</trd>
  <trd lng="id"><ind>gula</ind> bubuk</trd>
  <trd lng="it"><ind>zucchero</ind> a velo</trd>
  <trd lng="sk">práškový cukor</trd>
</drv>
</art>
