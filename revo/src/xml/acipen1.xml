<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: acipen1.xml,v 1.13 2015/09/24 12:10:20 revo Exp $">
<kap>
  <rad>acipensered</rad>/oj
</kap>
<drv mrk="acipen1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="acipenser.0formaj">acipenserformaj</ref>
      fi&scirc;oj (<trd lng="la">Acipenseridae</trd>), kun plata kapo
      kaj sendenta bu&scirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_ja;&c_t;&c_r;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">acipens&egrave;rids</trd>
  <trd lng="de">St&ouml;re</trd>
  <trdgrp lng="en">
    <trd>Acipenseridae</trd>,
    <trd>sturgeons</trd>
  </trdgrp>
  <trd lng="fr">acipens&eacute;rid&eacute;s</trd>
  <trd lng="hu">tokf&eacute;l&eacute;k</trd>
  <trd lng="it">storioni</trd>
  <trd lng="nl">steuren</trd>
  <trd lng="pl">jesiotrowate</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">st&ouml;rfiskar</trd>
</drv>
</art>
