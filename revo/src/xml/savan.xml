<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: savan.xml,v 1.10 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>savan</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="savan.0o">
  <kap><tld/>o</kap>
  <snc mrk="savan.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Senarbara regiono kun herbeca vegeta&jcirc;aro en tropikaj klimatzonoj:
      <ekz>
        mi faris transkapi&gcirc;ojn en la <tld/>oj, mi karesis 
        la sova&gcirc;ajn &ccirc;evalojn, mi deskuis kokosojn
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la paradizo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="step.0o.ebena">stepo</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_s;&c_a;&c_v;&c_a;&c_n;&c_a;</trd>
  <trd lng="cs">savana</trd>
  <trd lng="de">Savanne</trd>
  <trd lng="en">savanna</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_w;&a_A;&a_n;</trd>,
    <trd>&a_ayn;&a_l;&a_f;&a_z;&a_A;&a_r; &a_A;&a_s;&a_t;&a_w;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">savane</trd>
  <trd lng="hu">szavanna</trd>
  <trd lng="it">savana</trd>
  <trd lng="nl">savanne</trd>
  <trd lng="ru">&c_s;&c_a;&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>savana</trd>,
    <trd>step <klr>(tropická trávnatá rovina)</klr></trd>
  </trdgrp>
</drv>
</art>

