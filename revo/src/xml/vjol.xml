<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vjol.xml,v 1.11 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vjol</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>

<drv mrk="vjol.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ar&ccirc;a <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      pra-violono kun kvin kordoj, diversaj grandoj, formo pli longa kaj
      sono malpli la&ubreve;ta ol modernaj violonoj:
      <ekz>
        akompanate de <tld/>o, <klr>....</klr> li kantas sian beda&ubreve;ron
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 51a, p.
        380a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">viola</trd>
  <trdgrp lng="cs">
    <trd>prahousle</trd>,
    <trd>stará viola</trd>
  </trdgrp>
  <trd lng="de">Viola</trd>
  <trd lng="fr">viole</trd>
  <trd lng="hu">viola <klr tip="ind">(&odblac;sheged&udblac;)</klr></trd>
  <trd lng="it">viola<klr> (mus.)</klr></trd>
  <trd lng="pt">viola</trd>
  <trd lng="ru">&c_v;&c_i;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>prahusle</trd>,
    <trd>stará viola</trd>
  </trdgrp>
</drv>
</art>

