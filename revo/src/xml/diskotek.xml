<art mrk="$Id: diskotek.xml,v 1.7 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>diskotek</rad>/o
</kap>

<drv mrk="diskotek.0o">
  <kap><tld/>o</kap>
  <snc mrk="diskotek.0o.MUZ">
    <uzo tip="stl">KOMUNE</uzo>  
    <ref tip="dif" cel="danc.0ejo.MUZ">dancejo</ref>
    <ekz>
      alparolas min Kerstin &ndash; knabino, kiu &scirc;atas festojn kaj
      
      <tld/>ojn  
      <fnt>
        <aut>U. Matthias</aut>,
        <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
        Fajron sentas mi interne</url></vrk>,
        <lok>Pro Esperanto, 1990</lok>
      </fnt>.
    </ekz>
    <trd lng="bg">&c_d;&c_i;&c_s;&c_k;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
    <trd lng="de">Diskothek</trd>
    <trd lng="en">discoth&egrave;que</trd>
    <trd lng="es">discoteca</trd>
    <trd lng="pl">dyskoteka</trd>
    <trdgrp lng="pt">
      <trd>discoteca</trd>,
      <trd>danceteria</trd>
    </trdgrp>
  </snc>
  <snc mrk="diskotek.0o.BIB">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Kolekto de diskaj a&ubreve; aliaj sonregistra&jcirc;oj, ofte parto
      de <ref 
      tip="malprt" cel="biblio1.0o.ejo">biblioteko</ref>.
    </dif>
    <trd lng="pl">p&lstroke;ytoteka</trd>
    <trd lng="pt">discoteca</trd>
  </snc>
  <trd lng="cs">diskotéka</trd>
  <trd lng="fr">discoth&egrave;que</trd>
  <trd lng="sk">diskotéka</trd>
</drv>
</art>

