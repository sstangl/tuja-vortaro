<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akar.xml,v 1.23 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akar</rad>/o
</kap>
<drv mrk="akar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="akar.0oj.ZOO"><tld/>oj</ref>, 
      kies unu specio vivanta enha&ubreve;te ka&ubreve;zas la 
      <ref tip="dif" cel="skabi.0o">skabion</ref>.
    </dif>
  </snc>
  <ref tip="sub" cel="iksod.0o">iksodo</ref>
  <trd lng="be">&c_k;&c_l;&c_je;&c_sh;&c_ch;</trd>
  <trd lng="bg">&c_k;&c_malmol;&c_r;&c_l;&c_je;&c_zh;</trd>
  <trd lng="br">pre&ntilde;v ar gal</trd>
  <trd lng="de">Milbe</trd>
  <trd lng="en">mite</trd>
  <trd lng="fr">acarien</trd>
  <trd lng="he">&qof;&resh;&dalet;&yod;&tav;</trd>
  <trd lng="hu">atka</trd>
  <trd lng="nl">mijt</trd>
  <trd lng="es">&aacute;caro</trd>
  <trd lng="pt">&aacute;caro</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_shch;</trd>
  <trd lng="sk">roztoč svrabový</trd>
  <trd lng="cs">roztoč</trd>
</drv>

<drv mrk="akar.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="akar.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO"
      lst="voko:zoologiaj_grupoj">Grupo</ref>
      de <ref tip="malprt" cel="arane.0uloj.ZOO">araneuloj</ref>
      (<trd lng="la">Acari</trd>)
      kun proksimume 50000 konataj specioj,
      tre malgrandaj inter 0,1 &gcirc;is kelkaj milimetroj.
    </dif>
  </snc>
</drv>

</art>
