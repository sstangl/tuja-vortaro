<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: posed.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>posed</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="posed.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="posed.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Havi rajton a&ubreve; eblon uzadi ion:
      <ekz>
        <tld/>i ri&ccirc;a&jcirc;ojn, bienon, plurajn domojn;
      </ekz>
      <ekz>
        longa <tld/>ado kreas proprecon;
      </ekz>
      <ekz>
        lia koro forte jam decidis <tld/>i vin<fnt>Z</fnt>
        <klr>(edzi&gcirc;i al vi)</klr>;
      </ekz>
      <ekz>
        rajto de <tld/>mastreco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.0i">havi</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="man.0o">mano</ref>,
      <ref cel="propr.0a">propra</ref>,
      <ref cel="propr.0umi">proprumi</ref>,
      <ref cel="ten.0i">teni</ref>
    </refgrp>
  </snc>
  <snc mrk="posed.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Havi:
      <ekz>
        &ccirc;iu medalo du flankojn <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <subsnc>
      <dif>
        Havi je sia dispono:
        <ekz>
          <tld/>i ies sekreton, bonan organizon;
        </ekz>
        <ekz>
          al <tld/>anto de metio mankas nenio
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          mi <tld/>as anka&ubreve; tre malbone la anglan lingvon<fnt>Z</fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="sci.povo0i0povi">scipovi</ref>
      </refgrp>
      <trd lng="vo">labed&ouml;n</trd>
    </subsnc>
    <subsnc>
      <dif>
        Havi kiel kvaliton a&ubreve; karakteriza&jcirc;on:
        <ekz>
          <tld/>i &ccirc;armon, ioman sperton pri io, grandan
          prudenton<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>i famon de kura&gcirc;ulo<fnt>B</fnt>;
        </ekz>
        <ekz>
          sa&gcirc;o estas fonto de vivo por &gcirc;ia <tld/>anto<fnt>Z</fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>držet</trd>,
    <trd>mít</trd>,
    <trd>vlastnit</trd>
  </trdgrp>
  <trd lng="de">besitzen</trd>
  <trd lng="en">possess</trd>
  <trd lng="fr">poss&eacute;der</trd>
  <trdgrp lng="hu">
    <trd>b&iacute;r</trd>,
    <trd>birtokol</trd>,
    <trd>van neki</trd>
  </trdgrp>
  <trd lng="nl">bezitten</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mať</trd>,
    <trd>vlastniť</trd>
  </trdgrp>
  <trd lng="vo">lab&ouml;n</trd>
</drv>

<drv mrk="posed.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>ado:
      <ekz>
        <tld/>a dokumento
        <klr>(pruvanta la <tld/>on)</klr>;
      </ekz>
      <ekz>
        <uzo tip="fak">GRA</uzo>
        <ind><tld/>a pronomo</ind>
        <klr>(montranta la <tld/>anton)</klr>.
        <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y; <ind>&c_z;&c_a;&c_j;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>
        <trd lng="hu">birtokos n&eacute;vm&aacute;s</trd>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_t;&c_ja;&c_zh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">přivlastňovací</trd>
  <trdgrp lng="fr">
    <trd>possessif</trd>,
    <trd>de <ind>possession</ind></trd>,
    <trd>propri&eacute;taire<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>birtok-</trd>,
    <trd>birtokos</trd>
  </trdgrp>
  <trd lng="nl">bezittelijk</trd>
  <trd lng="sk">privlastňovací</trd>
</drv>

<drv mrk="posed.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io, kion oni pro a&ubreve; sen rajto <tld/>as.
    </dif>
    <refgrp tip="sin">
      <ref cel="propr.0ajxo">propra&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_jo;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Besitz</trd>
  <trd lng="fr">possession<klr tip="amb"> (chose poss&eacute;d&eacute;e)</klr></trd>
  <trd lng="hu">birtok</trd>
  <trd lng="nl">bezit</trd>
  <trd lng="ru">&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="vo">lab</trd>
</drv>

<drv mrk="posed.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="posed.ek0i.JUR">
    <dif>
      I&gcirc;i <tld/>anto de:
      <ekz>
        ili ek<tld/>is la urbon de Palmoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="posed.ek0i.okupi">
    <dif>
      Absorbi, okupi, preni la tutan atenton de:
      <ekz>
        tiu &ccirc;i penso ek<tld/>is min tute<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la frenezo ek<tld/>is lian animon<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>převzít do držby</trd>,
    <trd>ujmout se držby</trd>,
    <trd>zmocnit se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>emparer</ind> de</trd>,
    <trd>prendre <ind>possession</ind> de</trd>
  </trdgrp>
  <trd lng="hu">birtokba vesz</trd>
  <trd lng="nl">bezit nemen</trd>
  <trd lng="ru">&c_o;&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">zmocniť sa</trd>
</drv>
</art>
