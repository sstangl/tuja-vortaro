<kap>
  <rad>diakil</rad>/o
</kap>
<drv mrk="diakil.0o">
  <kap><tld/>o</kap>
  <snc mrk="diakil.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Plastro el aglutina substanco.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_h;&c_ib;&c_l;&c_jo;&c_n;</trd>
  <trd lng="ca">diaquil&oacute;</trd>
  <trd lng="cs">náplast</trd>
  <trdgrp lng="de">
    <trd>Diachylonpflaster</trd>,
    <trd>Bleipflaster</trd>
  </trdgrp>
  <trd lng="es">diaquil&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>diachylon</trd>,
    <trd>diachylum</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;lomtapasz</trd>,
    <trd>diachylum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>plaster<klr> samoprzylepny</klr></trd>,
    <trd>przylepiec</trd>
  </trdgrp>
  <trd lng="pt">diaquil&atilde;o</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_h;&c_i;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">náplasť</trd>
</drv>
</art>
