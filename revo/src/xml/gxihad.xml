<vortaro>
  <art mrk="$Id: gxihad.xml,v 1.7 2013/02/13 15:10:19 revo Exp $">
    <kap>
      <rad>&gcirc;ihad</rad>/o <fnt><bib>KrED</bib></fnt>
    </kap>

    <drv mrk="gxihad.0o">
      <kap><tld/>o</kap>
      <snc mrk="gxihad.0o.REL">
        <uzo tip="fak">REL</uzo>
        <dif>
          En <ref tip="malprt" cel="islam.0o">islamo</ref>,
          lukto a&ubreve; fortostre&ccirc;o celanta venkon de tiu kredo &#8208;
          &ccirc;u en la menso de islamano mem (la granda <tld/>o),
          a&ubreve; en la ekstera mondo (la malgranda <tld/>o).
        </dif>
      </snc>
      <snc mrk="gxihad.0o.POL">
        <uzo tip="stl">KOMUNE</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          Speco de la
          &leftquot;malgranda <tld/>o<sncref ref="gxihad.0o.REL"/>&rightquot;,
          sankta <ref tip="super" cel="milit.0o">milito</ref>:
          <ekz>
            Bin Laden deklaris militon (<tld/>on) kontra&ubreve;
            Usono en 1996
            <fnt>
              <bib>Viki</bib>,
              <lok><url
 ref="http://avobe.com/eo/wikipedia/1/11/11_a_de_septembro_2001.html"
                  >11a de septembro 2001</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="milit.kruc0o">krucmilito</ref>
      </snc>
      <trd lng="be">&c_d;&c_zh;&c_y;&c_h;&c_a;&c_d;</trd>
      <trd lng="cs">d&zcaron;ihad</trd>
      <trd lng="de">Dschihad</trd>
      <trd lng="en">jihad</trd>
      <trd lng="fr">jihad</trd>
      <trd lng="it">jihad</trd>
      <trd lng="pl">d&zdot;ihad</trd>
      <trd lng="pt">jihad</trd>
      <trd lng="ru">&c_d;&c_zh;&c_i;&c_h;&c_a;&c_d;</trd>
      <trd lng="tr">cihad</trd>
    </drv>
  </art>
