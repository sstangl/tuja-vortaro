<art mrk="$Id: gubern.xml,v 1.15 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gubernator</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="gubern.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <ref tip="dif" cel="gubern1.0estro">guberniestro</ref>
    <ekz>
      li <klr>...</klr> povus senplie gajni insulon kaj nomi lin ties
      <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 7a, p.
      63a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_e;&c_r;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">governador</trd>
  <trdgrp lng="cs">
    <trd>gubernátor</trd>,
    <trd>guvernér</trd>,
    <trd>místodržitel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gouverneur</trd>,
    <trd>Statthalter</trd>
  </trdgrp>
  <trd lng="es">gobernador</trd>
  <trd lng="fr">gouverneur</trd>
  <trd lng="hu">korm&aacute;nyz&oacute;</trd>
  <trd lng="nl">gouverneur</trd>
  <trd lng="pt">governador</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_je;&c_r;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>gubernátor</trd>,
    <trd>miestodržiteľ</trd>
  </trdgrp>
</drv>
</art>
