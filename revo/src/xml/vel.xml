<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vel.xml,v 1.36 2015/09/19 10:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vel.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Granda forta tuko, fiksita &ccirc;e masto kaj stre&ccirc;ita por
      kapti la venton:
      <ekz>
        ferli, malferli, brajli, hisi la <tld/>ojn;
      </ekz>
      <ekz>
        kvadrata, latina <tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_z;&c_mol;</trd>
    <trd lng="de">Segel</trd>
    <trd lng="en">sail</trd>
    <trd lng="es">vela</trd>
    <trd lng="fr">voile<klr> (marine)</klr></trd>
    <trd lng="hu">vitorla</trd>
    <trd lng="it">vela</trd>
    <trd lng="nl">zeil</trd>
    <trd lng="pl">&zdot;agiel</trd>
    <trd lng="pt">vela</trd>
    <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;</trd>
    <trd lng="sv">segel</trd>
  </snc>
  <snc mrk="vel.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">FON</uzo>
      <ref tip="dif" cel="palat.post0o.ANA">postpalato</ref>
    <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_a;&c_je; &c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
    <trdgrp lng="fr">
      <trd>palais mou</trd>,
      <trd>voile du palais</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&iacute;nyvitorla</trd>,
      <trd>l&aacute;gy <ind>&iacute;ny</ind></trd>,
      <trd>velum</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>velo</trd>,
      <trd><ind>palato</ind> molle</trd>,
      <trd><ind>palato</ind> posteriore</trd>
    </trdgrp>
    <trd lng="pl">podniebienie mi&eogonek;kkie</trd>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je; <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>m&ecaron;kk&eacute; patro</trd>,
    <trd>plachtovit&aacute; &rcaron;asa</trd>,
    <trd>v&eacute;lum</trd>,
    <trd>z&aacute;voj</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dvadsa&tcaron; jedna</trd>,
    <trd>oko</trd>,
    <trd>velus</trd>
  </trdgrp>
</drv>

<drv mrk="vel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vel.0i.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Veturi per <tld/>pelata veturilo:
      <ekz>
        du marojn inter kiuj staras Korint', mi <tld/>is
        <fnt>
          <aut>J. Racine, trad. G. Lagrange</aut>,
          <vrk>Fedra</vrk>,
          <lok>1991</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>plachtit</trd>,
    <trd>plout pod plachtami</trd>
  </trdgrp>
  <trd lng="fr">faire <ind>voile</ind></trd>
  <trd lng="sk">plachti&tcaron; <klr>(plachetnicou)</klr></trd>
</drv>

<drv mrk="vel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="vel.0aro.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Tuto da <tld/>oj de &scirc;ipo:
      <ekz>
        veturi kun plena <tld/>aro
        <fnt><bib>PIV2</bib>, <lok>Artikolo &leftquot;vel&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_o;&c_je; &c_v;&c_o;&c_o;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_a;<klr> (&c_v;&c_s;&c_je; &c_n;&c_a; &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_je;)</klr></trd>,
      <trd>&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr> (&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="vel.0aro.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      (<tld lit="V"/>aro.)
      <ref cel="stel.0aro">Stelaro</ref> de la suda duonsfero.
    </dif>
    <trd lng="la">Vela</trd>
    <trd lng="ru">&c_P;&c_a;&c_r;&c_u;&c_s;&c_a;<klr> (&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  </snc>
  <ref tip="hom" cel="velar.0o">velaro</ref>
  <trd lng="sk">plachtovie</trd>
  <trd lng="cs">plachtov&iacute;</trd>
</drv>

<drv mrk="vel.antaux0o">
  <kap>anta&ubreve;<tld/>o, <var><kap>front<tld/>o</kap></var></kap>
  <snc mrk="vel.antaux0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>,
      fiksita &ccirc;e la
      <ref tip="vid" cel="mast.front0o">anta&ubreve;masto</ref>:
      <ekz>
        suprenlevinte la anta&ubreve;<tld/>on kontra&ubreve; la vento,
        ili sin direktis al la sabla&jcirc;o
        <fnt><bib>NT</bib><lok>&Ago; 27:40</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxib.0o">&jcirc;ibo</ref>,
      <ref cel="vel.staj0o">stajvelo</ref>
    </refgrp>
  </snc>
  <trd lng="ru">&c_f;&c_o;&c_k;</trd>
  <trd lng="en">foresail</trd>
  <trdgrp lng="de">
    <trd>Fock</trd>,
    <trd>Focksegel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foc</trd>,
    <trd>misaine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orrvitorla</trd>,
    <trd>el&odblac;t&ouml;rzsvitorla</trd>
  </trdgrp>
  <trd lng="it">fiocco<klr> (naut.)</klr></trd>
</drv>

<drv mrk="vel.bram0o">
  <kap>bram<tld/>o</kap>
  <snc mrk="vel.bram0.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>
      de <ref tip="vid" cel="bram1.0o.SHI">brammasto</ref>,
      tria, kalkulante de malsupre.
    </dif>
  </snc>
  <trd lng="cs">nadko&scaron;ov&aacute; plachta</trd>
  <trd lng="de">Bramsegel</trd>
  <trd lng="en">top-gallant</trd>
  <trd lng="fr">perroquet</trd>
  <trd lng="hu">sud&aacute;rvitorla</trd>
  <trd lng="nl">bramzeil</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_m;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">ko&scaron;ov&aacute; plachta</trd>
</drv>

<drv mrk="vel.bumbram0o">
  <kap>bumbram<tld/>o, <var><kap>re&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="vel.bumbram0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Kvara <ref tip="super" cel="vel.0o.SHI"><tld/>o</ref>,
      kalkulante de malsupre.
    </dif>
  </snc>
  <trd lng="de">Royal</trd>
  <trd lng="en">royal</trd>
  <trd lng="hu">p&oacute;znavitorla</trd>
  <trd lng="ru">&c_b;&c_o;&c_m;-&c_b;&c_r;&c_a;&c_m;&c_s;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="vel.cxef0o">
  <kap>&ccirc;ef<tld/>o, <var><kap>grand<tld/>o</kap></var></kap>
  <snc mrk="vel.cxef0o.SHI">
  <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>,
      fiksita &ccirc;e la
      <ref tip="vid" cel="mast.cxef0o">&ccirc;efmasto</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_t;<klr tip="amb"> (&c_p;&c_a;&c_r;&c_u;&c_s;)</klr></trd>
  <trd lng="en">main sail</trd>
  <trd lng="de">Gro&szlig;segel</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;t&ouml;rzsvitorla</trd>,
    <trd>f&odblac;vitorla</trd>
  </trdgrp>
  <trd lng="it">randa</trd>
</drv>

<drv mrk="vel.flank0o">
  <kap>flank<tld/>o</kap>
  <snc mrk="vel.flank0o.SHI">
  <uzo tip="fak">SHI</uzo>
    <dif>
      Ekstra
      <ref tip="super" cel="vel.0o.SHI"><tld/>o</ref>,
      kun trapeza formo, kiun oni metas flanke de
      <ref tip="vid" cel="vel.rekta0o.SHI">rekta <tld/>o</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>sz&aacute;rnyvitorla</trd>,
    <trd>toldatvitorla</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="en">studding-sail</trd>
</drv>

<drv mrk="vel.oblikva0o">
  <kap>oblikva <tld/>o</kap>
  <snc mrk="vel.oblikva0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>,
      situanta la&ubreve;longe de la &scirc;ipo, ofte triangula.
    </dif>
  </snc>
  <trd lng="de">Gaffelsegel</trd>
  <trd lng="fr">voile go&eacute;lette</trd>
  <trd lng="hu">&eacute;lvitorla</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_o;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>
</drv>

<drv mrk="vel.rekta0o">
  <kap>rekta <tld/>o, <var><kap>kvadrata <tld/>o</kap></var></kap>
  <snc mrk="vel.rekta0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Rektangula <ref tip="super" cel="vel.0o.SHI"><tld/>o</ref>,
      pendanta sur velstango, situanta transverse de la &scirc;ipo.
    </dif>
  </snc>
  <trd lng="de">Quersegel</trd>
  <trd lng="fr">voile carr&eacute;</trd>
  <trd lng="hu">keresztvitorla</trd>
  <trd lng="it"><ind>vela</ind> quadra</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_m;&c_o;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>,
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j; <ind>&c_p;&c_a;&c_r;&c_u;&c_s;</ind></trd>,
  </trdgrp>
</drv>

<drv mrk="vel.staj0o">
  <kap>staj<tld/>o</kap>
  <snc mrk="vel.staj0o.SHI">
  <uzo tip="fak">SHI</uzo>
    <dif>
      Triangula <ref tip="super" cel="vel.oblikva0o.SHI">oblikva
      <tld/>o</ref>,
      hisata la&ubreve;
      <ref tip="vid" cel="staj.0o">stajo</ref>
      kaj situanta &ccirc;e la pruo a&ubreve; inter mastoj.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trd lng="en">staysail</trd>
  <trd lng="de">Flieger</trd>
  <trdgrp lng="hu">
    <trd>tarcsvitorla</trd>,
    <trd>el&odblac;k&ouml;t&eacute;l-vitorla</trd>
  </trdgrp>
  <trd lng="it"><ind>vela</ind> di straglio</trd>
</drv>

<drv mrk="vel.top0o">
  <kap>top<tld/>o</kap>
  <snc mrk="vel.top0.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="vel.0o.SHI"><tld lit="V"/>o</ref>
      de <ref tip="vid" cel="mast.top0o">topmasto</ref>,
      dua, kalkulante de malsupre.
    </dif>
  </snc>
  <trd lng="cs">svrchn&iacute; st&ecaron;&zcaron;&ncaron;ov&aacute; plachta</trd>
  <trdgrp lng="de">
    <trd>Toppsegel</trd>,
    <trd>Marssegel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>top sail</trd>,
    <trd>tops'l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&uacute;csvitorla</trd>,
    <trd>der&eacute;kvitorla</trd>
  </trdgrp>
  <trd lng="nl">marszeil</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">plachta na s&tcaron;a&zcaron;ni s&nbsp;plo&scaron;inkou</trd>
</drv>

<drv mrk="vel.skun0o">
  <kap>skun<tld/>o</kap>
  <snc mrk="vel.skun0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Kvarangula <ref tip="super" cel="vel.oblikva0o.SHI">oblikva
      <tld/>o</ref>,
      fiksita &ccirc;e <ref tip="vid" cel="gaf.0o">gafo</ref>.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>csonkavitorla</trd>,
    <trd>gaffvitorla</trd>
  </trdgrp>
  <trd lng="it"><ind>vela</ind> aurica</trd>
  <trd lng="ru">&c_g;&c_a;&c_f;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_r;&c_u;&c_s;</trd>
</drv>

<drv mrk="vel.0sono">
  <kap><tld/>sono</kap>
  <snc mrk="vel.0sono.FON">
   <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>,
      produktata per fermo a&ubreve; malvastigo de la bu&scirc;kanalo per
      alproksimigo de la langa dorso
      al la <ref cel="vel.0o.ANA">velo<sncref/></ref> (k, g, &hcirc;).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_l;&c_ja;&c_r;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
  <trd lng="ca">velar</trd>
  <trdgrp lng="de">
    <trd>Velar</trd>,
    <trd>Gaumensegellaut</trd>
  </trdgrp>
  <trd lng="es">velar</trd>
  <trd lng="fr">v&eacute;laire<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>vel&aacute;ris m&aacute;ssalhangz&oacute;</trd>,
    <trd>h&aacute;ts&oacute; <ind>sz&aacute;jpadl&aacute;shang</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>velaar</trd>,
    <trd>velair</trd>
  </trdgrp>
  <trd lng="pt">velar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_n;&c_je;&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
