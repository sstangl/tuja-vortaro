<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kapuce.xml,v 1.16 2012/09/01 18:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapucen</rad>/o
</kap>
<drv mrk="kapuce.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Franciskana mona&hcirc;o, primitive almozpetanta, kun bruna robo
      kaj pinta kapu&ccirc;o:
      <ekz>
        kun barbo tiel longa, kiel tiu de <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_u;&c_c;&c_y;&c_n;
  <klr>(&c_m;&c_a;&c_n;&c_a;&c_h;)</klr></trd>  
  <trd lng="br">kabusin<klr> (lean)</klr></trd>
  <trd lng="ca">caputx&iacute;</trd>
  <trd lng="de">Kapuziner</trd>
  <trd lng="en">Capuchin monk</trd>
  <trdgrp lng="fr">
    <trd>capucin<klr tip="ind"> (moine)</klr></trd>,
    <trd>moine capucin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapucinus</trd>,
    <trd>csukly&aacute;s bar&aacute;t</trd>
  </trdgrp>
  <trd lng="it">cappuccino<klr> (frate)</klr></trd>
  <trd lng="nl">kapucijn<klr tip="ind"> (monnik)</klr></trd>
  <trd lng="pl">kapucyn</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_u;&c_c;&c_i;&c_n;
  <klr>(&c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
</drv>
</art>
