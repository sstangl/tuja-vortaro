<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katod.xml,v 1.20 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>katod</rad>/o
</kap>
<drv mrk="katod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Negativa
      <ref tip="super" cel="elektr3.0o">elektrodo</ref> de
      <ref tip="malprt" cel="pil.0o">pilo</ref>,
      <ref tip="malprt" cel="valv.0o.ELE">valvo</ref> a&ubreve; alia
      elektra aparato.
    </dif>
    <refgrp tip="ant">
      <ref cel="anod.0o">anodo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_o;&c_d;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_o;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>c&agrave;tode</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>katoda</trd>,
    <trd>záporná elektroda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Minuspol</trd>,
    <trd>Kathode</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cathode</trd>,
    <trd>negative electrode</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>electrodo negativo</trd>,
    <trd>c&aacute;todo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_t;&a_d;</trd>,
    <trd>&a_q;&a_T;&a_b; &a_m;&a_n;&a_f;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cathode</trd>,
    <trd>&eacute;lectrode n&eacute;gative</trd>
  </trdgrp>
  <trd lng="hu">kat&oacute;d</trd>
  <trd lng="it">catodo</trd>
  <trd lng="nl">kathode</trd>
  <trd lng="pl">katoda</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>katóda</trd>,
    <trd>záporná elektróda</trd>
  </trdgrp>
  <trd lng="sv">katod</trd>
</drv>
</art>
