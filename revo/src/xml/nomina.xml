<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nomina.xml,v 1.19 2015/07/04 08:37:40 revo Exp $">
<kap>
   <ofc>3</ofc>
   <rad>nominal</rad>/a
</kap>
<drv mrk="nomina.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Noma, posedanta nur la nomon, ne la efektivan povon kaj rajton:
       <ekz>
         <tld/>a &ccirc;efo.
       </ekz>
     </dif>
     <trdgrp lng="de">
        <trd>nominell</trd>,
        <trd><klr>dem </klr>Namen <ind>nach</ind></trd>
     </trdgrp>
   </snc>
   <snc>
     <dif>
       Indikanta la valoron de io per cifero diferenca je la efektiva
       valoro:
       <ekz>
         a&gcirc;io estas la diferenco inter la <tld/>a kaj la efektiva 
la&ubreve;kurza
         valoro.
       </ekz>
     </dif>
     <trdgrp lng="de">
        <trd>Nominal-</trd>,
        <trd>Nenn-</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>jmenovitý</trd>,
    <trd>nominální</trd>
  </trdgrp>
  <trd 
lng="el">&omikron;&ny;&omikron;&my;&alfa;&sigma;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">nominal</trd>
  <trdgrp lng="hu">
     <trd>n&eacute;vleges</trd>,
     <trd>nomim&aacute;lis</trd>
   </trdgrp>
  <trd lng="nl">nominaal</trd>
  <trd lng="pt">nominal</trd>
  <trd 
lng="ru">&c_n;&c_o;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>menovitá</trd>,
    <trd>nominálna</trd>
  </trdgrp>
</drv>

<drv mrk="nomina.0alavaloro">
   <kap><tld/>a valoro</kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Valoro indikita sur la dokumento, ne nepre estanta la merkata 
valoro, ekz-e sur akcioj.
     </dif>
     <trd lng="de">Nennwert</trd>
   </snc>
  <trd 
lng="be">&c_n;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; 
&c_v;&c_a;&c_r;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">nominální hodnota</trd>
  <trd lng="fr">valeur <ind>nominale</ind></trd>
  <trdgrp lng="hu">
     <trd>n&eacute;vleges &eacute;rt&eacute;k</trd>,
     <trd>n&eacute;v&eacute;rt&eacute;k</trd>,
     <trd>nomim&aacute;lis &eacute;rt&eacute;k</trd>
   </trdgrp>
  <trd lng="nl">nominale waarde</trd>
  <trd 
lng="ru">&c_n;&c_o;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; 
&c_s;&c_t;&c_o;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">menovitá hodnota</trd>
</drv>

<drv mrk="nomina.0ismo">
   <kap><tld/>ismo</kap>
   <uzo tip="fak">FIL</uzo>
   <snc>
     <dif>
       Doktrino, la&ubreve; kiu la &gcirc;eneralaj ideoj estas nur 
nomoj, kaj ne posedas
       ian realan ekziston..
     </dif>
     <refgrp tip="sin">
       <ref cel="real.0ismo">realismo</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">nominalizmus</trd>
  <trd lng="de">Nominalismus</trd>
  <trd lng="fr">nominalisme</trd>
  <trd lng="hu">nominalizmus</trd>
  <trd lng="nl">nominalisme</trd>
  <trd lng="pt">nominalismo</trd>
  <trd lng="ru">&c_n;&c_o;&c_m;&c_i;&c_n;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">nominalizmus</trd>
</drv>
</art>
