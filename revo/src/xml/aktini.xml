<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aktini.xml,v 1.24 2015/10/29 13:10:20 revo Exp $">
<kap>
  <rad>aktini</rad>/o
</kap>

<drv mrk="aktini.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktini.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="anemon.mar0oj.ZOO">maranemono</ref>
    <ekz>
      multspecaj maraj kreska&jcirc;oj, ekzemple ostro,
      <tld/>o, krabo ktp
      <fnt>
        <lok><url
        ref="http://esperanto.cri.cn/1/2006/10/18/1@46911.htm"
            >Agrabla feriado en Haiyang</url></lok></fnt>.
    </ekz>
    <trdgrp lng="en">
      <trd>sea anemone</trd>,
      <trd>actinia</trd>
    </trdgrp>
  </snc>                         
  <snc mrk="aktini.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="aktinium.0o">aktiniumo</ref>
    <trd lng="be">&c_a;&c_k;&c_t;&c_y;&c_n;&c_ib;&c_j;</trd>
    <trd lng="bg">&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
    <trd lng="br">aktiniom</trd>
    <trd lng="ca">actini</trd>
    <trd lng="de">Actinium</trd>
    <trd lng="en">actinium</trd>
    <trd lng="es">actinio</trd>
    <trd lng="fr">actinium</trd>
    <trd lng="id">aktinium</trd>
    <trd lng="it">attinio</trd>
    <trd lng="la">Actium</trd>
    <trd lng="nl">actinium</trd>
    <trd lng="pl">aktyn</trd>
    <trd lng="pt">act&iacute;nio</trd>
    <trd lng="sk">akt&iacute;nium</trd>
    <trd lng="sv">aktinium</trd>
  </snc>
  <trd lng="cs">aktinium</trd>
  <trdgrp lng="sk">
    <trd>Ac</trd>,
    <trd>akt&iacute;nium, Ac</trd>
  </trdgrp>
</drv>
</art>

