<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oligoc.xml,v 1.15 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>oligocen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oligoc.0o">
  <kap><tld/>o</kap>
  <snc mrk="oligoc.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="lst" cel="epok.0o.periodo"
      lst="voko:geologiaj_epokoj" val="-33900000">epoko</ref> inter
      proksimume 34 kaj 23
      milionoj jaroj anta&ubreve; hodia&ubreve;.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_c;&c_e;&c_n;</trd>
  <trd lng="cs">oligocén</trd>
  <trdgrp lng="de">
     <trd>Oligoz&auml;n</trd>,
     <trd>Mittelterti&auml;r</trd>
  </trdgrp>
  <trd lng="fr">Oligoc&egrave;ne</trd>
  <trd lng="hu">oligoc&eacute;n</trd>
  <trd lng="nl">oligoceen</trd>
  <trd lng="pl">oligocen</trd>
  <trd lng="pt">oligoceno</trd>
  <trd lng="ru">&c_o;&c_l;&c_i;&c_g;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">oligocén</trd>
</drv>
</art>
