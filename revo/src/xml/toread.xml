<art mrk="$Id: toread.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>toreador</rad>/o
</kap>
<drv mrk="toread.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">SPO</uzo>
    <dif>
      Homo batalanta kontra&ubreve; la bovoj en la hispanaj bovaj
      kurludoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_r;&c_e;&c_a;&c_d;&c_o;&c_r;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>torero</trd>,
    <trd>torejador</trd>,
    <trd>destre</trd>
  </trdgrp>
  <trd lng="cs">toreador</trd>
  <trdgrp lng="de">
    <trd>Torero</trd>,
    <trd>Stierk&auml;mpfer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bullfighter</trd>,
    <trd>toreador</trd>
  </trdgrp>
  <trd lng="es">torero</trd>
  <trdgrp lng="fr">
    <trd>torero</trd>,
    <trd>tor&eacute;ro</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>torre&aacute;dor</trd>,
    <trd>bikaviador</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stierenvechter</trd>,
    <trd>torero</trd>,
    <trd>toreador</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>toreador</trd>,
    <trd>torero<klr> (pieszy toreador)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toureiro</trd>,
    <trd>toreador</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_r;&c_je;&c_a;&c_d;&c_o;&c_r;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_je;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">toreador</trd>
</drv>
</art>
