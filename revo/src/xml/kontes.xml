<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontes.xml,v 1.18 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kontest</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontes.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontes.0i.KOMUNE">
    <dif>
      Diskute malakcepti aserton a&ubreve; postulon:
      <ekz>
        nuntempaj sociaj luktoj neniel celas <tld/>i la sistemon sed 
        defendi interesojn apartajn, limigitajn
        <fnt>
         <aut>D. Kessous</aut>,
         <vrk><url
         ref="http://www.satesperanto.org/Klasbatalo-vorto-ideo-koncepto.html">
         Klasbatalo : vorto, ideo, koncepto?</url></vrk>,
         <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsen.mal0i">malkonsenti</ref>,
      <ref cel="rifuz.0i">rifuzi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_n;&c_ja;&c_z;&c_g;&c_o;&c_d;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>popírat</trd>,
    <trd>popřít</trd>,
    <trd>vyvracet</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>abstreiten</trd>,
     <trd>bestreiten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>call into question</trd>,
    <trd>contest</trd>,
    <trd>debate <klr>(the truth of)</klr></trd>,
    <trd>deny the truth of</trd>,
    <trd>gainsay</trd>,
    <trd>refuse to accept</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuestionar</trd>,
    <trd>contradecir</trd>
  </trdgrp>
  <trd lng="fr">contester</trd>
  <trd lng="nl">betwisten</trd>
  <trdgrp lng="pl">
    <trd>kwestionowa&cacute;</trd>,
    <trd>podwa&zdot;a&cacute;<klr> (czyje&sacute; racje)</klr></trd>,
    <trd>odrzuca&cacute;<klr> (przyj&eogonek;cie
    czego&sacute;)</klr></trd>,
    <trd>wyra&zdot;a&cacute; <ind>sprzeciw</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>contestar</trd>,
    <trd>impugnar</trd>,    
    <trd>refutar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;
    &c_n;&c_je;&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>popierať</trd>,
    <trd>poprieť</trd>
  </trdgrp>
</drv>

<drv mrk="kontes.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="kontes.ne0ebla.KOMUNE">
    <dif>
      Kiun oni ne povas <tld/>i, &ccirc;ar &gcirc;i
      evidentas, trudi&gcirc;as...:
      <ekz>
        <ind>ne<tld/>ebla pruvo</ind>.
        <trd
        lng="be">&c_n;&c_je;&c_a;&c_b;&c_v;&c_je;&c_r;&c_zh;&c_n;&c_y;
        <ind>&c_d;&c_o;&c_k;&c_a;&c_z;</ind></trd>
        <trd lng="nl">onweerlegbaar bewijs</trd>
        <trd lng="pl">niepodwa&zdot;alny dow&oacute;d</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_o;&c_s;&c_p;&c_o;&c_r;&c_i;&c_m;&c_o;&c_je;
          &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
          <trd>&c_n;&c_je;&c_o;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_zh;&c_i;&c_m;&c_o;&c_je;
          &c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nepopiratelný</trd>
  <trdgrp lng="es">
    <trd>incuestionable</trd>,
    <trd>indiscutible</trd>
  </trdgrp>
  <trd lng="fr">incontestable</trd>
  <trd lng="nl">onweerlegbaar</trd>
  <trdgrp lng="sk">
    <trd>neodškriepiteľný</trd>,
    <trd>nepopierateľný</trd>
  </trdgrp>
</drv>
</art>
