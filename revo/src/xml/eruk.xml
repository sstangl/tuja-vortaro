<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eruk.xml,v 1.11 2012/01/04 13:10:16 revo Exp $">
<kap>
  <rad>eruk</rad>/o
</kap>
<drv mrk="eruk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      G. de kruciferoj, kun flavaj, fortodoraj floroj, kulturata pro
      &gcirc;iaj
      folioj man&gcirc;ataj kiel salato.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>ravenissa groga</trd>,
    <trd>mostassa borda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rauke</trd>,
    <trd>Eruca</trd>
  </trdgrp>
  <trd lng="en">arugula</trd>
  <trd lng="fr">roquette</trd>
  <trd lng="hu">borsmust&aacute;r</trd>
  <trd lng="pl">rokieta</trd>
  <trdgrp lng="pt">
    <trd>eruca</trd>,
    <trd>r&uacute;cula</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_je;&c_p;&c_k;&c_a;</trd>
</drv>
</art>
