<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vitral.xml,v 1.14 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vitral</rad>/o
</kap>
<drv mrk="vitral.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Kuna&jcirc;o el diverskoloraj pecoj de vitro, kunigitaj per
      plumbostrioj
      por formi desegnojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trd lng="bg">&c_s;&c_t;&c_malmol;&c_k;&c_l;&c_o;&c_p;&c_i;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>mozaikové okno</trd>,
    <trd>vitrail</trd>
  </trdgrp>
  <trd lng="de">Bleiglas(fenster)</trd>
  <trd lng="el">&beta;&jota;&tau;&rho;&omikron_ton;</trd>
  <trdgrp lng="en">
    <trd>leadlight</trd>,
    <trd>leaded light</trd>,
    <trd>stained glass window</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vitrail</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nes <ind>&uuml;vegablak</ind></trd>
  <trd lng="nl">loodglas</trd>
  <trd lng="pt">vitral</trd>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>mozaikové farebné okno</trd>,
    <trd>vitráž</trd>
  </trdgrp>
</drv>
</art>
