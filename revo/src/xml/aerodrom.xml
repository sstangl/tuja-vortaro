<art mrk="$Id: aerodrom.xml,v 1.14 2015/07/04 08:37:29 revo Exp $">
<kap><rad>aerodrom</rad>/o</kap>
<drv mrk="aerodrom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      Loko aran&gcirc;ita por ekflugo kaj surteri&gcirc;o de avioj:
      <ekz>
        delegacio atendis nin <klr>[...]</klr> en la <tld/>o
        <fnt><aut>Ludoviko Enizan</aut>: <vrk>Mia Entuziasmo por
        Esperanto, Mia Fervora Favoro al &Ccirc;inio</vrk>, <bib>El popola
        &Ccirc;inio</bib><lok>jaro 1990a, numero 5a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_e;&c_r;&c_a;&c_d;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_l;&c_jo;&c_t;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aerodrom</trd>,
    <trd>letiště</trd>
  </trdgrp>
  <trd lng="de">Flugplatz</trd>
  <trd lng="el">&Alfa;&epsilon;&rho;&omikron;&delta;&rho;&omikron_ton;&my;&jota;&omikron;</trd>
  <trdgrp lng="es">
    <trd>aer&oacute;dromo</trd>
    <trd>aerodromo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;rodrome</trd>,
    <trd>terrain d'<ind>aviation</ind></trd>
  </trdgrp>
  <trd lng="he">&shin;&dalet;&he; &tav;&ayin;&vav;&pe;&he;</trd>
  <trd lng="hu">rep&uuml;l&odblac;t&eacute;r</trd>
  <trdgrp lng="it">
    <trd>aeroporto</trd>,
    <trd>aerodromo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>a&euml;rodroom</trd>,
    <trd>vliegveld</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aerodrom</trd>,
    <trd>lotnisko</trd>
  </trdgrp>
  <trd lng="ro">aerodrom</trd>
  <trd lng="ru">&c_a;&c_e;&c_r;&c_o;&c_d;&c_r;&c_o;&c_m;</trd>
  <trd lng="sk">letisko</trd>
</drv>
</art>
