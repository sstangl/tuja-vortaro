<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ureus.xml,v 1.8 2015/07/04 08:37:45 revo Exp $">
<kap>
    <rad>ureus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ureus.0o">
    <kap><tld/>o</kap>
    <snc mrk="ureus.0o.EGI">
      <uzo tip="fak">HIS</uzo>
      <dif>
        Figuro de kobra serpento elrekti&gcirc;a kaj sunoporta, simbolo de
        antikvaj egiptaj dioj kaj re&gcirc;oj interalie sur iliaj kronoj:
        <ekz>
          la <klr>...</klr> &ccirc;efpastro havis <klr>...</klr> la
          &ccirc;apon de Amenhotep, &ccirc;irka&ubreve;itan de la re&gcirc;a <tld/>o
          <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>.
        </ekz>
      </dif>
    </snc>
  <trd lng="cs">ureus</trd>
  <trd lng="de">Ur&auml;usschlange<klr tip="amb"> (Symbol)</klr></trd>
  <trd lng="fi">uraeus(k&auml;&auml;rme)</trd>
  <trd lng="fr">ur&aelig;us</trd>
  <trd lng="hu">ureuszk&iacute;gy&oacute;</trd>
  <trd lng="it">ureo</trd>
  <trdgrp lng="pt">
     <trd>uraeus</trd>,
     <trd>ur&eacute;u</trd>
    </trdgrp>
  <trd lng="sk">ureus</trd>
</drv>
</art>

