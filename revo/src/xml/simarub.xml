<vortaro>
<art mrk="$Id: simarub.xml,v 1.11 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>simarubac</rad>/oj
</kap>
<drv mrk="simarub.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      arboj (<trd lng="la">Simarubaceae</trd>), plej ofte tropikaj.
    </dif>
    <ref tip="sub" cel="ailant.0o">ailanto</ref>
  </snc>
  <trd lng="ca">simarub&agrave;cies</trd>
  <trd lng="cs">bukovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Bitterholzgew&auml;chse</trd>
  <trd lng="hu">b&aacute;lv&aacute;nyfaf&eacute;l&eacute;k</trd>
  <trd lng="nl">hemelboomfamilie</trd>
  <trd lng="pl">biegunecznikowate</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_a;&c_r;&c_u;&c_b;&c_o;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">bukovité</trd>
</drv>
</art>
