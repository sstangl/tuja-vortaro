<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trajek.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>trajektori</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="trajek.0o">
  <kap><tld/>o</kap>
  <snc mrk="trajek.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="voj.0o.KOMUNE">Vojo</ref>, linio sekvata de
      la pezocentro de movi&gcirc;a objekto:
      <ekz>
        li <klr>...</klr> sidis dum tutaj noktoj en la korto observante
        la <tld/>on de la astroj
        <fnt>
          <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
          <vrk>Cent Jaroj da Soleco</vrk>,
          <lok>Fonto, 1992</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_je;&c_k;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">traject&ograve;ria</trd>
  <trd lng="cs">dráha <klr>(částic)</klr></trd>
  <trdgrp lng="de">
    <trd>Flugbahn</trd>,
    <trd>Wurfbahn</trd>,
    <trd>Schussbahn</trd>
  </trdgrp>
  <trd lng="en">trajectory</trd>
  <trd lng="fr">trajectoire</trd>
  <trd lng="hu">r&ouml;pp&aacute;lya</trd>
  <trd lng="nl">baan<klr> (v.projectiel)</klr></trd>
  <trd lng="pt">trajet&oacute;ria</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_je;&c_k;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">dráha častice</trd>
</drv>
</art>
