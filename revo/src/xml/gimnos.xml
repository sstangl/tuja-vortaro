<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gimnos.xml,v 1.12 2015/07/04 08:37:35 revo Exp $">
<kap><rad>gimnosperm</rad>/a</kap>
<drv mrk="gimnos.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <uzo tip="klr">(pri ovolo a&ubreve; grajno)</uzo>
    <dif>
      Evoluanta sur karpelfolio restinta pli malpli malfermita.
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(pri plantoj)</uzo>
    <dif>
      Produktanta grajnojn en ne tute fermita ovario.
    </dif>
  </snc>
  <trd lng="ca">gimnosperm</trd>
  <trd lng="cs">nahosemenný</trd>
  <trd lng="nl">naaktzadig</trd>
  <trd lng="sk">nahosemenný</trd>
</drv>

<drv mrk="gimnos.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <dif>
      Subbran&ccirc;o de <tld/>aj plantoj, unu el la du grandaj dividoj
      de fanerogamoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_l;&c_a;&c_n;&c_a;&c_s;&c_je;&c_n;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">gimnospermes</trd>
  <trd lng="cs">nahosemenné</trd>
  <trd lng="de">Nacktsamer</trd>
  <trd lng="fr">gymnospermes</trd>
  <trd lng="nl">naaktzadigen</trd>
  <trd lng="pt">gimnosperma</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_s;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">nahosemenné rastliny</trd>
</drv>
</art>
