<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: forsiti.xml,v 1.9 2013/08/30 14:11:03 revo Exp $">
<kap>
  <rad>forsiti</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="forsiti.0o">
  <kap><tld/>o</kap>
  <snc mrk="forsiti.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;indevena ornama arbedo, kies brile flavaj floroj aperas
      printempe jam anta&ubreve; la folioj,
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj">oleacoj</ref>,
      (<trd lng="la">Forsythia</trd>):
      <ekz>
        &gcirc;oje briladas / la levi&gcirc;anta suno / sur <tld/>o
        <fnt>
          <aut>Serena</aut>,
          <vrk><url
          ref="http://blog.goo.ne.jp/serena_2005">[blogo]</url></vrk>,
          <lok>2005-09-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/forsiti.jpg">
    florantaj bran&ccirc;oj de <tld/>o
  </bld>
  <trdgrp lng="de">
    <trd>Forsythia</trd>,
    <trd>Goldflieder</trd>
  </trdgrp>
  <trd lng="es">campanita china</trd>
  <trd lng="fr">forsythia</trd>
  <trdgrp lng="hu">
    <trd>aranyfa</trd>,
    <trd>aranycserje</trd>,
    <trd>forzitia</trd>
  </trdgrp>
  <trd lng="nl">forsythia</trd>
  <trd lng="pt">forsythia</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_s;&c_a;&c_j;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
