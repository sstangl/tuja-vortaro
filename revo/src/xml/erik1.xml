<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: erik1.xml,v 1.8 2015/02/26 20:10:35 revo Exp $">
<kap>
  <rad>Erik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="erik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="erik1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        &scirc;i subite levis la kapon kaj rigardis <tld/>on
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">&Egrave;ric</trd>
  <trd lng="de">Erik</trd>
  <trd lng="fr">&Eacute;ric</trd>
  <trdgrp lng="it">
    <trd>Enrico</trd>,
    <trd>Errico</trd>
  </trdgrp>
  <trd lng="nl">Erik</trd>
  <trd lng="pl">Eryk</trd>
  <trdgrp lng="pt">
    <trd>&Eacute;rico</trd>,
    <trd>Eric</trd>
  </trdgrp>
</drv>
</art>

