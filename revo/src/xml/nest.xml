<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nest.xml,v 1.23 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nest</rad>/o
</kap>

<drv mrk="nest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="nest.0o.ZOO">
    <dif>
      Korbeto a&ubreve; konstrua&jcirc;eto farata de la birdoj kaj de kelkaj insektoj
      kaj fi&scirc;oj por demeti siajn ovojn kaj kovi la idojn:
      <ekz>
        la diversaj specoj de termitoj <klr>...</klr> konstruas
        <tld/>ojn
        de tre diversa formo
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, la lo&gcirc;ejoj
        de
        la termitoj</lok></fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu besto pla&ccirc;as &gcirc;ia <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="nest.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="hejm.0o.KOMUNE">Hejmo</ref> rigardata kiel
      restadejo, rifu&gcirc;ejo:
      <ekz>
        la amata kaj amanta edzo <klr>...</klr> aran&gcirc;is por &scirc;i molan,
        varman kaj oportunan <tld/>on
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fari sian <tld/>on
        <klr>(sin da&ubreve;re instali)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;u &ccirc;i tiun domon <klr>...</klr> vi rigardas kiel <tld/>on de
        rabistoj
        <fnt><bib>MT</bib>, <lok>&Jer; 7:11</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ja;&c_z;&c_d;&c_o;</trd>
  <trd lng="bg">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
  <trd lng="br">neizh</trd>
  <trd lng="cs">hnízdo</trd>
  <trd lng="de">Nest</trd>
  <trd lng="el">&phi;&omega;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="en">nest</trd>
  <trd lng="es">nido</trd>
  <trd lng="fr">nid</trd>
  <trd lng="hu">f&eacute;szek</trd>
  <trd lng="nl">nest</trd>
  <trd lng="pt">ninho</trd>
  <trd lng="ru">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
  <trd lng="sk">hniezdo</trd>
</drv>

<drv mrk="nest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="nest.0i.ZOO">
    <dif>
      Konstrui <tld/>on:
      <ekz>
        kolomboj, kiuj <tld/>as en truoj de kavernoj
        <fnt><bib>MT</bib>, <lok>&Jer; 48:28</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">nidifier</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_c;&c_mol; <ind>&c_g;&c_n;&c_ja;&c_z;&c_d;&c_o;</ind></trd>,
    <trd>&c_g;&c_n;&c_je;&c_z;&c_d;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">neizhia&ntilde;<klr tip="ind"> (sevel)</klr></trd>
  <trdgrp lng="cs">
    <trd>hnízdit</trd>,
    <trd>stavět hnízdo</trd>
  </trdgrp>
  <trd lng="de">nisten</trd>
  <trd lng="en">nest</trd>
  <trd lng="es">anidar</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;szkel</trd>,
    <trd>f&eacute;szket rak</trd>
  </trdgrp>
  <trd lng="nl">nesten</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_n;&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_mol; <ind>&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</ind></trd>
  </trdgrp>
  <trd lng="sk">hniezdiť</trd>
</drv>

<drv mrk="nest.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="nest.0ego.ZOO">
    <dif>
      Iom granda ku&scirc;ejo, kiun aran&gcirc;as bestoj por sia ripozo a&ubreve; por la
      idoj:
      <ekz>
        &ccirc;u vi <klr>...</klr> satigas la leonidojn, Kiam ili ku&scirc;as en la
        <tld/>egoj
        <fnt><bib>MT</bib>, <lok>&Ijo; 38:39-40</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">brloh</trd>
  <trd lng="en">nest</trd>
  <trd lng="fr">tani&egrave;re</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_g;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_l;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">brloh</trd>
</drv>

<drv mrk="nest.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="nest.0igxi.ZOO">
    <dif>
      Sin instali en <tld/>o.
    </dif>
   <trd lng="en">nest</trd>
   <trd lng="hu">f&eacute;szekbe k&ouml;lt&ouml;zik</trd>
  </snc>
  <snc mrk="nest.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin da&ubreve;re instali:
      <ekz>
        floroj <tld/>i&gcirc;as en la fendoj de iliaj radikoj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        la militfebro <tld/>i&gcirc;is en ilia kapo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="radik.en0igxi">enradiki&gcirc;i</ref>,
      <ref cel="logx.0i">lo&gcirc;i</ref>,
      <ref cel="okup.0i">okupi</ref>,
      <ref cel="rest.0i">resti</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>bef&eacute;szkeli mag&aacute;t</trd>,
      <trd>megtelepszik</trd>
    </trdgrp>
  </snc>
  <trd lng="de">sich <ind>einnisten</ind></trd>
  <trd lng="br">neizhia&ntilde;<klr tip="ind"> (anneza&ntilde;)</klr></trd>
  <trdgrp lng="fr">
    <trd>nicher</trd>,
    <trd>se <ind>nicher</ind></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>nestelen</ind></trd>
  <trd lng="ru">&c_u;&c_g;&c_n;&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
