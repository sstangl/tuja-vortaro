<vortaro>
<art mrk="$Id: rezed.xml,v 1.21 2015/09/11 22:10:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>rezed</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rezed.0o">
  <kap><tld/>o</kap>
  <snc mrk="rezed.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbo kun bonodoraj floretoj,
      vivanta en E&ubreve;ropo, Nordafriko, Okcidentazio;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="rezeda.0oj"><tld/>acoj</ref>
      (<trd lng="la">Reseda</trd>):
      <ekz>
        anta&ubreve; la fenestroj estis malgranda &gcirc;ardeneto,
        plena de rozoj, levkojoj, <tld/>o
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_z;&c_e;&c_d;&c_a;</trd>
  <trd lng="ca">reseda</trd>
  <trdgrp lng="cs">
    <trd>reseda</trd>,
    <trd>rezeda</trd>
  </trdgrp>
  <trd lng="de">Resede</trd>
  <trdgrp lng="en">
    <trd>mignonette</trd>,
    <trd>reseda</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;s&eacute;da</trd>
  <trd lng="hu">rezeda</trd>
  <trd lng="nl">reseda</trd>
  <trd lng="pt">resed&aacute;</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_d;&c_a;</trd>
  <trd lng="sk">rezeda</trd>
</drv>
</art>
