<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: marod.xml,v 1.13 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>marod</rad>/i
</kap>

<drv mrk="marod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxtel.0i">&Scirc;teli</ref> terproduktojn,
      kortbirdojn en kamparo, vagante de loko al loko:
      <ekz>
        li devas subteni sin <klr>...</klr> per tio, kion li povas
        <tld/>i
        propramane<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 38a, p.
        289a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>lovit nezákonně <klr>(páchat polní pych)</klr></trd>,
    <trd>pytlačit</trd>
  </trdgrp>
  <trd lng="de">marodieren</trd>
  <trd lng="en">maraud</trd>
  <trd lng="fr">marauder</trd>
  <trd lng="hu">lopkod</trd>
  <trd lng="nl">stropen</trd>
  <trdgrp lng="pt">
    <trd>gatunar</trd>,
    <trd>pilhar</trd>,
    <trd>roubar</trd>    
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_o;&c_d;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kradnúť poľné plodiny a hydinu</trd>
</drv>
</art>
