<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fresx.xml,v 1.20 2011/12/07 19:10:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fre&scirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fresx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="fresx.0a.KOMUNE">
    <dif>
      Ne malnova, &jcirc;usa:
      <ekz>
        <tld/>a modo, tombo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>data informo;
      </ekz>
      <ekz>
        plej <tld/>aj nova&jcirc;oj de gazeto;
      </ekz>
      <ekz>
        krimulo kaptita <ind>&ccirc;e l' <tld/>a faro</ind>
        <trdgrp lng="ca">
          <trd>in fraganti</trd>,
          <trd>en flagrant delicte</trd>
        </trdgrp>
        <trd lng="fr">la main dans le sac</trd>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ricevis <tld/>a&jcirc;ojn <klr>(<tld/>ajn sciigojn)</klr> pri
        li?
      </ekz>
    </dif>
    <subsnc mrk="fresx.0a.bonkvalita">
      <dif>
        Pro &jcirc;useco bonkvalita, bonstata, vigla:
        <ekz>
          mi havas pli <tld/>an panon, ol vi
          <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>a viando, ovo, akvo, aero:
        </ekz>
        <ekz>
          &jcirc;us mi vidis la sunon, tiel <tld/>ru&gcirc;an, tiel 
          belegan, kian mi neniam vidis anta&ubreve;e
          <fnt>
          <aut>&Ccirc;en Hongjing, trad. Miaohui</aut>,
          <vrk><url
          ref="http://gxangalo.com/modules/weblog/index.php?user_id=1995">Bloga&jcirc;oj
          de Miaohui</url></vrk>,
          <lok>2005-06-07</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="fresx.0a.nedifektita">
      <dif>
        Nedifektita de mal&jcirc;useco, de eluziteco:
        <ekz>
          homo ankora&ubreve; <tld/>a malgra&ubreve; longa voja&gcirc;o;
        </ekz>
        <ekz>
          ili floras ankora&ubreve; en la maljuneco, Estas sukplenaj kaj
          <tld/>aj
          <fnt><bib>MT</bib>, <lok>&Psa; 92:14</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>a vesto;
        </ekz>
        <ekz>
          &ccirc;iam <tld/>a estas la memoro pri lia morto;
        </ekz>
        <ekz>
          <tld/>a entuziasmo.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_zh;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_s;&c_je;&c_n;</trd>
  <trd lng="ca">fresc</trd>
  <trd lng="cs">&ccaron;erstv&yacute;</trd>
  <trdgrp lng="de">
    <trd>aktuell</trd>,
    <trd>frisch</trd>,
    <trd>neu</trd>
  </trdgrp>
  <trd lng="es">fresco</trd>
  <trdgrp lng="fr">
    <trd>frais<klr tip="ind"> (adj.)</klr></trd>,
    <trd>r&eacute;cent</trd>
  </trdgrp>
  <trd lng="en">fresh</trd>
  <trdgrp lng="nl">
    <trd>actueel</trd>,
    <trd>fris</trd>,
    <trd>nieuw</trd>,
    <trd>recent</trd>,
    <trd>vers</trd>
  </trdgrp>
  <trd lng="pl">&sacute;wie&zdot;y</trd>
  <trdgrp lng="pt">
    <trd>fresco</trd>,
    <trd>recente</trd>,
    <trd>novo</trd>
  </trdgrp>
  <trd lng="ro">proasp&abreve;t</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_zh;&c_i;&c_j;</trd>
  <trd lng="sk">&ccaron;erstv&yacute;</trd>
</drv>

<drv mrk="fresx.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="fresx.0eco.KOMUNE">
    <dif>
      Stato de io <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_zh;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">frescor</trd>
  <trd lng="de">Frische</trd>
  <trd lng="fr">fraicheur</trd>
  <trd lng="es">frescura</trd>
  <trd lng="nl">frisheid</trd>
  <trd lng="pl">&sacute;wie&zdot;o&sacute;&cacute;</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fresx.re0igi">
  <kap>re<tld/>igi</kap>
  <snc mrk="fresx.re0igi.KOMUNE">
    <dif>
      Fari ion denove <tld/>a:
      <ekz>
        fliki kaj re<tld/>igi vestojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        re<tld/>igi la kolorojn de pentra&jcirc;o, la aeron en la &ccirc;ambro,
        la memoron pri farita promeso;
      </ekz>
      <ekz>
        <ind>sin re<tld/>igi</ind>
        <klr>(sin meti en bonan, <tld/>an staton)</klr>;
        <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>
        <trd lng="ca">refrescar-se</trd>
        <trd lng="fr">se <ind>rafraichir</ind></trd>
        <trd lng="nl">zich <ind>verfrissen</ind></trd>
      </ekz>
      <ekz>
        <ind>re<tld/>iga</ind> spirita bano.
        <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trd lng="ca">refrescant</trd>
        <trd lng="fr">rafraichissant</trd>
        <trd lng="nl">verfrissend</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>refrescar</trd>,
    <trd>renovar</trd>
  </trdgrp>
  <trd lng="es">refrescar</trd>
  <trd lng="fr">rafraichir</trd>
  <trd lng="nl">verfrissen</trd>
  <trd lng="pl">od&sacute;wie&zdot;y&cacute;</trd>
  <trd lng="ru">&c_o;&c_s;&c_v;&c_je;&c_zh;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fresx.re0igajxo">
  <kap>re<tld/>iga&jcirc;o<fnt>Z</fnt></kap>
  <snc mrk="fresx.re0igajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Re<tld/>iga trinka&jcirc;o.
    </dif>
    <trd lng="fr">rafraichissement<klr tip="ind"> (boisson)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">refresc</trd>
  <trd lng="es">refresco</trd>
  <trd lng="nl">frisdrank</trd>
  <trd lng="pl">nap&oacute;j orze&zacute;wiaj&aogonek;cy</trd>
</drv>

<drv mrk="fresx.re0igxi">
  <kap>re<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fresx.re0igxi.KOMUNE">
    <dif>
      I&gcirc;i denove <tld/>a, bonstata:
      <ekz>
        Julio <klr>...</klr> permesis al li iri al siaj amikoj kaj
        re<tld/>i&gcirc;i
        <fnt><bib>NT</bib>, <lok>&Ago; 27:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>refrescar-se</trd>,
    <trd>revitalitzar-se</trd>
  </trdgrp>
  <trd lng="fr">se <ind>rafraichir</ind></trd>
  <trd lng="nl">zich <ind>verfrissen</ind></trd>
  <trd lng="pl">od&sacute;wie&zdot;y&cacute; si&eogonek;</trd>
</drv>

<drv mrk="fresx.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="fresx.mal0a.KOMUNE">
    <dif>
      Tute ne <tld/>a:
      <ekz>
        tiu &ccirc;i mal<tld/>a pano estas malmola, kiel &scirc;tono
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arkaik.0a">arkaika</ref>,
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="nov.mal0a">malnova</ref>,
      <ref cel="mucid.0a.odora">mucida <sncref/></ref>
      <ref cel="putr.0a">putra</ref>,
      <ref cel="velk.0a">velka</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_s;&c_mol;&c_v;&c_je;&c_zh;&c_y;</trd>,
    <trd>&c_ch;&c_e;&c_r;&c_s;&c_t;&c_v;&c_y; <klr tip="amb">(&c_p;&c_r;&c_a; &c_h;&c_l;&c_je;&c_b;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>marcit</trd>,
    <trd>ranci</trd>,
    <trd>m&uacute;stic</trd>,
    <trd>utilitzat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;fraichi</trd>,
    <trd>fatigu&eacute;<klr> (pas frais)</klr></trd>,
    <trd>pas <ind>frais</ind></trd>,
    <trd>rance</trd>,
    <trd>rassis<klr tip="ind"> (pas frais)</klr></trd>,
    <trd>us&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oudbakken</trd>,
    <trd>muf</trd>,
    <trd>rans</trd>,
    <trd>ranzig</trd>,
    <trd>vermoeid</trd>
  </trdgrp>
  <trd lng="pl">nie&sacute;wie&zdot;y</trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_v;&c_je;&c_zh;&c_i;&c_j;</trd>
</drv>
</art>
