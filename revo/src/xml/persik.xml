<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: persik.xml,v 1.27 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>persik</rad>/o
</kap>

<drv mrk="persik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bongusta frukto de
      <ref tip="malprt" cel="persik.0ujo0arbo"><tld/>ujo</ref>,
      <ref tip="super" cel="drup.0o">drupo</ref>.
    </dif>
  </snc>
  <bld lok="../bld/persik.jpg">
    bran&ccirc;o kun <tld/>o
  </bld>
  <trd lng="be">&c_p;&c_e;&c_r;&c_s;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_s;&c_k;&c_o;&c_v;&c_a;</trd>
  <trd lng="br">pechezenn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trdgrp lng="de">
    <trd>Pfirsich <klr>(laneca)</klr></trd>,
    <trd>Nektarine <klr>(glata)[fakte mikso inter persiko kaj
    pruno]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>peach <klr>(laneca)</klr></trd>,
    <trd>nectarine <klr>(glata)</klr></trd>
  </trdgrp>
  <trd lng="es">melocot&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>p&ecirc;che <klr>(laneca)</klr></trd>
    <trd>nectarine<klr>(glata)</klr></trd>
  </trdgrp>
  <trd lng="hu">&odblac;szibarack</trd>
  <trd lng="nl">perzik</trd>
  <trd lng="pl">brzoskwinia</trd>
  <trd lng="pt">p&ecirc;ssego</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_s;&c_i;&c_k;</trd>
  <trd lng="sv">persika</trd>
  <trd lng="tr">&scedil;eftali</trd>
  <trd lng="sk">broskyňa <klr>(ovocie)</klr></trd>
  <trd lng="cs">broskev <klr>(plod)</klr></trd>
</drv>

<drv mrk="persik.0ujo0arbo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus persica</trd>), devena de Persujo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_s;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_je;
&c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-bechez</trd>
    <trd>pechezenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trd lng="cs">broskvoň</trd>
  <trd lng="de">Pfirsichbaum</trd>
  <trd lng="es">melocotonero</trd>
  <trd lng="fr">p&ecirc;cher</trd>
  <trd lng="hu">&odblac;szibarackfa</trd>
  <trd lng="nl">perzikboom</trd>
  <trd lng="pl">brzoskwinia</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_s;&c_i;&c_k;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">broskyňa <klr>(strom)</klr></trd>
  <trd lng="sv">persikotr&auml;d</trd>
  <trd lng="tr">&scedil;eftali</trd>
</drv>
</art>
