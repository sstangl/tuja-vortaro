<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kantik.xml,v 1.15 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kantik</rad>/o
</kap>
<drv mrk="kantik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Pia edifa kanto.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n; <klr>(&c_c;&c_a;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">c&agrave;ntic</trd>
  <trdgrp lng="cs">
    <trd>cantica</trd>,
    <trd>chvalozpěv</trd>,
    <trd>kantikum</trd>
  </trdgrp>
  <trd lng="de">Loblied</trd>
  <trd lng="en">canticle</trd>
  <trd lng="es">c&aacute;ntico</trd>
  <trd lng="fr">cantique</trd>
  <trd lng="hu">egyh&aacute;zi <ind>&eacute;nek</ind></trd>
  <trd lng="it">cantico</trd>
  <trdgrp lng="nl">
    <trd>kerklied</trd>,
    <trd>loflied</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kantyk</trd>,
    <trd>kantyka</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n; <klr>(&c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>chrámový spev</trd>,
    <trd>chválospev</trd>
  </trdgrp>
</drv>
</art>
