<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: latrin.xml,v 1.11 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>latrin</rad>/o
</kap>

<drv mrk="latrin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plej simpla <ref tip="super" cel="neces.0ejo">necesejo</ref> kun
      necesse&gcirc;o a&ubreve; nur foso en la planko:
      <ekz>
        oni ne povas frotlavi <tld/>on, se oni tro lacas por meti la
        kuleron
        da rizo en la bu&scirc;on <fnt><bib>Mortul&Scirc;ip</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_u;&c_zh;&c_n;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>latrína</trd>,
    <trd>suchý záchod</trd>
  </trdgrp>
  <trd lng="de">Latrine</trd>
  <trd lng="en">latrine</trd>
  <trd lng="fr">latrine</trd>
  <trdgrp lng="hu">
    <trd>latrina</trd>,
    <trd>&aacute;rny&eacute;ksz&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">latrine</trd>
  <trd lng="pl">latryna</trd>
  <trdgrp lng="pt">
    <trd>latrina</trd>,
    <trd>privada</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_u;&c_b;&c_o;&c_r;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_o;&c_t;&c_h;&c_o;&c_zh;&c_je;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_r;&c_je;&c_b;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>latrína</trd>,
    <trd>suchý záchod</trd>
  </trdgrp>
  <trd lng="sr">&c_l;&c_a;&c_t;&c_r;&c_i;&c_n;&c_je;</trd>
</drv>
</art>
