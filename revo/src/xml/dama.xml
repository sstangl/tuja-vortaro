    <trd>g&egrave;nere Dama</trd>
  </trdgrp>
  <trd lng="cs">dan&ecaron;k</trd>
  <trd lng="de">Damwild</trd>
  <trd lng="en">fallow <ind>deer</ind></trd>
  <trdgrp lng="fr">
    <trd>daim</trd>,
    <trd>daine</trd>
  </trdgrp>
  <trd lng="he">&yod;&het;&mem;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>d&aacute;mszarvas</trd>,
    <trd>d&aacute;mvad</trd>
  </trdgrp>
  <trd lng="nl">damhert</trd>
  <trd lng="pl">daniel</trd>
  <trd lng="pt">gamo</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_mol;</trd>
  <trd lng="sk">daniel</trd>
  <trd lng="cs">danÄ›k</trd>
