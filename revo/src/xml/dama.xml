<kap>
<rad>dama</rad>/o
</kap>
<drv mrk="dama.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="huf.par0ulo"
      lst="voko:parhufuloj">Parhufulo</ref>
      kun palmoforma kornaro kaj
      makulpunktita somera felo;
