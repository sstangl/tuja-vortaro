<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lang.xml,v 1.33 2015/07/04 08:37:38 revo Exp $">
<kap><ofc>*</ofc>
<rad>lang</rad>/o</kap>
<drv mrk="lang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lang.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Movebla mola karna organo en la
      <ref tip="malprt" cel="busx.0o.ANA">bu&scirc;o</ref>, servanta por
      gustumi kaj paroli:
      <ekz>
        montri la <tld/>on al la kuracisto;
      </ekz><ekz>
        man&gcirc;i bov<tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lang.0o.parolkapablo">
    <dif>
      Tiu organo, &ccirc;e la homo, konsiderata kiel sidejo de parolkapablo:
      <ekz>
        ili parolas kun mi per mensogema <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 109:2</lok></fnt>;
      </ekz><ekz>
        milda<fnt>Z</fnt>,
      </ekz><ekz>
        malpia<fnt>Z</fnt>;
      </ekz><ekz>
        mia <tld/>o parolos veron<fnt>Z</fnt>;
      </ekz><ekz>
        gardi sian <tld/>on<fnt>Z</fnt>
        <klr>(silenti)</klr>;
      </ekz><ekz>
        plej bona gvidilo estas <tld/>o<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        lia nomo estas sur &ccirc;ies <tld/>o
        <klr>(li estas tre fama)</klr>;
      </ekz><ekz>
        <ctl>Fajro sur mia <tld/>o</ctl>
        <klr>(poemaro de Edwin de Kock)</klr>
        <fnt><aut>de Kock Edwin</aut>,
          <vrk>Fajro sur mia lango.</vrk>
          <lok>La Laguna: R&eacute;gulo, 1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lang.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io simila al <tld/>o:
      <ekz>
        longaj <tld/>oj de flamo;
      </ekz><ekz>
        ter<tld/>o <klr>(terkapo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="bg">&c_je;&c_z;&c_i;&c_k;<klr tip="amb"> (&c_a;&c_n;&c_a;&c_t;&c_o;&c_m;&c_i;&c_ja;)</klr></trd>
  <trd lng="br">teod</trd>
  <trdgrp lng="cs">
    <trd>jazyk (v &uacute;stech)</trd>,
    <trd>jazyk <klr>(v ústech)</klr></trd>
  </trdgrp>
  <trd lng="de">Zunge</trd>
  <trd lng="el">&gamma;&lambda;&omega_ton;&sigma;&sigma;&alfa;</trd>
  <trd lng="en">tongue</trd>
  <trd lng="es">lengua</trd>
  <trd lng="fr">langue<klr> (organe)</klr></trd>
  <trd lng="hu">nyelv<klr> (szerv)</klr></trd>
  <trd lng="id">lidah</trd>
  <trd lng="it">lingua<klr> (anat.)</klr></trd>
  <trd lng="nl">tong</trd>
  <trd lng="pl">j&eogonek;zyk</trd>
  <trd lng="pt">l&iacute;ngua</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_k;<klr tip="amb"> (&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  <trd lng="sk">jazyk</trd>
  <trd lng="sv">tunga</trd>
  <trd lng="tr">dil</trd>
</drv>

<drv mrk="lang.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata a&ubreve; apartenanta al <tld/>o:
      <ekz>
        <uzo tip="fak">ANA</uzo>
        <tld/>a arterio;
      </ekz><ekz>
        <tld/>a kiso;
      </ekz><ekz>
        <uzo tip="fak">KUI</uzo>
        <tld/>a kolbaso;
      </ekz><ekz>
        <uzo tip="fak">FON</uzo>
        <tld/>a konsonanto;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <uzo tip="stl">FRAZ</uzo>
        <tld/>a vundo plej profunda
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_z;&c_y;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lang.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Elstara parto de komputila <ref tip="malprt"
      cel="fenest.0o.KOMP">fenestro</ref>, facile alklakebla.
      <ekz>
        Firefox estas kompakta versio de la programo Mozilo. &Gcirc;i
        inkluzivas
        &scirc;ildon kontra&ubreve; <ref tip="vid"
        cel="fenest.sxpruc0o">&scirc;prucfenestroj</ref>, Guglo-ser&ccirc;ejon kaj
        la
        eblon navigi per <tld/>etoj
        <fnt><aut>Marko Naoki LINS</aut>:
        <vrk>Foliumiloj denove interbatalas</vrk>,
        <bib>Monato</bib><lok>jaro 2005, numero 1a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">pempilh<klr tip="ind"> (stlenn.)</klr></trd>
  <trd lng="cs">jazýček nástroje</trd>
  <trd lng="en">tab</trd>
  <trdgrp lng="fr">
    <trd>languette</trd>,
    <trd>onglet</trd>
  </trdgrp>
  <trd lng="id">tab</trd>
  <trdgrp lng="it">
    <trd>linguetta</trd>,
    <trd>scheda<klr> (tab)</klr></trd>
  </trdgrp>
  <trd lng="nl">lipje</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k;<klr tip="ind"> (&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="sk">jazýček nástroja</trd>
</drv>

<drv mrk="lang.0trinki">
  <kap><tld/>trinki</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trinki elprenante flua&jcirc;on per la <tld/>o, kiel hundo.
    </dif>
    <refgrp tip="vid">
      <ref cel="lek.0i">leki</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_ja;&c_b;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">lapa&ntilde;</trd>
  <trdgrp lng="de">
    <trd>lecken</trd>,
    <trd>auflecken</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trd lng="en">lap</trd>
  <trd lng="fr">laper</trd>
  <trd lng="hu">lefetyel</trd>
  <trd lng="it">leccare<klr> (bere con la lingua)</klr></trd>
  <trdgrp lng="nl">
    <trd>oplikken</trd>,
    <trd>slobberen</trd>
  </trdgrp>
  <trd lng="pl">ch&lstroke;epta&cacute;</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>chleptať</trd>,
    <trd>strebať</trd>
  </trdgrp>
  <trd lng="sv">lapa</trd>
</drv>
</art>
