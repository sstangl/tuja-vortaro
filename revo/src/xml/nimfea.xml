<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nimfea.xml,v 1.9 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>nimfeac</rad>/oj
</kap>
<drv mrk="nimfea.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      akvoplantoj (<trd lng="la">Nymphaeaceae</trd>).
    </dif>
    <ref tip="sub" cel="nufar.0o">nufaro</ref>
  </snc>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_n;&c_i; &c_r;&c_o;&c_z;&c_i;</trd>
  <trdgrp lng="cs">
    <trd>leknínovité</trd>,
    <trd>nocenkovité <klr>(čeleď)</klr></trd>
  </trdgrp>
  <trd lng="de">Seerosengew&auml;chse</trd>
  <trd lng="en">water lilies</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nd&eacute;rr&oacute;zsaf&eacute;l&eacute;k</trd>,
    <trd>tavir&oacute;zsaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">waterleliefamilie</trd>
  <trd lng="pt">ninfe&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je;<klr
tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_i;&c_m;&c_f;&c_je;&c_j;&c_n;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">leknovité</trd>
</drv>
</art>
