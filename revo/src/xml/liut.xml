<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: liut.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>liut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="liut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="liut.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumentoj</ref>
      kun sonkesto, unumane tenebla kolo kaj pluke, plektre
      a&ubreve; ar&ccirc;e sonigeblaj kordoj stre&ccirc;itaj
      la&ubreve;longe de la kolo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
  <trd lng="cs">loutna</trd>
  <trd lng="de">Laute</trd>
  <trd lng="el">&lambda;&alfa;&omikron;&ypsilon_ton;&tau;&omikron;</trd>
  <trd lng="en">lute</trd>
  <trdgrp lng="fr">
    <trd>luth</trd>
  </trdgrp>
  <trd lng="hu">lant</trd>
  <trd lng="nl">luit</trd>
  <trd lng="pt">ala&uacute;de</trd>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
  <trd lng="sk">lutna</trd>
</drv>
</art>
