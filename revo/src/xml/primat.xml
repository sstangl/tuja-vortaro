<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: primat.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>primat</rad>/o
</kap>
<drv mrk="primat.0o">
  <kap><tld/>o</kap>
  <snc mrk="primat.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mamulo havanta nur ok incizivojn kaj anta&ubreve;enrigardantajn okulojn:
      <ekz>
        Prasimioj, simioj kaj homoj estas <tld/>oj.
      </ekz>
    </dif>
    <ref tip="sin" cel="simi.0o">simio</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>nehetnatec</trd>,
    <trd>primát</trd>
  </trdgrp>
  <trd lng="de">Primat</trd>
  <trd lng="en">primate</trd>
  <trd lng="fr">primate</trd>
  <trd lng="hu">f&odblac;eml&odblac;s</trd>
  <trd lng="nl">primaat</trd>
  <trd lng="pt">primata</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">primát</trd>
</drv>
</art>
