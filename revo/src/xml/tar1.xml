<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tar1.xml,v 1.9 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tar1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara vegeta&jcirc;o kun grandaj folioj,
      kultivata en tropikaj landoj pro man&gcirc;eblaj
      rizomo kaj folioj;
      specio el la genro <ref tip="super"
      cel="kolokazi.0o.BOT">kolokazio</ref>
      (<trd lng="la">Colocasia esculenta</trd>):
      <ekz>
        en la monto &ccirc;efa man&gcirc;a&jcirc;o estis radiktuberoj kiel batato,
        <tld/>o, manioko
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>la perdita gusto de
        Pinatubo, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">taro</trd>
  <trdgrp lng="cs">
    <trd>tára</trd>,
    <trd>váha obalu</trd>
  </trdgrp>
  <trd lng="en">taro</trd>
  <trdgrp lng="fr">
    <trd>arouille violette</trd>,
    <trd>chou de chine</trd>,
    <trd>dachine</trd>,
    <trd>taro</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>taioba</trd>,
    <trd>inhame</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tara</trd>,
    <trd>váha obalu</trd>
  </trdgrp>
  <trd lng="sv">taro</trd>
</drv>
</art>

