<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kadi.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
<rad>kadi</rad>/o
</kap>
<drv mrk="kadi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ISL</uzo>
   <snc>
     <dif>
       Islama &scirc;tatoficisto, komisiita precipe ju&gcirc;adi 
la&ubreve; la
       <ref tip="vid" cel="koran.0o">Korano</ref> kaj
       <ref tip="vid" cel="suna.0o">Sunao</ref>, a&ubreve; la&ubreve; la 
konsultoj de
       <ref tip="vid" cel="mufti.0o">muftio</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_z;&c_ib;</trd>
  <trd lng="bg">&c_k;&c_a;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">cadi</trd>
  <trd lng="cs">kádí</trd>
  <trd lng="de">Kadi</trd>
  <trdgrp lng="en">
     <trd>cadi</trd>,
     <trd>kadee</trd>
   </trdgrp>
  <trd lng="es">cad&iacute;</trd>
  <trd lng="fa">&a_q;&a_A;&a_D;&f_y; &a_sh;&a_r;&a_ayn;</trd>
  <trd lng="fi">kadi</trd>
  <trd lng="fr">cadi</trd>
  <trd lng="he">&qof;&alef;&dalet;&yod;</trd>
  <trd lng="hu">k&aacute;di</trd>
  <trd lng="id">kadi</trd>
  <trd lng="nl">kadi</trd>
  <trd lng="pl">kadi</trd>
  <trd lng="pt">c&aacute;di</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>kádí</trd>,
    <trd>sudca v islamských krajinách</trd>
  </trdgrp>
  <trd lng="sv">qadi</trd>
</drv>
</art>

