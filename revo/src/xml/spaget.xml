<vortaro>
<art mrk="$Id: spaget.xml,v 1.11 2013/04/21 18:10:16 revo Exp $">
<kap>
  <rad>spaget</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="spaget.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fadenforma <ref tip="super" cel="nudel.0o">nudelo</ref>:
      <ekz>
        100 faskoj da ege fajnaj <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 332a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_g;&c_je;&c_t;&c_y;</trd>
  <trd lng="bg">&c_s;&c_p;&c_a;&c_g;&c_je;&c_t;&c_i;</trd>
  <trd lng="de">Spaghetti</trd>
  <trd lng="en">spaghetti</trd>
  <trd lng="es">espagueti</trd>
  <trdgrp lng="fr">
    <trd>spaghetti</trd>
  </trdgrp>
  <trd lng="he">&samekh;&pe;&gimel;&tet;&yod;</trd>
  <trd lng="hu">spagetti</trd>
  <trd lng="it">spaghetto</trd>
  <trdgrp lng="nl">
    <trd>spaghetti</trd>
  </trdgrp>
  <trd lng="pl">spaghetti</trd>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_g;&c_je;&c_t;&c_t;&c_i;</trd>
</drv>
</art>
