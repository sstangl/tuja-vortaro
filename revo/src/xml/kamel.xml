<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kamel.xml,v 1.27 2013/09/01 15:10:54 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>kamel</rad>/o
    </kap>

    <drv mrk="kamel.0o">
      <kap>
        <ofc>*</ofc>
        <tld/>o
      </kap>
      <snc mrk="kamel.0o.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <dif>
          <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
          cel="kameled.0oj"><tld/>edoj</ref>
          (<trd lng="la">Camelus</trd>), kun unu (<ref tip="sub"
          cel="dromed.0o.ZOO">dromedaro</ref>) a&ubreve; du
          (<ref tip="sub" cel="baktri.b0akamelo.ZOO">baktria
          kamelo</ref>) &gcirc;iboj &ccirc;e la dorso, vivanta en Azio
          kaj Afriko:
          <ekz>
            en la komenco pa&scirc;is unu&gcirc;iba <tld/>o kun blankaj
            haroj,
            unu el la unuaj kiujn oni kaptis en la dezerto
            <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 21a.</lok></fnt>;
          </ekz>
          <ekz>
            estas pli facile por <tld/>o iri tra trueton de kudrilo,
            ol por ri&ccirc;ulo eniri en la regnon de Dio
            <fnt><bib>NT</bib>. <lok>&Mar; 10:25</lok></fnt>;
          </ekz>
          <ekz>
            kion vi faris, juna <ind><tld/>ino</ind> rapidpieda,
            petolanta sur sia vojo
            <fnt><bib>MT</bib>, <lok>&Jer; 2:23</lok></fnt>.
            <trd lng="ca">camella</trd>
            <trdgrp lng="nl">
              <trd>merrie<klr tip="ind"> (v.kameel)</klr></trd>,
              <trd>vrouwelijke <ind>kameel</ind></trd>
            </trdgrp>
            <trd
            lng="ru">&c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_i;&c_c;&c_a;</trd>
          </ekz>
        </dif>
        <trd lng="be">&c_v;&c_ja;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
        <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_l;&c_a;</trd>
        <trd lng="br">ka&ntilde;val</trd>
        <trd lng="ca">camell</trd>
        <trd lng="de">Kamel</trd>
        <trd lng="en">camel</trd>
        <trd lng="es">camello</trd>
        <trd lng="fr">chameau</trd>
        <trd lng="hu">teve</trd>
        <trd lng="nl">kameel</trd>
        <trd lng="pl">wielb&lstroke;&aogonek;d</trd>
        <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
        <trd lng="sv">kamel</trd>
        <trd lng="tr">deve</trd>
      </snc>
    </drv>

    <drv mrk="kamel.de0igxi">
      <kap>de<tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Lasi la rajdadon de <tld/>o:
          <ekz>
            <klr>[unu karavanano]</klr> de<tld/>i&gcirc;is, donis la
            bridon
            al kunrajdanto, kaj rapidpa&scirc;e venis anta&ubreve;en
            <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 1</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">desmuntar del camell</trd>
      <trd lng="fr">descendre de chameau</trd>
      <trd lng="nl">afstijgen<klr tip="ind"> (v.e.kameel)</klr></trd>
      <trd lng="pl">zsiada&cacute; z
      <ind>wielb&lstroke;&aogonek;da</ind></trd>
      <trd lng="ru">&c_s;&c_p;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;
      &c_s; &c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_a;</trd>
    </drv>
  </art>
