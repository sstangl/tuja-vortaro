<art mrk="$Id: polistiren.xml,v 1.7 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>polistiren</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="polistiren.0o">
  <kap><tld/>o</kap>
  <snc mrk="polistiren.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="plast.0o.TEK">Plasto</ref> solida, 
      rompebla, kiun oni povas fari diafana a&ubreve; kolora, substanco de 
      multaj komunuzaj objektoj:
      <ekz>
        <tld/>o estas termoplasta rezino
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url 
          ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2006-09-12</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <rim mrk="polistiren.0o.RIM_SIN">
    Alia vortoformo:
    <ref tip="sin" cel="polistirol.0o">polistirolo</ref>.
  </rim>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_s;&c_t;&c_i;&c_r;&c_o;&c_l;</trd>
  <trd lng="ca">poliestir&eacute;</trd>
  <trd lng="en">polystyrene</trd>
  <trd lng="fr">polystyr&egrave;ne</trd>
  <trdgrp lng="it">
    <trd>polistirene</trd>,
    <trd>polistirolo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isomo</trd>,
    <trd>piepschuim</trd>,
    <trd>polystyreen</trd>
  </trdgrp>
  <trd lng="sk">polystyr√©n</trd>
  <trd lng="cs">polystyren</trd>
</drv>
</art>

