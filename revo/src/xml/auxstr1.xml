<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxstr1.xml,v 1.22 2014/09/28 03:10:24 revo Exp $">
<kap>
  <rad>A&ubreve;strali</rad>/o <fnt><bib>ZR</bib></fnt>
</kap>

<drv mrk="auxstr1.A0o">
  <kap><tld/>o</kap>
  <snc mrk="auxstr1.A0o.kontinento">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej malgranda inter la
      <ref tip="lst" cel="kontin.0o.terkuno"
      lst="voko:kontinentoj">kontinentoj</ref>,
      situanta inter Pacifiko kaj Hinda Oceano.
    </dif>
    <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
  </snc>
  <snc mrk="auxstr1.A0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      La sola <ref tip="lst" cel="sxtat.0o"
      lst="voko:&scirc;tatoj">&scirc;tato</ref>
      de la <ref tip="malprt" cel="auxstr1.A0o.kontinento"><tld
      lit="a"/>a</ref>
      kontinento:
      <ekz>
        &scirc;i helpis anka&ubreve; al elmigrintaj &ccirc;inoj ekz.
        en Usono, Kanado, <tld/>o, Singapuro kaj Hongkongo, kiuj
        deziris edzinigi junulinojn el la &ccirc;ina &ccirc;eflando
        <fnt><bib>Monato</bib>,
        <vrk>Noticoj &#8208; Eminenta svatistino</vrk>,
        <lok>1993:09</lok></fnt>;
      </ekz>
      <ekz>
        la pasio pri stereogramoj rapide disvasti&gcirc;is al aliaj
        landoj, la &leftquot;fundamento&rightquot; entenas tri librojn
        kun la titolo Magic Eye (Magia Okulo), da&ubreve;re inter la
        plej venditaj libroj en Usono, <tld/>o, Brazilio,
        Britio, Germanio
        <fnt><bib>Monato</bib>,
        <vrk>Stereogramoj &#8208; La modo de fiksokuloj</vrk>,
        <lok>1995:01</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_w;&c_s;&c_t;&c_r;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_A;&c_v;&c_s;&c_t;&c_r;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">Austr&agrave;lia</trd>
  <trd lng="de">Australien</trd>
  <trd lng="en">Australia</trd>
  <trd lng="es">Australia</trd>
  <trd lng="fr">Australie</trd>
  <trd lng="hu">Ausztr&aacute;lia</trd>
  <trd lng="id">Australia</trd>
  <trd lng="it">Australia</trd>
  <trd lng="nl">Australi&euml;</trd>
  <trd lng="pl">Australia</trd>
  <trd lng="ru">&c_A;&c_v;&c_s;&c_t;&c_r;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Austr&aacute;lia</trd>
  <trd lng="tr">Avusturalya</trd>
  <trd lng="vo">Talop</trd>
  <trd lng="pt">Austr&aacute;lia</trd>
</drv>

<drv mrk="auxstr1.okcidenta0o">
  <kap>Okcidenta <tld/>o</kap>
  <snc mrk="auxstr1.okcidenta0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Okcidenta sub&scirc;tato de
      <ref tip="malprt" cel="auxstr1.A0o.sxtato">A&ubreve;stralio</ref>:
      <ekz>
        estis plantitaj du ru&gcirc;aj rozarbustoj <klr>...</klr> 
        anta&ubreve; la kolegio &rightquot;Sankta Georgo&#8221; de Okcidenta 
        <tld/>o
        <fnt>
          <aut>J. Bishop</aut>,
          <vrk>En la plej izolita urbo de la mondo</vrk>,
          <lok><url ref="http://esperanto-ondo.ru/89-lode.htm">
          </url>La Ondo de Esperanto, 2002:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Western Australia</trd>
  <trd lng="fr">Australie occidentale</trd>
</drv>

<drv mrk="auxstr1.suda0o">
  <kap>Suda <tld/>o</kap>
  <snc mrk="auxstr1.suda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Suda sub&scirc;tato de
      <ref tip="malprt" cel="auxstr1.A0o.sxtato">A&ubreve;stralio</ref>:
      <ekz>
        la LKK informis &ccirc;iujn &jcirc;urnalojn en Suda <tld/>o pri la 
        kongreso
        <fnt>
          <aut>T. Steele</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/H-steele.htm">
          &Ccirc;u malesperi?</url></vrk>,
          <lok>La Ondo de Esperanto, 2011:11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">South Australia</trd>
  <trd lng="fr">Australie m&eacute;ridionale</trd>
</drv>
</art>
