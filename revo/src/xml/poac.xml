<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: poac.xml,v 1.12 2014/02/17 13:10:18 revo Exp $">
<kap><rad>poac</rad>/oj</kap>
<drv mrk="poac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="poac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      herboj (<trd lng="la">Poaceae</trd>), kun pli ol 300 genroj:
      <refgrp tip="sub">
        <ref cel="aven.0o">aveno</ref>,
        <ref cel="horde.0o">hordeo</ref>,
        <ref cel="maiz.0o">maizo</ref>,
        <ref cel="sekal.0o">sekalo</ref>,
        <ref cel="tritik.0o">tritiko</ref>.
      </refgrp>
    </dif>
  </snc>
  <bld lok="../bld/po1.png">
    folio<klr>: 1. foliplato (limbo), 2. ligulo, 3. oreleto, 4.
    foliingo,
    5. folinodo</klr>; floro<klr>: a. stameno, b. pistilo, c. lodiklo,
    d. interna paleo, e. ekstera paleo, f. aristo</klr>; spiketo<klr>:
    A. ekstera glumo, B. interna glumo, C. unuopa floro</klr>
  </bld>
  <trdgrp lng="ca">
    <trd>po&agrave;cies</trd>,
    <trd>gram&iacute;nies</trd>
  </trdgrp>
  <trd lng="de">S&uuml;&szlig;gr&auml;ser</trd>
  <trdgrp lng="en">
    <trd>poaceae</trd>,
    <trd>gramineae</trd>
    <trd>true grasses</trd>    
  </trdgrp>  
  <trd lng="es">gram&iacute;neas</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;zsitf&udblac;f&eacute;l&eacute;k</trd>,
    <trd>perjef&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">echte <ind>grassen</ind><klr tip="ind">
  (fam.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wiechlinowate</trd>,
    <trd>trawy</trd>
  </trdgrp>
  <trd lng="pt">po&aacute;ceas</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">gr&auml;s</trd>
</drv>
</art>

