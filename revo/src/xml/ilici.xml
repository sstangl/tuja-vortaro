<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ilici.xml,v 1.13 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>ilici</rad>/o
</kap>
<drv mrk="ilici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
cel="magnoli1.0oj">magnoliacoj</ref>
      (<trd lng="la">Illicium</trd>), la fruktoj de unu specio estas
      uzataj kiel
      spica&jcirc;o.
    </dif>
    <ref tip="prt" cel="aniz.stel0o">stelanizo</ref>
  </snc>
  <trd lng="cs">padián</trd>
  <trd lng="de">Sternanisbaum</trd>
  <trd lng="en">star anise</trd>
  <trdgrp lng="hu">
    <trd>csillag&aacute;nizs</trd>,
    <trd>&aacute;nizscsillag</trd>
  </trdgrp>
  <trd lng="nl">steranijsboom</trd>
  <trdgrp lng="pt">
   <trd>badiana</trd>, 
   <trd>anis-estrelado</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_z;&c_v;&c_jo;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;
&c_a;&c_n;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">padián</trd>
</drv>
</art>

