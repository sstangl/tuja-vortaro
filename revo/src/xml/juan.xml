<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: juan.xml,v 1.12 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>juan</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="juan.0o">
  <kap><tld/>o</kap>
  <mlg>CNY</mlg>
  <mlg>&#1200;</mlg>
  <mlg>&#20803;</mlg>
  <snc mrk="juan.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON"
      lst="voko:monunuoj">Monunuo</ref> 
      de &Ccirc;inujo:
      <ekz>
        la onklino ricevos nur akciojn valorantajn kvincent <tld/>ojn
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 35a, p. 505a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>iuan</trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="cs">juan</trd>
  <trd lng="de">Yuan</trd>
  <trd lng="en">yuan</trd>
  <trdgrp lng="fr">
    <trd>yuan<klr> (monnaie chinoise)</klr></trd>,
    <trd>renminbi</trd>
  </trdgrp>
  <trd lng="id">yuan</trd>
  <trd lng="it">yuan</trd>
  <trd lng="pl">juan</trd>
  <trdgrp lng="pt">
    <trd>iuane</trd>,
    <trd>yuan</trd>
  </trdgrp>
  <trd lng="sk">juan</trd>
</drv>
</art>

