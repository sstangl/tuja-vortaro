<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolon1.xml,v 1.9 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kolon</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kolon1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolon1.0o.EKON">
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON"
      lst="voko:monunuoj">Monunuo</ref> de Kostariko 
      kaj, kune kun la usona dolaro, de Salvadoro.
    </dif>
  </snc>
  <rim mrk="kolon1.0o.RIM_HOM">
    Fundamente <ref tip="hom" cel="kolon.0o">kolono</ref> estas 
    dika fosto subtenanta konstrua&jcirc;on.
    </rim>
  <trd lng="en">col&oacute;n</trd>
  <trd lng="es">col&oacute;n</trd>
  <trd lng="fr">col&oacute;n<klr> (monnaie)</klr></trd>
  <trd lng="id">colon</trd>
  <trd lng="nl">col&oacute;n<klr tip="ind"> (munt)</klr></trd>
  <trd lng="pl">col&oacute;n<klr> (1 col&oacute;n kostaryka&nacute;ski = 100 
  centavos)</klr></trd>
  <trdgrp lng="pt">
    <trd>colom</trd>,
    <trd>col&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolónka</trd>,
    <trd>stĺpec</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>colon</trd>,
    <trd>kolon</trd>,
    <trd>tračník</trd>
  </trdgrp>
</drv>
</art>
