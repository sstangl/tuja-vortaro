<kap>
  <rad>kinematik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kinema.0o">
  <kap><tld/>o</kap>
  <fnt><bib>MatTerm</bib><lok>p. 42</lok></fnt>
  <snc mrk="kinema.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Bran&ccirc;o de 
      <ref tip="malprt" cel="mehxan.0o.MAT">me&hcirc;aniko</ref>,
      pritraktanta la movojn nekonsiderante la fortojn, kiuj ilin
      elvokas:
      <ekz>
        mi <klr>...</klr> ofte uzis a&ubreve;tomobiletojn por komprenigi
        al miaj klasoj kelkajn principojn de la <tld/>o
        <fnt>
          <aut>R. Pigro</aut>,
          <vrk>&Scirc;ton&jcirc;eto sen cenzuro</vrk>,
          <bib>Monato</bib>,
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_e;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">cinem&agrave;tica</trd>
  <trd lng="cs">kinematika</trd>
  <trd lng="de">Kinematik</trd>
  <trd lng="en">kinematics</trd>
  <trd lng="es">cinem&aacute;tica</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&f_y;&a_n;&a_m;&a_A;&a_t;&f_y;&f_k;</trd>,
    <trd>&a_H;&a_r;&f_k;&a_t;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;matique</trd>
  <trdgrp lng="hu">
    <trd>kinematika</trd>,
    <trd>mozg&aacute;stan</trd>
  </trdgrp>
  <trd lng="it">cinematica</trd>
  <trdgrp lng="nl">
    <trd>kinematica</trd>,
    <trd>bewegingsleer</trd>
  </trdgrp>
  <trd lng="pl">kinematyka</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
