<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: panari.xml,v 1.13 2015/07/04 08:37:40 revo Exp $">
<kap><rad>panarici</rad>/o</kap>
<drv mrk="panari.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la &ccirc;ela histo de fingro.
    </dif>
  </snc>
  <trd lng="cs">panaritium</trd>
  <trd lng="fr">panaris</trd>
  <trdgrp lng="hu">
    <trd>panaritium</trd>,
    <trd>ujjphlegmone</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>cantengan</trd>,
    <trd>paronikia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fijt</trd>,
    <trd>vingerontsteking</trd>,
    <trd>omloop</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>paron&iacute;quia</trd>,
    <trd>panar&iacute;cio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_a;&c_r;&c_i;&c_c;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_o;&c_g;&c_t;&c_o;&c_je;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_t;&c_i;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">hnisavý zápal prstov</trd>
</drv>
</art>
