<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kartel.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kartel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kartel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Interkonsento de organizoj por komuna kunagado:
      <ekz>
        <ind><tld/>o ekonomia</ind>
        <klr>(bazi&gcirc;anta sur kontrakto de produktantoj
        kun la celo forigi la liberan konkuradon kaj influi la prezojn
        la&ubreve;
        komuna reguligo de produktado kaj debito)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>o politika</ind>
        <klr>(interkonsento de politikaj partioj por komuna
        kunagado)</klr>.
        <trd lng="de">Koalition</trd>
        <trd lng="fa">&a_A;&a_y_hamza;&a_t;&a_l;&a_A;&a_f;</trd>
        <trdgrp lng="hu">
          <trd>politikai kartell</trd>,
          <trd>koal&iacute;ci&oacute;</trd>
        </trdgrp>
        <trd lng="nl">coalitie</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konzer.0o">konzerno</ref>,
      <ref cel="trust.0o">trusto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="cs">kartel</trd>
  <trdgrp lng="de">
    <trd>Kartell</trd>,
    <trd>Ring</trd>
  </trdgrp>
  <trd lng="en">cartel</trd>
  <trd lng="es">c&aacute;rtel</trd>
  <trd lng="fa">&f_k;&a_A;&a_r;&a_t;&a_l;</trd>
  <trd lng="fr">cartel<klr tip="ind"> (entente)</klr></trd>
  <trd lng="hu">kartell</trd>
  <trd lng="nl">kartel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">kartel</trd>
</drv>
</art>
