<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolcxi.xml,v 1.13 2013/08/27 10:10:35 revo Exp $">
<kap>
<rad>kol&ccirc;ik</rad>/o
</kap>
<drv mrk="kolcxi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Colchicum</trd>), venena vegeta&jcirc;o kun violkoloraj
      floroj, perdanta dum la florado en a&ubreve;tuno la foliojn. 
    </dif>
  </snc>
  <trd lng="bg">&c_je;&c_s;&c_je;&c_n;&c_je;&c_n; &c_m;&c_i;&c_n;&c_z;&c_u;&c_h;&c_a;&c_r;</trd>
  <trd lng="de">Herbstzeitlose</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l; &a_H;&a_s;&a_r;&a_t;</trd>,
    <trd>&a_s;&a_w;&a_r;&a_n;&a_j;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>colchique</trd>,
    <trd>safran des pr&eacute;s</trd>,
    <trd>tue-<ind>chien</ind></trd>
  </trdgrp>
  <trd lng="hu">kikerics</trd>
  <trdgrp lng="nl">
    <trd>tijloos</trd>,
    <trd>herfsttijloos</trd>,
    <trd>naakt <ind>begijntje</ind></trd>,
    <trd>kind-voor-de-vader</trd>,
    <trd>kale <ind>juffer</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_i;&c_k; &c_o;&c_s;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
