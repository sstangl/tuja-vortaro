<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: divertikulit.xml,v 1.2 2013/11/16 20:10:22 revo Exp $">
<kap>
  <rad>divertikulit</rad>/o
</kap>
<drv mrk="divertikulit.0o">
  <kap><tld/>o</kap>
  <snc mrk="divertikulit.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="divertikul.0o.ANA">Divertikul</ref>-<ref
      tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>
      ka&ubreve;zata de infekti&gcirc;o en la sigmoida kojlo:
      <ekz>
        <tld/>o estas konstanta, kaj oni nur povas regi &gcirc;in per dieto
        <fnt>
          <aut>Skat&ccirc;jo</aut>,
          <vrk><url
          ref="http://usono.wordpress.com/2010/10/16/malsanulejo/">
          Malsanulejo</url></vrk>,
          <lok>2010-10-16</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <rim mrk="divertikulit.0o.RIM_SIN">
    <ctl>Lexique m&eacute;dical fran&ccedil;ais-esp&eacute;ranto</ctl>
    (http://vortareto.free.fr/indexlexique.htm)
    proponas la formojn <em>divertiklinflamo, 
    divertiklito</em>.
  </rim>
  <trd lng="en">diverticulitis</trd>
  <trdgrp lng="fr">
    <trd>diverticulite du sigmo&iuml;de</trd>,
    <trd>sigmo&iuml;dite</trd>
  </trdgrp>
</drv>
</art>

