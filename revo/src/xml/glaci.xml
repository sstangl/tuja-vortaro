<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: glaci.xml,v 1.34 2015/07/04 08:37:35 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>glaci</rad>/o
    </kap>

    <drv mrk="glaci.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Akvo malfluidi&gcirc;inta, solidi&gcirc;inta pro malvarmo:
          <ekz>
            en la printempo la <tld/>o kaj la ne&gcirc;o fluidi&gcirc;as
            <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
          </ekz><ekz>
            la rivero forrulas <tld/>on;
          </ekz><ekz>
            artefarita <tld/>o;
          </ekz><ekz>
            <ind>konstrui kastelon sur <tld/>o</ind>
            <klr>(<ref tip="vid"
                cel="aer.0o">aero</ref>)</klr><fnt><bib>PrV</bib></fnt>;
            <trd lng="hu">leveg&odblac;be &eacute;p&iacute;t v&aacute;rat</trd>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            &scirc;ia alveno disrompis la <tld/>on
            <klr>(&ccirc;esigis la inter&gcirc;enatecon)</klr>
            de la &gcirc;isnuna rigida konversacio<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <ind>mielo sur lango kaj <tld/>o <klr>(indiferenteco)</klr>
            en
              koro</ind><fnt><bib>PrV</bib></fnt>.
            <trd lng="hu">szava m&eacute;z, sz&iacute;ve j&eacute;g</trd>
          </ekz>
        </dif>
        <ref tip="vid" cel="frost.0o">frosto</ref>
      </snc>
  <trd lng="bg">&c_l;&c_je;&c_d;</trd>
  <trd lng="cs">led v přírodě</trd>
  <trd lng="de">Eis</trd>
  <trd lng="en">ice</trd>
  <trd lng="es">hielo</trd>
  <trd lng="fa">&f_y;&a_kh;</trd>
  <trd lng="fr">glace<klr tip="ind"> (eau gel&eacute;e)</klr></trd>
  <trd lng="hu">j&eacute;g</trd>
  <trd lng="id">es</trd>
  <trd lng="nl">ijs</trd>
  <trd lng="pl">l&oacute;d</trd>
  <trd lng="pt">gelo</trd>
  <trd lng="ru">&c_l;&c_jo;&c_d;</trd>
  <trd lng="sk">ľad</trd>
  <trd lng="sv">is</trd>
  <trd lng="tr">buz</trd>
</drv>

    <drv mrk="glaci.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Karakterizata per <tld/>o:
          <ekz>
            <tld/>a praepoko;
          </ekz><ekz>
            <tld/>aj maroj.
          </ekz>
        </dif>
        <trd lng="de">Eis-</trd>
        <trdgrp lng="hu">
          <trd>j&eacute;g-</trd>,
          <trd>jeges</trd>
        </trdgrp>
        <trd lng="nl">ijs-</trd>
        <trd lng="pt">glacial</trd>
        <trdgrp lng="ru">
          <trd>&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>,
          <trd>&c_l;&c_je;&c_d;&c_o;&c_v;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Malvarmega:
          <ekz>
            <tld/>a vento;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>a <klr>(malardega, malpasiega)</klr> viza&gcirc;o;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>a <klr>(malfavorega, malafablega)</klr> akcepto;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            la <tld/>a tavolo da anta&ubreve;ju&gcirc;oj de la mondo estas rompita
            <fnt>(<aut>Zamenhof</aut>, <vrk>Parolo anta&ubreve; la Tria
                Kongreso Esperantista en Cambridge, 1907</vrk>)</fnt>.
          </ekz>
        </dif>
        <trd lng="de">eisig</trd>
        <trd lng="hu">j&eacute;ghideg</trd>
        <trd lng="nl">ijskoud</trd>
        <trd lng="pt">g&eacute;lido</trd>
        <trd lng="ru">&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
      </snc>
      <trd lng="en">icy</trd>
      <trdgrp lng="fa">
        <trd>&f_y;&a_kh;&f_y;</trd>,
        <trd>&f_y;&a_kh;&zwnj;&f_p;&a_w;&a_sh;&f_y;&a_d;&a_h;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>glac&eacute;<klr tip="ind"> (froid)</klr></trd>,
        <trd>glaciaire</trd>,
        <trd>glacial</trd>
      </trdgrp>
      <trd lng="nl">ijzig</trd>
    </drv>

    <drv mrk="glaci.0aro">
      <kap><tld/>aro</kap>
      <snc mrk="glaci.0aro.GEOG">
        <uzo tip="stl">RAR</uzo>
        <uzo tip="fak">GEOG</uzo>
        <ref tip="dif" cel="glaci.0ejo.GEOG">Glaciejo<sncref/></ref>
      </snc>
  <trdgrp lng="bg">
        <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>,
        <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trd lng="cs">ledovec</trd>
  <trd lng="de">Gletscher</trd>
  <trd lng="en">glacier</trd>
  <trd lng="es">glaciar</trd>
  <trd lng="fa">&f_y;&a_kh;&f_ch;&a_A;&a_l; &a_T;&a_b;&f_y;&a_ayn;&f_y;</trd>
  <trd lng="fr">glacier<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trdgrp lng="hu">
        <trd>j&eacute;gtakar&oacute;</trd>,
        <trd>j&eacute;gmez&odblac;</trd>,
        <trd>gleccser</trd>
      </trdgrp>
  <trd lng="nl">gletsjer</trd>
  <trd lng="pt">geleira</trd>
  <trdgrp lng="ru">
        <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;<klr> (&c_v; &c_g;&c_o;&c_r;&c_a;&c_h;)</klr></trd>,
        <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>glečer</trd>,
    <trd>ľadovec</trd>
  </trdgrp>
</drv>

    <drv mrk="glaci.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="glaci.0ejo.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Surtera, precipe surmonta, amasego da <tld/>io el ne&gcirc;o dum
          jarcentoj transformi&gcirc;inta en <tld/>on:
          <ekz>
            la korpo de prahistoria homo, trovita anta&ubreve; dek jaroj en
            <tld/>ejo en la Alpoj, estos malfrostigita
            <fnt>
              <aut>vgeni GEORGIEV</aut>:
              <vrk>Pa&scirc;tisto de la pasinteco</vrk>. <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            la natura pejza&gcirc;o <klr>...</klr> estas varia rezulte de
            erozio de la antikvaj <tld/>ejoj, hodia&ubreve; ne plu
            ekzistantaj
            <fnt>
              <aut>Jos&eacute; de N. MARQUES</aut>:
              <vrk>Voja&gcirc;o al Ushuaia</vrk>. <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            vi falis en grandan fendegon, kiu trairas la tutan
            <tld/>ejon tra &gcirc;ia tuta dikeco
            <fnt>
              <aut>Henri Vallienne</aut>: <vrk>&Ccirc;u li?</vrk>.
              <lok>&Ccirc;ap. 6a.</lok>
            </fnt>
          </ekz>
        </dif>
        <trdgrp lng="bg">
          <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>,
          <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>
        </trdgrp>
        <trd lng="de">Gletscher</trd>
        <trd lng="en">glacier</trd>
        <trd lng="es">glaciar</trd>
        <trd lng="fa">&f_y;&a_kh;&f_ch;&a_A;&a_l; &a_T;&a_b;&f_y;&a_ayn;&f_y;</trd>
        <trd lng="fr">glacier<klr tip="ind"> (g&eacute;og.)</klr></trd>
        <trdgrp lng="hu">
          <trd>j&eacute;gtakar&oacute;</trd>,
          <trd>j&eacute;gmez&odblac;</trd>,
          <trd>gleccser</trd>
        </trdgrp>
        <trd lng="id">gletser</trd>
        <trd lng="nl">gletsjer</trd>
        <trd lng="pt">geleira</trd>
        <trdgrp lng="ru">
          <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;<klr tip="ind"> (&c_v; &c_g;&c_o;&c_r;&c_a;&c_h;)</klr></trd>,
          <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>
        </trdgrp>
      </snc>
      <snc mrk="glaci.0ejo.ujo">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="glaci.0ujo0sxranko0tenejo"><tld/>ujo</ref>
      </snc>
  <trd lng="cs">ledovec</trd>
  <trd lng="sk">ľadovec</trd>
</drv>

    <drv mrk="glaci.0ujo0sxranko0tenejo">
      <kap><tld/>ujo, <var><kap><tld/>&scirc;ranko</kap></var>,
      <var><kap><tld/>tenejo</kap></var></kap>
      <snc>
        <dif>
          Kesto a&ubreve; ejo destinita por konservi <tld/>on.
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="frid.0ujo">fridujo</ref>,
        <ref cel="frost.0ofako">frostofako</ref>,
        <ref cel="frost.0ujo">frostujo</ref>.
      </refgrp>
      <trd lng="bg">&c_h;&c_l;&c_a;&c_d;&c_i;&c_l;&c_n;&c_i;&c_k;</trd>
      <trd lng="de">Eisschrank</trd>
      <trdgrp lng="en">
        <trd>ice box</trd>,
        <trd>ice chest</trd>,
        <trd>ice cellar</trd>,
        <trd>cold store</trd>,
        <trd>freezer</trd>
      </trdgrp>
      <trdgrp lng="fa">
        <trd>&f_y;&a_kh;&f_ch;&a_A;&a_l;</trd>,
        <trd>&f_y;&a_kh;&a_d;&a_A;&a_n;</trd>
      </trdgrp>
      <trd lng="fr">glaci&egrave;re</trd>
      <trd lng="es">hielera</trd>
      <trdgrp lng="hu">
        <trd>j&eacute;gszekr&eacute;ny</trd>,
        <trd>j&eacute;gverem</trd>,
        <trd>h&udblac;t&odblac;l&aacute;da</trd>
      </trdgrp>
      <trd lng="nl">ijskast</trd>
      <trd lng="pt">geladeira</trd>
      <trd lng="ru">&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;<klr> (&c_ja;&c_shch;&c_i;&c_k; &c_s;&c_o; &c_l;&c_mol;&c_d;&c_o;&c_m;)</klr></trd>
      <trd lng="tr">buz dolab&inodot;</trd>
      <trd lng="sk">ľadovňa <klr>(na úschovu ľadu)</klr></trd>
  <trdgrp lng="cs">
    <trd>chladírna</trd>,
    <trd>ledárna</trd>
  </trdgrp>
</drv>

    <drv mrk="glaci.0ajxo">
      <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
      <snc mrk="glaci.0ajxo.KUI">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Franda&jcirc;o farita el sukero kaj <tld/>igita kremo kun
          bongustaj aldona&jcirc;oj:
          <ekz>
            <tld/>a&jcirc;o estas dol&ccirc;a glaciigita franda&jcirc;o
            <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
          </ekz><ekz>
            vanila, &ccirc;okolada <tld/>a&jcirc;o.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_s;&c_l;&c_a;&c_d;&c_o;&c_l;&c_je;&c_d;</trd>
      <trdgrp lng="de">
        <trd>Eis</trd>,
        <trd>Speiseeis</trd>
      </trdgrp>
      <trd lng="en">ice cream</trd>
      <trd lng="es">helado</trd>
      <trd lng="fa">&a_b;&a_s;&a_t;&a_n;&f_y;</trd>
      <trd lng="fr">glace<klr tip="ind"> (aliment)</klr></trd>
      <trd lng="hu">fagylalt</trd>
      <trd lng="id"><ind>es</ind> krim</trd>
      <trd lng="pl">lody</trd>
      <trd lng="nl">ijsroom</trd>
      <trd lng="pt">sorvete</trd>
      <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_o;&c_je;</trd>
      <trd lng="sv">glass</trd>
      <trd lng="tr">dondurma</trd>
    </drv>

    <drv mrk="glaci.0igi">
      <kap><ofc>*</ofc><tld/>igi</kap>
      <snc>
        <dif>
          Malfluecigi, solidigi flua&jcirc;on per malvarmo:
          <ekz>
            la frosto <tld/>igis la lagon.
          </ekz>
        </dif>
        <trd lng="hu">fagyaszt</trd>
        <trd lng="tr">dondurmak</trd>
        <trd lng="pt">congelar</trd>
        <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;
          &c_l;&c_mol;&c_d;&c_o;&c_m;</trd>
      </snc>
      <snc>
        <dif>
          Meti en malvarmegan staton:
          <ekz>
            la ekstera frosto <tld/>igis min, miajn fingrojn;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            tute <tld/>igita de teruro.
          </ekz>
        </dif>
        <trd lng="hu">dermeszt</trd>
        <trd lng="pt">congelar</trd>
        <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>mrazit</trd>,
    <trd>nechat zmrznout</trd>,
    <trd>zmrazit</trd>
  </trdgrp>
  <trd lng="de">vereisen</trd>
  <trd lng="en">freeze</trd>
  <trd lng="es">congelar</trd>
  <trd lng="fa">&a_m;&a_n;&a_j;&a_m;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
        <trd>glacer<klr tip="ind"> (transformer en glace)</klr></trd>,
        <trd>geler<klr tip="ind"> (tr.)</klr></trd>
      </trdgrp>
  <trd lng="id">[<ind>beku</ind>] membekukan</trd>
  <trd lng="nl">(doen) <ind>bevriezen</ind></trd>
  <trd lng="sk">mraziť</trd>
</drv>

    <drv mrk="glaci.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Malfluecigi pro malvarmo:
          <ekz>
            la rivero <tld/>i&gcirc;as.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>eljegesedik</trd>,
          <trd>j&eacute;gg&eacute; fagy</trd>
        </trdgrp>
        <trd lng="tr">donmak</trd>
      </snc>
      <snc>
        <dif>
          Veni en malvarmegan staton:
          <ekz>
            la soldatoj <tld/>i&gcirc;is dum la nokta gardostaro;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            la rideto <tld/>i&gcirc;is <klr>(haltis, malaperis)</klr>
            sur &scirc;iaj lipoj<fnt>B</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <tld/>i&gcirc;i de timo.
          </ekz>
        </dif>
        <trd lng="hu">megdermed</trd>
      </snc>
      <trd lng="de">vereisen</trd>
      <trd lng="en">freeze</trd>
      <trd lng="es">congelarse</trd>
      <trdgrp lng="fa">
        <trd>&f_y;&a_kh; &a_b;&a_s;&a_t;&a_n;</trd>,
        <trd>&a_m;&a_n;&a_j;&a_m;&a_d; &a_sh;&a_d;&a_n;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>se <ind>glacer</ind></trd>,
        <trd>geler<klr tip="ind"> (intr.)</klr></trd>
      </trdgrp>
      <trd lng="id">[<ind>beku</ind>] membeku</trd>
      <trd lng="nl">bevriezen</trd>
      <trd lng="pt">congelar-se</trd>
      <trdgrp lng="ru">
        <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_s;&c_t;&c_y;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="glaci.0pluvo">
      <kap><tld/>pluvo</kap>
      <snc mrk="glaci.0pluvo.MET">
        <uzo tip="fak">MET</uzo>
        <dif>
          Pluvo, kiu glitige <tld/>i&gcirc;as tu&scirc;ante la teron.
        </dif>
        <refgrp tip="vid">
          <ref cel="hajl.0o">hajlo</ref>,
          <ref cel="negx.0o">ne&gcirc;o</ref>,
          <ref cel="veter">glitvetero</ref>.
        </refgrp>
      </snc>
      <adm>
        La Portugala traduko versxajne estas erara. <aut>LP</aut>
      </adm>
      <trd lng="de">Eisregen</trd>
      <trd lng="en">sleet</trd>
      <trd lng="es">granizo</trd>
      <trd lng="fa">&a_b;&a_A;&a_r;&a_A;&a_n; &a_m;&a_n;&a_j;&a_m;&a_d;</trd>
      <trd lng="fr">pluie vergla&ccedil;ante</trd>
      <trd lng="hu">j&eacute;ges&odblac;</trd>
      <trd lng="id"><ind>hujan</ind> es</trd>
      <trd lng="nl">sneeuwwater</trd>
      <trd lng="pt">chuva de gelo</trd>
      <trd lng="sk">mrznúci dážď</trd>
  <trd lng="cs">mrznoucí déšť</trd>
</drv>

    <drv mrk="glaci.0insulo">
      <kap><tld/>insulo, <var><kap><tld/>monto</kap></var></kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="glaci.0insulo.GEOG">
        <dif>
          Pecego da <tld/>o libere flosanta en polusaj kaj apudpolusaj
          maroj.
        </dif>
        <refgrp tip="vid">
          <ref cel="bankiz.0o">bankizo</ref>
        </refgrp>
      </snc>
  <trd lng="cs">plovoucí ledovec <klr>(hora)</klr></trd>
  <trd lng="de">Eisberg</trd>
  <trd lng="en">iceberg</trd>
  <trd lng="es">t&eacute;mpano</trd>
  <trd lng="fa">&f_k;&a_w;&a_h; &f_y;&a_kh;</trd>
  <trd lng="fr">iceberg</trd>
  <trd lng="hu">j&eacute;ghegy</trd>
  <trd lng="id"><ind>gunung</ind> es</trd>
  <trd lng="nl">ijsberg</trd>
  <trd lng="pl">g&oacute;ra lodowa</trd>
  <trdgrp lng="pt">
        <trd>iceberg</trd>,
        <trd>aicebergue</trd>
      </trdgrp>
  <trd lng="ru">&c_a;&c_j;&c_s;&c_b;&c_je;&c_r;&c_g;</trd>
  <trd lng="sk">plávajúci ľadovec</trd>
</drv>

    <drv mrk="glaci.0rivero">
      <kap><tld/>rivero</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="glaci.0rivero.GEOG">
        <dif>
          Parto de
          <ref tip="malprt"
          cel="glaci.0ejo.GEOG"><tld/>ejo<sncref/></ref>
          forrampanta de supro de monto kaj pleniganta la fundon de
          ties valo:
          <ekz>
            kaj &scirc;i ridis, tiel sonorante, kiel la <tld/>o de la
            <tld/>rivero krakante disi&gcirc;as je eroj, kiam la sunbrilo
            varmigas &gcirc;in
            <fnt>
              <aut>Jen&odblac; Rejt&odblac;, tr. L&aacute;szl&oacute; BAL&Aacute;ZS</aut>:
              <vrk><url
              ref="http://www.freeweb.hu/verkoj/fredlamalpura.htm"
                  >Fred la malpura intervenas</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
