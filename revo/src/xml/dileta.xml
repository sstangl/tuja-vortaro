<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dileta.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>diletant</rad>/o
</kap>
<drv mrk="dileta.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Persono, kiu sin okupas pri arto a&ubreve; scienco neprofesie kaj
      celante nur sian plezuron.
    </dif>
    <refgrp tip="sin">
      <ref cel="amator.0o">amatoro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>diletant <klr>(subst.)</klr></trd>,
    <trd>aficionat <klr>(subst.)</klr></trd>,
    <trd>amateur <klr>(subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>diletant</trd>,
    <trd>neodborník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nichtfachmann</trd>,
    <trd>Dilettant</trd>,
    <trd>Amateur</trd>,
    <trd>Amat&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dilettante</trd>,
    <trd>dabbler</trd>,
    <trd>amateur</trd>
  </trdgrp>
  <trd lng="fr">dilettante</trd>
  <trdgrp lng="hu">
    <trd>m&udblac;kedvel&odblac;</trd>,
    <trd>dilett&aacute;ns</trd>,
    <trd>amat&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dyletant</trd>,
    <trd>nieprofesjonalista</trd>,
    <trd>niefachowiec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>diletante</trd>,
    <trd>amador</trd>,
    <trd>n&atilde;o <ind>profissional</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">neodborník</trd>
</drv>

<drv mrk="dileta.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Spirita stato de tiu, kiu ser&ccirc;as nur sian plezuron en la
      sciencaj
      a&ubreve; artaj okupoj, neniel celante veron a&ubreve;
      &gcirc;eneralan utilon:
      <ekz>
        tiu supra&jcirc;a konado de la arto, nomata artista <tld/>eco
        <fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    &Ccirc;u "spirita stato" a&ubreve; "animstato"? Esplori koherecon
    kaj eventualan senconuancon en la cetero de la verko.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>diletantisme</trd>,
    <trd>amateurisme</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dilettantentum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dilettantism</trd>,
    <trd>amateurism</trd>
  </trdgrp>
  <trd lng="fr">dilettantisme</trd>
  <trdgrp lng="hu">
    <trd>dilettantizmus</trd>,
    <trd>m&udblac;kedvel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dyletanctwo</trd>,
    <trd>nieprofesjonalno&sacute;&cacute;</trd>,
    <trd>niefachowo&sacute;&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">diletantizmus</trd>
  <trdgrp lng="cs">
    <trd>diletantství</trd>,
    <trd>neodbornost</trd>,
    <trd>povrchnost</trd>
  </trdgrp>
</drv>

<drv mrk="dileta.0ismo">
  <kap><tld/>ismo <fnt><bib>PV</bib></fnt></kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="dileta.0eco"><tld/>eco.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Dilettantismus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dilettantism</trd>,
    <trd>amateurism</trd>
  </trdgrp>
  <trd lng="hu">dilettantizmus</trd>
  <trdgrp lng="pl">
    <trd>dyletanctwo</trd>,
    <trd>nieprofesjonalno&sacute;&cacute;</trd>,
    <trd>niefachowo&sacute;&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="dileta.0a">
  <kap><tld/>a</kap>
  <fnt><bib>WED</bib></fnt>
  <snc>
    <dif>
      Neprofesie aganta.
    </dif>
    <refgrp tip="sin">
      <ref cel="amator.0eca">amatoreca</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_ib;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>diletant <klr>(adj.)</klr></trd>,
    <trd>amateur <klr>(adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>diletantský</trd>,
    <trd>neodborný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>laienhaft</trd>,
    <trd>dilettantisch</trd>,
    <trd>amateurhaft</trd>,
    <trd>unprofessionell</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dilettantish</trd>,
    <trd>amateurish</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;kedvel&odblac;</trd>,
    <trd>dilett&aacute;ns</trd>,
    <trd>amat&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dyletancki</trd>,
    <trd>nieprofesjonalny</trd>,
    <trd>niefachowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neodborný</trd>,
    <trd>ochotnícky</trd>
  </trdgrp>
</drv>
<adm>
  &Scirc;ajnas al mi, ke &ccirc;iuj deriva&jcirc;oj havas sufi&ccirc;e
  mal&scirc;atan nuancon kaj tio ne aperas en la difinoj.
  <aut>MB</aut>
</adm>
</art>
