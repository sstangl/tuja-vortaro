<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komik.xml,v 1.20 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>komik</rad>/a
</kap>
<drv mrk="komik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ridiga, amuza:
      <ekz>
        <tld/>a aventuro, gesto, aspekto, situacio;
      </ekz>
      <ekz>
        li <tld/>e tiris sian &ccirc;apelon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komedi.0a">komedia</ref>,
      <ref cel="rid.0inda">ridinda</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_c;&c_e;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>komický</trd>,
    <trd>legrační</trd>,
    <trd>směšný</trd>
  </trdgrp>
  <trd lng="de">komisch</trd>
  <trdgrp lng="en">
    <trd>comic</trd>,
    <trd>comical</trd>,
    <trd>droll</trd>,
    <trd>facetious</trd>,
    <trd>humorous</trd>,
    <trd>laughable</trd>,
    <trd>ludicrous</trd>,
    <trd>mirth-provoking</trd>,
    <trd>witty</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comique</trd>,
    <trd>amusant</trd>,
    <trd>cocasse</trd>,
    <trd>dr&ocirc;le<klr tip="ind"> (adj.)</klr></trd>,
    <trd>marrant</trd>,
    <trd>plaisant<klr tip="ind"> (amusant)</klr></trd>,
    <trd>rigolo<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>komikus</trd>,
    <trd>mulats&aacute;gos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>komisch</trd>,
    <trd>grappig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komiczny</trd>,
    <trd>zabawny</trd>,
    <trd>&sacute;mieszny</trd>
  </trdgrp>
  <trd lng="pt">c&ocirc;mico</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_sh;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komický</trd>,
    <trd>smiešny</trd>
  </trdgrp>
  <trd lng="sv">komisk</trd>
</drv>

<drv mrk="komik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de <tld/>a persono a&ubreve; situacio:
      <ekz>
        la <tld/>eco de la aventuro estas, ke...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    </trd>,
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">c&ograve;mic</trd>
  <trdgrp lng="cs">
    <trd>komično</trd>,
    <trd>komičnost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>comedy</trd>,
    <trd>comicality</trd>,
    <trd>comicalness</trd>,
    <trd>drollery</trd>,
    <trd>drollness</trd>,
    <trd>funniness</trd>,
    <trd>humoursness</trd>,
    <trd>wittiness</trd>
  </trdgrp>
  <trd lng="es">c&oacute;mico</trd>
  <trdgrp lng="fr">
    <trd>comique<klr tip="ind"> (de qc)</klr></trd>,
    <trd>cocasserie</trd>,
    <trd>dr&ocirc;lerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>komikuss&aacute;g</trd>,
    <trd>mulats&aacute;goss&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>grappig</ind>e</trd>,
    <trd>het <ind>komisch</ind>e</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komizm</trd>,
    <trd>komiczno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komickosť</trd>,
    <trd>smiešnosť</trd>,
    <trd>žartovnosť</trd>
  </trdgrp>
</drv>
</art>
