<art mrk="$Id: sitar.xml,v 1.8 2013/09/08 18:11:38 revo Exp $">
<kap>
  <rad>sitar</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="sitar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Nord-hinda <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      granda liuto longa-kola, kun kvar &ccirc;efaj kaj du flankaj kordoj,
      plektre sonigata.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Sitar</trd>
  <trd lng="en">sitar</trd>
  <trdgrp lng="fr">
    <trd>s&icirc;t&acirc;r</trd>
  </trdgrp>
  <trd lng="hu">szit&aacute;r</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
