<vortaro>
<art mrk="$Id: renom.xml,v 1.13 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>renom</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="renom.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda diskonateco:
      <ekz>
        disvasti&gcirc;is via <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fam.0o">famo</ref>,
      <ref cel="reputa.0o">reputacio</ref>,
      <ref cel="glor.0o">gloro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_n;&c_a;&c_m;&c_e;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_u;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>dobrá pověst</trd>,
    <trd>renomé</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ruf</trd>,
    <trd>Leumund</trd>,
    <trd>Renommee</trd>,
    <trd>Ansehen</trd>
  </trdgrp>
  <trd lng="en">renown</trd>
  <trdgrp lng="fr">
    <trd>renomm&eacute;e</trd>,
    <trd>renom</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&eacute;v</trd>,
    <trd>ismerts&eacute;g</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>faam</trd>,
    <trd>vermaardheid</trd>
  </trdgrp>
  <trd lng="pt">renome</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_n;&c_o;&c_m;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobrá povesť</trd>,
    <trd>dobré meno</trd>,
    <trd>renomé</trd>
  </trdgrp>
</drv>
</art>
