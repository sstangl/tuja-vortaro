<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: fand.xml,v 1.29 2015/07/04 08:37:34 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>fand</rad>/i <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="fand.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">TEK</uzo>
      <snc mrk="fand.0i.likvigi">
        <dif>
          Likvigi per varmo solidajn korpojn:
          <ekz>
            <tld/>i metalon<fnt>B</fnt>, vakson, buteron<fnt>B</fnt>,
            ne&gcirc;on;
          </ekz><ekz>
            unu posttagmezon <klr>fulmo</klr> <tld/>is sur Kapitolo
            bronzan statuon de Cereso
            <fnt>
              <aut>H. Sienkewicz, trad. Lidia Zamenhof</aut>,
              <vrk>Quo vadis</vrk>, <lok>&ccirc;ap. 58a</lok>
            </fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <klr>la suno</klr> <tld/>as la asfalton
            <fnt><bib>Metrop</bib></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            Mi ne scias, mi nur sentas, kaj turmento <tld/>as min
            <fnt>
              <aut>Catullus, trad. K. Kalocsay</aut>,
              <vrk>Elegioj</vrk>,
              <lok>Nica Literatura Revuo, 3:6, p. 221</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
          <trd>&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
        </trdgrp>
        <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig
        machen)</klr></trd>
        <trd lng="en">melt</trd>
        <trd lng="hu">olvaszt</trd>
        <trd lng="nl">doen <ind>smelten</ind></trd>
        <trd lng="no">smelte<klr tip="ind"> (gj&oslash;re til en
        v&aelig;ske)</klr></trd>
        <trdgrp lng="pl">
          <trd>topi&cacute;</trd>,
          <trd>roztapia&cacute;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;<klr tip="ind">
          (&c_v;&c_o;&c_s;&c_k;, &c_zh;&c_i;&c_r;)</klr></trd>
        </trdgrp>
      </snc>

      <snc mrk="fand.0i.ekstrakti">
        <ref tip="dif"
        cel="fand.el0i.ekstrakti">el<tld/>i<sncref/></ref>
        <ekz>
          el &scirc;tono oni <tld/>as la kupron
          <fnt><bib>MT</bib>, <lok>&Ijo; 28:2</lok></fnt>;
        </ekz>
        <ekz>
          ar&gcirc;ento, purigita en tera <ind><tld/>ujo</ind> kaj
          sepfoje
          re<tld/>ita
          <fnt><bib>MT</bib>, <lok>&Psa; 12:6</lok></fnt>.
          <trd lng="fr">creuset</trd>
          <trd lng="hu">olvaszt&oacute;t&eacute;gely</trd>
        </ekz>
        <trd lng="pl">wytapia&cacute;</trd>
        <trd
        lng="ru">&c_v;&c_y;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
      </snc>

      <snc mrk="fand.0i.gisi">
        <ref tip="dif" cel="gis.0i.muldi">gisi<sncref/></ref>
        <ekz>
          li <tld/>is por &gcirc;i kvar orajn ringojn sur &gcirc;iaj
          kvar anguloj
          <fnt><bib>MT</bib>, <lok>&Eli; 37:3</lok></fnt>;
        </ekz><ekz>
          ili faris al si <tld/>itan bovidon kaj adorklini&gcirc;is al
          &gcirc;i
          <fnt><bib>MT</bib>, <lok>&Eli; 32:8</lok></fnt>.
        </ekz>
        <rim>
          La&ubreve; UV kaj uzo Zamenhofa, <ekz>
            <tld/>i statuon</ekz>
          povas havi tute kontra&ubreve;ajn signifojn:
          krei (gisi) statuon; a&ubreve; malkrei (likvigi, dis<tld/>i)
          statuon. Tia konfuzo estas malakceptebla en racia lingvo
          kaj &scirc;oka por esperantistoj kies hejma lingvo neniam
          konfuzas tiujn kontra&ubreve;ojn. Tial jam en PV aperis pli
          preciza
          verbo <em>gisi</em> (prunto el la germana) &dash; cetere, mise
          difinita.

          Fojfoje &Z; provis eviti tiun &gcirc;enon uzante la

          deriva&jcirc;on
          el<tld/>i<sncref ref="fand.el0i.gisi"/>; tio tamen prezentas
          alian problemon, &ccirc;ar la&ubreve; la lingva logiko tia
          formo devas
          havi la signifon <ctl>ekstrakti per <tld/>o</ctl> (kaj tia
          logika uzo estas fakte atestita).

          Menciindas, ke en UV la indikitan konfuzon prezentas la
          tradukoj franca, germana kaj angla; dum la tradukoj rusa kaj
          pola donas nur la &ccirc;efsencon <em>likvigi</em>.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trdgrp lng="be">
          <trd>&c_l;&c_ib;&c_c;&c_mol; <klr
          tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>,
          <trd>&c_a;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr
              tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>
        </trdgrp>
        <trd lng="de">gie&szlig;en</trd>
        <trd lng="hu">&ouml;nt</trd>
        <trd lng="nl">gieten<klr tip="ind"> (v.metaal)</klr></trd>
        <trdgrp lng="pl">
          <trd>wytapia&cacute;</trd>,
          <trd>odlewa&cacute;</trd>,
          <trd>la&cacute;<klr> (metal)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind">
          (&c_f;&c_o;&c_r;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>,
          <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="ind">
          (&c_f;&c_o;&c_r;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>rozpustit</trd>,
    <trd>roztavit</trd>,
    <trd>tavit <klr>(kov)</klr></trd>,
    <trd>škvařit kov ,, slévat kov</trd>
  </trdgrp>
  <trd lng="es">fundir</trd>
  <trdgrp lng="fr">
        <trd>fondre<klr tip="ind"> (du m&eacute;tal, un objet,
        etc.)</klr></trd>,
        <trd>liqu&eacute;fier</trd>,
        <trd>couler<klr tip="ind"> (du m&eacute;tal, un objet,
        etc.)</klr></trd>,
        <trd>mouler<klr tip="ind"> (couler)</klr></trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>derreter</trd>,
        <trd>fundir</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>rozpustiť</trd>,
    <trd>taviť <klr>(kov)</klr></trd>,
    <trd>škvariť</trd>
  </trdgrp>
</drv>

    <drv mrk="fand.0ado">
      <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
      <uzo tip="fak">TEK</uzo>
      <snc mrk="fand.0ado.likvigo">
        <dif>
          La ago <tld/>i<sncref ref="fand.0i.likvigi"/> (likvigi):
          <ekz>
            la <tld/>o de plumbo ne postulas multe da varmo
            <fnt><bib>PIV1</bib>, <lok>art. fand</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="pl">
          <trd>roztapianie</trd>,
          <trd>wytop</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_l;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
          <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;
          <klr tip="ind">(&c_l;&c_mol;&c_d;&c_a;,
          &c_zh;&c_i;&c_r;&c_a;)</klr></trd>
        </trdgrp>
      </snc>
      <snc mrk="fand.0ado.ekstraktado">
        <dif>
          La ago <tld/>i<sncref ref="fand.0i.ekstrakti"/>,
          el<tld/>i<sncref ref="fand.el0i.ekstrakti"/>:
        </dif>
        <trdgrp lng="pl">
          <trd>wytop</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_v;&c_y;&c_p;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
          <trd>&c_v;&c_y;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </snc>
      <snc mrk="fand.0ado.gisado">
        <ref tip="dif" cel="gis.0ado.mulda">gisado<sncref/></ref>
        <trdgrp lng="pl">
          <trd>odlew</trd>,
          <trd>lanie</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>,
          <trd>&c_l;&c_i;&c_t;&c_mol;&c_jo;</trd>
        </trdgrp>
      </snc>
  <trd lng="cs">tavba</trd>
  <trd lng="sk">tavba</trd>
</drv>

    <drv mrk="fand.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Flueci&gcirc;i per varmo:
          <ekz>
            vakso <tld/>i&gcirc;as de fajro
            <fnt><bib>MT</bib>, <lok>&Psa; 68:2</lok></fnt>;
          </ekz><ekz>
            <tld/>i&gcirc;anta kandelo.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="degel.0i">degeli</ref>,
          <ref cel="solv">solvi&gcirc;i</ref>.
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
          <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig
        werden)</klr></trd>
        <trd lng="hu">megolvad</trd>
        <trdgrp lng="nl">
          <trd>smelten<klr tip="ind"> (vloeibaar worden)</klr></trd>,
          <trd>vloeibaar worden</trd>
        </trdgrp>
        <trd lng="no">smelte<klr tip="ind"> (bli
        v&aelig;ske)</klr></trd>
        <trdgrp lng="pl">
          <trd>roztapia&cacute; si&eogonek;</trd>,
          <trd>topi&cacute; si&eogonek;</trd>,
          <trd>taja&cacute;<klr> (o &sacute;niegu)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>

      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Kormoli&gcirc;i, morale malforti&gcirc;i:
          <ekz>
            <tld/>i&gcirc;i pro <klr>(a&ubreve; de)</klr> kompato;
          </ekz><ekz>
            &scirc;ia animo <tld/>i&gcirc;is pro plezurego;
          </ekz><ekz>
            lia fiereco <tld/>i&gcirc;is en la
            humili&gcirc;oj<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd
        lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        <trd lng="hu">megenyh&uuml;l</trd>
        <trd lng="nl">smelten</trd>
        <trd lng="no">smelte<klr tip="ind"> (emosjonelt)</klr></trd>
        <trdgrp lng="pl">
          <trd>topnie&cacute;</trd>,
          <trd>taja&cacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
      </snc>

      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Malaperi kunkonfuzi&gcirc;ante:
          <ekz>
            &ccirc;io <tld/>i&gcirc;is en pala krepusko
            <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
          </ekz><ekz>
            la nuboj <tld/>i&gcirc;as kun la horizonto.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="flu">forflui</ref>,
          <ref cel="perd">perdi&gcirc;i</ref>,
          <ref cel="neni">disnenii&gcirc;i</ref>.
        </refgrp>
        <trd
        lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        <trdgrp lng="hu">
          <trd>elmos&oacute;dik</trd>,
          <trd>&ouml;sszeolvad</trd>
        </trdgrp>
        <trd lng="nl">versmelten</trd>
        <trdgrp lng="pl">
          <trd>stapia&cacute; si&eogonek;</trd>,
          <trd>roztapia&cacute; si&eogonek;</trd>
        </trdgrp>
        <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
      </snc>
      <trdgrp lng="fr">
        <trd>fondre<klr tip="ind"> (intr)</klr></trd>,
        <trd>se <ind>fondre</ind></trd>,
        <trd>mollir<klr tip="ind"> (fig., s'attendrir)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="fand.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">TEK</uzo>
      <snc mrk="fand.0isto.materiala">
        <dif>
          Laboristo a&ubreve; fakulo pri
          <ref cel="fand.0ado.ekstraktado"><tld/>ado<sncref/></ref>:
          <ekz>
            vane <tld/>is la <tld/>isto: la malbonuloj ne
            aparti&gcirc;is
            <fnt><bib>MT</bib>, <lok>&Jer; 6:29</lok></fnt>.
          </ekz>
        </dif>
        <trd
        lng="ru">&c_p;&c_l;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
      </snc>
      <snc mrk="fand.0isto.gisisto">
        <ref tip="dif" cel="gis.0isto.muldisto">gisisto<sncref/></ref>
        <ekz>
          per honto kovri&gcirc;is &ccirc;iu <tld/>isto kun sia statuo,
          &ccirc;ar
          lia <tld/>ita&jcirc;o estas malvera&jcirc;o, &gcirc;i ne havas
          en si spiriton
          <fnt><bib>MT</bib>, <lok>&Jer; 10:14</lok></fnt>.
        </ekz>
        <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_j;&c_shch;&c_i;&c_k;</trd>
        <trd lng="fr">fondeur</trd>
        <trd lng="hu">olvaszt&aacute;r</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>slévač</trd>,
    <trd>tavič</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tavič</trd>,
    <trd>zlievač</trd>
  </trdgrp>
</drv>

    <drv mrk="fand.dis0ifor0i">
      <kap>dis<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Malaperigi:
          <ekz>
            lumaj radioj dis<tld/>is la rigidecon de &scirc;iaj
            trajtoj<fnt>Z</fnt>;
          </ekz><ekz>
            la montoj tremas anta&ubreve; Li, la montetoj
            dis<tld/>i&gcirc;as
            <fnt><bib>MT</bib>, <lok>&Nah; 1:5</lok></fnt>;
          </ekz><ekz>
            tia konduto for<tld/>is la anta&ubreve;ju&gcirc;ojn de la
            nobelaro<fnt>Z</fnt>;
          </ekz><ekz>
            la tempo for<tld/>is tiun rigidan indiferentecon.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>faire <ind>fondre</ind><klr tip="ind"> (faire
              dispara&icirc;tre)</klr></trd>,
          <trd>dissiper</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>stapia&cacute; si&eogonek;</trd>,
          <trd>roztapia&cacute; si&eogonek;</trd>
        </trdgrp>
        <trd
        lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;<klr
            tip="ind"> (&c_s;&c_n;&c_je;&c_g;,
            &c_h;&c_o;&c_l;&c_o;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_i;
            &c_t;.&c_p;.)</klr></trd>
      </snc>
  <trd
      lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">roztavit</trd>
  <trdgrp lng="hu">
        <trd>eloszlat</trd>,
        <trd>szertefoszlat</trd>
      </trdgrp>
  <trd lng="nl">wegsmelten</trd>
  <trd lng="no">smelte vekk</trd>
  <trd lng="sk">roztaviť</trd>
</drv>

    <drv mrk="fand.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">TEK</uzo>
      <snc mrk="fand.el0i.ekstrakti">
        <dif>
          Ekstrakti per <tld/>ado:
          <ekz>
            el<tld/>i metalon el erco;
          </ekz><ekz>
            el<tld/>i vakson el &ccirc;elaro;
          </ekz><ekz>
            el<tld/>i plumbon el alojo;
          </ekz><ekz>
            el<tld/>i lardon;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            ekzistas anka&ubreve; kiuj opinias, ke nun &gcirc;i
            <klr>(la Eklezio)</klr> estu &jcirc;etita en fornon, por ke
            la
            pura oro estu el<tld/>ita
            <fnt>
              <aut>M. Von Galli</aut>: <vrk>Nova pensado</vrk>.
              <lok><url
                  ref="http://www.esperokatolika.org/ek19611965/ek1965_03.htm#8"
                  >Espero katolika, 1965, n-ro 3</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="en">smelt</trd>
        <trd lng="pl">wytapia&cacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_y;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="fand.el0i.gisi">
        <ref tip="dif" cel="gis.0i.muldi">gisi<sncref/></ref>
        <ekz>
          cent kikaroj da ar&gcirc;ento estis uzitaj por el<tld/>i la
          bazojn
          de la sanktejo kaj la bazojn de la kurteno
          <fnt><bib>MT</bib>, <lok>&Eli; 38:27</lok></fnt>.
        </ekz>
      </snc>
  <trdgrp lng="cs">
    <trd>odlévat kov</trd>,
    <trd>odlévat tavbu</trd>
  </trdgrp>
  <trd lng="sk">odlievať tavbu</trd>
</drv>

    <drv mrk="fand.kun0i">
      <kap>kun<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="fand.kun0i.TEK">
        <uzo tip="fak">TEK</uzo>
        <dif>
          Miksi per <tld/>ado:
          <ekz>
            fero ne kun<tld/>i&gcirc;as kun argilo
            <fnt><bib>MT</bib>, <lok>&Dan; 2:43</lok></fnt>;
          </ekz>
          <ekz>
            <uzo tip="stl">FIG</uzo>
            pli a&ubreve; malpli frue la ideoj de la esperantismo kaj
            pacifismo tute kun<tld/>i&gcirc;os<fnt>Z</fnt>
            <klr>(sendiferenci&gcirc;os)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <ind>kun<tld/>i&gcirc;o</ind> de nacioj en pli lar&gcirc;an
            unuon;
            <trd lng="fr">fusion<klr tip="ind">
            (m&eacute;lange)</klr></trd>
            <trdgrp lng="hu">
              <trd>&ouml;sszeolvad&aacute;s</trd>,
              <trd>f&uacute;zi&oacute;</trd>
            </trdgrp>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            kun<tld/>i&gcirc;antaj <klr>(harmonie intermiksitaj)</klr>
            koloroj.
          </ekz>
        </dif>
        <ref tip="vid" cel="aloj.0o">alojo</ref>
        <trd
        lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
        <trdgrp lng="fr">
          <trd>fondre<klr tip="ind"> (fusionner)</klr></trd>,
          <trd>fusionner</trd>,
          <trd>m&eacute;langer</trd>
        </trdgrp>
        <trd lng="hu">&ouml;sszeolvaszt</trd>
        <trd lng="nl">samensmelten</trd>
        <trd lng="no">sammensmelte</trd>
        <trd
        lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;<klr>
        (&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y; &c_i;
        &c_t;.&c_p;.)</klr></trd>
      </snc>
      <snc mrk="fand.kun0i.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Kunigi plurajn ordigitajn datumarojn (listojn, dosierojn)
          tiel, ke la rezulto havu tian saman ordon:
          <ekz>
            tiuj jaraj aldonoj kun<tld/>i&gcirc;os fine en
            <ctl>Universala
              Vortaro plenigita</ctl>
            <fnt>
              <vrk>Unua Oficiala aldono al Universala vortaro</vrk>.
              <lok>Klariga noto. Paris, 1909. P. 5.</lok>
            </fnt>
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>merge</trd>,
          <trd>collate</trd>
        </trdgrp>
        <trd lng="hu">&ouml;sszef&eacute;s&uuml;l</trd>
        <trdgrp lng="pl">
          <trd>scala&cacute;</trd>,
          <trd>wi&aogonek;za&cacute;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
          <trd>&c_s;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>stavit</trd>,
    <trd>tavením spojit</trd>
  </trdgrp>
  <trd lng="sk">tavením spojiť</trd>
</drv>

    <drv mrk="fand.kun0ema.lingvo">
      <kap>kun<tld/>ema lingvo</kap>
      <snc mrk="fand.kun0ema.lingvo.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> en kies
          <ref cel="fleksi.0i">fleksiado</ref> oftas
          <ref tip="prt" cel="fand.kun0o.LIN">kunfandoj</ref>.
        </dif>
        <adm>
          Cxu "kun~ema (p.p. lingvo)" ne estus pli bona derivajxo?
          <aut>MB</aut>
        </adm>
        <ref tip="ant" cel="agluti.0a">aglutina lingvo</ref>
        <trd
        lng="be">&c_f;&c_l;&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
        <trd lng="en">inflective language</trd>
        <trd lng="fr">langue flexionnelle</trd>
        <trd lng="hu">flekt&aacute;l&oacute; <ind>nyelv</ind></trd>
        <trd lng="pl">j&eogonek;zyk <ind>fleksyjny</ind></trd>
        <trd
        lng="ru">&c_f;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
      </snc>
    </drv>

    <drv mrk="fand.kun0o">
      <kap>kun<tld/>o</kap>
      <snc mrk="fand.kun0o.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          &Ccirc;e <ref tip="malprt"
          cel="vort.0farado">vortfarado</ref>,
          tia unui&gcirc;o de kombinataj vortelementoj ke ne plu eblas
          indiki limon inter ili:
          <ekz>
            en la latina <ctl>rex</ctl> <em>x</em> prezentas kun<tld/>on
            de la
            radika konsonanto <em>-g</em> kun la nominativa
            fina&jcirc;o.
          </ekz>
        </dif>
        <trd lng="be">&c_f;&c_u;&c_z;&c_ib;&c_ja;</trd>
        <trd lng="en">fusion</trd>
        <trd lng="fr">fusion<klr tip="ind"> (ling.)</klr></trd>
        <trd lng="nl">fusie<klr tip="ind"> (ling.)</klr></trd>
        <trd lng="pl">fuzja</trd>
        <trd lng="ru">&c_f;&c_u;&c_z;&c_i;&c_ja;</trd>
      </snc>
    </drv>
