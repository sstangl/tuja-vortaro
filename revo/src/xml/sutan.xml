<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sutan.xml,v 1.17 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sutan</rad>/o
</kap>
<drv mrk="sutan.0o">
  <kap><tld/>o</kap>
  <snc mrk="sutan.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Longa &gcirc;ispieda a&ubreve; &gcirc;isgenua <ref tip="super"
      cel="rob.0o.surtuto">robo</ref> de pastroj butonumata sur la
      anta&ubreve;a flanko.
    </dif>
    <refgrp tip="vid">
      <ref cel="dalmat.0o">dalmatiko</ref>
      <ref cel="surpli.0o.TEKS">surpliso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hábit</trd>,
    <trd>klerika</trd>,
    <trd>sutana</trd>,
    <trd>řeholní roucho</trd>
  </trdgrp>
  <trd lng="de">Sutane</trd>
  <trdgrp lng="en">
    <trd>soutane</trd>,
    <trd>cassock</trd>
  </trdgrp>
  <trd lng="fr">soutane</trd>
  <trd lng="hu">reverenda</trd>
  <trdgrp lng="it">
    <trd>talare</trd>,
    <trd>abito <ind>talare</ind></trd>
  </trdgrp>
  <trd lng="nl">soutane</trd>
  <trd lng="pl">sutanna</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_ja;&c_s;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klerika</trd>,
    <trd>reverenda</trd>
  </trdgrp>
</drv>
</art>
