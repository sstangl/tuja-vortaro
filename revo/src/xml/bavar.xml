<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bavar.xml,v 1.15 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bavar</rad>/o
</kap>
<drv mrk="bavar.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Lo&gcirc;anto de Bavarujo.
    </dif>
    <trd lng="de">Bayer</trd>
    <trd lng="es">b&aacute;varo</trd>
    <trd lng="hu">bajor</trd>
    <trdgrp lng="pl">
      <trd>Bawarczyk</trd>,
      <trd>Baier<klr> (region.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_v;&c_a;&c_r;&c_je;&c_c;</trd>
  </snc>
  <snc>
    <dif>
      Lo&gcirc;anto de Malnovbavarujo kaj nomo de ano de iama
      &gcirc;ermana
      tribo.
    </dif>
    <trd lng="de">Baier</trd>
    <trd lng="es">b&aacute;varo</trd>
    <trdgrp lng="hu">
      <trd>bajor</trd>,
      <trd>bajuvar</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_v;&c_a;&c_r;</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_a;&c_r;&c_a;&c_c;</trd>
  <trd lng="cs">Bavor<klr>(ák)</klr></trd>
  <trd lng="en">Bavarian</trd>
  <trd lng="fr">Bavarois<klr> (habitant)</klr></trd>
  <trd lng="sk">Bavorák</trd>
</drv>

<drv mrk="bavar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj dialektoj, a&ubreve;toroj;
      </ekz>
      <ekz>
        <tld/>a konstitucio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bayrisch</trd>,
      <trd>bayerisch</trd>
    </trdgrp>
    <trd lng="pl">bawarski</trd>
  </snc>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia kulturo a&ubreve; iama &scirc;tato:
      <ekz>
        <tld/>aj dialektoj, a&ubreve;toroj;
      </ekz>
    </dif>
    <trd lng="de">bairisch</trd>
    <trd lng="hu">Baier</trd>
    <trd lng="pl">bawarski</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_B;&c_a;&c_v;&c_a;&c_r;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">bavorský</trd>
  <trd lng="en">Bavarian</trd>
  <trd lng="es">b&aacute;varo</trd>
  <trd lng="fr">bavarois<klr> (adjectif)</klr></trd>
  <trdgrp lng="hu">
    <trd>bajor</trd>,
    <trd>bajuvar</trd>
  </trdgrp>
  <trd lng="nl">beiers</trd>
  <trd lng="ru">&c_b;&c_a;&c_v;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">bavorský</trd>
</drv>

<drv mrk="bavar.B0ujo">
  <kap><tld lit="B"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> kun regurbo
      Munkeno;
      inkluzivanta la historiajn regionojn
      Malnovbavarujo, Frankujo kaj Supra &Scirc;vabujo.
    </dif>
    <trd lng="de">Bayern</trd>
    <trd lng="en">Bavaria</trd>
    <trd lng="es">Baviera</trd>
    <trd lng="lat">Bavaria</trd>
    <trdgrp lng="pl">
      <trd>Bawaria</trd>,
      <trd>Baiery<klr> (region.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Iama &scirc;tato de la bavara tribo, duklando. Kerno de la poste
      de
      Napoleono kreita re&gcirc;lando Bavarujo, parto de la germana
      imperio ekde
      1871.
    </dif>
    <trd lng="de">Baiern</trd>
    <trd lng="lat">Bavaria</trd>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_v;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_a;&c_v;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="fr">Bavi&egrave;re</trd>
  <trd lng="hu">Bajororsz&aacute;g</trd>
  <trd lng="nl">Beieren</trd>
  <trd lng="ru">&c_B;&c_a;&c_v;&c_a;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="bavar.Malnov0ujo">
  <kap>Malnov<tld/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Moderna nomo por la regionoj de nuna Bavarujo, kiuj iam formis la
      historian duklandon Bavarujo.
    </dif>
    </snc>
    <trdgrp lng="de">
      <trd>Altbaiern</trd>,
      <trd>Altbayern</trd>
    </trdgrp>
    <trd lng="en">Old <ind>Bavaria</ind></trd>
    <trd lng="hu">t&ouml;rt&eacute;nelmi
    <ind>Bajororsz&aacute;g</ind></trd>
</drv>
</art>
