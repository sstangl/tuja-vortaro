<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tubj.xml,v 1.5 2015/07/04 08:37:45 revo Exp $">
<kap>
    <rad>tubj</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="tubj.0o">
  <kap><tld/>o</kap>
  <snc mrk="tubj.0o.SNC">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Granda latuna blovinstrumento kun valvoj.
    </dif>
  </snc>
  <trd lng="ca">tuba</trd>
  <trdgrp lng="cs">
    <trd>basová trubka</trd>,
    <trd>tuba</trd>
  </trdgrp>
  <trd lng="en">tuba</trd>
  <trdgrp lng="it">
   <trd>tuba</trd>, 
   <trd>flicorno<klr> (basso o contrabbasso)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trúba</trd>,
    <trd>tuba</trd>
  </trdgrp>
</drv>
</art>

