<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vanel.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vanel</rad>/o
</kap>
<drv mrk="vanel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="vanele.0oj"><tld/>edoj</ref>
      (<trd lng="la">Vanellus</trd>):
      <ekz>la aviadilon trafis svarmo da sturnoj kaj
      <tld/>oj<fnt><aut>Guido Van Damme</aut>: <vrk>Pli bone unu birdo
      en la
mano</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 11a, p.
19a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_n;&c_ib;&c_g;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_u;&c_g;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">fredeluga</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ejka</trd>,
    <trd>čejka chocholatá</trd>
  </trdgrp>
  <trd lng="de">Kiebitz</trd>
  <trd lng="en">lapwing</trd>
  <trdgrp lng="es">
    <trd>avefr&iacute;a</trd>,
    <trd>ave fr&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_r;&a_gh; &a_z;&f_y;&a_b;&a_A;</trd>&a_komo;
    <trd>&a_z;&f_y;&a_A;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vanneau<klr> (oiseau)</klr></trd>
  </trdgrp>
  <trd lng="hu">b&iacute;bic</trd>
  <trd lng="nl">kievit</trd>
  <trd lng="pl">czajka</trd>
  <trdgrp lng="pt">
   <trd>abibe</trd>,
   <trd>abitoninha</trd>,
   <trd>pavoncino</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_i;&c_b;&c_i;&c_s;</trd>
  <trd lng="sk">čajka chocholúška</trd>
</drv>
</art>
