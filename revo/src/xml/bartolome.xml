<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bartolome.xml,v 1.7 2015/02/26 20:11:00 revo Exp $">
<kap>
  <rad>Bartolome</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bartolome.0o">
  <kap><tld/>o</kap>
  <snc mrk="bartolome.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> arame-devena:
      <ekz>
        la granda inkvizitoro <tld/>o en la jaro 1659 malpermesis al la
        pastroj fumi tabakon en la da&ubreve;ro de unu horo anta&ubreve;
la Diservo
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>       
  <trdgrp lng="br">
    <trd>Bertele</trd>,
    <trd>Berteleme</trd>
  </trdgrp>
  <trd lng="de">Bartholomeus</trd>
  <trdgrp lng="fr">
    <trd>Barth&eacute;lemy</trd>,
    <trd>Bartholom&eacute;</trd>
  </trdgrp>
  <trd lng="it">Bartolomeo</trd>
  <trdgrp lng="pl">
    <trd>Bart&lstroke;omiej</trd>,
    <trd>Bartosz</trd>,
    <trd>Bartek<klr> (zdrobnienie)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_V;&c_a;&c_r;&c_f;&c_o;&c_l;&c_o;&c_m;&c_je;&c_j;</trd>,
    <trd>&c_B;&c_a;&c_r;&c_t;&c_o;&c_l;&c_o;&c_m;&c_je;&c_o;<klr>
(&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_B;&c_a;&c_r;&c_t;&c_o;&c_l;&c_o;&c_m;&c_mol;&c_ju;<klr>
(&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>

