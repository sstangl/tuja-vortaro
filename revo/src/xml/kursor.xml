<vortaro>
  <art mrk="$Id: kursor.xml,v 1.11 2015/07/04 08:37:38 revo Exp $">
    <kap>
      <rad>kursor</rad>/o
    </kap>
    <drv mrk="kursor.0o">
      <kap><tld/>o</kap>
      <fnt><lok><url
      ref="&KompLeks;/KR.html#KURSORO">KompLeks</url></lok></fnt>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          En hom-komputila <ref tip="malprt"
          cel="dialog.0o.KOMP">dialogo</ref>,
          marko (ofte, lumpulsanta ortogramo a&ubreve; substreko) sur la
          ekrano,
          indikanta la pozicion &ccirc;e kiu plenumi&gcirc;as operacio (precipe,
          kien trafos la signoj enigataj per la klavaro).
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_s;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>bod</trd>,
    <trd>kurzor</trd>,
    <trd>pozice</trd>,
    <trd>tečka</trd>,
    <trd>řádová tečka</trd>
  </trdgrp>
  <trdgrp lng="de">
         <trd>Schreibmarke</trd>,
         <trd>Cursor</trd>,
         <trd>Kursor</trd>
       </trdgrp>
  <trd lng="en">cursor</trd>
  <trd lng="fr">curseur</trd>
  <trdgrp lng="hu">
      <trd>eg&eacute;rmutat&oacute;</trd>,
      <trd>kurzor</trd>
      </trdgrp>
  <trdgrp lng="nl">
      <trd>pijltje</trd>,
      <trd>cursor</trd>,
      <trd>muisaanwijzer</trd>
      </trdgrp>
  <trd lng="pl">kursor</trd>
  <trd lng="pt">cursor</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_s;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>bod</trd>,
    <trd>bodka</trd>
  </trdgrp>
  <trd lng="sv">mark&ouml;r</trd>
</drv>
