<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: santeri.xml,v 1.2 2013/09/22 19:11:10 revo Exp $">
<kap>
  <rad>Santeri</rad>/o
</kap>
<drv mrk="santeri.0o">
  <kap><tld/>o</kap>
  <snc mrk="santeri.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Multdia <ref tip="lst" cel="religi.0o.REL"
      lst="voko:religioj">religio</ref>
      populara precipe en Kubo, kun afrikdevenaj elementoj:
      <ekz>
        apud kristanismo estas multnombre praktikataj en Kubo
        aliaj religioj, aparte multe <tld lit="s"/>o
        <fnt>
          <aut>M. Blahu&scaron;</aut>,
          <vrk>Kristanaj Aran&gcirc;oj en la 95-a Universala
          Kongreso...</vrk>,
          <lok><url ref="http://www.ikue.org/cz/arkivo/a-80-4-10.pdf">
          Dio Benu, 2010:4 (80)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estas heredanto kaj sekvanto de la joruba kulturo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Santerio">
          Santerio</url>, 2011-01-24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kandomble.0o.REL">Kandombleo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">Santeria</trd>
</drv>
</art>

