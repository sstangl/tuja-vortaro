<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mespil.xml,v 1.22 2015/09/22 10:10:19 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mespil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mespil.0o">
  <kap><tld/>o</kap>
  <snc mrk="mespil.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <ref tip="malprt" cel="mespil.0ujo"><tld/>ujo</ref>,
      man&gcirc;ebla nur kiam &gcirc;i i&gcirc;is molmatura:
      <ekz>
        mi alportis <tld/>ojn
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>nespra</trd>,
    <trd>nespla</trd>
  </trdgrp>
  <trd lng="cs">mi&scaron;pule <klr>(strom)</klr></trd>
  <trd lng="de">Mispel</trd>
  <trd lng="en">medlar</trd>
  <trd lng="es">n&iacute;spero <klr>(fruto del Mespilus germanica)</klr></trd>
  <trd lng="fr">n&egrave;fle</trd>
  <trdgrp lng="hu">
    <trd>naspolya <klr tip="amb">(term&eacute;s)</klr></trd>,
    <trd>lasponya <klr tip="amb">(term&eacute;s)</klr></trd></trdgrp>
  <trd lng="nl">mispel</trd>
  <trd lng="pl">nieszpu&lstroke;ka</trd>
  <trdgrp lng="pt">
   <trd>n&ecirc;spera</trd>,
   <trd>ameixa-amarela</trd>,
   <trd>ameixa-amarela</trd>,
   <trd>m&ocirc;nica <klr tip="amb">(A&ccedil;ores)</klr></trd>,
   <trd>n&ecirc;spero</trd>   
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_sh;&c_m;&c_u;&c_l;&c_a;
<klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">mi&scaron;pu&lcaron;a <klr>(ovocie)</klr></trd>
</drv>

<drv mrk="mespil.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="mespil.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Mespilus</trd>).
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>nesprer</trd>,
    <trd>nespler</trd>
  </trdgrp>
  <trd lng="cs">mi&scaron;pule <klr>(strom)</klr></trd>
  <trd lng="de">Mispel</trd>
  <trd lng="en">medlar</trd>
  <trdgrp lng="es">
    <trd>n&iacute;spero <klr>europeo (Mespilus germanica)</klr></trd>,
    <trd>nisperero <klr>europeo</klr></trd>
  </trdgrp>
  <trd lng="fr">n&eacute;flier</trd>
  <trdgrp lng="hu">
    <trd>naspolya <klr tip="amb">(bokor)</klr></trd>,
    <trd>lasponya<klr tip="amb">(bokor)</klr></trd>
  </trdgrp>
  <trd lng="nl">mispelboom</trd>
  <trd lng="pl">nieszpu&lstroke;ka</trd>
  <trdgrp lng="pt">
   <trd>nespereira</trd>,
   <trd>ambone</trd>,
   <trd>ameixa-amarela</trd>,
   <trd>ameixa-americana</trd>,
   <trd>ameixa-do-canad&aacute;</trd>,
   <trd>ameixa-do-jap&atilde;o</trd>,
   <trd>ameixa-japonesa</trd>,
   <trd>japona</trd>,
   <trd>magn&oacute;lio</trd>,         
   <trd>nespereira-do-jap&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_sh;&c_m;&c_u;&c_l;&c_a; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">mi&scaron;pu&lcaron;a <klr>(strom)</klr></trd>
</drv>
</art>
