<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kukum.xml,v 1.27 2013/08/27 07:10:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kukum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kukum.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kukum.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kukurb1.0oj">kukurbacoj</ref>
      (<trd lng="la">Cucumis</trd>), al kiu apartenas
      <tld/>o <sncref ref="kukum.0o.specio"/> kaj <ref tip="sub"
      cel="melon.0o">melono</ref>.
    </dif>
    <trd lng="ca">g&egrave;nere Cucumis</trd>
    <trd lng="hu">Cucumis<klr tip="amb"> (nemzets&eacute;g)</klr></trd>
  </snc>
  <snc mrk="kukum.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro"><tld/>o
      <sncref ref="kukum.0o.genro"/></ref>
      (<trd lng="la">Cucumis sativus</trd>), kun longformaj kaj
      akvoplenaj
      fruktoj.
    </dif>
    <trd lng="ca">cogombrera</trd>
    <trd lng="en">cucumber</trd>
    <trd lng="hu">uborka<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  </snc>
  <snc mrk="kukum.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La <ref tip="super" cel="ber.0o.BOT">bero</ref> de
      <ref tip="malprt" cel="kukum.0o.specio"><tld/>o
      <sncref ref="kukum.0o.specio"/></ref>, man&gcirc;ata kruda,
      en salato a&ubreve; konservita en vinagro.
    </dif>
    <trd lng="hu">uborka<klr tip="amb"> (&eacute;tel)</klr></trd>
    <trd lng="sv">gurka</trd>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_r;&c_o;&c_k;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">cogombre</trd>
  <trd lng="de">Gurke</trd>
  <trd lng="es">pepino</trd>
  <trd lng="fa">&a_kh;&f_y;&a_A;&a_r;</trd>
  <trd lng="fr">concombre</trd>
  <trd lng="nl">komkommer</trd>
  <trd lng="pl">og&oacute;rek</trd>
  <trd lng="pt">pepino</trd>
  <trd lng="ru">&c_o;&c_g;&c_u;&c_r;&c_je;&c_c;</trd>
</drv>

<drv mrk="kukum.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc mrk="kukum.0eto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>o, kiun oni rikoltas anta&ubreve; maturi&gcirc;o, 
      ordinare por marinado kaj konservado en vinagro kiu igos &gcirc;in
      spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_a; <ind>&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_ch;&c_k;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>cogombre de mar</trd>,
    <trd>pardal de moro</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>saure <ind>Gurke</ind></trd>,
    <trd>Gew&uuml;rzgurke</trd>
  </trdgrp>
  <trd lng="en">gherkin</trd>
  <trd lng="fa">&a_kh;&f_y;&a_A;&a_r; &a_t;&a_r;&a_sh;&f_y;</trd>
  <trd lng="fr">cornichon</trd>
  <trdgrp lng="hu">
    <trd>fiatal <ind>uborka</ind></trd>,
    <trd>f&udblac;szeruborka</trd>
  </trdgrp>
  <trd lng="nl">augurk</trd>
  <trd lng="pl">korniszon</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_n;&c_i;&c_sh;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_u;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kukum.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="kukum.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa, mola besto rampanta en sablo, trovebla en varmaj 
      maroj &gcirc;is tre profunde, klaso el <ref tip="super" 
      cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>:
    <ekz>
      <ctl>Maturi&gcirc;o kaj Fekundi&gcirc;o &ccirc;e la Ovo de l' Mar<tld/>oj</ctl>
      <fnt>
        <aut>H. Ohshima</aut>,
        <vrk>Maturi&gcirc;o...</vrk>,
        <lok>Bulteno Scienca de la Fakultato Terkultura, 1925/2, 
        p.70a-102a</lok>
      </fnt>;
    </ekz>
    <ekz>
      &leftquot;kaj kiajn florojn tie vi kultivas?&rightquot;  &leftquot;La maranemonojn,&rightquot;
      diris nia 
      amiko, &leftquot;en kelkaj raraj specioj. Anka&ubreve; la marstelojn kaj 
      mar<tld/>ojn&rightquot;  
      <fnt><bib>Salamandroj</bib>, <lok>libro 2a, &ccirc;apitro 
      3a</lok></fnt>.
    </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Seegurke</trd>,
    <trd>Seewalze</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concombre de mer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zeekomkommer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trd lng="it">oloturia</trd>
</drv>
</art>
