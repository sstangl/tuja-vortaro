<art mrk="$Id: finial.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>finial</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="finial.0o">
  <kap><tld/>o</kap>
  <snc mrk="finial.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Florsimila ornamo finanta pinta&jcirc;on:
      <ekz>
        Pandiono <klr>...</klr> ekvidis meze de la halo marmoran kubon, 
        ornamitan per bluaj <tld/>oj kaj volva&jcirc;oj el vitro 
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.  
      </ekz>        
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fiála</trd>,
    <trd>květ</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kreuzblume</trd>,
     <trd>Bl&auml;tterknauf</trd></trdgrp>
  <trd lng="fr">finial</trd>
  <trdgrp lng="hu">
     <trd>keresztr&oacute;zsa</trd>,
     <trd>oromd&iacute;sz</trd></trdgrp>
  <trd lng="nl">fleuron</trd>
  <trdgrp lng="pt">
    <trd>remate</trd>,
    <trd>flor&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_z;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_mol;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kvetová ozdoba hrotu</trd>
</drv>
</art>
