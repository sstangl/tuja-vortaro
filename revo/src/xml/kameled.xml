<kap>
  <rad>kameled</rad>/oj
</kap>
<drv mrk="kameled.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Camelidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cam&egrave;lids</trd>
  <trd lng="de">Kamele</trd>
  <trd lng="fr">cam&eacute;lid&eacute;s</trd>
  <trd lng="hu">tevef&eacute;l&eacute;k</trd>
  <trd lng="nl">kamelenfamilie</trd>
  <trd lng="pl">wielb&lstroke;&aogonek;dowate</trd>
  <trd
  lng="ru">&c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;&c_o;&c_v;&c_y;&c_je;
  <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
