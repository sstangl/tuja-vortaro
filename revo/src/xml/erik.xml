<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: erik.xml,v 1.25 2015/09/22 10:10:24 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>erik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="erik.0o">
  <kap><tld/>o</kap>
  <snc mrk="erik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbedo a&ubreve; arbusto kun rozaj a&ubreve; blankaj floroj,
      kreskanta en sablaj teroj,
      genro el familio <ref tip="super"
      cel="erikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Erica</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>bruc</trd>,
    <trd>bruguera</trd>
  </trdgrp>
  <trd lng="cs">v&rcaron;esovec</trd>
  <trdgrp lng="de">
    <trd>Heide</trd>,
    <trd>Erika</trd>
  </trdgrp>
  <trd lng="en">heath</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_r;&f_y;&f_k;&a_A;</trd>,
    <trd>&a_kh;&a_l;&a_n;&f_g;</trd>
  </trdgrp>
  <trd lng="fr">bruy&egrave;re<klr> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hanga</trd>,
    <trd>erika</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heide</trd>,
    <trd>heidekruid</trd>,
    <trd>erica</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wrzos</trd>,
    <trd>wrzosiec</trd>
  </trdgrp>
  <trd lng="pt">urze</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_je;&c_s;&c_k;
  &c_d;&c_r;&c_je;&c_v;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vresovec</trd>
  <trd lng="sv">ljung</trd>
</drv>

<drv mrk="erik.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="erik.0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Regiono, loko surkreskata de <tld/>oj:
      <ekz>
        ni lo&gcirc;as sur la <tld/>ejo, sur la seka <tld/>ejo
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url
          ref="http://www.gutenberg.org/files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">bruguerar</trd>
  <trd lng="cs">v&rcaron;esovi&scaron;t&ecaron;</trd>
  <trdgrp lng="en">
    <trd>heath</trd>,
    <trd>moor</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brande</trd>,
    <trd>bruy&egrave;re<klr> (lieu)</klr></trd>,
    <trd>lande<klr> (&agrave; bruy&egrave;res)</klr></trd>
  </trdgrp>
  <trd lng="pl">wrzosowisko</trd>
  <trd lng="sk">vresovi&scaron;te</trd>
</drv>
</art>
