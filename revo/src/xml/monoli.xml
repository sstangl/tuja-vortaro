  <trd lng="cs">monolit</trd>
  <trd lng="de">Monolith</trd>
  <trd lng="en">monolith</trd>
  <trd lng="es">monolito</trd>
  <trd lng="fr">monolithe</trd>
  <trd lng="hu">monolit</trd>
  <trd lng="nl">monoliet</trd>
  <trd lng="pt">mon&oacute;lito</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_l;&c_i;&c_t;</trd>
  <trd lng="sk">monolit</trd>
</drv>
</art>
