<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tritik.xml,v 1.26 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tritik</rad>/o
</kap>

<drv mrk="tritik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Triticum</trd>), greno el kies grajnoj oni
      faras blankan farunon:
      <ekz>
        hordeo kaj <tld/>o bone kreskas sub tiu klimato<fnt><aut>Lode
Van
        De Velde</aut>: <vrk>Bierlando Belgio</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  <ref tip="sub" cel="spelt.0o">spelto</ref>
  </snc>
  <trd lng="be">&c_p;&c_sh;&c_a;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="bg">&c_p;&c_sh;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>blat</trd>,
    <trd>forment <klr>(reg)</klr></trd>
  </trdgrp>
  <trd lng="cs">pšenice</trd>
  <trd lng="de">Weizen</trd>
  <trd lng="en">wheat</trd>
  <trd lng="es">trigo</trd>
  <trd lng="fr">bl&eacute;</trd>
  <trd lng="hu">b&uacute;za</trd>
  <trdgrp lng="id">
    <trd>gandum</trd>,
    <trd>terigu</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>grano<klr> (frumento)</klr></trd>,
    <trd>frumento</trd>,
    <trd>tritico</trd>
  </trdgrp>
  <trd lng="nl">tarwe</trd>
  <trd lng="oc">blat</trd>
  <trd lng="pl">pszenica</trd>
  <trd lng="pt">trigo</trd>
  <trd lng="ru">&c_p;&c_sh;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">pšenica</trd>
  <trd lng="sv">vete</trd>
  <trd lng="tr">bu&gbreve;day</trd>
</drv>
</art>
