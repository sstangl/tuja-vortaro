<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: acetil.xml,v 1.17 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>acetil</rad>/o
</kap>
<drv mrk="acetil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>2</sub>H<sub>3</sub>O. Radikalo de acetata acido.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_c;&c_e;&c_t;&c_y;&c_l;</trd>
  <trd lng="ca">acetil</trd>
  <trd lng="cs">acetyl</trd>
  <trd lng="de">Azetyl</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&f_y;&a_l;</trd>
  <trd lng="fr">ac&eacute;tyle</trd>
  <trdgrp lng="hu">
    <trd>acetilgy&ouml;k</trd>,
    <trd>ecetsavgy&ouml;k</trd>
  </trdgrp>
  <trd lng="it">acetile</trd>
  <trd lng="nl">acetyl</trd>
  <trd lng="pl">acetyl</trd>
  <trdgrp lng="pt">
    <trd>acetilo</trd>,
    <trd>acetila</trd>
  </trdgrp>
  <trd lng="ro">acetil</trd>
  <trd lng="ru">&c_a;&c_c;&c_je;&c_t;&c_i;&c_l;</trd>
  <trd lng="sk">acetyl</trd>
  <trd lng="sv">acetyl</trd>
</drv>
</art>
