</kap>
<drv mrk="lima.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="limaed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lima</trd>), mara molusko kies konko havas
      &leftquot;orelojn&rightquot;.
    </dif>
  </snc>
  <trd lng="de">Feilenmuschel</trd>
  <trd lng="en">Lima</trd>
  <trd lng="hu">reszel&odblac;kagyl&oacute;</trd>
  <trd lng="nl">lima</trd>
  <trd lng="pl">lima</trd>
</drv>

</art>
