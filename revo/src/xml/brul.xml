<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brul.xml,v 1.44 2015/10/26 15:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brul</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.0i.konsumigxi">
    <dif>
      Konsumi&gcirc;i per fajro:
      <ekz>
        la ligno, la kandelo <tld/>as;
      </ekz>
      <ekz>
        fajro <tld/>etas sub la cindro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>la afero ne <tld/>as</ind>
        <klr>(ne ur&gcirc;as)</klr>
        <trd lng="fr">il n'y a pas le feu</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>spala&cacute; si&eogonek;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>comburir</trd>,
      <trd>esbrasear</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0i.varmi">
    <dif>
      Esti varmega kiel fajro:
      <ekz>
        via mano <tld/>as de febro;
      </ekz>
      <ekz>
        <tld/>e varmega fero
        <fnt><bib>NT</bib>, <lok>&Ti1; 4:2</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">gloeien</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>pra&zdot;y&cacute;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>escaldar</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en forta eksciteco:
      <ekz>
        <tld/>i de amo, de scivolo, de deziro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro senpacienco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="flam.0i">flami</ref>.
    </refgrp>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>spala&cacute; si&eogonek;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arder</trd>,
      <trd>inflamar-se</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ho&rcaron;et</trd>,
    <trd>ho&rcaron;et</trd>,
    <trd>p&aacute;lit</trd>,
    <trd>sv&iacute;tit <klr>(slunce)</klr></trd>
  </trdgrp>
  <trd lng="de">brennen</trd>
  <trd lng="en">burn</trd>
  <trd lng="es">quemar</trd>
  <trd lng="fr">bruler <klr tip="ind">(intr.)</klr></trd>
  <trd lng="hu">&eacute;g</trd>
  <trd lng="id">[<ind>bakar</ind>] terbakar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">horie&tcaron;</trd>
</drv>

<drv mrk="brul.0o">
  <kap><tld/>o</kap>
  <snc mrk="brul.0o.KOMUNE">
    <dif>
      Stato de <tld/>anta&jcirc;o:
      <ekz>
        kia granda <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>ho&rcaron;en&iacute;</trd>,
    <trd>ohe&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Brand</trd>
  <trd lng="en">combustion</trd>
  <trdgrp lng="es">
    <trd>quema</trd>,
    <trd>combusti&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">combustion</trd>
  <trdgrp lng="hu">
    <trd>&eacute;g&eacute;s</trd>,
    <trd>t&udblac;z</trd>
  </trdgrp>
  <trd lng="id">[<ind>bakar</ind>] pembakaran</trd>
  <trd lng="nl">brand</trd>
  <trdgrp lng="pl">
    <trd>spalanie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ti&ccedil;&atilde;o</trd>, <trd>brasa</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">horenie</trd>
</drv>

<drv mrk="brul.0a">
  <kap><tld/>a</kap>
  <snc mrk="brul.0a.brulanta">
    <dif>
      <tld lit="B"/>anta.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_ju;&c_ch;&c_y;</trd>
    <trd lng="de">brennend</trd>
    <trd lng="en">burning</trd>
    <trd lng="es">abrasador</trd>
    <trd lng="fr">brulant</trd>
    <trd lng="hu">&eacute;g&odblac;</trd>
    <trdgrp lng="pl">
      <trd>pal&aogonek;cy</trd>,
      <trd>p&lstroke;on&aogonek;cy</trd>,
      <trd>spalaj&aogonek;cy si&eogonek;</trd>,
      <trd>pra&zdot;&aogonek;cy</trd>,
      <trd>gorej&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ardente</trd>,
      <trd>candente</trd>,
      <trd>incandescente</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ja;&c_shch;&c_i;&c_j;</trd>
    <trd lng="sk">horiaci</trd>
  </snc>
  <snc mrk="brul.0a.bruligebla">
    <ref tip="dif" cel="brul.0igebla"><tld/>igebla</ref>
    <ekz>
      ligno estas bona <tld/>a materialo
      <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>.
    </ekz>
    <trd lng="be">&c_g;&c_a;&c_r;&c_u;&c_ch;&c_y;</trd>
    <trdgrp lng="de">
      <trd>Brenn-</trd>,
      <trd>brennbar</trd>
    </trdgrp>
    <trd lng="en">combustible</trd>
    <trd lng="es">combustible <klr tip="ind">(adj.)</klr></trd>
    <trd lng="fr">combustible <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">&eacute;ghet&odblac;</trd>
    <trd lng="nl">brandbaar</trd>
    <trdgrp lng="pl">
      <trd>palny</trd>
    </trdgrp>
    <trd lng="pt">combust&iacute;vel</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
    <trd lng="sk">hor&lcaron;av&yacute;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>o&zcaron;ehav&yacute;</trd>,
    <trd>pal&ccaron;iv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vznetliv&yacute;</trd>,
    <trd>z&aacute;paln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="brul.0ego.KOMUNE">
    <dif>
      Granda <tld/>o, ka&ubreve;zanta difektojn kaj perdojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="incend.0o">incendio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;</trd>
  <trd lng="cs">po&zcaron;&aacute;r</trd>
  <trdgrp lng="de">
    <trd>Feuersbrunst</trd>,
    <trd>Gro&szlig;brand</trd>
  </trdgrp>
  <trd lng="en">inferno</trd>
  <trd lng="es">incendio</trd>
  <trd lng="fr">incendie</trd>
  <trdgrp lng="hu">
    <trd>t&udblac;z</trd>,
    <trd>t&udblac;zv&eacute;sz</trd>
  </trdgrp>
  <trd lng="nl">brand</trd>
  <trd lng="pl">po&zdot;ar</trd>
  <trd lng="pt">inc&ecirc;ndio</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;</trd>
  <trd lng="sk">po&zcaron;iar</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_a;</trd>
</drv>

<drv mrk="brul.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="brul.0ema.KOMUNE">
    <dif>
      Facile <tld/>anta:
      <ekz>
        beda&ubreve;rinde la planko estis kovrita per <tld/>ema plasto.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="brul.0igxema"><tld/>i&gcirc;ema</ref>
  <trd
  lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_w;&c_z;&c_g;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">entz&uuml;ndlich</trd>
  <trdgrp lng="en">
    <trd>combustible</trd>,
    <trd>volatile</trd>
  </trdgrp>
  <trd lng="es">inflamable</trd>
  <trd lng="fr">inflammable</trd>
  <trd lng="hu">gy&uacute;l&eacute;kony</trd>
  <trd lng="nl">ontvlambaar</trd>
  <trd lng="pl">&lstroke;atwopalny</trd>
  <trd lng="pt">inflam&aacute;vel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">hor&lcaron;av&yacute;</trd>
  <trdgrp lng="sk">
    <trd>vznetliv&yacute;</trd>,
    <trd>z&aacute;paln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brul.0igi.konsumigi">
    <dif>
      Konsumigi per fajro:
      <ekz>
        <tld/>igi lignon, paperon, herezulon, domon;
      </ekz>
      <ekz>
        post infekta malsano oni ofte <tld/>igas la vestojn de la
        malsanulo
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cindr.0igi">cindrigi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>&eacute;get</trd>,
      <trd>el&eacute;get</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pali&cacute;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>spala&cacute;</trd>,
      <trd>gorze&cacute;</trd>,
      <trd>zapala&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0igi.vundi">
    <ref tip="dif" cel="vund.brul0i.MED"><tld lit="B"/>vundi</ref>
    <ekz>
      ne estingu la fajron, kiu vin ne
      <tld/>igas<fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="hu">meg&eacute;get</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podp&aacute;lit</trd>,
    <trd>spalovat</trd>,
    <trd>sp&aacute;lit <klr>(n&ecaron;co)</klr></trd>,
    <trd>up&aacute;lit</trd>,
    <trd>zapalovat</trd>,
    <trd>zap&aacute;lit</trd>,
    <trd>za&zcaron;ehovat</trd>
  </trdgrp>
  <trd lng="de">verbrennen</trd>
  <trdgrp lng="en">
    <trd>burn</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quemar <klr tip="ind">(tr.)</klr></trd>,
    <trd>incendiar</trd>
  </trdgrp>
  <trd lng="fr">bruler <klr tip="ind">(tr.)</klr></trd>
  <trd lng="id">[<ind>bakar</ind>] membakar</trd>
  <trd lng="nl">verbranden</trd>
  <trdgrp lng="pl">
    <trd>pali&cacute;</trd>,
    <trd>spala&cacute;</trd>,
    <trd>parzy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>queimar</trd>,
    <trd>atear <klr>(fogo)</klr></trd>,
    <trd>acender <klr>(o fogo)</klr></trd>,
    <trd>incendiar</trd>,
    <trd>p&ocirc;r fogo</trd>,
    <trd>tacar <klr>(fogo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;li&tcaron;</trd>,
    <trd>pop&aacute;li&tcaron;</trd>,
    <trd>sp&aacute;li&tcaron;</trd>,
    <trd>up&aacute;li&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="brul.0igajxo.KOMUNE">
    <dif>
      Substanco, uzata hejme a&ubreve; industrie por produkti
      varmon per <tld/>igo.
    </dif>
    <refgrp tip="sub">
      <ref cel="gas.brul0o">brulgaso</ref>,
      <ref cel="karb.0o">karbo</ref>,
      <ref cel="naft.0o">nafto</ref>,
      <ref cel="petrol.0o">petrolo</ref>,
      <ref cel="urani.0o">uranio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trdgrp lng="en">
    <trd>fuel</trd>
  </trdgrp>
  <trd lng="es">combustible <klr tip="ind">(subst.)</klr></trd>
  <trd lng="fr">combustible <klr tip="ind">(subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&uuml;zel&odblac;</trd>,
    <trd>t&uuml;zel&odblac;anyag</trd>
  </trdgrp>
  <trd lng="id"><ind>bahan</ind> bakar</trd>
  <trd lng="nl">brandstof</trd>
  <trdgrp lng="pl">
    <trd>opa&lstroke;</trd>,
    <trd>paliwo</trd>
  </trdgrp>
  <trd lng="pt">combust&iacute;vel</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_l;&c_i;&c_v;&c_o;</trd>
</drv>

<drv mrk="brul.0igebla">
  <kap><tld/>igebla</kap>
  <snc mrk="brul.0igebla.KOMUNE">
    <dif>
      Povanta <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_u;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>vzn&ecaron;tliv&yacute;</trd>,
    <trd>z&aacute;paln&yacute;</trd>
  </trdgrp>
  <trd lng="de">brennbar</trd>
  <trdgrp lng="en">
    <trd>combustible</trd>
  </trdgrp>
  <trd lng="es">combustible <klr tip="ind">(adj.)</klr></trd>
  <trd lng="fr">combustible <klr tip="ind">(adj.)</klr></trd>
  <trd lng="hu">&eacute;ghet&odblac;</trd>
  <trd lng="nl">brandbaar</trd>
  <trd lng="pl">palny</trd>
  <trd lng="pt">combust&iacute;vel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hor&lcaron;av&yacute;</trd>,
    <trd>z&aacute;paln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Facile ek<tld/>anta:
      <ekz>
        metano estas <tld/>i&gcirc;ema.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">inflammable</trd>
  <trd lng="es">inflamable</trd>
  <trd lng="fr">inflammable</trd>
  <trd lng="pl">&lstroke;atwopalny</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_n;&c_je;&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;&c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="brul.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de lumigilo a&ubreve; varmigilo, el kiu eli&gcirc;as la
      flamo.
    </dif>
  </snc>
  <adm>
    Kontrastigi rilate al "flamingo".
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_l;&c_k;&c_a; <klr tip="amb">
  (&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="de">Brenner</trd>
  <trdgrp lng="en">
    <trd>burner</trd>
  </trdgrp>
  <trd lng="es">quemador</trd>
  <trd lng="fr">bruleur</trd>
  <trd lng="hu">&eacute;g&odblac;</trd>
  <trd lng="nl">brander</trd>
  <trd lng="pl">palnik</trd>
  <trd lng="pt">pavio</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">hor&aacute;k</trd>
  <trdgrp lng="cs">
    <trd>ho&rcaron;&aacute;k</trd>,
    <trd>laboratorn&iacute; kahan</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc mrk="brul.0umo.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="inflam.0o">inflamo</ref>
    <ekz>
      personoj, kiuj rigardis la eklipson sen speciala protekto
      <klr>...</klr>, estas enhospitaligitaj kun pli malpli gravaj
      retin<tld/>umoj
      <fnt>
      <aut>P. Levy</aut>,
      <vrk>Re: Eklipso kaj katastrofo - Transmonden!</vrk>,
      <lok>
      <url
      ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo"
      >soc.culture.esperanto</url>, 1999-08-18</lok></fnt>;
    </ekz>
    <ekz>
      La infano havas <tld/>umon de la spira kanalo,
      tiel nomatan bronkiton
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
  </snc>
  <trd
  lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>vzplanut&iacute;</trd>,
    <trd>z&aacute;pal</trd>
  </trdgrp>
  <trd lng="de">Entz&uuml;ndung<klr tip="ind"> (K&ouml;rperreaktion)</klr></trd>
  <trdgrp lng="en">
    <trd>inflammation</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inflamaci&oacute;n</trd>,
    <trd>combusti&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">inflammation</trd>
  <trd lng="hu">gyullad&aacute;s</trd>
  <trd lng="id">inflamasi</trd>
  <trd lng="nl">ontsteking<klr tip="ind"> (ziekte)</klr></trd>
  <trd lng="pl">zapalenie</trd>
  <trd lng="pt">inflama&ccedil;&atilde;o</trd>
  <trd
  lng="ru">&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>vzplanutie</trd>,
    <trd>z&aacute;pal</trd>
  </trdgrp>
</drv>

<drv mrk="brul.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.al0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="B"/>glui&gcirc;i:
      <ekz>
        la terpomoj al<tld/>is en la kaserolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trd lng="es">cocinarse</trd>
  <trd lng="fr">attacher<klr> (cuisine)</klr></trd>
  <trdgrp lng="hu">
    <trd>oda&eacute;g</trd>,
    <trd>odakozm&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">aanbranden</trd>
  <trdgrp lng="pl">
    <trd>przypali&cacute; si&eogonek;</trd>,
    <trd>przywrze&cacute;<klr> (do naczynia)</klr></trd>,
    <trd>spali&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;<klr>
  (&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;., &c_n;&c_a;&c_p;&c_r;., &c_k;
  &c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>prihorie&tcaron;</trd>,
    <trd>prip&aacute;li&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="brul.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="B"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        miaj ostoj &ccirc;irka&ubreve;<tld/>is kiel en forno
        <fnt><bib>MT</bib><lok>&Psa; 102:3</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> komencis kolekti kaj glatigi la
        &ccirc;irka&ubreve;<tld/>intajn foliojn
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trd lng="es">quemar<klr tip="ind"> (intr., por
  el exterior)</klr></trd>
  <trd lng="fr">bruler<klr tip="ind"> (intr., en
  p&eacute;riph&eacute;rie)</klr></trd>
</drv>

<drv mrk="brul.ek0igi">
  <kap>ek<tld/>igi</kap>
  <snc mrk="brul.ek0igi.KOMUNE">
    <dif>
      Ekflamigi ion:
      <ekz>
        ek<tld/>igi fajron, kandelon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">encendre</trd>
  <trd lng="cs">zap&aacute;lit</trd>
  <trdgrp lng="de">
    <trd>anz&uuml;nden</trd>,
    <trd>entz&uuml;nden</trd>,
    <trd>entflammen</trd>,
    <trd>entfachen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ignite</trd>,
    <trd>light<klr> up</klr></trd>,
    <trd>spark</trd>,
    <trd>strike</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>allumer <klr tip="ind">(mettre le feu &agrave;)</klr></trd>,
    <trd>mettre le <ind>feu</ind> (&agrave;)</trd>
  </trdgrp>
  <trd lng="hu">meggy&uacute;jt</trd>
  <trd lng="nl">aansteken</trd>
  <trd lng="oc">alucar</trd>
  <trdgrp lng="pl">
    <trd>zapali&cacute;</trd>,
    <trd>rozpali&cacute;</trd>,
    <trd>zap&lstroke;on&aogonek;&cacute;</trd>,
    <trd>zagorze&cacute;</trd>,
    <trd>zaj&aogonek;&cacute; si&eogonek;<klr> ogniem</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atear <klr>(fogo)</klr></trd>,
    <trd>acender <klr>(o fogo)</klr></trd>,
    <trd>tacar <klr>(fogo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prip&aacute;li&tcaron;</trd>,
    <trd>zap&aacute;li&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0difekti">
  <kap><tld/>difekti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Difekti per fajro a&ubreve; varmego:
      <ekz>
        <ind><tld/>difekto</ind>;
        <trd lng="fr">brulure</trd>
      </ekz>
      <ekz>
        <klr>[donu]</klr>
        <tld/>difekton pro <tld/>difekto, vundon pro
        vundo, kontuzon pro kontuzo
        <fnt><bib>MT</bib>, <lok>&Eli; 21:25</lok></fnt>;
      </ekz>
      <ekz>
        la blovilo <tld/>difekti&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Jer; 6:29</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pop&aacute;lit</trd>,
    <trd>po&scaron;kodit ohn&ecaron;m</trd>,
    <trd>p&rcaron;ip&aacute;lit</trd>
  </trdgrp>
  <trd lng="de">verbrennen</trd>
  <trd lng="en">burn</trd>
  <trdgrp lng="fr">
    <trd>blesser<klr tip="amb"> (par brulure)</klr></trd>,
    <trd>bruler <klr tip="amb"> (tr.)</klr></trd>,
    <trd>se <ind>bruler</ind></trd>
  </trdgrp><trd lng="fr">bruler<klr> (abimer par le feu)</klr></trd>
  <trd lng="hu">meg&eacute;get</trd>
  <trd lng="nl">verbranden</trd>
  <trdgrp lng="pl">
    <trd>oparzy&cacute; si&eogonek;</trd>,
    <trd>sparzy&cacute; si&eogonek;</trd>,
    <trd>przypiec si&eogonek;</trd>,
    <trd>spali&cacute; si&eogonek;<klr> (podczas opalania)</klr></trd>,
    <trd>ogorze&cacute;</trd>
  </trdgrp>
  <trd lng="pt">queimar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">po&scaron;kodi&tcaron; oh&ncaron;om</trd>
</drv>

<drv mrk="brul.0imuna">
  <kap><tld/>imuna, <var><kap><tld/>rezista</kap></var></kap>
  <snc>
    <dif>
      Ne brulema, kapabla rezisti al <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odoln&yacute; proti ohni</trd>,
    <trd>ohnivzdorn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fire-proof</trd>,
    <trd>incombustible</trd>
  </trdgrp>
  <trd lng="fr">incombustible</trd>
  <trdgrp lng="pl">
    <trd>trudnopalny</trd>,
    <trd>niepalny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_g;&c_o;&c_r;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_g;&c_n;&c_je;&c_u;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">oh&ncaron;ovzdorn&yacute;</trd>
</drv>
