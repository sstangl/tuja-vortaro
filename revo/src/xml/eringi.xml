<vortaro>
<art mrk="$Id: eringi.xml,v 1.12 2013/09/12 13:10:10 revo Exp $">
<kap>
  <rad>eringi</rad>/o
</kap>
<drv mrk="eringi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio de <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Eryngium</trd>), kreskanta sur vojbordoj.
    </dif>
    <trd lng="de">Mannstreu</trd>
    <trd lng="en">eryngo</trd>
    <trd lng="fr">panicaut</trd>
    <trdgrp lng="hu">
      <trd>iring&oacute;</trd>,
      <trd>&ouml;rd&ouml;gszek&eacute;r</trd>
    </trdgrp>
    <trd lng="nl">kruisdistel</trd>
    <trd lng="pl">miko&lstroke;ajek</trd>
    <trd lng="pt">er&iacute;ngio</trd>
    <trd
    lng="ru">&c_s;&c_i;&c_n;&c_je;&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </snc>
</drv>
</art>
