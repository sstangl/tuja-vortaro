<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: laktoz.xml,v 1.17 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>laktoz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="laktoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="laktoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Sukereca kaj solida substanco ekstraktata el
      <ref tip="vid" cel="lakt.0o">lakto</ref>;
      <ref tip="sin" cel="suker.lakto0o">laktosukero</ref>:
      <ekz>
        fari froma&gcirc;on ebligis malpliigon de la kvanto
        de <tld/>o en lakto
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url
          ref="http://radioverda.com/programoj/2013/1/1/rv196-20130101.html">
          Radio Verda</url></vrk>,
          <lok>RV196, 2013-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  <trd lng="br">laktoz</trd>
  <trd lng="ca">lactosa</trd>
  <trdgrp lng="cs">
    <trd>laktóza</trd>,
    <trd>mléčný cukr</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Laktose</trd>,
     <trd>Milchzucker</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&kappa;&tau;&omikron;&sigma;&alfa_ton;&kappa;&chi;&alfa;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>milk <ind>sugar</ind></trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trd lng="es">lactosa</trd>
  <trdgrp lng="fr">
    <trd>lactose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tejcukor</trd>,
    <trd>lakt&oacute;z</trd>
  </trdgrp>
  <trd lng="it">lattosio</trd>
  <trdgrp lng="nl">
    <trd>melksuiker</trd>,
    <trd>lactose</trd>
  </trdgrp>
  <trd lng="pt">lactose</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_a;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_t;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mliečny cukor</trd>
</drv>
</art>
