    <ref tip="dif" cel="skarab.gren0o.ZOO">grenskarabo</ref>
  </snc>
  <trd lng="de">Kornk&auml;fer</trd>
  <trd lng="en">corn weevil</trd>
  <trdgrp lng="fi">
    <trd>jyv&auml;k&auml;rs&auml;k&auml;s</trd>,
    <trd>(Sitophilus-suvun) <ind>k&auml;rs&auml;k&auml;s</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>calandre<klr tip="ind"> (charan&ccedil;on)</klr></trd>,
    <trd>charan&ccedil;on</trd>
  </trdgrp>
  <trd lng="hu">zsizsik</trd>
  <trd lng="nl">kalander</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j; &c_d;&c_o;&c_l;&c_g;&c_o;&c_n;&c_o;&c_s;&c_i;&c_k;</trd>
</drv>
</art>
