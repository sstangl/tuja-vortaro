<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: talant.xml,v 1.12 2015/07/04 08:37:44 revo Exp $">
    <kap>
      <rad>talant</rad>/o
    </kap>

    <drv mrk="talant.0o">
    <kap><tld/>o</kap>
    <snc mrk="talant.0o.pezunuo">
        <uzo tip="fak">HIS</uzo>
        <dif>
            Granda antikva pez<ref tip="lst" cel="mezur.0o"
            lst="voko:kvantoj">mezuro</ref>:
            <ekz>
            aleksandria <tld/>o <klr>(35kg, poste 45kg)</klr>;
            </ekz>
            <ekz>
            atika <tld/>o <klr>(26,2kg)</klr>;
            </ekz>
            <ekz>
            babilona <tld/>o <klr>(30,5kg)</klr>;
            </ekz>
            <ekz>
            biblia <tld/>o <klr>(&ccirc;. 34kg)</klr>
            <fnt><bib>PIV2</bib></fnt>.
            </ekz>
        </dif>
    </snc>
    
    <snc mrk="talant.0o.monunuo">
        <uzo tip="fak">EKON</uzo>
        <uzo tip="fak">HIS</uzo>
        <dif>
            Antikva grandvalora
            <ref tip="lst" cel="unu.mon0o"
            lst="voko:monunuoj">monunuo</ref>.
            <ekz>
            Atena <tld/>o valore egalis 6000 dra&hcirc;mojn.
            </ekz>
            <ekz>
            ora, ar&gcirc;enta <tld/>o;
            </ekz>
            <ekz>
            sinjoro, vi komisiis al mi kvin <tld/>ojn; jen mi gajnis
            pluajn kvin <tld/>ojn
            <fnt><bib>NT</bib>, <lok>&Mat; 25:20.</lok></fnt>.
            </ekz>
        </dif>
        <trd lng="pl">talent</trd>
    </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="bg">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="ca">talent</trd>
  <trdgrp lng="cs">
    <trd>hřivna aténská</trd>,
    <trd>talent</trd>
  </trdgrp>
  <trd lng="de">Talent</trd>
  <trd lng="en">talent</trd><trd lng="en">talent</trd>
  <trd lng="es">talento</trd>
  <trd lng="fr">talant</trd>
  <trd lng="hu">talentum</trd>
  <trd lng="nl">talent</trd>
  <trdgrp lng="pt">
        <trd>talento</trd>
    </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>aténska hrivna</trd>,
    <trd>talent</trd>
  </trdgrp>
</drv>

</art>

