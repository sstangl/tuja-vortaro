<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id: tungst.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <rad>tungsten</rad>/o
    </kap>
 
    <drv mrk="tungst.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <uzo tip="stl">ARK</uzo>
      <snc>
        <ref tip="dif" cel="volfram.0o">volframo</ref>
        <ekz>
          &gcirc;i konsistas el treege akra pinto el <tld/>o, lokata je
          malmultaj
          anstromoj (dekmiliononoj de milimetro) super la ekzamenota
          surfaco
          <fnt><aut>Gianfranco Cazzaro</aut>:
            <vrk>Mikroskope vastaj horizontoj</vrk>,
            <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
            12a</lok></fnt>.
        </ekz>
      </snc>
  <trd lng="ca">tungst&eacute;</trd>
  <trdgrp lng="cs">
    <trd>tungsten <klr>(ve fr. a anl. prostředí)</klr></trd>,
    <trd>wolfram</trd>
  </trdgrp>
  <trd lng="de">Tungsten</trd>
  <trd lng="fr">tungst&egrave;ne</trd>
  <trd lng="hu">volfr&aacute;m</trd>
  <trd lng="nl">tungsteen</trd>
  <trd lng="pl">tungsten</trd>
  <trd lng="pt">tungst&ecirc;nio</trd>
  <trd lng="sk">volfrám</trd>
</drv>
  </art>
