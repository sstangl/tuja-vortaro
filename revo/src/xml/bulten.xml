<art mrk="$Id: bulten.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>5</ofc>
  <rad>bulten</rad>/o
</kap>

<drv mrk="bulten.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Perioda informilo de societo:
      <ekz>
        la bulteno <ctl>Esperanto en Azio</ctl><fnt><aut>Julian
        Modest</aut>: <vrk>Libroteko Tokio esee jubileas (Japana
        esearo)</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero 6a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ju;&c_l;&c_je;&c_t;&c_e;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_ju;&c_l;&c_je;&c_t;&c_i;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>bulletin</trd>,
    <trd>list</trd>,
    <trd>věstník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bulletin</trd>,
    <trd>Bericht</trd>
  </trdgrp>
  <trd lng="en">bulletin</trd>
  <trd lng="es">bolet&iacute;n</trd>
  <trd lng="fr">bulletin</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtes&iacute;t&odblac;</trd>,
    <trd>k&ouml;zl&ouml;ny</trd>
  </trdgrp>
  <trd lng="pl">biuletyn</trd>
  <trd lng="pt">boletim</trd>
  <trd lng="ru">&c_b;&c_ju;&c_l;&c_l;&c_je;&c_t;&c_je;&c_n;&c_mol;
  <klr>(&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">vestník</trd>
</drv>
</art>
