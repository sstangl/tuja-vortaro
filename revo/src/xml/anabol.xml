<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anabol.xml,v 1.5 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anabol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anabol.0o">
  <kap><tld/>o</kap>
  <snc mrk="anabol.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      &Ccirc;ia bio&hcirc;emia procezo, kiu kreskigas la &ccirc;elarojn de 
      viva&jcirc;oj uzante nutra&jcirc;ojn, 
      kontraste al <ref tip="ant" cel="katabol.0o.FIZL">katabolo</ref>:
      <ekz>
        <tld/>o uzas haveblan energion por la sintezo de novaj &ccirc;elaj 
        komponantoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Anabolo">
          anabolo</url>, 2010-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_b;&c_o;&c_l;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">anabolisme</trd>
  <trd lng="cs">anabolizmus</trd>
  <trd lng="de">Anabolismus</trd>
  <trd lng="fr">anabolisme</trd>
  <trd lng="nl">anabolisme</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_b;&c_o;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">anabolizmus</trd>
  <trd lng="uk">&c_a;&c_n;&c_a;&c_b;&c_o;&c_l;&c_ib;&c_z;&c_m;</trd>
</drv>
</art>

