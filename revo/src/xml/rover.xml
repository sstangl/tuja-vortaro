<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rover.xml,v 1.7 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>rover</rad>/o  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rover.0o">
  <kap><tld/>o</kap>
  <snc mrk="rover.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Pli a&gcirc;a (plej ofte pli ol 17-jara)
      <ref tip="super" cel="skolt.0o.SPO">skolto</ref>.
      <ekz>
        kompleta grupo konsistas el:
        svarmo - por skoltetoj malpli ol 11 jaraj,
        trupo - por knaboskoltoj, 11 - 17 jaraj,
        bando - por roveroj, 17 &gcirc;is 23 jaraj
        <fnt>
        <aut>J. K. Hammer</aut>
        <vrk>Skolta kaj tenduma terminaro</vrk>
        <lok>&ccirc;apitro 5. La Skoltgrupo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lup.0ido.SPO">lupido</ref>
    <trdgrp lng="fr">
      <trd>ain&eacute;<klr> (scoutisme)</klr></trd>,
      <trd>compagnon<klr> (scoutisme)</klr></trd>,
      <trd>routier<klr> (scoutisme)</klr></trd>
    </trdgrp>
    <trd lng="hu">&ouml;regcserk&eacute;sz</trd>
    <trd lng="nl">voortrekker</trd>
    <trdgrp lng="pl">
      <trd>&cacute;wik<klr> (stopie&nacute; harcerski,
skautowy)</klr></trd>,
      <trd><klr>(w og&oacute;lno&sacute;ci) </klr>starszy
<ind>harcerz</ind></trd>,
      <trd><klr>(w og&oacute;lno&sacute;ci) </klr><ind>instruktor</ind>
harcerski</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>s&ecirc;nior</trd>,
      <trd>escoteiro s&ecirc;nior</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>dospělý skaut</trd>,
    <trd>rover</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dospelý skaut</trd>,
    <trd>rover</trd>
  </trdgrp>
</drv>
</art>

