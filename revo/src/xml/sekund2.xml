<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekund2.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
<kap>
   <rad>sekundar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekund2.0a.KOMUNE">
  <kap><tld/>a</kap>
  <snc mrk="sekund2.0a.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Dua, duaranga duagrada.
    </dif>
    <subsnc mrk="sekund2.0a.GEOL">
      <uzo tip="fak">GEOL</uzo>
      <uzo tip="klr">(pri epoko)</uzo>
      <dif>
        Apartenanta al la dua epoko.
      </dif>
    </subsnc>
    <subsnc mrk="sekund2.0a.EKON">
      <uzo tip="fak">EKON</uzo>
      <dif>
        Kiu transformas kruda&jcirc;ojn al konsuma&jcirc;oj:
        <ekz>
          <tld/>a sektoro.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="primar.0a.KOMUNE">primara</ref>,
      <ref cel="tercia.0a.KOMUNE">terciara</ref>,
      <ref cel="kvater.0a.KOMUNE">kvaternara</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>druhotný</trd>,
    <trd>méně důležitý</trd>,
    <trd>postranní</trd>,
    <trd>sekundární</trd>,
    <trd>vedlejší</trd>
  </trdgrp>
  <trd lng="de">sekund&auml;r</trd>
  <trd lng="fr">secondaire<klr> (&eacute;poque)</klr></trd>
  <trd lng="hu">m&aacute;sodkori</trd>
  <trdgrp lng="it">
    <trd>Secondario<klr tip="ind"> (era geologica)</klr></trd>,
    <trd>Mesozoico</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Secundair<klr tip="ind"> (tijdperk)</klr></trd>,
    <trd>Mesozo&iuml;cum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drugorz&eogonek;dowy</trd>,
    <trd>mezozoiczny</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>druhoradý</trd>,
    <trd>druhotný</trd>,
    <trd>sekundárny</trd>
  </trdgrp>
</drv>
</art>
