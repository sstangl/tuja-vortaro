<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: strid.xml,v 1.7 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>strid</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="strid.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="strid.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Bruadi</ref> la&ubreve;te kaj
      akre, malagrable:
      <ekz>
        malanta&ubreve; &scirc;ia dorso en la dormo&ccirc;ambro ek<tld/>is la telefono
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 20a, p. 202a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ječet</trd>,
    <trd>vřeštět</trd>
  </trdgrp>
  <trd lng="fr">crever les tympans</trd>
  <trdgrp lng="hu">
    <trd>siv&iacute;t</trd>,
    <trd>s&uuml;v&iacute;t</trd>,
    <trd>sip&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">huilen</trd>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_r;&c_je;&c_z;&c_k;&c_i;&c_je; &c_z;&c_v;&c_u;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>hvižďať</trd>,
    <trd>jačať</trd>,
    <trd>vrešťať</trd>
  </trdgrp>
</drv>

<drv mrk="strid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="strid.0a.SON">
    <dif>
      Akra kaj la&ubreve;ta:
      <ekz>
        &scirc;i jelpe kaj <tld/>e ploras
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 82a, volumo 3a, p.
        25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">strident</trd>
  <trdgrp lng="hu">
    <trd>siv&iacute;t&oacute;</trd>,
    <trd>f&uuml;lsiket&iacute;t&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oorverscheurend</trd>,
    <trd>snijdend</trd>,
    <trd>schel</trd>,
    <trd>door <ind>merg</ind> en been gaand</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>,
    <trd>&c_p;&c_r;&c_o;&c_n;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>

