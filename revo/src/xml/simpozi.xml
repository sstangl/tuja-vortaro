<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: simpozi.xml,v 1.11 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>simpozi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="simpozi.0o">
  <kap><tld/>o</kap>
  <snc mrk="simpozi.0o.scienca">
    <dif>
      <ref tip="super" cel="ven.kun0o">Kunveno</ref>, ofte internacia,
      de sciencistoj
      a&ubreve; fakuloj por interkonsili&gcirc;o, prelegado, reciproka informo pri
      sciencaj problemoj:
      <ekz>
        <tld/>o okazinta en Zagrebo, organizita de la Akademio de
        Esperanto
        <fnt>
        <aut>Carlo Minnaja</aut>,
        <vrk>Du akademioj kune (Geraldo Mattos kaj Christer Kiselman,
        Lingva planado kaj leksikologio)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 1a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kolokv.0o">kolokvo</ref>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_m;&c_p;&c_o;&c_z;&c_ib;&c_ju;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>sympozium</trd>,
    <trd>sypmozion</trd>,
    <trd>vědecká konference</trd>
  </trdgrp>
  <trd lng="de">Symposium</trd>
  <trd lng="en">symposium</trd>
  <trd lng="fr">symposium</trd>
  <trd lng="hu">szimpozium</trd>
  <trd lng="it">simposio</trd>
  <trd lng="pt">simp&oacute;sio</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_p;&c_o;&c_z;&c_i;&c_u;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>sympózium</trd>,
    <trd>vedecká konferencia</trd>
  </trdgrp>
</drv>
</art>

