<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: terapi.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>terapi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="terapi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="terapi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Metoda <ref tip="super" cel="kurac.0ado">kuracado</ref>:
      <ekz>
        nur &ccirc;irka&ubreve; 500 el ili ricevadas la
        modernan <tld/>on per tri medikamentoj
        <fnt><aut>V. Lemelev</aut>, <vrk>Novaj strategioj en
        Rusio</vrk>,
        <bib>Monato</bib>, <lok>2001:5, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">ter&agrave;pia</trd>
  <trdgrp lng="cs">
    <trd>léčba</trd>,
    <trd>terapie</trd>
  </trdgrp>
  <trd lng="de">Therapie</trd>
  <trd lng="en">therapy</trd>
  <trd lng="es">terapia</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;rapie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gykezel&eacute;s</trd>,
    <trd>ter&aacute;pia</trd>,
  </trdgrp>
  <trd lng="nl">therapie</trd>
  <trd lng="pl">terapia</trd>
  <trdgrp lng="pt">
    <trd>terapia</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">liečba</trd>
</drv>

<drv mrk="terapi.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="terapi.0i.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="kurac.0i.MED">kuraci</ref>
    <ekz>
      neniam ankora&ubreve; kuracisto <tld/>is iun per tiom da hirudoj
      <fnt>
        <aut>Hevesi L., trad. Vizi L.</aut>,
        <vrk><url
        ref="https://sites.google.com/site/laaventurojdeandreojelky">
        La aventuroj de Andreo Jelky</url></vrk>,
        <lok>[2010]</lok>
      </fnt>.
    </ekz>
  </snc>
</drv>
</art>
