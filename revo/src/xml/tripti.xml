<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tripti.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>triptik</rad>/o
</kap>
<drv mrk="tripti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="tripti.0o.BELA">
    <dif>
      Pentra&jcirc;o sur tri tabuloj, tia ke la du flankaj tabuloj povas
      kunfermi&gcirc;i super la centra tabulo.
    </dif>
    <refgrp tip="vid">
      <ref cel="diptik.0o.BELA">diptiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_p;&c_t;&c_y;&c_h;</trd>
  <trd lng="ca">tr&iacute;ptic</trd>
  <trd lng="cs">triptych</trd>
  <trdgrp lng="de">
    <trd>Triptychon</trd>,
    <trd>dreiteiliges <ind>Altarbild</ind></trd>
  </trdgrp>
  <trd lng="fr">triptyque</trd>
  <trd lng="hu">triptichon</trd>
  <trd lng="it">trittico</trd>
  <trd lng="nl">triptiek</trd>
  <trd lng="pl">tryptyk</trd>
  <trd lng="pt">tr&iacute;ptico</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_p;&c_t;&c_i;&c_h;</trd>
  <trdgrp lng="sk">
    <trd>triptych</trd>,
    <trd>trojdielny obraz</trd>
  </trdgrp>
</drv>
</art>
