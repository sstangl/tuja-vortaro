<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ontogeni.xml,v 1.5 2015/10/05 14:10:18 revo Exp $">
<kap>
  <rad>ontogeni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ontogeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ontogeni.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="ontogenez.0o.BIO">ontogenezo</ref>
    <ekz>
      <tld/>o de la askosporoj  
      <fnt>
        <aut>P. Navarro&ndash;Rosin&eacute;s, M. P. Zhurbenko, C. Roux</aut>,
        <vrk><url
        ref="http://www.lichenologue.org/fichiers/docs/2010Navarro_Pseudopyrenidium.pdf">
        Pseudopyrenidium nova genro...</url></vrk>,
        <lok>Bull. Soc. linn. Provence, 2010 (61)</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="ca">
    <trd>ontog&egrave;nesi <klr>(proc&eacute;s)</klr></trd>,
    <trd>ontog&egrave;nia <klr>(ci&egrave;ncia)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ontogeneze</trd>,
    <trd>ontogenie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ontogeny</trd>,
    <trd>ontogenesis</trd>
  </trdgrp>
  <trd lng="sk">ontogen&eacute;za</trd>
</drv>
</art>

