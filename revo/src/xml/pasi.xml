<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pasi.xml,v 1.21 2015/10/11 22:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pasi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="pasi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pasi.0o.dezirego">
    <dif>
      Fervorega inklino al io; arda deziro:
      <ekz>
        sa&gcirc;o batalas kontra&ubreve; <tld/>oj;
      </ekz>
      <ekz>
        muziko estas lia sola <tld/>o;
      </ekz>
      <ekz>
        <tld/>o al ludo, por vino, al, pri, por la vero;
      </ekz>
      <ekz>
        malpla&ccirc;as nenio, se taksas <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">pasja</trd>
  </snc>
  <snc mrk="pasi.0o.amo">
    <dif>
      Arda kaj senbrida <ref tip="super" cel="am.0o">amo</ref>:
      <ekz>
        &scirc;i ne povis ka&scirc;i al li la <tld/>on, kiun &scirc;i sentis.
      </ekz>
    </dif>
    <trd lng="es">pasi&oacute;n amorosa</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_r;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">passi&oacute;</trd>
  <trd lng="cs">v&aacute;&scaron;e&ncaron;</trd>
  <trdgrp lng="de">
    <trd>Passion</trd>,
    <trd>Leidenschaft</trd>
  </trdgrp>
  <trd lng="en">passion</trd>
  <trd lng="es">pasi&oacute;n</trd>
  <trd lng="fr">passion</trd>
  <trd lng="hu">szenved&eacute;ly</trd>
  <trdgrp lng="id">
    <trd>berahi</trd>,
    <trd>gairah</trd>,
    <trd>hasrat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passie</trd>,
    <trd>hartstocht</trd>
  </trdgrp>
  <trd lng="pl">nami&eogonek;tno&sacute;&cacute;</trd>
  <trd lng="pt">paix&atilde;o</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">v&aacute;&scaron;e&ncaron;</trd>
  <trd lng="sv">passion</trd>
</drv>

<drv mrk="pasi.0a">
  <kap><tld/>a</kap>
  <snc mrk="pasi.0a.posedata">
    <dif>
      Posedata de <tld/>o.
    </dif>
  </snc>
  <snc mrk="pasi.0a.ema">
    <dif>
      <tld lit="P"/>i&gcirc;ema.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_r;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">apassionat</trd>
  <trd lng="cs">v&aacute;&scaron;niv&yacute;</trd>
  <trd lng="de">leidenschaftlich</trd>
  <trd lng="es">apasionado</trd>
  <trd lng="fr">passionn&eacute;</trd>
  <trd lng="id">[<ind>gairah</ind>] bergairah</trd>
  <trdgrp lng="nl">
     <trd>passioneel</trd>,
     <trd>hartstochtelijk</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>pe&lstroke;en <ind>pasji</ind></trd>,
     <trd>zapalony</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;ru&zcaron;iv&yacute;</trd>,
    <trd>ohniv&yacute;</trd>,
    <trd>v&aacute;&scaron;niv&yacute;</trd>
  </trdgrp>
</drv>
</art>
