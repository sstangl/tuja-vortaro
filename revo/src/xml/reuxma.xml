<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: reuxma.xml,v 1.15 2014/11/19 08:10:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>re&ubreve;matism</rad>/o
</kap>
<drv mrk="reuxma.0o">
  <kap><tld/>o</kap>
  <snc mrk="reuxma.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsan</ref>eca doloro en artikoj a&ubreve; muskoloj:
      <ekz>
        la princino kuracas sian <tld/>on, kaj la filino dio-scias-kion
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Rheumatismus</trd>,
    <trd>Rheuma</trd>
  </trdgrp>
  <trd lng="en">rheumatism</trd>
  <trd lng="fa">&a_r;&a_m;&a_A;&a_t;&f_y;&a_s;&a_m;</trd>
  <trd lng="fr">rhumatisme</trd>
  <trd lng="hu">reuma</trd>
  <trdgrp lng="id">
    <trd>encok</trd>,
    <trd>reumatisme</trd>
  </trdgrp>
  <trd lng="nl">reuma</trd>
  <trd lng="pt">reumatismo</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
