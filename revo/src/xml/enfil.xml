<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: enfil.xml,v 1.13 2015/07/04 08:37:33 revo Exp $">
  <kap>
    <rad>enfil</rad>/i
  </kap>
  <drv mrk="enfil.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">MIL</uzo>
    <snc>
      <dif>
      Pafi la&ubreve;longe:
      <ekz>per artilerio a&ubreve; ma&scirc;inpafiloj oni <tld/>as la
      malamikajn poziciojn.</ekz>
      </dif>
    </snc>
  <trd lng="cs">bočně ostřelovat</trd>
  <trdgrp lng="de">
       <trd>mit Feuer bestreichen</trd>,
       <trd>der L&auml;nge nach beschie&szlig;en</trd>
    </trdgrp>
  <trd lng="es">enfilar</trd>
  <trdgrp lng="fr">
      <trd>enfiler<klr tip="ind"> (prendre en enfilade)</klr></trd>,
      <trd>prendre en <ind>enfilade</ind></trd>
    </trdgrp>
  <trdgrp lng="hu">
      <trd>oldalaz&oacute; <ind>t&udblac;z</ind> al&aacute; vesz</trd>,
      <trd>v&eacute;gigp&aacute;szt&aacute;z<klr tip="ind">
      (l&ouml;v&eacute;ssorozattal)</klr></trd>
    </trdgrp>
  <trd lng="it">infilare<klr> (di artiglieria o sim.)</klr></trd>
  <trdgrp lng="pl">
      <trd>ostrzeliwa&cacute;</trd>,
      <trd>przykrywa&cacute; <ind>ogniem</ind><klr> (pot.)</klr></trd>
    </trdgrp>
  <trd lng="pt">enfiar <klr tip="amb">(mil.)</klr></trd>
  <trd lng="ru">&c_o;&c_b;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_o;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;
    &c_o;&c_g;&c_n;&c_jo;&c_m;</trd>
  <trd lng="sk">bočne ostreľovať</trd>
</drv>
</art>
