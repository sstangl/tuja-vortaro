<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id: hxolecistit.xml,v 1.10 2014/11/18 20:10:25 revo Exp $">
    <kap>
      <rad>&hcirc;olecistit</rad>/o
    </kap>
 
    <drv mrk="hxolecistit.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MED</uzo>
        <dif>
          <ref tip="lst" cel="inflam.0o"
          lst="voko:malsanoj">Inflamo</ref> de <ref
          cel="gal.0veziko">galveziko</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_h;&c_a;&c_l;&c_je;&c_c;&c_y;&c_s;&c_t;&c_y;&c_t;</trd>
      <trd lng="bg">&c_h;&c_o;&c_l;&c_je;&c_c;&c_i;&c_s;&c_t;&c_i;&c_t;</trd>
      <trd lng="de">Cholezystitis</trd>
      <trd lng="en">cholecystitis</trd>
      <trd lng="es">colecistitis</trd>
      <trd lng="id">kolesistitis</trd>
      <trd lng="pl">zapalenie p&eogonek;cherzyka
&zdot;&oacute;&lstroke;ciowego</trd>
      <trd lng="pt">colecistite</trd>
      <trdgrp lng="ru">
        <trd>&c_h;&c_o;&c_l;&c_je;&c_c;&c_i;&c_s;&c_t;&c_i;&c_t;</trd>,
        <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;
&c_zh;&c_je;&c_l;&c_ch;&c_n;&c_o;&c_g;&c_o;
&c_p;&c_u;&c_z;&c_y;&c_r;&c_ja;</trd>
      </trdgrp>
      <trd lng="it">colecistite</trd>
    </drv>
  </art>
 

