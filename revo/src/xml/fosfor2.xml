<vortaro>
  <art mrk="$Id: fosfor2.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
    <kap>
      <rad>fosforit</rad>/o
    </kap>
    <drv mrk="fosfor2.0o">
      <kap><tld/>o</kap>
      <snc mrk="fosfor2.0o.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Fosforerco, sedimenta mina&jcirc;o konsistanta el etgrajna miksa&jcirc;o
          de <ref cel="apatit.0o">apatito</ref> kun kvarco, karbonato,
          gipso, argilo kc; kruda&jcirc;o por produkti
          <ref cel="fosfor.0o">fosforon</ref> kaj fosforajn
          sterka&jcirc;ojn.
        </dif>
        <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_r;&c_y;&c_t;</trd>
        <trd lng="de">Phosphorit</trd>
        <trd lng="fr">phosphorite</trd>
        <trd lng="hu">foszforit</trd>
        <trd lng="pt">fosforita</trd>
        <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_i;&c_t;</trd>
      </snc>
  <trd lng="cs">fosforečnan</trd>
  <trd lng="sk">fosforečnan</trd>
</drv>
  </art>
