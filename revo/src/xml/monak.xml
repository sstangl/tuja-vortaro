<kap>
  <rad>monak</rad>/o
</kap>
<drv mrk="monak.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="monahx.0o">mona&hcirc;o</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_h;</trd>
  <trdgrp lng="cs">
    <trd>mnich</trd>,
    <trd>řeholník</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>religieux<klr> (subst.)</klr></trd>,
    <trd>moine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szerzetes</trd>,
    <trd>bar&aacute;t</trd>
  </trdgrp>
  <trd lng="nl">monnik</trd>
  <trd lng="pt">monge</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_h;</trd>
  <trdgrp lng="sk">
    <trd>mních</trd>,
    <trd>rehoľník</trd>
  </trdgrp>
</drv>
</art>
