<vortaro>
<art mrk="$Id: trauxm.xml,v 1.12 2015/10/09 13:10:19 revo Exp $">
<kap>
  <rad>tra&ubreve;m</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="trauxm.0o">
  <kap><tld/>o</kap>
  <snc mrk="trauxm.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="trauxmat.0o.MED">tra&ubreve;mato <sncref/></ref>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_s;&f_y;&a_b;</trd>,
      <trd>&a_j;&a_r;&a_A;&a_H;&a_t;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>s&eacute;r&uuml;l&eacute;s</trd>,
      <trd>trauma</trd>
    </trdgrp>
    <trd lng="pt">traumatismo</trd>
  </snc>
  <snc mrk="trauxm.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <ref tip="dif" cel="trauxmat.0o.PSI">tra&ubreve;mato <sncref/></ref>
      <ekz>
        el la vundo (la <tld/>o, se vi preferas fak&jcirc;argoni)
        <klr>...</klr> mi neniam resani&gcirc;is
        <fnt><bib>BonaLingvo</bib>, <lok>p. 9a</lok></fnt>.
      </ekz>
    <trd lng="fa">&a_D;&a_r;&a_b;&a_h;&a_hamza_sure; &a_r;&a_w;&a_H;&f_y;</trd>
    <trdgrp lng="hu">
      <trd>trauma</trd>,
      <trd>lelki <ind>megr&aacute;zk&oacute;dtat&aacute;s</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_w;&c_m;&c_a;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_v;&c_m;&c_a;</trd>
  <trd lng="ca">trauma</trd>
  <trd lng="de">Trauma</trd>
  <trdgrp lng="en">
    <trd>trauma</trd>,
    <trd>traumatism</trd>
  </trdgrp>
  <trd lng="es">trauma</trd>
  <trd lng="fr">traumatisme</trd>
  <trdgrp lng="hu">
    <trd>s&eacute;r&uuml;l&eacute;s</trd>,
    <trd>trauma</trd>,
    <trd>lelki <ind>megr&aacute;zk&oacute;dtat&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="it">trauma</trd>
  <trd lng="nl">trauma</trd>
  <trd lng="pt">trauma</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_m;&c_a;</trd>
</drv>
</art>

