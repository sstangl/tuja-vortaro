<art mrk="$Id: lupara.xml,v 1.12 2015/09/22 10:10:20 revo Exp $">
<kap>
  <rad>luparaneed</rad>/oj
</kap>
<drv mrk="lupara.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="lupara.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Flavbrunaj &gcirc;is nigraj araneoj, kiuj
      kaptas &ccirc;asa&jcirc;ojn per rapida kuro;
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">familio</ref> el la ordo <ref
      tip="malprt"
      cel="arane.0uloj">araneuloj</ref>
      (<trd lng="la">Lycosidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_v;&c_malmol;&c_l;&c_ch;&c_i; &c_p;&c_a;&c_ja;&c_c;&c_i;</trd>
  <trd lng="cs">tarantulovit&iacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Wolfsspinnen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">wolf spiders</trd>
  <trdgrp lng="fr">
    <trd>araign&eacute;e-loups<klr> (fam.)</klr></trd>,
    <trd>lycosidae</trd>
  </trdgrp>
  <trd lng="hu">farkasp&oacute;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">wolfsspinnen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">pogo&nacute;cowate</trd>
  <trd lng="pt">licos&iacute;deos</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">tarantulovit&eacute;</trd>
</drv>
</art>
