<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gana.xml,v 1.8 2014/06/10 04:10:52 revo Exp $">
<kap>
  <rad>Gana</rad>/o
</kap>

<drv mrk="gana.G0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gana.G0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordokcidenta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Akro.
      Teritorio de Respubliko Ganao.
      ISO-3166 kodo: GH.
      &Gcirc;i limas kun
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="ebur.E0Bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid" cel="togo.T0olando.GEOG">Togolando</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trd lng="bg">&c_G;&c_a;&c_n;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>Ghana</trd>,
      <trd>Rep&uacute;blica de <ind>Ghana</ind></trd>
    </trdgrp>
    <trd lng="de">Ghana</trd>
    <trd lng="en">Ghana</trd>
    <trdgrp lng="fr">
     <trd>Ghana</trd>,
     <trd>R&eacute;publique du Ghana</trd>
    </trdgrp>
    <trd lng="id">Ghana</trd>
    <trdgrp lng="pl">
      <trd>Ghana</trd>
      <trd>Republika <ind>Ghany</ind></trd>
    </trdgrp>
    <trd lng="pt">Gana</trd>
  </snc>
</drv>

</art>

