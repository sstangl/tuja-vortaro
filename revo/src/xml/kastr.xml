<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kastr.xml,v 1.16 2009/08/18 19:17:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kastr</rad>/i
</kap>
<drv mrk="kastr.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senigi homon a&ubreve; beston je naskigpovo, fortran&ccirc;ante la seksajn
      glandojn:
      <ekz>
        la <ind><tld/>ito</ind>j soprane kantas<fnt>B</fnt>.       
        <trd lng="br">spazhard</trd>
        <trd lng="fr">castrat</trd>
        <trdgrp lng="nl">
          <trd>castraat</trd>,
          <trd>gesnedene</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>                             
  <trd lng="be">&c_s;&c_p;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">spazha&ntilde;</trd>
  <trd lng="ca">castrar</trd>
  <trdgrp lng="de">
     <trd>kastrieren</trd>,
     <trd>verschneiden</trd>
  </trdgrp>
  <trd lng="en">castrate</trd>
  <trd lng="es">castrar</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_ayn;&a_q;&f_y;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>castrer</trd>,
    <trd>ch&acirc;trer</trd>,
    <trd>couper<klr tip="ind"> (castrer)</klr></trd>,
    <trd>&eacute;masculer</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kiher&eacute;l</trd>,
     <trd>misk&aacute;rol</trd>,
     <trd>ivartalan&iacute;t</trd>,
     <trd>kasztr&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>castreren</trd>,
    <trd>ontmannen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_k;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
