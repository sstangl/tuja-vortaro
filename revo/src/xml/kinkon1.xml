<kap>
  <rad>kinkonin</rad>/o
</kap>
<drv mrk="kinkon1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Unu el la alkaloidaj substancoj
      en <ref cel="kinkon.0o.KEM">kinkono</ref>:
      <ekz>
        la <tld/>o estas uzata en la medicino plej ofte
        kiel <tld/>a sulfato.
      </ekz>
    </dif>
    <ref tip="vid" cel="hxinin.0o">&hcirc;inino</ref>
  </snc>
  <trd lng="bg">&c_c;&c_i;&c_n;&c_h;&c_o;&c_n;&c_i;&c_n;</trd>
  <trd lng="cs">cinchonin</trd>
  <trd lng="de">Zinchonin</trd>
  <trd lng="en">cinchonine</trd>
  <trd lng="fr">cinchonine</trd>
  <trd lng="hu">cinkonin</trd>
  <trd lng="nl">cinchonine</trd>
  <trd lng="ru">&c_c;&c_i;&c_n;&c_h;&c_o;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">cinchon√≠n</trd>
  <trd lng="sv">cinkonin</trd>
</drv>
</art>
