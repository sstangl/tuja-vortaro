<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: hxalif.xml,v 1.10 2014/04/08 12:10:17 revo Exp $">
    <kap>
      <rad>&hcirc;alif</rad>/o
    </kap>

    <drv mrk="hxalif.0o">
      <kap><tld/>o</kap>
      <snc mrk="hxalif.0o.ISL">
        <uzo tip="fak">ISL</uzo>
        <ref tip="dif" cel="kalif.0o.ISL">kalifo</ref>
        <ekz>
          <tld/>o Mamuno sendis al imperiestro Teofilo <klr>...</klr>
          leteron
          <fnt><aut>Sergio Pokrovskij</aut>, <vrk>slavonaj
          skriboj</vrk>,
            <bib>LOdE</bib>, <lok>n-ro 51a, 1999:1a</lok></fnt>.
        </ekz><ekz>
          la <tld/>o kaj la anoj de la malnova osmanida dinastio
          estis ellandigitaj
          <fnt><bib>Viki</bib>,
            <lok>
<url ref="http://eo.wikipedia.org/wiki/Kemal_ATAT%C3%
9CRK#Reformado_kaj_moderni.C4.9Dado_de_Turkio">Kemal ATAT&Uuml;RK</url>
</lok></fnt>;
        </ekz><ekz>
          fiera &gcirc;ojo helpi, helpi senfine al &ccirc;iuj kaj &ccirc;iu, 
          iam ebla nur por fabelaj <tld/>oj de arabaj legendoj
          <klr>...</klr>
          <fnt>
            <aut>Ivan Efremov, tr. Jurij Finkel</aut>:
            <vrk>Horo de Bovo</vrk>,
            <lok>
<url ref="http://www.esperanto.mv.ru/Efremov/horo_13.html">&ccirc;ap. 13a
</url></lok>
          </fnt>.
        </ekz>
      </snc>
      <trd lng="be">&c_h;&c_a;&c_l;&c_ib;&c_f;</trd>
      <trd lng="bg">&c_h;&c_a;&c_l;&c_i;&c_f;</trd>
      <trd lng="de">Kalif</trd>
      <trd lng="en">caliph</trd>
      <trdgrp lng="fr">
      <trd>calife</trd>,
        <trd>khalife</trd>,
        <trd>caliphe</trd>
      </trdgrp>
      <trd lng="id">khalifah</trd>
      <trd lng="pl">kalif</trd>
      <trd lng="pt">califa</trd>
      <trd lng="ru">&c_h;&c_a;&c_l;&c_i;&c_f;</trd>
      <trd lng="tr">halife</trd>
      <trd lng="it">califfo</trd>
    </drv>

    <drv mrk="hxalif.0ujo">
      <kap><tld/>ujo</kap>
      <snc mrk="hxalif.0ujo.ISL">
        <uzo tip="fak">ISL</uzo>
        <dif>
          <ref tip="dif" cel="kalif.0ujo.ISL">kalifujo</ref>:
          <ekz>
            rekonkero de Iberio per e&ubreve;ropa civilizo, kiu iom post
            iom dispartigis la t.n. Kordovan <tld/>ujon
            <fnt>
              <bib>Viki</bib>,
              <lok>
<url ref="http://eo.wikipedia.org/wiki/Portugalio#Historio">Historio de 
Portugalio</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_h;&c_a;&c_l;&c_ib;&c_f;&c_a;&c_t;</trd>
      <trd lng="de">Kalifat</trd>
      <trd lng="fr">califat</trd>
      <trd lng="id">khilafah</trd>
      <trd lng="pl">kalifat</trd>
      <trd lng="ru">&c_h;&c_a;&c_l;&c_i;&c_f;&c_a;&c_t;</trd>
      <trd lng="tr">halifelik</trd>
     <trd lng="it">califfato</trd>
    </drv>
  </art>

