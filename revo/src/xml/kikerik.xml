<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kikerik.xml,v 1.9 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kikerik</rad>/i <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kikerik.0i">
  <kap><tld/>i</kap>
  <snc mrk="kikerik.0i.ZOO">
    <ref tip="dif" cel="kokeri.0i.ZOO">kokeriki</ref>
    <ekz>
      li &ccirc;irka&ubreve;rigardis <klr>[...]</klr> <tld/>antan kokon, 
      saltantan korvon
      <fnt><bib>Metrop</bib></fnt>..
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kokrhat</trd>
  <trd lng="de">kr&auml;hen</trd>
  <trdgrp lng="fr">
    <trd>chanter<klr tip="ind"> (en p. d'un coq)</klr></trd>,
    <trd>faire <ind>cocorico</ind></trd>
  </trdgrp>
  <trd lng="hu">kukor&eacute;kol</trd>
  <trd lng="nl">kraaien<klr tip="ind"> (als een haan)</klr></trd>
  <trd lng="pl">pia&cacute;<klr tip="ind"> (o kogucie)</klr></trd>
  <trd lng="sk">kikiríkať</trd>
</drv>
</art>

