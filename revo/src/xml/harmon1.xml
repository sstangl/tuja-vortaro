<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: harmon1.xml,v 1.31 2015/11/18 14:10:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>harmoni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="harmon1.0o">
  <kap><tld/>o</kap>
  <snc mrk="harmon1.0o.arangxo">
    <dif>
      Efekto de objekto, kies diversaj partoj pla&ccirc;e
      akordas inter si, a&ubreve; de la akordo inter diversaj
      objektoj:
      <ekz>
        <tld/>o de l' Universo;
      </ekz>
      <ekz>
        <tld/>o de bone komponita disertacio;
      </ekz>
      <ekz>
        Esperanto estas plena je <tld/>o kaj estetiko kaj
        memorigas per si la lingvon italan
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        necesas ia ekvilibro inter la tri aspektoj de lingva planado,
        do ia kunefiko kaj <tld/>o inter statusa, korpusa kaj akira
        planado
        <fnt>
        <aut>C. Kiselman</aut>
        <vrk>La sveda faklingvo en tekniko, matematiko kaj
        natursciencoj</vrk>
        <lok><url
        ref="http://htliu.nease.net/db/04_Kiselman.htm">Lingvoplanado
        kaj lingvopolitiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="harmon1.0o.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Agrabla samtempa sonado de tonoj:
        <ekz>
          <tld/>o de vo&ccirc;o, de versoj;
        </ekz>
        <ekz>
          <tld/>aj kantoj.
        </ekz>
      </dif>
      <ref tip="vid" cel="akord.0o.MUZ">akordo</ref>
      <trd lng="fr">harmonie<klr tip="ind"> (mus.)</klr></trd>
      <trd lng="id">harmonik</trd>
    </subsnc>
    <subsnc mrk="harmon1.0o.POE">
      <uzo tip="fak">POE</uzo>
      <dif>
        Baza principo de <ref cel="vers.0farado">versfarado</ref>,
        konsistanta en ripetado a&ubreve; kontrastigo de iuj sonoj;
        tiuj sonefektoj mem.
      </dif>
      <refgrp tip="vid">
        <ref cel="aliter.0o">aliteracio</ref>,
        <ref cel="asonan.0o.ena_ripetado">asonanco</ref>,
        <ref cel="rim.0o">rimo</ref>.
      </refgrp>
      <trd lng="fr">harmonie<klr tip="ind"> (po&eacute;.)</klr></trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="akord.0o.konveneco">akordo</ref>,
      <ref cel="ord.0o">ordo</ref>,
      <ref cel="propor">proporcieco</ref>
    </refgrp>
    <trd lng="en">harmony</trd>
    <trdgrp lng="fr">
      <trd>harmonie<klr tip="ind"> (effet esth&eacute;tique)</klr></trd>,
      <trd>accord<klr tip="ind"> (ad&eacute;quation)</klr></trd>,
      <trd>ad&eacute;quation</trd>,
      <trd>affinit&eacute;<klr tip="ind"> (harmonie)</klr></trd>,
      <trd>concordance</trd>
    </trdgrp>
  </snc>
  <snc mrk="harmon1.0o.paco">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="akord.inter0o.samopinieco">Interakordo</ref>,
      paco inter pluraj personoj:
      <ekz>
        la <tld/>o de edzoparo;
      </ekz>
      <ekz>
        konfuzi la <tld/>on de la &scirc;tato;
      </ekz>
      <ekz>
        en nia societo plene regas <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind>en malsata familio mankas <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="fr">
          <trd>quand le <ind>r&acirc;telier</ind> est vide, les chevaux se
          battent</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0o.samopinieco">akordo</ref>,
      <ref cel="konkor.0o">konkordo</ref>,
      <ref cel="konsen.0o">konsento</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>harmony</trd>,
      <trd>accord</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>harmonie<klr tip="ind"> (dans les rapports
      humains)</klr></trd>,
      <trd>accord<klr tip="ind"> (fait d'&ecirc;tre en accord)</klr></trd>,
      <trd>entente<klr tip="ind"> (accord)</klr></trd>,
      <trd>concorde</trd>
    </trdgrp>
    <trd lng="pl">zgoda</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">harmonia</trd>
  <trdgrp lng="cs">
    <trd>harmonie</trd>,
    <trd>jednota</trd>,
    <trd>soulad</trd>,
    <trd>souzvuk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Harmonie</trd>,
    <trd>Wohlklang</trd>,
    <trd>Zusammenklang</trd>
  </trdgrp>
  <trd lng="el">&alfa;&rho;&my;&omikron;&ny;&jota_ton;&alfa;</trd>
  <trd lng="es">armon&iacute;a</trd>
  <trdgrp lng="hu">
    <trd>harm&oacute;nia</trd>,
    <trd>&ouml;sszhang</trd>
  </trdgrp>
  <trd lng="id">harmoni</trd>
  <trd lng="it">armonia</trd>
  <trd lng="nl">harmonie</trd>
  <trd lng="pl">harmonia</trd>
  <trd lng="pt">harmonia</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>s&uacute;hra</trd>,
    <trd>s&uacute;lad</trd>,
    <trd>zhoda</trd>
  </trdgrp>
</drv>

<drv mrk="harmon1.0a">
  <kap><tld/>a</kap>
  <snc mrk="harmon1.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o, prezentanta <tld/>on:
      <ekz>
        &scirc;afaro <tld/>a lupon ne timas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>harm&ograve;nic</trd>,
    <trd>equilibrat</trd>
  </trdgrp>
  <trd lng="en">harmonious</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>harmonie</ind></trd>,
    <trd>harmonieux</trd>,
    <trd>en <ind>harmonie</ind><klr tip="ind"> (loc. adj.)</klr></trd>,
    <trd>&eacute;l&eacute;gant</trd>,
    <trd>&eacute;quilibr&eacute;</trd>,
    <trd>esth&eacute;tique</trd>,
    <trd>gracieux</trd>,
    <trd>m&eacute;lodieux</trd>,
    <trd>musical<klr tip="ind"> (m&eacute;lodieux)</klr></trd>,
    <trd>r&eacute;gulier</trd>
  </trdgrp>
  <trd lng="id">harmonis</trd>
  <trd lng="it">armonico</trd>
  <trd lng="pl">zharmonizowany</trd>
</drv>

<drv mrk="harmon1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="harmon1.0i.KOMUNE">
    <dif>
      Esti en <tld/>o kun:
      <ekz>
        niaj animoj <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio malmulte <tld/>as kun via nobla spirito;
      </ekz>
      <ekz>
        tiuj tapetoj bone <tld/>as kun la meblaro.
      </ekz>
    </dif>
    <ref tip="vid" cel="akord.0i">akordi</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">estar en <ind>harmonia</ind></trd>
  <trd lng="cs">harmonovat</trd>
  <trd lng="de">harmonieren</trd>
  <trdgrp lng="en">
    <trd>to be in harmony</trd>,
    <trd>agreement</trd>, <trd>accord</trd>, <trd>to harmonize</trd>
  </trdgrp>
  <trd lng="es">armonizar</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>harmonie</ind></trd>,
    <trd>s'<ind>harmoniser</ind></trd>,
    <trd>s'<ind>accorder</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>s'<ind>allier</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>s'<ind>assembler</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>s'<ind>associer</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>s'<ind>assortir</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>s'<ind>harmoniser</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd>se <ind>marier</ind><klr tip="ind"> (&ecirc;tre en
    harmonie)</klr></trd>,
    <trd><ind>aller</ind> avec</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>harmoniz&aacute;l</trd>,
    <trd>&ouml;sszhangban van</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>essere in <ind>armonia</ind></trd>,
    <trd>armonizzarsi</trd>
  </trdgrp>
  <trd lng="nl">in <ind>harmonie</ind> zijn</trd>
  <trd lng="pl">harmonijny</trd>
  <trd lng="pt">estar em <ind>harmonia</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>harmonizova&tcaron;</trd>,
    <trd>zn&aacute;&scaron;a&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="harmon1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="harmon1.0igi.KOMUNE">
    <dif>
      Aran&gcirc;i la&ubreve; <tld/>o, krei <tld/>on inter:
      <ekz>
        <tld/>igi la kolorojn.
      </ekz>
    </dif>
    <ref tip="vid" cel="akord.0igi">akordigi</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">harmonitzar<klr> (tr.)</klr></trd>
  <trd lng="cs">harmonizovat</trd>
  <trd lng="el">&epsilon;&ny;&alfa;&rho;&my;&omikron;&ny;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">to harmonize</trd>
  <trd lng="es">armonizar</trd>
  <trdgrp lng="fr">
    <trd>harmoniser<klr tip="ind"> (mettre en harmonie)</klr></trd>,
    <trd>accorder<klr tip="amb"> (harmoniser)</klr></trd>,
    <trd>mettre en <ind>accord</ind></trd>,
    <trd>faire s'<ind>entendre</ind></trd>,
    <trd>allier<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>assembler<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>assortir<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>associer<klr tip="ind"> (harmoniser)</klr></trd>,
    <trd>marier<klr tip="ind"> (harmoniser)</klr></trd>
  </trdgrp>
  <trd lng="hu">&ouml;sszhangba hoz</trd>
  <trd lng="it">armonizzare</trd>
  <trd lng="nl">harmoni&euml;ren</trd>
  <trd lng="pl">harmonizowa&cacute;</trd>
  <trd lng="pt">harmonizar</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zharmonizova&tcaron;</trd>
</drv>

<drv mrk="harmon1.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="harmon1.mal0o.FIG">
    <dif>
      Malakordo, malagrabla kunesto de a&jcirc;oj:
      <ekz>
        Vi volas min &ccirc;e-ekrane, /
        mi volas vin man-en-mane. /
        Ho! Kial tiu mal<tld/>' ?
        <fnt>
          <aut>A. Sartorato</aut>,
          <vrk><url
          ref="http://www.ipernity.com/doc/55897/2709869/comment/2801999#comment2801999">
          Malharmonio</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>disc&ograve;rdia</trd>,
    <trd>discrep&agrave;ncia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neshoda</trd>,
    <trd>nesoulad</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>discord</trd>, <trd>dissonance</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>discordance</trd>,
    <trd>disharmonie</trd>,
    <trd>dissonance</trd>
  </trdgrp>
  <trd lng="id">disharmoni</trd>
  <trdgrp lng="pl">
    <trd>niegodno&sacute;&cacute;</trd>,
    <trd>rozd&zacute;wi&eogonek;k</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nes&uacute;lad</trd>,
    <trd>rozpor</trd>
  </trdgrp>
</drv>
</art>
