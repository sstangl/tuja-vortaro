<vortaro>
<art mrk="$Id: skarla1.xml,v 1.16 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>skarlatin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skarla1.0o">
  <kap><tld/>o</kap>
  <snc mrk="skarla1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta febro karakterizata per ru&gcirc;aj makuloj sur la ha&ubreve;to:
      <ekz>
        &scirc;i vartis min senemocie, tenere, kvaza&ubreve; temus pri <tld/>o
        <fnt><aut>R. Radiguet, trad. M. Duc Goninaz</aut>, <vrk>Kun 
        Diablo en la korpo</vrk>, <lok>Mondial, 2006</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_a;&c_r;&c_l;&c_ja;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_s;&c_k;&c_a;&c_r;&c_l;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>skarlatina</trd>,
    <trd>spála</trd>,
    <trd>šarlach</trd>
  </trdgrp>
  <trd lng="de">Scharlach</trd>
  <trdgrp lng="en">
    <trd>scarlet fever</trd>,
    <trd>scarlatina</trd>
  </trdgrp>
  <trd lng="fr">scarlatine</trd>
  <trdgrp lng="hu">
    <trd>skarl&aacute;t</trd>,
    <trd>v&ouml;rheny</trd>
  </trdgrp>
  <trd lng="id">skarlatina</trd>
  <trd lng="nl">roodvonk</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_r;&c_l;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">šarlach</trd>
</drv>
</art>
