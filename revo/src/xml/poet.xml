    <trd>poet</trd>,
    <trd>poeta</trd>,
    <trd>pěvec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Poet</trd>,
    <trd>Dichter</trd>
  </trdgrp>
  <trd lng="en">poet</trd>
  <trd lng="es">poeta</trd>
  <trd lng="fr">po&egrave;te</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;lt&odblac;</trd>,
    <trd>po&eacute;ta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dichter</trd>,
    <trd>po&euml;et</trd>
  </trdgrp>
  <trd lng="pl">poeta</trd>
  <trd lng="pt">poeta</trd>
  <trd lng="ru">&c_p;&c_o;&c_e;&c_t;</trd>
  <trd lng="sk">básnik</trd>
</drv>
