        <vrk>Kura&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 2a,
        p.
        23a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_e;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_e;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_je;&c_t;</trd>
  <trd lng="ca">poeta</trd>
  <trdgrp lng="cs">
