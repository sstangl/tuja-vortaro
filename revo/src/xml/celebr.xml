<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: celebr.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>celebr</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="celebr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="celebr.0i.MOR">
    <dif>
      Plenumi la&ubreve;kutime ian
      <ref tip="vid" cel="ceremo.0o.solena">ceremonion</ref>:
     <ekz>
      <tld/>i feston<fnt>Z</fnt>,
     </ekz>
     <ekz>
      datrevenon, meson.
     </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.0i">ofici</ref>,
      <ref cel="fest.pri0i.KOMUNE">prifesti</ref>,
      <ref cel="solen.0i.MOR">soleni</ref>
    </refgrp>
  </snc>
  <trd
  lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">celebrar</trd>
  <trdgrp lng="cs">
    <trd>celebrovat</trd>,
    <trd>oslavovat</trd>,
    <trd>slavit</trd>
  </trdgrp>
  <trd lng="de">zelebrieren</trd>
  <trd lng="el">&gamma;&jota;&omikron;&rho;&tau;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">celebrate</trd>
  <trd lng="es">celebrar</trd>
  <trd lng="fr">c&eacute;l&eacute;brer</trd>
  <trdgrp lng="hu">
    <trd>celebr&aacute;l</trd>,
    <trd>meg&uuml;l</trd>
  </trdgrp>
  <trd lng="it">celebrare</trd>
  <trdgrp lng="pl">
    <trd>celebrowa&cacute;</trd>,
    <trd>odprawia&cacute; <ind>ceremoni&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">celebrar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;
    &c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oslavovať</trd>,
    <trd>sláviť</trd>
  </trdgrp>
</drv>
</art>
