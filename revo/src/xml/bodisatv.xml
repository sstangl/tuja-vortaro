<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bodisatv.xml,v 1.10 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bodisatv</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bodisatv.0o">
  <kap><tld/>o</kap>
  <snc mrk="bodisatv.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Kvaza&ubreve;-budao, rezignanta pri propra savo por unue helpi al aliaj
      vivantoj savi&gcirc;i:
      <ekz>
        &ccirc;iuj <tld/>oj a&ubreve;dinte &ccirc;i tiun Darmon jam forigis sian dubon
        <fnt>
        <aut>trad. Asano San&ccirc;i</aut>,
        <vrk><url
        ref="http://www001.upp.so-net.ne.jp/jble/sutro/sps02.html">Saddharma-Pundarika
        Sutro, 2</url></vrk>,
        <lok>La Japana Budhano, 1979-80, numero 187-190</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_d;&c_h;&c_ib;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>
  <trd lng="cs">bódhisattva</trd>
  <trd lng="fr">bodhisattva</trd>
  <trd lng="hu">bodhiszattva</trd>
  <trdgrp lng="pt">
    <trd>bodisatva</trd>,
    <trd>bodissatva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_d;&c_h;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_d;&c_i;&c_s;&c_a;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bodhisatva</trd>,
    <trd>bódisattva</trd>
  </trdgrp>
</drv>
</art>
