<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: samnit.xml,v 1.8 2009/09/26 16:31:25 revo Exp $">
<kap>
  <rad>samnit</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="samnit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Ano de antikva Italia popolo vivinta en centra apenina montaro
      &gcirc;is
      en la 3a jarcento a. K.:
      <ekz>
        konkeris la urbon <tld/>oj en la jaro 424 anta&ubreve;
        Kristo<fnt><bib>IntArkeol</bib>, <lok>p. 23a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_n;&c_ib;&c_t;</trd>
  <trd lng="ca">samnita</trd>
  <trd lng="de">Samnite</trd>
  <trd lng="es">samnita</trd>
  <trd lng="fr">Samnite<klr> (subst.)</klr></trd>
  <trd lng="hu">szamnit</trd>
  <trdgrp lng="it">
   <trd>sannita</trd>, 
   <trd>sabello</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>samnita</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_n;&c_i;&c_t;</trd>
</drv>

<drv mrk="samnit.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_n;&c_ib;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">samnitisch</trd>
  <trd lng="fr">samnite<klr> (adj.)</klr></trd>
  <trd lng="hu">szamnit</trd>
  <trdgrp lng="it">
   <trd>sannitico</trd>, 
   <trd>sabellico</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_n;&c_i;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

