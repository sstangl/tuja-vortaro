<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kompon.xml,v 1.33 2015/07/06 15:10:12 revo Exp $">
<kap>
  <ofc>1</ofc> <rad>kompon</rad>/i
</kap>
<drv mrk="kompon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompon.0i.verki">
    <dif>
      <ref tip="super" cel="verk.0i">Verki</ref>, kun aparta
      atento al la harmonio de la tuto:
    </dif>
    <subsnc mrk="kompon.0i.muzikajxon">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        <klr>(muzika&jcirc;on)</klr>
        <ekz>
          Verdi <tld/>is multajn operojn
          <fnt><bib>VdE</bib></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>komponieren</trd>
      </trdgrp>
      <trd lng="en">compose</trd>
      <trd lng="nl">componeren</trd>
      <trd lng="pl">komponowa&cacute;</trd>
      <trd lng="pt">compor</trd>
      <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_m;&c_u;&c_z;&c_y;&c_k;&c_u;</trd>
    </subsnc>
    <subsnc mrk="kompon.0i.artajxon">
      <dif>
        <klr>(verkon beletran, belartan a&ubreve; alian arta&jcirc;on)</klr>
        <ekz>
          <tld/>i versojn, romanon, himnon, kanton, pentra&jcirc;on,
        </ekz>
        <ekz>
          filmon
          <fnt><bib>PIV1</bib></fnt>,
        </ekz>
        <ekz>
          robon
          <fnt><bib>PIV1</bib></fnt>;
        </ekz>
        <ekz>
          Nero ludis kaj kantis himnon honore al la Cipra diino, al kiu
          li mem <tld/>is versojn kaj muzikon
          <fnt>
          <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
          <vrk>Quo vadis?</vrk><lok>&ccirc;ap. 41</lok>
          </fnt>;
        </ekz>
        <ekz>
          bone <tld/>ita libro, pentra&jcirc;o;
        </ekz>
        <ekz>
          tiu genio nesciis <tld/>i.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="kombin.0i.ideojn">kombini <sncref/></ref>,
        <ref cel="konstr.0i.arangxi">konstrui <sncref/></ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>zusammenstellen</trd>,
        <trd>zusammensetzen</trd>
      </trdgrp>
      <trd lng="en">compose</trd>
      <trd lng="nl">componeren</trd>
      <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </subsnc>
  </snc>
  <snc mrk="kompon.0i.kunmeti_konsistigajn_elementoj">
    <dif>
      Kunmeti la konsistigajn elementojn de io.
    </dif>
    <ref tip="ant" cel="kompon.mal0i">mal<tld/>i</ref>
    <subsnc mrk="kompon.0i.KEM">
      <uzo tip="fak">KEM</uzo>
      <fnt><bib>PV</bib></fnt>
      <ref tip="dif" cel="kombin.0i.KEM">kombini <sncref/></ref>
      <refgrp tip="vid">
        <ref cel="sintez.0i">sintezi</ref>
      </refgrp>
      <trd lng="nl">samenstellen</trd>
    </subsnc>
    <subsnc mrk="kompon.0i.GRA">
      <uzo tip="fak">GRA</uzo>
      <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="met.kun0i.GRA">kunmeti</ref>
      <ekz>
        Esperanto konsistas nur el simplaj kaj <tld/>itaj vortoj
        <fnt><bib>PV</bib></fnt>.
      </ekz>
      <adm>
        Cxu estas aliaj ekzemploj de tio?
        <aut>MB</aut>
      </adm>
      <trdgrp lng="de">
        <trd>zusammenf&uuml;gen</trd>,
        <trd>zusammensetzen</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>combine</trd>,
        <trd>conjoin</trd>
      </trdgrp>
      <trd lng="nl">samenstellen</trd>
      <trd lng="pl">tworzy&cacute; <ind>kompozycj&eogonek;</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="kompon.0i.konsistigi">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="konsis.0igi">konsistigi</ref>
    <rim mrk="kompon.0i.konsistigi_rim">
      &Ccirc;i tiu senco aperas nerekte en <fnt><bib>PIV1</bib></fnt>
      por difini la &hcirc;emian sencon de <ctl><tld/>i</ctl>, nome
      <ctl>konsistigi miksa&jcirc;on, kombina&jcirc;on</ctl>, kio ne
      identas kun la &ccirc;i-supre donita. La sencovastigo de
      <ctl>kunmeti</ctl> al <ctl>konsistigi</ctl> estas ver&scirc;ajna,
      sed apena&ubreve; troveblas ekzemploj de &gcirc;i por la
      verbo <ctl><tld/>i</ctl> (kiu diras, ekz-e, ke sodio kaj kloro 
      <tld/>as salon?). Plie tiu senco tute ne aperas en
      la deriva&jcirc;oj <ctl>mal<tld/>i</ctl> a&ubreve;
      <ctl><tld/>a&jcirc;o</ctl>, dum &gcirc;i estas nepra por pravigi
      la sciencajn sencojn de <ctl><tld/>anto</ctl>, t.e. konsista&jcirc;o.
      Se al tio oni aldonas, ke la origina senco de <ctl><tld/>i</ctl>
      &scirc;ajnas esti <ctl>verki muzika&jcirc;on</ctl>, oni povas
      suspekti, ke la nuna situacio fontas el perforta aligo de la
      internacia vorto <ctl>komponanto</ctl> a&ubreve; <ctl>komponento</ctl>
      al la radiko <em><tld/></em>
      (vd <ref cel="kompon.0anto.rim">rimarkon</ref>).
    </rim>
  </snc>
  <trdgrp lng="cs">
    <trd>komponovat</trd>,
    <trd>napsat</trd>,
    <trd>skl&aacute;dat <klr>(hudbu)</klr></trd>,
    <trd>slo&zcaron;it</trd>,
    <trd>ztv&aacute;rnit</trd>
  </trdgrp>
  <trd lng="en">combine</trd>
  <trd lng="es">componer</trd>
  <trdgrp lng="fr">
    <trd>composer</trd>,
    <trd>agencer<klr tip="ind"> (une &oelig;uvre d'art)</klr></trd>,
    <trd>arranger</trd>,
    <trd>assembler</trd>,
    <trd>b&acirc;tir<klr tip="ind"> (une &oelig;uvre)</klr></trd>,
    <trd>combiner</trd>,
    <trd>construire<klr tip="ind"> (une &oelig;uvre)</klr></trd>,
    <trd>fa&ccedil;onner<klr tip="ind"> (une &oelig;uvre d'art)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>sklada&tcaron;</trd>,
    <trd>skomponova&tcaron;</trd>,
    <trd>zlo&zcaron;i&tcaron;</trd>,
    <trd>zostavi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kompon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika verko:
      <ekz>
        la junaj fra&ubreve;linoj &ccirc;ie ludadis <ctl>la Pri&egrave;re
        d'une vierge</ctl>, tiun sentimentalan <tld/>a&jcirc;on, kies tonoj
        melankolie kunfandi&gcirc;adis kun la lunaj radioj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kompoz.0o">kompozicio</ref>
    <ref tip="super" cel="muzik.0ajxo">muzika&jcirc;o</ref>
    <rim>
      Pli vasta difino troveblas en <fnt><bib>PIV1</bib></fnt>,
      nome <ctl>io <tld/>ita</ctl>, kio supozigas, ke ne nur
      la muzika senco eblas, sed ekzemploj de tio malmultas.
      Aliflanke, oni ne uzas <ctl><tld/>a&jcirc;o</ctl> en
      &hcirc;emio: &ccirc;i-sence oni uzu
      &leftquot;<ref cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref>&rightquot;.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Komposition</trd>,
    <trd>Tonst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">composition</trd>
  <trdgrp lng="fr">
    <trd>composition<klr tip="ind"> (musicale)</klr></trd>,
    <trd>&oelig;uvre <ind>musicale</ind></trd>
  </trdgrp>
  <trd lng="nl">compositie</trd>
  <trd lng="pl">kompozycja</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="kompon.0anto">
  <kap><tld/>anto</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="kompon.0anto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu, kiu <tld/>as muzika&jcirc;on:
      <ekz>
        la <tld/>antoj trouzas <klr>[la trukon konsistantan en tio,
        ke]</klr> oni konversacias, kantetas, krietas, bruetas kaj
        ridegas inter la kanzonoj
        <fnt>
        <aut>A. Birjulin</aut>
        <vrk>Junulo, regalu min per vermuto</vrk>
        <lok><url ref="http://esperanto.org/Ondo/R-destin.htm">LOdE
        1992:3</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        1860 &#8208; naski&gcirc;is barono Menu de M&eacute;nil
        (la <tld/>into de la muziko por <ctl>La Espero</ctl>)
        <fnt><vrk>Niaj jubileoj kaj datrevenoj en 2000</vrk><lok><url
        ref="http://sezonoj.itgo.com/H-jub200.htm">LOdE
        2000:1</url></lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">compositeur</trd>
  </snc>
  <snc mrk="kompon.0anto.KEMTEK">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Io, kio konsistigas esencan parton de substanco, ekipa&jcirc;o, ks:
      <ekz>
        la <tld/>antoj de salo <klr>(sodio kaj kloro)</klr>;
      </ekz>
      <ekz>
        la <tld/>antoj de videbla lumo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la strebo al posedo de libera marnavigado estas unu el la
        <tld/>antoj de la rusa politiko
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konsis.0ajxo">konsista&jcirc;o</ref>
    <trdgrp lng="en">
      <trd>component</trd>,
      <trd>constituent</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>composant</trd>,
      <trd>composante<klr tip="ind"> (constituant)</klr></trd>,
      <trd>constituant</trd>
    </trdgrp>
    <trd lng="pl">komponent</trd>
  </snc>
  <snc mrk="kompon.0anto.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="vektor.0o.MAT">vektoro</ref>
      <frm><k>x</k></frm>
      la&ubreve; <ref cel="baz.0o.vektoraspaco">bazo</ref>
      <frm>(<g>e</g><sub><k>i</k></sub>)</frm>)</klr>
      &Ccirc;iu el la <ref cel="koefic.0o.lineara">koeficientoj</ref>
      en la nura <ref cel="kombin.lineara0ajxo">lineara kombina&jcirc;o</ref>
      de bazvektoroj, kiu egalas al <frm><k>x</k></frm>;
      alternative: la produto de tiu koeficiento per la responda
      bazvektoro:
      <ekz>
        la <tld/>anton de <frm><k>x</k></frm> rilate al indico
        <frm><k>i</k></frm> (a&ubreve; rilate al bazvektoro
        <frm><g>e</g><sub><k>i</k></sub></frm>) oni kutime signas per
        <frm><k>x</k><sub><k>i</k></sub></frm>, tiel ke
        <frm><k>x</k> = &#8721;
        <k>x</k><sub><k>i</k></sub>&#8729;<g>e</g><sub><k>i</k></sub></frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="koordi.0o.MAT">koordinato.</ref>
    <rim mrk="kompon.0anto.rim">
      Eblas diskuti, &ccirc;u la koncerna koeficiento ne estus pli bone
      nomata koordinato, lasante al <ctl><tld/>anto</ctl> la sencon,
      kiun
      ni difinis alternativa (vd ekz-e
      <fnt><bib>MatStokTerm</bib><lok>p. 26,50</lok></fnt>, kiu difinas
      la
      nocion kiel vektoron, sed uzas &gcirc;in kiel skalaron en la difino de
      <ctl>skalara produto</ctl>).
      Se la ambigueco i&gcirc;as &gcirc;ena, eblas paroli pri
      <ctl>skalara <tld/>anto</ctl> kaj <ctl>vektora <tld/>anto</ctl>.
    </rim>
    <trd lng="de">Komponente</trd>
    <trd lng="en">component</trd>
    <trd lng="fr">composante<klr tip="ind"> (math.)</klr></trd>
    <trdgrp lng="pl">
      <trd>sk&lstroke;adowa</trd>,
      <trd>wsp&oacute;&lstroke;rz&eogonek;dna<klr tip="ind"> (wektora wzgl&eogonek;dem
      bazy)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kompon.0anto.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &Ccirc;iu el la fortoj en aro da fortoj, kompare kun ilia sumo:
      <ekz>
        la movo rezultas ne de la efiko de &ccirc;iu aparta <tld/>anto,
        sed de ilia sumo.
      </ekz>
    </dif>
    <trd lng="en">component</trd>
    <trdgrp lng="fr">
      <trd>composante<klr tip="ind"> (d'une force)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>komponenta</trd>,
    <trd>slo&zcaron;ka</trd>,
    <trd>sou&ccaron;&aacute;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bestandteil</trd>,
    <trd>Komponente</trd>
  </trdgrp>
  <trd lng="pl">sk&lstroke;adowa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komponent</trd>,
    <trd>zlo&zcaron;ka</trd>
  </trdgrp>
</drv>

<drv mrk="kompon.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Muzikisto, kiu <tld/>as:
      <ekz>
        &ccirc;iuj malbonaj poetoj kaj poetinoj, <tld/>istoj, gazetistoj
        <klr>[...]</klr> rajdas en la Novjara nokto tra la aero al la
        insulo Amager
        <fnt><bib>Fab4</bib><lok>Unua vizito</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_a;&c_z;&c_y;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>hudebn&iacute; skladatel</trd>,
    <trd>komponista</trd>
  </trdgrp>
  <trd lng="de">Komponist</trd>
  <trd lng="en">composer</trd>
  <trd lng="es">compositor</trd>
  <trd lng="fr">compositeur</trd>
  <trd lng="nl">componist</trd>
  <trd lng="pl">kompozytor</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_o;&c_z;&c_i;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">skladate&lcaron;</trd>
</drv>

<drv mrk="kompon.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi la konsistigajn elementojn:
      <ekz>
        <uzo tip="fak">GRA</uzo>
        mal<tld/>i la erojn de vorto;
      </ekz>
      <ekz>
        <uzo tip="fak">KEM</uzo>
        mal<tld/>i la molekulojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analiz.0i">analizi</ref>,
      <ref cel="er.dis0igi">diserigi</ref>
    </refgrp>
    <trd lng="de">zerlegen</trd>
    <trdgrp lng="en">
      <trd>decompound</trd>,
      <trd>decompose</trd>,
      <trd>dissect</trd>,
      <trd>resolve</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;composer</trd>,
      <trd>d&eacute;sagr&eacute;ger</trd>,
      <trd>diss&eacute;quer<klr tip="ind"> (fig.)</klr></trd>,
      <trd>disloquer</trd>,
      <trd>scinder<klr tip="ind"> (en ses composants)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ontbinden</trd>,
      <trd>ontleden</trd>,
      <trd>dissecteren</trd>,
      <trd>oplossen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dekomponowa&cacute;</trd>,
      <trd>rozk&lstroke;ada&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;<klr tip="ind"> (&c_n;&c_a; &c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_n;&c_y;&c_je; &c_ch;&c_a;&c_s;&c_t;&c_i;)</klr></trd>,
      <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_n;&c_y;&c_je; &c_ch;&c_a;&c_s;&c_t;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="kompon.mal0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 10, &leftquot;malkomponebla&rightquot;</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Esprimi ion kiel rezulton de operacio, aplikita al iuj
      <ctl>konsistaj
      eroj</ctl> de &gcirc;i:
      <ekz>
        mal<tld/>i vektoron la&ubreve; bazo <klr>(trovi &gcirc;iajn
        <tld/>antojn)</klr>;
      </ekz>
      <ekz>
        neprima entjero estas, unike, <ind>mal<tld/>ebla</ind>
        en primojn <klr>(prezentebla kiel produto de primaj
        faktoroj)</klr>;
        <trd lng="de">zerlegbar</trd>
        <trd lng="en">decomposable</trd>
        <trd lng="fr">d&eacute;composable</trd>
        <trd lng="pl">rozk&lstroke;adalny</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_i;&c_m;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        kanona <ind>mal<tld/>a&jcirc;o</ind> de bildigo <klr>(prezento de
        &gcirc;i kiel kunliga&jcirc;o de enjekcio, bijekcio kaj
        surjekcio)</klr>.
        <trd lng="de">Zerlegung</trd>
        <trd lng="en">decomposition</trd>
        <trd lng="fr">d&eacute;composition</trd>
        <trd lng="pl">rozk&lstroke;ad</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="volv.el0i.MAT">elvolvi.</ref>
    <trd lng="de">zerlegen</trd>
    <trd lng="en">decompose</trd>
    <trd lng="fr">d&eacute;composer<klr tip="ind"> (math.)</klr></trd>
    <trd lng="pl">roz&lstroke;o&zdot;y&cacute;</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">rozkl&aacute;dat</trd>
  <trdgrp lng="sk">
    <trd>rozlo&zcaron;i&tcaron;</trd>,
    <trd>rozobra&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kompon.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i mal<tld/>ita:
      <ekz>
        per la sankta nomo de Jehovo <klr>[...]</klr> la maro refluas,
        la fajro estingi&gcirc;as, la elementoj de la naturo mal<tld/>i&gcirc;as
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 20</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KEM</uzo>
        &ccirc;e la temperaturoj, aperantaj en patoj (&ccirc;irka&ubreve;
        200 gradoj), &ccirc;iuj oleoj mal<tld/>i&gcirc;as
        <fnt>
        <bib>Voja&gcirc;o</bib>
        <aut>I. Litvina trad. B. Kolker</aut>
        <vrk>Kiel kiam kaj kiom man&gcirc;i</vrk><lok>leciono 21a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;composer</ind></trd>,
    <trd>se <ind>d&eacute;sagr&eacute;ger</ind></trd>,
    <trd>se <ind>diss&eacute;quer</ind><klr tip="ind"> (fig.)</klr></trd>,
    <trd>se <ind>disloquer</ind></trd>,
    <trd>se <ind>scinder</ind><klr tip="ind"> (en ses
    composants)</klr></trd>
  </trdgrp>
  <trd lng="pl">rozk&lstroke;ada&cacute; si&eogonek;</trd>
</drv>

<drv mrk="kompon.koneksa0anto">
  <kap>koneksa <tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
      &Ccirc;iu el la <ref cel="klas.ekvivalento0o">ekvivalento-klasoj</ref>
      la&ubreve; la rilato <ctl>ekzistas &ccirc;eno liganta <frm><k>A</k></frm>
      kun <frm><k>B</k></frm></ctl>:
      <ekz>
        <ref cel="koneks.0a.grafeo">koneksa</ref> grafeo havas nur unu
        koneksan <tld/>anton.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zusammenh&auml;ngender <ind>Bestandteil</ind></trd>
  <trd lng="en">connected <ind>component</ind></trd>
  <trd lng="fr">composante connexe<klr tip="ind"> (d'un
  graphe)</klr></trd>
  <trd lng="pl">sk&lstroke;adowa sp&oacute;jna<klr tip="ind"> (grafu)</klr></trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_n;&c_a;&c_ja; <ind>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;&c_a;</ind><klr tip="ind">
  (&c_g;&c_r;&c_a;&c_f;&c_a;)</klr></trd>
</drv>
