<vortaro>
<art mrk="$Id: deuxte.xml,v 1.11 2009/06/19 05:10:00 revo Exp $">
<kap>
  <rad>de&ubreve;teri</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="deuxte.0o">
  <kap><tld/>o</kap>
  <snc mrk="deuxte.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Peza <ref tip="vid" cel="hidrog.0o">hidrogeno</ref>, kun
      atompezo 2, kies atomkerno konsistas el
      <ref tip="vid" cel="proton.0o">protono</ref> kaj
      <ref tip="vid" cel="neuxtr2.0o">ne&ubreve;trono</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_w;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_e;&c_j;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="de">Deuterium</trd>
  <trd lng="fa">&a_d;&a_w;&a_t;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">deut&eacute;rium</trd>
  <trdgrp lng="hu">
    <trd>deut&eacute;rium</trd>,
    <trd>neh&eacute;zhidrog&eacute;n</trd>
  </trdgrp>
  <trd lng="nl">deuterium</trd>
  <trd lng="pl">deuter</trd>
  <trd lng="pt">deut&eacute;rio</trd>
  <trd lng="ru">&c_d;&c_je;&c_j;&c_t;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">deut&eacute;rium</trd>
</drv>
</art>
