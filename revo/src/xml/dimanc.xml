<art mrk="$Id: dimanc.xml,v 1.32 2015/06/26 07:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>diman&ccirc;</rad>/o
</kap>

<drv mrk="dimanc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Unua <ref tip="lst" cel="tag.0o.diurn"
      lst="voko:semajntagoj" val="1">tago<sncref
      ref="tag.0o.diurn"/></ref> de la 
      <ref tip="malprt" cel="semajn.0o.KAL">semajno</ref>,
      inter sabato kaj lundo:
      <ekz>
        hodia&ubreve; estas sabato, kaj morga&ubreve; estos <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
    </dif>
    <rim> Depende de la lando la semajno komenci&gcirc;as je sabato a&ubreve;
      lundo.</rim>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_d;&c_z;&c_je;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_n;&c_je;&c_d;&c_je;&c_l;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>Sul</trd>,
    <trd>disul</trd>
  </trdgrp>
  <trd lng="cs">ned&ecaron;le</trd>
  <trd lng="ca">diumenge</trd>
  <trd lng="de">Sonntag</trd>
  <trd lng="en">Sunday</trd>
  <trd lng="es">domingo</trd>
  <trd lng="fa">&f_y;&f_k;&a_sh;&a_n;&a_b;&a_h;</trd>
  <trd lng="fr">dimanche</trd>
  <trd lng="he">&yod;&vav;&fmem; &resh;&alef;&shin;&vav;&fnun;</trd>
  <trd lng="hu">vas&aacute;rnap</trd>
  <trd lng="it">domenica</trd>
  <trd lng="id">Minggu</trd>
  <trd lng="ja">&#26085;&#26332;&#26085; 
[<ind>&#12395;&#12385;&#12424;&#12358;&#12403;</ind>]</trd>
  <trd lng="nl">zondag</trd>
  <trd lng="pl">niedziela</trd>
  <trd lng="pt">domingo</trd>
  <trd lng="ro">duminic&abreve;</trd>
  <trd 
lng="ru">&c_v;&c_o;&c_s;&c_k;&c_r;&c_je;&c_s;&c_je;&c_n;&c_mol;&c_je;</trd>
  <trd lng="sk">nede&lcaron;a</trd>
  <trd lng="sv">s&ouml;ndag</trd>
  <trdgrp lng="vo">
    <trd>sold&eacute;l<klr> [1a]</klr></trd>,
    <trd>soldel<klr> [1b]</klr></trd>,
    <trd>bal&uuml;del<klr> [3]</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>星期日 [<ind>xīng qí rì</ind>]</trd>
    <trd>周日 [<ind>zhōu rì</ind>]</trd>
    <trd>礼拜天 [<ind>lǐ bài tiān</ind>]</trd>
  </trdgrp>
</drv>
</art>
