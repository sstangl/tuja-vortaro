<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hermet.xml,v 1.20 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hermet</rad>/a <fnt>Z</fnt>
</kap>
<drv mrk="hermet.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc mrk="hermet.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="hermetik.0a.TEK">hermetika<sncref/></ref>
  </snc>
  <snc mrk="hermet.0a.malklara">
    <ref tip="dif" cel="hermetik.0a.malklara">hermetika<sncref/></ref>
    <ekz>
      kelkaj, a&ubreve; eble multaj, trovas <klr>...</klr> liajn poemojn
      malfacilaj, nekompreneblaj, <tld/>aj
      <fnt>
        <aut>J. Camacho</aut>,
        <vrk><url ref="http://www.liberafolio.org/2006/kantojdeanteo">
        Poeto &ccirc;e mar&ccirc;ando kun Anubo</url></vrk>,
        <lok>libera folio, 2008-01-26</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">herm&egrave;tic</trd>
  <trdgrp lng="cs">
    <trd>hermetický</trd>,
    <trd>neprodyšný</trd>,
    <trd>vzduchotěsný</trd>
  </trdgrp>
  <trd lng="en">hermetic</trd>
  <trd lng="fr">herm&eacute;tique</trd>
  <trd lng="it">ermetico</trd>
  <trd lng="pt">herm&eacute;tico</trd>
  <trdgrp lng="sk">
    <trd>hermetický</trd>,
    <trd>nepriedušný</trd>
  </trdgrp>
</drv>
</art>
