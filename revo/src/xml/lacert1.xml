  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_ja;&c_sh;&c_ch;&c_a;&c_r;&c_k;&c_ib;</ind> <klr
