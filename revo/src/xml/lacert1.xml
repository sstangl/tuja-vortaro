  <trd lng="de">Eidechsen<klr> (Familie)</klr></trd>
  <trd lng="hu">nyak&ouml;rv&ouml;s <ind>gy&iacute;kf&eacute;l&eacute;k</ind></trd>
  <trd lng="nl">hagedisachtigen</trd>
  <trd lng="pt">lacert&iacute;deos</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_ja;&c_shch;&c_je;&c_r;&c_i;&c_c;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
