<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trikot.xml,v 1.15 2015/10/21 15:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trikot</rad>/o
</kap>
<drv mrk="trikot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Trikita teksa&jcirc;o a&ubreve; vesto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_k;&c_o;</trd>, 
    <trd>&c_t;&c_r;&c_y;&c_k;&c_a;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="cs">trikot <klr>(l&aacute;tka)</klr></trd>
  <trdgrp lng="de">
    <trd>Trikot</trd>,
    <trd>Trikotstoff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>knit fabric</trd>,
    <trd>knit</trd>, <trd>knitwear</trd>
  </trdgrp>
  <trd lng="fr">tricot</trd>
  <trd lng="hu">trik&oacute; <klr tip="amb">(anyag &eacute;s ruha)</klr></trd>
  <trdgrp lng="it">
    <trd>canottiera</trd>,
    <trd>maglia<klr> (tessuto)</klr></trd>,
    <trd>tricot</trd>
  </trdgrp>
  <trd lng="nl">tricot</trd>
  <trd lng="pt">tric&ocirc;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_k;&c_o;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_k;&c_o;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="sk">trikot <klr>(l&aacute;tka)</klr></trd>
</drv>
</art>
