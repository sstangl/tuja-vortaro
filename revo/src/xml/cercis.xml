<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cercis.xml,v 1.7 2012/08/26 12:10:24 revo Exp $">
<kap>
  <rad>cercis</rad>/o
</kap>

<drv mrk="cercis.0o">
  <kap><tld/>o</kap>
  <snc mrk="cercis.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro de arboj kaj arbustoj el familio
      cesalpiniacoj (<trd lng="la">Cercis</trd>) kun
      gu&scirc;oj kaj korformaj folioj, el E&ubreve;razio kaj Nordameriko:
      <ekz>
        antikve la familio Tian havis <tld/>on
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 94a, volumo 3a, p.
        204a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">arbre de l'amor</trd>
  <trd lng="de">Judasbaum</trd>
  <trd lng="fa">&a_A;&a_r;&a_gh;&a_w;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>arbre de jud&eacute;e</trd>,
    <trd>gainier</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cercis<klr> (bot. - genere)</klr></trd>,
    <trd>albero di Giuda<klr> (cercis siliquastrum)</klr></trd>
  </trdgrp>
  <trd lng="pl">judaszowiec</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j; &c_b;&c_a;&c_g;&c_r;&c_ja;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_i;&c_u;&c_d;&c_i;&c_n;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>

