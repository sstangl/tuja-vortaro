<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: start.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>start</rad>/i
</kap>
<drv mrk="start.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="start.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Ekpa&scirc;i, ekkuri, ekfunkcii:
      <ekz>
        <tld/>i al kuro, al luktokonkuro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komenc.0i">komenci</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">zahájit <klr>(ofenzívu| závod| chod stroje)</klr></trd>
  <trd lng="de">starten</trd>
  <trd lng="en">start</trd>
  <trd lng="fr">d&eacute;marrer</trd>
  <trdgrp lng="hu">
    <trd>indul</trd>,
    <trd>startol</trd>
  </trdgrp>
  <trd lng="nl">starten</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozbehnúť sa</trd>,
    <trd>štartovať</trd>
  </trdgrp>
  <trd lng="sv">starta</trd>
</drv>

<drv mrk="start.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="start.0anto.KOMUNE">
    <dif>
      Tiu, kiu <tld/>as, kiu partoprenas en la starto.
    </dif>
  </snc>
  <trd lng="de">Starter</trd>
  <trd lng="en">starter</trd>
  <trd lng="hu">indul&oacute;<klr tip="ind">(sport)</klr></trd> 
  <trd lng="nl">iem. die aan de <ind>start</ind> deelneemt</trd> 
</drv>

<drv mrk="start.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="start.0igi.KOMUNE">
    <dif>
      Signali a&ubreve; ka&ubreve;zi <tld/>on:
      <ekz>
        mi <klr>...</klr> <tld/>igis la motoron, kaj veturis malrapide
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lancx.0i">lan&ccirc;i</ref>,
      <ref cel="sxalt.0i">&scirc;alti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>nastartovat</trd>,
    <trd>natáčet <klr>(motor)</klr></trd>,
    <trd>spustit</trd>
  </trdgrp>
  <trd lng="de">starten lassen</trd>
  <trd lng="en">start</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>d&eacute;marrer</ind></trd>,
    <trd>donner le <ind>d&eacute;part</ind></trd>,
    <trd>mettre en marche</trd>,
    <trd>mettre en route</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>iets <ind>starten</ind></trd>,
    <trd>in <ind>gang</ind> zetten</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naštartovať</trd>,
    <trd>spustiť motor</trd>
  </trdgrp>
</drv>
</art>
