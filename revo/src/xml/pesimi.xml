<vortaro>
<art mrk="$Id: pesimi.xml,v 1.13 2006/02/08 17:32:18 revo Exp $">
<kap>
       <ofc>2</ofc>
       <rad>pesimist</rad>/o
</kap>
<drv mrk="pesimi.0o">
       <kap>
         <tld/>o
       </kap>
       <snc>
         <dif>
           Pesimismano.
         </dif>
       </snc>
       <snc>
         <dif>
           Homo, kiu &ccirc;iam vidas nur la malbonan flankon en &ccirc;iuj
aferoj.
         </dif>
       </snc>
       <refgrp tip="sin">
         <ref cel="vid.nigre0ulo">nigrevidulo</ref>
       </refgrp>
       <trd lng="be">&c_p;&c_e;&c_s;&c_y;&c_m;&c_ib;&c_s;&c_t;</trd>
       <trd lng="de">Pessimist</trd>
       <trd lng="es">pesimista</trd>
       <trdgrp lng="fr">
         <trd>d&eacute;faitiste</trd>,
         <trd>pessimiste</trd>
       </trdgrp>
       <trd lng="en">pessimist</trd>
       <trdgrp lng="he">
         <trd>&pe;&samekh;&yod;&mem;&yod;&samekh;&tet;</trd>,
         <trd>&resh;&vav;&alef;&he; &shin;&het;&vav;&resh;&vav;&tav;</trd>
       </trdgrp>
       <trdgrp lng="hu">
         <trd>pesszimista</trd>,
         <trd>bor&uacute;l&aacute;t&oacute;</trd>
       </trdgrp>
       <trdgrp lng="nl">
         <trd>pessimist</trd>,
         <trd>zwartkijker</trd>
       </trdgrp>
  <trdgrp lng="pt">
   <trd>pessimista</trd>
  </trdgrp>
       <trd lng="ru">&c_p;&c_je;&c_s;&c_s;&c_i;&c_m;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
