<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mizer.xml,v 1.22 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mizer</rad>/o
</kap>
<drv mrk="mizer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mizer.0o.POL">
    <dif>
      Da&ubreve;ra granda malri&ccirc;eco a&ubreve; malfeli&ccirc;o,
      inda je kompato:
      <ekz>
        kiu metion disponas, <tld/>on ne konas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        el la <tld/>o oni devas fari virton
        <klr>(rezignacie akcepti tion, kion oni ne povas forigi)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu havas sian ka&scirc;itan <tld/>on
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;i timas <klr>...</klr> enfali en <tld/>on kune kun siaj
        infanoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        Zamenhof dolore sentis la <tld/>on de lingva diverseco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nigr.0a">nigra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_ch;&c_a;</trd>,
    <trd>&c_n;&c_e;&c_n;&c_d;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>dienez</trd>,
    <trd>mizer<klr tip="ind"> (reuzeudigezh)</klr></trd>,
    <trd>reuzeudigezh</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bída</trd>,
    <trd>nouze</trd>,
    <trd>tíseň</trd>,
    <trd>ubohost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Elend</trd>,
    <trd>Jammer</trd>,
    <trd>Not</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>misery</trd>,
    <trd>wretchedness</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>paup&eacute;risme</trd>,
    <trd>mis&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&vav;&mem;&lamed;&vav;&vav;&tav;</trd>,
    <trd>&mem;&samekh;&kaf;&nun;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomor</trd>,
    <trd>nyomor&uacute;s&aacute;g</trd>,
    <trd>sz&uuml;ks&eacute;g</trd>,
    <trd>miz&eacute;ria</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ellende</trd>,
    <trd>nood</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>n&eogonek;dza</trd>,
    <trd>bieda</trd>,
    <trd>ub&oacute;stwo</trd>
  </trdgrp>
  <trd lng="pt">miseric&oacute;rdia</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;<klr tip="ind">
    (&c_n;&c_i;&c_shch;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bieda</trd>,
    <trd>psota</trd>,
    <trd>tvŕdza</trd>
  </trdgrp>
</drv>

<drv mrk="mizer.0a">
  <kap><tld/>a</kap>
  <snc mrk="mizer.0a.POL">
    <dif>
      Kompatinda pro sia malri&ccirc;eco, malfeli&ccirc;o a&ubreve;
      malbonstato:
      <ekz>
        <tld/>aj orfaj knaboj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mizer.0a.mizeraspekta">
    <dif>
      Malbonstata, malbonaspekta:
      <ekz>
        Malanja devis akiradi la <tld/>an &ccirc;iutagan nutra&jcirc;on
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo, rakonto
        de A. Edelmann, tradukis I. Kaminski</lok></fnt>;
      </ekz>
      <ekz>
        fari mienon fieran al ludo (sorto) <tld/>a<fnt>Z</fnt>
        <klr>(&scirc;ajnigi sin kontenta je la
neevitebla&jcirc;o)</klr>;
      </ekz>
      <ekz>
        kelkaj <tld/>aj arboj vegetas;
      </ekz>
      <ekz>
        ludi <tld/>an rolon<fnt>Z</fnt>
        (<ref tip="vid" cel="hund.0a">hunda</ref>);
      </ekz>
      <ekz>
        modesta, e&ccirc; <tld/>easpekta bro&scirc;uro;
      </ekz>
      <ekz>
        <ind><tld/>a <klr>(mal&scirc;atinda)</klr>
        kalumnio</ind><fnt>Z</fnt>.
        <trd lng="hu">nyomorult r&aacute;galom</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_b;&c_r;&c_a;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="br">
    <trd>ezhommek</trd>,
    <trd>reuzeudik</trd>,
    <trd>tavantek</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bídný</trd>,
    <trd>darebný</trd>,
    <trd>mizerný</trd>,
    <trd>nuzný</trd>,
    <trd>ubohý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>elend</trd>,
    <trd>elendig</trd>,
    <trd>j&auml;mmerlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>miserable</trd>,
    <trd>wretched</trd>
  </trdgrp>
  <trd lng="fr">mis&eacute;rable<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="he">
    <trd>&alef;&vav;&mem;&lamed;&lamed;</trd>,
    <trd>&mem;&samekh;&kaf;&fnun;</trd>,
    <trd>&ayin;&lamed;&vav;&bet;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomorult</trd>,
    <trd>nyomor&uacute;s&aacute;gos</trd>,
    <trd>szeg&eacute;ny</trd>,
    <trd>keserves</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ellendig</trd>,
    <trd>armzalig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mizerny</trd>,
    <trd>n&eogonek;dzny</trd>,
    <trd>biedny</trd>,
    <trd>ubogi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>biedny</trd>,
    <trd>úbohý</trd>
  </trdgrp>
</drv>

<drv mrk="mizer.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mizer.0i.POL">
    <dif>
      Esti en <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam sako <tld/>as <klr>(= estas senmona)</klr> amo malaperas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
      <ekz>
        <tld/>u kaj mal&gcirc;oju kaj ploru; via ridado turni&gcirc;u en
        ploron
        <fnt><bib>NT</bib>, <lok>&Jak; 4:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beza&ntilde; en <ind>dienez</ind></trd>,
    <trd>mizeri&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>být ubohý</trd>,
    <trd>trpět bídu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>elendig sein</trd>,
    <trd><klr>in </klr>Not sein</trd>,
    <trd>j&auml;mmerlich gehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be <ind>miserable</ind></trd>,
    <trd>be <ind>wretched</ind></trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre dans la <ind>mis&egrave;re</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&alef;&vav;&mem;&lamed;&lamed;
    </ind></trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&mem;&samekh;&kaf;&fnun;</ind>
    </trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&ayin;&lamed;&vav;&bet;</ind>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomorog</trd>,
    <trd>sz&uuml;ks&eacute;get szenved</trd>
  </trdgrp>
  <trd lng="nl">in <ind>ellende</ind> verkeren</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; w n&eogonek;dzy</trd>,
    <trd>znajdowa&cacute; si&eogonek; w n&eogonek;dzy</trd>,
    <trd>cierpie&cacute; n&eogonek;dz&eogonek;</trd>,
    <trd>znajdowa&cacute; si&eogonek; w biedzie</trd>,
    <trd>biedowa&cacute;</trd>,
    <trd>klepa&cacute; bied&eogonek;</trd>,
    <trd>trwa&cacute; w ub&oacute;stwie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <klr tip="ind">
    (&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>biediť</trd>,
    <trd>trieť biedu</trd>
  </trdgrp>
</drv>

<drv mrk="mizer.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Trafi en malri&ccirc;on, en <tld/>on:
      <ekz>
        por ke ne <tld/>i&gcirc;u vi kaj via domo
        <fnt><bib>MT</bib>, <lok>&Gen; 45:11</lok></fnt>.
      </ekz>
    </dif>
  </snc>          
  <trdgrp lng="br">
    <trd>kouezha&ntilde; an <ind>dienez</ind> war ub.</trd>,
    <trd>mont da <ind>reuzeudig</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>become <ind>miserable</ind></trd>,
    <trd>become <ind>wretched</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre dans le <ind>besoin</ind></trd>,
    <trd>tomber dans la <ind>mis&egrave;re</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&he;&tav;&alef;&mem;&lamed;&lamed;</ind>
  </trd>
  <trd lng="nl">in <ind>ellende</ind> geraken</trd>
  <trdgrp lng="pl">
    <trd>zmizernie&cacute;</trd>,
    <trd>zbiednie&cacute;</trd>,
    <trd>wpa&sacute;&cacute; w <ind>bied&eogonek;</ind></trd>,
    <trd>popa&sacute;&cacute; w <ind>bied&eogonek;</ind></trd>,
    <trd>zn&eogonek;dznie&cacute;</trd>,
    <trd>wpa&sacute;&cacute; w <ind>n&eogonek;dz&eogonek;</ind></trd>,
    <trd>popa&sacute;&cacute; w <ind>n&eogonek;dz&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_n;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="mizer.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="mizer.0ulo.POL">
    <dif>
      Homo, kiu spertas <tld/>on:
      <ekz>
        &scirc;i renkontis <tld/>ulon, kiu diris: &leftquot;Donu al mi
        ion por man&gcirc;i&rightquot;
        <fnt><bib>ElektFab</bib>, <lok>steloj talaroj</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hav.sen0ulo">senhavulo</ref>
  </snc>
  <trdgrp lng="br">
    <trd>ezhommeg</trd>,
    <trd>reuzeudig</trd>,
    <trd>tavanteg</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chudák</trd>,
    <trd>ubožák</trd>
  </trdgrp>
  <trd lng="en">wretch</trd>
  <trdgrp lng="fr">
    <trd>mis&eacute;reux<klr tip="amb"> (subst.)</klr></trd>,
    <trd>n&eacute;cessiteux<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&vav;&mem;&lamed;&lamed;</trd>,
    <trd>&mem;&samekh;&kaf;&fnun;</trd>
  </trdgrp>
  <trd lng="nl">ellendeling</trd>
  <trdgrp lng="pl">
    <trd>biedak</trd>,
    <trd>n&eogonek;dzarz</trd>,
    <trd>n&eogonek;dznik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_i;&c_j;<klr tip="amb">
    (&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_n;&c_je;&c_i;&c_m;&c_u;&c_shch;&c_i;&c_j;<klr tip="amb">
    (&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chudák</trd>,
    <trd>lazar</trd>,
    <trd>úbožiak</trd>
  </trdgrp>
</drv>
</art>
