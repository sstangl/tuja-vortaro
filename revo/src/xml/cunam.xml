<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cunam.xml,v 1.10 2015/07/04 08:37:32 revo Exp $">
<kap><rad>cunam</rad>/o</kap>
<drv mrk="cunam.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MET</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Maraj ondegoj, &gcirc;is 50 m altaj, precipe tiuj esti&gcirc;antaj
      sekve
      de marfunda sismo, erupcio a&ubreve; &scirc;ovo de la tektonaj
      platoj,
      kaj ka&ubreve;zantaj grandajn detruojn en la apudbordaj regionoj
      de
      la seka tero.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cunami</trd>,
    <trd>tsunami</trd>,
    <trd>tunami</trd>
  </trdgrp>
  <trd lng="de">Tsunami</trd>
  <trd lng="en">tsunami</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_l;&a_r;&a_z;&a_h;</trd>,
    <trd>&a_gh;&a_r;&f_y;&a_A;&a_l;&a_h;</trd>,
    <trd>&a_s;&a_w;&a_n;&a_A;&a_m;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">tsunami</trd>
  <trd lng="hu">sz&ouml;k&odblac;&aacute;r</trd>
  <trd lng="it">maremoto</trd>
  <trdgrp lng="nl">
    <trd>tsunami</trd>,
    <trd>vloedgolf</trd>
  </trdgrp>
  <trd lng="pl">tsunami</trd>
  <trd lng="ru">&c_c;&c_u;&c_n;&c_a;&c_m;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>cunami</trd>,
    <trd>tsunami</trd>
  </trdgrp>
</drv>
</art>
