<art mrk="$Id: kolimb1.xml,v 1.10 2015/10/16 14:10:23 revo Exp $">
<kap>
  <rad>kolimbed</rad>/oj
</kap>
<drv mrk="kolimb1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="kolimb.0oformaj">kolimboformaj
      birdoj</ref> (<trd lng="la">Gaviidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_g;&c_a;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Seetaucher<klr tip="ind"> (Familie)</klr></trd>
  <trdgrp lng="en">
    <trd>Gaviidae</trd>,
    <trd>loons <klr>(usone)</klr></trd>, <trd>divers
    <klr>(brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">gaviid&eacute;s</trd>
  <trd lng="nl">duikers<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">colimb&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;&c_r;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
