<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hun.xml,v 1.14 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hun</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hun.0o">
  <kap><tld/>o</kap>
  <snc mrk="hun.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de nomada popolo devena de orienta Azio,
      en la 5a jarcento sub Attila invadinta E&ubreve;ropon.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>Hun</trd>,
    <trd>Hunov√©</trd>
  </trdgrp>
  <trd lng="de">Hunne</trd>
  <trd lng="el">&omikron;&ypsilon_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="es">huno</trd>
  <trd lng="fr">hun</trd>
  <trd lng="hu">hun</trd>
  <trd lng="it">unno</trd>
  <trd lng="nl">Hun</trd>
  <trd lng="pt">huno</trd>
  <trd lng="ru">&c_g;&c_u;&c_n;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>Hun</trd>,
    <trd>Huni</trd>
  </trdgrp>
</drv>
</art>

