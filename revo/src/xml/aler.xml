<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aler.xml,v 1.3 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>aler</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aler.0o">
  <kap><tld/>o</kap>
  <snc mrk="aler.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <ref tip="dif" cel="markez.0o.ARKI">markezo</ref>
      <ekz>
        mi trovis malanta&ubreve;an pordegon el la korto,
        super kiu estis <tld/>o
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        papago vokas sub la <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 78a, volumo 2a, p.
        654a</lok></fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>stříška nad oknem</trd>,
    <trd>stříška nade dveřmi</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>auvent</trd>,
    <trd>marquise</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dverami</trd>,
    <trd>strieška nad oknom</trd>
  </trdgrp>
</drv>
</art>

