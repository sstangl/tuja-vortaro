<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: falk.xml,v 1.21 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>falk</rad>/o
</kap>
<drv mrk="falk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="falked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Falco</trd>), taga rabobirdo, dresata
      por &ccirc;asi malgrandajn bestojn:
      <ekz>
        la <tld/>o fari&gcirc;is simbolo de medio-protektado en
        Japanio<fnt><aut>Hori Yasuo</aut>: <vrk>K-riz-kampoj</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_k;&c_a;&c_l;</trd>
  <trd lng="bg">&c_s;&c_o;&c_k;&c_o;&c_l;</trd>
  <trd lng="br">falc'hun</trd>
  <trdgrp lng="ca">
    <trd>falc&oacute;</trd>,
    <trd>xoriguer</trd>,
    <trd>esmirla</trd>
  </trdgrp>
  <trd lng="cs">sokol</trd>
  <trd lng="de">Falke</trd>
  <trd lng="en">falcon</trd>
  <trd lng="es">halco&oacute;n</trd>
  <trd lng="fr">faucon</trd>
  <trd lng="hu">s&oacute;lyom</trd>
  <trd lng="nl">valk</trd>
  <trd lng="pl">sok&oacute;&lstroke;</trd>
  <trd lng="pt">falc&atilde;o</trd>
  <trd lng="ru">&c_s;&c_o;&c_k;&c_o;&c_l;</trd>
  <trd lng="sk">sokol</trd>
</drv>
</art>
