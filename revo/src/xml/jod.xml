<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jod.xml,v 1.24 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>jod</rad>/o
</kap>
<drv mrk="jod.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">I</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="53">elemento</ref> kun
      atomnumero 53, atompezo 126,9044; 
      <ref tip="super" cel="haloge.0o">halogeno</ref>, 
      havanta formon de folietoj blue grizaj kun metala brilo, 
      uzata en medicino kaj fotografado: 
      <ekz><tld/>a tinkturo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_jo;&c_d;</trd>
  <trd lng="bg">&c_j;&c_o;&c_d;</trd>
  <trd lng="br">iod</trd>
  <trd lng="ca">iode</trd>
  <trdgrp lng="cs">
    <trd>jod</trd>,
    <trd>jód</trd>
  </trdgrp>
  <trd lng="de">Jod</trd>
  <trd lng="el">&jota;&omega_ton;&delta;&jota;&omikron;</trd>
  <trd lng="en">iodine</trd>
  <trd lng="es">yodo</trd>
  <trd lng="fr">iode</trd>
  <trd lng="hu">j&oacute;d</trd>
  <trd lng="id">iodin</trd>
  <trdgrp lng="it">
     <trd>iodio</trd>,
     <trd>iodo</trd>
  </trdgrp>
  <trd lng="la">Jodum</trd>
  <trdgrp lng="nl">
     <trd>jood<klr tip="ind"> (jodium)</klr></trd>,
     <trd>jodium</trd>
  </trdgrp>
  <trd lng="pl">jod</trd>
  <trd lng="pt">iodo</trd>
  <trd lng="ru">&c_j;&c_o;&c_d;</trd>
  <trd lng="sk">jód</trd>
  <trd lng="sv">jod</trd>
</drv>
</art>
