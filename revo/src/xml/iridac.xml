<vortaro>
<art mrk="$Id: iridac.xml,v 1.9 2013/08/25 14:10:27 revo Exp $">
<kap>
  <rad>iridac</rad>/oj
</kap>
<drv mrk="iridac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Iridaceae</trd>).
    </dif>
  </snc>
  <trd lng="ca">irid&agrave;cies</trd>
  <trd lng="de">Schwertliliengew&auml;chse</trd>
  <trd lng="fr">iridac&eacute;es</trd>
  <trd lng="hu">n&odblac;sziromf&eacute;l&eacute;k</trd>
  <trd lng="nl">lissenfamilie</trd>
  <trd lng="pt">irid&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_a;&c_t;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_i;&c_r;&c_i;&c_s;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
