<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kortizon.xml,v 1.6 2009/07/03 16:30:41 revo Exp $">
<kap>
  <rad>kortizon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kortizon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kortizon.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Kortikosurrena <ref tip="super"
      cel="hormon.0o.FIZL">hormono</ref>,
      kontra&ubreve;inflama.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_t;&c_y;&c_z;&c_o;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Kortison</trd>,
      <trd>Cortison</trd>
    </trdgrp>
    <trd lng="fr">cortisone</trd>
    <trd lng="hu">kortizon</trd>
    <trd lng="nl">cortisone</trd>
    <trd lng="pt">cortisona</trd>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_t;&c_i;&c_z;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
