<art mrk="$Id: buf.xml,v 1.29 2015/10/06 12:10:19 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>buf</rad>/o
</kap>
<drv mrk="buf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="amfibi.0o" lst="voko:amfibioj">Amfibio</ref>,
      senvostulo kun horizontalaj pupiloj kaj veruka ha&ubreve;to;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la ordo <ref
      tip="malprt"
      cel="bufed1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Bufo</trd>).
    </dif>
  </snc>                    
  <ref tip="vid" cel="ran.0o">rano</ref>
  
  <trd lng="be">&c_r;&c_a;&c_p;&c_u;&c_h;&c_a;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a; <ind>&c_zh;&c_a;&c_b;&c_a;</ind></trd>
  <trd lng="br">touseg</trd>
  <trd lng="de">Kr&ouml;te</trd>
  <trd lng="en">toad</trd>
  <trd lng="es">sapo</trd>
  <trd lng="fr">crapaud</trd>
  <trd lng="he">&qof;&resh;&pe;&dalet;</trd>
  <trdgrp lng="hu">
    <trd>varangy</trd>,
    <trd>varangyos b&eacute;ka</trd></trdgrp>
  <trd lng="id">kodok</trd>
  <trdgrp lng="it">
    <trd>bufo</trd>,
    <trd>rospo</trd></trdgrp>
  <trd lng="nl">pad</trd>
  <trd lng="pl">ropucha</trd>
  <trd lng="pt">sapo</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_a;</trd>
  <trd lng="sv">padda</trd>
  <trd lng="sk">ropucha</trd>
  <trd lng="cs">ropucha</trd>
</drv>

</art>
