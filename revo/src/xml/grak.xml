<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grak.xml,v 1.18 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>grak</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="grak.0">
  <kap><tld/></kap>
  <snc mrk="grak.0.SON">
    <dif>
      Akra <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      de korvo aux simila sono:
      <ekz>
        korniko flugis super lia kapo, kaj 
        <tld/>is: <ctl><tld/>-<tld/>-<tld/>-stul&#173;tuleto</ctl>
        <fnt>
          <aut>trad. A. M'Callum</aut>,
          <vrk><url ref="http://www.literaturo.dk/muraho.htm">
          Mura&hcirc;o kaj Mina&hcirc;o</url></vrk>,
          <lok>The Esperanto Monthly, 1919-09 (8)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="grak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grak.0i.korve">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref
      tip="vid" cel="korv.0o.genro">korvo</ref>.
    </dif>
    <trd lng="ca">grallar</trd>
    <trd lng="de">kr&auml;chzen</trd>
    <trd lng="en">caw</trd>
    <trd lng="es">graznar</trd>
    <trd lng="fr">croasser</trd>
    <trd lng="hu">k&aacute;rog</trd>
    <trd lng="nl">krassen<klr tip="ind"> (v.kraai of raaf)</klr></trd>
    <trdgrp lng="pt">
     <trd>grasnar<klr tip="amb"> (o corvo)</klr></trd>, 
     <trd>crocitar</trd> 
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="grak.0i.home">
    <dif>
      Estigi ra&ubreve;kan bruon en la gor&gcirc;o, eligante mukon,
      <ref tip="vid" cel="vocx.klarigila0on.KOMUNE">klarigi
      la vo&ccirc;on</ref>.
    </dif>
    <trdgrp lng="ca">
      <trd>gargamellejar</trd>,
      <trd>escurar-se el coll</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sich <ind>r&auml;uspern</ind></trd>,
      <trd>heiser sprechen</trd>
    </trdgrp>
    <trd lng="es">tener <ind>carraspera</ind></trd>
    <trdgrp lng="en">
      <trd>croak</trd>,
      <trd>rasp</trd>
    </trdgrp>
    <trd lng="fr">se <ind>racler</ind> la gorge</trd>
    <trd lng="hu">kr&aacute;kog</trd>
    <trd lng="nl">rochelen</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_a;&c_sh;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chrchlat</trd>,
    <trd>odkašlávat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chrchlať</trd>,
    <trd>odkašlávať</trd>
  </trdgrp>
</drv>
</art>
