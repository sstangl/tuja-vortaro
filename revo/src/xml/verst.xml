<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: verst.xml,v 1.20 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>verst</rad>/o
</kap>
<drv mrk="verst.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rusa <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">mezurunuo</ref> de
      vojlongo valoranta 1067 metrojn, egala al 1500 <ref tip="sub"
      cel="arsxin.0o">ar&scirc;inoj</ref>:
      <ekz>
        la kale&scirc;o veturis ne malpli ol duonon de <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 3a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_s;&c_t;&c_a;</trd>
  <trd lng="bg">&c_v;&c_je;&c_r;&c_s;&c_t;&c_a;</trd>
  <trd lng="ca">versta</trd>
  <trd lng="cs">versta</trd>
  <trd lng="de">Werst</trd>
  <trd lng="en">verst</trd>
  <trd lng="es">verst&aacute;</trd>
  <trd lng="fa">&a_w;&a_kasra;&a_r;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>verste</trd>
  </trdgrp>
  <trd lng="hu">verszta</trd>
  <trd lng="nl">werst</trd>
  <trd lng="pl">wiorsta</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_s;&c_t;&c_a;</trd>
  <trd lng="sk">versta</trd>
</drv>
</art>
