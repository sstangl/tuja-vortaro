<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: zenit.xml,v 1.26 2015/07/04 08:37:46 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>zenit</rad>/o
</kap>
<drv mrk="zenit.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="zenit.0o.AST">
     <uzo tip="fak">AST</uzo>
     <dif>
       Punkto en kiu la vertikalo de iu loko renkontas la &ccirc;ielan
       imagan
       sferon:
       <ekz>
         la tagmeza suno fajre brulis de l' <tld/>o.
       </ekz>
     </dif>
     <ref tip="ant" cel="nadir.0o">nadiro</ref>
     <trd lng="nl">zenith</trd>
     <trd lng="pl">zenit</trd>
     <trd lng="pt">z&ecirc;nite</trd>
   </snc>
   <snc mrk="zenit.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <ref tip="dif" cel="kulmin.0o.FIG">kulmino <sncref/></ref>
     <ref tip="sin" cel="apoge.0o.FIG">apogeo <sncref/></ref>
     <ekz>
       lia famo estas &ccirc;e la <tld/>o;
     </ekz>
     <ekz>
       la entuziasmo atingis la <tld/>on<fnt>B</fnt>.
     </ekz>
     <trd lng="ca">zenit</trd>
     <trd lng="nl">hoogtepunt</trd>
     <trd lng="pl">zenit</trd>
     <trdgrp lng="pt">
      <trd>auge</trd>,
      <trd>apogeu</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_z;&c_e;&c_n;&c_ib;&c_t;</trd>
  <trd lng="ca">zenit</trd>
  <trdgrp lng="cs">
    <trd>nadhlavník</trd>,
    <trd>zenit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Zenit</trd>,
     <trd>Scheitelpunkt</trd>
   </trdgrp>
  <trd lng="el">&zeta;&epsilon;&ny;&jota_ton;&theta;</trd>
  <trd lng="en">zenith</trd>
  <trdgrp lng="es">
     <trd>cenit</trd>, 
     <trd>zenit</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&a_s;&a_r;&a_s;&a_w;</trd>,
     <trd>&a_s;&a_m;&a_t;&zwnj;&a_A;&a_l;&a_r;&a_A_hamza_sure;&a_s;</trd>
   </trdgrp>
  <trd lng="fr">z&eacute;nith</trd>
  <trdgrp lng="hu">
     <trd>zenit</trd>,
     <trd>tet&odblac;pont</trd>,
     <trd>cs&uacute;cspont</trd>
   </trdgrp>
  <trd lng="it">zenit</trd>
  <trd lng="ru">&c_z;&c_je;&c_n;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>nadhlavník</trd>,
    <trd>najvyšší bod</trd>,
    <trd>vrchol</trd>,
    <trd>zenit</trd>
  </trdgrp>
  <trd lng="sv">zenit</trd>
</drv>
</art>
