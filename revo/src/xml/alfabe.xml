<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: alfabe.xml,v 1.32 2015/07/04 08:37:30 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>alfabet</rad>/o
    </kap>

    <drv mrk="alfabe.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">GRA</uzo>
        <dif>
          Listo da &ccirc;iuj <ref tip="prt" cel="liter.0o">literoj</ref>
          de iu lingvo, aran&gcirc;ita la&ubreve; norma ordo:
          <ekz>
            la Esperanta <tld/>o enhavas 28 literojn, de A &gcirc;is Z;
          </ekz>
          <ekz>
          la literojn de nia <tld/>o mi proponas nomi en la sekvanta
          maniero: a, bo, co, &ccirc;o, do, e, fo, go, &gcirc;o, ho, &hcirc;o, i, jo ,&jcirc;o,
          ko, lo, mo, no, o, po, ro, so, &scirc;o, to, u, &ubreve;o, vo,
          zo<fnt><bib>LR</bib></fnt>;
          </ekz>
          <ekz>
            <ind>li neniam venkis la <tld/>on</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trdgrp lng="de">
              <trd>er ist niemals &uuml;ber die <ind>Grundlagen</ind> hinaus
              gekommen</trd>,
              <trd>er ist niemals &uuml;ber das <ind>Abc</ind> hinaus
              gekommen</trd>
            </trdgrp>
            <trdgrp lng="fr">
              <trd>il ne <ind>sait</ind> ni a ni b</trd>,
              <trd>c'est un <ind>ignorant</ind></trd>
            </trdgrp>
          </ekz><ekz>
            <ind>li &scirc;vitas ankora&ubreve; super la <tld/>o</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trdgrp lng="de">
               <trd>er lernt noch die <ind>Grundlagen</ind></trd>,
               <trd>er kennt noch nicht einmal die
               <ind>einfachsten</ind> Dinge</trd>
            </trdgrp>
            <trdgrp lng="fr">
              <trd>il ne <ind>sait</ind> ni a ni b</trd>,
              <trd>c'est un <ind>ignorant</ind></trd>
            </trdgrp>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="aboc.0o.alfabeto">aboco<sncref/></ref>.
        </refgrp>        
      </snc>
  <trdgrp lng="be">
        <trd>&c_a;&c_l;&c_mol;&c_f;&c_a;&c_b;&c_e;&c_t;</trd>,
        <trd>&c_a;&c_b;&c_e;&c_c;&c_e;&c_d;&c_a;</trd>
      </trdgrp>
  <trd lng="br">lizherenneg</trd>
  <trdgrp lng="cs">
    <trd>abeceda</trd>,
    <trd>alfabeta</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Alphabet</trd>,
        <trd>Alfabet</trd>,
        <trd>Abc</trd>,
        <trd>Buchstabenfolge</trd>
      </trdgrp>
  <trd lng="en">alphabet</trd>
  <trd lng="es">alfabeto</trd>
  <trd lng="fr">alphabet</trd>
  <trdgrp lng="hu">
        <trd>&aacute;b&eacute;c&eacute;</trd>,
        <trd>alfab&eacute;tum</trd>
      </trdgrp>
  <trd lng="nl">alfabet</trd>
  <trd lng="pt">alfabeto</trd>
  <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;</trd>
  <trd lng="sk">abeceda</trd>
  <trd lng="sv">alfabet</trd>
</drv>

    <drv mrk="alfabe.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            por reeldono estus bonvena <ind><tld/>a indekso</ind>
            je la fino de la teksto
            <fnt><aut>Walter Klag</aut>:
              <vrk>Facile legebla (Harald Schicke, La korpo de la
              homo)</vrk>
              <bib>Monato</bib><lok>jaro 1994a, numero 2a, p.
              22a</lok></fnt>;
            <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;&c_n;&c_y;&c_j;
              &c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
          </ekz><ekz>
            jen la respondoj (en <ind><tld/>a ordo</ind> de la &ccirc;efnomoj)
            <fnt><vrk>Gravaj opinioj</vrk><bib>Monato</bib>
              <lok>jaro 2004a, numero 3a, p. 7a</lok></fnt>.
            <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;&c_n;&c_y;&c_j;
              &c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
          </ekz>
        </dif>
      </snc>
  <trd lng="cs">abecední</trd>
  <trd lng="fr">alphab&eacute;tique</trd>
  <trdgrp lng="hu">
        <trd>bet&udblac;rendes</trd>,
        <trd>&aacute;b&eacute;c&eacute;-</trd>
      </trdgrp>
  <trd lng="pt">alfab&eacute;tico</trd>
  <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">abecedný</trd>
</drv>

    <drv mrk="alfabe.0igi">
      <kap><tld/>igi</kap>
      <snc mrk="alfabe.0igi.fari0a">
        <dif>
          Fari ion <tld/>a, igi <tld/>a:
        </dif>
        <subsnc mrk="alfabe.0igi.arangxi">
          <dif>
            Aran&gcirc;i la&ubreve; la ordo <tld/>a:
            <ekz>
              <tld/>igi liston;
            </ekz><ekz>
              tiel en la germana, ekzemple, <em>&auml;</em> estas <tld/>igata
              kvaza&ubreve; &gcirc;i estus simpla <em>a</em>, dum en la sveda &gcirc;i
              vici&gcirc;as kiel aparta litero je la fino de la <tld/>o
              <fnt>
                <aut>J.C.Wells</aut>:
                <vrk>Supersignoj, trasignoj kaj subsignoj</vrk>.
                <lok><url
 ref="http://www.uea.org/cgi/m.cgi?m=ghu;d=kongresoj/1996/supersignoj.html"
                    >Internacia Kongresa Universitato, Prago 1996 07
                    26</url>
                </lok>
              </fnt>;
            </ekz><ekz>
              &ccirc;u tiam ni <tld/>igu re&gcirc;ojn anta&ubreve; la litero 'A'
              (&ccirc;ar mankas familia nomo), &ccirc;u la&ubreve; ilia persona nomo,
              a&ubreve; &ccirc;u &ccirc;iujn sub 'R' <klr>...</klr>?
              <fnt><vrk><url
                    ref="http://eo.wikipedia.org/wiki/Vikipedio_diskuto:Nomoj_de_titoloj"
                    >Vikipedio-diskuto:Nomoj de
                    titoloj</url></vrk></fnt>.
            </ekz></dif>
            <trdgrp lng="hu">
              <trd>bet&udblac;rendbe szed</trd>,
              <trd>&aacute;b&eacute;c&eacute;rendbe szed</trd>
            </trdgrp>
            <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_p;&c_o;
            &c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;&c_u;</trd>
        </subsnc>
        <subsnc mrk="alfabe.0igi.literigi">
          <dif>
            Transformi skribon ideograman en skribon <tld/>an (per
            kelkdeko da literoj):
            <ekz>
              &leftquot;<tld lit="A"/>igi la japanan&rightquot;
              <fnt>
                <aut>&Ccirc;e&hcirc;i&gcirc;u &Scirc;a&ubreve;ma&jcirc;o</aut>:
                <lok>Artikoltitolo en &leftquot;Monato&rightquot;, 2001:3, p. 4a.</lok>
              </fnt>;
            </ekz><ekz>
              kvankam la movado por Esperanto tu&scirc;is plurajn gravajn
              intelektajn fluojn de la dudeka jarcento (de
              strukturismo &gcirc;is la movado por <tld/>igi la &ccirc;inan
              lingvon), &gcirc;i ne sukcesis havigi al si multan
              internacian influon
              <fnt>
                <aut>Humphrey Tonkin</aut>:
                <vrk>Esperanto: Lingvo preta por la internacia
                  vivo</vrk>.
                <lok><url
                    ref="http://www.espero.com.cn/z-uk-2004/04z-05f/04z-uk-05-1.htm"
                    >Kontribuo al la 89-a Universala Kongreso de
                    Esperanto kaj la 3-a Nitobe-Simpozio.
                    Pekino, julio 2004</url></lok>
              </fnt>.
            </ekz>
          </dif>
          <trd lng="hu">latinbet&udblac;sre &aacute;t&iacute;r</trd>
          <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;&c_t;&c_i; &c_n;&c_a;
            &c_b;&c_u;&c_k;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;</trd>
        </subsnc>
      </snc>

      <snc mrk="alfabe.0igi.malanalfabetigi">
        <uzo tip="stl">EVI</uzo>
        <dif>
          Malanalfabetigi, legopovigi, instrui al iu legi (kaj, kutime,
          anka&ubreve; skribi):
          <ekz>
            kursoj por <tld/>igi en la nacia lingvo afganajn
            analfabetajn 
            rifu&gcirc;intojn
            <fnt><aut>Renato Corsetti</aut><vrk>Lingvo de paco</vrk>
              <bib>LOdE</bib><lok>jaro 2002a, numero 1a</lok></fnt>.
          </ekz>
        </dif>
        <rim>
          Tiu senco estas plurrilate malkonvena al la vorto <tld/>igi.
          Unue, &gcirc;i malkonvenas al la senco de la sufikso -ig: la homo
          ne i&gcirc;as &leftquot;<tld/>a&rightquot;.  En analogia okazo oni ne diras
          <ctl>evangeliigi</ctl> por la senco <ctl>prediki
          evangelion</ctl>,
          sed uzas apartan radikvorton <ctl>evangelizi</ctl>.

          Due, e&ccirc; se akcepti la neekzistantan sencon
          <ctl>instrui</ctl> &ccirc;e -ig, anka&ubreve; la radiko <tld/>-
          malkonvenas por esprimi la ideon pri
          <ref cel="leg.0opova">legopovo</ref>.  Oni povas
          scipovi legi sen scii la <tld/>on; kaj e&ccirc; plej bona scio de
          la latina-franca <tld/>o neniom helpus onin vo&ccirc;legi la
          francan vorton <ctl>eau</ctl> kiel [o], a&ubreve; la anglan
          kvarlitera&jcirc;on <ctl>-ough</ctl> (kiu ne havas fiksitan
          legomanieron).

          Krome, por legopovigi &ccirc;inon oni entute ne bezonas <tld/>on
          &#8208; tiu vorto prezentas e&ubreve;ropcentrisman pensmanieron.

          Plej klare mallogikeco de la latinida pa&ubreve;sa&jcirc;o <tld/>igi
          videblas el analogio kun la vorto <ctl>ciferigi</ctl>, kiu
          neniel povas signifi <ctl>instrui al iu kalkulon</ctl>.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="pt">alfabetizar</trd>
        <trd lng="ru">&c_u;&c_ch;&c_i;&c_t;&c_mol; &c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_je;</trd>
      </snc>
  <trd lng="cs">učit číst</trd>
  <trd lng="sk">učiť čítať</trd>
</drv>

    <drv mrk="alfabe.0igo">
      <kap><tld/>igo</kap>
      <snc>
        <dif>
          Ago <tld/>igi:
        </dif>
        <subsnc mrk="alfabe.0igo.arangxo">
          <ekz>
            <tld/>igo <klr>(= <tld/>a ordigo)</klr> kun <em>cx...ux</em>
            okazas preskau senerare
            <fnt>
              <vrk><url
              ref="http://www.geocities.com/Paris/9231/translit.htm"
                  >La cx-transskribo (&leftquot;Kopenhaga Rekomendo&rightquot;)</url></vrk>
            </fnt>;
          </ekz>
          <trd lng="ru">&c_a;&c_l;&c_f;&c_a;&c_v;&c_i;&c_t;&c_n;&c_o;&c_je;
&c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
        </subsnc>
        <subsnc mrk="alfabe.0igo.legopovigo">
          <ekz>
            <tld/>igo de unu analfabeta afgana rifu&gcirc;into kostas 15 
            usonajn dolarojn
            <fnt><aut>Renato Corsetti</aut><vrk>Lingvo de paco</vrk>
              <bib>LOdE</bib><lok>jaro 2002a, numero 1a</lok></fnt>.
          </ekz>
          <trd lng="ru">&c_o;&c_b;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;
&c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_je;</trd>
        </subsnc>
      </snc>
      <trd lng="pt">alfabetiza&ccedil;&atilde;o</trd>
    </drv>

    <drv mrk="alfabe.0umo">
      <kap><tld/>umo</kap>
      <snc>
        <dif>
          Libro por lernigi <tld/>on al infanoj.
        </dif>
        <refgrp tip="sin">
          <ref cel="libr.aboco0o">abocolibro</ref>,
          <ref cel="aboc.0o.alfabetumo">aboco <sncref/></ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_l;&c_je;&c_m;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>,
        <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
      </trdgrp>
  <trd lng="br">levr deski&ntilde; lenn</trd>
  <trd lng="cs">slabikář</trd>
  <trdgrp lng="de">
        <trd>Abc-Buch</trd>,
        <trd>Fibel</trd>
      </trdgrp>
  <trd lng="en">book of ABC's</trd>
  <trdgrp lng="fr">
        <trd>ab&eacute;c&eacute;daire</trd>,
        <trd>ABC<klr tip="ind"> (ab&eacute;c&eacute;daire)</klr></trd>
      </trdgrp>
  <trd lng="hu">&aacute;b&eacute;c&eacute;sk&ouml;nyv</trd>
  <trd lng="nl">ABC-boek</trd>
  <trd lng="pt">cartilha de alfabetiza&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
        <trd>&c_b;&c_u;&c_k;&c_v;&c_a;&c_r;&c_mol;</trd>,
        <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;</trd>
      </trdgrp>
  <trd lng="sk">&scaron;labik&aacute;r</trd>
</drv>
  </art>
