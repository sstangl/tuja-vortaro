<kap>
    <rad>prezident</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prezident.0o">
  <kap><tld/>o</kap>
  <snc mrk="prezident.0o.SNC">
    <uzo tip="fak">POL</uzo>
    <dif>
      Respublikestro, <ref tip="vid" cel="prezid.0anto">prezidanto</ref>
      de respubliko, &scirc;tatprezidanto.
    </dif>
    <rim mrk="prezident.0rim">
    <tld lit="P"/>o estas speciala statuso por prezidanto, same kiel
    studento por studanto.
    </rim>
  </snc>
  <trd lng="cs">státní prezident</trd>
  <trd lng="fr">Pr&eacute;sident<klr> (de la R&eacute;publique)</klr></trd>
  <trd lng="sk">štátny prezident</trd>
</drv>
</art>

