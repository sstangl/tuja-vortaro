<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teozof.xml,v 1.17 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>teozofi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="teozof.0o">
  <kap><tld/>o</kap>
  <snc mrk="teozof.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Sinkretisma doktrino, celanta per intuaj procedoj havigi
      konadon pri Dio kaj vivsignifo kaj elvolvi&gcirc;on de metapsikiaj
      kapabloj; anka&ubreve; predikanta universalan homaran fratecon
      sen distingo pri religio, kredo, sekso a&ubreve; raso.
    </dif>
    <refgrp tip="vid">
      <ref cel="spirit1.0o">spiritismo</ref>,
      <ref cel="mistik.0o">mistiko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_z;&c_o;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="ca">teosofia</trd>
  <trd lng="cs">teozofie</trd>
  <trd lng="de">Theosophie</trd>
  <trd lng="en">theosophy</trd>
  <trd lng="fr">th&eacute;osophie</trd>
  <trd lng="hu">teoz&oacute;fia</trd>
  <trd lng="nl">theosofie</trd>
  <trd lng="pl">teozofia</trd>
  <trd lng="pt">teosofia</trd>
  <trd lng="ru">&c_t;&c_je;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">teozofia</trd>
</drv>
</art>
