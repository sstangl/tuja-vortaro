<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anarhx.xml,v 1.20 2015/12/07 06:10:16 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>anar&hcirc;i</rad>/o
</kap>

<drv mrk="anarhx.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="anarki2.0o">anarkio</ref>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_l;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gesetzlosigkeit</trd>,
    <trd>Anarchie</trd>,
    <trd>Unordnung</trd>,
    <trd>Durcheinander</trd>,
    <trd>Chaos</trd>
  </trdgrp>
  <trd lng="en">anarchy</trd>
  <trd lng="es">anarqu&iacute;a</trd>
  <trd lng="fr">anarchie</trd>
  <trd lng="nl">anarchie</trd>
  <trd lng="no">anarki</trd>
  <trdgrp lng="pl">
    <trd>anarchia</trd>,
    <trd>bez&lstroke;ad</trd>,
    <trd>chaos</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
  <trd lng="sk">anarchia</trd>
  <trdgrp lng="sv">
    <trd>anarki</trd>,
    <trd>lagl&ouml;shet</trd>
  </trdgrp>
</drv>

<drv mrk="anarhx.0a">
  <kap><tld/>a</kap>
  <ref tip="dif" cel="anarki2.0a">anarkia</ref>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="de">
    <trd>gesetzlos</trd>,
    <trd>anarchisch</trd>,
    <trd>chaotisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>anarchic</trd>,
    <trd>anarchist</trd>,
    <trd>chaotic</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anarchique</trd>,
    <trd>anarchiste<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="es">anarquista</trd>
  <trd lng="nl">anarchistisch</trd>
  <trd lng="no">anarkistisk</trd>
  <trdgrp lng="pl">
    <trd>anarchistyczny</trd>,
    <trd>bez&lstroke;adny</trd>,
    <trd>chaotyczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">anarchistick&yacute;</trd>
  <trd lng="sv">anarkistisk</trd>
</drv>

<drv mrk="anarhx.0ismo">
  <kap><tld/>ismo</kap>
  <ref tip="dif" cel="anarki2.0ismo">anarkiismo</ref>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Gesetzlosigkeit</trd>,
    <trd>Anarchie</trd>
  </trdgrp>
  <trd lng="en">anarchism</trd>
  <trd lng="fr">anarchisme</trd>
  <trd lng="nl">anarchisme</trd>
  <trd lng="no">anarkisme</trd>
  <trd lng="pl">anarchizm</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_m;</trd>
  <trd lng="sv">anarkism</trd>
</drv>

<drv mrk="anarhx.0isto">
  <kap><tld/>isto</kap>
  <ref tip="dif" cel="anarki2.0isto">anarkiisto</ref>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="de">Anarchist</trd>
  <trd lng="en">anarchist</trd>
  <trd lng="fr">anarchiste<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="es">anarquista</trd>
  <trd lng="nl">anarchist</trd>
  <trd lng="no">anarkist</trd>
  <trd lng="pl">anarchista</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">anarchista</trd>
  <trd lng="sv">anarkist</trd>
</drv>

<drv mrk="anarhx.0ulo">
  <kap><tld/>ulo</kap>
  <ref tip="dif" cel="anarki2.0ulo">anarkiulo</ref>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Anarch</trd>,
    <trd>Chaot</trd>
  </trdgrp>
  <trd lng="en">anarchist</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="fr">anarchiste<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="nl">anarchist</trd>
  <trd lng="no">anarkist</trd>
  <trd lng="pl">anarchista</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
