<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lini.xml,v 1.35 2016/02/28 14:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lini.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lini.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib>, <lok>p. 25</lok></fnt>
    <dif>
      Unudimensia <ref tip="super" cel="figur.geometria0o">geometria
      figuro</ref>:
      <ekz>
        rekta, kurba, rompita <tld/>o;
      </ekz>
      <ekz>
        <tld/>o estas la bildaro de kontinua
        <ref cel="bild.0igo.MAT">bildigo</ref> de la aro de reeloj al la
        koncerna spaco, kiun bildigon oni nomas
        <ind>parametra prezento</ind> de la <tld/>o.
        <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;&c_je;
        <ind>&c_v;&c_y;&c_ja;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
        <trd lng="de">Parameterdarstellung</trd>
        <trd lng="en">parametric <ind>representation</ind></trd>
        <trd lng="fr">repr&eacute;sentation param&eacute;trique</trd>
        <trd lng="hu">param&eacute;teres <ind>reprezent&aacute;ci&oacute;</ind></trd>
        <trdgrp lng="pl">
          <trd>parametryzacja</trd>,
          <trd>przedstawienie parametryczne</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
        <ind>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="kurb.0o.MAT">kurbo.</ref>
    <trdgrp lng="be">
      <trd>&c_l;&c_ib;&c_n;&c_ib;&c_ja;</trd>,
      <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Linie</trd>,
      <trd>Kurve</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>line</trd>,
      <trd>curve</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>courbe<klr tip="ind"> (g&eacute;om&eacute;trique)</klr></trd>,
      <trd>ligne<klr tip="ind"> (g&eacute;om&eacute;trique)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vonal</trd>,
      <trd>g&ouml;rbe</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>linia</trd>,
      <trd>krzywa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="lini.0o.KOMUNE">
    <dif>
      Strio, vico de sinsekvaj a&jcirc;oj.
    </dif>
    <subsnc mrk="lini.0o.vico">
      <dif>
        <ref tip="vid" cel="vic.0o">Vico</ref> da objektoj a&ubreve; personoj
        ordigitaj la&ubreve; difinita direkto:
        <ekz>
          duobla <tld/>o da arboj;
        </ekz>
        <ekz>
          armeo ordigita la&ubreve; tri <tld/>oj.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="lini.0o.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Kontinua <ref tip="vid" cel="strek.0o.KOMUNE">streko</ref>
        montranta difinitan direkton:
        <ekz>
          la belforma kaj delikata <tld/>o de tiu bu&scirc;o
          <fnt><bib>Marta</bib></fnt>;
        </ekz>
        <ekz>
          <tld/>oj de desegno, de la viza&gcirc;o, de la mano;
        </ekz>
        <ekz>
          konduki, streki, desegni, tiri <tld/>on;
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="kontur.0o">konturo</ref>
      </refgrp>
      <trd lng="hu">vonal</trd>
    </subsnc>
    <subsnc mrk="lini.0o.TRA">
      <tezrad fak="TRA"/>	  
      <uzo tip="fak">TRA</uzo>
      <dif>
        Sinsekvo de <ref tip="prt" cel="voj.0o.TRA">vojoj</ref>
        trairotaj:
        <ekz>
          marvetura, trafika <tld/>o;
        </ekz>
        <ekz>
          organizi, aran&gcirc;i <tld/>ojn de busoj, de avioj;
        </ekz>
        <ekz>
          sekvu la fervoj<tld/>on, &gcirc;is vi alvenos la stacion
          <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
          8a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="hu">&uacute;tvonal</trd>
    </subsnc>
    <subsnc mrk="lini.0o.TIP">
      <uzo tip="fak">TIP</uzo>
      <dif>
        Vico da skribosignoj metitaj la&ubreve; la ordinara legodirekto:
        <ekz>
          super<tld/>a signeto<fnt>Z</fnt>;
        </ekz>
        <ekz>
          iom pensinte, &scirc;i skribis la sekvantajn <tld/>ojn
          <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
          glacia&jcirc;o</lok></fnt>...
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="lini.0o.GEN">
      <uzo tip="fak">GEN</uzo>
      <dif>
        Sinsekvo de idoj ekde unu praulo:
        <ekz>
          li estis &scirc;ia malproksima parenco en la patrina <tld/>o
          <fnt>
            <aut>A. Grin, trad. J. Finkel</aut>,
            <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
            La ora &ccirc;eno</url></vrk>,
            <lok>2009</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">lign&eacute;e</trd>
    </subsnc>    
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>hrana</trd>,
    <trd>lajna</trd>,
    <trd>linie</trd>,
    <trd>linka</trd>,
    <trd>tra&tcaron;</trd>,
    <trd>v&ecaron;tev <klr>(p&rcaron;&iacute;buzensk&aacute;)</klr></trd>,
    <trd>&ccaron;&aacute;ra</trd>,
    <trd>&rcaron;&aacute;dek</trd>
  </trdgrp>
  <trd lng="en">line</trd>
  <trd lng="es">l&iacute;nea</trd>
  <trd lng="fr">ligne</trd>
  <trdgrp lng="hu">
    <trd>vonal</trd>,
    <trd>sor</trd>
  </trdgrp>
  <trd lng="nl">lijn</trd>
  <trd lng="pt">linha</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>linka</trd>,
    <trd>l&iacute;nia</trd>,
    <trd>riadok</trd>,
    <trd>&ccaron;iara</trd>
  </trdgrp>
</drv>

<drv mrk="lini.0a">
  <kap><tld/>a</kap>
  <snc mrk="lini.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a aran&gcirc;o, desegno.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>vonalas</trd>,
      <trd>vonal-</trd>
    </trdgrp>
  </snc>
  <snc mrk="lini.0a.MAT">
     <uzo tip="fak">MAT</uzo>
     <uzo tip="stl">ARK</uzo>
     <fnt><bib>PIV1</bib></fnt>
     <dif>
       <ref tip="dif" cel="linear.0a">Lineara</ref>:
       <ekz>
         <tld/>a ekvacio.
       </ekz>
     </dif>
     <rim>
       Kvankam ar&hcirc;aiki&gcirc;inta, tiu senco restas logika,
       radik&scirc;para,
       internacia kaj promociinda. Riskoj de ambigueco apena&ubreve;
       ekzistas.
     </rim>
     <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_y;</trd>
     <trd lng="de">linear</trd>
     <trd lng="en">linear</trd>
     <trd lng="hu">line&aacute;ris</trd>
     <trd lng="pl">liniowy</trd>
  </snc>  
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;m&yacute;</trd>,
    <trd>tra&tcaron;ov&yacute;</trd>
  </trdgrp>
  <trd lng="de">linear</trd>
  <trd lng="en">linear</trd>
  <trd lng="fr">lin&eacute;aire</trd>
  <trd lng="hu">line&aacute;ris</trd>
  <trd lng="nl">lineair</trd>
  <trd lng="pl">liniowy</trd>
  <trd lng="pt">linear</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">line&aacute;rny</trd>
</drv>

<drv mrk="lini.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a ordo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;</trd>
  <trd lng="es">linealmente</trd>
  <trd lng="fr">en <ind>ligne</ind></trd>
  <trdgrp lng="hu">
    <trd>egy vonalban</trd>,
    <trd>sorban</trd>
  </trdgrp>
  <trd lng="nl">rechtlijnig</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_l;&c_i;&c_n;&c_i;&c_ju;</ind></trd>,
    <trd>&c_p;&c_o; <ind>&c_l;&c_i;&c_n;&c_i;&c_i;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="lini.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn, marki <tld/>ojn sur io:
      <ekz>
        <tld/>ita papero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>linkovat</trd>,
    <trd>lin&yacute;rovat</trd>
  </trdgrp>
  <trd lng="en">line</trd>
  <trd lng="fr">ligner</trd>
  <trdgrp lng="hu">
    <trd>vonalaz</trd>,
    <trd>vonalk&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">lini&euml;ren</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>linajkova&tcaron;</trd>,
    <trd>linkova&tcaron;</trd>,
    <trd>riadkova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="lini.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo por streki <tld/>ojn:
      <ekz>
        kion tiuj homoj farus <klr>...</klr> se ne plu estus krajonoj?
        Sed
        tiukaze certe estus <tld/>ilo a&ubreve; inksorbilo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="cs">prav&iacute;tko</trd>
  <trd lng="fr">r&egrave;gle<klr tip="ind"> (pour tracer des
  traits)</klr></trd>
  <trdgrp lng="hu">
    <trd>vonalz&oacute;</trd>,
    <trd>l&eacute;nia</trd>
  </trdgrp>
  <trd lng="nl">liniaal</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja;
  &c_ch;&c_je;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trdgrp lng="sk">
    <trd>line&aacute;r</trd>,
    <trd>prav&iacute;tko</trd>
  </trdgrp>
</drv>

<drv mrk="lini.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="lini.0aro.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      En muzika skribo aro da horizontalaj <tld/>oj, sur kiuj estas
      lokitaj la diversaj notoj.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_t;&c_n;&c_y; &c_s;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>linkov&aacute;n&iacute;</trd>,
    <trd>notov&aacute; osnova</trd>
  </trdgrp>
  <trd lng="de">Notenlinien</trd>
  <trdgrp lng="en">
    <trd>staff</trd>,
    <trd>stave</trd>
  </trdgrp>
  <trd lng="es">pentagrama</trd>
  <trd lng="fr">port&eacute;e<klr tip="ind"> (mus.)</klr></trd>
  <trdgrp lng="hu">
    <trd>hangjegyvonal</trd>,
    <trd>vonalrendszer</trd>
  </trdgrp>
  <trd lng="nl">notenbalk</trd>
  <trd lng="ru">&c_n;&c_o;&c_t;&c_n;&c_y;&c_j; &c_s;&c_t;&c_a;&c_n;</trd>
  <trd lng="sk">elektrick&aacute; sie&tcaron;</trd>
</drv>

<drv mrk="lini.laux0igi">
  <kap>la&ubreve;<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ordigi la&ubreve; <tld/>oj:
      <ekz>
        la&ubreve;<tld/>igi monerojn sur la tablo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vic.0igi">vicigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol; &c_u; &c_l;&c_ib;&c_n;&c_ib;&c_ju;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_r;&c_a;&c_ib;&c_c;&c_mol; &c_u; &c_l;&c_ib;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>align</trd>,
    <trd>line up</trd>
  </trdgrp>
  <trd lng="es">alinear</trd>
  <trd lng="fr">aligner</trd>
  <trdgrp lng="hu">
    <trd>sorba rendez</trd>,
    <trd>sorba igaz&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">aligneren</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol; <klr>&c_v; &c_l;&c_i;&c_n;&c_i;&c_ju;</klr></trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <klr>&c_v; &c_l;&c_i;&c_n;&c_i;&c_ju;</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lini.gvid0o">
  <kap>gvid<tld/>o</kap>
  <snc mrk="lini.gvid0o.desegnita">
    <dif>
      Linio anta&ubreve;desegnita por gvidi dum posta skribado, inkado,
      kolorigo...:
      <ekz>
        J&aacute;nos, sen helpo de krajono a&ubreve; gvid<tld/>oj, tute nature kreas
        la&ubreve;
        bildo en sia kapo
        <fnt>
        <aut>E. Tantin Ackermann</aut>,
        <vrk>Akvareloj de J&aacute;nos T&oacute;th en Triesto/Italio</vrk>,
        <lok>Hungaraj Pa&gcirc;oj numero 14a, 1997-12, Suplemento al
        Eventoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxablo.0o">&scirc;ablono</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_b;&c_r;&c_o;&c_s;&c_o;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_m;&c_jo;&c_t;&c_k;&c_i;</trd>
    </trdgrp>
  </snc>
  <snc mrk="lini.gvid0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Gcirc;enerala <ref tip="super"
      cel="instru1.0o.KOMUNE">instrukcio</ref>
      entenanta &ccirc;efajn, ne detalajn kaj ne tute devigajn regulojn:
      <ekz>
        Politikaj Gvid<tld/>oj de la E&ubreve;ropa Federacio de Verdaj Partioj
        <fnt>
        <aut>M. P. Sion</aut>,
        <vrk>AVE seminario</vrk>,
        <lok>soc.culture.esperanto, 1998-01-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_n;&c_i;&c_ja; &c_p;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_o;&c_b;&c_shch;&c_je;&c_je; &c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;
      &c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>ligne de conduite</trd>,
    <trd>lin&eacute;ament</trd>
  </trdgrp>
  <trd lng="nl">richtlijn</trd>
</drv>

<drv mrk="lini.lim0o">
  <kap>lim<tld/>o</kap>
  <snc mrk="lini.lim0o.KOMUNE">
    <dif>
      Videbla <tld/>o markanta la limon de surfaco:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en &gcirc;i ku&scirc;as la lim<tld/>o de la prudento, kaj tiun lim<tld/>on
        
        oni ne devas transpa&scirc;i
        <fnt><bib>Fab4</bib>, <lok>Turgardisto Ole</lok></fnt>;
      </ekz>
      <ekz>
        la kvar lim<tld/>oj nomi&gcirc;as &leftquot;la unua linio&rightquot; <klr>...</klr>
        la 
        dua linio, la tria linio ktp
        <fnt>
          <aut>Tigro</aut>,
          <vrk><url
          ref="http://www.espero.com.cn/06s/06s-4/socio4-1.htm">
          Aboco de vej&ccirc;i-ludo</url></vrk>,
          <lok>[2004-11]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontur.0o.BELA">konturo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>hrani&ccaron;n&iacute; linie</trd>,
    <trd>hrani&ccaron;n&iacute; &ccaron;&aacute;ra</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;marcation</trd>,
    <trd>ligne de d&eacute;marcation</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>grenslijn</trd>,
    <trd>demarcatielijn</trd>
  </trdgrp>
  <trd lng="sk">demarka&ccaron;n&aacute; &ccaron;iara</trd>
</drv>

<drv mrk="lini.rompita0o">
  <kap>rompita <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="lini.0o.MAT"><tld lit="L"/>o</ref>,
      konsistanta el kontinua sinsekvo de <ref
      cel="strek.0o.elementa">strekoj</ref>:
      <ekz>
        la <ref cel="rand.0o.figuro">rando</ref> de
        <ref cel="later.plur0o">plurlatero</ref> estas fermita rompita
        <tld/>o.
