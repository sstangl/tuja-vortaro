<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: psikia.xml,v 1.15 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>psikiatr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="psikia.0o">
  <kap><tld/>o</kap>
  <snc mrk="psikia.0o.MED">
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="psikia1.0o"><tld/>io</ref>:
      <ekz>
        mi rekomendis al &scirc;i konsulti <tld/>on kaj, &ccirc;agrenite, &scirc;i foriris
        <fnt><aut>Werner Schad</aut>: <vrk>Nenio perfektas</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_ib;&c_ja;&c_t;&c_r;</trd>
  <trd lng="cs">psychiatr</trd>
  <trdgrp lng="de">
    <trd>Psychiater</trd>,
    <trd>Nervenarzt</trd>,
    <trd>Irrenarzt<klr> (arkaika)</klr></trd>
  </trdgrp>
  <trd lng="en">psychiatrist</trd>
  <trd lng="fr">psychiatre</trd>
  <trdgrp lng="hu">
    <trd>pszichi&aacute;ter</trd>,
    <trd>idegorvos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>psychiater</trd>,
    <trd>zenuwarts</trd>
  </trdgrp>
  <trd lng="pt">psiquiatra</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_i;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">psychiater</trd>
</drv>
</art>
