<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: facil.xml,v 1.42 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>facil</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="facil.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="facil.0a.farebla">
    <dif>
      Senpene farebla:
      <ekz>
        <tld/>a laboro<fnt>B</fnt>,
      </ekz>
      <ekz>
        tasko, problemo, lingvo, vojo<fnt>B</fnt>, spiro;
      </ekz>
      <ekz>
        <tld/>aj versoj
        <klr>(kiuj &scirc;ajnas senpene faritaj kaj fluantaj)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>senzill</trd>,
      <trd>simple</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>gampang</trd>,
      <trd>mudah</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_jo;&c_g;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_t;&c_r;&c_u;&c_d;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="facil.0a.eltenebla">
    <dif>
      Senpene eltenebla, ne peza, apena&ubreve; sentebla, neintensa:
      <ekz>
        <tld/>a <klr>(milda)</klr> vento<fnt>Z</fnt>,
        klimato, ripro&ccirc;o;
      </ekz>
      <ekz>
        <tld/>a <klr>(neforta)</klr> rideto,
        saluto<fnt>Z</fnt>, dormo.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lleuger</trd>,
      <trd>t&egrave;nue</trd>,
      <trd>suau</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>lembut</trd>,
      <trd>ringan</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_jo;&c_g;&c_k;&c_i;&c_j; <klr>
    (&c_v; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_n;&c_o;&c_m;
    &c_s;&c_m;&c_y;&c_s;&c_l;&c_je;)</klr></trd>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_l;&c_je;&c_s;&c_n;&c_o;</trd>
  <trdgrp lng="br">
    <trd>aes<klr tip="ind"> (ag.)</klr></trd>,
    <trd>aezet<klr tip="ind"> (ag.)</klr></trd>
  </trdgrp>
  <trd lng="ca">f&agrave;cil</trd>
  <trdgrp lng="cs">
    <trd>hladký</trd>,
    <trd>lehký</trd>,
    <trd>nenáhlý</trd>,
    <trd>nenáročný</trd>,
    <trd>snadn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einfach</trd>,
    <trd>leicht</trd>
  </trdgrp>
  <trd lng="en">easy</trd>
  <trd lng="es">f&aacute;cil</trd>
  <trdgrp lng="fr">
    <trd>ais&eacute;<klr tip="ind"> (facile)</klr></trd>,
    <trd>facile</trd>,
    <trd>l&eacute;ger</trd>
  </trdgrp>
  <trd lng="he">&qof;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;nny&udblac;</trd>,
    <trd>k&ouml;nnyed</trd>,
    <trd>enyhe</trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gemakkelijk</trd>,
    <trd>eenvoudig</trd>
  </trdgrp>
  <trd lng="oc">aisit</trd>
  <trdgrp lng="pl">
    <trd>lekki</trd>,
    <trd>&lstroke;atwy</trd>
  </trdgrp>
  <trd lng="pt">f&aacute;cil</trd>
  <trdgrp lng="sk">
    <trd>snadný</trd>,
    <trd>ľahký</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>enkel</trd>,
    <trd>l&auml;tt</trd>
  </trdgrp>
  <trd lng="tr">kolay</trd>
</drv>

<drv mrk="facil.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        <tld/>e malvarmumi
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 42</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e forgesi, pardoni, paroli;
      </ekz>
      <ekz>
        Esperanto estas <tld/>e lernebla.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;</trd>
  <trd lng="bg">&c_l;&c_je;&c_s;&c_n;&c_o;</trd>
  <trdgrp lng="br">
    <trd>aes<klr tip="ind"> (adv.)</klr></trd>,
    <trd>aezet<klr tip="ind"> (adv.)</klr></trd>
  </trdgrp>
  <trd lng="ca">f&agrave;cilment</trd>
  <trd lng="cs">facile</trd>
  <trdgrp lng="de">
    <trd>einfach</trd>,
    <trd>leicht</trd>
  </trdgrp>
  <trd lng="en">easily</trd>
  <trd lng="es">f&aacute;cilmente</trd>
  <trd lng="fr">facilement</trd>
  <trd lng="he">&bet;<ind>&qof;&lamed;&vav;&tav;</ind></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;nnyen</trd>,
    <trd>k&ouml;nnyed&eacute;n</trd>
  </trdgrp>
  <trd lng="id">dengan <ind>mudah</ind></trd>
  <trdgrp lng="nl">
    <trd>gemakkelijk</trd>,
    <trd>eenvoudig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekko</trd>,
    <trd>&lstroke;atwo</trd>,
    <trd>&lstroke;acno</trd>,
    <trd>z <ind>&lstroke;atwo&sacute;ci&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">facilmente</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;</trd>,
    <trd>&c_s; &c_l;&c_jo;&c_g;&c_k;&c_o;&c_s;&c_t;&c_mol;&c_ju;</trd>
  </trdgrp>
  <trd lng="sk">ľahko</trd>
  <trdgrp lng="sv">
    <trd>enkelt</trd>,
    <trd>l&auml;tt</trd>
  </trdgrp>
  <trd lng="tr">kolayca</trd>
</drv>

<drv mrk="facil.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="facil.0igi.farebligi">
    <dif>
      Igi ion <tld/>e farebla:
      <ekz>
        <tld/>igi al iu la laboron.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_u;&c_l;&c_je;&c_s;&c_n;&c_ja;&c_v;&c_a;&c_n;&c_je; &c_n;&c_a;</trd>
    <trd lng="ca">facilitar</trd>
    <trd lng="en">facilitate</trd>
    <trd lng="es">facilitar</trd>
    <trdgrp lng="id">
      <trd>[<ind>mudah</ind>] memudahkan</trd>,
      <trd>[<ind>mudah</ind>] mempermudah</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vergemakkelijken</trd>,
      <trd>vereenvoudigen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>u&lstroke;atwia&cacute;</trd>,
      <trd>upraszcza&cacute;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>underl&auml;tta</trd>,
      <trd>f&ouml;renkla</trd>
    </trdgrp>
    <trd lng="pt">facilitar</trd>
  </snc>
  <snc mrk="facil.0igi.mildigi">
    <dif>
      Mildigi, kvietigi:
      <ekz>
        <tld/>igi la koron;
      </ekz>
      <ekz>
        rakontu al mi vian mal&gcirc;ojon, per tio vi eble
        <tld/>igos vian animon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pez">malpezigi</ref>,
      <ref cel="sxargx">sen&scirc;ar&gcirc;igi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ricev.0i">ricevi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_y;&c_c;&c_mol; <klr>
      (&c_d;&c_u;&c_sh;&c_u;)</klr></trd>
    </trdgrp>
    <trd lng="ca">alleugerir</trd>
    <trd lng="en">ease</trd>
    <trd lng="id">[<ind>ringan</ind>] meringankan</trd>
    <trd lng="nl">verzachten</trd>
    <trd lng="pt">aquietar</trd>
    <trdgrp lng="pl">
      <trd>ul&zdot;y&cacute;</trd>,
      <trd>przynosi&cacute; <ind>ulg&eogonek;</ind></trd>,
      <trd>zrzuca&cacute; kamie&nacute; <klr> (np. z serca)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_i;&c_t;&c_mol; <klr>
      (&c_d;&c_u;&c_sh;&c_u;)</klr></trd>
    </trdgrp>
    <trd lng="sv">l&auml;tta</trd>
  </snc>
  <trd lng="br">aesaat<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="cs">
    <trd>usnadnit</trd>,
    <trd>zjednodušit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vereinfachen</trd>,
    <trd>erleichtern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>all&eacute;ger</trd>,
    <trd>att&eacute;nuer</trd>,
    <trd>faciliter</trd>,
    <trd>simplifier</trd>,
    <trd>soulager</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;<ind>&qof;&lamed;</ind></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;nny&iacute;t</trd>,
    <trd>megk&ouml;nny&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poľahčiť</trd>,
    <trd>uľahčiť</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>kolayla&scedil;t&inodot;rmak</trd>,
    <trd>basitle&scedil;tirmek</trd>
  </trdgrp>
</drv>

<drv mrk="facil.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="facil.0igxi.pene">
    <dif>
      I&gcirc;i <tld/>a, malpli peniga ol anta&ubreve;e:
      <ekz>
        mi neniam sugestis ke la tasko <tld/>i&gcirc;os
        <fnt>
          <aut>K. Enderby</aut>,
          <vrk><url
          ref="http://esperanto-ondo.ru/Ind-jaro.htm#1999-intervjuo">
          Ni ne devas esti sen influo...</url></vrk>,
          <lok>La Ondo de Esperanto, 2000:2 (64)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">devenir <ind>facile</ind></trd>
  </snc>
  <snc mrk="facil.0igxi.malpeze">
    <dif>
      I&gcirc;i <tld/>a, malpli peza, malpli prema:
      <ekz>
        nun tute <tld/>i&gcirc;is lia koro
        <fnt>
          <aut>M. Saltykov-&Scirc;&ccirc;edrin, trad. S. Pokrovskij</aut>,
          <vrk>La trompulo gazetisto kaj la kredema leganto</vrk>,
          <lok>La Ondo de Esperanto, 2000:7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pez.mal0igxi.KOMUNE">malpezi&gcirc;i</ref>,
      <ref cel="sxargx.sen0igxi.KOMUNE">sen&scirc;ar&gcirc;i&gcirc;i</ref>
    </refgrp>
    <trd lng="fr">s'<ind>all&eacute;ger</ind></trd>
  </snc>
</drv>

<drv mrk="facil.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta penon, peza, malagrable intensa:
      <ekz>
        la brusto levi&gcirc;adis per mal<tld/>aj &gcirc;emoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_r;&c_u;&c_d;&c_n;&c_o;</trd>
  <trd lng="br">diaes</trd>
  <trd lng="ca">dif&iacute;cil</trd>
  <trdgrp lng="cs">
    <trd>nesnadný</trd>,
    <trd>obtížný</trd>,
    <trd>těžký <klr>(duševně)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwer</trd>,
    <trd>schwierig</trd>
  </trdgrp>
  <trd lng="es">dif&iacute;cil</trd>
  <trd lng="fr">difficile</trd>
  <trdgrp lng="id">
    <trd>sukar</trd>,
    <trd>sulit</trd>,
    <trd>susah</trd>
  </trdgrp>
  <trd lng="nl">moeilijk</trd>
  <trd lng="pl">trudny</trd>
  <trd lng="pt">dif&iacute;cil</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_u;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;<klr tip="ind">
    (&c_t;&c_r;&c_u;&c_d;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neľahký</trd>,
    <trd>obtiažny</trd>
  </trdgrp>
  <trd lng="tr">zor</trd>
</drv>

<drv mrk="facil.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io pene farebla a&ubreve; plenumebla; malhelpa&jcirc;o:
      <ekz>
        la mal<tld/>a&jcirc;oj kaj problemoj de la arto aperadis tiam
        anta&ubreve; &scirc;i
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0o">baro</ref>,
      <ref cel="dorn.0o">dorno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_t;&c_r;&c_u;&c_d;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="br">
    <trd>diaester</trd>,
    <trd>diaezamant</trd>
  </trdgrp>
  <trd lng="ca">dificultat</trd>
  <trd lng="de">Schwierigkeit</trd>
  <trd lng="en">difficulty</trd>
  <trd lng="es">dificultad</trd>
  <trdgrp lng="fr">
    <trd>difficult&eacute;</trd>,
    <trd>probl&egrave;me<klr tip="ind"> (difficult&eacute;)</klr></trd>,
    <trd>complication</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>sukar</ind>] kesukaran</trd>,
    <trd>[<ind>sulit</ind>] kesulitan</trd>,
    <trd>[<ind>susah</ind>] kesusahan</trd>
  </trdgrp>
  <trd lng="nl">moeilijkheid</trd>
  <trd lng="hu">neh&eacute;zs&eacute;g</trd>
  <trd lng="pl">trudno&sacute;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;
    </trd>,
    <trd>&c_t;&c_r;&c_u;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">sv&aring;righet</trd>
  <trd lng="pt">dificuldade</trd>
  <trdgrp lng="tr">
    <trd>zorluk</trd>,
    <trd>problem</trd>
  </trdgrp>
</drv>

<drv mrk="facil.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi pli da peno, aldoni barojn kaj obstaklojn:
      <ekz>
        altaj muroj mal<tld/>igis la eniron
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="help.mal0i">malhelpi</ref>
    </refgrp>
  </snc>
  <trd lng="br">diaesaat<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="ca">dificultar</trd>
  <trdgrp lng="de">
    <trd>erschweren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rendre <ind>difficile</ind></trd>,
    <trd>faire <ind>obstacle</ind></trd>,
    <trd>s'<ind>opposer</ind>
    &agrave;<klr tip="amb"> (faire obstacle)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>sukar</ind>] menyukarkan</trd>,
    <trd>[<ind>sukar</ind>] mempersukar</trd>,
    <trd>[<ind>sulit</ind>] menyulitkan</trd>,
    <trd>[<ind>sulit</ind>] mempersulit</trd>,
    <trd>[<ind>susah</ind>] menyusahkan</trd>,
    <trd>[<ind>susah</ind>] mempersusah</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bemoeilijken</trd>,
    <trd>moeilijk maken</trd>
  </trdgrp>
  <trd lng="pl">utrudnia&cacute;</trd>
  <trd lng="pt">dificultar</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="tr">zorla&scedil;t&inodot;rmak</trd>
</drv>

<drv mrk="facil.0pieda">
  <kap><tld/>pieda</kap>
  <snc>
    <dif>
      <tld lit="F"/>e movanta la piedojn, <ref tip="super"
      cel="lert.0a.korpo">lerta</ref> en irado.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>mibin a droad</trd>,
    <trd>ska&ntilde;v e c'har</trd>
  </trdgrp>
  <trd lng="ca">de cames lleugeres</trd>
  <trd lng="cs">mrštný</trd>
  <trd lng="de">leichtf&uuml;&szlig;ig</trd>
  <trd lng="en">easy-going</trd>
  <trdgrp lng="fr">
    <trd>agile</trd>,
    <trd>alerte<klr tip="ind"> (ingambe)</klr></trd>,
    <trd>ingambe</trd>
  </trdgrp>
  <trd lng="he">&qof;&lamed; &resh;&gimel;&lamed;&yod;&yod;&fmem;</trd>
