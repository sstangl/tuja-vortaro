<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parter.xml,v 1.18 2015/07/04 08:37:41 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>parter</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parter.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="parter.0o.TEA">
     <uzo tip="fak">TEA</uzo>
     <dif>
       Parto de <ref tip="malprt" cel="teatr.0ejo">teatreja</ref>
       &ccirc;ambrego &ccirc;e tereta&gcirc;o inter la orkestrejo kaj la funda muro.
     </dif>                      
     <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;</trd>
     <trd lng="br">leur<klr tip="ind"> (c'hoariva)</klr></trd>
     <trd lng="ca">fossa de l'orquestra</trd>
     <trd lng="de">Parkett</trd>
     <trd lng="fr">parterre</trd>
     <trd lng="nl">parterre</trd>
     <trd lng="pt">plat&eacute;ia <klr>(parte de teatro)</klr></trd>
     <trd lng="ru">&c_p;&c_a;&c_r;&c_t;&c_je;&c_r;</trd>
   </snc>
   <snc mrk="parter.0o.ARKI">
     <ref tip="dif" cel="etagx.ter0o.ARKI">tereta&gcirc;o</ref>
     <ekz>
       la ru&gcirc;aj kurtenoj en la &ccirc;ambroj de la <tld/>o kaj la blankaj de
       la unua eta&gcirc;o<fnt>Z</fnt>;
     </ekz>
     <ekz>
       en la <tld/>o estis nur magazeno<fnt>Z</fnt>.
     </ekz>
     <trd lng="be">&c_p;&c_je;&c_r;&c_sh;&c_y; <ind>&c_p;&c_a;&c_v;&c_je;&c_r;&c_h;</ind></trd>  
     <trd lng="br">rez-an-douar</trd>
     <trd lng="ca">planta baixa</trd>
     <trd lng="en">ground floor</trd>
     <trd lng="fr">rez-de-chauss&eacute;e</trd>
     <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j; &c_e;&c_t;&c_a;&c_zh;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>květinová zahrada</trd>,
    <trd>parter</trd>,
    <trd>přízemí hlediště</trd>
  </trdgrp>
  <trd lng="de">Erdgeschoss</trd>
  <trd lng="hu">f&ouml;ldszint<klr tip="ind"> (sz&iacute;nh&aacute;zban is)</klr></trd>
  <trd lng="nl">benedenverdieping</trd>
  <trdgrp lng="pt">
     <trd>t&eacute;rreo</trd>,
     <trd>r&eacute;s-do-ch&atilde;o</trd>
   </trdgrp>
  <trd lng="sk">prízemie</trd>
</drv>
</art>
