<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amplek.xml,v 1.24 2015/10/30 11:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ampleks</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="amplek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="amplek.0o.KOMUNE">
    <dif>
      Dimensio, grado de grandeco, konsiderata rilate al tio, kion
      &gcirc;i enhavas:
      <ekz>
        bildo la&ubreve; natura <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de katastrofo, organizo, krizo, &jcirc;eto,
        balanci&gcirc;o, signifo, ideo;
      </ekz>
      <ekz>
        tiu principo havas pli grandan <tld/>on ol nure juran;
      </ekz>
      <ekz>
        vasta <tld/>o da farita lernado<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spac.0o">spaco</ref>,
      <ref cel="volume.0o">volumeno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd>,
    <trd>&c_a;&c_b;'&c_jo;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dosah</trd>,
    <trd>p&aacute;smo</trd>,
    <trd>rozloha</trd>,
    <trd>rozmez&iacute;</trd>,
    <trd>rozsah</trd>,
    <trd>&scaron;&iacute;&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausdehnung <klr>(Gr&ouml;&szlig;e)</klr></trd>,
    <trd>Ausma&szlig;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extent</trd>,
    <trd>scope</trd>,
    <trd>magnitude</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>amplitud</trd>,
    <trd>extensi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ampleur</trd>,
    <trd>&eacute;tendue</trd>,
    <trd>port&eacute;e</trd>,
    <trd>taille</trd>
  </trdgrp>
  <trd lng="nl">omvang</trd>
  <trdgrp lng="pt">
    <trd>amplitude</trd>,
    <trd>amplid&atilde;o</trd>
    <trd>dimens&atilde;o</trd>
    <trd>extens&atilde;o</trd>
    <trd>grandeza</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_jo;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dosah</trd>,
    <trd>p&aacute;smo</trd>,
    <trd>rozsah</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.0a">
  <kap><tld/>a</kap>
  <snc mrk="amplek.0a.iel">
    <dif>
      Iel rilata al <tld/>o:
      <ekz>
        kodoj povas esti malsame <tld/>aj
        <fnt>
          <aut>Pejno S.</aut>, <vrk>Tekstotraktado</vrk>,
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        sam<tld/>aj grandoj.
      </ekz>
    </dif>
    <trd lng="en">dimensional</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="amplek.0a.vasta">
    <ref tip="dif" cel="amplek.grand0a.KOMUNE">grand<tld/>a</ref>
    <ekz>
      <tld/>a verko, spirito;
    </ekz>
    <ekz>
      la arto de verkado <klr>(...)</klr> nepre postulas certan
      sufi&ccirc;e vastan <tld/>on da farita lernado, certan sufi&ccirc;e
      <tld/>an sciadon komune sciencan kaj speciale teknikan
      <fnt><bib>Marta</bib></fnt>;
    </ekz>
    <ekz> la agado de
      nia societo tagon post tago dis<tld/>i&gcirc;as.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>ob&scaron;&iacute;rn&yacute;</trd>,
    <trd>rozs&aacute;hl&yacute;</trd>
  </trdgrp>
  <trd lng="en">extensive</trd>
  <trd lng="fr">&eacute;tendu<klr> (vaste)</klr></trd>
  <trdgrp lng="sk">
    <trd>objemn&yacute;</trd>,
    <trd>obsiahly</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amplek.0i.KOMUNE">
    <dif>
      Havi <tld/>on de; enhavi, enteni:
      <ekz>
        la &ccirc;ielo ne povus vin
        <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la registara deklaro <tld/>as &ccirc;iujn nunajn
        politikajn demandojn;
      </ekz>
      <ekz>
        ni ordinare kunlegas tutan serion da vortoj <tld/>itan per
        unu vido.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pren.cxirkaux0i">&ccirc;irka&ubreve;preni</ref>,
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="spac.en0i">enspaci</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="volv.en0i">envolvi</ref>,
      <ref cel="etend.0igxi">etendi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>obsahovat</trd>,
    <trd>zahrnovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umfassen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extend to</trd>,
    <trd>include</trd>
  </trdgrp>
  <trd lng="es">abarcar</trd>
  <trdgrp lng="fr">
    <trd>comporter</trd>,
    <trd>comprendre</trd>,
    <trd>embrasser</trd>
  </trdgrp>
  <trd lng="nl">omvatten</trd>
  <trdgrp lng="pt">
    <trd>extender-se at&eacute;</trd>,
    <trd>abranger</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; rozsah</trd>,
    <trd>obsahova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="amplek.grand0a">
  <kap>grand<tld/>a, <var><kap>mult<tld/>a</kap></var></kap>
  <snc mrk="amplek.grand0a.KOMUNE">
    <dif>
      Havanta grandan <tld/>on,
      <ref tip="vid" cel="vast.0a">vasta</ref>:
      <ekz>
        bonvolu doni al li tiun grand<tld/>an leteron
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio ne estas grand<tld/>a propagando?
        <fnt>
          <aut>E. Lanti</aut>,
          <vrk>Publika letero al Romen Rolan</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili eniris <klr>...</klr> korton en la mezo de la
        mult<tld/>a templa konstrua&jcirc;aro
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">objemn&yacute;</trd>
  <trdgrp lng="de">
    <trd>ausgedehnt</trd>,
    <trd><klr>von </klr>gro&szlig;em <ind>Ausma&szlig;</ind></trd>,
    <trd>weitr&auml;umig</trd>,
    <trd>extensiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>comprehensive</trd>,
    <trd>extensive</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>amplio</trd>,
    <trd>amplia</trd>,
    <trd>extenso</trd>,
    <trd>extensa</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ample</trd>,
    <trd>vaste</trd>
  </trdgrp>
  <trd lng="nl">omvangrijk</trd>
  <trd lng="pt">amplo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">objemn&yacute;</trd>
</drv>

<drv mrk="amplek.korp0a">
  <kap>korp<tld/>a</kap>
  <snc mrk="amplek.korp0a.KOMUNE">
    <dif>
      Vastkorpa, 
      <ref tip="sin" cel="korpul.0a.KOMUNE">korpulenta</ref>:
      <ekz>
        al la leona ka&gcirc;o per elasta danca pa&scirc;o
        trotas korp<tld/>a viro
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_zh;&c_n;&c_y;</trd>,
     <trd>&c_t;&c_l;&c_u;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
     <trd>&c_p;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>korpulent</trd>,
     <trd>beleibt</trd>,
     <trd>wohlbeleibt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>corpulent</trd>,
    <trd>fat</trd>,
    <trd>fleshy</trd>,
    <trd>obese</trd>,
    <trd>stout</trd>
  </trdgrp>
  <trd lng="es">corpulento</trd>
  <trd lng="fr">corpulent</trd>
  <trdgrp lng="hu">
     <trd>testes</trd>,
     <trd>korpulens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>corpulent</trd>,
    <trd>zwaarlijvig</trd>
    <trd>dik</trd>,
    <trd>gezet</trd>,
    <trd>vlezig</trd>
  </trdgrp>
  <trd lng="pt">corpulento</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
