<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fakultativ.xml,v 1.6 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fakultativ</rad>/a
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fakultativ.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="klr">(oficeja stilo)</uzo>
  <snc mrk="fakultativ.0a.KOMUNE">
    <ref tip="dif" cel="dev.ne0iga.KOMUNE">nedeviga</ref>
    <ref tip="dif" cel="nepr.mal0a.KOMUNE">malnepra</ref>
    <ekz>
      Parto A - Devigaj elementoj /
      Parto B - <tld lit="F"/>aj elementoj
      <fnt>
      <aut>-</aut>,
      <vrk><url ref="http://www.recim.org/ascop/projekto.htm">
      Kondi&ccirc;aro [...] de ASKOP</url></vrk>,
      <lok>2007-04</lok>
      </fnt>;
    </ekz>
    <ekz>
      vi skribis al mi <klr>...</klr> por konsciigi min pri la intereso
      enkonduki <tld/>an ekzamenon de Esperanto en la abiturienta
      ekzameno
      <fnt>
      <aut>trad. Lee J.-k.</aut>,
      <vrk><url
      ref="http://www.esperanto.co.kr/bbs/zboard.php?id=mia_taglibro">
      Traduka&jcirc;oj/Nova&jcirc;oj</url></vrk>,
      <lok>2007-05-06</lok>
      </fnt>.
    </ekz>
    <trdgrp lng="pl">
      <trd>fakultatywny</trd>,
      <trd>dobrowolny</trd>,
      <trd>dowolny</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>facultatif</trd>,
      <trd>optionnel</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>fakultativní</trd>,
    <trd>nepovinný</trd>,
    <trd>výběrový</trd>
  </trdgrp>
  <trd lng="sk">nepovinný</trd>
</drv>
</art>

