<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: efemer2.xml,v 1.10 2013/08/31 15:10:33 revo Exp $">
<kap>
  <rad>efemered</rad>/oj
</kap>
<drv mrk="efemer2.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="efemer.0oformaj">efemeroformaj
      insektoj</ref>
      (<trd lng="la">Ephemeridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Eintagsfliegen</trd>
  <trdgrp lng="en">
    <trd>dayflies</trd>,
    <trd>mayflies</trd>
  </trdgrp>
  <trd lng="hu">tarka <ind>k&eacute;r&eacute;szek</ind></trd>
  <trd lng="nl">eendagsvliegen<klr tip="ind"> (familie)</klr></trd>
  <trd lng="pt">efem&eacute;ridas</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_jo;&c_n;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;
  &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;)</klr></trd>
</drv>
</art>

