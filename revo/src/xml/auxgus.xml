<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxgus.xml,v 1.29 2013/09/06 18:10:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;gust</rad>/o
</kap>

<drv mrk="auxgus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="auxgus.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Oka <ref tip="lst" cel="monat.0o.KAL"
      lst="voko:monatoj" val="8">monato</ref>  de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> en la gregoria kalendaro.
    </dif>
    <rim>
      Vidu la rimarkon &ccirc;e <ref tip="vid" 
cel="januar.0o.RIM">januaro</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_zh;&c_n;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_a;&c_v;&c_g;&c_u;&c_s;&c_t;</trd>
  <trd lng="br">Eost<klr tip="ind"> (miz)</klr></trd>
  <trd lng="ca">agost</trd>
  <trd lng="cs">srpen</trd>
  <trd lng="de">August</trd>
  <trd lng="en">August</trd>
  <trd lng="es">agosto</trd>
  <trd lng="fr">ao&ucirc;t</trd>
  <trd lng="hu">augusztus</trd>
  <trd lng="he">&alef;&vav;&gimel;&vav;&samekh;&tet;</trd>
  <trd lng="id">Agustus</trd>
  <trd lng="it">agosto</trd>
  <trd lng="ja">&#20843;&#26376; [<ind>&#12399;&#12385;&#12364;&#12388;
</ind>]</trd>
  <trdgrp lng="lat">
  <trd>Sextilis</trd>, 
  <trd><ind>Augustus</ind> mensis</trd>
  </trdgrp>
  <trd lng="nl">augustus</trd>
  <trd lng="oc">agost</trd>
  <trd lng="pl">sierpie&nacute;</trd>
  <trd lng="ro">august</trd>
  <trd lng="ru">&c_a;&c_v;&c_g;&c_u;&c_s;&c_t;</trd>
  <trd lng="sk">august</trd>
  <trd lng="sv">augusti</trd>
  <trd lng="tr">a&gbreve;ustos</trd>
  <trd lng="vo">gustul</trd>
</drv>

<drv mrk="auxgus.A0o">
  <kap><ofc>*</ofc><tld lit="A"/>o</kap>
  <snc mrk="auxgus.A0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld lit="A"/>o estas mia plej amata filo
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">August</trd>
  <trd lng="de">August<klr tip="amb"> (Vorname)</klr></trd>
  <trd lng="fr">Auguste<klr tip="amb"> (pr&eacute;nom)</klr></trd>
  <trd lng="it">Augusto</trd>
  <trd lng="ja">&#12450;&#12454;&#12464;&#12473;&#12488;&#12453;&#12473; 
[<ind>&#12354;&#12358;&#12368;&#12377;&#12392;&#12357;&#12377;</ind>]</trd>
  <trd lng="lat">Augustus</trd>
  <trd lng="oc">August<klr tip="amb"> (prenom)</klr></trd>
  <trd lng="pl">August</trd>
</drv>
</art>
