<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klan.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>klan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klan.0o">
  <kap><tld/>o</kap>
  <snc mrk="klan.0o.ETN">
    <dif>
      Grupo de individuoj, kiuj konsideras
      sin kiel parencoj unu al aliaj, sed kiuj rekonas ekskluzive
      tiun parencecon per tio, ke ili havas saman 
      <ref cel="totem.0o">totemon</ref>:
      <ekz>
        la virinaj adeptoj <klr>...</klr> estas <klr>...</klr>
        rekrutataj en la <tld/>o al kiu apartenas la dio
        <fnt>
        <aut>Gbadamassi Latifou</aut>,
        <vrk>Vuduo en Benino</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 6a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gent.0o">gento</ref>,
      <ref cel="trib.0o">tribo</ref>
    </refgrp>
    <trd lng="ca">clan</trd>
    <trdgrp lng="de">
      <trd>Sippe</trd>,
      <trd>Clan</trd>
    </trdgrp>
    <trd lng="en">clan</trd>
    <trd lng="es">clan</trd>
    <trdgrp lng="hu">
      <trd>kl&aacute;n</trd>,
      <trd>nemzets&eacute;g</trd>
    </trdgrp>
    <trd lng="nl">stam<klr tip="ind"> (clan)</klr></trd>
    <trd lng="pl">klan</trd>
  </snc>
  <snc mrk="klan.0o.FIG">
    <dif>
      <ref tip="super" cel="grup.0acxo">Grupa&ccirc;o</ref> de
      intrigantoj, aparti&gcirc;ema partieto:
      <ekz>
        en Latvio ekzistas certaj <tld/>oj, kie entreprenoj estas
        kunligitaj kun la politiko
        <fnt>
        <aut>Antanas Grincevi&ccaron;ius</aut>,
        <vrk>Interpu&scirc;i&gcirc;ado en eksterlanda merkato</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 7a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kamari.0o">kamarilo</ref>,
      <ref cel="klik.0o.grupacxo">kliko</ref>,
      <ref cel="koteri.0o">koterio</ref>,
      <ref cel="sekt.0o.FIG">sekto</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_l;&c_a;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Kl&uuml;ngel</trd>,
      <trd>Sippschaft</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kl&aacute;n</trd>,
      <trd>&eacute;rdeksz&ouml;vets&eacute;g</trd>
    </trdgrp>
    <trd lng="nl">kliek</trd>
  </snc>
  <snc mrk="klan.0o.POL">
    <dif>
      En Skotlando kaj Irlando, grupo de familioj kun komuna prapatro, 
      komunaj kiltkoloroj ktp.
    </dif>
      <trd lng="en">clan</trd>
      <trd lng="nl">clan</trd>
      <trd lng="pl">klan</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>klan</trd>,
    <trd>politická klika</trd>,
    <trd>rod</trd>
  </trdgrp>
  <trd lng="fr">clan</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>kmeň; stranícka skupina intrigánov</trd>,
    <trd>rod</trd>
  </trdgrp>
</drv>
</art>
