<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fascio.xml,v 1.15 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>fasciol</rad>/o
</kap>
<drv mrk="fascio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super"
      cel="tremat.0oj">trematodoj</ref>
      (<trd lng="la">Fasciola</trd>), parazita platvermo, kiu vivas en
      la
      hepato de brutoj kaj homoj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>distoma hep&agrave;tic</trd>,
    <trd>g&egrave;nere Fasciola</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fasciola</trd>,
    <trd>motolice</trd>
  </trdgrp>
  <trd lng="de">Leberegel</trd>
  <trdgrp lng="fr">
    <trd>douve<klr tip="ind"> (ver)</klr></trd>,
    <trd>fasciole</trd></trdgrp>
  <trd lng="hu">m&aacute;jm&eacute;tely</trd>
  <trd lng="nl">leverbot</trd>
  <trdgrp lng="pt">
    <trd>fasc&iacute;ola</trd>,
    <trd>barata-do-f&iacute;gado</trd>,
    <trd>baratinha-do-f&iacute;gado</trd>,
    <trd>d&uacute;via</trd>,
    <trd>saguaip&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j; &c_g;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">pečeňová motolica <klr>(hlísta)</klr></trd>
</drv>
</art>
