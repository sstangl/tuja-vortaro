<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kares.xml,v 1.22 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kares</rad>/i
</kap>
<drv mrk="kares.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate kaj ame tu&scirc;eti:
      <ekz>
        selante &ccirc;evalon, oni &gcirc;in <tld/>as
        <klr>(oni devas flati tiun, de kiu oni postulas ion al li
        malagrablan)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i siajn lipharojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i al iu la barbon</ind>
        <klr>(flategi; <ref tip="vid" cel="lek.0i">leki</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="ca">
          <trd>fer la barbeta</trd>,
          <trd>afalagar</trd>
        </trdgrp>
        <trd lng="fr">flatter</trd>
        <trd lng="es">halagar</trd>
        <trd lng="en">flatter</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><tld/>i kontra&ubreve; la haroj</ind>
        <klr>(malflati, malpla&ccirc;i)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="ca">
          <trd>venir a rep&egrave;l</trd>,
          <trd>actuar a contracor</trd>
        </trdgrp>
        <trd lng="fr">prendre &agrave; <ind>rebrousse-poil</ind></trd>
        <trd lng="es">acariciar a <ind>contrapelo</ind></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i iluzion <klr>(dorloti)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lumo <tld/>as la murojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ondoj <tld/>is la dezertan bordon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>liebkosen</trd>,
      <trd>herzen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>liefkozen</trd>,
      <trd>aaien</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Plezurigi, feli&ccirc;igi:
      <ekz>
        tiu koloro <tld/>as la rigardon, la okulojn;
      </ekz>
      <ekz>
        min mono neniam <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viaj konsoloj <tld/>as mian vunditan animon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schmeicheln</trd>,
      <trd>gef&auml;llig sein</trd>
    </trdgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <snc>
    <dif>
      Favori, protekti:
      <ekz>
        kiun la sorto <tld/>as, al tiu &ccirc;io sukcesas
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dorlot.0i">dorloti</ref>.
    </refgrp>
    <trd lng="nl">strelen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>choura&ntilde;<klr tip="ind"> (floura&ntilde;)</klr></trd>,
    <trd>floura&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acariciar</trd>,
    <trd>amanyagar</trd>,
    <trd>acaronar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskat se <klr>(s někým)</klr></trd>,
    <trd>mazlit se</trd>
  </trdgrp>
  <trd lng="en">caress</trd>
  <trd lng="es">acariciar</trd>
  <trdgrp lng="fr">
    <trd>caresser</trd>,
    <trd>cajoler</trd>,
    <trd>c&acirc;liner</trd>,
    <trd>dorloter</trd>,
    <trd>effleurer</trd>,
    <trd>enj&ocirc;ler</trd>
    <trd>nourrir<klr tip="ind"> (un r&ecirc;ve, un espoir)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat</trd>,
    <trd>cir&oacute;gat</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hladkať sa <klr>(s niekým)</klr></trd>,
    <trd>láskať</trd>
  </trdgrp>
</drv>

<drv mrk="kares.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        ne ekzistas <tld/>o sen intereso
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ursa (mallerta) <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>allazig</trd>,
    <trd>chouradenn</trd>,
    <trd>flouradenn</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>car&iacute;cia</trd>,
    <trd>manyaga</trd>,
    <trd>moixaina</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskání</trd>,
    <trd>mazlení</trd>,
    <trd>pohlazení</trd>
  </trdgrp>
  <trd lng="es">caricia</trd>
  <trdgrp lng="fr">
    <trd>caresse</trd>,
    <trd>cajolerie</trd>,
    <trd>c&acirc;lin<klr tip="ind"> (subst.)</klr></trd>,
    <trd>effleurement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&aacute;s</trd>,
    <trd>cir&oacute;gat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozing</trd>,
    <trd>aai</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">maznanie</trd>
</drv>

<drv mrk="kares.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>anta:
      <ekz>
        <tld/>a tu&scirc;o, kiso;
      </ekz>
      <ekz>
        <tld/>aj vortoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_ja;&c_sh;&c_ch;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>chourus</trd>,
    <trd>flourus</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acariciador</trd>,
    <trd>amanyagador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laskavý</trd>,
    <trd>vlídný</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cari&ntilde;oso</trd>,
    <trd>acariciador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caressant</trd>,
    <trd>cajoleur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>c&acirc;lin<klr tip="ind"> (adj.)</klr></trd>,
    <trd>enjoleur<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>simogat&oacute;</trd>,
    <trd>cir&oacute;gat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>liefkozend</trd>,
    <trd>aaiend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>láskavý</trd>,
    <trd>vľúdny</trd>
  </trdgrp>
</drv>

<drv mrk="kares.mal0i">
  <kap>mal<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Malmilde trakti.
    </dif>
  </snc>
  <trd lng="br">gwallgas</trd>
  <trdgrp lng="ca">
    <trd>maltractar</trd>,
    <trd>tractar rudament</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsně zacházet</trd>,
    <trd>nevlídně zacházet</trd>
  </trdgrp>
  <trd lng="es">maltratar</trd>
  <trdgrp lng="fr">
    <trd>rudoyer</trd>,
    <trd>brutaliser</trd>,
    <trd>maltraiter</trd>,
    <trd>molester</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helybenhagy</trd>,
    <trd>elt&aacute;ng&aacute;l</trd>,
    <trd>megruh&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">ruw behandelen</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o; &c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_y;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">drsne zaobchádzať</trd>
</drv>
</art>
