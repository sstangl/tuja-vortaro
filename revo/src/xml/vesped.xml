<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vesped.xml,v 1.10 2013/08/31 15:10:38 revo Exp $">
<kap>
  <rad>vesped</rad>/oj
</kap>
<drv mrk="vesped.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Vespidae</trd>), kiuj faldas la flugilojn dum
      ripozo.
    </dif>
  </snc>
  <trd lng="ca">v&egrave;spids</trd>
  <trd lng="de">Faltenwespen</trd>
  <trd lng="hu">t&aacute;rsasdar&aacute;zsf&eacute;l&eacute;k</trd>
  <trd lng="nl">plooivleugelwespen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">vesp&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd><ind>&c_o;&c_s;&c_y;</ind> &c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_a;&c_t;&c_o;-&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <ind>&c_o;&c_s;&c_y;</ind> <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
