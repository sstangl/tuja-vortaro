<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sole.xml,v 1.17 2015/10/15 11:10:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sole</rad>/o
</kap>
<drv mrk="sole.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Plata mara bongusta <ref tip="lst" cel="fisx.0o"
      lst="voko:fi&scirc;oj">fi&scirc;o</ref>, kun iom langoforma korpo,
      kun okuloj sur unu flanko bruna makulita kaj kun nesimetria bu&scirc;o;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="pleuxr3.0oj">ple&ubreve;ronektedoj</ref>
      (<trd lng="la">Solea</trd>).
    </dif>
  </snc>
  <trd lng="ca">llenguado</trd>
  <trd lng="cs">l&yacute;tkov&yacute; sval</trd>
  <trd lng="de">Seezunge</trd>
  <trd lng="en">Solea <klr>(includes common sole)</klr></trd>
  <trd lng="fr">sole</trd>
  <trd lng="hu">nyelvhal</trd>
  <trd lng="nl">tong<klr tip="ind"> (vis)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_ja;&c_z;&c_y;&c_k;<klr tip="ind"> (&c_r;&c_y;&c_b;&c_a;)</klr></trd>,
    <trd>&c_s;&c_o;&c_l;&c_mol; <klr tip="amb">(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="sk">l&yacute;tkov&yacute; sval</trd>
</drv>
</art>
