<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arek.xml,v 1.6 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>arek</rad>/o
</kap>

<drv mrk="arek.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="arek.0nukso"><tld/>nukso</ref>
    <ekz>
      mi forgesis kunporti <tld/>an saketon. Franjo, &ccirc;u vi havas
      <tld/>ojn
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p. 397a</lok></fnt>?
    </ekz>
    <trd lng="ca">nou d'areca</trd>
    <trdgrp lng="de">
      <trd>Beteln&uuml;sse</trd>,
      <trd>Arekan&uuml;sse</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noix d'arec</trd>,
      <trd>noix de b&eacute;tel</trd>
    </trdgrp>
  </snc>
  <snc>
    <fnt><bib>PIV1</bib></fnt>
    <ref tip="dif" cel="arek.0ujo0arbo"><tld/>ujo, <tld/>arbo</ref>
    <trd lng="ca">areca</trd>
    <trdgrp lng="de">
      <trd>Betelpalmen</trd>,
      <trd>Arekapalmen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ar&eacute;quier</trd>,
      <trd>arec</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>areka</trd>,
    <trd>palma areková</trd>
  </trdgrp>
  <trd lng="sk">palma areková</trd>
</drv>

<drv mrk="arek.0nukso">
  <kap><tld/>nukso</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kern.0o.BOT">Kerno</ref> de la frukto de
      <tld/>arbo kun man&gcirc;ebla grajno.
    </dif>
  </snc>
  <trd lng="ca">nou d'areca</trd>
  <trdgrp lng="de">
    <trd>Beteln&uuml;sse</trd>,
    <trd>Arekan&uuml;sse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>noix d'arec</trd>,
    <trd>noix de b&eacute;tel</trd>
  </trdgrp>
</drv>

<drv mrk="arek.0ujo0arbo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="arek.0arbo">
    <dif>
      Genro de palmoj el Sudorientazio (<trd lng="la">Areca</trd>) ,
      el familio arekacoj; unu specio (Areca catechu) produktas
      man&gcirc;eblajn
      nuksojn.
    </dif>
  </snc>
  <trd lng="ca">areca</trd>
  <trdgrp lng="de">
    <trd>Betelpalmen</trd>,
    <trd>Arekapalmen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ar&eacute;quier</trd>,
    <trd>arec</trd>
  </trdgrp>
</drv>
</art>

