<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ondatr.xml,v 1.6 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>ondatr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ondatr.0o">
  <kap><tld/>o</kap>
  <snc mrk="ondatr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="mus.kampo0o"
      lst="voko:ron&gcirc;uloj">Kampomuso</ref>
       vivanta apud lagoj a&ubreve; riveroj en Nordameriko,
      bredata pro felo, disvastin&gcirc;inta tra E&ubreve;razio;
      unuspecia <ref tip="lst" cel="genr.0o.BIO" 
      lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="hamstr1.0oj">hamstredoj</ref>
      (<trd lng="la">Ondatra</trd>):
      <ekz>
        aliaj plantman&gcirc;antoj enmigris memstare, kiel <klr>...</klr>
        kastoroj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Oostvaardersplassen">
          Oostvaardersplassen</url>, 2012-10-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mosk.0ulo">moskulo</ref>
  </snc>
  <trd lng="bg">&c_o;&c_n;&c_d;&c_a;&c_t;&c_r;&c_a;</trd>
  <trd lng="cs">ondatra</trd>
  <trd lng="de">Bisamratte</trd>
  <trd lng="en">muscrat</trd>
  <trd lng="fr">rat musqu&eacute;</trd>
  <trd lng="sk">ondatra</trd>
</drv>
</art>

