<vortaro>
<art mrk="$Id: piastr.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>piastr</rad>/o
</kap>
<drv mrk="piastr.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Ar&gcirc;enta <ref tip="super" cel="mon.0ero">monero</ref>
      uzata en Hispanujo, Turkujo kaj orientaj landoj kun
      tre diversa valoro.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_ja;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">piastr</trd>
  <trd lng="de">Piaster</trd>
  <trd lng="fr">piastre</trd>
  <trd lng="hu">piaszter</trd>
  <trd lng="nl">piaster</trd>
  <trd lng="pt">piastra</trd>
  <trd lng="ru">&c_p;&c_i;&c_a;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">piaster</trd>
</drv>
</art>
