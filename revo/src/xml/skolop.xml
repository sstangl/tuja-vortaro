<art mrk="$Id: skolop.xml,v 1.16 2015/06/29 16:33:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skolop</rad>/o
</kap>
<drv mrk="skolop.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="skolop2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Scolopax</trd>) de longbekaj arbaraj birdoj,
      vivantaj
      en mezvarma klimato, &ccirc;asataj pro sia karno:
      <ekz>
        mi fortimigis kvin, ses <tld/>ojn; vi povas motpafi ilin,
        Arkadio
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 6a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_k;&c_a;&c_s;</trd>, 
    <trd>&c_s;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>, 
    <trd>&c_v;&c_a;&c_l;&c_mol;&c_d;&c_sh;&c_n;&c_e;&c_p;</trd>
  </trdgrp>
  <trd lng="cs">sluka lesn&iacute;</trd>
  <trd lng="de">Waldschnepfe</trd>
  <trd lng="en">woodcock</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;casse des bois</trd>
  </trdgrp>
  <trd lng="hu">erdei szalonka</trd>
  <trd lng="nl">houtsnip</trd>
  <trd lng="pl">s&lstroke;onka</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_k;&c_a;&c_s;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_mol;&c_d;&c_sh;&c_n;&c_je;&c_p;</trd>
  </trdgrp>
  <trd lng="sk">sluka otavn√°</trd>
</drv>
</art>
