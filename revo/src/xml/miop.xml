<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miop.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>miop</rad>/a
</kap>
<drv mrk="miop.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta nenormale mallongan vidkapablon, vidanta nur de proksime.
    </dif>
    <trd lng="ca">miop</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Neanta&ubreve;zorgema, neklarvida, nesagaca.
    </dif>
    <trd lng="ca">llosc</trd>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_u;&c_k;&c_ib;</trd>
  <trd lng="br">berrwel</trd>
  <trd lng="ca">miop</trd>
  <trdgrp lng="cs">
    <trd>krátkozraký</trd>,
    <trd>myopický</trd>
  </trdgrp>
  <trd lng="de">kurzsichtig</trd>
  <trdgrp lng="en">
    <trd>myopic</trd>,
    <trd>near-sighted</trd>,
    <trd>short-sighted</trd>
  </trdgrp>
  <trd lng="fr">myope</trd>
  <trd lng="hu">r&ouml;vidl&aacute;t&aacute;</trd>
  <trdgrp lng="nl">
    <trd>bijziend</trd>,
    <trd>myoop</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_i;&c_z;&c_o;&c_r;&c_u;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">krátkozraký</trd>
  <trdgrp lng="sv">
    <trd>myopisk</trd>,
    <trd>n&auml;rsynt</trd>
  </trdgrp>
</drv>

<drv mrk="miop.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta nenormale longan vidkapablon, vidanta nur de malproksime.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_z;&c_o;&c_r;&c_k;&c_ib;</trd>
  <trd lng="br">hirwel</trd>
  <trd lng="ca">hipermetrop</trd>
  <trd lng="cs">dalekozraký</trd>
  <trd lng="de">weitsichtig</trd>
  <trdgrp lng="en">
    <trd>far-sighted</trd>,
    <trd>long-sighted</trd>
  </trdgrp>
  <trd lng="fr">hyperm&eacute;trope</trd>
  <trd lng="hu">t&aacute;voll&aacute;t&aacute;</trd>
  <trdgrp lng="nl">
    <trd>verziend</trd>,
    <trd>hypermetroop</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_z;&c_o;&c_r;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">ďalekozraký</trd>
  <trdgrp lng="sv">
    <trd>&ouml;versynt</trd>,
    <trd>l&aring;ngsynt</trd>
  </trdgrp>
</drv>
</art>
