<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sifon.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sifon</rad>/o
</kap>
<drv mrk="sifon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aparato konsistanta el u-forma tubo kun neegalaj brakoj, per kiu
      oni
      transver&scirc;as flua&jcirc;ojn pro la premo de la atmosfero.
    </dif>
    <trd lng="en">siphon</trd>
    <trd lng="hu">szifon</trd>    
    <trd lng="nl">hevel</trd>
  </snc>
  <snc>
    <dif>
      Speciala botelo destinita por enteni kaj la&ubreve;vole ellasi per
      propra
      gaspremo gasan akvon.
    </dif>
    <trd lng="hu">sz&oacute;daszifon</trd>    
    <trdgrp lng="nl">
      <trd>sifon</trd>,
      <trd>spuitfles</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>násoska</trd>,
    <trd>sifon</trd>
  </trdgrp>
  <trd lng="de">Syphon</trd>
  <trd lng="ru">&c_s;&c_i;&c_f;&c_o;&c_n;</trd>
  <trd lng="sk">násoska</trd>
</drv>
</art>
