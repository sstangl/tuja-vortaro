<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: melis.xml,v 1.23 2015/05/30 23:10:15 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>melis</rad>/o
</kap>
<drv mrk="melis.0o">
  <kap>
  <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Melissa</trd>);
      <ref tip="lst" cel="stauxd.0o"
      lst="voko:lamioparencoj">sta&ubreve;do</ref>
      kun ovo- &gcirc;is kor-formaj kontra&ubreve;sidaj folioj citrone odorantaj,
      kiam
      oni 
      tu&scirc;as ilin;
      kun dulipaj floroj, kies supra lipo estas plata, unuloba, mallonge
      
      tridenta, blanka, flaveta a&ubreve; pale rozkolora, kaj kies
      
      malsupra
      lipo ests triloba, dudenta,
      kaj kun stiluso kaj kvar stamenoj pli longaj ol la korolo,
      el la stamenoj du estas mallongaj kaj du pli longaj.
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_je;&c_l;&c_i;&c_s;&c_a;</trd>
  <trd lng="de">Melisse</trd>
  <trd lng="en">melissa</trd>
  <trd lng="fr">m&eacute;lisse</trd>
  <trdgrp lng="hu">
    <trd>citromf&udblac;</trd>,
    <trd>melissza</trd>
  </trdgrp>
  <trd lng="nl">melisse</trd>
  <trd lng="pl">melisa</trd>
  <trdgrp lng="pt">
    <trd>melissa</trd>,
    <trd>erva-cidreira</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_i;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="melis.citron0o">
  <kap>
  citron<tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref>
       de <ref tip="lst" cel="melis.0o" lst="voko:spicoj"><tld/>o</ref>
      (<trd lng="la">Melissa officinalis</trd>), havanta
      citronan odoron kaj guston kaj uzata kiel
      <ref tip="super" cel="spic.0o">spico</ref>, por infuza&jcirc;oj kaj en
      farmacio.
    </dif>
  </snc>                                 
  <trd lng="bg">&c_m;&c_a;&c_t;&c_o;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="br">begar</trd>
  <trd lng="de">Zitronenmelisse</trd>
  <trdgrp lng="en">
    <trd>lemon <ind>balm</ind></trd>,
    <trd>balm <ind>mint</ind></trd>,
    <trd>balm</trd>,
  </trdgrp>
  <trd lng="fr">m&eacute;lisse citronelle</trd>
  <trdgrp lng="hu">
    <trd>citromszag&uacute; <ind>m&eacute;zf&udblac;</ind></trd>,
    <trd>kertim&eacute;hf&udblac;</trd>,
    <trd>m&eacute;regnyom&oacute;f&udblac;</trd>,
    <trd>citromszag&uacute; <ind>melissza</ind></trd>
  </trdgrp>
  <trd lng="nl">citroenmelisse</trd>
  <trdgrp lng="pl">
    <trd>melisa lekarska</trd>,
    <trd>cytrynowe <ind>ziele</ind> <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_i;&c_s;&c_s;&c_a; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_l;&c_i;&c_m;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_m;&c_ja;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
