<kap>
  <ofc>*</ofc>
  <rad>artrit</rad>/o
</kap>
<drv mrk="artrit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la artikoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="podagr.0o">podagro</ref>,
      <ref cel="reuxma.0o">re&ubreve;matismo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>artritida</trd>,
    <trd>artritis</trd>,
    <trd>dna</trd>,
    <trd>pakostnice</trd>,
    <trd>z&aacute;n&ecaron;t kloub&uring;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Arthritis</trd>,
     <trd>Gelenkentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">arthritis</trd>
  <trd lng="es">artritis</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_r;&a_t;&a_r;&f_y;&a_t;</trd>
    <trd>&a_w;&a_r;&a_m; &a_m;&a_f;&a_A;&a_S;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">arthrite</trd>
  <trdgrp lng="hu">
     <trd>iz&uuml;leti <ind>gyullad&aacute;s</ind></trd>,
     <trd>arthritis</trd>
  </trdgrp>
  <trd lng="id">artritis</trd>
  <trd lng="it">artrite</trd>
  <trdgrp lng="nl">
    <trd>gewrichtsontsteking</trd>,
    <trd>artritis</trd>
  </trdgrp>
  <trd lng="pt">artrite</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>artrit&iacute;da</trd>,
    <trd>z&aacute;pal k&lacute;bu</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>inflammation i en led</trd>,
    <trd>gikt</trd>
  </trdgrp>
</drv>
</art>
