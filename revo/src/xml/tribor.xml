<art mrk="$Id: tribor.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>tribord</rad>/o
</kap>
<drv mrk="tribor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Dekstra flanko de &scirc;ipo rigardante al la <ref tip="vid"
      cel="pru.0o">pruo</ref>:
      <ekz>
        la <tld/>o estas signita per verda fajro.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="babord.0o">babordo</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_shch;&c_i;&c_r;&c_b;&c_o;&c_r;&c_d;</trd>
  <trd lng="ca">estribord</trd>
  <trd lng="cs">pravobok</trd>
  <trd lng="de">Steuerbord</trd>
  <trd lng="en">starboard</trd>
  <trd lng="es">estribor</trd>
  <trd lng="fr">tribord</trd>
  <trd lng="hu">haj&oacute; <ind>jobb</ind> oldala</trd>
  <trd lng="nl">stuurboord</trd>
  <trdgrp lng="pl">
    <trd>sterburta</trd>,
    <trd>prawa <ind>burta</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>boreste</trd>,
    <trd>estibordo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_i;&c_r;&c_b;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_b;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">prav√° strana lode</trd>
</drv>
</art>
