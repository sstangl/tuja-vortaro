<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mole.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
   <rad>mole</rad>/o
</kap>
<drv mrk="mole.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MAR</uzo>
   <snc>
     <dif>
       Ligna a&ubreve; &scirc;tona digo konstruita anta&ubreve; la eniro de <ref
       tip="malprt"
       cel="haven.0o.MAR">haveno</ref> por &scirc;irmi &gcirc;in kontra&ubreve; la ondoj
       kaj
       sendan&gcirc;erigi la &scirc;ipojn:
       <ekz>
         sur la <tld/>o staris viro, li fumis kaj kra&ccirc;adis en la maron
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a, p. 71a</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_m;&c_o;&c_l;</trd>,
       <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>breakwater</trd>,
       <trd>mole</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>dique</trd>,
       <trd>muelle <klr tip="amb">(marino)</klr></trd>
       <trd>rompeolas</trd>
     </trdgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>molo</trd>,
    <trd>přístavní hráz</trd>
  </trdgrp>
  <trd lng="fr">m&ocirc;le</trd>
  <trdgrp lng="hu">
     <trd>m&oacute;l&oacute;</trd>,
     <trd>kik&ouml;t&odblac;g&aacute;t</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>havendam</trd>,
     <trd>pier<klr tip="ind"> (havendam)</klr></trd>
   </trdgrp>
  <trdgrp lng="pt">
   <trd>molhe</trd>,
   <trd>mole <klr>(molhe)</klr></trd>,
   <trd>quebra-mar</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_l;</trd>,
     <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>mólo</trd>,
    <trd>prístavná hrádza</trd>,
    <trd>vlnolam</trd>
  </trdgrp>
</drv>
</art>
