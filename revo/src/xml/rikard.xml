<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rikard.xml,v 1.7 2015/02/26 19:11:08 revo Exp $">
<kap>
  <rad>Rikard</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rikard.0o">
  <kap><tld/>o</kap>
  <snc mrk="rikard.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        eminenta traduko de <ctl><tld/>o Tria</ctl>
        <fnt><bib>Plumamikoj</bib>, <lok>tradukoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_R;&c_i;&c_ch;&c_a;&c_r;&c_d;</trd>
  <trd lng="de">Richard</trd>
  <trd lng="en">Richard</trd>
  <trd lng="fr">Richard</trd>
  <trd lng="it">Riccardo</trd>
  <trdgrp lng="pl">
    <trd>Ryszard</trd>,
    <trd>Rysiek<klr> (zdrob.)</klr></trd>
  </trdgrp>
  <trd lng="pt">Ricardo</trd>
  <trdgrp lng="ru">
    <trd>&c_R;&c_i;&c_h;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_R;&c_i;&c_ch;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_R;&c_i;&c_k;&c_a;&c_r;&c_d;&c_o;</trd>,
    <trd>&c_R;&c_i;&c_sh;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>

