<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lili.xml,v 1.32 2016/02/28 19:10:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lili</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lili.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lili.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Vegeta&jcirc;o kun bulbo kaj grandaj bonodoraj floroj,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Lilium</trd>):
      <ekz>
        <tld/>o estas simbolo de pureco;
      </ekz>
      <ekz>
        kial vi zorgas pri vesta&jcirc;o? Pripensu la <tld/>ojn de la
        kampo, kiel ili kreskas; ili ne laboras, nek &scirc;pinas
        <fnt><bib>NT</bib>, <lok>&Mat; 6:28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_l;&c_je;&c_ja;</trd>
  <trd lng="br">lilienn</trd>
  <trd lng="ca">lliri</trd>
  <trdgrp lng="cs">
    <trd>lilie</trd>,
    <trd>lilium</trd>
  </trdgrp>
  <trd lng="de">Lilie</trd>
  <trd lng="el">&kappa;&rho;&jota_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">lily</trd>
  <trd lng="es">lirio</trd>
  <trd lng="fa">&a_s;&a_w;&a_s;&a_n; &f_ch;&a_l;&f_ch;&a_r;&a_A;&a_gh;</trd>
  <trdgrp lng="fr">
    <trd>lis</trd>,
    <trd>lys</trd>
  </trdgrp>
  <trd lng="hu">liliom</trd>
  <trd lng="nl">lelie</trd>
  <trd lng="pl">lilia</trd>
  <trd lng="pt">l&iacute;rio</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">&lcaron;alia</trd>
  <trd lng="sv">lilja</trd>
</drv>

<drv mrk="lili.akvo0o">
  <kap>akvo<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="lili.akvo0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Vegeta&jcirc;o kreskanta en mar&ccirc;oj, kun flavaj floroj kaj
      verdaj botelformaj fruktoj,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" 
      cel="nimfea.0oj">nimfeacoj</ref>
      (<trd lng="la">Nuphar</trd>):
      <ekz>
        akvo<tld/>oj balanci&gcirc;adis sur la akvo
        <fnt><bib>Fab3</bib>,
        <lok>Ib kaj malgranda kristino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">nen&uacute;far</trd>
  <trdgrp lng="cs">
    <trd>lekn&iacute;n</trd>,
    <trd>lotos</trd>,
    <trd>nymfea</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Teichrose</trd>,
    <trd>Mummel</trd>
  </trdgrp>
  <trd lng="es">nenufar</trd>
  <trd lng="fa">&a_n;&f_y;&a_l;&a_w;&a_f;&a_r; &a_A_madda;&a_b;&f_y;</trd>
  <trd lng="fr">n&eacute;nuphar</trd>
  <trdgrp lng="hu">
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zit&ouml;k</trd>
  </trdgrp>
  <trd lng="nl">gele <ind>plomp</ind></trd>
  <trdgrp lng="pl">
    <trd>gr&aogonek;&zdot;el &zdot;&oacute;&lstroke;ty</trd>,
    <trd>b&aogonek;czywie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>g&oacute;lf&atilde;o</trd>,
    <trd>aguap&eacute;</trd>,    
    <trd>gigoga</trd>,
    <trd>golfo</trd>,
    <trd>jigoga</trd>,
    <trd>l&iacute;rio-d'&aacute;gua</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_y;&c_sh;&c_k;&c_a;
    <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr>
    </trd>,
    <trd>&c_zh;&c_jo;&c_l;&c_t;&c_a;&c_ja;
    <ind>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lekno</trd>,
    <trd>lotos</trd>
  </trdgrp>
  <adm>
   &Ccirc;u temas pri Nuphar lutea &ccirc;u pri Nymphaea alba?
   Flava estas la unua, sed la duan oni kutime nomas akvolilio
   a&ubreve; foje nenufaro.
   <aut>SC</aut>
  </adm>
</drv>

<drv mrk="lili.tago0o">
  <kap>tago<tld/>o, <var><kap>tag<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="lili.tago0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara vegeta&jcirc;o vivanta de E&ubreve;ropo &gcirc;is
      Orientazio, rizoma, kun belaj, unutage da&ubreve;rantaj floroj,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref> (<trd
      lng="la">Hemerocallis</trd>):
      <ekz>
        en &Ccirc;inio, anka&ubreve; ekzistas floro de patrino
        <klr>...</klr> 
        tago<tld/>o estas &gcirc;ia komunuza termino
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://es.chinabroadcast.cn/1/2004/05/07/1@8333.htm">
          Panjo estas plej amata...</url></vrk>,
          <lok>2004-05-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">hemerocal&middot;lis</trd>
  <trd lng="de">Taglilie</trd>
  <trd lng="fa">&a_z;&a_n;&a_b;&a_q; &a_r;&a_sh;&a_t;&f_y;</trd>
  <trd lng="fr">h&eacute;m&eacute;rocalle</trd>
  <trd lng="hu">s&aacute;sliliom</trd>
  <trd lng="nl">daglelie</trd>
  <trd lng="pl">liliowiec ogrodowy</trd>
  <trd lng="pt">hemerocale</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_o;&c_d;&c_n;&c_je;&c_v;</trd>,
    <trd>&c_l;&c_i;&c_l;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
