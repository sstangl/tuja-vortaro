<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: negx.xml,v 1.34 2016/01/02 11:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ne&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="negx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="negx.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Kristali&gcirc;inta akva vaporo, kiu defalas de la atmosfero en
      formo
      de
      blankaj malpezaj floketoj:
      <ekz>
        la <tld/>o estas <klr>...</klr> blanka
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o falis &ccirc;iam pli dense<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>li zorgas pri &gcirc;i kiel pri <tld/>o pasintjara</ind>
        <trd lng="fr">il s'en soucie comme de sa premi&egrave;re
        chemise</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_n;&c_je;&c_g;</trd>
  <trd lng="bg">&c_s;&c_n;&c_ja;&c_g;</trd>
  <trd lng="cs">sn&iacute;h</trd>
  <trd lng="de">Schnee</trd>
  <trd lng="el">&chi;&jota;&omikron_ton;&ny;&jota;</trd>
  <trd lng="en">snow</trd>
  <trd lng="es">nieve</trd>
  <trd lng="fa">&a_b;&a_r;&a_f;</trd>
  <trd lng="fr">neige</trd>
  <trd lng="he">&shin;&lamed;&#8503;</trd>
  <trd lng="hu">h&oacute;</trd>
  <trd lng="it">neve</trd>
  <trd lng="nl">sneeuw</trd>
  <trd lng="pt">neve</trd>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_g;</trd>
  <trd lng="sv">sn&ouml;</trd>
  <trd lng="tr">kar</trd>
</drv>

<drv mrk="negx.0i">
  <kap><tld/>i</kap>
  <snc mrk="negx.0i.MET">
    <dif>
      Fali <klr>(parolante pri <tld/>o)</klr>:
      <ekz>
        ekstere estas tre malvarme, <tld/>as kaj frostas
        <fnt><bib>Fab1</bib>, <lok>Elinjo-fingreto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_a;&c_l;&c_i; &c_s;&c_n;&c_ja;&c_g;</trd>
  <trd lng="de">schneien</trd>
  <trd lng="fa">&a_b;&a_r;&a_f; &a_b;&a_A;&a_r;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">neiger</trd>
  <trd lng="it">nevicare</trd>
  <trd lng="nl">sneeuwen</trd>
  <trd lng="sv">sn&ouml;a</trd>
  <trd lng="tr">kar ya&gbreve;mak</trd>
</drv>

<drv mrk="negx.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="negx.0ero.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Aparta floko de falanta <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_n;&c_ja;&c_zh;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_n;&c_je;&c_zh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="de">Schneeflocke</trd>
  <trd lng="es">copo de nieve</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_A;&a_n;&a_h;&a_hamza_sure; &a_b;&a_r;&a_f;</trd>,
    <trd>&a_b;&a_r;&a_f;&zwnj;&a_d;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_r;&a_f;&zwnj;&a_r;&f_y;&a_z;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">flocon de neige</trd>
  <trd lng="he">&pe;&tav;&yod;&tav; &shin;&lamed;&#8503;</trd>
  <trd lng="hu">h&oacute;pehely</trd>
  <trd lng="it">fiocco di <ind>neve</ind></trd>
  <trd lng="nl">sneeuwvlok</trd>
  <trd lng="pt">floco de neve</trd>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_zh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sv">sn&ouml;flinga</trd>
  <trd lng="tr">kar tanesi</trd>
</drv>

<drv mrk="negx.0ulino">
  <kap><tld lit="N"/>ulino <fnt>B</fnt></kap>
  <snc mrk="negx.0ulino.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Knabino blanka kiel <tld/>o, &ccirc;efpersono de fabelo de la
      fratoj
      Grimm:
      <ekz>
        Plej bela vi estas &ccirc;e ni, ho re&gcirc;ino,
        Sed miloble pli bela estas <tld lit="N"/>ulino
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_B;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_n;&c_je;&c_zh;&c_k;&c_a;</trd>
  <trd lng="de">Schneewittchen</trd>
  <trd lng="en">Snowhite</trd>
  <trd lng="es">Blanca Nieves</trd>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&a_b;&a_r;&a_f;&f_y;</trd>
  <trd lng="fr">Blanche Neige</trd>
  <trd lng="he">&shin;&lamed;&#8503;&yod;&he;</trd>
  <trd lng="hu">H&oacute;feh&eacute;rke</trd>
  <trd lng="it">Biancaneve</trd>
  <trd lng="nl">Sneeuwwitje</trd>
  <trd lng="pt">Branca de Neve</trd>
  <trd
  lng="ru">&c_B;&c_je;&c_l;&c_o;&c_s;&c_n;&c_je;&c_zh;&c_k;&c_a;</trd>
  <trd lng="tr">Pamuk Prenses</trd>
</drv>

<drv mrk="negx.0blovado">
  <kap><tld/>blovado</kap>
  <snc mrk="negx.0blovado.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Ventego kun <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ja;&c_c;&c_je;&c_l;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&a_r;&a_A;&a_n;</trd>,
    <trd>&a_t;&a_w;&a_f;&a_A;&a_n; &a_b;&a_r;&a_f;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blizzard</trd>,
    <trd>temp&ecirc;te de neige</trd>
  </trdgrp>
  <trd lng="he">&samekh;&vav;&pe;&tav;
  &shin;&lamed;&#8503;&yod;&fmem;</trd>
  <trd lng="hu">h&oacute;f&uacute;v&aacute;s</trd>
  <trd lng="it">tormenta</trd>
  <trd lng="nl">sneeuwstorm</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="negx.eterna0o">
  <kap>eterna <tld/>o, <var><kap>&ccirc;iama <tld/>o</kap></var></kap>
  <snc mrk="negx.eterna0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="N"/>a kovro, kiu en malvarma loko restas tra &ccirc;iuj
      sezonoj, 
      neniam akvi&gcirc;ante:
      <ekz>
        la hirundo levi&gcirc;is alte en la aeron, trans arbarojn kaj
        lagojn,
        alte trans la grandajn montojn, sur kiuj ku&scirc;as eterna
        <tld/>o
        <fnt>
        <bib>Fab1</bib>,
        <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili pasigos la nokton en Schwarzegg, montodometo, kiun la Alpa
        klubo konstruigis, meze de eternaj <tld/>oj, por ke la turistoj
        &gcirc;in
        utiligu kiel rifu&gcirc;ejon
        <fnt>
        <aut>Henri Vallienne</aut>,
        <vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej interesaj vidinda&jcirc;oj estas gla&ccirc;eroj, fjordoj
        kaj 
        altaj montaroj kun &ccirc;iama <tld/>o
        <fnt>
          <aut>H. St&uuml;cke, trad. C. Pinardon</aut>,
          <vrk><url
          ref="http://horizonte.free.fr/rakontoj/heinz/heinz-rakonto.html">
          &Ccirc;irka&ubreve; la Mondo per Biciklo</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="fa">&a_b;&a_r;&a_f; &a_h;&a_m;&f_y;&a_sh;&f_g;&f_y;</trd>
  <trd lng="fr">neiges &eacute;ternelles</trd>
  <trd lng="it">nevi eterne</trd>
  <trd lng="nl">eeuwige <ind>sneeuw</ind></trd>
</drv>

<drv mrk="negx.0bulo">
  <kap><tld/>bulo</kap>
  <snc mrk="negx.0bulo.SPO">
    <dif>
      Bulo el <tld/>o:
      <ekz>
        batu lin!... &Jcirc;etu <tld/>obulojn al li! &ndash; ili kriis
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schneeball</trd>
  <trd lng="en">snowball</trd>
  <trd lng="fa">&f_g;&a_l;&a_w;&a_l;&a_h;&a_hamza_sure;
  &a_b;&a_r;&a_f;</trd>
  <trd lng="fr">boule de neige</trd>
  <trd lng="it">palla di <ind>neve</ind></trd>
  <trd lng="nl">sneeuwbal</trd>
  <trd lng="sv">sn&ouml;boll</trd>
  <trd lng="tr">kar topu</trd>
</drv>
</art>
