<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekster1.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>eksterm</rad>/i
</kap>
<drv mrk="ekster1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plene pereigi, tute detrui, neniigi:
      <ekz>
        la fajro <tld/>os la palacojn
        <fnt><bib>MT</bib>, <lok>&Amo; 1:12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i popolon;
      </ekz>
      <ekz>
        <tld/>i urbon;
      </ekz>
      <ekz>
        <tld/>i la memoron pri
        <fnt><bib>MT</bib>, <lok>&Psa; 34:16</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i malbonan kutimon;
      </ekz>
      <ekz>
        ili estos <tld/>itaj kiel tute seka pajlo
        <fnt><bib>MT</bib>, <lok>&Nah; 1:10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj pereos de glavo k de malsato &gcirc;is ili tute
        <tld/>i&gcirc;os
        <fnt><bib>MT</bib>, <lok>&Jer; 44:27</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>anta fajro
        <fnt><bib>MT</bib>, <lok>&Psa; 18:8</lok></fnt>..
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="bucx.0i">bu&ccirc;i</ref>,
      <ref cel="cindr.0igi">cindrigi</ref>,
      <ref cel="polv">dispolvigi</ref>,
      <ref cel="radik.el0igi">elradikigi</ref>,
      <ref cel="visx.for0i">forvi&scirc;i</ref>,
      <ref cel="neni.0igi">neniigi</ref>.
    </refgrp>
  </snc>
  <adm>
  Mi volonte dirus, ke ~i normale apliki&gcirc;as al homoj,
  popoloj ks. La uzo "~i palacon", "~i kutimon" estas figursencaj.
  <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_ib;&c_ch;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_r;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausrotten</trd>,
    <trd>vertilgen</trd>,
    <trd>ausl&ouml;schen</trd>,
    <trd>vernichten</trd>
  </trdgrp>
  <trd lng="en">exterminate</trd>
  <trd lng="es">exterminar</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&f_y;&a_sh;&a_h;&zwnj;&f_k;&a_n;
    &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_n;&a_A;&a_b;&a_w;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>an&eacute;antir</trd>,
    <trd>exterminer</trd></trdgrp> 
  <trd lng="hu">ki&iacute;rt</trd>
  <trd lng="nl">uitroeien</trd>
  <trdgrp lng="pl">
    <trd>eksterminowa&cacute;</trd>,
    <trd>wyt&eogonek;pi&cacute;</trd>,
    <trd>wygubi&cacute;</trd>
  </trdgrp>
  <trd lng="pt">exterminar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_r;&c_je;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_o;&c_r;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">utrota</trd>
  <trdgrp lng="sk">
    <trd>vyhubiť</trd>,
    <trd>vykynožiť</trd>,
    <trd>zničiť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubit</trd>,
    <trd>sprovodit</trd>,
    <trd>vybít</trd>,
    <trd>vyhladit</trd>,
    <trd>vyhubit</trd>
  </trdgrp>
</drv>
</art>
