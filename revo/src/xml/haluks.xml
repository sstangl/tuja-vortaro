        <trd>&c_h;&c_a;&c_l;&c_u;&c_k;&c_s;</trd>,
        <trd>&c_p;&c_a;&c_l;&c_je;&c_c; &c_n;&c_a; &c_k;&c_r;&c_a;&c_k;&c_a;</trd>
