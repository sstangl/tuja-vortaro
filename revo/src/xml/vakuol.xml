<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vakuol.xml,v 1.5 2015/06/28 12:03:50 revo Exp $">
<kap>
  <rad>vakuol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vakuol.0o">
  <kap><tld/>o</kap>
  <snc mrk="vakuol.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Vezika <ref tip="super" cel="organ.0eto.FIZL">organeto</ref>
      entenanta flua&jcirc;on pli maldensan
      ol la &ccirc;irka&ubreve;a &ccirc;eloplasmo, trovebla precipe en vegeta&jcirc;aj &ccirc;eloj:
      <ekz>
        balantidio havas grandan kaj malgrandan nukleojn
        <klr>...</klr> kaj <tld/>on de sekreciado
        <fnt>
          <aut>O. A. Fischer</aut>,
          <vrk>Parazitoj de dombestoj</vrk>,
          <lok><url ref="http://www.volny.cz/malovec/start300.htm">
          Starto 2000:3 (194)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;iro de kukurbo estas multaj <tld/>oj, inter kiuj 
        cirkulas la plasmo
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Planta_movi%C4%9Do">
          Planta movi&gcirc;o</url>, 2010-08-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">vakuool</trd>
  <trd lng="ar">&#1700;&a_A;&f_k;&f_y;&a_w;&a_y_hamza;&#1734;&a_l;</trd>
  <trd lng="be">&c_v;&c_a;&c_k;&c_u;&c_o;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_a;&c_k;&c_u;&c_o;&c_l;&c_a;</trd>
  <trd lng="ca">vac&uacute;ol</trd>
  <trd lng="cs">vakuola</trd>
  <trd lng="da">vakuole</trd>
  <trd lng="de">Vakuole</trd>
  <trd lng="en">vacuole</trd>
  <trd lng="fr">vacuole</trd>
  <trdgrp lng="hu">
    <trd>sejtnedv&uuml;reg</trd>,
    <trd>vaku&oacute;lum</trd>
  </trdgrp>
  <trd lng="nl">vacuole</trd>
  <trd lng="pl">Wakuola</trd>
  <trd lng="sk">vakuola</trd>
</drv>
</art>

