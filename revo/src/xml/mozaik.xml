<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mozaik.xml,v 1.19 2015/11/19 08:10:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mozaik</rad>/o
</kap>
<drv mrk="mozaik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Bildo el koloraj &scirc;tonaj a&ubreve; vitraj kubetoj
      kuncementitaj, uzata por ornami grundon a&ubreve; muron.
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la libro estas agrabla <tld/>o da rakontoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_ib;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_z;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="cs">mozaika</trd>
  <trd lng="de">Mosaik</trd>
  <trd lng="en">mosaic</trd>
  <trd lng="es">mosaico</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_r;&a_q;&f_k;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>mosa&iuml;que</trd>
  </trdgrp>
  <trd lng="hu">mozaik</trd>
  <trd lng="nl">moza&iuml;ek</trd>
  <trd lng="pt">mosaico <klr tip="amb">(artes, arq.)</klr></trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_a;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">mozaika</trd>
</drv>
</art>
