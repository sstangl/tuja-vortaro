<kap>
  <rad>tipolog</rad>/o
</kap>
<drv mrk="tipolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="tipolog.0o.KLA">
    <dif>
      Konstruanto a&ubreve; konstruinto de
      <ref tip="vid" cel="tipologi.0o.KLA">tipologio</ref>:
      <ekz>
        &ccirc;iu <tld/>o devus klare scii <klr>...</klr>
        sur kiu tereno li konstruas, sub kiu lumo kaj
        perspektivo li vidas sian tipon
        <fnt>
          <aut>T. Haecker, trad. V. O&scaron;lak</aut>,
          <vrk>Vergilio, la patro de Okcidento</vrk>,
          <lok>1998</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sistematik.0isto.SCI">sistematikisto</ref>
    </refgrp>
  </snc>
  <trd lng="fr">typologiste</trd>
</drv>
</art>

