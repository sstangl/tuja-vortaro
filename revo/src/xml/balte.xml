<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: balte.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>balte</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="balte.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
    Rubando, rimeno el ledo a&ubreve; &scirc;tofo sur&scirc;ultre
    a&ubreve; trabruste portata, servanta por subteni glavon, sakon
    a&ubreve;
    alian peza&jcirc;on.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>balteus</trd>,
    <trd>baltheus</trd>,
    <trd>pás přes rameno</trd>,
    <trd>svinutý pás vymezený volutami jónské hlavice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schulterriemen</trd>,
    <trd>Degengeh&auml;nge</trd>,
    <trd>Wehrgeh&auml;nge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>baldrick</trd>,
    <trd>shoulder belt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bandouli&egrave;re</trd>,
    <trd>baudrier</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;llsz&iacute;j</trd>
  <trd lng="nl">schouderriem</trd>
  <trd lng="pl">pendent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_u;&c_p;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>náplecný nosný remeň</trd>,
    <trd>pás alebo stuha</trd>
  </trdgrp>
</drv>
</art>
