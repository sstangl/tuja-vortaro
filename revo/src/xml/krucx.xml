<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krucx.xml,v 1.22 2015/04/18 20:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kru&ccirc;</rad>/o
</kap>
<drv mrk="krucx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krucx.0o.VAZ">
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref> el diversaj substancoj,
      kun
      lar&gcirc;a ventro, tenilo kaj ver&scirc;ilo, destinita por enteni flua&jcirc;on:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>longe &ccirc;erpas la <tld/>o, &gcirc;is &gcirc;i fine rompi&gcirc;as</ind>
        <klr>(kiu tro ofte sin riskas al dan&gcirc;ero, fine falas en
        &gcirc;in)</klr>
        <trd lng="es">tanto va el c&aacute;ntaro a la fuente que al final se
        rompe</trd>
        <trdgrp lng="fr">
          <trd>tant va la cruche &agrave; l'eau, qu'elle se casse</trd>
        </trdgrp>
        <trd lng="hu">addig j&aacute;r a kors&oacute; a k&uacute;tra, m&iacute;g el nem
        t&ouml;rik</trd>
        <trd lng="nl">de <ind>kruik</ind> gaat zo lang te water tot ze
        breekt</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Arkadio trinkis akvon, kiun la mastrino de l' gastejo alportis
        al
        li en fera <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 2a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_l;&c_a;&c_ch;</trd>,
    <trd>&c_z;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_f;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">gerra</trd>
  <trdgrp lng="de">
    <trd>Krug</trd>,
    <trd>Kanne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jug</trd>,
    <trd>cruse</trd>,
    <trd>ewer</trd>,
    <trd>pitcher</trd>,
    <trd>pot</trd>,
    <trd>vessel</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>c&aacute;ntaro</trd>,
    <trd>jarra</trd>,
    <trd>jarr&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>broc</trd>,
    <trd>cruche</trd>,
    <trd>pichet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kors&oacute;</trd>,
    <trd>kancs&oacute;</trd>,
    <trd>kanna</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kruik</trd>,
    <trd>kan<klr tip="ind"> (kruik)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>jarro</trd>,
    <trd>jarra</trd>,
    <trd>bilha</trd>,
    <trd>c&acirc;ntaro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>testi</trd>,
    <trd>s&uuml;rahi</trd>
  </trdgrp>
</drv>

<drv mrk="krucx.kaf0o">
  <kap>kaf<tld/>o</kap>
  <snc mrk="krucx.kaf0o.VAZ">
    <dif>
      <ref tip="super" cel="krucx.0o"><tld lit="K"/>o</ref> por fari,
      konservi      varma a&ubreve; ver&scirc;i kafon.
    </dif>
  </snc>
  <trd lng="de">Kaffeekanne</trd>
  <trd lng="en">coffee pot</trd>
  <trd lng="ca">cafetera</trd>
  <trd lng="es">cafetera</trd>
  <trd lng="fr">cafeti&egrave;re<klr tip="amb"> (verseuse)</klr></trd>
  <trd lng="hu">k&aacute;v&eacute;skanna</trd>
  <trd lng="it">caffettiera</trd>
  <trd lng="nl">koffiekan</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_j;&c_n;&c_i;&c_k;</trd>
  <trd lng="tr">cezve</trd>
</drv>

<drv mrk="krucx.te0o">
  <kap>te<tld/>o</kap>
  <snc mrk="krucx.te0o.VAZ">
    <dif>
      <ref tip="super" cel="krucx.0o"><tld lit="K"/>o</ref> por konservi
      varma kaj ver&scirc;i teon.
    </dif>
  </snc>
  <trd lng="de">Teekanne</trd>
  <trd lng="es">tetera</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;i&egrave;re<klr> (verseuse)</klr></trd>
  </trdgrp>
  <trd lng="hu">te&aacute;skanna</trd>
  <trd lng="ca">tetera</trd>
  <trd lng="nl">theekan</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_a;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j; <ind>&c_ch;&c_a;&c_j;&c_n;&c_i;&c_k;</ind></trd>
  <trd lng="tr">&ccedil;aydanl&inodot;k</trd>
</drv>
</art>
