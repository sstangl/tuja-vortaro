<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fizion.xml,v 1.20 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fizionomi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fizion.0o">
  <kap><tld/>o</kap>
  <snc mrk="fizion.0o.MIEN">
    <dif>
      Tuto de la viza&gcirc;aj trajtoj kun ilia esprimo:
      <ekz>
        pla&ccirc;a <tld/>o;
      </ekz>
      <ekz>
        dankoplena <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de feli&ccirc;ulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mien.0o.ANA">mieno</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>fesomia</trd>,
      <trd>faccions</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>Physiognomie</trd>,
     <trd>Gesichtsausdruck</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;br&aacute;zat</trd>,
      <trd>fizimiska</trd>,
      <trd>fiziogn&oacute;mia</trd>
    </trdgrp>
    <trd lng="pt">fisionomia</trd>
  </snc>
  <snc mrk="fizion.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karakteriza aspekto de iu afero:
      <ekz>
        &ccirc;iu popolo havas sian apartan <tld/>on;
      </ekz>
      <ekz>
        priskribi la <tld/>on de proceso, de urbo, de lando;
      </ekz>
      <ekz>
        Esperanto havas sian propran spiriton kaj
        <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la traduko beda&ubreve;rinde &scirc;an&gcirc;is la <tld/>on de la
        verko;
      </ekz>
      <ekz>
        la homoj plej volonte akceptas la proponojn, kiuj havas
        instruitan <klr>(sciencan)</klr> <tld/>on<fnt>Z</fnt>.
      </ekz>  
    </dif>
    <trdgrp lng="ca">
      <trd>trets</trd>,
      <trd>caracter&iacute;stiques</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_z;&c_ib;&c_ja;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_f;&c_i;&c_z;&c_i;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>fyziognomie</trd>,
    <trd>vizáž</trd>,
    <trd>vzezření</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gesicht</trd>,
     <trd>Gepr&auml;ge</trd>,
     <trd>Charakteristika</trd></trdgrp>
  <trdgrp lng="en">
     <trd>physiognomy</trd>,
     <trd>facial features</trd>,
     <trd>facial expression</trd> 
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_q;&f_y;&a_A;&a_f;&a_h;</trd>,
    <trd>&a_s;&f_y;&a_m;&a_A;</trd>,
    <trd>&f_ch;&a_h;&a_r;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>physionomie</trd>,
    <trd>allure<klr tip="ind"> (aspect)</klr></trd>,
    <trd>aspect<klr tip="ind"> (allure)</klr></trd>,
    <trd>caract&egrave;re<klr tip="ind"> (physionomie)</klr></trd>,
    <trd>visage<klr tip="ind"> (physionomie)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>arculat</trd>,
    <trd>jellegzetess&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>fysionomie</trd>,
    <trd>fysiognomie</trd>,
    <trd>gelaatsuitdrukking</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">výraz tváre</trd>
</drv>
</art>
