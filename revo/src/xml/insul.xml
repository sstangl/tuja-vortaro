<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: insul.xml,v 1.31 2015/09/21 05:10:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>insul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="insul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="insul.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto, &ccirc;iuflanke &ccirc;irka&ubreve;ata de akvo.
      <ekz>
        ni devas transflugi la grandan maron, kaj sur nia vojo
        trovi&gcirc;as neniu <tld/>o, sur kiu ni povus pasigi la nokton
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        akurate kombitaj <tld/>etoj <klr>(izolitaj tufoj)</klr> de la
        ru&gcirc;aj haroj sur lia brila kalvo atestis sufi&ccirc;an
        atenton al sia propra persono
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>inzula</trd>,
    <trd>ostrov</trd>
  </trdgrp>
  <trd lng="de">Insel</trd>
  <trd lng="en">island</trd>
  <trd lng="es">isla</trd>
  <trd lng="fa">&a_j;&a_z;&f_y;&a_r;&a_h;</trd>
  <trd lng="fr">ile</trd>
  <trd lng="id">pulau</trd>
  <trd lng="it">isola</trd>
  <trd lng="nl">eiland</trd>
  <trd lng="no">&oslash;y</trd>
  <trd lng="pl">wyspa</trd>
  <trd lng="pt">ilha</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
  <trd lng="sk">ostrov</trd>
  <trd lng="tr">ada</trd>
  <trd lng="vo">nisul</trd>
</drv>

<drv mrk="insul.0a">
  <kap><tld/>a</kap>
  <snc mrk="insul.0a.GEOG">
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">Insel-</trd>
  <trd lng="es">insular</trd>
  <trd lng="fa">&a_j;&a_z;&f_y;&a_r;&a_h;&zwnj;&a_A;&f_y;</trd>
  <trd lng="fr">insulaire<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="it">insulare</trd>
  <trd lng="no">&oslash;y-</trd>
  <trd lng="pl">wyspiarski</trd>
  <trd lng="pt">insular</trd>
  <trd lng="tr">adaya ait</trd>
</drv>

<drv mrk="insul.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="insul.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        ceteraj <tld/>anoj, kiuj havis malsanojn, venis kaj estis
        sanigitaj
        <fnt><bib>NT</bib>, <lok>&Ago; 28:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">ostrovan</trd>
  <trd lng="de">Inselbewohner</trd>
  <trd lng="en">islander</trd>
  <trd lng="es">isle&ntilde;o</trd>
  <trd
  lng="fa">&a_j;&a_z;&f_y;&a_r;&a_h;&zwnj;&a_n;&a_sh;&f_y;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>insulaire<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ilien<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="it">isolano</trd>
  <trd lng="nl">eilandbewoner</trd>
  <trd lng="no">&oslash;yboer</trd>
  <trd lng="pl">wyspiarz</trd>
  <trdgrp lng="pt">
   <trd>insulano</trd>, 
   <trd>ilh&eacute;u<klr tip="amb"> (pessoa)</klr></trd> 
  </trdgrp>
  <trd lng="ru">
  &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">ostrovan</trd>
  <trd lng="tr">adal&inodot;</trd>
  <trd lng="vo">nisulel</trd>
</drv>

<drv mrk="insul.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="insul.0aro.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Grupo de <tld/>oj.
    </dif>
    <ref tip="sin" cel="arkipe.0o.insularo">arkipelago</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_p;&c_e;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">souostrov&iacute;</trd>
  <trdgrp lng="de">
    <trd>Inselgruppe</trd>,
    <trd>Archipel</trd>
  </trdgrp>
  <trd lng="en">archipelago</trd>
  <trd lng="es">archipi&eacute;lago</trd>
  <trd
  lng="fa">&a_m;&a_j;&a_m;&a_ayn;&zwnj;&a_A;&a_l;&a_j;&a_z;&a_A;&f_y;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>archipel</trd>,
    <trd>chapelet d'iles</trd>
  </trdgrp>
  <trd lng="it">arcipelago</trd>
  <trd lng="nl">archipel</trd>
  <trd lng="no">arkipel</trd>
  <trd lng="pl">archipelag</trd>
  <trd lng="pt">arquip&eacute;lago</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
  <trd lng="sk">s&uacute;ostrovie</trd>
  <trd lng="tr">tak&inodot;mada</trd>
</drv>

<drv mrk="insul.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="insul.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malvasta <tld/>o:
      <ekz>
        <tld/>eto, nomata Ka&ubreve;da
        <fnt><bib>NT</bib>, <lok>&Ago; 27:16</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">ostr&uring;vek</trd>
  <trd lng="fr">ilot<klr tip="ind"> (petite ile)</klr></trd>
  <trdgrp lng="it">
   <trd>isoletta</trd>, 
   <trd>isolotto</trd> 
  </trdgrp>
  <trd lng="pl">wysepka</trd>
  <trdgrp lng="pt">
   <trd>ilhota</trd>, 
   <trd>ilh&eacute;u</trd> 
  </trdgrp>
  <trd lng="sk">ostrov&ccaron;ek</trd>
  <trd lng="tr">adac&inodot;k</trd>
</drv>

<drv mrk="insul.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="insul.duon0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto &ccirc;irka&ubreve;ata de akvo escepte de unu flanko,
      per kiu &gcirc;i kuni&gcirc;as kun la kontinento:
      <ekz>
        la itala duon<tld/>o;
      </ekz>
      <ekz>
        la Jutlanda duon<tld/>o
        <fnt><bib>Fab4</bib>,
        <lok>filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;</trd>
  <trd lng="cs">poloostrov</trd>
  <trd lng="de">Halbinsel</trd>
  <trd lng="en">peninsula</trd>
  <trd lng="es">pen&iacute;nsula</trd>
  <trd lng="fa">&a_sh;&a_b;&a_h;&zwnj;&a_j;&a_z;&f_y;&a_r;&a_h;</trd>
  <trd lng="fr">p&eacute;ninsule</trd>
  <trd lng="it">penisola</trd>
  <trd lng="nl">schiereiland</trd>
  <trd lng="no">halv&oslash;y</trd>
  <trd lng="pl">p&oacute;&lstroke;wysep</trd>
  <trd lng="pt">pen&iacute;nsula</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
  <trd lng="sk">polostrov</trd>
  <trd lng="tr">yar&inodot;mada</trd>
</drv>

<drv mrk="insul.I0odePrincoEduardo">
  <kap><tld lit="I"/>o de Princo Eduardo</kap>
  <snc mrk="insul.I0odePrincoEduardo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de
      <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref>
      en &gcirc;ia orienta parto:
      <ekz>
        <klr>...</klr> Akadianoj lo&gcirc;antaj <klr>...</klr> en partoj
        de
        la Kanadaj &ccirc;e-Atlantikaj provincoj Nov-Brunsviko,
        Nova-Skotio,
        <tld lit="I"/>o de Princo Eduardo
        <fnt>
          <aut>C. Richard</aut>,
          <vrk>PIV2: Akadio kaj franc-lingvaj Kanadanoj</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>, 1999-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Prince Edward Island</trd>
  <trd lng="fr">Ile-du-Prince-&Eacute;douard</trd>
  <trd lng="pl">Wyspa Ksi&eogonek;cia Edwarda</trd>
  <trd lng="tr">Prens Edward Adas&inodot;</trd>
</drv>
</art>
