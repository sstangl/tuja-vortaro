<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komedon.xml,v 1.4 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>komedon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="komedon.0o">
  <kap><tld/>o</kap>
  <snc mrk="komedon.0o.KOS">
    <dif>
      &Scirc;topi&gcirc;o de
      <ref tip="vid" cel="por1.0o.ANA">poro</ref>
      pro sebumo ka&ubreve;zanta pustuleton:
      <ekz>
        Aureliano Jos&eacute; frotas klingon kontra&ubreve; ledan bendon por unuafoje 
        razi sin; sangas liaj <tld/>oj, vundi&gcirc;as lia supra lipo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>komedo</trd>,
    <trd>komedon</trd>,
    <trd>uher</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>com&eacute;don</trd>,
    <trd>point noir</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komedo</trd>,
    <trd>uhor</trd>
  </trdgrp>
</drv>
</art>

