<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ramn.xml,v 1.17 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>ramn</rad>/o
</kap>
<drv mrk="ramn.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="arbust.0o"
      lst="voko:rozoparencoj">Arbusto</ref>
      a&ubreve; arbeto multspecia, mondvaste trovebla, ofte kun dornoj,
      simplaj segilrandaj folioj, malgrandaj verde-blankaj a&ubreve; flavetaj
      floroj kun po kvar a&ubreve; kvin stamenoj, korolaj kaj kalikaj folioj,
      berosimilaj drupoj enhavantaj flavan kolorilon kaj kies kernoj
      estas
      disvastigataj de birdoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el la familio <ref
      tip="malprt"
      cel="ramnac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Rhamnus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">řešetlák počistivý</trd>
  <trd lng="de">Kreuzdorn</trd>
  <trd lng="fr">nerprun</trd>
  <trd lng="hu">benge</trd>
  <trd lng="nl">rhamnus</trd>
  <trd lng="pt">ramnale</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">rešetliak prečisťujúci</trd>
</drv>
</art>
