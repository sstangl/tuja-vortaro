<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: orient2.xml,v 1.14 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>orientalist</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="orient2.0o">
  <kap><tld/>o</kap>
  <snc mrk="orient2.0o.SCI">
    <dif>
      Scienculo, kiu studas kaj esploras la orientajn lingvojn,
      literaturojn, kulturojn; specialisto pri
      <ref tip="vid" cel="orientalistik.0o.SCI">orientalistiko</ref>:
      <ekz>
        la&ubreve; la <tld/>o Carl Brockelmann, hebreoj 
        konservis sian lingvon dum la kaptado babilona
        <fnt>
          <aut>Jepibo</aut>,
          <vrk><url
          ref="http://jopeible.blogs.uv.es/2011/04/14/ne-forlasi-malnovajn-vojojn-pro-novajn/">
          Ne forlasi malnovajn vojojn pro novaj</url></vrk>,
          <lok>2011-04-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_s;&c_h;&c_o;&c_d;&c_a;&c_z;&c_n;&c_a;&c_v;&c_je;&c_c;</trd>,
      <trd>&c_u;&c_s;&c_h;&c_o;&c_d;&c_a;&c_z;&c_n;&c_a;&c_w;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
    </trdgrp>
    <trd lng="ca">orientalista</trd>
    <trd lng="de">Orientalist</trd>
    <trd lng="fr">orientaliste</trd>
    <trdgrp lng="hu">
      <trd>keletkutat&oacute;</trd>,
      <trd>orientalista</trd>
    </trdgrp>
    <trd lng="nl">ori&euml;ntalist</trd>
    <trd lng="pt">orientalista <klr tip="amb">(subst.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_k;&c_o;&c_v;&c_je;&c_d;</trd>,
      <trd>&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">orientalista</trd>
  <trd lng="sk">orientalista</trd>
</drv>
</art>
