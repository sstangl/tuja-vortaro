<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: subjun.xml,v 1.17 2015/07/04 08:37:44 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>subjunktiv</rad>/o
    </kap>

    <drv mrk="subjun.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">LIN</uzo>
      <snc>
	<dif>
	  <ref tip="super" cel="modal.verba0o">Verba modalo</ref> ne
	  ekzistanta en Esperanto kaj montranta, ke iu
	  propozicio dependas de alia kaj ke iu ago estas konceptata
	  kiel dependa de alia ago, do kiel duba kaj necerta.
	</dif>
	<refgrp tip="vid">
	  <ref cel="voliti">volitivo</ref>.
	</refgrp>
      </snc>
  <trd lng="be">&c_u;&c_m;&c_o;&c_w;&c_n;&c_y; &c_l;&c_a;&c_d;</trd>
  <trd lng="ca">subjuntiu</trd>
  <trdgrp lng="cs">
    <trd>spojovací způsob</trd>,
    <trd>subjunktiv</trd>
  </trdgrp>
  <trdgrp lng="de">
	<trd>Subjunktiv</trd>,
	<trd>Konjunktiv</trd>
      </trdgrp>
  <trd lng="en">subjunctive</trd>
  <trd lng="es">subjuntivo</trd>
  <trd lng="fa">&a_w;&a_j;&a_h; &a_A;&a_l;&a_t;&a_z;&a_A;&a_m;&f_y;</trd>
  <trd lng="fr">subjonctif</trd>
  <trdgrp lng="hu">
	<trd>k&ouml;t&odblac;m&oacute;d</trd>,
	<trd>coniunctivus</trd>
      </trdgrp>
  <trdgrp lng="nl">
	<trd>subjunctief</trd>,
	<trd>conjunctief</trd>,
	<trd>aanvoegende <ind>wijs</ind></trd>
      </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
      &c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>spojovací spôsob</trd>,
    <trd>subjunktív</trd>
  </trdgrp>
</drv>
  </art>
