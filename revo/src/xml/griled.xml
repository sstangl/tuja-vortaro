  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">syrsor</trd>
</drv>
