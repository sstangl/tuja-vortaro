  <trd lng="en">crickets<klr tip="amb"> (insect)</klr></trd>
  <trd lng="fr">gryllid&eacute;s</trd>
  <trd lng="hr">zrikavci</trd>
  <trd lng="hu">val&oacute;di <ind>t&uuml;csk&ouml;k</ind><klr tip="ind">
  (csal&aacute;d)</klr></trd>
