<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mate1.xml,v 1.17 2015/02/26 19:11:34 revo Exp $">
<kap>
  <rad>Mate</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="mate1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        kaj Jesuo <klr>...</klr> vidis viron nomatan <tld/>o, sidantan
        &ccirc;e la impostejo
        <fnt>
          <bib>NT</bib>,
          <lok>&Mat; 9:9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                    
  <trd lng="be">&c_M;&c_a;&c_c;&c_je;&c_j;</trd>
  <trd lng="bg">&c_M;&c_a;&c_t;&c_je;&c_j;</trd>
  <trd lng="br">Mazhev</trd>
  <trdgrp lng="ca">
    <trd>Mateu</trd>,
    <trd>Maties</trd>
  </trdgrp>
  <trd lng="de">Matth&auml;us</trd>
  <trd lng="en">Matthew</trd>
  <trd lng="fr">Matthieu</trd>
  <trd lng="hu">M&aacute;t&eacute;</trd>
  <trdgrp lng="it">
    <trd>Matteo</trd>,
    <trd>Mattia</trd>
  </trdgrp>
  <trd lng="nl">Mathias</trd>
  <trd lng="pl">Mateusz</trd>
  <trd lng="pt">Mateus</trd>
  <trdgrp lng="ru">
    <trd>&c_M;&c_a;&c_t;&c_v;&c_je;&c_j;</trd>,
    <trd>&c_M;&c_a;&c_t;&c_f;&c_je;&c_j;<klr>
    (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>
  </trdgrp>
</drv>
</art>
