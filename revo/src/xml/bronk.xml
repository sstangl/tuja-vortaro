<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: bronk.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>bronk</rad>/o
    </kap>

    <drv mrk="bronk.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">ANA</uzo>
      <snc>
        <dif>
          &Ccirc;iu el amba&ubreve; kanaloj, kiuj sekvas post la
          <ref cel="trahxe.0o">tra&hcirc;eo</ref> kaj kondukas
          la aeron en la pulmojn.  
        </dif>
      </snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_h;</trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_n;&c_h;&c_i;</trd>
  <trdgrp lng="cs">
    <trd>bronchus</trd>,
    <trd>průduška</trd>
  </trdgrp>
  <trd lng="de">Bronchus</trd>
  <trd lng="en">bronchial tube</trd>
  <trdgrp lng="fa">
        <trd>&a_n;&a_A;&f_y;&f_zh;&a_h;</trd>,
        <trd>&a_b;&a_r;&a_w;&a_n;&a_sh;</trd>
      </trdgrp>
  <trd lng="fr">bronche</trd>
  <trd lng="it">bronco</trd>
  <trd lng="nl">&eacute;&eacute;n van beide <ind>bronchi&euml;n</ind></trd>
  <trd lng="pl">oskrzela</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_h;</trd>
  <trd lng="sk">prieduška</trd>
</drv>
  </art>
