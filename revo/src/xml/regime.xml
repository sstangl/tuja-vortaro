<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: regime.xml,v 1.18 2015/08/24 18:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>regiment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="regime.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="regime.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Parto de armeo, konsistanta  el kelkaj
      <ref tip="prt" cel="batali.0o.MIL">batalionoj</ref>
      a&ubreve; <ref tip="prt" cel="skadr.0o.MIL">skadroj</ref>:
      <ekz>
        la oficira rango responda al <tld/>estro estas
        <ref cel="kolone.0o">kolonelo</ref>;
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        li ne estas el la <tld/>o de timuloj
        <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="divizi.0o">divizio</ref>,
      <ref cel="legi.0o">legio</ref>
    </refgrp>
    <refgrp tip="prt">
      <ref cel="tacxme.0o.MIL">ta&ccirc;mento</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_k;</trd>
  <trd lng="ca">regiment</trd>
  <trdgrp lng="cs">
    <trd>pluk</trd>,
    <trd>zast. regiment</trd>
  </trdgrp>
  <trd lng="de">Regiment</trd>
  <trd lng="en">regiment</trd>
  <trd lng="fr">r&eacute;giment</trd>
  <trd lng="hu">ezred</trd>
  <trd lng="it">reggimento</trd>
  <trd lng="nl">regiment</trd>
  <trd lng="pl">regiment</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_k;</trd>
  <trd lng="sk">pluk</trd>
</drv>
</art>
