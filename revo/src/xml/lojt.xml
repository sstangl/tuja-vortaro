<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lojt.xml,v 1.17 2013/10/07 19:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lojt</rad>/o
</kap>
<drv mrk="lojt.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Man&gcirc;ebla <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      gado
      vivanta en nesala akvo, kun longa mallar&gcirc;a korpo, du dorsna&gcirc;iloj,
      kies 
      anta&ubreve;a etendi&gcirc;as la&ubreve;longe de la duona korpo, &gcirc;ia ha&ubreve;to havas
      brunajn kaj
      nigrajn 
      makuloj sur brune flava fono;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la familio <ref
      tip="malprt" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Lota</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_n;&c_t;&c_u;&c_z;</trd>
  <trdgrp lng="de">
    <trd>Quappe</trd>,
    <trd>Tr&uuml;sche</trd>,
    <trd>Ruppe</trd>,
    <trd>Aalrutte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burbot</trd>,
    <trd>eel pout</trd>
  </trdgrp>
  <trd lng="fr">lotte</trd>
  <trd lng="hu">menyhal</trd>
  <trd lng="nl">kwabaal</trd>
  <trdgrp lng="pt">
    <trd>barboto <klr>(&ocirc;)</klr></trd>,
    <trd>barbota <klr>(PT)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_m;</trd>
</drv>
</art>
