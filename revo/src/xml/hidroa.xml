<kap>
  <rad>hidroaeroplan</rad>/o
</kap>
<drv mrk="hidroa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="hidrop.0o">hidroplano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Wasserflugzeug</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&pi;&lambda;&alfa_ton;&ny;&omikron;</trd>
  <trd lng="hu">hidropl&aacute;n</trd>
  <trd lng="it">idrovolante</trd>
  <trd lng="nl">watervliegtuig</trd>
  <trdgrp lng="pl">
    <trd>hydroplan</trd>, <trd>wodnop&lstroke;atowiec</trd>
  </trdgrp>
  <trd lng="es">hidroavi&oacute;n</trd>
  <trdgrp lng="pt">
    <trd>hidroavi&atilde;o</trd>,
    <trd>hidravi&atilde;o</trd>,
    <trd>hidroplano</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
