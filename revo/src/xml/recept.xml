<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: recept.xml,v 1.21 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>recept</rad>/o
</kap>
<drv mrk="recept.0o">
  <kap><tld/>o</kap>
  <snc mrk="recept.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Skriba klarigo pri preparado de man&gcirc;a&jcirc;o, trinka&jcirc;o
      a&ubreve; ia teknika&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili postulas, ke ni rebaku la tutan lingvon
        la&ubreve; ilia teoria <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>recipe</trd>,
      <trd>formula</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_d;&a_s;&a_t;&a_w;&a_r; &a_A_madda;&a_sh;&f_p;&a_z;&f_y;</trd>,
      <trd>&a_T;&a_r;&a_z; &a_t;&a_h;&f_y;&a_h;</trd>,
      <trd>&a_d;&a_s;&a_t;&a_w;&a_r;&a_A;&a_l;&a_ayn;&a_m;&a_l;</trd>
    </trdgrp>
    <trd lng="he">&mem;&tav;&kaf;&vav;&fnun;</trd>
    <trd lng="hu">&eacute;telrecept</trd>
  </snc>
  <snc mrk="recept.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Skriba formulo pri preparado kaj uzado de kuracilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="preskr.0o.MED">preskribo</ref>.
    </refgrp>
    <trd lng="ca">prescripci&oacute;</trd>
    <trd lng="en">prescription</trd>
    <trd lng="fa">&a_n;&a_s;&a_kh;&a_h;&a_hamza_sure; &a_d;&a_A;&a_r;&a_w;</trd>
    <trd lng="he">&mem;&resh;&shin;&fmem;</trd>
    <trd lng="tr">re&ccedil;ete</trd>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_c;&c_e;&c_p;&c_t;</trd>
  <trd lng="bg">&c_r;&c_je;&c_c;&c_je;&c_p;&c_t;&c_a;</trd>
  <trd lng="ca">recepta</trd>
  <trdgrp lng="cs">
    <trd>lékopis</trd>,
    <trd>předpis <klr>(lék, kuch.)</klr></trd>,
    <trd>recepis</trd>,
    <trd>recept</trd>
  </trdgrp>
  <trd lng="de">Rezept</trd>
  <trd lng="es">receta</trd>
  <trd lng="fr">recette</trd>
  <trd lng="he">&resh;&tsadi;&pe;&tet;</trd>
  <trdgrp lng="hu">
    <trd>recept</trd>,
    <trd>v&eacute;ny</trd>,
    <trd>rendelv&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">recept</trd>
  <trd lng="ru">&c_r;&c_je;&c_c;&c_je;&c_p;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>liekopis</trd>,
    <trd>predpis</trd>,
    <trd>recept</trd>
  </trdgrp>
</drv>
</art>
