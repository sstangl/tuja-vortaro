<vortaro>
<art mrk="$Id: numid.xml,v 1.15 2015/09/18 14:10:16 revo Exp $">
<kap>
  <rad>numid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="numid.0o">
  <kap><tld/>o</kap>
  <snc mrk="numid.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="bird.korto0o.AGR">Kortobirdo</ref>
      el familio de kokoj, kun griza nigre punktita plumaro kaj
      delikata viando:
      <ekz>
        sur herbo promenis <klr>...</klr> &scirc;prucmakulaj mukokulaj 
        <tld/>oj
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/V.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>pintada</trd>,
    <trd>gallina de Guinea</trd>    
  </trdgrp>
  <trd lng="cs">perli&ccaron;ka</trd>
  <trd lng="de">Perlhuhn</trd>
  <trd lng="fr">pintade</trd>
  <trd lng="hu">gy&ouml;ngyty&uacute;k</trd>
  <trd lng="nl">parelhoen</trd>
  <trd lng="pl">perliczka</trd>
  <trd lng="pt">galinha d'angola</trd>
  <trd lng="ru">&c_c;&c_je;&c_s;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">perli&ccaron;ka oby&ccaron;ajn&aacute;</trd>
</drv>
</art>
