<art mrk="$Id: akile.xml,v 1.18 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akile</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akile.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Achillea</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n; <ind>&c_r;&c_a;&c_v;&c_n;&c_je;&c_c;</ind></trd>
  <trd lng="cs">řebříček</trd>
  <trd lng="de">Schafgarbe</trd>
  <trdgrp lng="en">
    <trd>yarrow</trd>,
    <trd>milfoil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>achill&eacute;e</trd>,
    <trd>millefeuille</trd>
  </trdgrp>
  <trd lng="hu">cickafark</trd>
  <trd lng="nl">duizendblad</trd>
  <trd lng="ru">&c_t;&c_y;&c_s;&c_ja;&c_ch;&c_je;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">myší chvost</trd>
  <trd lng="sv">r&ouml;lleka</trd>
</drv>
</art>
