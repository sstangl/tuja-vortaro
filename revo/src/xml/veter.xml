<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: veter.xml,v 1.24 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>veter</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="veter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="veter.0o.MET">
    <tezrad fak="MET"/>
    <uzo tip="fak">MET</uzo>
    <dif>
      Stato de la atmosfero:
      <ekz>
        <ind>aprila <tld/>o, trompa aero</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">en avril ne te d&eacute;couvre pas d'un fil</trd>
        <trd lng="hu">tavasz - ravasz</trd>
      </ekz>
      <ekz>
        en trankvila <tld/>o &ccirc;iu remas sen dan&gcirc;ero
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bela, frosta, kota, malbona <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_v;&c_o;&c_r;'&c_je;</trd>
  <trdgrp lng="ca">
    <trd>temps</trd>,
    <trd>oratge</trd>
  </trdgrp>
  <trd lng="cs">počasí</trd>
  <trd lng="de">Wetter</trd>
  <trd lng="el">&kappa;&alfa;&jota;&rho;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">weather</trd>
  <trd lng="es">tiempo<klr> (meteorol&oacute;gico)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_w; &a_h;&a_w;&a_A;</trd>,
    <trd>&a_w;&a_D;&a_ayn; &a_h;&a_w;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;o</trd>,
    <trd>temps<klr> (qu'il fait)</klr></trd>
  </trdgrp>
  <trd lng="hu">id&odblac;j&aacute;r&aacute;s</trd>
  <trd lng="id">cuaca</trd>
  <trd lng="nl">weer</trd>
  <trd lng="pt">tempo</trd>
  <trd lng="ru">&c_p;&c_o;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">po&ccaron;asie</trd>
  <trd lng="tr">hava <klr>(hava derecesi, g&uuml;ne&scedil;li hava vesaire)
  </klr></trd>
</drv>
</art>
