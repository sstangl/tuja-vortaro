<kap>
  <rad>sensac</rad>/o
</kap>
<drv mrk="sensac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <ref tip="dif" cel="sensac1.0o.fil">Sensacio <sncref/></ref>
    <ekz>
      homdrama situacio de pasioj, sentoj, emocioj kaj
      <tld/>oj<fnt><aut>Christian Declerck</aut>: <vrk>Dua kvaropo,
      sproniva kiel la unua</vrk>, <bib>Monato</bib><lok>jaro 1994a,
      numero 4a, p. 22a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">senzace</trd>
  <trdgrp lng="de">
    <trd>Empfindung</trd>,
    <trd>Sinneswahrnehmung</trd>
  </trdgrp>
  <trd lng="en">sensation</trd>
  <trdgrp lng="fr">
    <trd>perception</trd>,
    <trd>sensation<klr> (perception)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;rzet</trd>
  <trdgrp lng="it">
    <trd>percezione</trd>,
    <trd>sensazione<klr> (percezione)</klr></trd>
  </trdgrp>
  <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  <trdgrp lng="pl">
    <trd>sensacja</trd>,
    <trd>wra&zdot;enie</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">senz&aacute;cia</trd>
</drv>
</art>
