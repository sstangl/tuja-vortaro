<kap><rad>fenikoptered</rad>/oj</kap>
<drv mrk="feniko1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="feniko.0oformaj">fenikopteroformaj
      birdoj</ref> (<trd lng="la">Phoenicopteridae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">plameňákovití</trd>
  <trd lng="fr">ph&oelig;nicopt&eacute;rid&eacute;s</trd>
  <trd lng="hu">flaming&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="nl">flamingo's</trd>
  <trd lng="pt">fenicopter&iacute;deo</trd>
  <trd lng="sk">plameniakovité</trd>
</drv>
</art>

