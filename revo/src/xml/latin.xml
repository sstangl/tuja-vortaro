<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: latin.xml,v 1.14 2014/10/26 02:10:21 revo Exp $">
    <kap>
      <rad>Latin</rad>/o<fnt>Z</fnt>
    </kap>

    <drv mrk="latin.0o">
      <kap><tld/>o</kap>
      <snc mrk="latin.0o.LIN">
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">LIN</uzo>
        <dif>
          <ref tip="super" cel="hind.0euxropa">Hinde&ubreve;ropa</ref>
          <ref tip="lst" cel="lingv.0o" lst="voko:lingvoj">lingvo</ref>,
          
          en kiu parolis antikvaj <ref
          cel="lati.0ano">latianoj</ref>,
          poste la romanoj kaj la okcidentaj
          <ref tip="vid" cel="romi.0ano">romianoj</ref>,
          plu uzata interalie de la
          <ref tip="vid" cel="katoli.rom0a">Romkatolika</ref>
          eklezio:
          <ekz>
            &scirc;ajnas, ke Izidoro de Sevilo (m. 636) unua proklamis
            <tld/>on
            <ctl>sankta lingvo</ctl>;
            <fnt>
              <aut>S. Pokrovskij</aut>: <vrk>Slavonaj skriboj</vrk>.
              <bib>LOdE</bib>, <lok>1999:2 (n-ro 52), p. 13a</lok>
            </fnt>;
          </ekz><ekz>
            la tradicio esperantigi <tld lit="l"/>ajn nomojn pruvas,
            ke, kiel dirus Neves, Esperanto estas nepinjo de <tld/>o
            <fnt>
              <aut>Jorge CAMACHO</aut>: <vrk>Pretas jen
              monument'!</vrk>.
              <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            li bone sciis ofendi neniun: li ridis la <tld/>on de la
            seminarioj kaj defendis &ccirc;e alia okazo sian episkopon
            <fnt>
              <aut>I. Turgenjev, tr. Kabe</aut>:
              <vrk>Patroj kaj filoj</vrk>.
              <lok>Inko Tyres&ouml; 2001. &#8208; &Ccirc;ap. 21a.</lok>
            </fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            Esperanto estas la <tld lit="l"/>o de la demokratio;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>venis fino al mia <tld lit="l"/>o</ind>
            <klr>(el&ccirc;erpi&gcirc;is mia tuta scio)</klr>
            <trd lng="fr">je donne ma langue au chat</trd>
            <trd lng="nl">ik ben aan het eind van mijn
            <ind>Latijn</ind></trd>
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <tld lit="l"/>igi tekston.
          </ekz>
        </dif>
      </snc>
      <ref tip="vid" cel="lati.0o.HIS">Latio<sncref/></ref>
      <trdgrp lng="be">
        <trd>&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
        <trd>&c_l;&c_a;&c_c;&c_ib;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
      </trdgrp>
      <trd lng="bg">&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i; &c_je;&c_z;&c_i;&c_k;</trd>
      <trd lng="de">Latein</trd>
      <trd lng="br">latin</trd>
      <trd lng="el">&lambda;&alfa;&tau;&jota;&ny;&jota;&kappa;&alfa_ton;</trd>
      <trd lng="en">Latin</trd>
      <trd lng="fr">latin</trd>
      <trd lng="hu">latin</trd>
      <trd lng="id">Latin</trd>
      <trd lng="nl">Latijn</trd>
      <trd lng="pt">latim</trd>
      <trdgrp lng="ru">
        <trd>&c_l;&c_a;&c_t;&c_y;&c_n;&c_mol;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="latin.0idaj.lingvoj">
      <kap><tld lit="l"/>idaj lingvoj</kap>
      <snc mrk="latin.0idaj.lingvoj.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Grupo da parencaj
          <ref tip="super" cel="hind.0euxropa">hinde&ubreve;ropaj</ref>
          lingvoj,
          devenintaj el <tld/>o:
          la itala, la hispana, la portugala, la franca kaj aliaj.
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib;&c_ja; &c_m;&c_o;&c_v;&c_y;</trd>
      <trdgrp lng="br">
        <trd>yezho&ugrave; <ind>latin</ind></trd>,
        <trd>yezho&ugrave; <ind>romanek</ind></trd>
      </trdgrp>
      <trd lng="de">romanische Sprachen</trd>
      <trdgrp lng="en">
        <trd>Romance languages</trd>,
        <trd>Romanic languages</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>langues n&eacute;olatines</trd>,
        <trd>langues romanes</trd>
      </trdgrp>
      <trd lng="hu">latin nyelvek</trd>
      <trd lng="id"><ind>bahasa</ind> Romanik</trd>
      <trd lng="nl">Romaanse talen</trd>
      <trd lng="pt">l&iacute;nguas latinas</trd>
      <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je;
        &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
    </drv>
  </art>
