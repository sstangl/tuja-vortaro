<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: infan.xml,v 1.40 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>infan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="infan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="infan.0o.juna">
    <dif>
      Homido kontraste al plenkreskinta homo, &gcirc;is la 12a jaro
      proksimume:
      <ekz>
      parolante pri <tld/>o, pri kiu ni scias, ke &gcirc;i ne estas knabino
      [...], ni povas uzi la vorton "li"<fnt><bib>LR</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>o ploras, &ccirc;ar &gcirc;i volas man&gcirc;i
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        en <tld/>o vidi&gcirc;as, kia homo fari&gcirc;os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        gardejo por <tld/>oj.
      </ekz>
    </dif>
    <ref tip="super" cel="id.0o.BIO">ido<sncref/></ref>
    <refgrp tip="sub">
      <ref cel="bub.0o">bubo</ref>,
      <ref cel="knab.0o">knabo</ref>
    </refgrp>
  </snc>
  <snc mrk="infan.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <ref tip="dif" cel="id.0o.GEN">ido<sncref/></ref>
    <ekz>
      li havas dek unu <tld/>ojn
      <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>;
    </ekz>
    <ekz>
      por patrino ne ekzistas <tld/>o malbela
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      li kondukis la patron de la <tld/>ino
      <fnt><bib>NT</bib>, <lok>&Mar; 5:40</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_c;&c_ja;</trd>,
    <trd>&c_d;&c_z;&c_ib;&c_c;&c_jo;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>nen</trd>,
    <trd>xiquet</trd>,
    <trd>infant</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&iacute;t&ecaron;</trd>,
    <trd>dítě</trd>,
    <trd>děti</trd>,
    <trd>děťátko</trd>
  </trdgrp>
  <trd lng="de">Kind</trd>
  <trd lng="en">child</trd>
  <trd lng="es">ni&ntilde;o</trd>
  <trd lng="fr">enfant</trd>
  <trdgrp lng="hu">
    <trd>gyerek</trd>,
    <trd>gyermek</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>anak</ind>-anak</trd>,
    <trd><ind>anak</ind> kecil</trd>
  </trdgrp>
  <trd lng="it">bambino</trd>
  <trdgrp lng="ku">
    <trd>zarok</trd>,
    <trd>mindal</trd>
  </trdgrp>
  <trd lng="nl">kind</trd>
  <trd lng="pl">dziecko</trd>
  <trd lng="pt">crian&ccedil;a</trd>
  <trd lng="ro">copil</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_i;&c_t;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">dieťa</trd>
  <trd lng="sv">barn</trd>
  <trd lng="tr">&ccedil;ocuk</trd>
  <trd lng="uk">&c_d;&c_i;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="vo">cil</trd>
  <trdgrp lng="zh">
    <trd>&#23401;&#23376;<klr> [h&aacute;izi]</klr></trd>,
    <trd>&#23376;<klr> [z&#464;]</klr></trd>,
    <trd>&#20799;</trd>,
    <trd><klr>tr. </klr>&#20818;<klr> [&eacute;r]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="infan.0a">
  <kap><tld/>a</kap>
  <snc mrk="infan.0a.deInfano">
    <dif>
      Rilata, apartenanta al <tld/>o:
      <ekz>
        <tld/>a ludo, vesto, pupo, klaso.
      </ekz>
    </dif>
    <trd lng="de">Kinder-</trd>
    <trdgrp lng="hu">
      <trd>gyerek-</trd>,
      <trd>gyermek-</trd>
    </trdgrp>
    <trd lng="nl">kinder-</trd>
  </snc>
  <snc mrk="infan.0a.sensperta">
    <dif>
      Naive sensperta kaj senprudenta:
      <ekz>
        <tld/>a argumento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a senzorgeco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kindlich</trd>,
      <trd>kindisch</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>gyerekes</trd>,
      <trd>gyermekes</trd>,
      <trd>gyermeki</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>[<ind>kanak</ind>] kekanak-kanakan</trd>,
      <trd>naif</trd>
    </trdgrp>
    <trd lng="nl">kinderlijk</trd>
    <trd lng="sv">barnslig</trd>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>infantil</trd>,
    <trd>pueril</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dětinský</trd>,
    <trd>dětský</trd>
  </trdgrp>
  <trd lng="es">infantil</trd>
  <trdgrp lng="fr">
    <trd>enfantin</trd>,
    <trd>infantile</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>infantile</trd>,
    <trd>bambinesco</trd>
  </trdgrp>
  <trd lng="pl">dzieci&eogonek;cy</trd>
  <trd lng="pt">infantil</trd>
  <trd lng="ro">de <ind>copil</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>detský</trd>,
    <trd>nerozumný</trd>,
    <trd>pren: neskúsený</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_i;&c_t;&c_ja;&c_ch;&c_i;&c_j;</trd>
  <trd lng="vo">cilik</trd>
</drv>

<drv mrk="infan.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="infan.0eco.KOMUNE">
    <dif>
      Stato de infano:
      <ekz>
        li diras, ke en la <tld/>eco la nutristino lin kontuzis
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno
        1a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agx.infan0o">infana&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_c;&c_ib;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">inf&agrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>dětinskost</trd>,
    <trd>dětinství</trd>
  </trdgrp>
  <trd lng="fr">enfance</trd>
  <trd lng="id"><ind>masa</ind> kanak-kanak</trd>
  <trd lng="it">infanzia</trd>
  <trd lng="pl">dzieci&nacute;stwo</trd>
  <trd lng="pt">inf&acirc;ncia</trd>
  <trd lng="ro">copil&abreve;rie</trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">infantilizmus</trd>
</drv>

<drv mrk="infan.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="infan.0eto.bebo">
    <dif>
      Malgranda, juna infano, <ref tip="vid"
      cel="et.0ulo">etulo</ref> 
      a&ubreve;
      <ref tip="vid" cel="beb.0o">bebo</ref>:
      <ekz>
        oni venigis al li anka&ubreve; siajn <tld/>etojn, por ke li 
        tu&scirc;u ilin
        <fnt><bib>NT</bib>, <lok>&Luk; 18:15</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="id">bayi</trd>
  </snc>
  <snc mrk="infan.0eto.feto">
    <dif>
      Homa <ref tip="vid" cel="fet.0o">feto</ref>:
      <ekz>
        kiam Elizabeto a&ubreve;dis la saluton de Maria, la <tld/>eto 
        eksaltis en
        &scirc;ia ventro
        <fnt><bib>NT</bib>, <lok>&Luk; 1:41</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="id">janin</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>miny&oacute;</trd>,
    <trd>beb&eacute;</trd>,
    <trd>nounat</trd>
  </trdgrp>
  <trd lng="cs">děťátko</trd>
  <trd lng="fr">petit<klr tip="amb"> (enfant)</klr></trd>
  <trdgrp lng="it">
    <trd>pargolo</trd>,
    <trd>infante</trd>,
    <trd>bambinello</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>berbe&cacute;</trd>,
    <trd>bobas</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>criancinha</trd>,
    <trd>beb&ecirc;</trd>,
    <trd>nen&ecirc;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_y;&c_sh;</trd>
  <trdgrp lng="sk">
    <trd>dieťatko</trd>,
    <trd>pachoľa</trd>
  </trdgrp>
</drv>

<drv mrk="infan.0istino">
  <kap><ofc>*</ofc><tld/>istino</kap>
  <snc mrk="infan.0istino.PED">
    <uzo tip="fak">PED</uzo>
    <ref tip="dif" cel="vart.0istino">vartistino</ref>
    <ekz>
      ni havas diversajn servantojn: kuiriston, &ccirc;ambristinon, 
      <tld/>istinon
      <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_n;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_n;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_h;&c_a;&c_v;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>mainadera</trd>,
    <trd>puericultora</trd>
  </trdgrp>
  <trd lng="cs">chůva</trd>
  <trdgrp lng="de">
    <trd>Kinderg&auml;rtnerin</trd>,
    <trd>Erzieherin</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assistante maternelle</trd>,
    <trd>pu&eacute;ricultrice</trd>
  </trdgrp><trdgrp lng="fr">
    <trd>maestra d'infanzia</trd>,
    <trd>puericultrice</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dada</trd>,
    <trd>dajka</trd>
  </trdgrp>
  <trd lng="id">pramusiwi</trd>
  <trd lng="nl">kinderverzorgster</trd>
  <trdgrp lng="pl">
    <trd>bona</trd>,
    <trd>opiekunka</trd>,
  </trdgrp>
  <trd lng="pt">ama-seca</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_ja;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">pestúnka</trd>
</drv>

<drv mrk="infan.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="infan.0umi.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Afekti <tld/>ecan teni&gcirc;on:
      <ekz>
        ni &ccirc;esu <tld/>umi
        <fnt>
          <aut>F. Dostojevskij, trad. G. Arosev</aut>,
          <vrk>Blankaj noktoj</vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">b&ecirc;tifier</trd>
</drv>

<drv mrk="infan.mir0o">
  <kap>mir<tld/>o</kap>
  <snc mrk="infan.mir0o.PED">
    <dif>
      <tld lit="I"/>o havanta mirigajn kapablojn:
      <ekz>
        Mozarto estis vera mir<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_n;&c_d;&c_e;&c_r;&c_k;&c_ib;&c_n;&c_d;</trd>
  <trd lng="ca">nen <ind>prodigi</ind></trd>
  <trdgrp lng="cs">
    <trd>udivující dítě</trd>,
    <trd>zázračné dítě</trd>
  </trdgrp>
  <trd lng="de">Wunderkind</trd>
  <trdgrp lng="fr">
    <trd>enfant prodige</trd>,
    <trd>enfant <ind>surdou&eacute;</ind></trd>
  </trdgrp>
  <trd lng="hu">csodagyerek</trd>
  <trd lng="it">bambino prodigio</trd>
  <trd lng="nl">wonderkind</trd>
  <trd lng="pl">cudowne <ind>dziecko</ind></trd>
  <trd lng="pt">crian&ccedil;a prod&iacute;gio</trd>
  <trd lng="ru">&c_v;&c_u;&c_n;&c_d;&c_je;&c_r;&c_k;&c_i;&c_n;&c_d;</trd>
  <trd lng="sk">zázračné dieťa</trd>
  <trd lng="sv">underbarn</trd>
  <trd lng="uk">&c_v;&c_u;&c_n;&c_d;&c_je;&c_r;&c_k;&c_ib;&c_n;&c_d;</trd>
</drv>

<drv mrk="infan.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="infan.sen0a.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Kiu ne naskis a&ubreve; naskigis, ne havas a&ubreve; ne plu havas 
      idojn:
      <ekz>
        estis do sep fratoj; kaj la unua edzi&gcirc;is, kaj mortis 
        sen<tld/>a
        <fnt><bib>NT</bib>, <lok>&Luk; 20:29</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="steril.0a.KOMUNE">sterila <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd><ind>sense </ind>descend&egrave;ncia</trd>,
    <trd>est&egrave;ril</trd>
  </trdgrp>
  <trd lng="cs">bezdětný</trd>
  <trd lng="de">kinderlos</trd>
  <trd lng="fr">sans descendance</trd>
  <trdgrp lng="pl">
    <trd>bezdzietny</trd>,
    <trd>bezpotomny</trd>,
  </trdgrp>
  <trd lng="pt">sem <ind>filho</ind></trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">bezdetný</trd>
</drv>

<drv mrk="infan.sol0o">
  <kap>sol<tld/>o</kap>
  <snc mrk="infan.sol0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Ununura ido de siaj gepatroj:
      <ekz>
        ruinigita nobelo, kiu eble iam revis pli brilantan sorton
        por sia  sol<tld/>o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;&c_je; 
    &c_d;&c_z;&c_ib;&c_c;&c_ja;</trd>
  <trd lng="ca">fill <ind>&uacute;nic</ind></trd>
  <trd lng="cs">jedináček</trd>
  <trd lng="de">Einzelkind</trd>
  <trd lng="fr">enfant unique</trd>
  <trdgrp lng="id">
    <trd><ind>anak</ind> semata wayang</trd>,
    <trd><ind>anak</ind> tunggal</trd>
  </trdgrp>
  <trd lng="it">figlio unico</trd>
  <trd lng="pl">jedynak</trd>
  <trd lng="pt">filho &uacute;nico</trd>
  <trd lng="ru">
    &c_je;&c_d;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j; 
    &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">jedináčik</trd>
</drv>

<drv mrk="infan.sucx0o">
  <kap>su&ccirc;<tld/>o</kap>
  <snc>
    <dif>
      Juna <ref tip="super" cel="infan.0o"><tld/>o</ref> ankora&ubreve;
      mamnutrata:
      <ekz>
        <uzo tip="stl">FIG</uzo> la su&ccirc;<tld/>a periodo
        de nia lingvo
        <fnt>(<aut>Zamenhof</aut>, <vrk>Parolo anta&ubreve;
            la Kvara Kongreso Esperantista en Dresden
            1908</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_m;&c_a;&c_w;&c_l;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>S&auml;ugling</trd>,
    <trd>Baby</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>lactant</trd>,
    <trd>beb&eacute; <ind>de pit</ind></trd>
  </trdgrp>
  <trd lng="en">baby</trd>
  <trd lng="es">lactante</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;b&eacute;</trd>,
    <trd>nourrisson</trd>
  </trdgrp>
  <trd lng="hu">csecsem&odblac;</trd>
  <trd lng="id">bayi</trd>
  <trd lng="nl">zuigeling</trd>
  <trdgrp lng="pl">
    <trd>niemowl&eogonek;</trd>,
    <trd>osesek</trd>
  </trdgrp>
  <trd lng="pt">crian&ccedil;a de peito</trd>
  <trd lng="ro">sugar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_o;&c_j; 
      &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_m;&c_l;&c_a;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="uk">&c_n;&c_je;&c_m;&c_o;&c_v;&c_l;&c_ja;</trd>
  <trdgrp lng="it">
    <trd>poppante</trd>,
    <trd>lattante</trd>,
    <trd>beb&egrave;</trd>
  </trdgrp>
</drv>
</art>
