<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: labi.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
   <rad>labi</rad>/o
</kap>
<drv mrk="labi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Unupetala <ref tip="super" cel="korol.0o">korolo</ref>, 
       kies rando estas partigita en du kvaza&ubreve;lipojn.
     </dif>
   </snc>
  <trd lng="ca">corol&middot;la unilabiada</trd>
  <trdgrp lng="cs">
    <trd>labium</trd>,
    <trd>pysk</trd>,
    <trd>ret</trd>
  </trdgrp>
  <trd lng="de">Labium</trd>
  <trd lng="fr">corolle <ind>labi&eacute;e</ind></trd>
  <trd lng="hu">ajak<klr tip="ind"> (vir&aacute;g&eacute;)</klr></trd>
  <trd lng="nl">lipvormige <ind>bloemkroon</ind></trd>
  <trdgrp lng="pt">
    <trd>l&aacute;bio</trd>,
    <trd>corola labial</trd>
   </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; 
&c_g;&c_u;&c_b;&c_a;<klr tip="amb"> 
(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">pysk</trd>
</drv>
</art>
