<vortaro>
<art mrk="$Id: basi.xml,v 1.11 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>basi</rad>/o
</kap>
<drv mrk="basi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio de <ref
      tip="malprt"
      cel="sapot.0oj">sapotacoj</ref>
      (<trd lng="la">Diploknema</trd>).
    </dif>
  <trd lng="de">Butterbaum</trd>
  <trd lng="en">butter <ind>tree</ind> (of Nepal)</trd>
  <trdgrp lng="hu">
    <trd>indiai vajfa</trd>,
    <trd>ken&odblac;csfa</trd>
  </trdgrp>
  <trd lng="pl">mas&lstroke;osz</trd>
  </snc>
  <trd lng="cs">baissa</trd>
  <trdgrp lng="sk">
    <trd>baissa</trd>,
    <trd>pokles kurzov na burze</trd>
  </trdgrp>
</drv>
</art>
