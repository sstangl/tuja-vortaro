  </snc>
  <refgrp tip="vid">
    <ref cel="cxel.ovo0o.FIZL">ovo&ccirc;elo</ref>
  </refgrp>
  <trd lng="bg">&c_s;&c_p;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_z;&c_o;&c_i;&c_d;</trd>
  <trd lng="ca">espermatozou</trd>
  <trd lng="en">spermatozoon</trd>
  <trd lng="es">espermatozoide</trd>
  <trd lng="fr">spermatozo&iuml;de</trd>
  <trd lng="nl">spermatozo&iuml;de</trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_z;&c_o;&c_i;&c_d;</trd>
  <trd lng="sk">spermia</trd>
  <trdgrp lng="cs">
    <trd>spermatozoon</trd>,
    <trd>spermie</trd>
  </trdgrp>
</drv>
