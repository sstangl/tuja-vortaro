<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hes.xml,v 1.13 2011/12/07 11:10:17 revo Exp $">
<kap>
  <rad>hes</rad>/o
</kap>
<drv mrk="hes.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de Hesujo kaj nomo de ano de iama &gcirc;ermana tribo.
    </dif>
  </snc>
  <trd lng="ca">hesseni&agrave; <klr>(subst.)</klr></trd>
  <trd lng="fr">hessois<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="de">Hesse</trd>
  <trd lng="hu">hesseni<klr tip="ind"> (lakos)</klr></trd>
  <trd lng="it">assiano<klr> (sost.)</klr></trd>
  <trdgrp lng="lat">
     <trd>Hassus, i</trd>,
     <trd>Chattus, i</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_s;&c_s;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>

<drv mrk="hes.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>aj dialektoj, a&ubreve;toroj;
      </ekz>
      <ekz>
        <tld/>a konstitucio.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">hesseni&agrave; <klr>(adj.)</klr></trd>
  <trd lng="de">hessisch</trd>
  <trd lng="fr">hessois<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">hesszeni</trd>
  <trd lng="it">assiano<klr> (agg.)</klr></trd>
  <trd lng="nl">Hessisch</trd>
  <trd lng="ru">&c_g;&c_je;&c_s;&c_s;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hes.H0ujo">
  <kap><tld lit="H"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>. Regurbo
      estas
      <ref cel="vizbaden.V0o">Vizbadeno</ref>, plej granda urbo estas
      <ref cel="frankfurt.F0oMajno">Frankfurto &ccirc;e Majno</ref>.
    </dif>
  </snc>
  <trd lng="ca">Hessen</trd>
  <trd lng="de">Hessen</trd>
  <trd lng="en">Hesse</trd>
  <trd lng="fr">Hesse</trd>
  <trd lng="hu">Hessen</trd>
  <trd lng="it">Assia</trd>
  <trdgrp lng="lat">
     <trd>Hassia</trd>,
     <trd>terra Chattorum</trd>
  </trdgrp>
  <trd lng="nl">Hessen</trd>
  <trd lng="pl">Hesja</trd>
  <trd lng="pt">Hesse</trd>
  <trd lng="ru">&c_G;&c_je;&c_s;&c_s;&c_je;&c_n;</trd>
</drv>
</art>
