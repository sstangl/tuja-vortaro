<art mrk="$Id: erinac1.xml,v 1.10 2013/08/31 08:10:32 revo Exp $">
<kap>
  <rad>erinaced</rad>/oj
</kap>
<drv mrk="erinac1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Erinaceidae</trd>), kun mallongaj kruroj kaj longa
      bu&scirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_zh;&c_y;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">erinaceids</trd>
  <trd lng="de">Igel <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">hedgehogs <klr tip="ind">(family)</klr></trd>
  <trd lng="fr">&eacute;rinac&eacute;id&eacute;s</trd>
  <trd lng="hu">s&uuml;nf&eacute;l&eacute;k</trd>
  <trd lng="pl">je&zdot;owate</trd>
  <trd lng="pt">erinace&iacute;deos</trd>
  <trd lng="ru">&c_je;&c_zh;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

