<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sistematik.xml,v 1.9 2016/04/13 15:10:34 revo Exp $">
<kap>
  <rad>sistematik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sistematik.0o">
  <kap><tld/>o</kap>
  <snc mrk="sistematik.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Parto de scienco difinanta kaj ordiganta klasojn, precipe vivulajn
      grupojn en <ref tip="malprt" cel="biolog1.0o">biologio</ref>:
      <ekz>
        mamula <tld/>o <klr>...</klr> hazarde estas mia fako
        <fnt>
        <aut>A. Martins</aut>,
        <vrk>Salmo trutta...</vrk>,
        <lok>soc.culture.esperanto, 1997-04-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="en">systematics</trd>
  <trd lng="cs">systematika</trd>
  <trd lng="fr">syst&eacute;matique<klr tip="amb"> (biologie...)</klr></trd>
  <trd lng="pl">systematyka</trd>
  <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>systematika</trd>,
    <trd>triedenie</trd>,
    <trd>uvedenie do syst&eacute;mu</trd>
  </trdgrp>
</drv>

<drv mrk="sistematik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sistematik.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        oni neniam produktis hibridojn inter specioj
        kiujn <tld/>istoj klasifikas en apartaj familioj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
        8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="en">systematist</trd>
  <trd lng="cs">systematik</trd>
  <trd lng="fr">syst&eacute;maticien</trd>
  <trd lng="pl">systematyk</trd>
  <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">systematik</trd>
</drv>
</art>

