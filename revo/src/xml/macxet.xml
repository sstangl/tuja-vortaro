<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: macxet.xml,v 1.4 2013/04/11 15:10:13 revo Exp $">
<kap>
  <rad>ma&ccirc;et</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="macxet.0o">
  <kap><tld/>o</kap>
  <snc mrk="macxet.0o.KOMUNE">
    <dif>
      Brazildevena <ref tip="super" 
      cel="trancx.0ilo.KOMUNE">tran&ccirc;il</ref>ego
      kun klingo proksimume 3-centimetre lar&gcirc;a, uzata por fortran&ccirc;i 
      kreska&jcirc;ojn:
      <ekz>
        la <tld/>oj detruis sange ru&gcirc;ajn liliojn
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        alia soldato tenas <tld/>on kaj sin pretigas por apartigi la 
        kapon disde la trunko
        <fnt>
          <aut>J. Saramago, trad. TdB</aut>,
          <vrk><url ref="http://satenhispanio.eresmas.com/artik13a.htm">
          La faktoro Dio</url></vrk>,
          <lok>Maldekstro kaj Esperanto, [vidita en 2007-04]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ch;&c_e;&c_t;&c_e;</trd>
  <trd lng="bg">&c_m;&c_a;&c_ch;&c_je;&c_t;&c_je;</trd>
  <trd lng="ca">matxet</trd>
  <trd lng="de">Machete</trd>
  <trd lng="fr">machette</trd>
  <trd lng="it">machete</trd>
</drv>
</art>

