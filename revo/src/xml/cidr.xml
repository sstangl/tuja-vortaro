<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cidr.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>cidr</rad>/o
</kap>
<drv mrk="cidr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Fermentinta trinka&jcirc;o el suko de pomoj; pomvino.
    </dif>
    <trd lng="be">&c_s;&c_ib;&c_d;&c_a;&c_r;</trd>
    <trd lng="br">sistr</trd>
    <trdgrp lng="de">
    <trd>Zider</trd>
    <trd>Apfelwein</trd>
    </trdgrp>
    <trd lng="el">&my;&eta;&lambda;&omikron_ton;&chi;&ypsilon;&my;&omikron;&sigma_fina;</trd>
    <trd lng="fr">cidre</trd>
    <trd lng="es">sidra</trd>
    <trd lng="hu">almabor</trd>
    <trd lng="it">sidro</trd>
    <trd lng="nl">cider</trd>
    <trdgrp lng="pl">
      <trd>cydr<klr> (rzadk.)</klr></trd>,
      <trd>jab&lstroke;ecznik</trd>
    </trdgrp>
    <trd lng="pt">cidra</trd>
    <trd lng="ru">&c_s;&c_i;&c_d;&c_r;</trd>
    <trd lng="tr">elma &scedil;arab&inodot;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>cidre</trd>,
    <trd>hruškové víno</trd>,
    <trd>jablečné víno</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cider</trd>,
    <trd>jablčné víno</trd>
  </trdgrp>
</drv>
</art>

