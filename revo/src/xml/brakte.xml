<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brakte.xml,v 1.13 2012/01/05 15:10:14 revo Exp $">
<kap>
<rad>brakte</rad>/o
</kap>
<drv mrk="brakte.0o">
  <kap><tld/>o</kap>
  <snc mrk="brakte.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="foli.0o.BOT">Folio</ref> en la regiono
      de <ref tip="malprt" cel="inflor.0o">infloresko</ref>, ordinare
      eta, skvamforma, foje kolora kaj kunformanta la floron,
      en kies angulo elkreskas la floroj a&ubreve; flortigoj.
    </dif>
  </snc>
  <ref tip="malprt" cel="involu.0o">involukro</ref>
  <trd lng="be">&c_p;&c_r;&c_y;&c_k;&c_v;&c_je;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">br&agrave;ctea</trd>
  <trdgrp lng="de">
    <trd>Tragblatt</trd>,
    <trd>Deckblatt</trd>,
    <trd>H&uuml;llblatt</trd>
  </trdgrp>
  <trd lng="en">bract</trd>
  <trd lng="es">br&aacute;ctea</trd>
  <trd lng="fr">bract&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>murvalev&eacute;l</trd>,
    <trd>fellev&eacute;l</trd>,
    <trd>braktea</trd>
  </trdgrp>
  <trd lng="nl">schutblad<klr tip="ind"> (v.plant)</klr></trd>
  <trd lng="pl">podsadka</trd>
  <trd
  lng="ru">&c_p;&c_r;&c_i;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
