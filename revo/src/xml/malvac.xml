<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: malvac.xml,v 1.12 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>malvac</rad>/oj
</kap>
<drv mrk="malvac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="malvac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Malvaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_l;&c_je;&c_z;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">malv&agrave;cies</trd>
  <trd lng="cs">slézovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Malvengew&auml;chse</trd>
  <trd lng="en">mallows</trd>
  <trd lng="fr">malvac&eacute;es</trd>
  <trd lng="hu">m&aacute;lyvaf&eacute;l&eacute;k</trd>
  <trd lng="nl">kaasjeskruidfamilie</trd>
  <trd lng="pt">malv&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">slezovité</trd>
</drv>
</art>
