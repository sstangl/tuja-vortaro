<art mrk="$Id: virtuo.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>virtuoz</rad>/o
</kap>
<drv mrk="virtuo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="talent.0ulo">talentulo</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_r;&c_t;&c_u;&c_o;&c_z;</trd>
  <trd lng="ca">virtu&oacute;s</trd>
  <trd lng="cs">virtuóz</trd>
  <trd lng="de">Virtuose</trd>
  <trd lng="en">virtuoso</trd>
  <trd lng="es">virtuoso<klr> (en la m&uacute;sica)</klr></trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nie</trd>,
    <trd>virtuose</trd>
  </trdgrp>
  <trd lng="hu">virtu&oacute;z</trd>
  <trd lng="it">virtuoso<klr> (musica)</klr></trd>
  <trd lng="nl">virtuoos</trd>
  <trd lng="pt">virtuoso</trd>
  <trd lng="ru">&c_v;&c_i;&c_r;&c_t;&c_u;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>majster</trd>,
    <trd>virtuóz</trd>
  </trdgrp>
</drv>
</art>
