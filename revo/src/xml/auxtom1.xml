<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxtom1.xml,v 1.21 2010/05/17 16:30:28 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>a&ubreve;tomobil</rad>/o
</kap>
<drv mrk="auxtom1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AUT</uzo>
    <ref tip="dif" cel="auxt.0o">a&ubreve;to</ref>
    <ekz> se oni posedas domon kaj <tld/>on oni devas forte labori por
    repagi la prunton<fnt><aut>Remigiusz Mielcarek</aut>:
  <vrk>Enmigrintoj en ora ka&gcirc;o</vrk>,
  <bib>Monato</bib><lok>jaro 1994a, numero 1a, p. 15a</lok></fnt>.</ekz>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;
  </trd>
  <trd lng="ca">autom&ograve;bil</trd>
  <trdgrp lng="de">
    <trd>Auto</trd>,
    <trd>Automobil</trd>,
    <trd>Kraftwagen</trd>,
    <trd>Kraftfahrzeug</trd>,
    <trd>Kfz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>automobile</trd>
  </trdgrp>
  <trd lng="es">autom&oacute;vil</trd>
  <trd lng="fr">automobile</trd>
  <trd lng="hu">szem&eacute;lykocsi</trd>
  <trdgrp lng="it">
    <trd>automobile</trd>,
    <trd>vettura<klr> (auto)</klr></trd>, 
    <trd>macchina<klr> (auto)</klr></trd>,
    <trd>auto<klr> (vettura)</klr></trd>,
  </trdgrp>
  <trd lng="nl">auto</trd>
  <trdgrp lng="os">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&aelig;</trd>,
    <trd>&c_h;&aelig;&c_d;&c_t;&c_u;&c_l;&c_g;&aelig;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>automobil<klr> (przest.)</klr></trd>,
    <trd>auto</trd>,
    <trd>samoch&oacute;d</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>autom&oacute;vel</trd>,
    <trd>carro</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;
  </trd>
  <trd lng="sv">bil</trd>
</drv>

<drv mrk="auxtom1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="auxt.0isto">a&ubreve;tisto</ref>
    <ekz> stirante, <tld/>istoj kaj motorciklistoj telefonas<fnt>
    <aut>Christian Fabossou</aut>:
  <vrk>&Ccirc;u-vi-vidas-min-ismo</vrk>, <bib>Monato</bib>
  <lok>jaro 2001a, numero 12a, p. 12a</lok></fnt>.</ekz>
  </snc>
  <trd lng="be">
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">automobilista</trd>
  <trdgrp lng="de">
    <trd>Autofahrer</trd>,
    <trd>Wagenf&uuml;hrer</trd>,
    <trd>Kraftfahrer</trd>
  </trdgrp>
  <trd lng="en">motorist</trd>
  <trd lng="es">automovilista</trd>
  <trd lng="fr">automobiliste</trd>
  <trd lng="hu">&uacute;rvezet&odblac;</trd>
  <trdgrp lng="it">
    <trd>automobilista</trd>,
    <trd>autista<klr> (conducente di auto)</klr></trd>
  </trdgrp>
  <trd lng="nl">automobilist</trd>
  <trdgrp lng="pl">
    <trd>automobilista<klr> (przest.)</klr></trd>,
    <trd>kierowca</trd>
  </trdgrp>
  <trd lng="ru">
  &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">bilf&ouml;rare</trd>
</drv>
</art>
