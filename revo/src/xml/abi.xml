<?xml version="1.0"?>    

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: abi.xml,v 1.34 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>abi</rad>/o
</kap>

<drv mrk="abi.0o">
  <kap><ofc>*</ofc><tld/>o</kap><fnt>F</fnt>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;iamverda arbo el <ref tip="lst" cel="konifer.0o" 
      lst="voko:koniferoj">koniferoj</ref> kun starantaj konusoj kaj
      pingloformaj maldikaj, iom flekseblaj, folioj, ofte havantaj
      malsupre du 
      helajn striojn, kaj elkreskantaj kun piedeto rekte el la bran&ccirc;oj;
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="pinac.0oj">pinacoj</ref>
      (<trd lng="la">Abies</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_je;&c_l;&c_a;</trd>,
    <trd>&c_je;&c_l;&c_h;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
     <trd>saprenn</trd>, 
     <trd>gwezenn-sapr</trd>
  </trdgrp>
  <trd lng="ca">avet</trd>
  <trd lng="cs">jedle</trd>
  <trd lng="de">Tanne</trd>
  <trd lng="el">&epsilon_ton;&lambda;&alfa;&tau;&omikron;</trd>
  <trd lng="en">fir</trd>
  <trd lng="es">abeto</trd>
  <trd lng="fa">&a_n;&a_r;&a_A;&a_d;</trd>
  <trd lng="fr">sapin</trd>
  <trd lng="hu">jegenyefeny&odblac;</trd>
  <trd lng="id">cemara</trd>
  <trd lng="it">abete</trd>
  <trd lng="ja">&#12514;&#12511; [<ind>&#12418;&#12415;</ind>]</trd>
  <trd lng="nl">spar</trd>
  <trd lng="oc">avet</trd>
  <trd lng="pl">jod&lstroke;a</trd>
  <trd lng="pt">pinheiro</trd>
  <trd lng="ru">&c_p;&c_i;&c_h;&c_t;&c_a;</trd>
  <trd lng="sk">jed&lcaron;a</trd>
  <trd lng="sv">gran</trd>
  <trd lng="tr">g&ouml;knar</trd>
</drv>
</art>
