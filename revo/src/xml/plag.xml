<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plag.xml,v 1.17 2015/07/04 08:37:41 revo Exp $">
<kap>
    <ofc>4</ofc>
    <rad>plag</rad>/o
    <fnt>Z</fnt>
</kap>
<drv mrk="plag.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Tutlanda longeda&ubreve;ra katastrofo, ka&ubreve;zita de Dio kiel puno por la
        homoj:
        <ekz>
          <ind>la sep <tld/>oj de Egiptujo</ind>.
          <trd lng="ru">&c_s;&c_je;&c_m;&c_mol; &c_k;&c_a;&c_z;&c_n;&c_je;&c_j; &c_je;&c_g;&c_i;&c_p;&c_je;&c_t;&c_s;&c_k;&c_i;&c_h;</trd>
        </ekz>
      </dif>
      <trd lng="pl">plaga</trd>
    </snc>
    <snc>
      <dif>
        Tutlanda granda longeda&ubreve;ra katastrofo:
        <ekz>
          la ma&scirc;ino povas esti liberigilo a&ubreve; <tld/>o por la homaro.
        </ekz>
      </dif>
    <trdgrp lng="fr">
      <trd>fl&eacute;au</trd>,
      <trd>calamit&eacute;</trd>
     </trdgrp>
    </snc>
  <trdgrp lng="be">
      <trd>&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>, 
      <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_a;</trd>, 
      <trd>&c_p;&c_l;&c_ja;&c_g;&c_a;</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>kalamita</trd>,
    <trd>neštěstí</trd>,
    <trd>pohroma</trd>,
    <trd>strast</trd>,
    <trd>trest boží</trd>
  </trdgrp>
  <trd lng="de">Plage</trd>
  <trd lng="en">plague</trd>
  <trd lng="es">plaga</trd>
  <trd lng="fr">plaie<klr tip="ind"> (fl&eacute;au)</klr></trd>
  <trdgrp lng="hu">
      <trd>sorscsap&aacute;s</trd>,
      <trd>baj</trd>
    </trdgrp>
  <trd lng="nl">plaag</trd>
  <trd lng="pt">praga</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>pliaga</trd>,
    <trd>pohroma</trd>
  </trdgrp>
</drv>

<drv mrk="plag.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Puni per <tld/>o:
        <ekz>
          vian popolon ne <tld/>u<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_n;&c_a;&c_s;&c_l;&c_a;&c_c;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_a;</trd>
      <trd lng="hu">ver<klr tip="ind"> (pl. isten)</klr></trd>
      <trd lng="ru">&c_n;&c_a;&c_s;&c_l;&c_a;&c_t;&c_mol;
&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
    </snc>
    <snc>
      <dif>
        Ruinigi per <tld/>o:
        <ekz>
          la <tld/>ita regiono.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
        <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="hu">csap&aacute;ssal s&uacute;jt</trd>
      <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>potrestat pohromou</trd>,
    <trd>zničit pohromou</trd>
  </trdgrp>
  <trd lng="de">plagen</trd>
  <trd lng="en">plague</trd>
  <trd lng="es">sufrir una plaga</trd>
  <trd lng="nl">teisteren</trd>
  <trdgrp lng="sk">
    <trd>pliagou</trd>,
    <trd>trestať pohromou</trd>
  </trdgrp>
</drv>
</art>
