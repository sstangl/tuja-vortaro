<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: poz.xml,v 1.20 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>poz</rad>/i
</kap>
<drv mrk="poz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="poz.0i.KOMUNE">
    <dif>
      Preni, plej ofte konscie, specialan teni&gcirc;on:
    </dif>
    <subsnc mrk="poz.0i.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Preni kaj konservi teni&gcirc;on por esti desegnata, pentrata, fotata:
        <ekz>
          ili <tld/>as la nimfojn.
        </ekz>
      </dif>
      <trd lng="de">posieren</trd>
      <trd lng="hu">modellt &uuml;l</trd>
      <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="poz.0i.afekti">
      <dif>
        Afekti gravan mienon; &scirc;ajnigi sin pla&ccirc;ulo, eminentulo:
        <ekz>
          &Hcirc;lestakov &ndash; <tld/>ante &ndash; &leftquot;Mi petas vin, sinjorino, al mi estas
          tre agrable
          <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno
          12a</lok></fnt>...
        </ekz>
        <ekz>
          <tld/>i spritulon, scienculon.
        </ekz>
      </dif>
      <trd lng="de">eine <ind>Pose</ind> einnehmen</trd>
      <trdgrp lng="hu">
        <trd>p&oacute;zol</trd>,
        <trd>nagyk&eacute;p&udblac;sk&ouml;dik</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_i;&c_z; &c_s;&c_je;&c_b;&c_ja;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="poz.0i.3">
      <uzo tip="stl">EVI</uzo>
      <dif>
        Teni sin iamaniere.
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>pózovat</trd>,
    <trd>sedět modelem</trd>,
    <trd>stát modelem</trd>
  </trdgrp>
  <trd lng="en">pose</trd>
  <trdgrp lng="fr">
    <trd>frimer</trd>,
    <trd>poser<klr tip="amb"> (prendre une pose)</klr></trd>
  </trdgrp>
  <trd lng="nl">poseren</trd>
  <trd lng="pl">pozowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>posar</trd>,
    <trd>fazer <ind>pose</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>postaviť sa do pózy</trd>,
    <trd>pózovať</trd>
  </trdgrp>
</drv>

<drv mrk="poz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aparta teni&gcirc;o a&ubreve; mieno:
      <ekz>
        teatra <tld/>o;
      </ekz>
      <ekz>
        ekvidinte la enirintan virinon, li prenis <tld/>on duone
        salutan,
        duone atendan
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>držení těla</trd>,
    <trd>postoj</trd>,
    <trd>póza</trd>
  </trdgrp>
  <trd lng="de">Pose</trd>
  <trdgrp lng="en">
    <trd>pose</trd>,
    <trd>posture</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attitude physique</trd>,
    <trd>pose</trd>
  </trdgrp>
  <trd lng="hu">p&oacute;z</trd>
  <trd lng="nl">pose</trd>
  <trd lng="pl">poza</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>postoj</trd>,
    <trd>póza</trd>
  </trdgrp>
</drv>

<drv mrk="poz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="poz.0isto.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Homo, kiu profesie <tld/>as anta&ubreve; artistoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="model.0o.BELA">modelo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="cs">živý model</trd>
  <trd lng="de">Modell</trd>
  <trd lng="en">model</trd>
  <trd lng="fr">mod&egrave;le vivant<klr tip="amb"> (arts)</klr></trd>
  <trd lng="hu">modell</trd>
  <trd lng="nl">model</trd>
  <trd lng="pl">model</trd>
  <trd lng="ru">&c_n;&c_a;&c_t;&c_u;&c_r;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sk">živý model</trd>
</drv>
</art>
