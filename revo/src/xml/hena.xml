<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hena.xml,v 1.11 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hena</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hena.0o">
  <kap><tld/>o</kap>
  <snc mrk="hena.0o.KOS">
    <dif>
      Pulvoro el sekigitaj folioj de kofero preparita por uzo kiel <ref 
      
      tip="super" cel="sxmink.0o">&scirc;minko</ref> kuprokolore
      
      tinkturanta 
      harojn, ungojn, ha&ubreve;ton, populara kaj rigardata kiel sanfavora en 
      Nordafriko:
      <ekz>
        muelitaj kiel <tld/>o a&ubreve; tritiko
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 28a, p.
        553a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_n;&c_a;</trd>
  <trd lng="ca">henna</trd>
  <trd lng="cs">hena</trd>
  <trd lng="de">Henna</trd>
  <trd lng="fa">&a_H;&a_n;&a_A;<klr tip="ind"> (&f_g;&a_r;&a_d;)</klr></trd>
  <trd lng="fr">henn&eacute;</trd>
  <trd lng="it">henn&eacute;<klr> (tintura)</klr></trd>
  <trd lng="pt">hena</trd>
  <trd lng="ru">&c_h;&c_n;&c_a;</trd>
  <trd lng="sk">hena</trd>
</drv>

<drv mrk="hena.0arbo">
  <kap><tld/>arbo</kap>
  <snc mrk="hena.0arbo.BOT">
    <ref tip="dif" cel="kofer.0o.BOT">kofero</ref>
  </snc>
  <trd lng="ca">alquena</trd>
  <trd lng="fa">&a_H;&a_n;&a_A;<klr tip="ind"> (&a_d;&a_r;&a_kh;&a_t;&f_ch;&a_h;)</klr></trd>
  <trd lng="it">henn&eacute;<klr> (pianta)</klr></trd>
</drv>
</art>
