<art mrk="$Id: korteks.xml,v 1.7 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>korteks</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="korteks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <ref tip="dif" cel="kortik.0o">kortiko</ref>
  <ekz>
    la nervoj de tiuj artikoj kaj muskoloj ligi&gcirc;as kun la cerba <tld/>o
    
    <fnt>
      <aut>&Ccirc;ina Radio Internacia</aut>,
      <vrk><url
      ref="http://esperanto.cri.cn/chinaabc/chapter18/chapter180302.htm">
      Man&gcirc;bastonetoj</url></vrk>,
      <lok>[vidita en 2008-07]</lok>
    </fnt>.
  </ekz>
  <trd lng="fr">cortex</trd>
  <trd lng="it">corteccia<klr> (anat.)</klr></trd>
  <trdgrp lng="nl">
    <trd>cortex</trd>,
    <trd>hersenschors</trd>
  </trdgrp>
  <trd lng="pl">kora</trd>
  <trd lng="sk">mozgová kôra</trd>
  <trd lng="cs">mozková kůra</trd>
</drv>
</art>

