<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alcion.xml,v 1.20 2015/07/04 08:37:30 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>alcion</rad>/o
</kap>
<drv mrk="alcion.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Genro el familio <tld/>edoj
       (<trd lng="la">Alcedo</trd>),
       birdo kun lazura dorso kaj rustkolora ventro,
       vivanta &ccirc;e fi&scirc;ori&ccirc;aj akvejoj.
     </dif>
   </snc>
  <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="bg">&c_z;&c_je;&c_m;&c_je;&c_r;&c_o;&c_d;&c_n;&c_o; &c_r;&c_i;&c_b;&c_a;&c_r;&c_ch;&c_je;</trd>
  <trd lng="br">diredig</trd>
  <trdgrp lng="ca">
     <trd>blauet</trd>,
     <trd>botiguer</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>korek mořský</trd>,
    <trd>led&ncaron;&aacute;&ccaron;ek</trd>,
    <trd>ledňáček obecný</trd>,
    <trd>ledňáček říční</trd>
  </trdgrp>
  <trd lng="de">Eisvogel</trd>
  <trd lng="en">kingfisher</trd>
  <trd lng="es">mart&iacute;n pescador</trd>
  <trd lng="fa">&a_m;&a_A;&a_h;&f_y;&zwnj;&a_kh;&a_w;&a_r;&f_k;</trd>
  <trd lng="fr">martin-p&ecirc;cheur</trd>
  <trd lng="he">&shin;&lamed;&dalet;&gimel;</trd>
  <trd lng="hu">j&eacute;gmad&aacute;r</trd>
  <trd lng="mk">&c_r;&c_i;&c_b;&c_a;&c_r;&c_ch;&c_je;</trd>
  <trd lng="nl">ijsvogel</trd>
  <trd lng="pl">zimorodek</trd>
  <trdgrp lng="pt">
     <trd>alc&iacute;one</trd>,
     <trd>alcedo</trd>
   </trdgrp>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>
  <trd lng="sk">rybárik modrosivý</trd>
</drv>
<drv mrk="alcion.0edoj">
   <kap><tld/>edoj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio de birdoj (<trd lng="la">Alcedinidae</trd>),
       al kiu apartenas la <tld/>o.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_k;&c_ib; <klr
   tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="ca">alced&iacute;nids</trd>
   <trd lng="de">Eisv&ouml;gel</trd>
   <trd lng="fr">alc&eacute;dinid&eacute;s</trd>
   <trd lng="hu">j&eacute;gmad&aacute;rf&eacute;l&eacute;k</trd>
   <trd lng="nl">ijsvogels</trd>
   <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_k;&c_i;<klr tip="amb">
   (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
