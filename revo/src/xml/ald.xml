<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ald.xml,v 1.23 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ald</rad>/o
</kap>
<drv mrk="ald.0o">
  <kap><tld/>o, <var><kap><tld/>violono</kap></var></kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      La plej malalta virina vo&ccirc;o.
    </dif>
    <trd lng="ca">contralt</trd>
    <trdgrp lng="de">
     <trd>Altstimme</trd>,
     <trd>Alt</trd>
    </trdgrp>
    <trd lng="en">alto</trd>
    <trd lng="hu">alt</trd>
    <trd lng="it">contralto</trd>
    <trd lng="nl">alt</trd>
    <trd lng="pl">alt</trd>
    <trd lng="pt">contralto</trd>
    <trd lng="sk">alt</trd>
  </snc>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="violon.0o">Violono</ref>, 
      intera inter violono kaj violon&ccirc;elo.
    </dif>
    <ref tip="lst" cel="instru2.kord0o.MUZ"
    lst="voko:kordinstrumentoj">kordinstrumento</ref>
    <trd lng="ca">viola</trd>
    <trd lng="en">viola</trd>
    <trd lng="nl">altviool</trd>
    <adm>&ccirc;u eble registri tion pli kutime kiel aldviolono kaj referencie
    tien [wd]</adm>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="cs">viola</trd>
  <trd lng="fr">alto</trd>
  <trd lng="hu">m&eacute;lyheged&udblac;</trd>
  <trd lng="it">viola</trd>
  <trd lng="pl">alt&oacute;wka</trd>
  <trd lng="pt">violeta</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">viola</trd>
</drv>
</art>
