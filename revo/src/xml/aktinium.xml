<vortaro>
  <art mrk="$Id: aktinium.xml,v 1.8 2014/06/07 03:10:25 revo Exp $">
    <kap>
      <rad>aktinium</rad>/o
    </kap>
    <drv mrk="aktinium.0o">
      <kap><tld/>o</kap>
      <mlg kod="IUPAC">Ac</mlg>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
          lst="voko:&hcirc;emiaj_elementoj" val="89">elemento</ref> kun
          atomnumero 89, atompezo 227,0278, kies &ccirc;iuj
          <ref tip="super" cel="izotop.0o">izotopoj</ref> 
          estas radioaktivaj.
        </dif>
      </snc>                         
      <trd lng="be">&c_a;&c_k;&c_t;&c_y;&c_n;&c_ib;&c_j;</trd>
      <trd lng="bg">&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
      <trd lng="br">aktiniom</trd>
      <trd lng="ca">actini</trd>
      <trd lng="de">Actinium</trd>
      <trd lng="en">actinium</trd>
      <trd lng="es">actinio</trd>
      <trd lng="fr">actinium</trd>
      <trd lng="id">aktinium</trd>
      <trd lng="it">attinio</trd>
      <trd lng="la">Actinium</trd>
      <trd lng="nl">actinium</trd>
      <trd lng="pl">aktyn</trd>
      <trd lng="pt">act&iacute;nio</trd>
      <trd lng="ru">&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
      <trd lng="sk">akt&iacute;nium</trd>
      <trd lng="sv">aktinium</trd>
    </drv>
  </art>

