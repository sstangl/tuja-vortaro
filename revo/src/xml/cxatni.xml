<vortaro>
<art mrk="$Id: cxatni.xml,v 1.2 2013/12/12 16:10:14 revo Exp $">
<kap>
  <rad>&ccirc;atni</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="cxatni.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxatni.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Hinddevena dol&ccirc;a kaj akra
      <ref tip="super" cel="spic.0ajxo.KUI">spica&jcirc;o</ref>
      el fruktoj a&ubreve; legomoj boligitaj en vinagro
      kun sukero kaj diversaj spicoj:
      <ekz>
        samoso estas <klr>...</klr> ofte man&gcirc;ata kun <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;samoso&rightquot;, 
        2009-04-01</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_ch;&c_malmol;&c_t;&c_n;&c_i;</trd>
  <trd lng="en">chutney</trd>
  <trdgrp lng="fr">
    <trd>chatni</trd>,
    <trd>chutney</trd>
  </trdgrp>
  <trd lng="nl">chutney</trd>
</drv>
</art>

