<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kajer.xml,v 1.25 2015/06/29 16:33:16 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kajer</rad>/o
</kap>

<drv mrk="kajer.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Kuniga&jcirc;o de kelkaj paperfolioj, falditaj a&ubreve; ne:
     </dif>
     <subsnc mrk="kajer.0o.porskribi">
       <dif>
         uzataj por skribi:
         <ekz>
           <tld/>o por skriblernado;
         </ekz>
       </dif>
       <trdgrp lng="nl">
         <trd>schrift<klr tip="ind"> (schrijfboek)</klr></trd>,
         <trd>schrijfboek</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="kajer.0o.de_libro">
       <uzo tip="fak">TIP</uzo>
       <dif>
         <klr>(de libro)</klr>
         <ekz>
           tiu libro enhavas dek <tld/>ojn kune binditajn;
         </ekz>
         <ekz>
           eldoni libron per apartaj <tld/>oj<fnt>Z</fnt>;
         </ekz>
         <ekz>
         la eldonado de la ceteraj <tld/>oj de la "Dua Libro" nun jam
         tial ne estas bezona, la nuna <tld/>o estas la
         lasta<fnt>Z</fnt>.
         </ekz>
       </dif>
        <trdgrp lng="nl">
        <trd>katern</trd>
        </trdgrp>
     </subsnc>
   </snc>
  <trd lng="be">&c_s;&c_sh;&c_y;&c_t;&c_a;&c_k;</trd>
  <trd lng="bg">&c_t;&c_je;&c_t;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="br">kaier</trd>
  <trd lng="cs">se&scaron;it</trd>
  <trd lng="de">Heft</trd>
  <trdgrp lng="en">
     <trd>notebook</trd>,
     <trd>pamphlet</trd>
   </trdgrp>
  <trd lng="es">cuaderno</trd>
  <trd lng="fi">vihko</trd>
  <trd lng="fr">cahier</trd>
  <trd lng="he">&het;&vav;&bet;&resh;&tav;</trd>
  <trd lng="hu">f&udblac;zet</trd>
  <trd lng="pt">caderno</trd>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_r;&c_a;&c_d;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>písanka</trd>,
    <trd>zošit</trd>
  </trdgrp>
  <trd lng="sv">h&auml;fte</trd>
  <trd lng="tr">defter</trd>
</drv>
</art>
