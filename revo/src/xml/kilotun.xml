<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kilotun.xml,v 1.8 2014/09/22 14:10:11 revo Exp $">
  <kap>
    <rad>kilotun</rad>/o
  </kap>
<drv mrk="kilotun.0o">
  <kap><tld/>o</kap>
  <mlg>KT</mlg>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Praktika 
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref>,
      kiu respondas al la <ref cel="energi.0o.FIZ">energio</ref>
      ellasata de 
      1000 tunoj da TNT dum eksplodo.
    </dif>
  </snc>
  <trd lng="ca">quilotona</trd>
  <trd lng="en">kiloton</trd>
  <trd lng="fr">kilotonne</trd>
  <trd lng="id">kiloton</trd>
  <trd lng="it">chilotone</trd>
  <trd lng="nl">kiloton</trd>
  <trd lng="pl">kilotona</trd>
</drv>
</art>

