<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cign.xml,v 1.37 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cign</rad>/o
</kap>

<drv mrk="cign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cign.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda, ordinare blanka 
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">na&gcirc;birdo</ref>,
      kun longa fleksebla kolo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Cygnus</trd>), :
      <ekz>
        el la densejo elna&gcirc;is tri belaj blankaj cignoj
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/cign.jpg">
      <ind>muta <tld/>o</ind>
      <trd lng="ca"><ind>cigne</ind> mut</trd>
      <trd lng="cs">labu&tcaron; velk&aacute;</trd>
      <trd lng="de">H&ouml;ckerschwan</trd>
      <trd lng="en">mute swan</trd>
      <trd lng="hu">b&uuml;tyk&ouml;s hatty&uacute;</trd>
      <trd lng="la">Cygnus olor</trd>
      <trd lng="pl">&lstroke;ab&eogonek;d&zacute; niemy</trd>
      <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;-&c_sh;&c_i;&c_p;&c_u;&c_n;</trd>
    </bld>
    <trd lng="be">&c_l;&c_je;&c_b;&c_je;&c_d;&c_z;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_je;&c_b;&c_je;&c_d;</trd>
    <trd lng="br">alarc'h</trd>
    <trd lng="ca">cigne</trd>
    <trd lng="cs">labu&tcaron;</trd>
    <trd lng="de">Schwan</trd>
    <trd lng="el">&kappa;&ypsilon_ton;&kappa;&ny;&omikron;&sigma_fina;</trd>
    <trd lng="en">swan</trd>
    <trd lng="es">cisne</trd>
    <trd lng="fr">cygne</trd>
    <trd lng="he">&bet;&resh;&bet;&vav;&resh;</trd>
    <trd lng="hu">hatty&uacute;</trd>
    <trd lng="id">angsa</trd>
    <trd lng="it">cigno</trd>
    <trd lng="nl">zwaan</trd>
    <trd lng="pl">&lstroke;ab&eogonek;d&zacute;</trd>
    <trd lng="pt">cisne</trd>
    <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;</trd>
  </snc>
  
  <snc mrk="cign.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Harmonia poeto, a&ubreve;toro:
      <ekz>
      <ind>kanto de <tld/>o</ind><fnt>Z</fnt>
      <klr>(lasta plej bela verko de a&ubreve;toro)</klr>.
      <trdgrp lng="ca">
        <trd>cant del <ind>cigne</ind></trd>,
        <trd>darrera revifada</trd>
      </trdgrp>
      <trd lng="de">Schwanengesang</trd>
      <trd lng="fr">chant du <ind>cygne</ind></trd>
      <trd lng="es">cisne</trd>
      <trd lng="hu">hatty&uacute;dal</trd>
      <trd lng="it">canto del <ind>cigno</ind></trd>
      <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_i;&c_n;&c_a;&c_ja; 
      &c_p;&c_je;&c_s;&c_n;&c_ja;</trd>
      </ekz>
    </dif>
    <trd lng="ca">poeta</trd>
  </snc>
  <trd lng="sk">labu&tcaron;</trd>
</drv>

<drv mrk="cign.C0o">
  <kap><tld lit="C"/>o</kap>
  <mlg>Cyg</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="lst" 
cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
lng="la">Cygnus</trd>).
    </dif>
  </snc>
  <trd lng="ca">Cigne</trd>
  <trd lng="cs">labuť</trd>
  <trd lng="de">Schwan</trd>
  <trd lng="en">Cignus</trd>
  <trd lng="es">Cisne</trd>
  <trd lng="it">Cigno</trd>
  <trd lng="sk">labuť</trd>
</drv>

</art>
