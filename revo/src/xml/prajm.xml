<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prajm.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>prajm</rad>/o
</kap>
<drv mrk="prajm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Ujeto, enhavanta eksplodeman substancon kaj per kiu oni
      eksplodigas
      la pulvon de karto&ccirc;o.
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_i;&c_t;&c_je;&c_l;</trd>,
    <trd></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>roznětka</trd>,
    <trd>zápalka <klr>(náboje)</klr></trd>
  </trdgrp>
  <trd lng="de">Z&uuml;ndkapsel</trd>
  <trd lng="hu">gyutacs</trd>
  <trdgrp lng="it">
    <trd>cappellotto<klr> (mil.)</klr></trd>,
    <trd>capsula<klr> (di cartuccia)</klr></trd>,
    <trd>innesco</trd>,
    <trd>detonatore</trd>
  </trdgrp>
  <trd lng="nl">slaghoedje</trd>
  <trdgrp lng="pt">
    <trd>espoleta</trd>,
    <trd>detonador</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_s;&c_ju;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_t;&c_o;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">rozbuška <klr>(tech.)</klr></trd>
</drv>

<drv mrk="prajm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>on.
    </dif>
  </snc>
  <trd lng="cs">způsobit roznět</trd>
  <trd lng="de">Z&uuml;ndkapsel aufsetzen</trd>
  <trd lng="nl">v.e. <ind>slaghoedje</ind> voorzien</trd>
  <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_z;&c_a;&c_p;&c_a;&c_l;</ind></trd>
  <trd lng="sk">spôsobiť roznet</trd>
</drv>
</art>
