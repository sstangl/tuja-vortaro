<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: partik.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>partikul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="partik.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      Nedeklinaciebla vorteto; ekz. <ekz>ajn, ja.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>neohebné slovo</trd>,
    <trd>částice <klr>(ne, nur, ja, la ...)</klr></trd>
  </trdgrp>
  <trd lng="de">Partikel</trd>
  <trd lng="en">particle</trd>
  <trd lng="es">part&iacute;cula</trd>
  <trd lng="fr">particule</trd>
  <trd lng="hu">partikula</trd>
  <trd lng="nl">partikel</trd>
  <trd lng="pl">partyku&lstroke;a</trd>
  <trd lng="pt">partic&iacute;pio</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">nesamostatná častica</trd>
</drv>
</art>
