<art mrk="$Id: kantor.xml,v 1.17 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kantor</rad>/o
</kap>
<drv mrk="kantor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Pre&gcirc;eja <ref tip="super"
      cel="kant.0isto.profesio">kantisto</ref>:
      <ekz>
        la solena&jcirc;on finis Jacob Malowany, vaste en la mondo konata
        <tld/>o el Usono<fnt><aut>Zofia Banet-Fornalowa</aut>:
        <vrk>Kadi&scirc;
        por Jedwabne</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a,
        p. 18-20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>xantre</trd>,
    <trd>cantor d'esgl&eacute;sia</trd>
  </trdgrp>
  <trd lng="cs">kantor</trd>
  <trd lng="de">Kantor</trd>
  <trdgrp lng="en">
    <trd>cantor</trd>,
    <trd>precentor</trd>,
    <trd>choir member</trd>
  </trdgrp>
  <trd lng="es">cantor</trd>
  <trdgrp lng="fr">
    <trd>cantor</trd>,
    <trd>chantre</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;ntor</trd>
  <trdgrp lng="nl">
    <trd>voorzanger</trd>,
    <trd>cantor</trd>
  </trdgrp>
  <trd lng="pl">kantor</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_v;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kantor</trd>,
    <trd>spev√°k v kostole</trd>
  </trdgrp>
</drv>
</art>
