<vortaro>
<art mrk="$Id: kumis.xml,v 1.18 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kumis</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kumis.0o">
  <kap><tld/>o</kap>
  <snc mrk="kumis.0o.KUI">
   <dif>
      Fermentinta acida &ccirc;evalina lakto, uzata kiel
      <ref tip="super" cel="trink.0ajxo.KUI">trinka&jcirc;o</ref>:  
      <ekz>
        Aj&scirc;&aacute; alportis al ni kru&ccirc;ojn da bongusta <tld/>o
        <fnt>
          <aut>V. Glazunov</aut>,
          <vrk>Belulino Ajsul&uacute;</vrk>,
          <lok>La sfinkso de Steplando, 1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kefir.0o">kefiro</ref>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_m;&c_y;&c_s;</trd>
  <trd lng="bg">&c_k;&c_u;&c_m;&c_i;&c_s;</trd>
  <trd lng="cs">kumys</trd>
  <trd lng="de">Kumyss</trd>
  <trdgrp lng="en">
    <trd>koumiss</trd>,
    <trd>kumiss</trd>
  </trdgrp>
  <trd lng="fr">koumis</trd>
  <trd lng="hu">kumisz</trd>
  <trd lng="nl">koemis</trd>
  <trd lng="pt">c&uacute;mis</trd>
  <trd lng="ru">&c_k;&c_u;&c_m;&c_y;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kumys</trd>,
    <trd>kvasený nápoj z kobylieho</trd>,
    <trd>ťavieho alebo oslieho mlieka</trd>
  </trdgrp>
</drv>
</art>
