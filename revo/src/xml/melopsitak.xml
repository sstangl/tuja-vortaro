<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: melopsitak.xml,v 1.7 2013/09/03 17:10:20 revo Exp $">
<kap>
    <rad>melopsitak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="melopsitak.0o">
  <kap><tld/>o</kap>
  <snc mrk="melopsitak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      A&ubreve;stralia birdo,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="papage.0oj"><tld/>edoj</ref>
      (<trd lng="la">Melopsittacus undulatus</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_malmol;&c_l;&c_n;&c_i;&c_s;&c_t;&c_o; &c_p;&c_a;&c_p;&c_a;&c_g;&c_a;&c_l;&c_ch;&c_je;</trd>
  <trd lng="de">Wellensittich</trd>
  <trd lng="en">budgerigar</trd>
  <trd lng="fr">perruche ondul&eacute;e</trd>
  <trdgrp lng="hu">
  <trd>hull&aacute;mos <ind>papag&aacute;j</ind></trd>,
  <trd>t&ouml;rpepapag&aacute;j</trd>
  </trdgrp>
  <trd lng="nl">grasparkiet</trd>
</drv>
</art>

