<vortaro>
<art mrk="$Id: katako.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>katakomb</rad>/o
</kap>
<drv mrk="katako.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Subteraj galerioj, pasintece uzataj kiel tombejo a&ubreve; ostejo.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_k;&c_o;&c_m;&c_b;&c_a;</trd>
  <trd lng="ca">catacumba</trd>
  <trdgrp lng="cs">
    <trd>katakomba</trd>,
    <trd>katakomby</trd>,
    <trd>podzemní pohřebiště</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Katakombe</trd>
  </trdgrp>
  <trd lng="en">catacomb</trd>
  <trd lng="es">catacumba</trd>
  <trd lng="fr">catacombe</trd>
  <trd lng="hu">katakomba</trd>
  <trd lng="nl">catacombe</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_k;&c_o;&c_m;&c_b;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>katakomba</trd>,
    <trd>katakomby</trd>
  </trdgrp>
</drv>
</art>
