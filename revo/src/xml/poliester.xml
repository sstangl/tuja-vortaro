<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: poliester.xml,v 1.4 2015/06/29 16:40:18 revo Exp $">
<kap>
  <rad>poliester</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="poliester.0o">
  <kap><tld/>o</kap>
  <snc mrk="poliester.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Plur<ref tip="prt" cel="ester.0o.KEM">estero</ref>;
      malpeza, malmola, acidorezista substanco
      <ref tip="super" cel="polimer.0o.substanco">polimera</ref> 
      uzata por industriaj pecoj, membranoj, boteloj,
      kaj en fibra formo por tapi&scirc;oj, vestoj...:
      <ekz>
        firmao <ctl>3M</ctl> produktas diafanan <tld/>an filmon 
        <ctl>Y 3523</ctl>
        <fnt>
          <aut>[B. Kolker]</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Esp16/X09.html">
          Esperanto en 16 tagoj</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="poliester.0o.RIM_MARK">
    Kiel fibroj <tld/>oj aperas
    sub diversaj lokaj markonomoj:
    <em>Dacron</em>,
    <em>Diolen</em>,
    <em>Tergal</em>, 
    <em>Terital</em>, 
    <em>Terlenka</em>,
    <em>Terylene</em>,
    <em>Trevira</em>.
  </rim>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_je;&c_s;&c_t;&c_je;&c_r;</trd>
  <trd lng="ca">poli&egrave;ster</trd>
  <trd lng="cs">polyester</trd>
  <trd lng="da">polyester</trd>
  <trd lng="de">Polyester</trd>
  <trd lng="en">polyester</trd>
  <trd lng="fr">polyester</trd>
  <trd lng="hr">poliester</trd>
  <trd lng="it">poliestere</trd>
  <trd lng="nl">polyester</trd>
  <trd lng="pl">poliestry</trd>
  <trd lng="pt">poli&eacute;ster</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_e;&c_f;&c_i;&c_r;&c_y;</trd>
  <trd lng="sv">polyester</trd>
  <trd lng="sk">polyester</trd>
</drv>
</art>

