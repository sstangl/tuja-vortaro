<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolocint.xml,v 1.5 2012/01/02 11:10:20 revo Exp $">
<kap>
  <rad>kolocint</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kolocint.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolocint.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="koloki.0o">kolokinto</ref>
      <ekz>
        tiel amara ke, kompare kun &gcirc;i, <tld/>o dol&ccirc;as
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 39a, p.
        611a</lok></fnt>.
      </ekz>
  </snc>
  <trd lng="ca">coloquinta</trd>
  <trd lng="de">Koloquinte</trd>
  <trd lng="fr">coloquinte</trd>
  <trdgrp lng="hu">
    <trd>takarm&aacute;nydinnye</trd>,
    <trd>s&aacute;rt&ouml;k</trd>,
    <trd>keser&udblac; s&aacute;rt&ouml;k</trd>,
    <trd>takarm&aacute;ny-g&ouml;r&ouml;gdinnye</trd>
  </trdgrp>
  <trd lng="nl">kolokwint</trd>
  <trd lng="pt">coloqu&iacute;ntida</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_v;&c_i;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_c;&c_i;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>

