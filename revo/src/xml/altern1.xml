  <rad>alternanc</rad>/o
</kap>
<drv mrk="altern1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Duonperiodo de alterna fenomeno.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>alternace</trd>,
    <trd>alternanta</trd>,
    <trd>polokmit</trd>,
    <trd>střídání</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>halbe Schwingungsdauer</trd>,
    <trd>Alternanz</trd>
  </trdgrp>
  <trd lng="en">half-period</trd>
  <trdgrp lng="es">
    <trd>semiperiodo</trd>,
    <trd>semiper&iacute;odo</trd>
  </trdgrp>
  <trd lng="fr">alternance</trd>
  <trd lng="hu">f&eacute;lperi&oacute;dus</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
  <trd lng="sk">striedanie</trd>
</drv>
</art>
