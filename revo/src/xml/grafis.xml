<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grafis.xml,v 1.13 2015/09/22 09:10:18 revo Exp $">
<kap>
  <rad>grafism</rad>/o
</kap>

<drv mrk="grafis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <ref tip="dif" cel="sistem.skrib0o">skribsistemo</ref>
    <refgrp tip="vid">
      <ref cel="sistem.son0o.FON">sonsistemo</ref>,
      <ref cel="foneti1.0o">fonetismo</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">grafia</trd>
  <trdgrp lng="cs">
    <trd>grafika</trd>,
    <trd>pravopis</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Graphismus</trd>,
    <trd>grafisches System</trd>
  </trdgrp>
  <trd lng="es">graf&iacute;a</trd>
  <trd lng="fr">graphie</trd>
  <trdgrp lng="hu">
    <trd>grafikai rendszer</trd>,
    <trd>bet&udblac;rendszer</trd>
  </trdgrp>
  <trd lng="pl">grafika</trd>
  <trd lng="pt">grafismo</trd>
  <trdgrp lng="sk">
    <trd>grafika</trd>,
    <trd>pravopis</trd>
  </trdgrp>
</drv>
</art>
