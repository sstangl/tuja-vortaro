<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mont.xml,v 1.33 2015/07/04 08:37:39 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>mont</rad>/o
    </kap>

    <drv mrk="mont.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="mont.0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          Granda natura amaso da roko kaj tero, kiu tre alti&gcirc;as
          super la nivelo de la &ccirc;irka&ubreve;anta grundo:
          <ekz>
            la ondegoj levi&gcirc;as kiel <tld/>oj<fnt>Z</fnt>;
          </ekz><ekz>
            jen staras la bovoj anta&ubreve; la <tld/>o
            <klr>(jen la malfacila&jcirc;o!)</klr>
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="mont.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Amasego, multego:
          <ekz>
            <tld/>oj da kadavroj, da ruba&jcirc;oj, da papera&jcirc;oj;
          </ekz>
          <ekz>
            ni ne povas senfine toleri tiun <tld/>on da mizero;
          </ekz>
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>promesi orajn <tld/>ojn</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="ru">&c_s;&c_u;&c_l;&c_i;&c_t;&c_mol;
              <ind>&c_z;&c_o;&c_l;&c_o;&c_t;&c_y;&c_je; &c_g;&c_o;&c_r;&c_y;</ind>
            </trd>
          </ekz>
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>inter faro kaj rakonto staras meze granda <tld/>o</ind>
            <fnt><bib>PrV</bib></fnt>
            <trd lng="es">del dicho al hecho, hay mucho trecho</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="abism.0o">abismo</ref>
        </refgrp>
      </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hora</trd>,
    <trd>mont</trd>,
    <trd>mount</trd>
  </trdgrp>
  <trd lng="de">Berg</trd>
  <trd lng="en">mountain</trd>
  <trd lng="es">monta&ntilde;a</trd>
  <trdgrp lng="fr">
        <trd>mont</trd>,
        <trd>montagne</trd>
      </trdgrp>
  <trd lng="hu">hegy</trd>
  <trd lng="id">gunung</trd>
  <trd lng="nl">berg</trd>
  <trd lng="os">&c_h;&c_o;&c_h;</trd>
  <trd lng="pl">g&oacute;ra</trd>
  <trd lng="pt">monte</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hora</trd>,
    <trd>kopec</trd>,
    <trd>vrch</trd>
  </trdgrp>
  <trd lng="sv">berg</trd>
  <trd lng="vo">bel</trd>
</drv>

    <drv mrk="mont.0aro">
      <kap><tld/>aro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Masivo a&ubreve; &ccirc;eno de kunigitaj <tld/>oj formantaj
          tuton:
          <ekz>
            la Ka&ubreve;kaza, Pirenea <tld/>aro.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_g;&c_o;&c_r;&c_y;</trd>,
        <trd>&c_g;&c_o;&c_r;&c_n;&c_y; &c_m;&c_a;&c_s;&c_ib;&c_w;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>horstvo</trd>,
    <trd>pohoří</trd>
  </trdgrp>
  <trd lng="de">Gebirge</trd>
  <trd lng="en">mountain range</trd>
  <trd lng="es">cordillera</trd>
  <trdgrp lng="fr">
        <trd>montagne<klr> (chaine ou massif)</klr></trd>
      </trdgrp>
  <trd lng="hu">hegys&eacute;g</trd>
  <trd lng="id">[<ind>gunung</ind>] pegunungan</trd>
  <trd lng="nl">gebergte</trd>
  <trdgrp lng="pl">
        <trd>g&oacute;ry</trd>,
        <trd>pog&oacute;rze</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_g;&c_o;&c_r;&c_y;</trd>,
        <trd>&c_g;&c_o;&c_r;&c_n;&c_y;&c_j; &c_m;&c_a;&c_s;&c_s;&c_i;&c_v;</trd>
      </trdgrp>
  <trd lng="sk">horské pásmo</trd>
  <trd lng="sv">bergskedja</trd>
</drv>

    <drv mrk="mont.0eto">
      <kap><tld/>eto</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Malalta, malgranda <tld/>o, precipe tia kun plata
          a&ubreve; ronda supro.
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_g;&c_o;&c_r;&c_a;&c_k;</trd>,
        <trd>&c_u;&c_z;&c_g;&c_o;&c_r;&c_a;&c_k;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>kopec</trd>,
    <trd>návrší</trd>,
    <trd>pahorek</trd>,
    <trd>vrch</trd>
  </trdgrp>
  <trd lng="de">H&uuml;gel</trd>
  <trd lng="en">hill</trd>
  <trd lng="es">colina</trd>
  <trd lng="fr">colline</trd>
  <trd lng="id">bukit</trd>
  <trdgrp lng="pl">
        <trd>wzg&oacute;rze</trd>,
        <trd>wzniesienie</trd>
      </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_l;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>hrbolček</trd>,
    <trd>pahrbok</trd>,
    <trd>výčnelok</trd>
  </trdgrp>
</drv>

    <drv mrk="mont.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Alti&gcirc;i kvaza&ubreve; <tld/>o:
          <ekz>
            de via kolera blovo <tld/>i&gcirc;is akvo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">s'<ind>amonceler</ind></trd>
      <trd lng="pl">g&oacute;rowa&cacute;</trd>
    </drv>

    <drv mrk="mont.inter0o0pasejo">
      <kap>inter<tld/>o, <var><kap><tld/>pasejo</kap></var></kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Malpli alta trairejo inter du <tld/>oj.
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_l;</trd>
      <trdgrp lng="de">
        <trd>Bergpass</trd>,
        <trd>Pass</trd>
      </trdgrp>
      <trd lng="en">(mountain) <ind>pass</ind></trd>
      <trd lng="es">paso</trd>
      <trd lng="fr">col<klr> (de montagne)</klr></trd>
      <trd lng="hu">h&aacute;g&oacute;</trd>
      <trd lng="nl">bergpas</trd>
      <trd lng="pl">prze&lstroke;&eogonek;cz</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_a;&c_l;</trd>
    </drv>

    <drv mrk="mont.0bastono">
      <kap><tld/>bastono</kap>
      <snc>
        <dif>
          Longa bastono kun fera pinto uzata en <tld/>oj por faciligi
          grimpadon.
        </dif>
      </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_p;&c_e;&c_n;&c_sh;&c_t;&c_o;&c_k;</trd>
  <trd lng="cs">turistická hůl</trd>
  <trd lng="de">Wanderstock</trd>
  <trd lng="fr">b&acirc;ton de marche</trd>
  <trd lng="nl">bergstok</trd>
  <trd lng="pl">czekan</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_p;&c_je;&c_n;&c_sh;&c_t;&c_o;&c_k;</trd>
  <trd lng="sk">turistická palica</trd>
</drv>

    <drv mrk="mont.0fendo">
      <kap><tld/>fendo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Interkrutejo.
        </dif>
      </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">horská strž</trd>
  <trd lng="fr">crevasse</trd>
  <trd lng="nl">bergkloof</trd>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">horská strž</trd>
</drv>

    <drv mrk="mont.tomb0eto">
      <kap>tomb<tld/>eto</kap>
      <uzo tip="fak">ARKE</uzo>
      <snc>
        <dif>
          Tera a&ubreve; &scirc;tona amaso super tombo, &gcirc;in
          &scirc;irmanta kontra&ubreve; besta a&ubreve; homa
          rabado:
          <ekz>
            diference de la anta&ubreve;historia tomb<tld/>eto, havanta
            konusan formon, la vikingaj tomb<tld/>etoj estas kutime
            plataj
            <fnt><bib>IntArkeol</bib>, <lok>p. 41a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trd lng="cs">rov</trd>
  <trd lng="de">Grabh&uuml;gel</trd>
  <trd lng="en">(burial) <ind>mound</ind></trd>
  <trd lng="es">t&uacute;mulo</trd>
  <trd lng="fr">tumulus</trd>
  <trd lng="pl">kurchan</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>hrob</trd>,
    <trd>rov</trd>
  </trdgrp>
</drv>

    <drv mrk="mont.0oprediko">
      <kap><tld/>oprediko</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Prediko farita de <ref cel="jesu.0o">Jesuo</ref> sur
          (la&ubreve; Mateo) a&ubreve; apud (la&ubreve; Luko) <tld/>o al
          siaj dis&ccirc;iploj
          kaj granda homamaso:
          <ekz>
            min mem trafis, ekzemple, la paragrafoj 612&ndash;621
            kvaza&ubreve; e&hcirc;oj de l' t.n. <tld/>oprediko de Jesuo en la
            evangelio la&ubreve; Mateo
            <fnt>
              <aut>Gerrit BERVELING</aut>:
              <vrk>
                <url
                ref="http://www.esperanto.be/fel/mon/rec/dvoj.html">
                  Sankta Skribo de Oomoto en Esperanto
                </url>
              </vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Bergpredigt</trd>
      <trd lng="en">Sermon on the Mount</trd>
      <trd lng="ru">&c_n;&c_a;&c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_p;&c_o;&c_v;&c_je;&c_d;&c_mol;</trd>
    </drv>

    <drv mrk="mont.tablo0o">
      <kap>tablo<tld/>o <fnt>PIV2</fnt></kap>
      <snc mrk="mont.tablo0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <tld lit="M"/>o kun ebena supra&jcirc;o.
        </dif>
      </snc>
      <trd lng="de">Tafelberg</trd>
      <trd lng="en">mesa</trd>
      <trd lng="pl">g&oacute;ra sto&lstroke;owa</trd>
      <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_g;&c_o;&c_r;&c_a;</trd>
    </drv>

    <drv mrk="mont.Tablo0o">
      <kap>Tablo<tld/>o</kap>
      <snc mrk="mont.Tablo0o.AST">
        <uzo tip="fak">AST</uzo>
        <mlg>Men</mlg>
        <dif>
          <ref tip="lst" cel="stel.0figuro"
          lst="voko:stelfiguroj">Stelfiguro</ref>
          en la suda duonsfero (<trd lng="la">Mensa</trd>).
        </dif>
      </snc>
      <trd lng="de">Tafelberg</trd>
      <trd lng="en">Mensa</trd>
      <trd lng="es">Mesa</trd>
      <trd lng="pl">G&oacute;ra Sto&lstroke;owa<klr> (gwiazdozbi&oacute;r)</klr></trd>
      <trd lng="ru">&c_S;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; &c_G;&c_o;&c_r;&c_a;<klr>
      (&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
    </drv>

  </art>
