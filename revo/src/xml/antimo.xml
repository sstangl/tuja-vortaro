<kap>
  <ofc>*</ofc>
  <rad>antimon</rad>/o
</kap>
<drv mrk="antimo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Sb</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="51">elemento</ref> kun
      atomnumero 51, atompezo 121,75; blanka
      blueta <ref tip="super" cel="metal.0o">metalo</ref>,
      de lamena strukturo, uzata en industrio kaj medicino.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_m;&c_a;</trd>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_i;&c_m;&c_o;&c_n;</trd>
