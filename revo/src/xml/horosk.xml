<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: horosk.xml,v 1.18 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>horoskop</rad>/o
</kap>
<drv mrk="horosk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Observado pri la situacio de iuj planedoj a&ubreve; stelaroj,
      je la horo de naski&gcirc;o de infano, la&ubreve; kiu la
      <ref tip="vid" cel="astrol1.0o">astrologoj</ref> pretendas
      anta&ubreve;diri ties estontan sorton:
      <ekz>starigi ies <tld/>on.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxgur.0o">a&ubreve;guro</ref>,
      <ref cel="profet.0ajxo">profeta&jcirc;o</ref>.
    </refgrp>
    <ref tip="malprt" cel="astrol2.0o">astrologio</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="cs">horoskop</trd>
  <trd lng="de">Horoskop</trd>
  <trd lng="el">&omega;&rho;&omikron;&sigma;&kappa;&omikron_ton;&pi;&jota;&omikron;</trd>
  <trd lng="en">horoscope</trd>
  <trd lng="es">hor&oacute;scopo</trd>
  <trd lng="fr">horoscope</trd>
  <trd lng="hu">horoszk&oacute;p</trd>
  <trd lng="it">oroscopo</trd>
  <trd lng="nl">horoscoop</trd>
  <trd lng="pl">horoskop</trd>
  <trd lng="pt">hor&oacute;scopo</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="sk">horoskop</trd>
  <trd lng="sv">horoskop</trd>
</drv>
</art>
