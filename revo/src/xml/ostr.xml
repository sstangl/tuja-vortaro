<kap><ofc>*</ofc> <rad>ostr</rad>/o</kap>
<drv mrk="ostr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ostred.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ostrea</trd>), man&gcirc;ebla konko.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_t;&c_r;&c_y;&c_c;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_i;&c_d;&c_a;</trd>
  <trd lng="br">istrenn</trd>
  <trd lng="cs">&uacute;st&rcaron;ice</trd>
  <trd lng="de">Auster</trd>
  <trd lng="el">&sigma;&tau;&rho;&epsilon;&jota_ton;&delta;&jota;</trd>
  <trd lng="en">oyster</trd>
  <trd lng="es">ostra</trd>
  <trd lng="fr">hu&icirc;tre</trd>
  <trd lng="hu">osztriga</trd>
  <trd lng="nl">oester</trd>
  <trd lng="pl">ostryga</trd>
  <trd lng="pt">ostra</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">ustrica</trd>
  <trd lng="sv">ostron</trd>
</drv>
</art>
