<kap>
  <rad>kopernici</rad>/o
</kap>
<drv mrk="kopernici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Cn</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="112">elemento</ref> kun
      atomnumero 112, atommaso 285 u;
      radioaktiva, rapide disfalanta likva a&ubreve; eble gasa transirmetalo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_c;&c_y;&c_j;</trd>
  <trd lng="bg">&c_k;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_c;&c_i;&c_j;</trd>
  <trd lng="br">kopernisiom</trd>
  <trd lng="ca">copernici</trd>
  <trd lng="de">copernicium</trd>
  <trd lng="en">copernicium</trd>
  <trd lng="es">copernicio</trd>
  <trd lng="fr">copernicium</trd>
  <trd lng="hr">kopernicij</trd>
  <trd lng="hu">kopern&iacute;cium</trd>
  <trd lng="id">kopernicium</trd>
  <trd lng="it">copernicio</trd>
  <trd lng="la">copernicium</trd>
  <trd lng="nl">copernicium</trd>
  <trd lng="pl">kopernik</trd>
  <trd lng="pt">copern&iacute;cio</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_c;&c_i;&c_j;</trd>
  <trd lng="sk">kopernikium</trd>
  <trd lng="sr">&c_k;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_c;&c_i;&#1112;&c_u;&c_m;</trd>
  <trd lng="sv">copernicium</trd>
</drv>
</art>

