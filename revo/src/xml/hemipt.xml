<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hemipt.xml,v 1.15 2015/07/29 16:10:18 revo Exp $">
<kap>
  <rad>hemipter</rad>/oj
</kap>

<drv mrk="hemipt.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Heteroptera</trd>), kun du paroj da
      flugiloj, kies ekstera paro estas ofte korneca.
    </dif>
    <ref tip="vid" cel="homopt.0oj">homopteroj</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_u;&c_t;&c_v;&c_malmol;&c_r;&c_d;&c_o;&c_k;&c_r;&c_i;&c_l;&c_i;</trd>
  <trd lng="ca">heter&ograve;pters</trd>
  <trdgrp lng="de">
    <trd>Wanzen</trd>,
    <trd>Ungleichfl&uuml;gler</trd>,
    <trd>Halbfl&uuml;gler</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;mipt&egrave;res</trd>
  <trdgrp lng="hu">
    <trd>polosk&aacute;k</trd>,
    <trd>felem&aacute;ssz&aacute;rny&uacute;ak</trd>
  </trdgrp>
  <trd lng="it">emitteri</trd>
  <trdgrp lng="nl">
    <trd>snavelinsecten</trd>,
    <trd>halfvleugeligen<klr tip="amb"> (insecten)</klr></trd>
  </trdgrp>
  <trd lng="pt">hem&iacute;pteros</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;</trd>
</drv>

<drv mrk="hemipt.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Apartenanta al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Wanzen-</trd>
  </trdgrp>
  <trd lng="ca">heter&ograve;pter</trd>
  <trd lng="fr">h&eacute;mipt&egrave;re<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">felem&aacute;ssz&aacute;rny&uacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_j;</trd>
</drv>
</art>
