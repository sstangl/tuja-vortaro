<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eferve.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>efervesk</rad>/i
</kap>
<drv mrk="eferve.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Boli produktante eli&gcirc;an gason en ia likva&jcirc;o:
      <ekz>
        kiam acido atakas iujn substancojn &gcirc;i <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_mol;&c_p;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>perlit se</trd>,
    <trd>pěnit</trd>,
    <trd>zpěnit</trd>,
    <trd>šlehat</trd>,
    <trd>šumět</trd>,
    <trd>šumět <klr>(víno)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausen</trd>,
    <trd>aufwallen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervesce</trd>,
    <trd>bubble</trd>,
    <trd>fizz</trd>
  </trdgrp>
  <trd lng="fr">bouillonner<klr tip="ind"> (par
  effervescence)</klr></trd>
  <trdgrp lng="hu">
    <trd>pezseg</trd>,
    <trd>habzik</trd>
  </trdgrp>
  <trd lng="nl">bruisen</trd>
  <trdgrp lng="pl">
    <trd>pieni&cacute; si&eogonek;</trd>,
    <trd>musowa&cacute;</trd>,
    <trd>buzowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">efervescer</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bublať</trd>,
    <trd>perliť</trd>,
    <trd>šumieť</trd>
  </trdgrp>
</drv>

<drv mrk="eferve.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <tld/>a pulvoro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>prchlivý (přen.)</trd>,
    <trd>šumivý <klr>(nápoj)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausend</trd>,
    <trd>aufwallend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervescent</trd>,
    <trd>fizzy</trd>
  </trdgrp>
  <trd lng="es">efervescente</trd>
  <trd lng="fr">effervescent</trd>
  <trdgrp lng="hu">
    <trd>pezsg&odblac;</trd>,
    <trd>habz&oacute;</trd>
  </trdgrp>
  <trd lng="nl">bruisend</trd>
  <trdgrp lng="pl">
    <trd>pieni&aogonek;cy</trd>,
    <trd>musuj&aogonek;cy</trd>,
    <trd>buzuj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_p;&c_u;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">šumivý</trd>
</drv>

<drv mrk="eferve.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tio, kio <tld/>as; rezulto de tiu ago.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>efervescence</trd>,
    <trd>klokot</trd>,
    <trd>napěnění</trd>,
    <trd>pěnění</trd>,
    <trd>rozčilení</trd>,
    <trd>zpěnění</trd>,
    <trd>šlehání</trd>,
    <trd>šumění <klr>(minerálky)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufbrausen</trd>,
    <trd>Aufwallen</trd>,
    <trd>Aufwallung</trd>
  </trdgrp>
  <trd lng="en">effervescence</trd>
  <trd lng="fr">effervescence</trd>
  <trdgrp lng="hu">
    <trd>pezsg&eacute;s</trd>,
    <trd>habz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spienianie</trd>,
    <trd>musowanie</trd>,
    <trd>buzowanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_k;&c_i;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_sh;&c_i;&c_p;&c_je;&c_n;&c_i;&c_je;<klr tip="amb">
    (&c_p;&c_r;&c_i; &c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_i;
    &c_g;&c_a;&c_z;&c_o;&c_v;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>penenie</trd>,
    <trd>šumenie</trd>
  </trdgrp>
</drv>
</art>
