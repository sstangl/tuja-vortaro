<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ampelo.xml,v 1.15 2015/07/04 08:37:30 revo Exp $">
<kap>
<rad>ampelops</rad>/o
</kap>
<drv mrk="ampelo.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="vitac.0oj">vitacoj</ref>
       (<trd lng="la">Ampelopsis</trd>), ornama grimpa 
       <ref tip="lst" cel="arbust.0o"
       lst="voko:vitoparencoj">arbusto</ref>,
       kies folioj similas tiujn de vinberujo.
     </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>divoké víno</trd>,
    <trd>loubinec</trd>
  </trdgrp>
  <trd lng="de">Doldenrebe</trd>
  <trd lng="en">ampelopsis creeper</trd>
  <trdgrp lng="fr">
     <trd>amp&eacute;lopsis</trd>,
     <trd>vigne vierge</trd></trdgrp>
  <trd lng="hu">borosty&aacute;nsz&odblac;l&odblac;</trd>
  <trd lng="nl">wilde <ind>wingerd</ind></trd>
  <trdgrp lng="ru">
 
<trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
,
     <trd>&c_a;&c_m;&c_p;&c_je;&c_l;&c_o;&c_p;&c_s;&c_i;&c_s;</trd>
   </trdgrp>
  <trd lng="sk">divoké víno</trd>
  <trd lng="sv">vildvin</trd>
</drv>
</art>
