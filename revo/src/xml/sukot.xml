<vortaro>
<art mrk="$Id: sukot.xml,v 1.2 2013/04/11 10:10:19 revo Exp $">
<kap>
  <rad>Sukot</rad>/o
</kap>
<drv mrk="sukot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sukot.0o.JUD">
    <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="fest.0odelauxboj.JUD">Festo de La&ubreve;boj</ref>
    <ekz>
      la festo nomata <tld/>o <klr>...</klr> rememor<klr>ig</klr>is la 
      liberigon de la egipta sklaveco
      <fnt>
        <aut>P. A. Maggi</aut>,
        <vrk><url
        ref="http://www.studibiblici.it/ESPERANTO/Omelie/28%2002%2010%20rev.pdf">
        Dua Karesma Diman&ccirc;o</url></vrk>,
        <lok>2010-02-28</lok>
      </fnt>;
    </ekz>
    <ekz>
      dum bibliaj tempoj <tld/>o estis la festo, kiam la nacioj de la 
      mondo venis al Jerusalemo por celebri kun la juda popolo
      <fnt>
        <aut>Rita</aut>,
        <vrk><url ref="http://www.ipernity.com/blog/ritafoto/311553">
        Sukoto</url></vrk>,
        <lok>2011-02-25</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">Souccot</trd>
  <trdgrp lng="pl">
    <trd>Sukkot</trd>,
    <trd>Kuczki</trd>,
    <trd>&Sacute;wi&eogonek;to <ind>Namiot&oacute;w</ind></trd>
  </trdgrp>
</drv>
</art>

