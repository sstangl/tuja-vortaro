<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: makler.xml,v 1.21 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>makler</rad>/i
</kap>
<drv mrk="makler.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Profesie servi kiel profitanta peranto inter vendantoj kaj
      a&ccirc;etantoj:
      <ekz>
        <tld/>i pri teksa&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u;
  &c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_ib;)</klr></trd>
  <trd lng="ca">mitjan&ccedil;ar</trd>
  <trdgrp lng="cs">
    <trd>prostředkovat</trd>,
    <trd>zprostředkovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>makeln</trd>,
    <trd>als <ind>Makler</ind> t&auml;tig sein</trd>,
    <trd>vermitteln</trd>
  </trdgrp>
  <trd lng="en">broker</trd>
  <trd lng="es"><ind>intermediar</ind> a comisi&oacute;n en una transacci&oacute;n
  comercial</trd>
  <trdgrp lng="fr">
    <trd>faire l'<ind>interm&eacute;diaire</ind> commercial</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;zletet k&ouml;zvet&iacute;t</trd>,
    <trd>&uuml;gyn&ouml;k&ouml;sk&ouml;dik</trd>
  </trdgrp>
  <trd lng="nl">makelen</trd>
  <trdgrp lng="pt">
    <trd>corretar</trd>,
    <trd>promover a <ind>corretagem</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">sprostredkovať</trd>
</drv>

<drv mrk="makler.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Pago al la perinto en vendafero:
      <ekz>
        oni pagas iun procenta&jcirc;on kiel <tld/>a&jcirc;on al specialistoj
        <klr>...</klr> kiuj helpas dum la negocado<fnt><aut>Roland
        Rotsaert</aut>: <vrk>&Scirc;mirmono</vrk>, <bib>Monato</bib><lok>jaro
        2000a, numero 2a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_j;&c_n;&c_a;&c_je; &c_w;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>comissi&oacute;</trd>,
    <trd>corretatge</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Maklergeb&uuml;hr</trd>,
    <trd>Maklerprovision</trd>,
    <trd>Maklercourtage</trd>,
    <trd>Vermittlungsgeb&uuml;hr</trd>,
    <trd>Vermittlungsprovision</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>comisi&oacute;n</trd>,
    <trd>corretaje</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commission<klr> (courtage)</klr></trd>,
    <trd>courtage<klr> (commission)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>alkuszd&iacute;j</trd>,
    <trd>k&ouml;zvet&iacute;t&odblac;i d&iacute;j</trd>
  </trdgrp>
  <trd lng="nl">makelaarsloon</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; &c_v;&c_o;&c_z;&c_n;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
</drv>

<drv mrk="makler.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref> kiu profesie
      <tld/>adas.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</trd>
  <trd lng="ca">mitjancer</trd>
  <trdgrp lng="cs">
    <trd>dohazovač</trd>,
    <trd>makléř</trd>,
    <trd>makléř</trd>,
    <trd>zprostředkovatel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Makler</trd>,
    <trd>Vermittler</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>agente <klr tip="amb">(vendendor)</klr></trd>,
    <trd>corredor <klr tip="amb">(vendendor)</klr></trd>,
    <trd>intermediario <klr tip="amb">(vendedor)</klr></trd>,
    <trd>comisionista</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>broker</trd>,
    <trd>courtier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;gyn&ouml;k</trd>,
    <trd>k&ouml;zvet&iacute;t&odblac; <klr tip="ind">(&uuml;zlet&eacute;)</klr></trd>,
    <trd>alkusz</trd>,
    <trd>br&oacute;ker</trd>
  </trdgrp>
  <trd lng="nl">makelaar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>maklér</trd>,
    <trd>sprostredkovateľ</trd>
  </trdgrp>
</drv>

<drv mrk="makler.bil0isto">
  <kap>bil<tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Makleristo pri biloj, ekz-e akcioj.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_n;&c_d;&c_a;&c_v;&c_y; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>
  <trd lng="ca">marxant</trd>
  <trdgrp lng="de">
    <trd>Effektenmakler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stockbroker</trd>
  </trdgrp>
  <trd lng="hu">&eacute;rt&eacute;kpap&iacute;r&uuml;gyn&ouml;k</trd>
  <trd lng="nl">effectenmakelaar</trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_d;&c_o;&c_v;&c_y;&c_j; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>
</drv>

<drv mrk="makler.borsa0isto">
  <kap>borsa <tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Makleristo makleranta en borso, ekz-e rilate al akcioj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>
  <trdgrp lng="de">
    <trd>B&ouml;rsenmakler</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>agent de borsa</trd>,
    <trd>broker</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>corredor</ind> de bolsa</trd>,
    <trd><ind>agente</ind> de bolsa</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&odblac;zsde&uuml;gyn&ouml;k</trd>,
    <trd>r&eacute;szv&eacute;ny&uuml;gyn&ouml;k</trd>,
    <trd>t&odblac;zsdealkusz</trd>,
    <trd>br&oacute;ker</trd>
  </trdgrp>
  <trd lng="nl">beursmakelaar</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="makler.kambi0isto">
  <kap>kambia <tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Makleristo pri kambioj. Kambi-makleristo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_e;&c_k;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_v;&c_e;&c_k;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="ca">agent de canvi</trd>
  <trd lng="cs">směnečný makléř</trd>
  <trdgrp lng="de">
    <trd>Wechselmakler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bill broker</trd>
  </trdgrp>
  <trd lng="es"><ind>agente</ind> de cambio</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;lt&oacute;alkusz</trd>,
    <trd>lesz&aacute;m&iacute;tol&aacute;si &uuml;gyn&ouml;k</trd>,
    <trd>lesz&aacute;m&iacute;tol&aacute;si alkusz</trd>
  </trdgrp>
  <trd lng="nl">wisselmakelaar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="sk">zmenkový maklér</trd>
</drv>

</art>
