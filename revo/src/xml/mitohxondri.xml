<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mitohxondri.xml,v 1.7 2014/11/10 05:10:17 revo Exp $">
<kap>
  <rad>mito&hcirc;ondri</rad>/o
</kap>
<drv mrk="mitohxondri.0o">
  <kap><tld/>o</kap>
  <snc mrk="mitohxondri.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Iu el la <ref tip="super" cel="organ.0eto.FIZL">organetoj</ref>
      troveblaj en plejmultaj e&ubreve;kariotaj &ccirc;eloj,
      kiuj konsumas sukeron kaj oksigenon kaj la &ccirc;elojn provizas
      per energio:
      <ekz>
        <tld/>oj havas multajn trajtojn de bakterioj, inkluzive de
        propra DNA
        <fnt>
          <aut>K. Lendon</aut>,
          <vrk>Proterozoika Erao</vrk>,
          <lok><url
          ref="http://groups.yahoo.com/group/aeko/message/845">
          aeko</url>, 2002-06-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>...</klr> provizas la &ccirc;elon per energio
        <fnt>
          <aut>Teknika Altlernejo Brno</aut>,
          <vrk><url
          ref="http://www.uea.org/vikio/Komputilo_Kaj_La_Homa_Cerbo">
          Komputilo Kaj La Homa Cerbo</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_t;&c_a;&c_h;&c_o;&c_n;&c_d;&c_r;&c_y;&c_ja;</trd>
   <trd lng="ca">mitocondri</trd>
   <trdgrp lng="de">
     <trd>Mitochondrium</trd>
     <trd>Mitochondrion</trd>
   </trdgrp>
   <trd lng="en">mitochondrion</trd> 
   <trd lng="es">mitocondria</trd>   
   <trd lng="fr">mitochondrie</trd>
   <trdgrp lng="nl">
     <trd>mitochondrium</trd>
     <trd>mitochondrion</trd>
   </trdgrp>
   <trd lng="no">mitokondrium</trd>
   <trd lng="pl">mitochondrium</trd>
  </snc>
</drv>
</art>

