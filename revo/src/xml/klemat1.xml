<kap>
  <rad>klematid</rad>/o
</kap>
<drv mrk="klemat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Clematis</trd>), volve grimpanta vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Clematis</trd>,
    <trd>clem&agrave;tide</trd>
  </trdgrp>
  <trd lng="de">Waldrebe</trd>
  <trdgrp lng="en">
    <trd>clematis</trd>,
    <trd>old man's beard</trd>,
    <trd>traveller's joy</trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_l;&a_m;&a_A;&a_t;&f_y;&a_s;</trd>
  <trd lng="fr">cl&eacute;matite</trd>
  <trdgrp lng="hu">
    <trd>iszalag</trd>,
    <trd>b&eacute;rcse</trd>,
    <trd>klem&aacute;tisz</trd>
  </trdgrp>
  <trd lng="nl">clematis</trd>
  <trd lng="ru">&c_l;&c_o;&c_m;&c_o;&c_n;&c_o;&c_s;</trd>
</drv>
</art>
