<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ial.xml,v 1.17 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ial</rad>
</kap>
<drv mrk="ial.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo, signifanta: "pro ia ka&ubreve;zo":
      <ekz>strange, la mondo <tld/> rigardas nin senkonfide;</ekz>
      <ekz>niaj leteroj <tld/> ne estas alvenintaj.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_m;&c_u;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>z jakéhosi důvodu</trd>,
    <trd>z nějaké příčiny</trd>
  </trdgrp>
  <trd lng="de">aus irgendeinem <ind>Grund</ind></trd>
  <trd lng="en">for some reason</trd>
  <trd lng="es">por alguna raz&oacute;n</trd>
  <trd lng="fr">pour une certaine <ind>raison</ind></trd>
  <trdgrp lng="hu">
    <trd>valami&eacute;rt<klr tip="ind"> (valami ok miatt)</klr></trd>,
    <trd>valami miatt</trd>,
  </trdgrp>
  <trd lng="nl">om de een of andere <ind>reden</ind></trd>
  <trdgrp lng="pl">
    <trd>z jakiego&sacute; powodu</trd>,
    <trd>z jakiej&sacute; przyczyny</trd>,
    <trd>jako&sacute;</trd>,
    <trd>jako&sacute;tam</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>por algum <ind>motivo</ind></trd>, 
   <trd>por um <ind>motivo</ind> qualquer</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_je;&c_m;&c_u;-&c_t;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_je;&c_m;-&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pre niečo</trd>,
    <trd>z nejakej príčiny</trd>
  </trdgrp>
</drv>
</art>
