<vortaro>
<art mrk="$Id: lakrimal.xml,v 1.12 2015/07/04 08:37:38 revo Exp $">
<kap><rad>lakrimal</rad>/o</kap>
<drv mrk="lakrimal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Ccirc;iu el la paro de etaj <ref tip="super" cel="ost.0o">osto</ref>j
      situantaj &ccirc;e la anta&ubreve;a
      interna parto de <ref cel="orbit.0o.ANA">orbito</ref>).
    </dif>
  </snc>
  <trd lng="ca">unguis</trd>
  <trd lng="cs">slzná kost</trd>
  <trd lng="de">Tr&auml;nenbein</trd>
  <trd lng="es">hueso <ind>lagrimal</ind></trd>
  <trd lng="hu">k&ouml;nnycsont</trd>
  <trd lng="it">osso <ind>lacrimale</ind></trd>
  <trd lng="la">Os lacrimale</trd>
  <trd lng="nl">traanbeen</trd>
  <trdgrp lng="pt">
    <trd>&uacute;ng&uuml;e</trd>,
    <trd>&uacute;ngui</trd>,
    <trd>lacrimal <klr tip="amb">(subst., anat.)</klr></trd>
  </trdgrp>
  <trd lng="sk">slzná kosť</trd>
</drv>
</art>
