<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: noc.xml,v 1.10 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>noc</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="noc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="util.mal0i">malutili</ref>
    <ekz>
      vi <klr>...</klr> devas mem faciligi la koron, por ne <tld/>i la
      farton
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p. 
385-386a</lok></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="difekt.0i">difekti</ref>,
      <ref cel="domagx.0i">doma&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>danyar</trd>,
    <trd>perjudicar</trd>
  </trdgrp>
  <trd lng="cs">škodit</trd>
  <trd lng="de">schaden</trd>
  <trdgrp lng="es">
    <trd>da&ntilde;ar</trd>,
    <trd>perjudicar</trd>
  </trdgrp>
  <trd lng="fr">nuire</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rt</trd>,
    <trd>k&aacute;ros</trd>
  </trdgrp>
  <trd lng="nl">schaden<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pt">
    <trd>prejudicar</trd>,
    <trd>ser <ind>prejudicial</ind> a</trd>,
    <trd>ser <ind>nocivo</ind> a</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">škodiť</trd>
  <trd lng="tp">ike</trd>
</drv>
</art>

