<drv mrk="radiot3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Metodo de kuracado per radioj:
      <ekz>
        pli bona kvalito en <tld/>o povus altigi la procenton de
        postvivantoj<fnt><aut>Evgeni Georgiev</aut>: <vrk>Novaj
        kuracmetodoj kaj prevento</vrk>, <bib>Monato</bib><lok>jaro
        2001a,
        numero 4a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_jo;&c_t;&c_e;&c_r;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_l;&c_malmol;&c_ch;&c_je;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>radiol&eacute;&ccaron;ba</trd>,
    <trd>radioterapie</trd>
  </trdgrp>
  <trd lng="de">Strahlentherapie</trd>
  <trd lng="en">radiation therapy</trd>
  <trdgrp lng="fr">
    <trd>radioth&eacute;rapie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sug&aacute;rkezel&eacute;s</trd>,
    <trd>sug&aacute;rter&aacute;pia</trd>,
    <trd>radioter&aacute;pia</trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>radiotherapie</trd>,
    <trd>bestrahlingstherapie</trd>
  </trdgrp>
  <trd lng="pt">radioterapia</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">r&aacute;dioterapia</trd>
</drv>
