<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: papu.xml,v 1.7 2014/06/16 15:10:15 revo Exp $">
<kap>
  <rad>Papu</rad>/o
</kap>

<drv mrk="papu.P0o">
  <kap>
    <tld lit="P"/>o
  </kap>
  <snc mrk="papu.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Iama lando en la sudorienta parto de la insulo <ref tip="malprt" 
      cel="gvine.NovG0o.GEOG">Nov Gvineo</ref>,
      teritorio dependa de Britujo kaj poste de A&ubreve;stralio,
      nuntempe parto de <ref tip="malprt"
      cel="papu.P0oNovGvineo.sxtato">Papuo-Nov-Gvineo</ref>.
    </dif>
    <trd lng="fa">&f_p;&a_A;&f_p;&a_w;&a_A_madda;</trd>
    <trd lng="fr">Papouasie</trd>
    <trdgrp lng="pl">
      <trd>Papua</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="papu.P0oNovGvineo">
  <kap>
    <tld lit="P"/>o-Nov-Gvineo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="papu.P0oNovGvineo.sxtato">
    <uzo tip="fak">POL</uzo>
    <mlg>PG</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
      (&ccirc;efe en la insulo <ref tip="malprt"
      cel="gvine.NovG0o.GEOG">Nov Gvineo</ref>)
      kun &ccirc;efurbo Port-Moresbo;
      &gcirc;i limas kun
      <ref tip="vid" cel="indonezi.I0o.GEOG">Indonezio</ref>.
    </dif>
    <trd lng="fa">&f_p;&a_A;&f_p;&a_w;&a_A_madda; &f_g;&f_y;&a_n;&a_h;&a_hamza_sure; &a_n;&a_w;</trd>
    <trdgrp lng="fr">
      <trd>Papouasie-Nouvelle-Guin&eacute;e</trd>,
      <trd>Etat ind&eacute;pendant de Papouasie-Nouvelle-Guin&eacute;e</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Papua-Nowa Gwinea</trd>,
      <trd>Niezale&zdot;ne Pa&nacute;stwo <ind>Papui-Nowej
      Gwinei</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

