<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bid.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bid</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bid.0o">
  <kap><tld/>o</kap>
  <snc mrk="bid.0o.JUV">
    <dif>
      Globeto a&ubreve; alia solida&jcirc;eto kun tratruo por tredi
      fadenon:
      <ekz>
        kol&ccirc;eno el smeraldaj <tld/>oj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 72a, volumo 2a, p.
        527a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_je;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>korálek</trd>,
    <trd>olivka</trd>
  </trdgrp>
  <trd lng="de">Perle<klr tip="amb"> (Lochk&uuml;gelchen)</klr></trd>
  <trd lng="en">bead</trd>
  <trdgrp lng="fr">
    <trd>grain<klr> (de chapelet etc.)</klr></trd>,
    <trd>boule<klr> (de boulier)</klr></trd>,
    <trd>perle<klr> (de collier etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>koralik</trd>,
    <trd>paciorek</trd>
  </trdgrp>
  <trd lng="pt">conta</trd>
  <trd lng="ru">&c_b;&c_u;&c_s;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">korále <klr>(nepravé)</klr></trd>
</drv>

<drv mrk="bid.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="bid.0aro.JUV">
    <dif>
      Vico de <tld/>oj sur fadeno, uzebla kiel ludilo, galanterio
      (kol&ccirc;ene, brak&ccirc;ene), pre&gcirc;ohelpilo,
      pordopenda&jcirc;o...:
      <ekz>
        mi sendas al vi parfuman <tld/>aron por esprimi mian koron
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        566a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="pt">
    <trd>colar</trd>,
    <trd>ros&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_c;&c_je;&c_r;&c_k;&c_ib;</trd>,
    <trd>&c_r;&c_u;&c_zh;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="de">Kette<klr tip="amb"> (mit K&uuml;gelchen)</klr></trd>
  <trd lng="en">beads</trd>
  <trdgrp lng="fr">
    <trd>collier<klr> (de perles, verroterie...)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korale</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_s;&c_y;</trd>,
    <trd>&c_ch;&c_jo;&c_t;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>
