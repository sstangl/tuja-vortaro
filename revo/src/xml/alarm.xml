<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alarm.xml,v 1.22 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>alarm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="alarm.0o">
  <kap><tld/>o</kap>
  <snc mrk="alarm.0o.KOMUNE">
    <dif>
      Signalo por anonci minacantan dan&gcirc;eron a&ubreve; voki al
      batalo a&ubreve; helpo:
      <ekz>
        krii <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_v;&c_o;&c_g;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>alarm</trd>,
    <trd>poplach</trd>
  </trdgrp>
  <trd lng="de">Alarm</trd>
  <trd lng="en">alarm</trd>
  <trd lng="es">alarma</trd>
  <trdgrp lng="fr">
    <trd>alarme</trd>,
    <trd>alerte<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>riad&oacute;</trd>,
    <trd>riaszt&aacute;s</trd>
  </trdgrp>
  <trd lng="it">allarme</trd>
  <trd lng="nl">alarm</trd>
  <trdgrp lng="pt">
    <trd>alarme</trd>,
    <trd>alarma</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>alarm</trd>,
    <trd>poplach</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0a">
  <kap><tld/>a</kap>
  <snc mrk="alarm.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, maltrankviliga:
      <ekz>
        la <tld/>a stato de la regno.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_v;&c_o;&c_zh;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>poplachový</trd>,
    <trd>poplašný</trd>
  </trdgrp>
  <trd lng="de">alarmierend</trd>
  <trd lng="en">alarming</trd>
  <trd lng="fr">alarmant</trd>
  <trd lng="hu">riaszt&oacute;</trd>
  <trd lng="nl">alarmerend</trd>
  <trd lng="pt">alarmante</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">poplašný</trd>
</drv>

<drv mrk="alarm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="alarm.0i.KOMUNE">
    <dif>
      Voki <tld/>ante.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol; &c_t;&c_r;&c_y;&c_v;&c_o;&c_g;&c_u;</trd>
  <trdgrp lng="cs">
    <trd>alarmovat</trd>,
    <trd>volat do zbraně</trd>,
    <trd>vyhlásit poplach</trd>
  </trdgrp>
  <trd lng="de">alarmieren</trd>
  <trd lng="en">alarm</trd>
  <trdgrp lng="fr">
    <trd>alarmer</trd>,
    <trd>alerter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>riaszt</trd>,
    <trd>riad&oacute;ztat</trd>
  </trdgrp>
  <trd lng="nl">alarmeren</trd>
  <trdgrp lng="pt">
    <trd>alarmar</trd>,
    <trd>dar <ind>alarme</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>alarmovať</trd>,
    <trd>robiť poplach</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="alarm.0ilo.KOMUNE">
    <dif>
      Aparato uzata por sonigi <tld/>on,
      okaze de dan&gcirc;ero a&ubreve; &scirc;telo:
      <ekz>
        mi ne pagas e&ccirc; unu cendon por kontra&ubreve;&scirc;telistaj 
        <tld/>iloj
        <fnt>
          <aut>M. Twain, trad. R. Williams</aut>,
          <vrk><url ref="http://sezonoj.ru/2014/05/twain/">
          La familio McWilliams kaj la kontra&ubreve;&scirc;telista
          alarmilo</url></vrk>,
          <lok>La Ondo de Esperanto, 2014:3 (234)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">poplašné zařízení</trd>
  <trd lng="de">Alarmanlage</trd>
  <trd lng="en">alarm</trd>
  <trd lng="es">alarma</trd>
  <trd lng="fr">alarme<klr> (dispositif)</klr></trd>
  <trdgrp lng="hu">
    <trd>riaszt&oacute;berendez&eacute;s</trd>,
    <trd>riaszt&oacute;k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">alarm</trd>
  <trd lng="pt">alarme</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_r;&c_je;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>popla&scaron;n&eacute; zariadenie</trd>,
    <trd>alarm</trd>
  </trdgrp>
</drv>

<drv mrk="alarm.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="alarm.0isto.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pesimista homo, kiu maltrankviligas la publikon, konstante
      anoncante minacantajn, plej ofte fantaziajn, dan&gcirc;erojn.
    </dif>
    <trd lng="de">Panikmacher</trd>
    <trd lng="en">alarmist</trd>
    <trd lng="fr">alarmiste</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;szmad&aacute;r</trd>,
      <trd>r&eacute;mh&iacute;rterjeszt&odblac;</trd>
    </trdgrp>
    <trd lng="pt">alarmista</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_n;&c_i;&c_k;&c_jo;&c_r;</trd>,
      <trd>&c_a;&c_l;&c_a;&c_r;&c_m;&c_i;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">panikař</trd>
  <trd lng="sk">panikár</trd>
</drv>
</art>
