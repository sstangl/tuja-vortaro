<kap><rad>bu</rad>/o</kap>

<drv mrk="bu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Na&gcirc;barelo, flosanta ilo alfiksita &ccirc;e la fundo, en maro
      a&ubreve;
      rivero, kaj uzata por signali ian dan&gcirc;eran lokon a&ubreve;
      por alligi
      &scirc;ipon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_n;&c_d;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">boia</trd>
  <trd lng="cs">bóje</trd>
  <trd lng="de">Boje</trd>
  <trd lng="en">buoy</trd>
  <trd lng="es">boya</trd>
  <trd lng="fr">bou&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>b&oacute;ja</trd>,
    <trd>&uacute;sz&oacute;</trd>
  </trdgrp>
  <trd lng="it">boa</trd>
  <trdgrp lng="nl">
    <trd>boei</trd>,
    <trd>baken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>boja</trd>,
    <trd>p&lstroke;awa</trd>,
    <trd>baka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_j;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&oacute;ja</trd>,
    <trd>bója</trd>,
    <trd>plav&aacute;k</trd>
  </trdgrp>
</drv>
</art>
