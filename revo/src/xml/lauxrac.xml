  <trd lng="nl">laurierfamilie</trd>
  <trd lng="pt">laur&aacute;ceas</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_r;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
