<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: beribe.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <rad>beriber</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>
  
  <drv mrk="beribe.0o">
    <kap><tld/>o</kap>
    <snc mrk="beribe.0o.MED">
      <uzo tip="fak">MED</uzo>
      <dif>
        <ref tip="lst" cel="san.mal0o.MED"
        lst="voko:malsanoj">Malsano</ref>, ofta en varmegaj landoj,
        ka&ubreve;zata de
        man&gcirc;ado de 
        senvitaminaj man&gcirc;a&jcirc;oj, ekzemple polurita rizo:
        <ekz>
          li timis anka&ubreve; la <tld/>on <klr>...</klr> varmegojn,
          kaj 
          tiujn nemezureblajn teritoriojn, kiuj dividis ilin
          ankora&ubreve; de 
          la oceano
          <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url 
          ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
          praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
        </ekz>
      </dif>
    </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_y;-&c_b;&c_e;&c_r;&c_y;</trd>
  <trd lng="bg">&c_b;&c_je;&c_r;&c_i;-&c_b;&c_je;&c_r;&c_i;</trd>
  <trd lng="ca">beri-beri</trd>
  <trd lng="cs">beri-beri</trd>
  <trd lng="de">Beriberi</trd>
  <trd lng="en">beriberi</trd>
  <trd lng="es">beriberi</trd>
  <trd lng="fa">&a_b;&a_r;&f_y;-&a_b;&a_r;&f_y;</trd>
  <trd lng="fr">b&eacute;rib&eacute;ri</trd>
  <trd lng="hu">beriberi</trd>
  <trd lng="id">beri-beri</trd>
  <trd lng="pl">beri-beri</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_i;-&c_b;&c_je;&c_r;&c_i;</trd>
  <trd lng="sk">beri-beri</trd>
</drv>
</art>
