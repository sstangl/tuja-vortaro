<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gangre.xml,v 1.23 2015/07/04 08:37:34 revo Exp $">
<kap><ofc>1</ofc>
  <rad>gangren</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="gangre.0o">
  <kap><tld/>o</kap>
  <snc mrk="gangre.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Detrua iom post iom etendi&gcirc;anta putri&gcirc;o de ia parto de vivanta
      korpo.
    </dif>
    <trd lng="de">Brand</trd>
    <trdgrp lng="hu">
      <trd>&uuml;sz&ouml;k</trd>,
      <trd>&uuml;szk&ouml;s&ouml;d&eacute;s</trd>,
      <trd>gangr&eacute;na</trd>
    </trdgrp>
    <trd lng="nl">gangreen</trd>
  </snc>
  <snc mrk="gangre.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Morala infekti&gcirc;o, malvirteco.
    </dif>
    <trd lng="de">Verderbtheit</trd>
    <trd lng="hu">r&aacute;kfene</trd>
    <trd lng="nl">verdorvenheid</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_g;&c_r;&c_e;&c_n;&c_a;</trd>
  <trd lng="bg">&c_g;&c_a;&c_n;&c_g;&c_r;&c_je;&c_n;&c_a;</trd>
  <trd lng="ca">gangrena</trd>
  <trdgrp lng="cs">
    <trd>brant</trd>,
    <trd>gangréna</trd>,
    <trd>sněť</trd>
  </trdgrp>
  <trd lng="de">Gangr&auml;n</trd>
  <trd lng="el">&gamma;&alfa_ton;&gamma;&gamma;&rho;&alfa;&jota;&ny;&alfa;</trd>
  <trd lng="en">gangrene</trd>
  <trd lng="es">gangrena</trd>
  <trd lng="fr">gangr&egrave;ne</trd>
  <trd lng="pt">gangrena</trd>
  <trd lng="ru">&c_g;&c_a;&c_n;&c_g;&c_r;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hnilobný rozklad tkaniva</trd>,
    <trd>sneť</trd>
  </trdgrp>
  <trd lng="tr">kangren</trd>
</drv>

<drv mrk="gangre.0i">
  <kap><tld/>i, <var><kap><tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi <tld/>a, ka&ubreve;zi <tld/>on:
      <ekz>
        <tld/>ita membro, spirito, mondo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_n;&c_g;&c_r;&c_e;&c_n;&c_u;</ind></trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_c;&c_a; <ind>&c_g;&c_a;&c_n;&c_g;&c_r;&c_e;&c_n;&c_o;&c_z;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="ca">gangrenar</trd>
  <trd lng="cs">způsobovat gangrény</trd>
  <trd lng="de">verderben</trd>
  <trd lng="es">gangrenar</trd>
  <trd lng="fr">gangr&eacute;ner</trd>
  <trd lng="hu">el&uuml;szk&ouml;s&iacute;t</trd>
  <trd lng="nl">door <ind>gangreen</ind> aantasten</trd>
  <trd lng="pt">gangrenar</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol; <ind>&c_g;&c_a;&c_n;&c_g;&c_r;&c_je;&c_n;&c_o;&c_j;</ind></trd>
  <trdgrp lng="sk">
    <trd>gangrénu</trd>,
    <trd>vyvolať sneť</trd>,
    <trd>zapríčiniť hnilobný rozklad</trd>
  </trdgrp>
</drv>
</art>
