<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekv.xml,v 1.5 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>ekv</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="ekv.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> (<trd
      lng="la">Equus</trd>) de <ref 
tip="super" cel="huf.nepar0uloj">neparhufuloj</ref> el la
familio <ref tip="malprt" cel="cxeval1.0oj">&ccirc;evaledoj</ref>, al kiu
apartenas 
<ref tip="sub" cel="azen.0o.ZOO">azeno</ref>, <ref tip="sub" 
cel="cxeval.0o">&ccirc;evalo</ref>, hemiono, kaj <ref tip="sub" 
cel="zebr.0o">zebro</ref>.
    </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>kůň</trd>,
    <trd>osel n. kříženec</trd>,
    <trd>zebra</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Pferde</trd>,
     <trd>Einhufer</trd>
   </trdgrp>
  <trd lng="pt">eq&uuml;&iacute;deo</trd>
  <trdgrp lng="sk">
    <trd>kôň</trd>,
    <trd>osol</trd>,
    <trd>zebra</trd>
  </trdgrp>
</drv>

</art>
