<vortaro>
<art mrk="$Id: formiked.xml,v 1.8 2013/08/31 15:10:37 revo Exp $">
<kap>
  <rad>formiked</rad>/oj
</kap>
<drv mrk="formiked.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Formica</trd>), vivanta en socioj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_r;&c_a;&c_sh;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Ameisen <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">formicid&eacute;s</trd>
  <trd lng="hu">hangyaf&eacute;l&eacute;k</trd>
  <trd lng="pt">formic&iacute;deos</trd>
  <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
