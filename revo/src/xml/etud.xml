<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: etud.xml,v 1.11 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>etud</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="etud.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika verko kreita por ekzercoj:
      <ekz><tld/>oj de Chopin <klr>(&Scirc;open)</klr>.</ekz> 
    </dif>
    <trd lng="en">etude</trd>
    <trd lng="pl">etiuda</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o a&ubreve; desegno rapide farita por ekzerco, provo,
      studo,
      sen zorgo pri detaloj.  
    </dif>
    <trdgrp lng="en">
      <trd>study<klr> (art)</klr></trd>,
      <trd>sketch</trd>
    </trdgrp>
    <trd lng="pl">szkic</trd>
  </snc>
  <trd lng="be">&c_e;&c_c;&c_ju;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>cvičná skladba</trd>,
    <trd>etuda</trd>,
    <trd>studie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Et&uuml;de</trd>,
    <trd>&Uuml;bungsst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tude<klr tip="ind"> (mus., beaux
  arts)</klr></trd>
  <trd lng="hu">et&udblac;d</trd>
  <trd lng="pt">estudo</trd>
  <trd lng="ru">&c_e;&c_t;&c_ju;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>cvičná skladba</trd>,
    <trd>štúdia</trd>
  </trdgrp>
</drv>
</art>
