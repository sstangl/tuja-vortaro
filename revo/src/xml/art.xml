    <trd>&c_d;&c_je;&c_k;&c_o;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okr&aacute;&scaron;&lcaron;ovacia technika</trd>,
    <trd>ozdobovacie umenie</trd>
  </trdgrp>
</drv>

<drv mrk="art.parol0o">
  <kap>parol<tld/>o</kap>
  <snc mrk="art.parol0o.LIN">
    <ref tip="dif" cel="retori.0o">retoriko</ref>
    <ekz>
      la celo de la Oratora Konkurso de UEA estas evoluigi la
      Esperantlingvan parol<tld/>on
      <fnt>
      <aut>-</aut>,
      <vrk>Oratora Konkurso de UEA, regularo...</vrk>,
      <lok>soc.culture.esperanto, 2002-05-06</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_t;&c_o;&c_r;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">&rcaron;e&ccaron;nictv&iacute;</trd>
  <trdgrp lng="de">
    <trd>Rhetorik</trd>,
    <trd>Redekunst</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;thorique</trd>
  <trdgrp lng="hu">
    <trd>&eacute;kessz&oacute;l&aacute;s</trd>,
    <trd>sz&oacute;noklattan</trd>,
    <trd>retorika</trd>
  </trdgrp>
  <trd lng="id">retorika</trd>
  <trdgrp lng="it">
    <trd>oratoria</trd>,
    <trd>retorica</trd>
  </trdgrp>
  <trd lng="nl">retoriek</trd>
  <trdgrp lng="pl">
    <trd>retoryka</trd>,
    <trd>sztuka <ind>krasom&oacute;wstwa</ind></trd>,
    <trd>krasom&oacute;wstwo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>retorica</trd>,
    <trd>art&abreve; oratoriei</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_t;&c_o;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; <ind>&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="sk">re&ccaron;n&iacute;ctvo</trd>
</drv>

<drv mrk="art.pentr0o">
  <kap>pentr<tld/>o</kap>
  <snc mrk="art.pentr0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Grafika <ref tip="lst" cel="art.bel0o"
      lst="voko:artoj">bel<tld/>o</ref> 
      produktanta bildojn per koloriloj:
      <ekz>
        sur la monumento trovi&gcirc;as <klr>...</klr> anka&ubreve; tri 
        figuroj: la Skulpturo, la Pentr<tld/>o kaj la Ar&hcirc;itekturo
        <fnt><bib>Fab1</bib>, <lok>kupra porko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pintura</trd>
  <trdgrp lng="cs">
    <trd>malba</trd>,
    <trd>mal&iacute;&rcaron;stv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Malerei</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr> (art)</klr></trd>
  </trdgrp>
  <trd lng="hu">fest&eacute;szet</trd>
  <trd lng="id"><ind>seni</ind> lukis</trd>
  <trd lng="it">pittura<klr> (arte)</klr></trd>
  <trd lng="nl">schilderkunst</trd>
  <trd lng="pl">malarstwo</trd>
  <trd lng="ro">pictur&abreve;</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_mol;</trd>
  <trd lng="sk">maliarstvo</trd>
</drv>

<drv mrk="art.rajd0o">
  <kap>rajd<tld/>o</kap>
  <snc mrk="art.rajd0o.RAJ">
    <dif>
      <ref tip="lst" lst="voko:artoj"
      cel="art.0o.tehxniko"><tld/>o</ref> 
      sursidi &ccirc;evalon a&ubreve; alian <ref tip="vid" 
      cel="rajd.0i">rajdo</ref>beston kaj &gcirc;in gvidi:
      <ekz>
        por restarigi la tradicion de la araba nacio pri lerto en 
        rajd<tld/>o, li aparte fondis rajd<tld/>an klubon
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn">
          Saudi-arabia nova re&gcirc;o Abdullah Bin Abdul-Aziz</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2005-08-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jezdeck&eacute; um&ecaron;n&iacute;</trd>,
    <trd>jezdectv&iacute;</trd>,
    <trd>um&ecaron;n&iacute; j&iacute;zdy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Reitkunst</trd>,
    <trd>Voltigieren</trd>
  </trdgrp>
  <trd lng="fr">&eacute;quitation</trd>
  <trd lng="it">equitazione</trd>
  <trd lng="pt">equita&ccedil;&atilde;o</trd>
  <trd lng="sk">jazdeck&eacute; umenie</trd>
</drv>

<drv mrk="art.skulpt0o">
  <kap>skulpt<tld/>o</kap>
  <snc mrk="art.skulpt0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Plastika <ref tip="lst" cel="art.bel0o"
      lst="voko:artoj">bel<tld/>o</ref>
      produktanta bareliefajn, mez-reliefajn a&ubreve; plen-reliefajn
      verkojn:
      <ekz>
        &scirc;i elektis iun &ndash; sandalfariston, kiu estis ne nur ne
        re&gcirc;o; ne nur nenion sciis pri la skulpt<tld/>o, sed li \
        estis anka&ubreve; mallaborema, pezmovi&gcirc;a, stulta homo
        <fnt><bib>SatirRak</bib>, <lok>tria pre&gcirc;o de Pygmalion
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">escultura</trd>
  <trd lng="cs">socha&rcaron;stv&iacute;</trd>
  <trd lng="de">Bildhauerei</trd>
  <trdgrp lng="fr">
    <trd>sculpture<klr> (art)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szobr&aacute;szat</trd>,
    <trd>szobr&aacute;szm&udblac;v&eacute;szet</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>seni</ind> pahat</trd>,
    <trd><ind>seni</ind> ukir</trd>
  </trdgrp>
  <trd lng="it">scultura<klr> (arte)</klr></trd>
  <trd lng="pl">rze&zacute;biarstwo</trd>
  <trd lng="ro">sculptur&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_l;&c_mol;&c_p;&c_t;&c_u;&c_r;&c_a;
    <klr tip="amb"> (&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;)
    </klr></trd>
  </trdgrp>
  <trd lng="sk">soch&aacute;rstvo</trd>
</drv>

<drv mrk="art.sorcx0o">
  <kap>sor&ccirc;<tld/>o</kap>
  <snc mrk="art.sorcx0o.MAG">
    <dif>
      Praktika parto de <ref tip="malprt" cel="magi.0o.MAG">magio</ref>
      uzanta sor&ccirc;ojn, objektojn, ritojn:
      <ekz>
        &scirc;i sciis la sor&ccirc;<tld/>on kaj faris venenan kombilon
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zauberkunst</trd>
  <trd lng="fr">sorcellerie</trd>
  <trd lng="id"><ind>ilmu</ind> sihir</trd>
  <trdgrp lng="it">
    <trd>magia<klr> (arte magica)</klr></trd>,
    <trd><ind>arte</ind> magica</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>magia</trd>,
    <trd>sztuka magiczna</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>magie</trd>,
    <trd>art&abreve; de magie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_d;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">trollkonst</trd>
</drv>

<drv mrk="art.vide0o">
  <kap>vide<tld/>o, <var><kap>video<tld/>o</kap></var></kap>
  <snc mrk="art.vide0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <ref tip="dif" cel="vide.0o.KIN">video<sncref/></ref>
    <ekz>
      Carlos Devizia estas argentina artisto kun multaj talentoj, kiu 
      kiel artisto movi&gcirc;as inter pentrado, grafiko, muziko kaj 
      video<tld/>o
      <fnt>
        <aut>-</aut>,
        <vrk>&Ccirc;iam Bizara Lando</vrk>,
        <lok><url ref="http://www.vinilkosmo.com/?newsletter=22">
        Informletero de Vinilkosmo, 2008-03 (22)</url></lok>
      </fnt>
    </ekz>
    <ekz>
      la celoj de la festivalo estas <klr>...</klr> anta&ubreve;enigi kreemon 
      kaj originalecon en la areo de kinearto kaj vide<tld/>o
      <fnt>
        <bib>Viki</bib>,
        <lok><url
        ref="http://eo.wikipedia.org/wiki/Erevana_Internacia_Filmfestivalo">
        Erevana Internacia Filmfestivalo</url>, 2010-10-23</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">art vid&eacute;o</trd>
  <trd lng="id"><ind>seni</ind> video</trd>
  <trd lng="it">video arte</trd>
</drv>
</art>
