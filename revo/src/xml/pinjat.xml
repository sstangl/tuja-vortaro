<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pinjat.xml,v 1.4 2013/09/08 19:10:37 revo Exp $">
<kap>
  <rad>pinjat</rad>/o
</kap>
<drv mrk="pinjat.0o">
  <kap><tld/>o</kap>
  <snc mrk="pinjat.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Penda&jcirc;o, kiun ludanto kun ka&scirc;itaj okuloj provas bastone 
      frakasi por gajni entenatajn franda&jcirc;ojn kaj <ref tip="lst"
      cel="lud.0ilo" lst="voko:ludiloj">ludilojn</ref>;
      tradicia ludilo de meksikaj festoj:
      <ekz>
        vico de infanoj, kaj iufoje anka&ubreve; plenkreskuloj, &ccirc;iu el ili 
        portante okulbanda&gcirc;o<klr>n</klr>, provas rompi la <tld/>on per 
        bastona batado de &gcirc;i
        <fnt>
          <aut>-, trad. A. Mendelawitz</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/esperantoaustralia/message/4339">
          Fari Komercon en Meksikio</url></vrk>,
          <lok>Esperantoaustralia, 2007-06-15</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj kuiris, aliaj &scirc;veligis balonojn, plenigis la <tld/>ojn 
        per dol&ccirc;a&jcirc;oj
        <fnt>
          <aut>Mallely</aut>,
          <vrk><url ref="http://www.esperanto-mexico.org/eo/node/223">
          Kunveno en Guadalajara</url></vrk>,
          <lok>Meksika Esperanto-Federacio, 2009-05-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_i;&c_n;&c_ja;&c_t;&c_a;</trd>
  <trd lng="de">Pi&ntilde;ata</trd>
  <trd lng="en">pi&ntilde;ata</trd>
  <trd lng="fr">pi&ntilde;ata</trd>
  <trd lng="nl">pi&ntilde;ata</trd>
</drv>
</art>

