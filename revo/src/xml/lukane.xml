<vortaro>
<art mrk="$Id: lukane.xml,v 1.8 2013/08/31 15:10:49 revo Exp $">
<kap>
  <rad>lukaned</rad>/oj
</kap>

<drv mrk="lukane.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lucanidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_a;&c_ch;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja; &c_zh;&c_u;&c_k;&c_o;&c_w;)</klr></trd>
  <trd lng="de">Schr&ouml;ter<klr tip="amb"> (K&auml;fer)</klr></trd>
  <trd lng="hu">szarvasbog&aacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">vliegende <ind>herten</ind><klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="pt">lucan&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_g;&c_a;&c_ch;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_u;&c_k;&c_o;&c_v;)</klr></trd>,
    <trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_u;&c_s;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_u;&c_k;&c_o;&c_v;)</klr></trd>
  </trdgrp>
</drv>
</art>
