<vortaro>
<art mrk="$Id: franko.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap><rad>frankolin</rad>/o</kap>
<drv mrk="franko.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Francolinus</trd>), simila al la perdriko.
    </dif>
  </snc>
  <trd lng="ca">francol&iacute;</trd>
  <trdgrp lng="cs">
    <trd>frankolin</trd>,
    <trd>frankolín</trd>
  </trdgrp>
  <trd lng="de">Frankolin</trd>
  <trd lng="en">francolin</trd>
  <trd lng="es">francol&iacute;n</trd>
  <trd lng="fr">francolin</trd>
  <trd lng="hu">frankolin</trd>
  <trdgrp lng="nl">
    <trd>frankolijn</trd>,
    <trd>frankolien</trd>
  </trdgrp>
  <trd lng="pl">frankolin</trd>
  <trdgrp lng="pt">
    <trd>francolino</trd>,
    <trd>francolim</trd>,
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_a;&c_ch;</trd>
  <trd lng="sk">frankolín</trd>
</drv>
</art>
