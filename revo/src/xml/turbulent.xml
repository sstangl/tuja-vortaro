<art mrk="$Id: turbulent.xml,v 1.10 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>turbulent</rad>/a <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="turbulent.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turbulent.0a.malkvieta">
  <dif>
    <ref tip="dif" cel="kviet.mal0a.KOMUNE">malkvieta</ref>
      <ekz>
        la maro &scirc;ajnas aparte <tld/>a
        <fnt>
        <aut>Trevor Steele</aut>,
        <vrk>Babilado inter la novaj &Gcirc;enerala Direktoro kaj
Redaktoro</vrk>,
        <bib>Esperanto</bib>,
        <lok>2002-01</lok>
        </fnt>.
      </ekz>
      </dif>
      </snc>
  <trd lng="ca">turbulent</trd>
  <trdgrp lng="cs">
    <trd>bouřlivý</trd>,
    <trd>turbulentní</trd>,
    <trd>vířivý</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_t;&a_l;&a_A;&a_T;&a_m;</trd>
  <trd lng="fr">turbulent</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;borg&oacute;</trd>,
    <trd>forrong&oacute;</trd>,
    <trd>turbulens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>turbulent</trd>,
    <trd>roerig</trd>,
    <trd>onrustig</trd>
  </trdgrp>
  <trdgrp lng="pl">
  	<trd>wzburzony</trd>
  	<trd>poruszony</trd>
  	<trd>niespokojny<klr> (o t&lstroke;umie)</klr></trd>
  	<trd>zgie&lstroke;kliwy</trd>
  	<trd>ha&lstroke;a&sacute;liwy</trd>
  	<trd>turbulentny<klr> (fiz.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>turbulento</trd>,
    <trd>agitado</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>búrlivý</trd>,
    <trd>nepokojný</trd>,
    <trd>turbulentný</trd>
  </trdgrp>
</drv>
</art>

