<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: simultan.xml,v 1.5 2015/09/21 10:10:19 revo Exp $">
<kap>
  <rad>simultan</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="simultan.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="simultan.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="temp.sam0a.KOMUNE">samtempa</ref>,
      sammomenta:
      <ekz>
        sentu tion nun, &ccirc;i sekunde, <tld/>e kun mi
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">simultaneous</trd>
  <trdgrp lng="cs">
    <trd>simult&aacute;nn&iacute;</trd>,
    <trd>soub&ecaron;&zcaron;n&yacute;</trd>,
    <trd>soudob&yacute;</trd>
  </trdgrp>
  <trd lng="fr">concomitant</trd>
  <trdgrp lng="pl">
    <trd>symultaniczny</trd>,
    <trd>jednoczesny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>simult&aacute;nny</trd>,
    <trd>s&uacute;be&zcaron;n&yacute;</trd>,
    <trd>s&uacute;&ccaron;asn&yacute;</trd>
  </trdgrp>
</drv>
</art>

