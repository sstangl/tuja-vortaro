<art mrk="$Id: antifr.xml,v 1.12 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>antifraz</rad>/o
</kap>
<drv mrk="antifr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="figur.stil0o">Stilfiguro</ref>
      per kiu oni ironie uzas vorton a&ubreve; esprimon
      en senco kontra&ubreve;a al &gcirc;ia normala signifo.
    </dif>
  </snc>
  <trd lng="ca">ant&iacute;frasi</trd>
  <trd lng="cs">antifráze</trd>
  <trd lng="de">Antiphrase</trd>
  <trd lng="en">antiphrase</trd>
  <trd lng="es">ant&iacute;frasis</trd>
  <trd lng="fa">&a_b;&f_y;&a_A;&a_n; &a_m;&a_ayn;&f_k;&a_w;&a_s;</trd>
  <trd lng="fr">antiphrase</trd>
  <trd lng="hu">antifr&aacute;zis</trd>
  <trd lng="sk">úmyselné použitie slova alebo výrazu v opačnom význame</trd>
</drv>
</art>
