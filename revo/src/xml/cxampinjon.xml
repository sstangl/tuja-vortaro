<vortaro>
<art mrk="$Id: cxampinjon.xml,v 1.15 2013/10/27 18:10:11 revo Exp $">
<kap>
<rad>&ccirc;ampinjon</rad>/o
</kap>
<drv mrk="cxampinjon.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Bongusta <ref tip="lst" cel="fung.0o.fruktokorpo" 
      lst="voko:fungoj">fungo</ref>, ofte kun blanketa
      &ccirc;apelo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
       el familio <ref tip="malprt"
      cel="agarik1.0oj">agarikacoj</ref>
      (<trd lng="la">Agaricus</trd>)
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_m;&c_p;&c_ib;&c_n;&c_mol;&c_jo;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Egerling</trd>,
    <trd>Champignon</trd>
  </trdgrp>
  <trd lng="es">champi&ntilde;&oacute;n</trd>
  <trd lng="fa">&a_q;&a_A;&a_r;&f_ch; &a_sh;&a_A;&a_m;&f_p;&f_y;&a_n;&f_y;&a_w;&a_n;</trd>
  <trd lng="fr">champignon de Paris</trd>
  <trdgrp lng="hu">
    <trd>csiperke</trd>,
    <trd>sampinyon</trd>
  </trdgrp>
  <trd lng="it">champignon</trd>
  <trd lng="nl">champignon</trd>
  <trd lng="pl">pieczarka</trd>
  <trd lng="pt">champinhom</trd>
  <trd lng="ru">&c_sh;&c_a;&c_m;&c_p;&c_i;&c_n;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">&scaron;ampi&ncaron;&oacute;n</trd>
</drv>
</art>

