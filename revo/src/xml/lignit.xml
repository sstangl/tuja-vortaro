<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lignit.xml,v 1.15 2006/09/06 15:15:34 revo Exp $">
<kap>
  <rad>lignit</rad>/o
</kap>
<drv mrk="lignit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de fosilia <ref cel="karb.0o">karbo</ref>, tre ofte prezentanta
      la &scirc;ajnon de duonbrulinta ligno.
    </dif>
    <refgrp tip="super">
      <ref cel="brul.0igajxo">bruliga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_g;&c_n;&c_ib;&c_t; </trd>,
    <trd>&c_b;&c_u;&c_r;&c_y; <ind>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lignit</trd>,
    <trd>Braunkohle</trd>,
    <trd>Holzkohle</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lignite</trd>,
    <trd>brown coal</trd>
  </trdgrp>
  <trd lng="es">lignito</trd>
  <trd lng="fr">lignite</trd>
  <trd lng="hu">lignit</trd>
  <trdgrp lng="nl">
    <trd>ligniet</trd>,
    <trd>bruinkool</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>linhito</trd>,
    <trd>lignito</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_g;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y;&c_j; &c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
