<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cinabr.xml,v 1.20 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cinabr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cinabr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">HgS</mlg>
  <snc mrk="cinabr.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Ru&gcirc;a sulfido de hidrargo, uzata kiel 
      <ref tip="lst" cel="kolor.0ilo" lst="voko:koloroj">kolorilo</ref>:
      <ekz>
        sur &gcirc;ia sojlo ka&ubreve;ris laboristo kaj kirlis brike
        ru&gcirc;koloran 
        <tld/>on
        <fnt>
          <aut>A. N. Tolstoj, trad. E. Pill</aut>,
          <vrk><url ref="http://akileo.boom.ru/aelita3.htm">
          Aelita</url></vrk>,
          <lok>1928</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      La vorto "sulfido" estas enkondukenda. <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_n;&c_o;&c_b;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">cinabri</trd>
  <trdgrp lng="cs">
    <trd>cinabarit</trd>,
    <trd>cinabro</trd>,
    <trd>rumÄ›lka</trd>
  </trdgrp>
  <trd lng="de">Zinnober</trd>
  <trd lng="en">cinnabar</trd>
  <trd lng="fr">cinabre</trd>
  <trd lng="hu">cin&oacute;ber</trd>
  <trd lng="it">cinabro</trd>
  <trd lng="pl">cynober</trd>
  <trd lng="pt">cinabre</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">cinabarit</trd>
</drv>
</art>
