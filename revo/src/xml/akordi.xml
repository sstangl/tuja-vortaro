<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akordi.xml,v 1.26 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akordion</rad>/o
</kap>
<drv mrk="akordi.0o">
  <kap><tld/>o</kap>
  <snc mrk="akordi.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Portebla <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      kun klavaro, kiun oni sonigas alterne tirante kaj pu&scirc;ante balgon:
      <ekz>
        li havas <tld/>on kaj li povus dancigi nin dum la tuta nokto
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="harmon2.tir0o">tirharmoniko</ref>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_r;&c_d;&c_e;&c_o;&c_n;</trd>
  <trd lng="bg">&c_a;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;</trd>
  <trd lng="ca">acordi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>akordeon</trd>,
    <trd>tahací harmonika</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Harmonika</trd>,
    <trd>Ziehharmonika</trd>,
    <trd>Schifferklavier</trd>,
    <trd>Akkordeon</trd>
  </trdgrp>
  <trd lng="en">accordion</trd>
  <trd lng="es">acerde&oacute;n</trd>
  <trd lng="fr">accord&eacute;on</trd>
  <trd lng="hu">tang&oacute;harmonika</trd>
  <trd lng="id">akordeon</trd>
  <trdgrp lng="it">
    <trd>fisarmonica</trd>, 
    <trd>concertina</trd>,
    <trd>organetto<klr> (accordion)</klr></trd>
  </trdgrp>
  <trd lng="nl">accordeon</trd>
  <trdgrp lng="pl">
    <trd>akordeon</trd>,
    <trd>harmonia</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>concertina</trd>, 
   <trd>acorde&atilde;o</trd>,
   <trd>cordeona</trd>, 
   <trd>sanfona</trd>, 
   <trd>gaita<klr tip="amb"> (concertina)</klr></trd>, 
   <trd>harm&ocirc;nica<klr tip="amb"> (concertina)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akorde&oacute;n</trd>,
    <trd>akordeón</trd>
  </trdgrp>
  <trd lng="sv">dragspel</trd>
</drv>
</art>
