<vortaro>
<art mrk="$Id: lampir1.xml,v 1.9 2015/12/03 15:10:19 revo Exp $">
<kap>
  <rad>lampired</rad>/oj
</kap>
<drv mrk="lampir1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lampyridae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">lamp&iacute;rids</trd> 
  <trd lng="de">Leuchtk&auml;fer</trd>
  <trdgrp lng="en">
    <trd>fireflies</trd>,
    <trd>lightning bugs</trd>
  </trdgrp>
  <trd lng="hu">szentj&aacute;nosbog&aacute;r-f&eacute;l&eacute;k</trd>
  <trd lng="nl">glimwormen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">lampir&iacute;deos</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
