        2005a, p.
	21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_j;&c_s; <klr>(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja;
  &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="de">Reis</trd>
  <trdgrp lng="fr">
    <trd>real</trd>,
    <trd>r&eacute;is</trd>
  </trdgrp>
  <trd lng="hu">reis</trd>
  <trd lng="id"><ind>real</ind> (Brazil)</trd>
  <trd lng="nl">reis<klr tip="ind"> (munt)</klr></trd>
  <trd lng="pt">r&eacute;is</trd>
</drv>
