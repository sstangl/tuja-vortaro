<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gigant.xml,v 1.21 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gigant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gigant.0o">
  <kap><tld/>o</kap>
  <snc mrk="gigant.0o.MIT">
    <dif>
      <ref tip="sin" cel="grand.0egulo">Grandegulo</ref>,
      homo eksterordinare, nenormale granda:
      <ekz>
        tiun lokon lo&gcirc;as gento de <tld/>oj
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url
          ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolos.0o">koloso</ref>,
      <ref cel="titan.0o">titano</ref>.
    </refgrp>
    <trd lng="en">giant</trd>
    <trdgrp lng="pl">
      <trd>gigant</trd>,
      <trd>olbrzym</trd>,
      <trd>wielkolud</trd>
    </trdgrp>
  </snc>
  <snc mrk="gigant.0o.MIT_helena">
    <uzo tip="fak">MIT</uzo>
    <dif>
      En la helena mito, &ccirc;iu el la cent grandegaj filoj de
      <ref tip="vid" cel="ge1.0o">Geo</ref> kaj de la sango de
      <ref tip="vid" cel="uran.0o.MIT">Urano</ref>;
      nekonfuzindaj kun  
      <ref tip="vid" cel="titan.T0oj">Titanoj</ref>.
    </dif>
    <trd lng="pl">gigant</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">gegant</trd>
  <trdgrp lng="cs">
    <trd>gigant</trd>,
    <trd>obr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gigant</trd>,
    <trd>Riese</trd>
  </trdgrp>
  <trd lng="es">gigante</trd>
  <trd lng="fr">g&eacute;ant<klr> (subst.)</klr></trd>
  <trd lng="hu">&oacute;ri&aacute;s</trd>
  <trdgrp lng="it">
    <trd>gigante</trd>,
    <trd>Gigante<klr tip="ind"> (mit.)</klr></trd>
  </trdgrp>
  <trd lng="nl">reus</trd>
  <trd lng="pt">gigante<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gigant</trd>,
    <trd>obor</trd>
  </trdgrp>
</drv>

<drv mrk="gigant.0a">
  <kap><tld/>a</kap>
  <snc mrk="gigant.0a.KOMUNE">
    <dif>
      Grandega,
      <ref tip="vid" cel="kolos.0a">kolosa</ref>,
      <ref tip="vid" cel="mamut">mamuta</ref>.:
      <ekz>
        <tld/>a homo, monto, laboro;
      </ekz>
      <ekz>
        mi vidis <tld/>an virinan figuron
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        iliaj abelaj klopodoj kaj iliaj <tld/>aj projektoj
        <fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gegant&iacute;</trd>,
    <trd>col&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gigantický</trd>,
    <trd>mocný</trd>,
    <trd>mohutný</trd>,
    <trd>obrovský</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gigantisch</trd>,
    <trd>riesig</trd></trdgrp>
  <trdgrp lng="en">
    <trd>gigantic</trd>,
    <trd>giant</trd>
  </trdgrp>
  <trd lng="es">gigante</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;ant<klr> (adj.)</klr></trd>,
    <trd>gigantesque</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;ri&aacute;s</trd>,
    <trd>gigantikus</trd>
  </trdgrp>
  <trd lng="it">gigantesco</trd>
  <trd lng="nl">reusachtig</trd>
  <trdgrp lng="pl">
    <trd>gigantyczny</trd>,
    <trd>bajo&nacute;ski<klr> (o cenie)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gigante<klr tip="ind"> (adj.)</klr></trd>,
    <trd>gigantesco</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>gigantický</trd>,
    <trd>obrovský</trd>
  </trdgrp>
</drv>
</art>
