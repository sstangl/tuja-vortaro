<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: popl.xml,v 1.21 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>popl</rad>/o
</kap>
<drv mrk="popl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="salikac.0oj">salikacoj</ref>
      (<trd lng="la">Populus</trd>) de
      <ref tip="lst" cel="arb.0o"
      lst="voko:malpigioparencoj">arboj</ref>
      kaj arbedoj kun triangulaj, kor- a&ubreve; ovoformaj longtigaj folioj,
      kun <ref tip="prt" cel="ament.0o">amentoj</ref>
      kaj <ref tip="prt" cel="kapsul.0o.BOT">kapsuloj</ref>
      du- &gcirc;is kvarpartaj, enhavantaj multajn lanugajn
      flugsemojn, similajn al ne&gcirc;eroj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_o;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_o;&c_p;&c_o;&c_l;&c_a;</trd>
  <trd lng="cs">topol</trd>
  <trd lng="de">Pappel</trd>
  <trd lng="en">poplar</trd>
  <trd lng="fr">peuplier</trd>
  <trd lng="hu">ny&aacute;r(fa)</trd>
  <trd lng="nl">populier</trd>
  <trd lng="pl">topola</trd>
  <trdgrp lng="pt">
    <trd>choupo</trd>,
    <trd>&aacute;lamo</trd>
  </trdgrp>
  <trd lng="ro">plop</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">topoÄ¾</trd>
  <trd lng="sv">poppel</trd>
</drv>
</art>
