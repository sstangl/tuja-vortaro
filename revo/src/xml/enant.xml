    <trd>Pferdesaat</trd>,
    <trd>Wasserfenchel</trd>
  </trdgrp>
  <trd lng="fa">&a_A_madda;&a_b;&zwnj;&f_ch;&f_k;&a_A;&a_n;</trd>
  <trd lng="fr">&oelig;nanthe</trd>
  <trd lng="hu">borgy&ouml;k&eacute;r</trd>
  <trd lng="pl">kropid&lstroke;o</trd>
  <trdgrp lng="pt">
    <trd>enante</trd>,
    <trd>enanto</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">chaluha</trd>
</drv>
</art>
