<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kocxed.xml,v 1.4 2013/08/31 15:10:35 revo Exp $">
  <kap>
    <rad>ko&ccirc;ed</rad>/oj
  </kap>
<drv mrk="kocxed.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="hemipt.0oj">hemipteroj</ref>
      (<trd lng="la">Coccidae</trd>) de insektoj, kies inoj senflugilaj
      kaj
      rondformaj parazitas sur diversspeciaj plantoj, dum la maturaj
      virseksuloj
      flugas kaj sin ne nutras.
    </dif>
    <ref tip="vid" cel="kocxen.0o.ZOO">ko&ccirc;enilo <sncref/></ref>
  </snc>
  <trd lng="fr">coccid&eacute;s</trd>
</drv>
</art>

