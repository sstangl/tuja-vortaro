<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korpul.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>korpulent</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korpul.0a">
  <kap><tld/>a</kap>
  <snc mrk="korpul.0a.KOMUNE">
    <ref tip="dif" cel="amplek.korp0a.KOMUNE">korpampleksa</ref>
    <ekz>
      interne sidis la <tld/>a figuro de virino de matura
      a&gcirc;o<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_zh;&c_n;&c_y;</trd>,
     <trd>&c_t;&c_l;&c_u;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
     <trd>&c_p;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korpulentní</trd>,
    <trd>tělnatý</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>korpulent</trd>,
     <trd>beleibt</trd>,
     <trd>wohlbeleibt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>corpulent</trd>,
    <trd>fat</trd>,
    <trd>fleshy</trd>,
    <trd>obese</trd>,
    <trd>stout</trd>
  </trdgrp>
  <trd lng="es">corpulento</trd>
  <trd lng="fr">corpulent</trd>
  <trdgrp lng="hu">
     <trd>testes</trd>,
     <trd>korpulens</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>corpulent</trd>,
    <trd>zwaarlijvig</trd>
    <trd>dik</trd>,
    <trd>gezet</trd>,
    <trd>vlezig</trd>
  </trdgrp>
  <trd lng="pt">corpulento</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>korpulentný</trd>,
    <trd>telnatý</trd>
  </trdgrp>
</drv>
</art>
