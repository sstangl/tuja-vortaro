<vortaro>
<art mrk="$Id: hiskia.xml,v 1.20 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hiskiam</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hiskia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hiskia.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Venena 
      <ref tip="lst" cel="herb.0o" lst="voko:solanoparencoj">herbo</ref>
      kun betoforma a&ubreve; plurkapa rizoma radiko,
      kaj kun <ref tip="prt" cel="kapsul.0o">kapsuloj</ref>
      kun kovrilo, ordinare tute envolvita en la urnoforma kaliko,
      enhavantaj plurcent semojn;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el la familio <ref
      tip="malprt"
      cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Hyoscyamus</trd>):
      <ekz>
        kio okazis, &mdash; demandis Volke, &mdash; &ccirc;u vi <tld/>on troman&gcirc;is?
        <fnt>
          <aut>U. Karatkevi&ccirc;, trad. Z. Lapcionak</aut>,
          <vrk><url ref="http://zmila.at.tut.by/Libroportistoj.html">
          Libroportistoj</url></vrk>,
          <lok>2003</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_jo;&c_k;&c_a;&c_t;</trd>
  <trdgrp lng="br">                      
   <trd>louzaouenn-ar-c'housked</trd>,
   <trd>mallc'heot</trd>
  </trdgrp>
  <trd lng="cs">bl√≠n</trd>
  <trd lng="de">Bilsenkraut</trd>
  <trd lng="el">&ypsilon;&omikron;&sigma;&kappa;&ypsilon_ton;&alfa;&my;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>henbane</trd>,
  </trdgrp>
  <trd lng="fr">jusquiame</trd>
  <trd lng="hu">bel&eacute;ndek</trd>
  <trd lng="nl">bilzenkruid</trd>
  <trd lng="pl">lulek</trd>
  <trd lng="pt">meimendro</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">blen</trd>
</drv>
</art>
