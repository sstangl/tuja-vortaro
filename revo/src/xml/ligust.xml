<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ligust.xml,v 1.15 2013/09/16 13:10:11 revo Exp $">
<kap>
  <rad>ligustr</rad>/o
</kap>
<drv mrk="ligust.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Ligustrum</trd>),
      arbedo kun bonodoraj blankaj floroj:
      <ekz>
        he&gcirc;o el <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_u;&c_g;&c_u;&c_s;&c_t;&c_r;&c_u;&c_m;</trd>
  <trd lng="de">Liguster</trd>
  <trd lng="en">ligustrum</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_kasra;&a_n;&a_d;&a_A;&a_r;&f_ch;&a_h;</trd>,
    <trd>&a_b;&a_r;&f_g; &a_n;&a_w;</trd>
  </trdgrp>
  <trd lng="hu">fagyal</trd>
  <trd lng="nl">liguster</trd>
  <trd lng="pt">ligustro</trd>
  <trd lng="ru">&c_b;&c_i;&c_r;&c_ju;&c_ch;&c_i;&c_n;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>
