<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lamina.xml,v 1.14 2014/03/27 07:10:16 revo Exp $">
<kap>
  <rad>laminari</rad>/o
</kap>
<drv mrk="lamina.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      G. de maraj algoj, kies kelkaj specoj estas man&gcirc;eblaj
      (<trd lng="la">Laminaria</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">lamin&agrave;ria</trd>
  <trdgrp lng="de">
     <trd>Blatttang</trd>,
     <trd>Riementang</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brown <ind>seaweeds</ind></trd>,
    <trd>brown algae</trd>,
    <trd>kelp</trd>
  </trdgrp>
  <trd lng="fr">laminaire</trd>
  <trdgrp lng="hu">
    <trd>lemezmoszat</trd>,
    <trd>sz&iacute;jmoszat</trd>,
    <trd>mannalev&eacute;l</trd></trdgrp>
  <trd lng="nl">zeewier</trd>
  <trd lng="pt">lamin&aacute;ria</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_i;&c_n;&c_a;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
