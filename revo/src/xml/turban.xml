<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: turban.xml,v 1.18 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>turban</rad>/o
</kap>
<drv mrk="turban.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vira <ref tip="super" cel="vest.kap0o">kapvesto</ref>
      &ccirc;e Orientaj popoloj, konsistanta el tukstrio plurfoje
      volvita &ccirc;irka&ubreve; la kapo:
      <ekz>
        kiel anoj de la sika religio tutmonde, la sika komunumo en
        Irlando
        uzas <tld/>ojn kaj tondas nek la barbon, nek la hararon,
        la&ubreve; la
        postuloj de sia tradicio<fnt><aut>GMA</aut>: <vrk>Irlandaj sikoj
        razas sin</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 2a, p.
        17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_r;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_c;&c_ju;&c_r;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_ch;&c_a;&c_l;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">turbant</trd>
  <trd lng="cs">turban</trd>
  <trd lng="de">Turban</trd>
  <trd lng="en">turban</trd>
  <trd lng="es">turbante</trd>
  <trdgrp lng="fr">
    <trd>turban</trd>
  </trdgrp>
  <trd lng="hu">turb&aacute;n</trd>
  <trd lng="nl">tulband</trd>
  <trdgrp lng="pl">
    <trd>turban</trd>,
    <trd>zaw&oacute;j</trd>
  </trdgrp>
  <trd lng="pt">turbante</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ju;&c_r;&c_b;&c_a;&c_n;</trd>,
    <trd>&c_ch;&c_a;&c_l;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">turban</trd>
  <trd lng="sv">turban</trd>
</drv>
</art>
