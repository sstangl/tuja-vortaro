<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suflor.xml,v 1.15 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>suflor</rad>/o
</kap>
<drv mrk="suflor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Homo, kies okupo estas malla&ubreve;te flustri al aktoroj la
parolojn, kiujn
      ili ne memoras.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nápověda <klr>(osoba)</klr></trd>,
    <trd>suflér</trd>
  </trdgrp>
  <trd lng="de">Souffleur</trd>
  <trd lng="en">prompter</trd>
  <trdgrp lng="fr">
    <trd>souffleur</trd>
  </trdgrp>
  <trd lng="hu">s&uacute;g&oacute;<klr tip="ind">(sz&iacute;nh&aacute;zi)</klr></trd>
  <trd lng="it">suggeritore</trd>
  <trd lng="nl">souffleur</trd>
  <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_jo;&c_r;</trd>
  <trd lng="sk">šepkár</trd>
</drv>

<drv mrk="suflor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="suflor.0i.flustri">
    <dif>
      Flustri kiel <tld/>o.
    </dif>
    <ref tip="vid" cel="slip.suflor0eto"><tld/>slipeto</ref>
    <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="suflor.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="sugest.0i">sugesti</ref>
    <ekz>
      mi devas nur le&gcirc;ere premi du klavojn &ndash; la
      vortaro sufloras la bezonatan vorton<fnt><aut>Sergeo
      Dereviannykh</aut>: <vrk>&Ccirc;u vi tradukas france?</vrk>,
      <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 18a</lok></fnt>.
    </ekz>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>napovídat</trd>,
    <trd>suflovat</trd>
  </trdgrp>
  <trd lng="de">soufflieren</trd>
  <trd lng="en">prompt</trd>
  <trdgrp lng="fr">
    <trd>souffler<klr> (chuchoter, sugg&eacute;rer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&uacute;g</trd>,
    <trd>megs&uacute;g</trd>
  </trdgrp>
  <trd lng="it">suggerire</trd>
  <trd lng="nl">souffleren</trd>
  <trdgrp lng="sk">
    <trd>poradiť</trd>,
    <trd>vnuknúť (pren.)</trd>,
    <trd>šepkať <klr>(div.)</klr></trd>
  </trdgrp>
</drv>
</art>
