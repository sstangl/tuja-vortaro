<art mrk="$Id: motaci.xml,v 1.19 2015/07/04 08:37:39 revo Exp $">
<kap>
<rad>motacil</rad>/o
</kap>
<drv mrk="motaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="motaci.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="motaci1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Motacilla</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konipas</trd>,
    <trd>konipas bílý</trd>
  </trdgrp>
  <trd lng="de">Bachstelze <klr>(k.a.)</klr></trd>
  <trd lng="en">wagtail</trd>
  <trdgrp lng="fr">
    <trd>bergeronnette</trd>,
    <trd>hoche-queue</trd>
  </trdgrp>
  <trd lng="hu">billeget&odblac;</trd>
  <trd lng="nl">kwikstaart</trd>
  <trd lng="pl">pliszka</trd>
  <trdgrp lng="pt">
   <trd>alv&eacute;loa <klr tip="amb">(ornit., PT)</klr></trd>,
    <trd>alv&eacute;ola <klr tip="amb">(ornit., PT)</klr></trd>,
    <trd>alv&eacute;ola-branca <klr tip="amb">(ornit.)</klr></trd>,
    <trd>boieira <klr tip="amb">(ornit.)</klr></trd>,
    <trd>lavandeira <klr tip="amb">(ornit.)</klr></trd>,
    <trd>lavandisca <klr tip="amb">(ornit.)</klr></trd>,
    <trd>pastorinha <klr tip="amb">(ornit.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_ja;&c_s;&c_o;&c_g;&c_u;&c_z;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>trasochvost biely</trd>,
    <trd>trasorítka</trd>
  </trdgrp>
  <trd lng="sv">&auml;rla</trd>
</drv>

</art>
