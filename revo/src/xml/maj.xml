<vortaro>
<art mrk="$Id: maj.xml,v 1.35 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>maj</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="maj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="maj.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kvina <ref tip="lst" cel="monat.0o.KAL"
      lst="voko:monatoj" val="5">monato</ref>  de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la gregoria kalendaro.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_j;</trd>
  <trd lng="br">Mae</trd>
  <trd lng="ca">maig</trd>
  <trd lng="cs">kv&ecaron;ten</trd>
  <trdgrp lng="de">
    <trd>Mai</trd>,
    <trd>Wonnemonat<klr> [kromnomo]</klr></trd>,
    <trd>Wonnemond<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">May</trd>
  <trd lng="es">mayo</trd>
  <trd lng="fr">mai</trd>
  <trd lng="he">&mem;&alef;&yod;</trd>
  <trd lng="hu">m&aacute;jus</trd>
  <trd lng="id">Mei</trd>
  <trd lng="it">maggio</trd>
  <trd lng="ja">&#20116;&#26376; [<ind>&#12372;&#12364;&#12388;</ind>]</trd>
  <trd lng="nl">mei</trd>
  <trd lng="pl">maj</trd>
  <trd lng="pt">maio</trd>
  <trd lng="ro">mai</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;</trd>
  <trd lng="sv">maj</trd>
  <trd lng="tr">mayis</trd>
  <trd lng="vo">mayul</trd>
  <trd lng="sk">máj</trd>
  <trdgrp lng="cs">
    <trd>květen</trd>,
    <trd>máj</trd>
  </trdgrp>
</drv>
</art>
