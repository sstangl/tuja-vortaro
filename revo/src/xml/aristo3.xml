<kap>
  <rad>aristolokiac</rad>/oj
</kap>
<drv mrk="aristo3.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotelidonaj</ref>
      plantoj (<trd lng="la">Aristolochiaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Osterluzeigew&auml;chse</trd>
  <trd lng="fr">aristolochiac&eacute;es</trd>
  <trd lng="hu">farkasalmaf&eacute;l&eacute;k</trd>
  <trd
lng="ru">&c_k;&c_i;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">piprankev&auml;xter</trd>
</drv>
</art>
