<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxik.xml,v 1.16 2013/09/28 15:10:13 revo Exp $">
<kap>
  <rad>&scirc;ik</rad>/a
  <fnt>B</fnt>
</kap>
<drv mrk="sxik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      La&ubreve;mode bongusta.
    </dif>
    <refgrp tip="vid">
      <ref cel="elegan.0a">eleganta</ref>,
      <ref cel="pimp.0a">pimpa</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>schick</trd>,
    <trd>modisch</trd>,
    <trd>elegant</trd>
  </trdgrp>
  <trd lng="el">&kappa;&omikron;&my;&psi;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>chic</trd>,
    <trd>fashionable</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>de buen gusto</trd>,
     <trd>elegante</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_y;&f_k;</trd>
  <trd lng="fr">chic</trd>
  <trd lng="hu">sikkes</trd>
  <trdgrp lng="it">
    <trd>chic</trd>,
    <trd>elegante</trd>,
    <trd>fine<klr> (elegante, con stile)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>chic</trd>,
    <trd>hip</trd>,
    <trd>deftig</trd>
  </trdgrp>
  <trd lng="pt">chique</trd>
  <trd lng="pl">szykowny</trd>
  <trd lng="ru">&c_sh;&c_i;&c_k;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
