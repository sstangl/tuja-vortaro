<vortaro>
<art mrk="$Id: pergol.xml,v 1.11 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pergol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pergol.0o">
  <kap><tld/>o</kap>
  <snc mrk="pergol.0o.HOR">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Gcirc;ardena konstrua&jcirc;o el traboj, sur kiu oni kreskigas lianojn:
      <ekz>
        iu frapas vi&scirc;ile ion sub la vinbera <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 67a, volumo 2a, p.
        438a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kiosk.0o">kiosko</ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="spalir.0o.HOR">spaliro</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_g;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>loub√≠</trd>,
    <trd>pergola</trd>
  </trdgrp>
  <trd lng="en">pergola</trd>
  <trd lng="es">p&eacute;rgola</trd>
  <trdgrp lng="fr">
    <trd>pergola</trd>,
    <trd>pergole</trd>,
    <trd>treille &agrave; l'italienne</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pergola</trd>,
    <trd>nyitott <ind>lugas</ind></trd>
  </trdgrp>
  <trd lng="nl">pergola</trd>
  <trd lng="pt">p&eacute;rgula</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_je;&c_d;&c_k;&c_a; &c_i;&c_z; &c_p;&c_o;&c_l;&c_z;&c_u;&c_ch;&c_i;&c_h; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_t;&c_a;&c_ja; &c_g;&c_a;&c_l;&c_je;&c_r;&c_je;&c_ja; &c_i;&c_z; &c_p;&c_o;&c_l;&c_z;&c_u;&c_ch;&c_i;&c_h;
    &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pergola</trd>
</drv>
</art>
