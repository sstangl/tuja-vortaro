<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: neuxtr3.xml,v 1.13 2009/06/29 18:05:11 revo Exp $">
<kap>
  <rad>ne&ubreve;trin</rad>/o
</kap>
<drv mrk="neuxtr3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lepton.0o">Leptono</ref> kun maso multe pli
      malgranda ol tiu de <ref cel="elektr14.0o">elektrono</ref>,
      se ne tute nula, kaj sen elektra &scirc;argo:
      <ekz>
        inter la multaj antipartikloj e&ccirc; ne&ubreve;traj partikloj
        (ne&ubreve;tronoj, <tld/>oj) estas distingeblaj pro sia
        kontra&ubreve;a spino
        <fnt><aut>OH</aut>: <vrk>&Ccirc;u antimaterio en la
        universo?</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muon.0o">muono</ref>,
      <ref cel="neuxtr2.0o">ne&ubreve;trono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Neutrino</trd>
  <trd lng="en">neutrino</trd>
  <trd lng="es">neutrino</trd>
  <trd lng="fa">&a_n;&a_w;&a_t;&a_r;&f_y;&a_n;&a_w;</trd>
  <trd lng="fr">neutrino</trd>
  <trd lng="hu">neutr&iacute;n&oacute;</trd>
  <trd lng="nl">neutrino</trd>
  <trd lng="pl">neutrino</trd>
  <trd lng="pt">neutrino</trd>
  <trd lng="ru">&c_n;&c_je;&c_j;&c_t;&c_r;&c_i;&c_n;&c_o;</trd>
</drv>
</art>
