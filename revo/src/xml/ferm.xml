<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ferm.xml,v 1.61 2015/07/06 12:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ferm</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ferm.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.0i.eniro">
    <dif>
      Meti ion en staton, kiu malebligas eniron a&ubreve; eliron:
      <ekz>
        li <tld/>is kolere la pordon
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i &ccirc;ambron, domon, havenon, barilon, tirkeston;
      </ekz>
      <ekz>
        <tld/>i enirejon per barilo, bre&ccirc;on per tabuloj;
      </ekz>
      <ekz>
        <tld/>i libron;
      </ekz>
      <ekz>
        <tld/>i la manon, lipojn, okulojn;
      </ekz>
      <ekz>
        kun <tld/>itaj <klr>(kunpremitaj)</klr> dentoj;
      </ekz>
      <ekz>
        <ind><tld/>i la bu&scirc;on al iu</ind>
        <klr>(lin silentigi)</klr>;
        <trd lng="fr">clouer le <ind>bec</ind> &agrave; qn</trd>
        <trd lng="pl">zamyka&cacute; <klr>komu&sacute;</klr>
        <ind>usta</ind></trd>
      </ekz>
      <ekz>
        <ind>ne<tld/>ita letero</ind>
        <klr>(kiun oni sendas al la ricevonto pere de rimedoj
        de publika informado)</klr>;
        <trd lng="fr"><ind>lettre</ind> ouverte</trd>
        <trd lng="pl"><ind>list</ind> otwarty</trd>
      </ekz>
      <ekz>
        ne<tld/>ita vundo;
      </ekz>
      <ekz>
        <tld/>i iun en &ccirc;ambro
        <klr>(<ref cel="ferm.en0i">en<tld/>i</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>i &gcirc;ardenon per krado
        <klr>(<ref cel="ferm.cxirkaux0i">&ccirc;irka&ubreve;<tld/>i
        </ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>i la landlimojn al la trafiko;
      </ekz>
      <ekz>
        <tld/>ita <klr>(ekskluziva)</klr> societo, sekto.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="sxlos.0i">&scirc;losi</ref>,
    </refgrp>
    <refgrp tip="vid">
      <ref cel="hermet">hermetika</ref>.
    </refgrp>
    <trd lng="hu">becsuk</trd>
    <trd lng="ca">tancar</trd>
    <trdgrp lng="nl">
      <trd>sluiten</trd>,
      <trd>toedoen</trd>,
      <trd>dichtmaken</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>fechar</trd>,
      <trd>cerrar</trd>
    </trdgrp>
  </snc>
  <snc mrk="ferm.0i.cxesigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi:
      <ekz>
        <tld/>i kunvenon<fnt>B</fnt>, kongreson<fnt>Z</fnt>,
        diskuton;
      </ekz>
      <ekz>
        &ccirc;iuj rimedoj estas <tld/>itaj <klr>(fari&gcirc;is
        neuzeblaj)</klr> por
        vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la mar&scirc;on, procesion
        <klr>(esti la lasta en)</klr>;
      </ekz>
      <ekz>
        tiu balo brile <tld/>is la serion da festoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kron.0i">kroni</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>bez&aacute;r<klr tip="ind"> (befejez)</klr></trd>,
      <trd>lez&aacute;r<klr tip="ind"> (befejez)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>sluiten</trd>,
      <trd>be&euml;indigen</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>encerrar</trd>,
      <trd>concluir</trd>,
      <trd>cessar a actividade</trd>,
      <trd>terminar</trd>,
      <trd>findar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">tancar</trd>
  <trdgrp lng="cs">
    <trd>uzav&rcaron;&iacute;t</trd>,
    <trd>zacelit</trd>,
    <trd>zacelovat</trd>,
    <trd>zad&ecaron;lat</trd>,
    <trd>zav&iacute;rat</trd>,
    <trd>zav&rcaron;&iacute;t</trd>
  </trdgrp>
  <trd lng="de">schlie&szlig;en</trd>
  <trd lng="en">shut</trd>
  <trd lng="es">cerrar</trd>
  <trdgrp lng="fr">
    <trd>clore</trd>,
    <trd>fermer</trd>
  </trdgrp>
  <trd lng="hu">becsuk</trd>
  <trdgrp lng="ku">
    <trd>girtin</trd>,
    <trd>dadan</trd>
  </trdgrp>
  <trd lng="pl">zamyka&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uzatvori&tcaron;</trd>,
    <trd>zavrie&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">kapatmak</trd>
</drv>

<drv mrk="ferm.0o">
  <kap><tld/>o</kap>
  <snc mrk="ferm.0o.KOMUNE">
    <dif>
      &Ccirc;esigo de trairado, de funkciado:
      <ekz>
        <tld/>o de teatro, de muzeo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>sepnut&iacute;</trd>,
    <trd>sev&rcaron;en&iacute;</trd>,
    <trd>skon&ccaron;en&iacute;</trd>,
    <trd>uzav&rcaron;en&iacute;</trd>,
    <trd>z&aacute;v&ecaron;r</trd>
  </trdgrp>
  <trd lng="es">cierre</trd>
  <trd lng="fr">fermeture</trd>
  <trd lng="pl">zamkni&eogonek;cie</trd>
  <trdgrp lng="sk">
    <trd>uzatv&aacute;ranie</trd>,
    <trd>z&aacute;ver</trd>
  </trdgrp>
</drv>

<drv mrk="ferm.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc mrk="ferm.0ajxo.MAT">
    <dif>
      <klr>(de subaro <frm><g>A</g></frm> en
      <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      La plej malgranda <ref cel="ferm.0ita.topologio"><tld/>ita</ref>
      <ref tip="super" cel="ar.sub0o">subaro</ref>, inkluzivanta
      <frm><g>A</g></frm> (alidire: la
      <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref> de
      &ccirc;iuj <tld/>itaj subaroj, inkluzivantaj &gcirc;in) :
      <ekz>
        la <tld/>a&jcirc;on de <frm><g>A</g></frm> oni kutime signas per
        <frm><g>A</g></frm> kun supra horizontala streko, nome
        <frm><g>A</g>&#773;</frm> <klr>(legu: a trabo)</klr>;
      </ekz>
      <ekz>
        <frm><g>A</g></frm> havas komunajn punktojn kun ajna
        <ref cel="cxirka.0ajxo.topologio">&ccirc;irka&ubreve;a&jcirc;o
        </ref> de ajna punkto en la <tld/>a&jcirc;o de
        <frm><g>A</g></frm>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;o de <frm><g>A</g></frm> estas la kuna&jcirc;o
        de &gcirc;iaj mal<tld/>a&jcirc;o kaj
        <ref cel="rand.0o.topologio">rando</ref>.
      </ekz>
    </dif>
    <ref tip="sin" cel="adher.0ajxo.MAT">adhera&jcirc;o.</ref>
    <rim>
      Ekzistas pluraj terminoj por &ccirc;i tiu nocio: en
      <fnt><bib>MatVort</bib></fnt> trovi&gcirc;as <ctl>
      <tld/>ita&jcirc;o</ctl> kaj <ctl>kloza&jcirc;o</ctl>
      (&ccirc;i-lasta anka&ubreve; en
      <fnt><bib>Deneva</bib></fnt>). Reiers&oslash;l
      <fnt><bib>MatStokTerm</bib>, <lok>p. 25</lok></fnt> proponas
      <ctl>klozuro</ctl>. Ni ne opiniis utila enkonduki novajn radikojn,
      &ccirc;ar <ctl>ferm</ctl> bone ta&ubreve;gas &ccirc;i-cele, sed al
      <ctl><tld/>ita&jcirc;o</ctl> preferis la malpli precizan
      <ctl><tld/>a&jcirc;o</ctl>, &ccirc;ar la unua aspektas kiel
      sinonimo por <ctl>io <tld/>ita</ctl>, do <ctl><tld/>ita subaro
      </ctl>.
    </rim>
    <trd lng="de">abgeschlossene <ind>H&uuml;lle</ind></trd>
    <trd lng="en">closure</trd>
    <trdgrp lng="fr">
      <trd>fermeture<klr tip="ind"> (d'un sous-ensemble)</klr></trd>,
      <trd>adh&eacute;rence<klr tip="ind"> (d'un sous-ensemble)</klr>
      </trd>
    </trdgrp>
    <trd lng="hu">lez&aacute;r&aacute;s<klr tip="ind">
    (r&eacute;szhalmaz&eacute;)</klr></trd>
    <trd lng="pl">domkni&eogonek;cie<klr tip="ind"> (podzbioru)</klr>
    </trd>
    <trd lng="ru">&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_n;&c_i;&c_je;
    <klr tip="ind">
    (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_a;)
    </klr></trd>
  </snc>
</drv>

<drv mrk="ferm.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ferm.0igxi.KOMUNE">
    <dif>
      Seni&gcirc;i je libere trairebla spaco,
      &ccirc;esigi sian funkciadon:
      <ekz>
        la pordoj de la salono <tld/>i&gcirc;is
        <fnt>
          <aut>Carlo</aut>,
          <vrk><url
          ref="http://www.radio-vatikana-esperanto.org/elsendo.php?d=2012-01-15">
          Internaciaj informoj</url></vrk>,
          <lok>Radio Vatikana, 2012-01-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">cerrarse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>clore</ind></trd>,
    <trd>se <ind>fermer</ind></trd>
  </trdgrp>
  <trd lng="pl">zamyka&cacute; si&eogonek;</trd>
</drv>

<drv mrk="ferm.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="ferm.0ita.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <klr>(pri <ref cel="silab.0o">silabo</ref>)</klr>
      Fini&gcirc;anta per <ref cel="konson1.0o">konsonanto</ref>
      a&ubreve; per <ref
      cel="vokal.kvazaux0o.FON">kvaza&ubreve;vokalo</ref>:
      <ekz>
        &leftquot;jam&rightquot;, &leftquot;kaj&rightquot; estas
        <tld/>itaj silaboj.
      </ekz>
    </dif>
    <trd lng="nl">gesloten<klr tip="ind"> (lettergreep)</klr></trd>
    <trdgrp lng="de">
      <trd>zu</trd>,
      <trd>geschlossen</trd>
    </trdgrp>
    <trd lng="es">(s&iacute;laba) trabada</trd>
    <trd lng="fr"><klr tip="amb">(syllabe)</klr> ferm&eacute;e</trd>
    <trd lng="hu">z&aacute;rt<klr tip="ind"> (mag&aacute;nhangz&oacute;)
    </klr></trd>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="ferm.0ita.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Kiu ne facile komunikas, ne volonte ekkonas novajn
      homojn, novajn aferojn:
      <ekz>
        li korespondis kun Rotkvi&ccirc;, sed Rotkvi&ccirc; estis tre 
        <tld/>ita persono
        <fnt>
          <aut>Jelena</aut>,
          <vrk>Mi vidis la mondon</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ferm.0ita.topologio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. subaro de
      <ref cel="spac.topologia0o">topologia spaco</ref>
      <frm>(<g>E</g>,<g>T</g>)</frm>)</klr>
      Tia, ke &gcirc;ia <ref cel="komple1.0o.aro">komplemento</ref>
      estas <ref cel="ferm.mal0ita.topologio">malfermita</ref>.
    </dif>
    <rim>
      Grandapene oni klopodas pruvi en
      <fnt><bib>MatStokTerm</bib>, <lok>p. 72</lok></fnt>, ke la paro
      <ctl><tld/>ita aro</ctl>/<ctl>mal<tld/>ita aro</ctl> ne
      ta&ubreve;gas, kaj ke multe pli bona estus <ctl>klozo</ctl>/<ctl>
      aperto</ctl>. La argumentoj estas beda&ubreve;rinde tre malfortaj:
      male al la aserto &ccirc;iu komprenas, ke la uzo de prefikso <ctl>
      mal-</ctl> en epiteto ne signifas, ke mal<tld/>ita aro estus la
      malo de <tld/>ita aro, same kiel mal<tld/>ita pordo ne estas la
      malo de <tld/>ita pordo! Estas tamen prave rimarkigi, ke
      mal<tld/>ita aro estas rezulto de neniu malferma ago,
      sed tiaj metaforoj abundas tra la tuta matematika terminologio kaj
      tu&scirc;as anka&ubreve; <ctl>aperto</ctl>, kiu
      la&ubreve;etimologie signifas... <ctl>malfermita&jcirc;o</ctl>.
      Ni do adoptis
      <ctl><tld/>ita</ctl>/<ctl>mal<tld/>ita</ctl>, kiel faris
      <fnt><bib>PIV2</bib></fnt>.
    </rim>
    <trd lng="de">abgeschlossen<klr tip="ind"> (-e Untermenge)</klr>
    </trd>
    <trd lng="en">closed<klr tip="ind"> (subset)</klr></trd>
    <trd lng="fr"><klr tip="ind">(sous-ensemble) </klr>ferm&eacute;
    </trd>
    <trd lng="hu">z&aacute;rt<klr tip="ind"> (r&eacute;szhalmaz)</klr>
    </trd>
    <trd lng="nl">gesloten<klr tip="ind"> (deelverzameling)</klr></trd>
    <trd lng="pl"><klr tip="ind">(podzbi&oacute;r)
    </klr>domkni&eogonek;ty</trd>
    <trd lng="ru">&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_o;&c_je;
    <klr tip="amb">
    (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)
    </klr></trd>
  </snc>
  <trd lng="es">cerrado</trd>
  <trd lng="fr">ferm&eacute;</trd>
</drv>

<drv mrk="ferm.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="F"/>i &ccirc;iuflanke:
      <ekz>
        malvasta &ccirc;irka&ubreve;<tld/>ita korto
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        kelkaj interspacoj, kiujn tiu malhela
        <ind>&ccirc;irka&ubreve;<tld/>o</ind> restigis, prezentis transe
        nur apike tran&ccirc;itajn rokojn
        <fnt><bib>Revadoj</bib>, <lok>Sepa promenado</lok></fnt>.
        <trd lng="fr">enceinte<klr> (fermeture)</klr></trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="bar.cxirkaux0i">&ccirc;irka&ubreve;bari</ref>
  </snc>
  <trdgrp lng="es">
    <trd>encerrar</trd>,
    <trd>rodear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ceindre<klr tip="ind"> (entourer d'une fermeture)</klr></trd>,
    <trd>confiner<klr tip="ind"> (entourer d'une fermeture)</klr></trd>
    <trd>enceindre</trd>,
    <trd>encercler</trd>,
    <trd>enclore</trd>,
    <trd>enfermer</trd>,
    <trd>enserrer</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zamyka&cacute; woko&lstroke;o</trd>,
    <trd>otacza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
    &c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_mol; &c_s;&c_o;
    &c_v;&c_s;&c_je;&c_h; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;
    </trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ferm.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.en0i.KOMUNE">
    <dif>
      Meti en ion <tld/>itan; teni <tld/>inte en io:
      <ekz>
        en<tld/>i iun en &ccirc;ambro;
      </ekz>
      <ekz>
        zorge en<tld/>i la provizojn;
      </ekz>
      <ekz>
        en<tld/>u la katon;
      </ekz>
      <ekz>
        peza en<tld/>ita aero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxlos">en&scirc;losi</ref>,
      <ref cel="ten.en0i">enteni</ref>
    </refgrp>
  </snc>
  <trd lng="cs">uzav&rcaron;&iacute;t uvnit&rcaron;</trd>
  <trd lng="de">einschlie&szlig;en</trd>
  <trd lng="es">encerrar</trd>
  <trd lng="fr">enfermer</trd>
  <trd lng="hu">bez&aacute;r <klr>(vhov&aacute;)</klr></trd>
  <trd lng="nl">opsluiten</trd>
  <trdgrp lng="pl">
    <trd>zamyka&cacute;<klr> (w czym&sacute;)</klr></trd>,
    <trd>zasklepia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">encerrar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
  </trdgrp><trd lng="ru">&c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_mol; <klr>
  (&c_v; &c_ch;&c_jo;&c_m;-&c_l;., &c_g;&c_d;&c_je;-&c_l;.)</klr></trd>
  <trd lng="sk">zavrie&tcaron;</trd>
</drv>

<drv mrk="ferm.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.mal0i.eniro">
    <dif>
      Meti en staton, kiu ebligas eniron kaj eliron:
      <ekz>
        kun bruo oni mal<tld/>is la pordegon, kaj la kale&scirc;o
        enveturis en la
        korton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>i siajn brakojn al amiko;
      </ekz>
      <ekz>
        mal<tld/>ita letero <klr>(kiun oni mal<tld/>is por legi)</klr>.
      </ekz>
    </dif>
    <trd lng="de">&ouml;ffnen</trd>
    <trdgrp lng="nl">
      <trd>openen</trd>,
      <trd>opendoen</trd>,
      <trd>openmaken</trd>
    </trdgrp>
  </snc>
  <snc mrk="ferm.mal0i.komenci">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komenci:
      <ekz>
        mal<tld/>i balon<fnt>B</fnt>;
      </ekz>
      <ekz>
        mal<tld/>i kongreson<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>i baloton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inauxg.0i">ina&ubreve;guri</ref>.
    </refgrp>
    <trd lng="de">er&ouml;ffnen</trd>
    <trdgrp lng="nl">
      <trd>openen</trd>,
      <trd>beginnen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">obrir</trd>
  <trdgrp lng="cs">
    <trd>otev&rcaron;&iacute;t</trd>,
    <trd>zah&aacute;jit</trd>,
    <trd>zav&eacute;st</trd>
  </trdgrp>
  <trd lng="es">abrir</trd>
  <trd lng="fr">ouvrir</trd>
  <trdgrp lng="hu">
    <trd>nyit</trd>,
    <trd>kinyit</trd>,
    <trd>megnyit</trd>
  </trdgrp>
  <trd lng="ku">vekirin</trd>
  <trd lng="pl">otwiera&cacute;</trd>
  <trd lng="pt">abrir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>otvori&tcaron;</trd>,
    <trd>zah&aacute;ji&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">a&ccedil;mak</trd>
</drv>

<drv mrk="ferm.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="ferm.mal0ajxo.KOMUNE">
    <dif>
      Libera, ne barita spaco a&ubreve; loko:
      <ekz>
        mal<tld/>a&jcirc;o de puto, groto, en muro;
      </ekz>
      <ekz>
        porda mal<tld/>a&jcirc;o, formita el tri grandegaj, brile
        poluritaj kalk&scirc;tonoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 5a, Lemuel
        aktivi&gcirc;as</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="apertu.0o">aperturo</ref>
    <trd lng="fr">ouverture</trd>
    <trd lng="pl">otw&oacute;r</trd>
  </snc>
  <snc mrk="ferm.mal0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(de subaro <frm><g>A</g></frm> en
      <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      La plej granda <ref cel="ferm.mal0ita.topologio">mal<tld/>ita
      </ref>
      <ref tip="super" cel="ar.sub0o">subaro</ref>, inkluzivata
      de <frm><g>A</g></frm> (alidire: la
      <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref> de
      &ccirc;iuj mal<tld/>itaj subaroj, inkluzivataj de &gcirc;i) :
      <ekz>
        la mal<tld/>a&jcirc;on de <frm><g>A</g></frm> oni kutime signas
        per <frm><g>A</g></frm> kun supra ringo, nome
        <frm><g>A</g>&#778;</frm> <klr>(legu: a ringo)</klr>;
      </ekz>
      <ekz>
        mal<tld/>ita aro identas kun sia mal<tld/>a&jcirc;o;
      </ekz>
      <ekz>
        la komplemento de la mal<tld/>a&jcirc;o de <frm><g>A</g></frm>
        egalas al la <tld/>a&jcirc;o de la komplemento de <frm><g>A</g>
        </frm>.
      </ekz>
    </dif>
    <ref tip="sin" cel="intern.0o">interno</ref>
    <trdgrp lng="de">
      <trd>Inneres</trd>,
      <trd>offener <ind>Kern</ind></trd>
    </trdgrp>
    <trd lng="en">interior</trd>
    <trd lng="fr">int&eacute;rieur<klr tip="ind"> (d'un sous-ensemble)
    </klr></trd>
    <trd lng="hu">ny&iacute;lt mag<klr tip="ind">
    (r&eacute;szhalmaz&eacute;)</klr></trd>
    <trd lng="pl">wn&eogonek;trze<klr tip="ind"> (zbioru)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_o;&c_je;
      <ind>&c_ja;&c_d;&c_r;&c_o;</ind>
      </trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="ferm.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ferm.mal0igxi.KOMUNE">
    <dif>
      Disspaci&gcirc;i, bre&ccirc;i&gcirc;i, disigi siajn partojn kaj
      ebligi trairon, funkciadon; komenci&gcirc;i:
      <ekz>
        de tie mal<tld/>i&gcirc;as bela vido al la maro;
      </ekz>
      <ekz>
        nova periodo mal<tld/>i&gcirc;as.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">abrirse</trd>
  <trd lng="fr">s'<ind>ouvrir</ind></trd>
</drv>

<drv mrk="ferm.mal0ita">
  <kap>mal<tld/>ita</kap>
  <snc mrk="ferm.mal0ita.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <klr>(pri <ref cel="silab.0o">silabo</ref>)</klr>
      Fini&gcirc;anta per <ref cel="vokal.0o">vokalo</ref>:
      <ekz>
        &leftquot;du&rightquot;, &leftquot;la&rightquot;,
        &leftquot;mi&rightquot; estas mal<tld/>itaj silaboj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vast.0a.FON">vasta</ref>
    </refgrp>
    <trd lng="nl">open<klr tip="ind"> (lettergreep)</klr></trd>
    <trdgrp lng="de">
      <trd>auf</trd>,
      <trd>offen</trd>,
      <trd>ge&ouml;ffnet</trd>
    </trdgrp>
    <trd lng="es">(s&iacute;laba) libre</trd>
    <trd lng="fr"><klr tip="amb">(syllabe) </klr>ouverte</trd>
    <trd lng="hu">ny&iacute;lt<klr tip="ind">
    (mag&aacute;nhangz&oacute;)</klr></trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="ferm.mal0ita.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Kiu volonte komunikas, interesi&gcirc;as pri nova&jcirc;oj:
      <ekz>
        mal<tld/>itaj estis la okuloj de Francisko, 
        mal<tld/>ita estis anka&ubreve; lia koro
        <fnt>
          <aut>G. F. De Ruyver</aut>,
          <vrk><url ref="http://www.esperanto.be/fel/2011/010221.php">
          Einstein havis okulojn malfermitajn</url></vrk>,
          <lok>Monato, 2011:01, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ferm.mal0ita.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="ferm.mal0ita.topologio">
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        <klr>(p.p. subaro de <ref cel="spac.topologia0o">topologia
        spaco</ref>
        <frm>(<g>E</g>,<g>T</g>)</frm>)</klr>
        Apartenanta al <frm><g>T</g></frm>.
      </dif>
      <rim>
        Ial nur la neologismo <ctl>aperta</ctl> aperas en
        <fnt><bib>MatVort</bib></fnt>, dum la sama konas <ctl>fermita
        </ctl> paralele kun <ctl>kloza</ctl>. Vd anka&ubreve; la
        rimarkon sub
        <ref cel="ferm.0ita.topologio"><tld/>ita</ref>.
      </rim>
    </subsnc>
    <subsnc mrk="ferm.mal0ita.metrikaspaco">
      <uzo tip="fak">MAT</uzo>
      <dif>
        <klr>(p.p. subaro de
        <ref cel="spac.metrika0o">metrika spaco</ref>)</klr>
        Tia, ke por &ccirc;iu &gcirc;ia punkto <frm><k>x</k></frm>
        &gcirc;i inkluzivas malfermitan
        <ref cel="glob.0o.metrikaspaco">globon</ref> kun centro
        <frm><k>x</k></frm> kaj radiuso <frm>&rho; &gt; 0</frm>:
        <ekz>
          la mal<tld/>itaj aroj de metrika spaco konsistigas &gcirc;ian
          kutiman <ref cel="topolo.0o.strukturo">topologion</ref>.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">offen<klr tip="ind"> (-e Untermenge)</klr></trd>
    <trd lng="en">open<klr tip="ind"> (subset)</klr></trd>
    <trd lng="nl">open<klr tip="ind"> (deelverzameling)</klr></trd>
    <trd lng="fr"><klr tip="ind">(sous-ensemble) </klr>ouvert</trd>
    <trd lng="hu">ny&iacute;lt<klr tip="ind"> (r&eacute;szhalmaz)</klr>
    </trd>
    <trd lng="pl"><klr tip="ind">(podzbi&oacute;r) </klr>otwarty</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_o;&c_je;
    <klr tip="amb">
    (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)
    </klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>otev&rcaron;en&yacute;</trd>,
    <trd>pr&uring;chodn&yacute;</trd>
  </trdgrp>
  <trd lng="es">abierto</trd>
  <trd lng="fr">ouvert</trd>
  <trdgrp lng="sk">
    <trd>otvoren&yacute;</trd>,
    <trd>priechodn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ferm.fulmo0ilo">
  <kap>fulmo<tld/>ilo</kap>
  <snc mrk="ferm.fulmo0ilo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="zip.0o">zipo</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>zdrhovadlo</trd>,
    <trd>zip</trd>
  </trdgrp>
  <trd lng="de">Rei&szlig;verschluss</trd>
  <trdgrp lng="es">
    <trd>cierre</trd>,
    <trd>zipper</trd>,
    <trd>cremallera</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ind>fermeture</ind> &agrave; glissi&egrave;re</trd>,
    <trd><ind>fermeture</ind> &eacute;clair</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vill&aacute;mz&aacute;r</trd>,
    <trd>cipz&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zamek b&lstroke;yskawiczny</trd>,
    <trd>suwak</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a; &leftquot;
  <ind>&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;</ind>&rightquot;</trd>
  <trd lng="sk">zips</trd>
</drv>

<drv mrk="ferm.jxet0i">
  <kap>&jcirc;et<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.jxet0i.KOMUNE">
    <dif>
      <tld lit="F"/>i subite per &jcirc;eta movo:
      <ekz>
        la juna vidvino <klr>[...]</klr> forte &jcirc;et<tld/>is
        post si la pordeton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">dar un portazo</trd>
  <trd lng="fr">claquer<klr tip="ind"> (fermer brutalement)</klr></trd>
  <trd lng="pl">zatrzaskiwa&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_h;&c_l;&c_o;&c_p;&c_n;&c_u;&c_t;&c_mol;
  </trd>
</drv>

<drv mrk="ferm.pusxmal0i">
  <kap>pu&scirc;mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ferm.pusxmal0i.KOMUNE">
    <dif>
      Mal<tld/>i per ekpu&scirc;o a&ubreve; premo:
      <ekz>
        la edukejestro <klr>...</klr> pu&scirc;mal<tld/>is la fenestron
        <fnt>
          <aut>Liu Houming</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/epc/epc8303/sago.html">
          Nigra Sago</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1983:03, p. 38a-41a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ouvrir en poussant</trd>
</drv>
