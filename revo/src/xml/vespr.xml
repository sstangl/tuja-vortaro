<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vespr.xml,v 1.20 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vespr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vespr.0o">
  <kap><tld/>o</kap>
  <snc mrk="vespr.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Vespera <ref tip="super" cel="serv.Di0o">diservo</ref>,
      konsistanta el psalmoj kaj himnoj:
      <ekz>
        kiam la re&gcirc;o iris al la <tld/>oj,
        Audun intencis renkonti lin
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Audun kaj la urso</vrk>,
          <lok>en: Islandaj Pravo&ccirc;oj, 1964</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="vespr.0o.RIM_1">
    Foje uzata plurnombre imite al naciaj lingvoj.
  </rim>
  <trd lng="be">&c_v;&c_ja;&c_ch;&c_e;&c_r;&c_n;&c_ja;</trd>
  <trd lng="ca">vespres</trd>
  <trdgrp lng="de">
    <trd>Vesper</trd>,
    <trd>Abendandacht</trd>
  </trdgrp>
  <trd lng="en">vespers</trd>
  <trd lng="es">v&iacute;spera<klr> (oficio religioso de tarde)</klr></trd>
  <trd lng="fa">&a_n;&a_m;&a_A;&a_z; &a_sh;&a_A;&a_m;</trd>
  <trd lng="fr">v&ecirc;pres</trd>
  <trdgrp lng="hu">
    <trd>vesper&aacute;s</trd>,
    <trd>vecsernye</trd>
  </trdgrp>
  <trd lng="it">vespro</trd>
  <trd lng="nl">vesper</trd>
  <trd lng="pl">nieszpora</trd>
  <trd lng="pl">nieszpory</trd>
  <trd lng="pt">v&eacute;spera</trd>
  <trd lng="ru">&c_v;&c_je;&c_ch;&c_je;&c_r;&c_n;&c_ja;</trd>
  <trd lng="sk">večerná pobožnosť</trd>
  <trdgrp lng="cs">
    <trd>nešpory</trd>,
    <trd>přen. večer</trd>,
    <trd>večerní bohoslužba</trd>
  </trdgrp>
</drv>
</art>
