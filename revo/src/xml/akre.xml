<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akre.xml,v 1.13 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akre</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="akre.0o">
  <kap><tld/>o</kap>
  <snc mrk="akre.0o.MEZ">
    <dif>
      Eksa <ref tip="lst" cel="unu.mezur0o"
      lst="voko:areoj">mezurunuo</ref> de
      tersurfaco valoranta inter 0,2 kaj 0,9 hektarojn la&ubreve; lokoj kaj
      epokoj; <ref tip="sin" cel="akr1.0o.MEZ">akro</ref>:
      <ekz>
        vi posedas nur <klr>...</klr> du <tld/>ojn da plugotero kaj
        apena&ubreve; paron da &ccirc;ifonoj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 2a, p.
        407a</lok></fnt>
      </ekz>
    </dif>
    <subsnc mrk="akre.0o.brita">
      <dif>
        Brita <tld/>o: proksimume 0,4 hektarojn.
      </dif>
    </subsnc>
    <subsnc mrk="akre.0o.franca">
      <dif>
        Franca <tld/>o: proksimume 0,5 hektarojn.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>akr</trd>,
    <trd>jitro <klr>(0,817 ha)</klr></trd>
  </trdgrp>
  <trd lng="de">Acre<klr> (Einheit)</klr></trd>
  <trd lng="en">acre</trd>
  <trd lng="es">acre</trd>
  <trd lng="fa">&a_A_madda;&f_k;&a_r;<klr> (&a_w;&a_A;&a_H;&a_d; &a_s;&a_T;&a_H;)</klr></trd>
  <trd lng="fr">acre<klr> (mesure de surface)</klr></trd>
  <trd lng="hu">hold<klr> (f&ouml;ldm&eacute;rt&eacute;k)</klr></trd>
  <trd lng="id">ekar</trd>
  <trd lng="pt">acre</trd>
  <trd lng="ru">&c_a;&c_k;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>aker</trd>,
    <trd>aker <klr>(plošná miera 40 árov)</klr></trd>
  </trdgrp>
</drv>
</art>

