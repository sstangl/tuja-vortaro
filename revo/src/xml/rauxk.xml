<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rauxk.xml,v 1.14 2013/01/01 12:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ra&ubreve;k</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rauxk.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="rauxk.0a.SON">
    <dif>
      Sonanta mal<ref tip="ant" cel="klar.0a.malrauxka">klare</ref>,
      obtuze, kvaza&ubreve; skrapante:
      <ekz>
        <tld/>a vo&ccirc;o, krio;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i pro troa kriado, pro malvarmumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rasp">raspa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_r;&c_y;&c_p;&c_l;&c_y;</trd>,
    <trd>&c_h;&c_r;&c_y;&c_p;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_r;&c_k;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rau</trd>,
    <trd>heiser</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hoarse</trd>,
    <trd>husky</trd>
  </trdgrp>
  <trd lng="fr">rauque</trd>
  <trd lng="hu">rekedt</trd>
  <trdgrp lng="nl">
    <trd>hees</trd>,
    <trd>rauw</trd>,
    <trd>schor</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chrypliwy</trd>,
    <trd>ochryp&lstroke;y</trd>,
    <trd>zachrypniety</trd>,
    <trd>chrypi&aogonek;cy</trd>
  </trdgrp>
  <trd lng="pt">rouco</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">hes</trd>
</drv>
</art>
