<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ilustr.xml,v 1.17 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ilustr</rad>/i
</kap>
<drv mrk="ilustr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Ornami tekston per bildoj a&ubreve; desegna&jcirc;oj rilataj al &gcirc;i:
      <ekz><tld/>i libron;</ekz>
      <ekz><tld/>ita revuo.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Ornami per bildoj: <ekz><tld/>ita po&scirc;tkarto.</ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klarigi ideon per komparoj a&ubreve; ekzemploj: 
      <ekz><tld/>i regulon per multaj ekzemploj;</ekz>
      <ekz><tld/>i sian penson per metaforoj kaj figuroj.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>ilustrovat</trd>,
    <trd>zobrazit</trd>,
    <trd>zobrazovat</trd>
  </trdgrp>
  <trd lng="de">illustrieren</trd>
  <trd lng="en">illustrate</trd>
  <trd lng="es">ilustrar</trd>
  <trd lng="fr">illustrer</trd>
  <trd lng="hu">illusztr&aacute;l</trd>
  <trd lng="nl">illustreren</trd>
  <trd lng="pt">ilustrar</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ilustrovať</trd>,
    <trd>zobraziť</trd>
  </trdgrp>
</drv>

<drv mrk="ilustr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc num="1">
    <dif>
      Desegno, kiu <tld/>as. 
    </dif>
    <trd lng="nl">illustratie</trd>
  </snc>
  <snc num="2">
    <dif>
      Ekzemplo, kiu klarigas.  
    </dif>
    <trd lng="de">Illustration</trd>
    <trd lng="en">illustration</trd>
    <trd lng="es">ilustraci&oacute;n</trd>
    <trd lng="nl">voorbeeld</trd>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">illustration<klr tip="ind"> (ce qui illustre)</klr></trd>
  <trd lng="hu">illusztr&aacute;ci&oacute;</trd>
  <trd lng="pt">ilustra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
