<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aceton.xml,v 1.22 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>aceton</rad>/o
</kap>
<drv mrk="aceton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Senkolora likva&jcirc;o, kun odoro de vinagro, kiun oni produktas
      per distilo de ligno - &hcirc;emia simbolo:
      CH<sub>3</sub>(CO)CH<sub>3</sub>.
      La plej simpla ketono.
      Ofte uzata kiel organika
      <ref tip="malprt" cel="solv.0ilo">solvilo</ref> kaj dum produktado
      de plastoj.
    </dif>
    <rim>SIN: propanono</rim>
  </snc>
  <trd lng="be">&c_a;&c_c;&c_e;&c_t;&c_o;&c_n;</trd>
  <trd lng="bg">&c_a;&c_c;&c_je;&c_t;&c_o;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>acetona</trd>,
    <trd>propanona</trd>,
    <trd>2-propanona</trd>,
    <trd>dimetilcetona</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aceton</trd>,
    <trd>propanon</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Azeton</trd>,
    <trd>Propanon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>acetone</trd>,
    <trd>dimethyl ketone</trd>,
    <trd>2-propanone</trd>,
    <trd>beta-ketopropane</trd>
  </trdgrp>
  <trd lng="es">acetona</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_w;&a_n;</trd>
  <trd lng="fr">ac&eacute;tone</trd>
  <trd lng="hu">aceton</trd>
  <trd lng="it">acetone</trd>
  <trd lng="nl">aceton</trd>
  <trd lng="pl">aceton</trd>
  <trd lng="pt">acetona</trd>
  <trd lng="ru">&c_a;&c_c;&c_je;&c_t;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>acet&oacute;n</trd>,
    <trd>acet√≥n</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>aceton</trd>,
    <trd>propanon</trd>,
    <trd>dimetylketon</trd>
  </trdgrp>
</drv>
</art>
