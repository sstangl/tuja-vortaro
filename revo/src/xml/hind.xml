<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hind.xml,v 1.27 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hind</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hind.0o">
  <kap><tld/>o</kap>
  <snc mrk="hind.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="hind.H0ujo"><tld lit="H"/>ujo</ref>.
    </dif>
    <trd lng="de">Indischer Staatsb&uuml;rger</trd>
    <trd lng="hu">indiai (&aacute;llampolg&aacute;r)</trd>
  </snc>
  <snc mrk="hind.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        fakte &ccirc;inoj kaj <tld/>oj <klr>(en Malajzio)</klr> scipovas
        paroli sian gepatran lingvon, sed estas analfabetaj en tiu
        lingvo
        <fnt>
          <vrk>Malajzio - Multlingveco</vrk>
          <bib>Monato</bib>,
          <lok>1994-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia longa ru&gcirc;a vesto staras la <tld lit="H"/>ino sur
        la brullignaro, avide la flamoj el&scirc;ovas siajn langojn
        supren
        &ccirc;irka&ubreve; la virino kaj &scirc;ia mortinta edzo
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Inder</trd>
    <trd lng="hu">hindu (nemzetis&eacute;g&udblac;)</trd>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_je;&c_c;</trd>
  <trd lng="ca">indi<klr> (subst.)</klr></trd>
  <trd lng="cs">Ind</trd>
  <trd lng="el">&jota;&ny;&delta;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="es">
    <trd>indio</trd>,
    <trd>hind&uacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Indien<klr tip="ind"> (habitant de l'Inde)</klr></trd>
  </trdgrp>
  <trd lng="it">indiano<klr> (dell'India - sost.)</klr></trd>
  <trd lng="nl">Indi&euml;r</trd>
  <trd lng="pl">Hindus</trd>
  <trd lng="pt">indiano</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_je;&c_c;</trd>
  <trd lng="sk">Ind</trd>
</drv>

<drv mrk="hind.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilantanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        sur la folio de la lotuso &gcirc;i pelas la sanktan akvon de
        la rivero Gango kaj la okuloj de la <tld lit="H"/>a knabino
        eklumas,
        kiam &scirc;i &gcirc;in ekvidas
        <fnt><bib>Fab3</bib>, <lok>Birdo Fenikso</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[en Malajzio]</klr> en hindaj kaj &ccirc;inaj familioj
        la infanoj parolas hinde a&ubreve; &ccirc;ine kaj en lernejoj
        ili lernas la malajan kaj anglan
        <fnt>
          <vrk>Malajzio - Multlingveco</vrk>
          <bib>Monato</bib>,
          <lok>1994-02</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">indi<klr> (adj.)</klr></trd>
  <trd lng="cs">indický</trd>
  <trd lng="de">indisch</trd>
  <trd lng="el">&jota;&ny;&delta;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="es">
    <trd>indio</trd>,
    <trd>hind&uacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indien<klr tip="ind"> (adj., d'Inde)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hindu (nemzetis&eacute;g&udblac;)</trd>,
    <trd>indiai</trd>
  </trdgrp>
  <trd lng="it">indiano<klr> (dell'India - agg.)</klr></trd>
  <trd lng="nl">Indisch</trd>
  <trd lng="pl">indyjski</trd>
  <trd lng="pt">indiano</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">indický</trd>
</drv>

<drv mrk="hind.H0ujo">
  <kap><tld lit="H"/>ujo</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc mrk="hind.H0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La <tld/>a subkontinento, situanta en suda
      <ref tip="malprt" cel="azi.A0o">Azio</ref>,
      de Himalajo norde &gcirc;is en la <tld lit="H"/>an
      Oceanon sude:
      <ekz>
        la ideo de Kolumbo ke devas ekzisti okcidenta vojo <tld
        lit="H"/>ujon
        &scirc;ajnas al ni nun tiel simpla, tiel natura
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="hind.H0ujo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> 
      limanta norde al Pakistano, &Ccirc;inujo, Nepalo kaj
      etendi&gcirc;anta sude &gcirc;is la pinto de la subkontinento;
      <ref tip="sin" cel="barat.B0o">Barato</ref>:
      <ekz>
        tio estis en la tempo de A&hcirc;a&scirc;vero&scirc;, kiu
        re&gcirc;is de <tld lit="H"/>ujo &gcirc;is Etiopujo super
        cent dudek sep landoj
        <fnt><bib>MT</bib>, <lok>&Est; 1:1</lok></fnt>;
      </ekz>
      <ekz>
        Holger la Dano militakiris la grandan landon <tld lit="H"/>ujon
        &gcirc;is la orienta fino de la mondo, &gcirc;is tiu arbo, kiu
        estas nomata arbo de la suno
        <fnt><bib>Fab3</bib>, <lok>&Scirc;tono de la
        sa&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>&Iacute;ndia</trd>,
    <trd>Rep&uacute;blica de l'&Iacute;ndia</trd>
  </trdgrp>
  <trd lng="de">Indien</trd>
  <trd lng="el">&Jota;&ny;&delta;&jota_ton;&alfa;</trd>
  <trd lng="es">India</trd>
  <trd lng="fr">Inde</trd>
  <trd lng="hu">India</trd>
  <trd lng="it">India</trd>
  <trd lng="nl">Indi&euml;</trd>
  <trd lng="pl">Indie</trd>
  <trd lng="pt">&Iacute;ndia</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hind.H0aOceano">
  <kap><tld lit="H"/>a Oceano</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      inter Afriko, Azio kaj Oceanio.
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_a;&c_k;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="ca">oce&agrave; &Iacute;ndic</trd>
  <trd lng="de">Indischer Ozean</trd>
  <trd lng="es">Oceano <ind>&Iacute;ndico</ind></trd>
  <trd lng="fr">Oc&eacute;an Indien</trd>
  <trd lng="hu">Indiai-&oacute;ce&aacute;n</trd>
  <trd lng="it">Oceano <ind>Indiano</ind></trd>
  <trd lng="nl">Indische oceaan</trd>
  <trd lng="pl">Ocean <ind>Indyjski</ind></trd>
  <trd lng="pt">Oceano &Iacute;ndico</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
</drv>

<drv mrk="hind.0euxropa">
  <kap><tld/>e&ubreve;ropa</kap>
  <tezrad fak="LIN"/>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Rilata al la granda lingvofamilio, disvasti&gcirc;inta en 15-10aj
      jc a.K. en <tld lit="H"/>ujo, Persujo, Anatolio kaj E&ubreve;ropo:
      <ekz>
        klopodon rekonstrui lingvan praformon (same kiel oni
        rekonstruis la <tld/>e&ubreve;ropan pralingvon) oni povas opinii
        a&ubreve; esprimo de estimo por la lingvaj tradicioj, a&ubreve;
        lingvistika ludismo
        <fnt><aut>V. Barandovsk&aacute;-Frank</aut>,
        <vrk><url
        ref="http://www.uni-paderborn.de/extern/fb/2/Kyb.Paed/INT/07-int.htm">Enkonduka
        lernolibro de interlingvistiko</url></vrk>,
        <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_ib;&c_n;&c_d;&c_a;&c_e;&c_w;&c_r;&c_a;&c_p;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">indoeuropeu<klr> (adj.)</klr></trd>
  <trd lng="de">indogermanisch</trd>
  <trd lng="en">Indo-European</trd>
  <trd lng="es">indoeuropeo</trd>
  <trd lng="fr">indoeurop&eacute;en</trd>
  <trdgrp lng="it">
   <trd>indeuropeo<klr> (agg.)</klr></trd>, 
   <trd>indoeuropeo<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Indo-Europees</trd>
  <trd lng="pl">indoeuropejski</trd>
  <trd lng="hu">indoeur&oacute;pai</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
