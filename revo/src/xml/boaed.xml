<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boaed.xml,v 1.1 2013/11/02 11:10:15 revo Exp $">
<kap>
  <rad>boaed</rad>/oj
</kap>
<drv mrk="boaed.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Boidae</trd>), de grandaj
      sufokserpentoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Boas</trd>,
    <trd>Riesenschlangen</trd>
  </trdgrp>
</drv>
</art>

