<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gxener.xml,v 1.25 2009/03/18 17:32:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&gcirc;eneral</rad>/a
</kap>

<drv mrk="gxener.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta senescepte al tuto a&ubreve; al la plej granda parto el 
     personoj a&ubreve; objektoj; neaparta, neindividua:
      <ekz>
        <tld/>a kunveno, regulo, opinio;
      </ekz>
      <ekz>
        <tld/>aj ideoj;
      </ekz>
      <ekz>
        <tld/>a malarmado;
      </ekz>
      <ekz>
        mi sendas al vi mian <tld/>an prezaron;
      </ekz>
      <ekz>
        <tld/>a pardono.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="specia.0a">speciala</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_je;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_b;&c_shch;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_v;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">v&scaron;eobecn&yacute;</trd>
  <trdgrp lng="de">
    <trd>allgemein</trd>,
    <trd>generell</trd>
  </trdgrp>
  <trd lng="el">
&gamma;&epsilon;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">general</trd>
  <trdgrp lng="es">
    <trd>general</trd>,
    <trd>gen&eacute;rico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;ral</trd>,
    <trd>commun</trd>,
    <trd>global</trd>,
    <trd>ordinaire</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ltal&aacute;nos</trd>
  <trdgrp lng="it">
    <trd>generale</trd>
    <trd>generico</trd>
  </trdgrp>
  <trd lng="nl">algemeen</trd>
  <trdgrp lng="pl">
    <trd>generalny</trd>,
    <trd>walny</trd>,
    <trd>og&oacute;lnikowy</trd>,
    <trd>gremialny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>geral</trd>,
    <trd>gen&eacute;rico</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>generell</trd>,
    <trd>allm&auml;n</trd>
  </trdgrp>
</drv>

<drv mrk="gxener.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En maniero <tld/>a, ne speciala.
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_o;&c_b;&c_shch;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_b;&c_shch;&c_o;</trd>,
    <trd>&c_v;&c_malmol;&c_o;&c_b;&c_shch;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;ralement</trd>,
    <trd>commun&eacute;ment</trd>,
    <trd>en <ind>gros</ind></trd>,
    <trd>globalement</trd>,
    <trd>ordinairement</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>generalmente</trd>,
    <trd>gen&eacute;ricamente</trd>,
    <trd>en general</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>generalmente</trd>,
    <trd>in generale</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w og&oacute;le</trd>,
    <trd>generalnie</trd>,
    <trd>walnie</trd>,
    <trd>og&oacute;lnikowo</trd>,
    <trd>gremialnie</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>geralmente</trd>, 
   <trd>em <ind>geral</ind><klr tip="ind"> (adv.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_b;&c_shch;&c_je;</trd>,
    <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_c;&c_je;&c_l;&c_o;&c_m;</ind></trd>,
    <trd>&c_v; <ind>&c_o;&c_b;&c_shch;&c_je;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="gxener.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io havu <tld/>an amplekson:
      <ekz>
        <tld/>igi regulon, doktrinon,
        le&gcirc;on, kutimon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>
&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;
	</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verallgemeinern</trd>,
    <trd>generalisieren</trd>
  </trdgrp>
  <trd lng="el">
  &gamma;&epsilon;&ny;&jota;&kappa;&epsilon;&ypsilon_ton;&omega;</trd>
  <trd lng="en">generalize</trd>
  <trd lng="es">generalizar</trd>
  <trd lng="fr">g&eacute;n&eacute;raliser</trd>
  <trd lng="hu">&aacute;ltal&aacute;nos&iacute;t</trd>
  <trd lng="it">generalizzare</trd>
  <trd lng="nl">veralgemenen</trd>
  <trdgrp lng="pl">
    <trd>uog&oacute;lnia&cacute;</trd>,
    <trd>generalizowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">generalizar</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_b;&c_shch;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
