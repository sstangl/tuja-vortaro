<kap>
  <rad>hiened</rad>/oj
</kap>
<drv mrk="hiened.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Hyaenidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_je;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">hi&egrave;nids</trd>
  <trd lng="de">Hy&auml;nen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hy&eacute;nid&eacute;s</trd>
  <trd lng="hu">hi&eacute;naf&eacute;l&eacute;k</trd>
  <trd lng="it">ienidi</trd>
  <trd lng="nl">hyena's<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">hien&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_i;&c_je;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
