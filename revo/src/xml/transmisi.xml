<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: transmisi.xml,v 1.6 2015/10/13 09:10:39 revo Exp $">
<kap>
  <rad>transmisi</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="transmisi.0i">
  <kap><tld/>i</kap>
  <snc mrk="transmisi.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="per.0i.KOMUNE">Peri</ref> movon, impulson, 
      energion, informon al alia ma&scirc;ino, al alia loko:
      <ekz>
        la penso <tld/>i povumon de jetmotoro al helico ne estas nova
        <fnt>
          <aut>J. Kubat</aut>,
          <vrk><url
          ref="http://www.fw.hu/eventoj/steb/ingxenierado/ceteraj/aviado/saest82-15.htm">
          Nova erao de helicoj</url></vrk>,
          <lok>SAEST 1982, p. 62a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">transmetre</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ev&aacute;d&ecaron;t energii</trd>,
    <trd>p&rcaron;ev&aacute;d&ecaron;t pohyb</trd>,
    <trd>p&rcaron;ev&aacute;d&ecaron;t s&iacute;lu</trd>
  </trdgrp>
  <trd lng="en">to transmit</trd>
  <trd lng="fr">transmettre<klr> (m&eacute;canique etc.)</klr></trd>
  <trd lng="it">trasmettere<klr> (mecc. ecc.)</klr></trd>
  <trdgrp lng="pl">
    <trd>transmitowa&cacute;</trd>,
    <trd>przesy&lstroke;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prev&aacute;dza&tcaron; energiu</trd>,
    <trd>prev&aacute;dza&tcaron; pohyb</trd>
  </trdgrp>
</drv>

<drv mrk="transmisi.0o">
  <kap><tld/>o</kap>
  <snc mrk="transmisi.0o.TEK">
    <dif>
      Pero de movo,impulso, energio, informo...:
      <ekz>
        biciklo kun muntita <tld/>o
        <fnt>
          <aut>M. Cruz</aut>,
          <vrk><url
          ref="http://alinome.net/bici/con_bici01_bici.eo.html">
          Bicikle vivi</url></vrk>,
          <lok>2003-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">transmissi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;evod energie</trd>,
    <trd>p&rcaron;evod pohybu</trd>
  </trdgrp>
  <trd lng="en">transmission</trd>
  <trd lng="fr">transmission<klr> (m&eacute;canique etc.)</klr></trd>
  <trd lng="it">trasmissione<klr> (mecc. ecc.)</klr></trd>
  <trd lng="pl">transmisja</trd>
  <trdgrp lng="sk">
    <trd>prenos</trd>,
    <trd>pren&aacute;&scaron;anie</trd>
  </trdgrp>
</drv>
</art>

