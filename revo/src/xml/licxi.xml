<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: licxi.xml,v 1.13 2014/08/18 01:10:14 revo Exp $">
<kap>
  <rad>li&ccirc;i</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="licxi.0o">
  <kap><tld/>o</kap>
  <snc mrk="licxi.0o.KUI">
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, malgranda sfera <ref tip="super"
      cel="drup.0o">drupo</ref> kun tubereta &scirc;elo kaj diafana, suka
      karno &ccirc;irka&ubreve; la kerno:
      <ekz>
        mi devas transdoni &scirc;ian saluton al vi kaj anka&ubreve;
        dankon pro la <tld/>oj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 83a, volumo 3a, p.
        48a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_ch;&c_y;<klr tip="amb"> (&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_ch;&c_i;</trd>
  <trd lng="ca">litxi</trd>
  <trd lng="de">Litschi</trd>
  <trdgrp lng="en">
    <trd>lychee</trd>,
    <trd>litchi</trd>
  </trdgrp>
  <trd lng="fr">litchi<klr> (fruit)</klr></trd>
  <trd lng="hu">licsi</trd>
  <trd lng="nl">lychee</trd>
  <trd lng="pl">liczi</trd>
  <trdgrp lng="pt">
    <trd>lechia <klr>(fruta)</klr></trd>,
    <trd>lichi <klr>(fruta)</klr></trd>,
    <trd>lichia <klr>(fruta)</klr></trd>,
    <trd>litchi <klr>(fruta)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_ch;&c_i; <klr tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
</drv>

<drv mrk="licxi.0arbo">
  <kap><tld/>arbo,
  <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="licxi.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Orientazia arbo kultivata
      por ligno kaj man&gcirc;eblaj fruktoj
      (<trd lng="la">Litchi chinensis</trd>):
      <ekz>
        floris la <tld/>arboj
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_ch;&c_y; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">litxi</trd>
  <trd lng="de">Litschibaum</trd>
  <trdgrp lng="en">
    <trd>lychee</trd>,
    <trd>litchi</trd>
  </trdgrp>
  <trd lng="fr">litchi<klr> (arbre)</klr></trd>
  <trd lng="hu">licsifa</trd>
  <trd lng="nl">lychee-boom</trd>
  <trdgrp lng="pt">
    <trd>lechia <klr>(&aacute;rvore)</klr></trd>,
    <trd>lichieira <klr>(&aacute;rvore)</klr></trd>,
    <trd>lichi <klr>(&aacute;rvore)</klr></trd>,
    <trd>lichia <klr>(&aacute;rvore)</klr></trd>,
    <trd>litchi <klr>(&aacute;rvore)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_ch;&c_i; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
</drv>
</art>

