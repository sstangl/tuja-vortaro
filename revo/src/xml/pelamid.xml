<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pelamid.xml,v 1.4 2009/07/08 16:31:30 revo Exp $">
<kap>
  <rad>pelamid</rad>/o
</kap>
<drv mrk="pelamid.0o">
  <kap><tld/>o</kap>
  <snc mrk="pelamid.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mediteranea specio de <ref tip="super" cel="tinus.0o">tinuso</ref>
      (<trd lng="la">Thynnus pelamis</trd>):
      <ekz>
        la gastoj estis metantaj sur la telerojn sprotojn, sardinojn,
        <tld/>ojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bonite</trd>,
    <trd>leiche</trd>,
    <trd>p&eacute;lamide</trd>,
    <trd>p&eacute;lamyde</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boniet</trd>,
    <trd>bonetvis</trd>,
    <trd>albikoor</trd>
  </trdgrp>
  <trd lng="pt">bonito <klr tip="amb"> (peixe)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_l;&c_a;&c_m;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_r;&c_je;&c_d;&c_i;&c_z;&c_je;&c_m;&c_n;&c_o;&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j; &c_t;&c_u;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>

