<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krokod1.xml,v 1.8 2013/08/31 15:10:20 revo Exp $">
<kap>
  <rad>krokodiled</rad>/oj
</kap>
<drv mrk="krokod1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="krokod.0uloj">krokodiluloj</ref>
      (<trd lng="la">Crocodylidae</trd>), &ccirc;e kiuj la kvara dento estas
      videbla &ccirc;e fermita bu&scirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_k;&c_r;&c_a;&c_k;&c_a;&c_d;&c_z;&c_ib;&c_l;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Echte <ind>Krokodile</ind></trd>
  <trd lng="hu">krokodilf&eacute;l&eacute;k</trd>
  <trd lng="pt">crocodil&iacute;deos</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_k;&c_r;&c_o;&c_k;&c_o;&c_d;&c_i;&c_l;&c_y;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

