<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: san.xml,v 1.63 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>san</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="san.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="san.0a.bonfarta">
    <dif>
      Havanta korpon, organaron, menson, kiuj bone kaj normale
      funkcias:
      <ekz>
        Se mi estus <tld/>a, mi estus feli&ccirc;a
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        <ind>en korpo <tld/>a la menso <tld/>a</ind>;
        <trd lng="fr">un esprit sain dans un corps sain</trd>
        <trd lng="hu">&eacute;p testben &eacute;p l&eacute;lek</trd>
        <trd lng="ru">&c_v; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_o;&c_m; &c_t;&c_je;&c_l;&c_je; &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;
        &c_d;&c_u;&c_h;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto <tld/>e eltenis &ccirc;iujn ventojn kaj
        pu&scirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio malpermesas al la lingvo libere kaj <tld/>e
        disvolvi&gcirc;i<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="san.0a.saniga">
    <ref tip="dif" cel="favor.san0a.MED"><tld/>favora</ref>
    <ekz>
      <tld/>a pano<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_a;&c_r;&c_o;&c_v;&c_y;</trd>
  <trd lng="bg">&c_z;&c_d;&c_r;&c_a;&c_v;</trd>
  <trdgrp lng="cs">
    <trd>zdrav&yacute;</trd>,
    <trd>zdravý <klr>(mající neporušené zdraví)</klr></trd>
  </trdgrp>
  <trd lng="de">gesund</trd>
  <trd lng="en">healthy</trd>
  <trdgrp lng="fr">
    <trd>sain</trd>,
    <trd>en bonne <ind>sant&eacute;</ind></trd>
  </trdgrp>
  <trd lng="he">&bet;&resh;&yod;&alef;</trd>
  <trd lng="hu">eg&eacute;szs&eacute;ges</trd>
  <trd lng="id">sehat</trd>
  <trd lng="it">sano</trd>
  <trd lng="nl">gezond</trd>
  <trd lng="pl">zdrowy</trd>
  <trdgrp lng="pt">
    <trd>s&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">zdravý</trd>
  <trdgrp lng="sv">
    <trd>frisk</trd>,
    <trd>vid god <ind>h&auml;lsa</ind></trd>
  </trdgrp>
  <trd lng="tr">sa&gbreve;l&inodot;kl&inodot;</trd>
  <trd lng="vo">saunik</trd>
</drv>

<drv mrk="san.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="san.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Stato de iu a&ubreve; io <tld/>a, en <ref tip="vid"
      cel="fart.bon0o">bonfarto</ref>:
      <ekz>
        <tld/>o estas pli valora ol oro;
      </ekz>
      <ekz>
        <tld/>osubtenanta ridado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        trinki pro ies <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;i estu por vi al <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_a;&c_r;&c_o;&c_w;&c_je;</trd>
  <trd lng="cs">zdraví</trd>
  <trd lng="de">Gesundheit</trd>
  <trd lng="en">health</trd>
  <trdgrp lng="fr">
    <trd>sant&eacute;</trd>
  </trdgrp>
  <trd lng="he">&bet;&resh;&yod;&alef;&vav;&tav;</trd>
  <trd lng="hu">eg&eacute;szs&eacute;g</trd>
  <trd lng="id">[<ind>sehat</ind>] kesehatan</trd>
  <trd lng="it">salute</trd>
  <trd lng="nl">gezondheid</trd>
  <trd lng="pl">zdrowie</trd>
  <trd lng="ru">&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>
  <trd lng="sk">zdravie</trd>
  <trd lng="sv">h&auml;lsa</trd>
  <trd lng="tr">sa&gbreve;l&inodot;k</trd>
  <trd lng="vo">saun</trd>
</drv>

<drv mrk="san.0iga">
  <kap><ofc>*</ofc><tld/>iga</kap>
  <snc mrk="san.0iga.MED">
    <dif>
      Favora por la <tld/>o, ta&ubreve;ga por subteni la bonfarton:
      <ekz>
        maldol&ccirc;a por la lango, sed <tld/>iga por la sango
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;oja koro estas <tld/>iga
        <fnt><bib>MT</bib>, <lok>&Sen; 17:22</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="higien.0a">higiena</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_z;&c_d;&c_a;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_ch;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_n;&c_y; &c_d;&c_l;&c_ja;
    &c_z;&c_d;&c_a;&c_r;&c_o;&c_w;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hojivý</trd>,
    <trd>léčivý</trd>,
    <trd>uzdravující</trd>
  </trdgrp>
  <trd lng="en">salubrious</trd>
  <trdgrp lng="fr">
    <trd>sain<klr> (favorable &agrave; la sant&eacute;)</klr></trd>,
    <trd>sanitaire</trd>,
    <trd>salubre</trd>    
  </trdgrp>
  <trdgrp lng="he">
    <trd>&bet;&resh;&yod;&alef;</trd>,
    <trd>&mem;&resh;&pe;&alef;</trd>
  </trdgrp>
  <trd lng="hu">eg&eacute;szs&eacute;ges<klr tip="ind"> (pl.
  &eacute;letm&oacute;d)</klr></trd>
  <trd lng="id">[<ind>sehat</ind>] menyehatkan</trd>
  <trdgrp lng="it">
    <trd>salutare</trd>,
    <trd>salubre</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gezond</trd>,
    <trd>heilzaam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>leczniczy</trd>,
    <trd>zdrowotny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja;
    &c_z;&c_d;&c_o;&c_r;&c_o;&c_v;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">prospešný zdraviu</trd>
  <trd lng="tr">sa&gbreve;l&inodot;&gbreve;a yararl&inodot;</trd>
</drv>

<drv mrk="san.0ilo">
  <kap><ofc>*</ofc><tld/>ilo<fnt>B</fnt></kap>
  <snc mrk="san.0ilo.MED">
    <ref tip="dif" cel="kurac.0ilo">kuracilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Medizin</trd>
  <trdgrp lng="en">
    <trd>cure</trd>,
    <trd>medicine</trd>,
    <trd>remedy</trd>
  </trdgrp>
  <trd lng="fr">rem&egrave;de</trd>
  <trd lng="he">&tav;&resh;&vav;&pe;&he;</trd>
  <trd lng="hu">gy&oacute;gy&aacute;szati eszk&ouml;z</trd>
  <trd lng="id">obat</trd>
  <trd lng="it">medicina<klr> (farmaco)</klr></trd>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekarstwo</trd>,
    <trd>medykament</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="tr">
    <trd>&ccedil;are</trd>,
    <trd>ila&ccedil;</trd>
  </trdgrp>
</drv>

<drv mrk="san.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="san.0igi.MED">
    <dif>
      Sukcese <ref tip="sin" cel="kurac.0i.MED">kuraci</ref>, meti iun
      en <tld/>an staton:
      <ekz>
        Li <tld/>igas la korprematojn kaj banda&gcirc;as iliajn vundojn
        <fnt><bib>MT</bib>, <lok>&Psa; 147:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>léčit</trd>,
    <trd>uzdravit</trd>,
    <trd>vyléčit</trd>,
    <trd>zahojit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>heilen</trd>,
    <trd>kurieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cure</trd>,
    <trd>heal</trd>,
    <trd>make well</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gu&eacute;rir<klr tip="amb"> (tr.)</klr></trd>,
    <trd>r&eacute;tablir<klr> (gu&eacute;rir)</klr></trd>
  </trdgrp>
  <trd lng="id">[<ind>sembuh</ind>] menyembuhkan</trd>
  <trdgrp lng="it">
    <trd>guarire<klr> (tr)</klr></trd>,
    <trd>sanare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>leczy&cacute;</trd>,
    <trd>uzdrawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>liečiť</trd>,
    <trd>uzdraviť</trd>,
    <trd>vyliečiť</trd>,
    <trd>zahojiť</trd>
  </trdgrp>
  <trd lng="tr">i&gbreve;le&scedil;tirmek</trd>
</drv>

<drv mrk="san.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="san.mal0a.MED">
    <dif>
      <klr>(Pri homo, besto a&ubreve; vegetalo)</klr> suferanta
      misfunkciadon de siaj organoj:
      <ekz>
        li estas morte mal<tld/>a, li ne vivos pli, ol unu
        tagon
        <fnt><bib>F</bib>, <lok>&FE;31.</lok></fnt>;
      </ekz>
      <ekz>
        venigu la kuraciston, &ccirc;ar mi estas mal<tld/>a
        <fnt><bib>F</bib>, <lok>&FE;39.</lok></fnt>;
      </ekz>
      <ekz>
        <ind>cerbomal<tld/>a</ind>;
        <trdgrp lng="hu">
          <trd>elmebeteg</trd>,
          <trd>hibbant</trd>,
          <trd>eszel&odblac;s</trd>
        </trdgrp>
      </ekz>
      <ekz>
        mal<tld/>eca paleco;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu okazo mal<tld/>igis nian financan staton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_r;&c_y;</trd>
  <trd lng="bg">&c_b;&c_o;&c_l;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>marod (hovor.)</trd>,
    <trd>nemocný <klr>(čl.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungesund</trd>,
    <trd>krank</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>unhealthy</trd>,
    <trd>ill</trd>,
    <trd>sick</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>malade<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&het;&vav;&lamed;&he;</trd>
  <trd lng="hu">beteg</trd>
  <trd lng="id">sakit</trd>
  <trdgrp lng="it">
    <trd>malato<klr> (agg.)</klr></trd>,
    <trd>infermo<klr> (agg.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ziek</trd>,
    <trd>ongezond</trd>
  </trdgrp>
  <trd lng="pl">chory</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">chorý</trd>
  <trd lng="tp">ike</trd>
  <trd lng="tr">hasta</trd>
</drv>

<drv mrk="san.mal0o">
  <kap><ofc>*</ofc>mal<tld/>o</kap>
  <tezrad fak="MED"/>
  <uzo tip="fak">MED</uzo>
  <snc mrk="san.mal0o.MED">
    <dif>
      Stato de iu a&ubreve; io mal<tld/>a; malbonfarto:
      <ekz>
        post infekta mal<tld/>o oni ofte bruligas la
        vestojn de la mal<tld/>ulo
      </ekz>
      <ekz>
        <fnt><bib>F</bib>, <lok>&FE;39</lok></fnt>;
        mal<tld/>o de gor&gcirc;o;
      </ekz>
      <ekz>
        Li pardonas &ccirc;iujn viajn pekojn, Li <tld/>igas &ccirc;iujn
        viajn mal<tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Psa; 103:3</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iutage homoj kolapsas pro kormal<tld/>o
        <fnt>
        <aut>J. Ipfelkofer</aut>,
        <vrk>Esploroj kortu&scirc;aj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 9a, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:malsanoj">malsanoj</lstref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>choroba</trd>,
    <trd>nemoc</trd>
  </trdgrp>
  <trd lng="de">Krankheit</trd>
  <trdgrp lng="en">
    <trd>disease</trd>,
    <trd>sickness</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>maladie</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&het;&lamed;&he;</trd>,
    <trd>&het;&vav;&lamed;&yod;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>betegs&eacute;g</trd>,
    <trd>k&oacute;r</trd>
  </trdgrp>
  <trd lng="id">[<ind>sakit</ind>] penyakit</trd>
  <trdgrp lng="it">
    <trd>malattia</trd>,
    <trd>malanno</trd>, <trd>morbo</trd>
  </trdgrp>
  <trd lng="nl">ziekte</trd>
  <trd lng="pl">choroba</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>choroba</trd>,
    <trd>neduh</trd>,
    <trd>nemoc</trd>
  </trdgrp>
  <trd lng="tr">hastal&inodot;k</trd>
</drv>

<drv mrk="san.mal0ema">
  <kap><ofc>*</ofc>mal<tld/>ema</kap>
  <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>
  <snc>
    <dif>
      Kiu ofte kaj facile mal<tld/>i&gcirc;as:
      <ekz>
        mal<tld/>ema korpo bezonas nur malgrandan pu&scirc;on de ekstere
        por fali en mal<tld/>on
        <fnt>
        <aut>Platono, trad. D. Broadribb</aut>,
        <vrk>La Respubliko</vrk>,
        <lok>parto 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_v;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_t;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_je;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ailing</trd>,
    <trd>sickly</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fragile de sant&eacute;</trd>,
    <trd>maladif</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>sakit</ind>] berpenyakit</trd>,
    <trd><ind>sakit</ind>-sakitan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cagionevole</trd>,
    <trd>malaticcio</trd>
  </trdgrp>
  <trd lng="nl">ziekelijk</trd>
  <trd lng="pl">chorowity</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>chorľavý</trd>,
    <trd>neduživý</trd>
  </trdgrp>
  <trd lng="tr">sa&gbreve;l&inodot;&gbreve;&inodot; hasas</trd>
</drv>

<drv mrk="san.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="san.mal0igxi.MED">
    <dif>
      Eksperti korpan a&ubreve; spiritan misfunkciadon:
      <ekz>
        de rigardo tro alta mal<tld/>i&gcirc;as okulo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>i&gcirc;as pro kancero ankora&ubreve; triono de la
        viroj kaj kvarono de la virinoj anta&ubreve; la 75a jaro
        <fnt>
        <aut>E. Georgiev</aut>,
        <vrk>Pli maljunaj, pli sa&gcirc;aj, pli flekseblaj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 3a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_v;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trd lng="de">krank werden</trd>
  <trdgrp lng="en">
    <trd>fall ill</trd>,
    <trd>sicken</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tomber <ind>malade</ind></trd>
  </trdgrp>
  <trd lng="id"><ind>jatuh</ind> sakit</trd>
  <trd lng="it">ammalarsi</trd>
  <trd lng="nl"><ind>ziek</ind> worden</trd>
  <trd lng="pl">zachorowa&cacute;</trd>
  <trd lng="ru">&c_z;&c_a;&c_b;&c_o;&c_l;&c_je;&c_t;&c_mol;</trd>
  <trd lng="tr">hastalanmak</trd>
</drv>

<drv mrk="san.mal0ulejo">
  <kap><ofc>*</ofc>mal<tld/>ulejo</kap>
  <snc mrk="san.mal0ulejo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Granda, ordinare publika kuracejo kun ekipa&jcirc;oj kaj oficantaj
      medicinistoj:
      <ekz>
        la konstruado de lernejoj, mal<tld/>ulejoj kaj pontoj
        kondukis al la estigo de modesta bonstato
        <fnt>
        <aut>E. Georgiev</aut>,
        <vrk>Sir Edmund Hillary</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 10a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
