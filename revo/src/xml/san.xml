  <trd lng="sk">nemocnica</trd>
  <trd lng="tr">hastahane</trd>
</drv>

<drv mrk="san.re0igi">
  <kap><ofc>*</ofc>re<tld/>igi</kap>
  <snc mrk="san.re0igi.MED">
    <dif>
      Redoni la <tld/>on al iu, sukcese <ref tip="sin"
      cel="kurac.0i.MED">kuraci</ref>:
      <ekz>
        la Eternulo banda&gcirc;os la difektojn de sia popolo kaj
        re<tld/>igos &gcirc;iajn vundojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>nere<tld/>igebla</ind> vundo<fnt>Z</fnt>.
        <trd lng="hu">gy&oacute;gy&iacute;thatatlan</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>léčit</trd>,
    <trd>uzdravit</trd>,
    <trd>vyléčit</trd>,
    <trd>zahojit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cure</trd>,
    <trd>heal</trd>,
    <trd>make well</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gu&eacute;rir<klr> (tr.)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&resh;&pe;&alef;</ind></trd>
  <trd lng="hu">meggy&oacute;gy&iacute;t</trd>
  <trd lng="id">[<ind>sembuh</ind>] menyembuhkan</trd>
  <trdgrp lng="it">
    <trd>guarire<klr> (tr.)</klr></trd>,
    <trd>risanare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>genezen<klr tip="ind"> (tr.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="pl">
    <trd>leczy&cacute;</trd>,
    <trd>uzdrawia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>liečiť</trd>,
    <trd>uzdraviť</trd>,
    <trd>vyliečiť</trd>,
    <trd>zahojiť</trd>
  </trdgrp>
  <trd lng="tr">iyle&scedil;tirmek</trd>
</drv>

