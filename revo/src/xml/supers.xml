<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: supers.xml,v 1.18 2015/09/21 06:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>supersti&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="supers.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="supers.0o.KOMUNE">
    <dif>
      Malsa&gcirc;a kredo je mistera kvaza&ubreve;sor&ccirc;a influo de diversaj
      objektoj, agoj a&ubreve; paroloj sur la okazonta&jcirc;oj:
        <ekz>
	mi volas rakonti pri kelkaj <tld/>oj de mia tribo
	<fnt>
          <aut>&Ograve;.-D. Ol&uacute;w&agrave;b&ugrave;nmi</aut>,
  	  <vrk>Supersti&ccirc;a matematiko</vrk>,
          <lok>Kontakto, 2002:188, p. 13a</lok>
        </fnt>.
	</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_a;&c_b;&c_o;&c_n;</trd>
  <trd lng="bg">&c_s;&c_u;&c_je;&c_v;&c_je;&c_r;&c_i;&c_je;</trd>
  <trd lng="ca">superstici&oacute;</trd>
  <trd lng="de">Aberglaube</trd>
  <trd lng="en">superstition</trd>
  <trd lng="es">superstici&oacute;n</trd>
  <trd lng="fr">superstition</trd>
  <trd lng="hu">babona</trd>
  <trd lng="id">takhayul</trd>
  <trd lng="it">superstizione</trd>
  <trd lng="nl">bijgeloof</trd>
  <trdgrp lng="pl">
    <trd>przes&aogonek;d</trd>,
    <trd>zabobon</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>supersti&ccedil;&atilde;o</trd>
    <trd>crendice</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_je;&c_v;&c_je;&c_r;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_r;&c_a;&c_s;&c_s;&c_u;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="supers.0a">
  <kap><tld/>a</kap>
  <snc mrk="supers.0a.iu">
    <dif>
      Havanta <tld/>on:
      <ekz>
        dum infana&gcirc;o anka&ubreve; li estis <tld/>a
        <fnt>
          <aut>M. de Assis, trad. Paulo Viana</aut>,
          <vrk>La divenistino kaj aliaj rakontoj</vrk>,
          <lok>Oportuno, 2005, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">superstitious</trd>
  <trd lng="bg">&c_s;&c_u;&c_je;&c_v;&c_je;&c_r;&c_je;&c_n;</trd>
  <trd lng="de">abergl&auml;ubisch</trd>
  <trd lng="fr">superstitieux</trd>
  <trd lng="it">superstizioso</trd>
  <trdgrp lng="pl">
    <trd>przes&aogonek;dny</trd>,
    <trd>zabobonny</trd>
  </trdgrp>
  <trd lng="pt">supersticioso<klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="supers.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="supers.0ulo.KOMUNE">
    <dif>
      <tld lit="S"/>a homo, facile kredanta je
      nevideblaj influoj, je magiaj efikoj:
      <ekz>
        tiu, kiu vidas tutan lumon <klr>...</klr>
        ne malestimu <tld/>ulon
        <fnt>
          <aut>L. Tolstoj, trad. A. &Scirc;aparov</aut>,
          <vrk>Dio &ndash; unu por &ccirc;iuj</vrk>,
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">superstitious person</trd>
  <trd lng="fr">personne <ind>supersticieuse</ind></trd>
  <trdgrp lng="pl">
    <trd>osoba <ind>przes&aogonek;dna</ind></trd>,
    <trd>osoba <ind>zabobonna</ind></trd>
  </trdgrp>
</drv>
</art>
