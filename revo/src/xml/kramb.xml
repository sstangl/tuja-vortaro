<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kramb.xml,v 1.14 2013/11/02 12:10:28 revo Exp $">
<kap>
  <rad>kramb</rad>/o
</kap>
<drv mrk="kramb.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Plurjara <ref tip="lst" cel="herb.0o.BOT" 
      lst="voko:brasikoparencoj">herbo</ref>
      kun longa <ref tip="prt" cel="bet.0o.radiko">beto</ref>,
      parenca al brasiko, sed kun blankaj floretoj kaj
      dupartaj silikvoj kun po unu globforma semgrajno;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Crambe</trd>).
    </dif>
  </snc>
  <trd lng="de">Meerkohl</trd>
  <trd lng="en">sea kale</trd>
  <trd lng="fa">&a_s;&f_p;&f_y;&a_d;&a_h;</trd>
  <trd lng="hu">t&aacute;torj&aacute;n</trd>
  <trdgrp lng="pt">
    <trd>crambe</trd>,
    <trd>couve-marinha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_r;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
