<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: murmur.xml,v 1.21 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>murmur</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="murmur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="murmur.0i.brui">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte,
      malklare,
      obtuze, kiel fluanta akvo sur &scirc;tonoj, vento tra foliaro,
      amaso da
      homoj samtempe parolantaj:
      <ekz>
        la rivereto, la vento <tld/>as;
      </ekz><ekz>
        la birdoj gaje <tld/>as sur arboj;
      </ekz><ekz>
        akvo <tld/>etas en la bolkru&ccirc;o;
      </ekz><ekz>
        eksonis <tld/>o de pa&scirc;oj<fnt>B</fnt>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>bruire</trd>,
      <trd>bruisser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mormol</trd>,
      <trd>z&uacute;g</trd>,
      <trd>csobog</trd>,
      <trd>cs&ouml;rgedezik</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>murmurar</trd>,
      <trd>farfalhar <klr tip="amb">(folhas)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_u;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="murmur.0i.paroleti">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref tip="super" cel="parol.0i.vocxi">Paroli</ref> malla&ubreve;te:
      <ekz>
        <tld/>i minacojn;
      </ekz><ekz>
        <tld/>eti al ies orelo<fnt>B</fnt>;
      </ekz><ekz>
        post longa momento &scirc;i <tld/>is: &leftquot;Vi jam estas
        dudek-sesjara...&rightquot;
        <fnt>
          <aut>Shi Chengtai</aut>,
          <vrk>La kompensa kiso</vrk>,
          <bib>Monato</bib>,
          <lok>1995:6, p. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flustr.0i">flustri</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>murmurer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mormol</trd>,
      <trd>suttog</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="murmur.0i.protesti">
    <uzo tip="klr">(abs.)</uzo>
    <ref tip="dif" cel="grumbl.0i">grumbli</ref>
    <ekz>
      <tld/>i tra la dentoj
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      ili ek<tld/>is kontra&ubreve; Moseo<fnt>Z</fnt>;
    </ekz><ekz>
      sur la ventro veluro, en la ventro <tld/>o
      <klr>(ekstere &scirc;ajnas luksa sed efektive estas
      malprospera)</klr>
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trdgrp lng="fr">
      <trd>bougonner</trd>,
      <trd>grommeler</trd>,
      <trd>grogner<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
      <trd>r&acirc;ler<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
      <trd>r&eacute;criminer</trd>,
      <trd>ronchonner</trd>,
      <trd>rousp&eacute;ter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&ouml;rm&ouml;g</trd>,
      <trd>morog</trd>
    </trdgrp>
    <trd lng="pt">resmungar</trd>
    <trd lng="ru">&c_r;&c_o;&c_p;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bručet</trd>,
    <trd>bublat</trd>,
    <trd>hučet</trd>,
    <trd>mručet</trd>,
    <trd>skuhrat</trd>,
    <trd>šumět</trd>
  </trdgrp>
  <trd lng="de">murmeln</trd>
  <trd lng="en">murmur</trd>
  <trdgrp lng="nl">
    <trd>murmelen</trd>,
    <trd>brommen</trd>,
    <trd>morren</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hundrať; žblnkotať; štebotať</trd>,
    <trd>mrmlať</trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0o">
  <kap><tld/>o</kap>
  <snc mrk="murmur.0o.SON">
    <dif>
      Malla&ubreve;ta <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>
      de io a&ubreve; iu obtuze bruanta:
      <ekz>
        lia akra a&ubreve;do perceptis venantan de sube, <klr>...</klr>
        obtuzan
        <tld/>on
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_m;&c_y;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_u;&c_m;</trd>,
    <trd>&c_sh;&c_o;&c_l;&c_a;&c_h;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_g;&c_u;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bublání</trd>,
    <trd>hučení</trd>,
    <trd>mručení</trd>,
    <trd>obtěžující hluk</trd>,
    <trd>skuhrání</trd>,
    <trd>šumění</trd>
  </trdgrp>
  <trd lng="de">Murmeln</trd>
  <trd lng="en">murmur</trd>
  <trdgrp lng="fr">
    <trd>bougonnement</trd>,
    <trd>murmure</trd>,
    <trd>ronchonnement</trd>,
    <trd>rumeur<klr tip="amb"> (bruit confus)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mormol&aacute;s</trd>,
    <trd>csobog&aacute;s</trd>,
    <trd>z&uacute;g&aacute;s</trd>,
    <trd>cs&ouml;rgedez&eacute;s</trd>,
    <trd>morg&aacute;s</trd>,
    <trd>d&ouml;rm&ouml;g&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_o;&c_h;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_o;&c_p;&c_o;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_m;&c_o;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_g;&c_u;&c_l; &c_g;&c_o;&c_l;&c_o;&c_s;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bručanie <klr>(nespokojné)</klr></trd>,
    <trd>mručanie</trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="murmur.0egi.SON">
    <dif>
      Tre la&ubreve;te, obtuze a&ubreve; konfuze <ref tip="super"
        cel="bru.0i.soni">brui</ref> a&ubreve; <ref tip="super"
        cel="blek.0i.beste">bleki</ref>:
      <ekz>
        la leono ek<tld/>egis kaj foriris
        <fnt>
          <bib>FK</bib>,
          <lok>La deveno de la virino</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">silně bručet</trd>
  <trd lng="en">growl</trd>
  <trdgrp lng="fr">
    <trd>gronder<klr tip="amb"> (bruit)</klr></trd>,
    <trd>rugir</trd>,
    <trd>faire du <ind>vacarme</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">silno bručať</trd>
</drv>

<drv mrk="murmur.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="murmur.0ego.SON">
    <dif>
      Troa, obtuza bruego:
      <ekz>
        la <tld/>ego de la intermiksitaj paroloj surdigis &scirc;in,
        la mallumo sur la trotuaroj &scirc;in timigis
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">growl</trd>
  <trdgrp lng="fr">
    <trd>brouhaha</trd>,
    <trd>rugissement</trd>,
    <trd>vacarme</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_u;&c_h;&c_o;&c_j; <ind>&c_sh;&c_u;&c_m;</ind></trd>,
    <trd>&c_g;&c_u;&c_l;</trd>,
    <trd>&c_g;&c_o;&c_m;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0eti">
  <kap><tld/>eti</kap>
  <snc mrk="murmur.0eti.SON">
    <dif>
      Malla&ubreve;te, delikate <ref tip="super"
      cel="bru.0i.soni">brui</ref>:
      <ekz>
        li sin turnis sur la kalkano, stari&gcirc;is anta&ubreve; la spegulo,
        &gcirc;ustigis la kravaton kaj frizon, ek<tld/>etis kanteton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="zum.0i">zumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>broukat</trd>,
    <trd>mumlat</trd>,
    <trd>vrnět</trd>,
    <trd>ševelit</trd>
  </trdgrp>
  <trd lng="en">murmur softly</trd>
  <trdgrp lng="fr">
    <trd>bourdonner</trd>,
    <trd>fredonner</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_sh;&c_jo;&c_p;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_je;&c_v;&c_a;&c_t;&c_mol;<klr>
(&c_v;&c_p;&c_o;&c_l;&c_g;&c_o;&c_l;&c_o;&c_s;&c_a;)</klr></trd>,
    <trd>&c_m;&c_u;&c_r;&c_l;&c_y;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mrmlať</trd>,
    <trd>ševeliť</trd>
  </trdgrp>
</drv>

<drv mrk="murmur.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="murmur.0eto.SON">
    <dif>
      Malla&ubreve;ta, neklara brueto:
      <ekz>
        <tld/>eto de admiro traflugis inter la &ccirc;eestantoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">soft murmur</trd>
  <trdgrp lng="fr">
    <trd>bourdonnement</trd>,
    <trd>bruissement</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_r;&c_o;&c_h;</trd>,
    <trd>&c_sh;&c_o;&c_p;&c_o;&c_t;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="murmur.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="murmur.sen0e.KOMUNE">
    <dif>
      Ne <tld/>ante, ne kontra&ubreve;ante,
      senproteste:
      <ekz>
        la popolo, tiom subpremata per laboro
        <klr>...</klr> sen<tld/>e obeas al ili
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url
         ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>sans mot dire</trd>,
    <trd>sans regimber</trd>
  </trdgrp>
</drv>
</art>
