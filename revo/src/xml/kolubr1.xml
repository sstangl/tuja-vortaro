<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolubr1.xml,v 1.12 2013/08/31 15:10:24 revo Exp $">
<kap>
  <rad>kolubred</rad>/oj
</kap>
<drv mrk="kolubr1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Colubridae</trd>), al kiu apartenas la plej
      multaj serpentoj, normale nedan&gcirc;eraj al la homo.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_zh;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">col&uacute;brids </trd>
  <trd lng="de">Nattern</trd>
  <trd lng="fr">colubrid&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>colubrides</trd>,
    <trd>gladde <ind>slangen</ind></trd>,
    <trd>ringslangachtigen</trd>
  </trdgrp>
  <trd lng="pl">po&lstroke;ozowate</trd>
  <trd lng="pt">colubr&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_u;&c_zh;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>
