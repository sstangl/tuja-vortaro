<kap>
    <rad>artroz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="artroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Neinflama degenera kronika afekcio de la <ref
      cel="artik.0o">artikoj</ref>:
      <ekz>
        <tld lit="A"/>o estas neinflama degenera procezo de artiko
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">artr贸za</trd>
  <trd lng="en">arthrosis</trd>
  <trd lng="fr">arthrose</trd>
  <trd lng="nl">artrose</trd>
  <trd lng="no">artrose</trd>
  <trd lng="sk">artr贸za</trd>
</drv>

<drv mrk="artroz.genu0o">
  <kap>genu<tld/>o</kap>
  <snc mrk="artroz.genu0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> de la genua
      artiko:
      <ekz>
         La <tld/>o de la kokso (koks<tld/>o) estas malpli ofta ol tiu
         de la genuo (genu<tld/>o)
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">gonarthrosis</trd>
  <trd lng="fr">gonarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de knie</trd>,
    <trd>gonartrose</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>artrose i kne</trd>,
    <trd>gonartrose</trd>
  </trdgrp>
</drv>

