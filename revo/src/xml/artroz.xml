<drv mrk="artroz.lumb0o">
  <kap>lumb<tld/>o</kap>
  <snc mrk="artroz.lumb0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="artroz.0o">Artrozo</ref> &ccirc;e la lumba
      vertebraro:
      <ekz>
         La vertebraro estas anka&ubreve; ofta loko trafita de la <tld/>o, &ccirc;efe
         &ccirc;e la nuka kaj lumba eta&gcirc;oj (nuk<tld/>o, lumb<tld/>o).
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Artrozo">Artrozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">lumbar arthrosis</trd>
  <trd lng="fr">lombarthrose</trd>
  <trdgrp lng="nl">
    <trd>artrose van de lendenwervels</trd>,
    <trd>lumbartrose</trd>
  </trdgrp>
  <trd lng="no">artrose i korsryggen</trd>
</drv>

