<kap>
  <rad>agatid</rad>/o <fnt><bib>PIV2</bib></fnt> 
</kap>
<drv mrk="agatid.0o">
  <kap><tld/>o</kap>
  <snc mrk="agatid.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda granda arbo el <ref tip="lst" cel="konifer.0o" 
      lst="voko:koniferoj">koniferoj</ref>, kun
      lar&gcirc;aj ledecaj folioj,
      kreskanta en tropikaj landoj de 
      Okcidenta Pacifiko;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" 
      cel="arauxk1.0oj.BOT">ara&ubreve;kariacoj</ref>
      (<trd lng="la">Agathis</trd>).
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>damara</trd>,
    <trd>jedle moluck√°</trd>
  </trdgrp>
  <trd lng="de">Kauribaum</trd>
  <trd lng="fr">agathis</trd>
  <trd lng="id">damar</trd>
  <trd lng="sk">damara</trd>
</drv>
</art>

