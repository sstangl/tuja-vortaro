<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mula.xml,v 1.8 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mula</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mula.0o">
  <kap><tld/>o</kap>
  <snc mrk="mula.0o.ISL">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">ISL</uzo>
    <dif>
      Diservanto &ccirc;e <ref tip="vid" cel="islam.0ano">islamanoj</ref>,
      scianto de <ref tip="vid" cel="koran.0o">Korano</ref> kaj
      de religiaj ritoj, ordinare elektata de kredantoj el si.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_a;</trd>
  <trd lng="cs">mullah</trd>
  <trd lng="de">Mullah</trd>
  <trd lng="en">mullah</trd>
  <trdgrp lng="fr">
    <trd>mollah</trd>,
    <trd>mullah</trd>
  </trdgrp>
  <trd lng="hu">mullah</trd>
  <trd lng="pt">mul&aacute;</trd>
  <trd lng="ru">&c_m;&c_u;&c_l;&c_l;&c_a;</trd>
  <trd lng="sk">mullah</trd>
  <trdgrp lng="sv">
    <trd>mulla</trd>,
    <trd>molla</trd>
  </trdgrp>
</drv>
</art>
