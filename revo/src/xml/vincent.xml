<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vincent.xml,v 1.6 2015/02/26 19:11:57 revo Exp $">
<kap>
  <rad>Vincent</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vincent.0o">
  <kap><tld/>o</kap>
  <snc mrk="vincent.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        la glora hispana predikisto, <tld/>o Ferrier
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>Vicen&ccedil;</trd>,
    <trd>Vicent</trd>
  </trdgrp>
  <trd lng="de">Vincent</trd>
  <trd lng="fr">Vincent</trd>
  <trd lng="hu">Vince</trd>
  <trd lng="it">Vincenzo</trd>
</drv>
</art>

