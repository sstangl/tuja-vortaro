<art mrk="$Id: svastik.xml,v 1.7 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>svastik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="svastik.0o">
  <kap><tld/>o</kap>
  <snc mrk="svastik.0o.FORM">
    <ref tip="dif" cel="kruc.hoko0o.FORM">hokokruco</ref>
      <ekz>
        ortodoksaj oraj krucoj, /
        ru&gcirc;aj steloj kaj <tld/>oj /
        buntas, kiel farbaj &scirc;prucoj, /
        sur &ccirc;iel&#8217; de politiko
        <fnt>
          <aut>M. Gi&scirc;pling</aut>,
          <vrk>Elektonte</vrk>,
          <lok><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/ckk/ckk9912.htm">
          Cerbe kaj kore, 1999-12</url></lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_s;&c_v;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>creu gammada</trd>,
    <trd>esv&agrave;stica</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hákový kříž</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trd lng="de">Swastika</trd>
  <trdgrp lng="es">
    <trd>esv&aacute;stica</trd>,
    <trd>cruz gamada</trd>
  </trdgrp>
  <trd lng="it">svastica</trd>
  <trdgrp lng="sk">
    <trd>hákový kríž</trd>,
    <trd>svastika</trd>
  </trdgrp>
</drv>
</art>

