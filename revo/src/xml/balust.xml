<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: balust.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <rad>balustr</rad>/o
    <fnt>V</fnt>
  </kap>
  <drv mrk="balust.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">ARKI</uzo>
    <snc>
    <dif>
      Ronda, kvadrata a&ubreve; diversforma koloneto ordinare
      vazoforma en sia  mezo.
    </dif>
    </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_s;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>balustr</trd>,
    <trd>balustra</trd>,
    <trd>kuželka</trd>,
    <trd>tyčka zábradlí</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Baluster</trd>,
      <trd>kleine S&auml;ule</trd></trdgrp>
  <trd lng="en">baluster</trd>
  <trd lng="fr">balustre</trd>
  <trdgrp lng="hu">
      <trd>mellv&eacute;db&aacute;b</trd>,
      <trd>korl&aacute;tb&aacute;b</trd>,
      <trd>baluszter</trd>
    </trdgrp>
  <trd lng="pl">balasek</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_ja;&c_s;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>stĺpik rôzneho priemeru</trd>,
    <trd>uprostred vázovitý</trd>
  </trdgrp>
</drv>
  <drv mrk="balust.0aro">
    <kap><tld/>aro</kap>
    <snc>
    <dif>
      Vico da <tld/>oj.
    </dif>
    <ref tip="super" cel="balust1.0o">balustrado.</ref>
    </snc>
    <trd
    lng="be">&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Balustrade</trd>,
      <trd>Br&uuml;stung</trd>,
      <trd>S&auml;ulengel&auml;nder</trd>
    </trdgrp>
    <trd lng="en">balustrade</trd>
    <trd lng="fr">balustrade</trd>
    <trdgrp lng="hu">
      <trd>mellv&eacute;d</trd>,
      <trd>balusztr&aacute;d</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>balaski</trd>,
      <trd>balustrada</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_i;&c_l;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_p;&c_je;&c_t;</trd>
    </trdgrp>
  </drv>
</art>
