<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: helebo.xml,v 1.20 2015/10/16 05:10:15 revo Exp $">
<kap>
<rad>helebor</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="helebo.0o">
  <kap>
  <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Helleborus</trd>), ornama planto, kies radiko estas
      venena.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_m;&c_je;&c_r;</trd>
  <trd lng="de">Nieswurz</trd>
  <trdgrp lng="en">
    <trd>Helleborus</trd>,
    <trd>hellebore</trd>, <trd>Lenten rose</trd>
  </trdgrp>
  <trd lng="fr">hell&eacute;bore</trd>
  <trd lng="hu">hunyor</trd>
  <trd lng="it">elleboro</trd>
  <trd lng="nl">nieskruid</trd>
  <trd lng="pl">ciemiernik</trd>
  <trd lng="pt">hel&eacute;boro</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="helebo.nigra0o">
  <kap>
  nigra <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="helebo.0o"><tld/>o</ref>
      (<trd lng="la">Helleborus niger</trd>).
    </dif>
  </snc>
  <bld lok="../bld/helebor1.jpg">folioj kaj floro de <tld/>o</bld>
  <bld lok="../bld/helebor2.jpg">floro de <tld/>o</bld>
  <trdgrp lng="de">
    <trd>Schwarze Nieswurz</trd>,
    <trd>Schneerose</trd>,
    <trd>Christrose</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>black hellebore</trd>, <trd>Christmas
  rose</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>hell&eacute;bore noir</trd>,
    <trd>rose de No&euml;l</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>feh&eacute;r <ind>hunyor</ind></trd>,
    <trd>fekete <ind>hunyor</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>elleboro<klr> nero</klr></trd>,
    <trd>erba rocca</trd>,
    <trd>rosa<klr> di Natale</klr></trd>
  </trdgrp>
  <trd lng="nl">kerstroos</trd>
</drv>
</art>
