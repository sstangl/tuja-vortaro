<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amput.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>amput</rad>/i
</kap>
<drv mrk="amput.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amput.0i.KIR">
    <uzo tip="fak">KIR</uzo>
    <dif>
      Detran&ccirc;i membron, organon de vivanta esta&jcirc;o:
      <ekz>
        kruelaj punoj samkiel <tld/>i, vipi, &scirc;tonumi<fnt><aut>Morteza
        Mirbaghian</aut>: <vrk>Islamo, &ccirc;u religio de paco?</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">amputar</trd>
  <trdgrp lng="cs">
    <trd>amputovat</trd>,
    <trd>odejmout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>amputieren</trd>,
    <trd>abschneiden</trd>,
    <trd>abnehmen</trd>
  </trdgrp>
  <trd lng="en">amputate</trd>
  <trd lng="es">amputar</trd>
  <trd lng="fr">amputer</trd>
  <trd lng="nl">amputeren</trd>
  <trd lng="pt">amputar</trd>
  <trd lng="ru">&c_a;&c_m;&c_p;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">amputova&tcaron;</trd>
</drv>
</art>
