<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: epolet.xml,v 1.19 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>epolet</rad>/o
</kap>
<drv mrk="epolet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="epolet.0o.TEKS">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Militista sur&scirc;ultra ornama&jcirc;o, ordinare <ref tip="super"
      cel="insign.0o.signo">insigno</ref> montranta la rangon:
      <ekz>
        balda&ubreve; rekoninte armeajn <tld/>ojn, ili forturnis sin kun
        indigno
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_g;&c_o;&c_n;</trd>,
    <trd>&c_e;&c_p;&c_a;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>epoleta</trd>,
    <trd>nárameník</trd>,
    <trd>výložka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Achselst&uuml;ck</trd>,
    <trd>Schulterst&uuml;ck</trd>,
    <trd>Schulterklappe</trd>,
    <trd>Epaulette</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>epaulet</trd>,
    <trd>epaulette</trd>
  </trdgrp>
  <trd lng="es">divisa<klr> (insignia militar indicando
  rango)</klr></trd>
  <trd lng="fr">&eacute;paulette</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;llrojt</trd>,
    <trd>v&aacute;llbojt</trd>,
    <trd>epolett</trd>
  </trdgrp>
  <trd lng="it">spallina<klr> (mil.)</klr></trd>
  <trdgrp lng="nl">
    <trd>epaulet</trd>,
    <trd>schouderstuk<klr tip="ind"> (mil.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pagon</trd>,
    <trd>epolet</trd>
  </trdgrp>
  <trd lng="pt">dragona</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_o;&c_n;</trd>,
    <trd>&c_e;&c_p;&c_o;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>náplecník</trd>,
    <trd>výložka</trd>
  </trdgrp>
  <trd lng="sv">axelklaff</trd>
</drv>
</art>
