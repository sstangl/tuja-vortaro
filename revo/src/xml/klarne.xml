<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klarne.xml,v 1.19 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klarnet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klarne.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klarne.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika ligna <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref> kun beko,
      langeto kaj
      truoj a&ubreve; klavoj:
      <ekz>
        ni tiam e&ccirc; dancu la&ubreve; <tld/>tonoj de tajloro Andreo
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url
          ref="http://www.gutenberg.org/files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_r;&c_n;&c_e;&c_t;</trd>
  <trd lng="ca">clarinet</trd>
  <trd lng="cs">klarinet</trd>
  <trd lng="de">Klarinette</trd>
  <trd lng="en">clarinet</trd>
  <trd lng="es">clarinete</trd>
  <trd lng="fr">clarinette</trd>
  <trd lng="id">klarinet</trd>
  <trd lng="nl">klarinet</trd>
  <trd lng="pl">klarnet</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_r;&c_n;&c_je;&c_t;</trd>
  <trd lng="sk">klarinet</trd>
</drv>
</art>
