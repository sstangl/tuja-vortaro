<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: latron.xml,v 1.10 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>latron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="latron.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <ref tip="dif" cel="rab.0isto">rabisto</ref>
    <ekz>
      la <tld/>oj prirabis kaj murdis lin kaj poste lin enterigis en &ccirc;i
      loko tiel dezerta<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 23a, p.
      165a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">lladre</trd>
  <trdgrp lng="cs">
    <trd>kriminálník</trd>,
    <trd>lotr</trd>,
    <trd>zloděj</trd>
  </trdgrp>
  <trd lng="de">R&auml;uber</trd>
  <trd lng="es">ladr&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>brigand</trd>,
    <trd>larron</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lator</trd>,
    <trd>&uacute;ton&aacute;ll&oacute;</trd>
  </trdgrp>
  <trd lng="nl">rover<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pt">
    <trd>ladr&atilde;o</trd>,
    <trd>salteador</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>kriminálnik</trd>,
    <trd>lotor</trd>
  </trdgrp>
</drv>
</art>

