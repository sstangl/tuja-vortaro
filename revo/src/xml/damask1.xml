<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: damask1.xml,v 1.14 2015/07/04 08:37:32 revo Exp $">
<kap>
<rad>damasken</rad>/i
</kap>
<drv mrk="damask1.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Inkrusti orajn a&ubreve; ar&gcirc;entajn fadenojn en metalon:
      <ekz><tld/>ita klingo, &scirc;talo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;
  &c_z;&c_a;&c_l;&c_a;&c_t;&c_y;&c_m;&c_ib; &c_a;&c_b;&c_o;
  &c_s;&c_r;&c_e;&c_b;&c_n;&c_y;&c_m;&c_ib;
  &c_n;&c_ib;&c_t;&c_k;&c_a;&c_m;&c_ib;</trd>
  <trd lng="ca">damasquinar</trd>
  <trdgrp lng="cs">
    <trd>damaskovat</trd>,
    <trd>vyrábět damaskenskou ocel</trd>
  </trdgrp>
  <trd lng="de">damaszieren</trd>
  <trd lng="en">damascene</trd>
  <trd lng="fr">damasquiner</trd>
  <trd lng="hu">damaszkol</trd>
  <trdgrp lng="pl">
    <trd>damaskowa&cacute;</trd>,
    <trd>dziwerowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tauxiar</trd>,
    <trd>damasquiar</trd>
  </trdgrp>
  <trd lng="sk">vyrábať damascénsku oceľ</trd>
</drv>
</art>
