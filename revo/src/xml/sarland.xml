<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sarland.xml,v 1.8 2014/10/11 11:10:27 revo Exp $">
<kap>
  <rad>Sarland</rad>/o
</kap>
<drv mrk="sarland.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>Lkn</bib></fnt>
  <snc>
    <dif>
      Unu el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta 
      en la sudokcidenta parto inter <ref tip="vid"
      cel="franc.F0ujo">Francujo</ref> kaj
      Rejnland-Palatinato. Regurbo kaj plej granda urbo estas Sarbruko.
    </dif>
    <trd lng="bg">&c_S;&c_a;&c_a;&c_r;&c_l;&c_a;&c_n;&c_d;</trd>
    <trd lng="ca">Saarland</trd>
    <trd lng="de">Saarland</trd>
    <trd lng="es">Sarre</trd>
    <trd lng="fr">Sarre<klr> (&Eacute;tat)</klr></trd>
    <trd lng="hu">Saar-vid&eacute;k</trd>
    <trd lng="it">Saarland</trd>
    <trd lng="ru">&c_S;&c_a;&c_a;&c_r;</trd>
  </snc>
</drv>

<drv mrk="sarland.0ano">
  <kap><tld lit="s"/>ano</kap>
  <snc>
    <dif>
      Civitano de Sarlando.
    </dif>
  </snc>
  <trd lng="ca">saarlandi&agrave; <klr>(subst.)</klr></trd>
  <trd lng="de">Saarl&auml;nder</trd>
  <trd lng="fr">sarrois<klr> (subst.)</klr></trd>
  <trd lng="hu">Saar-vid&eacute;ki</trd>
</drv>

<drv mrk="sarland.0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilatanta al Sarlando.
    </dif>
  </snc>
  <trd lng="ca">saarlandi&agrave; <klr>(adj.)</klr></trd>
  <trd lng="de">saarl&auml;ndisch</trd>
  <trd lng="fr">sarrois<klr> (adj.)</klr></trd>
  <trd lng="hu">Saar-vid&eacute;ki</trd>
</drv>
</art>
