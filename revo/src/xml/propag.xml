<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: propag.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>propagand</rad>/o
</kap>
<drv mrk="propag.0o">
  <kap><tld/>o</kap>
  <snc mrk="propag.0o.KOMUNE">
    <dif>
      Klopodado por disvastigi ideon:
      <ekz>
        <tld/>o por Esperanto;
      </ekz>
      <ekz>
        libreto difinita speciale por la <tld/>o de nia
        afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a verko, parolado.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel">akcelo</ref>,
      <ref cel="reklam.0o">reklamo</ref>,
      <ref cel="pusx">pu&scirc;ado</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>propagace</trd>,
    <trd>propaganda</trd>,
    <trd>šíření</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Werbung</trd>,
    <trd>Propaganda</trd>
  </trdgrp>
  <trd lng="es">propaganda</trd>
  <trd lng="fr">propagande</trd>
  <trdgrp lng="hu">
    <trd>propaganda</trd>,
    <trd>n&eacute;pszer&udblac;s&iacute;t&eacute;s</trd>,
    <trd>h&iacute;rver&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">propaganda</trd>
  <trdgrp lng="pt">
    <trd>propaganda</trd>,
    <trd>divulga&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sk">propaganda</trd>
  <trd lng="sv">propaganda</trd>
</drv>

<drv mrk="propag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="propag.0i.KOMUNE">
    <dif>
      Fari <tld/>on por:
      <ekz>
        <tld/>i Esperanton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="adept.0igi">adeptigi</ref>,
      <ref cel="fam.dis0igi">disfamigi</ref>,
      <ref cel="anonc">disanonci</ref>,
      <ref cel="rekrut">rekrutigi</ref>,
      <ref cel="sem.0i.FIG">semi <sncref/></ref>,
      <ref cel="varb.0i">varbi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>propagovat</trd>,
    <trd>šířit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>werben</trd>,
    <trd>propagieren</trd>
  </trdgrp>
  <trd lng="es">hacer propaganda</trd>
  <trd lng="fr">faire de la <ind>propagande</ind></trd>
  <trdgrp lng="hu">
    <trd>propag&aacute;l</trd>,
    <trd>n&eacute;pszer&udblac;s&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">propaganda maken</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">propagovať</trd>
</drv>

<drv mrk="propag.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="propag.0ilo.KOMUNE">
    <dif>
      &Ccirc;ia dokumento, objekto... helpanta en <tld/>o:
      <ekz>
        tie &ccirc;i estas a&ccirc;eteblaj lernolibroj kaj <tld/>iloj de la lingvo
        internacia
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 40a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>leták</trd>,
    <trd>prostředek propagace</trd>
  </trdgrp>
  <trd lng="es">material de propaganda (folletos, carteles&#8230;)</trd>
  <trdgrp lng="fr">
    <trd>objet promotionnel</trd>,
    <trd>mat&eacute;riel de <ind>propagande</ind></trd>
  </trdgrp>
  <trd lng="sk">leták</trd>
</drv>

<drv mrk="propag.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="propag.0isto.KOMUNE">
    <dif>
      Homo, kiu da&ubreve;re <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">propagátor</trd>
  <trd lng="de">Propagandist</trd>
  <trd lng="es">propagandista</trd>
  <trd lng="fr">propagandiste</trd>
  <trd lng="hu">propagandista</trd>
  <trd lng="nl">propagandist</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_a;&c_g;&c_a;&c_n;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">propagátor</trd>
</drv>
</art>
