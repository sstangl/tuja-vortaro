<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sirdarj.xml,v 1.11 2014/06/05 01:10:14 revo Exp $">
<kap>
  <rad>Sirdarj</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>

<drv mrk="sirdarj.S0o">
  <kap><tld/>o</kap> 
  <snc mrk="sirdarj.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
	<dif>
	  Granda <ref tip="lst" cel="river.0o"
  lst="voko:riveroj">rivero</ref> de la
	  <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
	  fluanta el la Fergana Valo al (la Malgranda)
	  <ref cel="aral.A0o">Aralo</ref>.
	</dif>
  </snc>
  <trd lng="be">&c_S;&c_y;&c_r;&c_d;&c_a;&c_r;'&c_ja;</trd>
  <trd lng="ca">Sirdari&agrave;</trd>
  <trd lng="de">Syr-Darja</trd>
  <trdgrp lng="en">
	<trd>Syr Darya</trd>,
	<trd>Jaxartes</trd>,
	<trd>Yaxartes</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&f_y;&a_r;&a_d;&a_r;&f_y;&a_A;</trd>,
    <trd>&a_s;&f_y;&a_H;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">Syr-Daria</trd>
  <trd lng="hu">Szir-darja</trd> 
  <trdgrp lng="ru">
	<trd>&c_S;&c_y;&c_r;&c_d;&c_a;&c_r;&c_mol;&c_ja;</trd>,
	<trd>&c_Ja;&c_k;&c_s;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
</art>
