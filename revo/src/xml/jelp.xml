<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jelp.xml,v 1.11 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>jelp</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="jelp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="jelp.0i.ZOO">
    <dif>
      Akute ek<ref tip="super" cel="blek.0i.beste">bleki</ref>, kiel
      hundeto, &scirc;akalo.
    </dif>
    <refgrp tip="vid">
      <ref cel="boj.0i.ZOO">boji <sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="jelp.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kria&ccirc;i per akuta vo&ccirc;o, ordinare pro kolero:
      <ekz>
        &scirc;i <tld/>is kaj hurlis, kio forte ekscitis la mastrinon
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 83a, p. 45a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_v;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ječet</trd>,
    <trd>kňučet</trd>,
    <trd>skučet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bellen</trd>,
    <trd>kl&auml;ffen</trd>
  </trdgrp>
  <trd lng="en">yelp</trd>
  <trdgrp lng="es">
     <trd>aullar</trd>,
     <trd>gru&ntilde;ir</trd>
  </trdgrp>
  <trd lng="fr">japper</trd>
  <trdgrp lng="it">
    <trd>uggiolare</trd>,
    <trd>guaire</trd>
  </trdgrp>
  <trd lng="pl">ujada&cacute;</trd>
  <trdgrp lng="pt">
    <trd>cainhar</trd>,
    <trd>ganir</trd>,
    <trd>uivar <klr tip="amb">(chorosamente)</klr></trd>,
    <trd>chorar <klr tip="amb">(uivando)</klr></trd>,
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kňučať</trd>,
    <trd>skučať</trd>
  </trdgrp>
</drv>
</art>

