<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arkad.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>arkad</rad>/o
</kap>

<drv mrk="arkad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arka&jcirc;o sur kolonoj, prezentanta sub si
      malplena&jcirc;on:
      <ekz>
        promeni sub la <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">arcada</trd>
  <trdgrp lng="cs">
    <trd>arkáda</trd>,
    <trd>podloubí</trd>
  </trdgrp>
  <trd lng="de">Arkade</trd>
  <trd lng="en">arcade</trd>
  <trd lng="fa">&a_t;&a_A;&a_q;&f_g;&a_A;&a_n;</trd>
  <trd lng="fr">arcade</trd>
  <trd lng="hu">&aacute;rk&aacute;d</trd>
  <trd lng="it">arcata</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">oblúkmi spojené stĺporadie</trd>
</drv>

<drv mrk="arkad.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Vico da <tld/>oj.
    </dif>
  </snc>
  <trd lng="ca">arcuaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>arkáda</trd>,
    <trd>podloubí</trd>
  </trdgrp>
  <trd lng="de">Arkaden</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rk&aacute;dok</trd>,
    <trd>&aacute;rk&aacute;dsor</trd></trdgrp>
  <trd lng="it">portico</trd>
  <trd lng="sk">arkáda</trd>
</drv>
</art>
