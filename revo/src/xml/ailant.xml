<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ailant.xml,v 1.16 2013/08/29 12:10:20 revo Exp $">
<kap>
  <rad>ailant</rad>/o
</kap>
<drv mrk="ailant.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="simarub.0oj">simarubacoj</ref>
      (<trd lng="la">Ailanthus</trd>), orientaziaj kaj a&ubreve;straliaj
arboj
      kun longaj, akre odorantaj folioj, kulturataj por bredado de
      silkra&ubreve;poj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_j;&c_l;&c_ja;&c_n;&c_t;</trd>
  <trd lng="ca">g&egrave;nere Ailanthus</trd>
  <trd lng="de">G&ouml;tterbaum</trd>
  <trdgrp lng="en">
    <trd>Ailanthus</trd>,
    <trd>tree of heaven</trd>
  </trdgrp>
  <trd lng="fr">ailante</trd>
  <trd lng="hu">b&aacute;lv&aacute;nyfa</trd>
  <trd lng="nl">hemelboom</trd>
  <trd lng="pl">bo&zdot;odrzew</trd>
  <trd lng="pt">ailanto</trd>
  <trd lng="ru">&c_a;&c_j;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="uk">&c_a;&c_j;&c_l;&c_ja;&c_n;&c_t;</trd>
</drv>
</art>
