<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: azale.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>azale</rad>/o
</kap>
<drv mrk="azale.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Azalea</trd>),
      kun grandaj multnombraj belkoloraj floroj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="ca">azalea</trd>
  <trd lng="cs">azalka</trd>
  <trd lng="de">Azalee</trd>
  <trd lng="en">azalea</trd>
  <trd lng="es">azalea</trd>
  <trd lng="fr">azal&eacute;e</trd>
  <trd lng="hu">az&aacute;lea</trd>
  <trd lng="it">azalea</trd>
  <trd lng="nl">azalea</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_z;&c_a;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_a;&c_z;&c_a;&c_l;&c_je;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">azalka</trd>
  <trd lng="sv">azalea</trd>
</drv>
</art>
