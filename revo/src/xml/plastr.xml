<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plastr.xml,v 1.14 2015/07/04 08:37:41 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>plastr</rad>/o
</kap>

<drv mrk="plastr.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Medikamento el glueca substanco, per kiu oni &scirc;miras pecon 
de tolo,
       kiun oni metas sur la kuracota korpa parto.
     </dif>
     <refgrp tip="sin">
       <ref cel="katapl.0o">kataplasmo</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_r;</trd>
  <trd lng="bg">&c_l;&c_je;&c_p;&c_je;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>sádra</trd>,
    <trd>sádrový obvaz</trd>
  </trdgrp>
  <trd lng="de">Pflaster</trd>
  <trdgrp lng="fr">
      <trd>empl&acirc;tre</trd>,
      <trd>diachylon</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>tapasz</trd>,
     <trd>flastrom</trd>
   </trdgrp>
  <trd lng="nl">pleister</trd>
  <trd lng="pt">emplastro</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_y;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>sadra</trd>,
    <trd>sadrový obväz</trd>
  </trdgrp>
</drv>
</art>
