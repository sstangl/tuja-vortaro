<vortaro>
<art mrk="$Id: irene.xml,v 1.6 2015/02/26 19:12:10 revo Exp $">
<kap>
  <rad>Irene</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="irene.0o">
  <kap><tld/>o</kap>
  <snc mrk="irene.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        mi a&ubreve;dis la altan kaj &scirc;erceman vo&ccirc;on de <tld/>o
        <fnt>
          <aut>J. L. Borges, trad. M. Giorgini</aut>,
          <vrk><url
          ref="http://www.yksi.org/tekst/ald/borges_if-eo.html">
          Funes, la memoranta</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_r;&c_e;&c_n;&c_e;&c_j;</trd>
  <trd lng="fr">Ir&eacute;n&eacute;e</trd>
  <trd lng="ca">Ireneu</trd>
  <trd lng="oc">Iren&egrave;u</trd>
  <trd lng="pl">Ireneusz</trd>
  <trd lng="ru">&c_I;&c_r;&c_je;&c_n;&c_je;&c_j;</trd>
</drv>
</art>

