<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kadavr.xml,v 1.28 2015/07/04 08:37:36 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>kadavr</rad>/o
</kap>
<drv mrk="kadavr.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Korpo de mortinta homo a&ubreve; besto:
       <ekz><tld/>a palo kovris lian viza&gcirc;on.</ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="skelet.0o">skeleto</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="bg">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="ca">cad&agrave;ver</trd>
  <trdgrp lng="cs">
    <trd>kadáver</trd>,
    <trd>mrtvola</trd>,
    <trd>mrtvé tělo</trd>,
    <trd>mršina</trd>,
    <trd>tělo zemřelého</trd>,
    <trd>uhynulé zvíře</trd>,
    <trd>umrlec</trd>,
    <trd>zdechlina</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Leiche</trd>,
      <trd>Leichnam</trd>,
      <trd>Kadaver</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>cadaver</trd>,
     <trd>corpse</trd>,
     <trd>(dead) <ind>body</ind></trd>
   </trdgrp>
  <trd lng="es">cad&aacute;ver</trd>
  <trdgrp lng="fa">
     <trd>&a_j;&a_s;&a_d;</trd>,
     <trd>&a_j;&a_n;&a_A;&a_z;&a_h;</trd>,
     <trd>&a_n;&a_ayn;&a_sh;</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd><klr tip="amb">(kuollut) </klr> ruumis</trd>,
     <trd>raato</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>cadavre</trd>,
     <trd>corps<klr tip="ind"> (d&eacute;pouille mortelle)</klr></trd>,
     <trd>d&eacute;pouille<klr tip="ind"> (mortelle)</klr></trd>
   </trdgrp>
  <trdgrp lng="he">
     <trd>&gimel;&vav;&pe;&he;</trd>,
     <trd>&gimel;&vav;&vav;&yod;&yod;&he;</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>hulla</trd>,
     <trd>holttest</trd>,
     <trd>d&ouml;g</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>lijk</trd>,
     <trd>kadaver</trd>
   </trdgrp>
  <trd lng="pl">trup</trd>
  <trd lng="pt">cad&aacute;ver</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_p;</trd>
  <trd lng="sk">mŕtvola</trd>
  <trdgrp lng="sv">
     <trd>lik</trd>,
     <trd>kadaver</trd>
   </trdgrp>
  <trdgrp lng="tr">
     <trd>ceset</trd>,
     <trd>kadavra</trd>
   </trdgrp>
</drv>
</art>
