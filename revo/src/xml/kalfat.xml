<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kalfat.xml,v 1.19 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalfatr</rad>/i
</kap>
<drv mrk="kalfat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">MAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;topi fendojn de &scirc;ipo 
      per <ref cel="stup.0o">stupo</ref> 
      kaj <ref cel="gudr.0o">gudro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_ib; &c_s;&c_m;&c_a;&c_l;&c_ib;&c_c;&c_mol;
    <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;)</klr></trd>,
    <trd>&c_z;&c_a;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol; &c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_y; <klr>(&c_u; &c_k;&c_o;&c_r;&c_p;&c_u;&c_s;&c_je;
    &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="br">kalafeti&ntilde;</trd>
  <trd lng="ca">calafatar</trd>
  <trdgrp lng="cs">
    <trd>těsnit</trd>,
    <trd>ucpávat <klr>(spáry zejm. v lodi)</klr></trd>,
    <trd>upěchovat</trd>,
    <trd>utěsnit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>kalfatern</trd>,
     <trd>abdichten</trd>
  </trdgrp>
  <trd lng="en">caulk</trd>
  <trd lng="es">calafatear</trd>
  <trd lng="fa">&a_q;&f_y;&a_r;&a_A;&a_n;&a_d;&a_w;&a_d; &f_k;&a_r;&a_d;&a_n;<klr> (&a_b;&a_d;&a_n;&a_h;&a_hamza_sure;
  &f_k;&a_sh;&a_t;&f_y;)</klr></trd>
  <trd lng="fr">calfater</trd>
  <trdgrp lng="hu">
    <trd>dugaroz</trd>,
    <trd>kalaf&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>breeuwen</trd>
  </trdgrp>
  <trd lng="pt">calafetar</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zapchávať diery <klr>(na lodi, kúdeľou a dechtom)</klr></trd>,
    <trd>zapchávať diery <klr>(na lodi| kúdeľou a dechtom)</klr></trd>
  </trdgrp>
</drv>
</art>
