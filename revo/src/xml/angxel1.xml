        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk>
          <url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_jo;&c_l; &c_P;&c_a;&c_n;&c_s;&c_k;&c_ib;
  <klr tip="amb">(&c_m;&c_a;&c_l;&c_ib;&c_t;&c_v;&c_a;
  &c_ib; &c_b;&c_ib;&c_c;&c_mol;&c_c;&c_jo; &c_w;
  &c_z;&c_v;&c_a;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">&agrave;ngelus</trd>
  <trd lng="de">Angelus</trd>
  <trd lng="en">the angelus</trd>
  <trd lng="es">&aacute;ngelus</trd>
  <trd lng="fr">ang&eacute;lus</trd>
  <trd lng="nl">angelus</trd>
  <trd lng="hu">angelus <klr>(harangsz&oacute;)</klr></trd>
