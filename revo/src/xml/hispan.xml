<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hispan.xml,v 1.11 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hispan</rad>/o
</kap>

<drv mrk="hispan.0o">
  <kap><tld/>o</kap>
  <snc mrk="hispan.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="hispan.H0ujo"><tld lit="H"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="hispan.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_s;&c_p;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="cs">Španěl</trd>
  <trd lng="de">Spanier</trd>
  <trd lng="en">Spaniard</trd>
  <trd lng="es">espa&ntilde;ol</trd>
  <trd lng="fr">Espagnol<klr> (subst.)</klr></trd>
  <trd lng="it">spagnolo<klr> (sost.)</klr></trd>
  <trd lng="nl">Spanjaard</trd>
  <trd lng="no">spanjol</trd>
  <trd lng="pl">Hiszpan</trd>
  <trd lng="pt">espanhol <klr tip="ind"> (subst.)</klr></trd>
  <trd lng="sk">španiel</trd>
</drv>

<drv mrk="hispan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">španělský</trd>
  <trd lng="de">spanisch</trd>
  <trd lng="en">Spanish</trd>
  <trd lng="fr">espagnol<klr> (adj.)</klr></trd>
  <trd lng="it">spagnolo<klr> (agg.)</klr></trd>
  <trd lng="nl">Spaans</trd>
  <trd lng="no">spansk</trd>
  <trd lng="pl">hiszpa&nacute;ski</trd>
  <trd lng="pt">espanhol <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="sk">španielsky</trd>
</drv>

<drv mrk="hispan.H0ujo">
  <kap><tld lit="H"/>ujo</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      sudokcidenta <ref tip="malprt"
      cel="euxrop1.E0o">E&ubreve;ropo</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_I;&c_s;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="da">Spanien</trd>
  <trd lng="de">Spanien</trd>
  <trd lng="en">Spain</trd>
  <trd lng="es">Espa&ntilde;a</trd>
  <trdgrp lng="fr">
    <trd>Espagne</trd>,
    <trd>Royaume d'Espagne</trd>
  </trdgrp>
  <trd lng="id">Spanyol</trd>
  <trd lng="it">Spagna</trd>
  <trd lng="nl">Spanje</trd>
  <trd lng="no">Spania</trd>
  <trd lng="pl">Hiszpania</trd>
  <trd lng="pt">Espanha</trd>
</drv>

</art>

