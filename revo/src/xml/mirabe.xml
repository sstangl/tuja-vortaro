<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirabe.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mirabel</rad>/o
</kap>
<drv mrk="mirabe.0o">
  <kap><tld/>o</kap>
  <snc mrk="mirabe.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Flavkolora tre dol&ccirc;a <ref tip="super" cel="prun.0o">pruno</ref>:
      <ekz>
        proprahejme distilita <tld/>brando
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_y;&c_ch;&c_a;</trd>
  <trd lng="ca">mirabol&agrave;</trd>
  <trd lng="cs">mirabelka <klr>(plod)</klr></trd>
  <trd lng="de">Mirabelle</trd>
  <trd lng="en">mirabelle</trd>
  <trd lng="fr">mirabelle</trd>
  <trd lng="hu">mirabella</trd>
  <trd lng="nl">mirabel</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_a;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_a;&c_l;&c_y;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mirabelka <klr>(plod)</klr></trd>
</drv>
</art>
