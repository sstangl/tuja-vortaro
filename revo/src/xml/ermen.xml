<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ermen.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ermen</rad>/o
</kap>
<drv mrk="ermen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
     Tre malgranda <ref tip="lst" cel="best.rabo0o"
     lst="voko:rabobestoj">rabobesto</ref> kun vintra felo tute blanka
     krom la nigra vostopinto; dum mezepoko la pelton rajtis uzi en siaj
     vestoj nur kavaliroj kaj doktoroj; 
     <ref tip="lst" cel="speci.0o.BIO"
     lst="voko:zoologiaj_specioj">specio</ref> de <ref tip="super"
     cel="mustel.0o">mustelo</ref>
    (<trd lng="la">Mustela erminea</trd>).
    </dif>
    <trd lng="de">Hermelin</trd>
    <trd lng="en">ermine</trd>
    <trd lng="fr">hermine</trd>
    <trd lng="hu">hermelin</trd>
    <trd lng="id">cerpelai</trd>
    <trd lng="nl">hermelijn</trd>
    <trd lng="pl">gronostaj</trd>
    <trd lng="pt">arminho</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_o;&c_s;&c_t;&c_a;&c_j;</trd>
  </snc>
  <trd lng="cs">hranostaj</trd>
  <trd lng="sk">hranostaj</trd>
</drv>
</art>
