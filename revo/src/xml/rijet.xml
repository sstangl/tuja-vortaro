  <rad>rijet</rad>/o
</kap>
<drv mrk="rijet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Speco de man&gcirc;a&jcirc;o, konsistanta el hakita porka&jcirc;o a&ubreve; kokida&jcirc;o, miksita
      kun graso, kutime man&gcirc;ata &scirc;mirita sur pantran&ccirc;a&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fašírka</trd>,
    <trd>karbanátek</trd>
  </trdgrp>
  <trd lng="de">Schmalz</trd>
  <trd lng="fr">rillette</trd>
  <trd lng="hu">p&aacute;st&eacute;tom</trd>
  <trd lng="nl">pastei</trd>
  <trd lng="pt">picado de porco ou de galinha</trd>
  <trd lng="ru">&c_r;&c_i;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>fašírka</trd>,
    <trd>karbonátka</trd>
  </trdgrp>
</drv>
</art>
