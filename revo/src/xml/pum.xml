<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pum.xml,v 1.11 2013/11/21 02:10:27 revo Exp $">
<kap>
  <rad>pum</rad> <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pum.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="pum.0o.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de subita bato a&ubreve; falo;
      <ref tip="vid" cel="bum1.0.brui">bum</ref>:
      <ekz>
        per la bastono mi volas &gcirc;in kapti,
        sed, <tld/>, mi glitas de sur la kruta&jcirc;o malsupren
        <fnt>
          <aut>G. von Lettow-Vorbeck, trad. W. Gantzckow</aut>,
          <vrk><url ref="http://www.literaturo.dk/printempatago.htm">
          Printempa tago</url></vrk>,
          <lok>Esperanto-Praktiko, 1928-03, (10:3)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bum1.0">bum</ref>
    </refgrp>
  </snc>
  <trd lng="de">bumm!</trd>
  <trdgrp lng="fr">
    <trd>boum !</trd>,
    <trd>plaf !</trd>
  </trdgrp>
  <trd lng="hu">bumm!</trd>
  <trdgrp lng="nl">
    <trd>boem!</trd>,
    <trd>plof!</trd>
  </trdgrp>
  <trd lng="pt">pum!</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_h;!</trd>,
    <trd>&c_b;&c_u;&c_m;!</trd>
  </trdgrp>
</drv>

<drv mrk="pum.pum0ado">
  <kap><tld/><tld/>ado</kap>
  <snc mrk="pum.pum0ado.SON">
    <dif>
      Ripetata a&ubreve; ritma bata <ref tip="super"
        cel="bru.0o.sonaro">bruo</ref>:
      <ekz>
        Li <klr>...</klr> a&ubreve;skultis la kriojn kaj la <tld/><tld/>adon
        venantajn de la lumplena dormo&ccirc;ambro
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 20a, p. 202-203a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">martellement</trd>
  <trd lng="hu">d&uuml;b&ouml;rg&eacute;s</trd>
  <trd lng="nl">geplof</trd>
  <trd lng="pt">martelagem</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_b;&c_a;&c_h;&c_a;&c_n;&c_mol;&c_je;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_h;&c_o;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>

