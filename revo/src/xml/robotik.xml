<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: robotik.xml,v 1.4 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>robotik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="robotik.0o">
  <kap><tld/>o</kap>
  <snc mrk="robotik.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Arto plani kaj konstrui
	  <ref tip="vid" cel="robot.0o.MAS">robotojn</ref>:
      <ekz>
        s<klr>injo</klr>ro Nagata <klr>...</klr>
        estas profesoro de <tld/>o
        <fnt>
          <aut>Takaoka M.</aut>,
          <vrk><url ref="http://iran_esperanto.tripod.com/kartaka.htm">
          Voja&gcirc;o al Irano</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtomatik.0o.MAS">a&ubreve;tomatiko</ref>
    </refgrp>
  </snc>
  <trd lng="cs">robotika</trd>
  <trd lng="en">robotics</trd>
  <trd lng="fr">robotique<klr> (subst.)</klr></trd>
  <trd lng="sk">robotika</trd>
</drv>

<drv mrk="robotik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="robotik.0isto.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="robotik.0o.MAS"><tld/>o</ref>,
      pri farado de robotoj:
      <ekz>
        Asimo de Honda nun kapablas eviti movi&gcirc;antajn objektojn 
        <klr>...</klr> dank' al laboro de <tld/>istoj de la
        Universitato de Carnegie Mellon
        <fnt>
          <aut>L. G. Restrepo Rivas</aut>,
          <vrk><url
          ref="http://skribitaj-pensoj.blogspot.fr/2009/08/asimo-evitas-movi-obstaklojn.html">
          Asimo evitas movi&gcirc;antajn obstaklojn </url></vrk>,
          <lok>2009-08-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">roboticien</trd>
</drv>
</art>

