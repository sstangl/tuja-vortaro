<vortaro>
<art mrk="$Id: tarant1.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tarantul</rad>/o
</kap>
<drv mrk="tarant1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="arane.lup0o">luparaneo</ref>
      (<trd lng="la">Lycosa tarentula</trd>), helbruna kun nigraj strioj
      sur la dorso, vivanta en tertruoj en Mediteraneo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="ca">tar&agrave;ntula</trd>
  <trd lng="cs">tarantule</trd>
  <trd lng="de">Tarantel</trd>
  <trdgrp lng="fa">
   <trd>&a_r;&a_t;&f_y;&a_l;&a_A;&a_hamza;</trd>
   <trd>&a_r;&a_t;&f_y;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">tarentule</trd>
  <trd lng="hu">tarantella(p&oacute;k)</trd>
  <trd lng="nl">tarantula</trd>
  <trd lng="pl">tarantula</trd>
  <trdgrp lng="pt">
   <trd>tar&acirc;ntula</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_u;&c_l;</trd>
  <trd lng="sk">tarantula</trd>
</drv>
</art>
