<art mrk="$Id: tenor.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tenor</rad>/o
</kap>
<drv mrk="tenor.0o">
  <kap><tld/>o</kap>
  <snc mrk="tenor.0o.vocxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Plej alta vira vo&ccirc;o.
    </dif>
  </snc>
  <snc mrk="tenor.0o.ulo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="T"/>ulo:
      <ekz>
        singardaj <klr>...</klr> kantistoj evitas <klr>...</klr>
        trinkojn
        alkoholajn, precipe la <tld/>oj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_a;&c_r;</trd>
  <trd lng="cs">tenor <klr>(zpěvák)</klr></trd>
  <trd lng="de">Tenor</trd>
  <trd lng="en">tenor</trd>
  <trd lng="es">tenor</trd>
  <trd lng="fr">t&eacute;nor</trd>
  <trd lng="hu">tenor</trd>
  <trd lng="it">tenore</trd>
  <trd lng="nl">tenor</trd>
  <trd lng="pl">tenor</trd>
  <trdgrp lng="pt">
    <trd>tenor</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_o;&c_r;</trd>
  <trd lng="sk">tenor <klr>(spevák)</klr></trd>
</drv>
</art>
