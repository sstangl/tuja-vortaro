<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sorg.xml,v 1.15 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>sorg</rad>/o
</kap>
<drv mrk="sorg.0o">
  <kap><tld/>o</kap>
  <snc mrk="sorg.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref
      tip="super" cel="poac.0oj">poacoj</ref> (<trd
      lng="la">Sorghum</trd>), 
      kelkaj specioj estas kulturataj kiel greno en Afriko kaj Hindujo:
      <ekz>
        la <tld/>o povas atingi alton de kvar &gcirc;is kvin metroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_r;&c_g;&c_a;</trd>
  <trd lng="ca">melca</trd>
  <trdgrp lng="cs">
    <trd>proso</trd>,
    <trd>ƒçirok</trd>
  </trdgrp>
  <trd lng="de">Sorghumhirse</trd>
  <trd lng="en">sorghum</trd>
  <trd lng="fr">sorgho</trd>
  <trd lng="hu">cirok</trd>
  <trd lng="nl">gierst</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_g;&c_o;</trd>
  <trd lng="sk">cirok</trd>
</drv>
</art>
