<art mrk="$Id: poleks.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>poleks</rad>/o
</kap>
<drv mrk="poleks.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="poleks.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="super" cel="fingr.dik0o.super">Dikfingro</ref> mana:
      <ekz>
        jen tion &ccirc;i mi komprenas! &ndash; levas la <tld/>on Sa&scirc;ka
        <klr>(signo de aprobo)</klr>
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 21a, p. 122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; &c_p;&c_a;&c_l;&c_je;&c_c; &c_r;&c_u;&c_k;&c_ib;</trd>
  <trd lng="ca">polze</trd>
  <trd lng="cs">palec ruky</trd>
  <trd lng="de">Daumen</trd>
  <trd lng="en">thumb</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_s;&a_t;</trd>,
    <trd>&a_A;&a_n;&f_g;&a_sh;&a_t; &a_sh;&a_s;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">pouce</trd>
  <trd lng="hu">h&uuml;velykujj</trd>
  <trd lng="it">pollice</trd>
  <trd lng="nl">duim</trd>
  <trd lng="pl">kciuk</trd>
  <trdgrp lng="pt">
    <trd>dedo <ind>polegar</ind></trd>,
    <trd>polegar</trd>,
    <trd>p&oacute;lex</trd>    
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_o;&c_j; &c_p;&c_a;&c_l;&c_je;&c_c; &c_r;&c_u;&c_k;&c_i;</trd>
  <trd lng="sk">palec ruky</trd>
</drv>
</art>
