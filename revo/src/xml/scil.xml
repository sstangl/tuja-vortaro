<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: scil.xml,v 1.4 2015/07/04 08:37:43 revo Exp $">
    <kap>
      <rad>Scil</rad>/o <fnt>Z</fnt><fnt>, <bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="scil.S0o">
      <kap><tld lit="S"/>o</kap>
      <snc mrk="scil.S0o.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          Roko en la Mesina markolo, &ccirc;e la kalabria bordo,
          je la sagofluga distanco de &Hcirc;aribdo:
          <ekz>
            <uzo tip="stl">FRAZ</uzo>
            trafi de <tld lit="S"/>o al &Hcirc;aribdo
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="scil.S0o.MIT">
        <uzo tip="fak">MIT</uzo>
        <uzo tip="fak">PERS</uzo>
        <dif>
          Monstrino kiu lo&gcirc;is en kavo de la roko
          <tld lit="S"/>o<sncref ref="scil.S0o.MAR"/>
          kaj forvoradis preternavigantajn maristojn.
        </dif>
      </snc>
  <trd lng="cs">Skylla</trd>
  <trd lng="el">&Sigma;&kappa;&ypsilon_ton;&lambda;&lambda;&alfa;</trd>
  <trd lng="en">Scylla</trd>
  <trd lng="fr">Scylla</trd>
  <trdgrp lng="ru">
        <trd>&c_S;&c_k;&c_i;&c_l;&c_l;&c_a;</trd>,
        <trd>&c_S;&c_c;&c_i;&c_l;&c_l;&c_a;</trd>,
      </trdgrp>
  <trd lng="sk">Skylla</trd>
</drv>
  </art>

