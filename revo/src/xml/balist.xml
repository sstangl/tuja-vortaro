<art mrk="$Id: balist.xml,v 1.21 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>balist</rad>/o
</kap>
<drv mrk="balist.0o">
  <kap><tld/>o</kap>
  <snc mrk="balist.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Antikva militma&scirc;ino por &jcirc;etado de &scirc;tonoj,
sagegoj, ktp.:
      <ekz>
        la krioj de la soldataro kaj la bruo de la ruli&gcirc;antaj
<tld/>oj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_p;&c_u;&c_l;&c_mol;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="br">bangounell</trd>
  <trd lng="ca">balista</trd>
  <trdgrp lng="cs">
    <trd>balista</trd>,
    <trd>vrhací stroj <klr>(kamenů, šípů)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Balliste</trd>,
    <trd>r&ouml;misches <ind>Katapult</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ballista</trd>,
    <trd>catapult</trd>
  </trdgrp>
  <trd lng="es">catapulta</trd>
  <trd lng="fa">&a_m;&a_n;&a_j;&a_n;&f_y;&a_q;</trd>
  <trd lng="fr">baliste</trd>
  <trdgrp lng="hu">
    <trd>haj&iacute;t&oacute;g&eacute;p</trd>,
    <trd>baliszta</trd>
  </trdgrp>
  <trd lng="it">balista</trd>
  <trd lng="nl">katapult</trd>
  <trd lng="pl">balista</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_l;&c_i;&c_s;&c_t;&c_a;</trd>
  <trd lng="sk">vrhací stroj</trd>
  <trd lng="sv">katapult</trd>
</drv>
</art>
