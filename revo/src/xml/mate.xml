<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mate.xml,v 1.12 2012/08/19 16:10:10 revo Exp $">
<kap>
  <ofc>9</ofc><rad>mate</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mate.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <snc mrk="mate.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Speco de ilekso el Sud-Ameriko, el kies sekigitaj folioj oni faras
      teon.
    </dif>
  </snc>
  <snc mrk="mate.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="M"/>-trinka&jcirc;o:
      <ekz>
        &jcirc;anaina reveme plu trinketis sian <tld/>on
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_e;</trd>
  <trd lng="ca">mate</trd>
  <trd lng="de">Mate</trd>
  <trd lng="en">yerba mate</trd>
  <trd lng="fr">mat&eacute;</trd>
  <trd lng="hu">mat&eacute;<klr tip="ind"> (paragyai
tean&ouml;v&eacute;ny</klr></trd>
  <trd lng="nl">mat&eacute;</trd>
  <trdgrp lng="pt">
    <trd>mate <klr tip="amb">(bot.)</klr></trd>,
    <trd>erva-mate</trd>
    <trd>ca&aacute;</trd>,
    <trd>ch&aacute;-dos-jesu&iacute;tas</trd>,    
    <trd>ch&aacute;-mate</trd>,    
    <trd>congonha</trd>,
    <trd>congonha-mansa</trd>,    
    <trd>congonha-verdadeira</trd>,    
    <trd>congonheira</trd>,    
    <trd>erva <klr tip="ind">(erva-mate)</klr></trd>,    
    <trd>mate-do-paraguai</trd>,
    <trd>pau-de-erva</trd>    
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_je;</trd>
</drv>
</art>
