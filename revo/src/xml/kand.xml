<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kand.xml,v 1.15 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kand</rad>/o
</kap>
<drv mrk="kand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kand.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Purigita, kristaligita kaj preska&ubreve; diafana sukero:
      <ekz>
        &scirc;in profunde okupis negoco pri <tld/>aj bestetoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_d;&c_z;&c_ja;&c_sh;</trd>
  <trdgrp lng="cs">
    <trd>kandys</trd>,
    <trd>zast. cukrkandl</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kandis</trd>,
     <trd>Kandiszucker</trd>,
     <trd>Kandelzucker</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sugar candy</trd>,
    <trd>crystallized sugar</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caramelo</trd>,
    <trd>az&uacute;car caramelizado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>candi</trd>,
    <trd>sucre candi</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kandiscukor</trd>,
     <trd>jegescukor</trd>
  </trdgrp>
  <trd lng="nl">kandij</trd>
  <trd lng="ru">&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">kandys</trd>
</drv>
</art>
