<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gurd.xml,v 1.23 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gurd</rad>/o
</kap>
<drv mrk="gurd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="gurd.0o.MUZ">
    <dif>
      A&ubreve;tomata <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>, portebla
      orgeneto,
      kiun
      sonigas trukartoj movataj per manturnata cilindro:
      <ekz>
        &gcirc;i ludis <ctl>Ave Maria</ctl> de Gounod, kiun ludis &ccirc;iuj
        <tld/>oj
        anta&ubreve; tridek jaroj
        <fnt><bib>Kiso</bib>, <lok>&Aring;ge</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>orguenet</trd>,
    <trd>orgue de maneta</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>flašinet</trd>,
    <trd>kolovrátek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leierkasten</trd>,
    <trd>Drehorgel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hurdy-gurdy</trd>,
    <trd>barrel organ</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>organillo</trd>,
    <trd>cilindro</trd>
  </trdgrp>
  <trd lng="fr">orgue de Barbarie</trd>
  <trdgrp lng="hu">
    <trd>verkli</trd>,
    <trd>kintorna</trd>,
    <trd>s&iacute;pl&aacute;da</trd>
  </trdgrp>
  <trd lng="nl">draaiorgel</trd>
  <trd lng="pl">katarynka</trd>
  <trd lng="pt">realejo</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>verkeľ</trd>,
    <trd>verklík</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>positiv<klr> (musikinstrument)</klr></trd>,
    <trd>vevorgel</trd>
  </trdgrp>
</drv>

<drv mrk="gurd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gurd.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sonigi <tld/>on:
      <ekz>
        la mizerulo <tld/>is melankolian arion.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_k;&c_a;&c_t;&c_r;&c_y;&c_n;&c_c;&c_y;</ind></trd>
    <trd lng="ca">tocar l'orguenet</trd>
    <trd lng="fr">jouer<klr tip="ind"> (sur un orgue de
    Barbarie)</klr></trd>
    <trdgrp lng="hu">
      <trd>verkliz</trd>,
      <trd>verklin j&aacute;tszik</trd>
    </trdgrp>
    <trd lng="nl">een <ind>draaiorgel</ind> bespelen</trd>
    <trd lng="pt">tocar realejo</trd>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_je;</trd>
    <trdgrp lng="sv">
      <trd>spela <ind>positiv</ind></trd>,
      <trd>spela <ind>vevorgel</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="gurd.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Agi, precipe paroli a&ubreve;tomate, ripete, kvaza&ubreve; turnante
      senpense la turnilon de <tld/>o:
      <ekz>
        <tld/>itaj <klr>(banalaj, malmulte signifaj, tede
        ripetitaj)</klr>
        frazoj.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;&c_c;&c_mol; &c_a;&c_d;&c_n;&c_o; &c_j; &c_t;&c_o;&c_je; &c_zh;</trd>
    <trdgrp lng="ca">
      <trd>reiterar, repetir-se</trd>,
      <trd>tornar-se tedi&oacute;s, mon&ograve;ton</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>rab&acirc;cher</trd>,
      <trd>ressasser</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>verkliz<klr tip="ind"> (&aacute;tv...)</klr></trd>,
      <trd>elcs&eacute;pel<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>hajtogat<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
    <trd lng="nl">tot treurens toe <ind>herhalen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol; &c_o;&c_d;&c_n;&c_o; &c_i; &c_t;&c_o; &c_zh;&c_je;</trd>,
      <trd>&c_t;&c_a;&c_l;&c_d;&c_y;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>hrát na kolovrátek</trd>,
    <trd>omílat stále totéž</trd>
  </trdgrp>
  <trd lng="sk">hrať na verklíku</trd>
</drv>
</art>
