<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fazan.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fazan</rad>/o
</kap>
<drv mrk="fazan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="fazane.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phasianus</trd>),
      granda birdo kun longaj vostplumoj kaj belkolora plumaro
      &ccirc;e la viro.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>ba&zcaron;ant</trd>,
    <trd>bažant kolchický</trd>,
    <trd>bažant obecný</trd>
  </trdgrp>
  <trd lng="de">Fasan</trd>
  <trd lng="en">pheasant</trd>
  <trd lng="fr">faisan</trd>
  <trd lng="hu">f&aacute;c&aacute;n</trd>
  <trd lng="nl">fazant</trd>
  <trd lng="pl">ba&zdot;ant</trd>
  <trd lng="pt">fais&atilde;o</trd>
  <trd lng="ru">&c_f;&c_a;&c_z;&c_a;&c_n;</trd>
  <trd lng="sk">bažant</trd>
</drv>

</art>
