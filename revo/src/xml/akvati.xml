<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akvati.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akvatint</rad>/o
</kap>
<drv mrk="akvati.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvati.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Gravura procedo per acido, imitanta la lavumadon.
    </dif>
  </snc>
  <snc mrk="akvati.0o.verko">
    <dif>
      <tld lit="A"/>a&jcirc;o, verko farita per <tld/>a procedo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_v;&c_a;&c_t;&c_y;&c_n;&c_t;&c_a;</trd>
  <trd lng="ca">aiguatinta</trd>
  <trd lng="cs">akvatinta</trd>
  <trdgrp lng="en">
    <trd>aquatint</trd>,
    <trd>acid-washing</trd>
  </trdgrp>
  <trd lng="fr">aquatinte</trd>
  <trdgrp lng="hu">
    <trd>akvatinta</trd>,
    <trd>foltmar&aacute;s</trd>
  </trdgrp>
  <trd lng="it">acquatinta</trd>
  <trd lng="nl">aquatint</trd>
  <trd lng="pt">aquatinta</trd>
  <trd lng="ru">&c_a;&c_k;&c_v;&c_a;&c_t;&c_i;&c_n;&c_t;&c_a;</trd>
  <trd lng="sk">akvatina</trd>
</drv>
</art>
