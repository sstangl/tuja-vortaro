<vortaro>
<art mrk="$Id: verben1.xml,v 1.10 2013/08/25 16:11:27 revo Exp $">
<kap>
  <rad>verbenac</rad>/oj
</kap>
<drv mrk="verben1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="verben1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Verbenaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_r;&c_b;&c_e;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">verben&agrave;cies</trd>
  <trd lng="de">Eisenkrautgew&auml;chse</trd>
  <trd lng="fr">verb&eacute;nac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>verb&eacute;naf&eacute;l&eacute;k</trd>,
    <trd>vasf&udblac;f&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">IJzerhardfamilie</trd>
  <trd lng="pt">verben&aacute;ceas</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_je;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
