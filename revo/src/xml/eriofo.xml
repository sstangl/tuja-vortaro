<kap>
<rad>eriofor</rad>/o
</kap>
<drv mrk="eriofo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="ciper.0oj">ciperacoj</ref>,
      (<trd lng="la">Eriophorum</trd>).   
    </dif>
  </snc>
  <bld lok="../bld/eriofo.jpg"></bld>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_je;&c_j;</trd>
  <trd lng="bg">&c_p;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Wollgras</trd>
  <trd lng="en">cotton grass</trd>
  <trdgrp lng="fr">
    <trd>linaigrette</trd>,
    <trd>jonc &agrave; coton</trd></trdgrp> 
  <trd lng="hu">gyapj&uacute;s&aacute;s</trd>
  <trd lng="nl">wollegras</trd>
  <trd lng="pl">we&lstroke;nianka</trd>
  <trd lng="pt">er&iacute;ngio</trd>
  <trd lng="ru">&c_p;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">páperník</trd>
  <trd lng="cs">suchopýr</trd>
</drv>
</art>
