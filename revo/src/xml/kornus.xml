<vortaro>
<art mrk="$Id: kornus.xml,v 1.17 2014/09/29 12:10:31 revo Exp $">
<kap>
  <rad>kornus</rad>/o
</kap>
<drv mrk="kornus.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kornus1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cornus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_z;&c_ib;&c_l;</trd>
  <trd lng="de">Hartriegel</trd>
  <trdgrp lng="en">
    <trd>cornel</trd>,
    <trd>dogwood</trd>
  </trdgrp>
  <trd lng="fr">cornus</trd>
  <trd lng="hu">som</trd>
  <trd lng="nl">kornoelje</trd>
  <trd lng="pl">dere&nacute;</trd>
  <trdgrp lng="pt">
    <trd>corniso</trd>,
    <trd>sangrinho</trd>,    
    <trd>sanguinho-leg&iacute;timo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_z;&c_i;&c_l;</trd>
</drv>
</art>
