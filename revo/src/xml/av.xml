<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: av.xml,v 1.37 2015/09/29 12:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>av</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="av.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="av.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      patro a&ubreve; patrino:
      <ekz>
        patrinflanka <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="br">tad-kozh</trd>
  <trdgrp lng="ca">
    <trd>avi</trd>,
    <trd>iaio</trd>,
    <trd>jai</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;d</trd>,
    <trd>d&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;vater</trd>,
    <trd>Opa</trd>
  </trdgrp>
  <trd lng="en">grandfather</trd>
  <trd lng="es">abuelo</trd>
  <trd lng="eu">aitite</trd>
  <trd lng="fr">grand-p&egrave;re</trd>
  <trdgrp lng="he">
    <trd>&samekh;&bet;</trd>,
    <trd>&samekh;&bet;&alef;</trd>
  </trdgrp>
  <trd lng="hu">nagyapa</trd>
  <trd lng="id">kakek</trd>
  <trd lng="it">nonno</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#29238; [<ind>&#12381;&#12405;</ind>]</trd>,
    <trd>&#12362;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">grootvader</trd>
  <trd lng="no">bestefar</trd>
  <trd lng="pt">av&ocirc;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_d;</trd>,
    <trd>&c_d;&c_je;&c_d;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>star&yacute; otec</trd>,
    <trd>dedko</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>dede</trd>,
    <trd>b&uuml;y&uuml;k baba</trd>
  </trdgrp>
</drv>

<drv mrk="av.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="av.0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de patro a&ubreve; patrino:
      <ekz>
        vi &gcirc;uos intimon kaj apogon &ccirc;e la <tld/>ino
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 3a, volumo 1a, p.
        33a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="br">mamm-gozh</trd>
  <trdgrp lng="ca">
    <trd>&agrave;via</trd>,
    <trd>iaia</trd>,
    <trd>jaia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>babi&ccaron;ka</trd>,
    <trd>b&aacute;ba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gro&szlig;mutter</trd>,
    <trd>Oma</trd>
  </trdgrp>
  <trd lng="en">grandmother</trd>
  <trd lng="es">abuela</trd>
  <trd lng="eu">amama</trd>
  <trd lng="fr">grand-m&egrave;re</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef;</trd>
  <trd lng="hu">nagyanya</trd>
  <trd lng="id">nenek</trd>
  <trd lng="it">nonna</trd>
  <trdgrp lng="ja">
    <trd>&#31062;&#27597; [<ind>&#12381;&#12412;</ind>]</trd>,
    <trd>&#12362;&#12400;&#12354;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">grootmoeder</trd>
  <trd lng="no">bestemor</trd>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>star&aacute; mama</trd>,
    <trd>babka</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>nine</trd>,
    <trd>b&uuml;y&uuml;k anne</trd>
  </trdgrp>
</drv>

<drv mrk="av.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="av.pra0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0o" lst="voko:parencoj">Patro</ref> de
      <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la pra<tld/>on &ccirc;agrenis tia familiara parolo
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_z;&c_je;&c_d;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_d;&c_ja;&c_d;&c_o;</trd>
  <trd lng="br">tad-ku&ntilde;v</trd>
  <trd lng="ca">besavi</trd>
  <trdgrp lng="cs">
    <trd>prad&ecaron;d</trd>,
    <trd>prad&ecaron;de&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Urgro&szlig;vater</trd>,
    <trd>Uropa</trd>
  </trdgrp>
  <trd lng="en">great-grandfather</trd>
  <trd lng="es">bisabuelo</trd>
  <trd lng="fr">arri&egrave;re-grand-p&egrave;re</trd>
  <trd lng="he">&samekh;&bet;&alef; &resh;&bet;&alef;</trd>
  <trd lng="hu">d&eacute;dapa</trd>
  <trdgrp lng="it">
    <trd>bisnonno</trd>, <trd>bisavo</trd>, <trd>bisavolo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26366;&#31062;&#29238; [<ind>&#12381;&#12358;&#12381;&#12405;</ind>]</trd>,
    <trd>&#12402;&#12356;&#12362;&#12376;&#12356;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">overgrootvader</trd>
  <trd lng="no">oldefar</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_d;&c_je;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>praded</trd>,
    <trd>pradedko</trd>
  </trdgrp>
</drv>

<drv mrk="av.pra0ino">
  <kap>pra<tld/>ino</kap>
  <snc mrk="av.pra0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="patr.0ino" lst="voko:parencoj">Patrino</ref>
      de <tld/>o a&ubreve; <tld/>ino:
      <ekz>
        la pra<tld/>ino pli bone fartas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 64a, volumo 2a, p.
        393a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_a;</trd>
  <trd lng="br">mamm-gu&ntilde;v</trd>
  <trd lng="ca">bes&agrave;via</trd>
  <trd lng="cs">prababi&ccaron;ka</trd>
  <trdgrp lng="de">
    <trd>Urgro&szlig;mutter</trd>,
    <trd>Uroma</trd>
  </trdgrp>
  <trd lng="en">great-grandmother</trd>
  <trd lng="es">bisabuela</trd>
  <trd lng="fr">arri&egrave;re-grand-m&egrave;re</trd>
  <trd lng="he">&samekh;&bet;&tav;&alef; &resh;&bet;&tav;&alef;</trd>
  <trd lng="hu">d&eacute;danya</trd>
  <trdgrp lng="it">
    <trd>bisnonna</trd>, <trd>bisava</trd>, <trd>bisavola</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26366;&#31062;&#27597; [<ind>&#12381;&#12358;&#12381;&#12412;</ind>]</trd>,
    <trd>&#12402;&#12356;&#12362;&#12400;&#12354;&#12373;&#12435;</trd>
  </trdgrp>
  <trd lng="nl">overgrootmoeder</trd>
  <trd lng="no">oldemor</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_b;&c_a;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">prababka</trd>
</drv>

<drv mrk="av.pra0ismo">
  <kap>pra<tld/>ismo</kap>
  <snc mrk="av.pra0ismo.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">GEN</uzo>
    <ref tip="dif" cel="atavis.0o.GEN">atavismo</ref>
    <ekz>
      trajtoj reaperas pro la le&gcirc;o de pra<tld/>ismo
      <fnt>
        <bib>OriginoDeSpecioj</bib>,
        <lok>&ccirc;apitro 6a</lok>
      </fnt>.
    </ekz>  
  </snc>
  <trd lng="be">&c_a;&c_t;&c_a;&c_v;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">atavisme</trd>
  <trd lng="de">Atavismus</trd>
  <trd lng="en">atavism</trd>
  <trd lng="es">atavismo</trd>
  <trd lng="fr">atavisme</trd>
  <trdgrp lng="hu">
    <trd>atavizmus</trd>,
    <trd>vissza&uuml;t&eacute;s</trd>
  </trdgrp>
  <trd lng="it">atavismo</trd>
  <trd lng="nl">atavisme</trd>
  <trd lng="no">atavisme</trd>
  <trd lng="ru">&c_a;&c_t;&c_a;&c_v;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
