<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skorzo.xml,v 1.12 2006/05/19 16:31:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>skorzoner</rad>/o
</kap>
<drv mrk="skorzo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Scorzonera</trd>),
      legomo, kies dika rekta radiko kaj folioj estas man&gcirc;eblaj.
    </dif>
  </snc>
  <trd lng="en">black salsify</trd>
  <trd lng="de">Schwarzwurzel</trd>
  <trdgrp lng="fr">
    <trd>salsifis noir</trd>,
    <trd>scorson&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">pozdor</trd>
  <trd lng="nl">schorseneer</trd>
  <trd lng="pl">w&eogonek;&zdot;ymord</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_z;&c_je;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_c;&c_o;&c_n;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;
&c_k;&c_o;&c_r;&c_je;&c_n;&c_mol;<klr tip="ind">
(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
