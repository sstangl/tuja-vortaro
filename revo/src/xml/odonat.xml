<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: odonat.xml,v 1.8 2013/07/29 11:10:10 revo Exp $">
<kap>
  <rad>odonat</rad>/oj
</kap>
<drv mrk="odonat.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Odonata</trd>).
    </dif>
    <ref tip="prt" cel="libel.0o">libelo</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_k;&c_o;&c_z;&c_y; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_n;&c_i; &c_k;&c_o;&c_n;&c_ch;&c_je;&c_t;&c_a;</trd>
  <trd lng="de">Libellen<klr tip="ind"> (Ordnung)</klr></trd>
  <trd lng="hu">szitak&ouml;t&odblac;k<klr tip="amb"> (rend)</klr></trd>
  <trdgrp lng="nl">
    <trd>libellen</trd>,
    <trd>waternimfen</trd>
  </trdgrp>
  <trd lng="pt">odonatos</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_k;&c_o;&c_z;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
