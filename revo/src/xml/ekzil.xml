<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekzil.xml,v 1.18 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ekzil</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekzil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekzil.0i.POL">
    <dif>
      Devigi iun foriri el sia patrujo, malpermesante, ke li tien
      revenu:
      <ekz>
        Atenanoj <tld/>is Temistoklon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pel.el0i">elpeli</ref>,
      <ref cel="pusx.el0i">elpu&scirc;i</ref>,
      <ref cel="proskr.0i">proskribi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">exiliar</trd>
  <trdgrp lng="cs">
    <trd>vyhnat do vyhnanství</trd>,
    <trd>vykázat do vyhnanství</trd>,
    <trd>vypovědět ze země</trd>
  </trdgrp>
  <trd lng="de">verbannen</trd>
  <trd lng="en">exile</trd>
  <trd lng="es">exiliar</trd>
  <trd lng="fr">exiler</trd>
  <trdgrp lng="pl">
    <trd>deportowa&cacute;</trd>,
    <trd>wydala&cacute;<klr> (z kraju)</klr></trd>,
    <trd>wygna&cacute;</trd>,
    <trd>zsy&lstroke;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>exilar</trd>,
    <trd>banir</trd>,
    <trd>degredar</trd>,
    <trd>desterrar</trd>
  </trdgrp>
  <trdgrp lng="ru">
  <trd>&c_i;&c_z;&c_g;&c_n;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_v;&c_y;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_s;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poslať do vyhnanstva</trd>,
    <trd>vypovedať <klr>(z krajiny)</klr></trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>landsf&ouml;rvisa</trd>,
    <trd>utvisa</trd>
  </trdgrp>
</drv>

<drv mrk="ekzil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ekzil.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Deviga foriro el sia patrujo, sen ebleco tien reveni:
      <ekz>
        neniun ni <tld/>u, &ccirc;ar malpia estas la <tld/>o!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_k;&c_z;&c_y;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">exili</trd>
  <trdgrp lng="cs">
    <trd>exil</trd>,
    <trd>vyhnanství</trd>
  </trdgrp>
  <trd lng="es">exilio</trd>
  <trd lng="fr">exil<klr tip="ind"> (acte, &eacute;tat)</klr></trd>
  <trdgrp lng="pl">
    <trd>deportacja</trd>,
    <trd>wydalenie<klr> (z kraju)</klr></trd>,
    <trd>wygnanie</trd>,
    <trd>zsy&lstroke;ka</trd>
  </trdgrp>
  <trd lng="sk">vyhnanstvo</trd>
</drv>

<drv mrk="ekzil.0ito">
  <kap><tld/>ito</kap>
  <snc mrk="ekzil.0ito.POL">
    <dif>
      Homo <tld/>ita.
    </dif>
  </snc>
  <trd lng="ca">exiliat</trd>
  <trdgrp lng="cs">
    <trd>exulant</trd>,
    <trd>vyhnanec</trd>
  </trdgrp>
  <trd lng="es">exiliado</trd>
  <trd lng="fr">exil&eacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>wychod&zacute;ca</trd>,
    <trd>wygnaniec</trd>,
    <trd>zes&lstroke;aniec</trd>
  </trdgrp>
  <trd lng="sk">vyhnanec</trd>
</drv>
</art>
