<art mrk="$Id: cxarpi.xml,v 1.12 2009/04/27 16:31:21 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>&ccirc;arpi</rad>/o
</kap>
<drv mrk="cxarpi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Fadena&jcirc;o el diserigita eluzita tola&jcirc;o por surkovri vundojn
      (nun ne plu uzata).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_r;&c_p;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Charpie</trd>,
    <trd>Zupflinnen</trd>
  </trdgrp>
  <trd lng="fr">charpie</trd>
  <trd lng="hu">t&eacute;p&eacute;s<klr tip="ind"> (sebre)</klr></trd>
  <trd lng="it">filaccia</trd>
  <trdgrp lng="pl">
    <trd>szarpie</trd>,
    <trd>skubanka</trd>
  </trdgrp>
  <trd lng="pt">mecha</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_p;&c_i;&c_ja;</trd>
</drv>
</art>
