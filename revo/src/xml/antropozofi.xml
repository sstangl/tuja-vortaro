<vortaro>
<art mrk="$Id: antropozofi.xml,v 1.4 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>antropozofi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="antropozofi.0o">
  <kap><tld/>o</kap>
  <snc mrk="antropozofi.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino derivita de 
      <ref tip="vid" cel="teozof.0o.FIL">teozofio</ref>:
      <ekz>
        la historio de la <tld/>a idearo komenci&gcirc;is dum la jaro 
        1902, kiam Rudolf Steiner i&gcirc;is gvidanto de la germana sekcio de 
        la <ctl>Teozofia Societo</ctl>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Antropozofio">
          Antropozofio</url>, 2012-09-12</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_s;&c_o;&c_f;&c_i;&c_ja;</trd>
  <trd lng="cs">antropozofie</trd>
  <trd lng="de">Anthroposophie</trd>
  <trd lng="en">anthroposophy</trd>
  <trd lng="fr">anthroposophie</trd>
  <trd lng="sk">antropozofia</trd>
</drv>
</art>

