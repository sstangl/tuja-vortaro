<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pian.xml,v 1.24 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pian</rad>/o
</kap>
<drv mrk="pian.0o">
  <kap><tld/>o</kap>
  <snc mrk="pian.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">Kordinstrumento</ref> pli
      malgranda ol fortepiano, kun vertikalaj kordoj kaj klavaro:
      <ekz>
        li sidi&gcirc;is sur la se&gcirc;o anta&ubreve; la <tld/>o kaj komencis
        transturnadi
        la foliojn en la notolibro
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
        glacia&jcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fortep.0o">fortepiano</ref>
    </refgrp>  </snc>
  <trd lng="be">&c_p;&c_ib;&c_ja;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">piano</trd>
  <trdgrp lng="cs">
    <trd>klavír</trd>,
    <trd>piano</trd>,
    <trd>pianoforte</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Klavier</trd>,
    <trd>Piano</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>piano</trd>,
    <trd>upright piano</trd>
  </trdgrp>
  <trd lng="es">piano</trd>
  <trd lng="fr">piano</trd><trd lng="fr">piano</trd>
  <trd lng="hu">pian&iacute;n&oacute;</trd>
  <trd lng="id">piano</trd>
  <trd lng="it">pianoforte<klr> verticale</klr></trd>
  <trd lng="nl">piano</trd>
  <trd lng="pl">pianino</trd>
  <trdgrp lng="pt">
    <trd>piano</trd>,
    <trd>piano vertical</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_a;&c_n;&c_i;&c_n;&c_o;</trd>
  <trd lng="sk">klavír</trd>
  <trd lng="sv">piano</trd>
</drv>
</art>
