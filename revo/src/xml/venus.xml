<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: venus.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <rad>Venus</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="venus.0o">
      <kap><tld/>o</kap>
      <snc mrk="venus.0o.mita">
        <uzo tip="fak">MIT</uzo>
        <ref tip="dif" cel="vener1.0o">Venero</ref>
        <trd lng="hu">Venus</trd>    
      </snc>
      <snc mrk="venus.0o.AST">
        <uzo tip="fak">AST</uzo>
        <mlg>&#9792;</mlg>
        <dif>
          La dua (la&ubreve; distanco de la <ref tip="vid"
            cel="sun.0o.nia">Suno</ref>)
          <ref tip="lst" cel="planed.0o"
          lst="voko:planedoj">planedo</ref>
          de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>.
        </dif>
        <trd lng="hu">V&eacute;nusz</trd>
        <trd lng="zh">&#37329;&#26143;<klr>
        [j&#299;nx&#299;ng]</klr></trd>
      </snc>
  <trd lng="be">&c_V;&c_e;&c_n;&c_e;&c_r;&c_a;</trd>
  <trd lng="bg">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
  <trd lng="ca">Venus</trd>
  <trdgrp lng="cs">
    <trd>Venuše</trd>,
    <trd>večernice</trd>
  </trdgrp>
  <trd lng="de">Venus</trd>
  <trd lng="el">&Alfa;&phi;&rho;&omikron;&delta;&jota_ton;&tau;&eta;
      </trd>
  <trd lng="en">Venus</trd>
  <trd lng="es">Venus</trd>
  <trd lng="fr">V&eacute;nus</trd>
  <trd lng="id">Venus</trd>
  <trd lng="it">Venere</trd>
  <trd lng="la">Venus</trd>
  <trd lng="nl">Venus</trd>
  <trd lng="no">Venus</trd>
  <trd lng="pl">Wenus</trd>
  <trd lng="pt">V&ecirc;nus</trd>
  <trd lng="ru">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
  <trd lng="sk">Venuša</trd>
</drv>

    <drv mrk="venus.0a">
      <kap><tld/>a</kap>
      <snc mrk="venus.0a.mita">
        <uzo tip="fak">MIT</uzo>
        <dif>
          Rilata al <tld/>o<sncref ref="venus.0o.mita"/>.
        </dif>
        <trd lng="hu">venusi</trd>
        <trd lng="la">Venereus</trd>
        <trd lng="ru">&c_v;&c_je;&c_n;&c_je;&c_r;&c_i;&c_n;</trd>
      </snc>
      <snc mrk="venus.0a.planeda">
        <uzo tip="fak">AST</uzo>
        <dif>
          Rilata al <tld/>o<sncref ref="venus.0o.AST"/>.
        </dif>
        <trd lng="hu">V&eacute;nusz-</trd>
        <trd lng="ru">
          &c_v;&c_je;&c_n;&c_je;&c_r;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <trd lng="ca">venusi&agrave;</trd>
      <trd lng="de">Venus-</trd>
      <trd lng="en">Venusian</trd>
      <trd lng="pl">wenusja&nacute;ski</trd>
      <trd lng="pt">Venusiano</trd>
      <trd lng="fr">v&eacute;nusien<klr> (adj.)</klr></trd>
    </drv>
  </art>
