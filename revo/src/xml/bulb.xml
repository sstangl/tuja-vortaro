<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bulb.xml,v 1.24 2015/10/07 11:10:20 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>bulb</rad>/o
</kap>
<drv mrk="bulb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bulb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Ordinare subtera diskforma <ref tip="super"
      cel="tig.0o">tigo</ref>
      kun multaj, dense sidantaj, skvamformaj, karnecaj folioj:
      <ekz>
        kesto kun neordinaraj floraj bulboj
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o de lilio, de tulipo, de cepo.
      </ekz>
    </dif>
    <trd lng="en">bulb</trd>
    <trd lng="fr">bulbe <klr tip="ind">(botanique)</klr></trd>
    <trd lng="pl">cebulka</trd>
  </snc>
  <snc mrk="bulb.0o.cepo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La <tld/>o de <ref tip="malprt" cel="cep.0o">cepo</ref>:
      <ekz>
        la kukumojn kaj la melonojn kaj la poreojn kaj la bulbojn kaj 
        la ajlojn
        <fnt><bib>MT</bib>, <lok>&Nom; 11:5</lok></fnt>;
      </ekz>
      <ekz>
        mi ricevis bulbojn kaj rezine odoretantan vinon
        <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="en">onion <ind>bulb</ind></trd>
    <trd lng="fr">oignon</trd>
    <trd lng="pl">cebula</trd>
  </snc>
  <snc mrk="bulb.0o.formo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="B"/>oforma objekto:
      <ekz>
        oni de malproksime rekonis la <tld/>ojn de
        la rusaj pre&gcirc;ejoj.
      </ekz>
    </dif>
    <trd lng="en">bulb dome</trd>
    <trd lng="fr">bulbe <klr tip="ind">(fig.)</klr></trd>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_b;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_l;&c_u;&c_k;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">bulb</trd>
  <trdgrp lng="cs">
    <trd>cibule</trd>,
    <trd>cibulka</trd>,
    <trd>hl&iacute;za rostliny</trd>
  </trdgrp>
  <trd lng="de">Zwiebel</trd>
  <trd lng="he">&pe;&qof;&ayin;&tav;</trd>
  <trd lng="hu">hagyma<klr tip="ind">
  (&aacute;ltal&aacute;ban)</klr></trd>
  <trd lng="pt">bulbo</trd>
  <trd lng="ru">&c_l;&c_u;&c_k;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">cibu&lcaron;a <klr>(druh kore&ncaron;a)</klr></trd>
</drv>
</art>
