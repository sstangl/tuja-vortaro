<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gxin2.xml,v 1.2 2014/12/15 21:10:13 revo Exp $">
<kap>
  <rad>&gcirc;in</rad>/o
</kap>
<drv mrk="gxin2.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxin2.0o.MEZ">
    <dif>
      &Ccirc;ina tradicia <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">mezurunuo</ref> de pezo
      kun diversaj historiaj valoroj,
      nuntempe egala al 500 gramoj:
      <ekz>
        &ccirc;iu familio en nia vila&gcirc;o donis al li cent 
        <tld/>ojn da rizo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ekzistas anka&ubreve;
    <ref tip="hom" cel="gxin.0o">&gcirc;ino</ref>,
    alkohola trinka&jcirc;o, kaj
    <ref tip="hom" cel="gxin.0o">&gcirc;ino</ref>,
    feo en islama tradicio.
  </rim>
  <trdgrp lng="fr">
    <trd>jin</trd>,
    <trd>livre chinoise</trd>
  </trdgrp>
</drv>
</art>

