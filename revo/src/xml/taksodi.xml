<art mrk="$Id: taksodi.xml,v 1.12 2013/08/31 19:10:23 revo Exp $">
<kap>
  <rad>taksodiac</rad>/oj
</kap>
<drv mrk="taksodi.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="taksodi.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Iama nomo de <ref cel="famili.0o.BIO"
      >familio</ref> de <ref
      cel="konifer.0oj">koniferoj</ref>,
      altaj pinglarboj (<trd lng="la">Taxodiaceae</trd>), plu nur
      subfamilio (<trd lng="la">Taxodioideae</trd>).
    </dif>
  </snc>
  <trd lng="ca">taxodi&agrave;cies</trd>
  <trd lng="de">Sumpfzypressengew&auml;chse</trd>
  <trd lng="fr">taxiodac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>mocs&aacute;rfeny&odblac;f&eacute;l&eacute;k</trd>,
    <trd>mocs&aacute;rciprusf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">moerascypresfamilie</trd>
  <trdgrp lng="pt">
   <trd>taxodi&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_o;&c_d;&c_i;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
