<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fosili.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fosili</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fosili.0o">
  <kap><tld/>o</kap>
  <snc mrk="fosili.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      &Scirc;toni&gcirc;inta vegeta&jcirc;a a&ubreve; besta resta&jcirc;o
      a&ubreve; postsigno, kiujn oni trovas en tre
      antikva tertavolo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fosílie</trd>,
    <trd>zkamenělina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fossil</trd>,
    <trd>Petrefakt</trd>,
    <trd>Versteinerung</trd>
  </trdgrp>
  <trd lng="en">fossil</trd>
  <trd lng="es">f&oacute;sil</trd>
  <trd lng="fr">fossile<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;v&uuml;let</trd>,
    <trd>fossz&iacute;lia</trd>
  </trdgrp>
  <trd lng="nl">fossiel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pt">f&oacute;ssil</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_o;&c_je;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">skamenelina</trd>
</drv>

<drv mrk="fosili.0a">
  <kap><tld/>a</kap>
  <snc mrk="fosili.0a.MIN">
    <dif>
      &Scirc;toneca, havanta econ de <tld/>o,
      rilatanta al <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fossil</trd>,
    <trd>versteinert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fossile<klr tip="ind"> (adj.)</klr></trd>,
    <trd>fossilis&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedett</trd>,
    <trd>fosszilis</trd>,
    <trd>&aacute;satag</trd>
  </trdgrp>
  <trd lng="nl">fossiel<klr tip="ind"> (b.nw.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_m;&c_je;&c_n;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fosili.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="fosili.0igxi.MIN">
    <dif>
      <ref tip="vid" cel="sxton.0igxi.MIN">&Scirc;toni&gcirc;i</ref>,
      fari&gcirc;i <tld/>o:
      <ekz>
        grandaj <tld/>i&gcirc;intaj arboj <klr>...</klr>
        ankora&ubreve; staras rektaj kiel ili kreskis
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_c;&c_mol; <ind>&c_v;&c_y;&c_k;&c_a;&c_p;&c_n;&c_je;&c_m;</ind></trd>
  <trd lng="de">versteinern</trd>
  <trd lng="en">fossilize</trd>
  <trd lng="es">fosilizarse</trd>
  <trd lng="fr">se <ind>fossiliser</ind></trd>
  <trdgrp lng="hu">
    <trd>megk&ouml;vesedik</trd>,
    <trd>megk&ouml;v&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verstenen</trd>,
    <trd>fossiliseren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol; &c_i;&c_s;&c_k;&c_o;&c_p;&c_a;&c_je;&c_m;&c_y;&c_m;</trd>
</drv>
</art>
