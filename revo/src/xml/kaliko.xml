<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaliko.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalikot</rad>/o
</kap>
<drv mrk="kaliko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="vid" cel="koton.0o">Kotona</ref> teksa&jcirc;o, similanta
      <ref tip="vid" cel="tol.0o">tolon</ref> sed pli dika ol
      <ref tip="vid" cel="perkal.0o">perkalo</ref>:
      <ekz>
        tabio estas farata el silko, kotono, veluro ktp. kun plandumo el
        <tld/>o
        <fnt><bib>EncJap</bib> <lok>artikolo &leftquot;tabio&rightquot;, p.
        162a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="katun.0o">katuno</ref>
  <trd lng="be">&c_k;&c_a;&c_l;&c_je;&c_n;&c_k;&c_o;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>calic&oacute;</trd>,
    <trd>calicot</trd>
  </trdgrp>
  <trd lng="en">calico</trd>
  <trd lng="es">calic&oacute;</trd>
  <trdgrp lng="fr">
    <trd>calicot<klr tip="ind"> (tissu)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pamutv&aacute;szon</trd>,
    <trd>kalik&oacute;</trd>
  </trdgrp>
  <trd lng="nl">calicot</trd>
  <trdgrp lng="pl">
    <trd>kaliko</trd>,
    <trd>p&lstroke;&oacute;tno angielskie</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_n;&c_k;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kaliko</trd>,
    <trd>plátenná tkanina <klr>(india)</klr></trd>
  </trdgrp>
  <trd lng="cs">kaliko</trd>
</drv>
</art>
