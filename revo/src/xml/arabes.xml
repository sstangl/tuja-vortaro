<vortaro>
<art mrk="$Id: arabes.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>arabesk</rad>/o
</kap>
<drv mrk="arabes.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ornama desegna&jcirc;o, la&ubreve; la araba maniero, konsistanta el
      kunplektitaj linioj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tra la verda foliaro la suno desegnis sur la
        tero lumajn <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_b;&c_e;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_r;&c_a;&c_b;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="ca">arabesc</trd>
  <trd lng="cs">arabeska</trd>
  <trd lng="de">Arabeske</trd>
  <trd lng="en">arabesque</trd>
  <trd lng="es">arabesco</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&f_y;&a_m;&f_y;</trd>
  <trd lng="fr">arabesque</trd>
  <trd lng="hu">arabeszk</trd>
  <trd lng="nl">arabesk</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_a;&c_b;&c_je;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">arabeska</trd>
</drv>
</art>
