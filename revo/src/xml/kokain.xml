<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kokain.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kokain</rad>/o
</kap>
<drv mrk="kokain.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokain.0o.MED">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="alkalo.0o">Alkaloido</ref>,
      ekstraktata el la folioj de <ref cel="koka.0o">kokao</ref>:
      <ekz>
        la <tld/>o estas tre &scirc;atata anestezilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_ib;&c_n;</trd>
  <trd lng="ca">coca&iuml;na</trd>
  <trd lng="cs">kokain</trd>
  <trd lng="de">Kokain</trd>
  <trdgrp lng="fr">
    <trd>coca&iuml;ne</trd>,
    <trd>coco<klr tip="ind"> (coca&iuml;ne)</klr></trd>
  </trdgrp>
  <trd lng="hu">kokain</trd>
  <trd lng="id">kokaina</trd>
  <trd lng="nl">coca&iuml;ne</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;</trd>
  <trd lng="sk">kokaín</trd>
</drv>

<drv mrk="kokain.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kokain.0ismo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsana</ref> stato, ka&ubreve;zita de veneni&gcirc;o
      per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_ib;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">coca&iuml;nisme</trd>
  <trdgrp lng="cs">
    <trd>kokainizmus</trd>,
    <trd>závislost na kokainu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coca&iuml;nisme</trd>,
    <trd>coca&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">kokainizmus</trd>
  <trd lng="id">kokainisme</trd>
  <trd lng="nl">coca&iuml;nisme</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">závislosť na kokaíne</trd>
</drv>
</art>
