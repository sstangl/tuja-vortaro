    <trd>fish trap</trd>,
    <trd>lobster trap</trd>,
    <trd>fishing basket</trd>,
    <trd>eel buck</trd>,
    <trd>eel pot</trd>,
    <trd>crayfish net</trd>,
    <trd>hoop net</trd>
