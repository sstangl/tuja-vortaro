<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prikl.xml,v 1.3 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>prikl</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prikl.0i">
  <kap><tld/>i</kap>
  <snc mrk="prikl.0i.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="vid" cel="plant.trans0i.HOR">Transplanti</ref>
      junan plantidon al definitiva kreskoloko:
      <ekz>
        li diris al mi, ke anta&ubreve; kelke da jaroj li ankora&ubreve; povis 
        plugi, <tld/>i kaj fal&ccirc;i
        <fnt>
          <aut>Pang Naiying</aut>,
          <vrk><url
          ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/9909/bd99-9-16.html">
          Lando de longvivuloj</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1999-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pikýrovat</trd>,
    <trd>přepichovat</trd>,
    <trd>přepíchávat</trd>,
    <trd>přesazovat sazenice</trd>,
    <trd>rozsazovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>pikieren</trd>,
    <trd>vereinzeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>repiquer<klr> (hort.)</klr></trd>,
    <trd>transplanter<klr> (hort.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prepichovať</trd>,
    <trd>presadzovať sadenice</trd>,
    <trd>rozsadzovať</trd>
  </trdgrp>
</drv>
</art>

