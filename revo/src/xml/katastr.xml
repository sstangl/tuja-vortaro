<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katastr.xml,v 1.13 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>katastr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="katastr.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="regist.teren0o">terenregistro</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_a;&c_s;&c_t;&c_r;</trd>
  <trd lng="ca">cadastre</trd>
  <trdgrp lng="cs">
    <trd>katastrální rejstřík</trd>,
    <trd>pozemkové knihy</trd>,
    <trd>rejstřík pozemků</trd>,
    <trd>soupis pozemků</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kataster</trd>,
    <trd>Grundbuch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>land register</trd>,
    <trd>register of title deeds</trd>,
    <trd>official real estate register</trd>
  </trdgrp>
  <trd lng="es">catastro</trd>
  <trd lng="fr">cadastre</trd>
  <trdgrp lng="hu">
    <trd>telekk&ouml;nyv</trd>,
    <trd>kataszter</trd>
  </trdgrp>
  <trd lng="nl">kadaster<klr tip="ind"> (register)</klr></trd>
  <trdgrp lng="pl">
  <trd>kataster</trd>,
  <trd>spis nieruchomo&sacute;ci</trd>
</trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_a;&c_s;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>pozemková kniha</trd>,
    <trd>pozemkový register</trd>,
    <trd>súpis pozemkov</trd>
  </trdgrp>
</drv>

<drv mrk="katastr.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      La oficejo, kie oni flegas la <tld/>ojn. Terenregistrejo.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_d;&c_a;&c_s;&c_t;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Katasteramt</trd>,
      <trd>Grundbuchamt</trd>
    </trdgrp>
    <trd lng="fr">cadastre<klr tip="ind"> (service)</klr></trd>
    <trd lng="hu">f&ouml;ldhivatal</trd>
    <trd lng="nl">kadaster<klr tip="ind"> (dienst)</klr></trd>
    <trd lng="pl">kataster</trd>
    <trd lng="ru">&c_k;&c_a;&c_d;&c_a;&c_s;&c_t;&c_r;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>katastr</trd>,
    <trd>katastrální území obce</trd>,
    <trd>obvod pozemků obce</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kataster</trd>,
    <trd>katastrálne územie</trd>
  </trdgrp>
</drv>
</art>
