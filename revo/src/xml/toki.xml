<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: toki.xml,v 1.14 2015/06/26 09:10:15 revo Exp $">
<kap><rad>Toki</rad>/o<fnt>Z</fnt></kap>
<drv mrk="toki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" 
lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      kaj plej granda urbo de
      <ref tip="malprt" cel="japan.0ujo">Japanujo</ref>,
      kun origina nomo <ctl>Edo</ctl>:
      <ekz>
        <tld/>o signifas <ctl>orienta
        &ccirc;efurbo</ctl> kaj iam oficiale anstata&ubreve;is la nomon Edo.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_T;&c_o;&c_k;&c_ib;&c_jo;</trd>
    <trd lng="ca">T&ograve;quio</trd>
    <trd lng="de">Tokio</trd>
    <trd lng="fa">&a_t;&a_w;&f_k;&f_y;&a_w;</trd>
    <trd lng="fr">Tokyo</trd>
    <trd lng="hu">Toki&oacute;</trd>
    <trd lng="nl">Tokio</trd>
    <trd lng="pl">Tokio</trd>
    <trd lng="pt">T&oacute;quio</trd>
    <trd lng="ru">&c_T;&c_o;&c_k;&c_i;&c_o;</trd>
    <trd lng="zh">东京 [<ind>dōng jīng</ind>]</trd>
</drv>
</art>
