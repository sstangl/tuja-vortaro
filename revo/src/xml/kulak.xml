<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kulak.xml,v 1.10 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kulak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kulak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kulak.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Ri&ccirc;i&gcirc;inta kamparano, kiajn kontra&ubreve;batalis bol&scirc;evismo en
      Sovetio
      komence de la 20a jarcento:
      <ekz>
        la partio nia, oni diras, <tld/>ojn nur kontentigas
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trd lng="bg">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kulak</trd>,
    <trd>velký rolník</trd>
  </trdgrp>
  <trd lng="de">Kulak</trd>
  <trd lng="en">kulak</trd>
  <trd lng="fr">koulak</trd>
  <trd lng="nl">koelak</trd>
  <trd lng="pl">ku&lstroke;ak</trd>
  <trdgrp lng="pt">
    <trd>c&uacute;laque</trd>,
    <trd>kulak</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>kulak</trd>,
    <trd>veľký roľník</trd>
  </trdgrp>
</drv>
</art>

