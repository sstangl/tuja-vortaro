      <ind>&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>,
    <trd>&c_d;&c_je;&c_z;&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="ord.publika0o">
  <kap>publika <tld/>o</kap>
