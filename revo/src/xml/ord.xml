      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol;<klr>
        (&c_s;&c_i;&c_n;&c_t;&c_a;&c_k;&c_s;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;)</klr></trd>
  </snc>
