<drv mrk="hag.H0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la nederlanda provinco
      <ref tip="malprt" cel="holand.SudH0o">Sud-Holando</ref>
      (4&#730;18'E, 52&#730;05'N):
      <ekz>[Ivo Lapenna estis] advokato-konsilisto de Albanio en la
      proceso kontra&ubreve; Britio pro
      la incidentoj en la Markolo de Korfuo anta&ubreve; la Internacia Kortumo
      en <tld/>o en 1947/48 
      <fnt><vrk>Biografiaj notoj pri Ivo Lapenna</vrk>, 
      <lok>Horizonto, n-ro 6/1984</lok>
      </fnt>
 </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_H;&c_a;&c_g;&c_a;</trd>
  <trd lng="ca">l'<ind>Haia</ind></trd>
  <trd lng="de">Den Haag</trd>
  <trd lng="el">&Chi;&alfa_ton;&gamma;&eta;</trd>
  <trd lng="en">The Hague</trd>
  <trd lng="es">La Haya</trd>
  <trd lng="fr">La <ind>Haye</ind></trd>
  <trd lng="gd">an <ind>H&aacute;ig</ind></trd>
  <trd lng="hu">H&aacute;ga</trd>
  <trd lng="it">L'Aia</trd>
  <trd lng="lat">Haga-Comitis</trd>
  <trdgrp lng="nl">
    <trd>Den Haag</trd>,
    <trd>'s-<ind>Gravenhage</ind></trd>
  </trdgrp>
  <trd lng="pl">Haga</trd>
  <trd lng="pt">Haia</trd>
  <trd lng="ru">&c_G;aa&c_g;a</trd>
  </drv>
</art>

