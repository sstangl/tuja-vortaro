<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: luksemb.xml,v 1.12 2016/02/21 11:10:16 revo Exp $">
<kap>
  <rad>Luksemburg</rad>/o
</kap>
<drv mrk="luksemb.L0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="luksemb.L0o.urb">
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:urboj">Urbo</ref> en la
      centro de la &scirc;tato
      <ref tip="malprt" cel="luksemb.L0o.sxtat">Luksemburgo</ref>
      (6&#730;8'E,
      49&#730;36'N):
    </dif>
    <trd lng="cs">Lucemburk</trd>
  </snc>
  <snc mrk="luksemb.L0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en
      <ref tip="malprt" cel="valon.V0ujo">Valonujo</ref> (suda
      Belgujo):
      <ekz>
        la E&ubreve;<klr>[rop]</klr>regiono Mozo-Rejno konsistas el la belgaj
        provincoj
        Limburgo
        kaj <tld/>o,
        el la suda parto de nederlanda Limburgo, kaj el la subregiono
        Akeno
        <fnt><aut>Jean-Marie JACQUES</aut>:
        <vrk>Senlime</vrk>,
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="luksemb.L0o.sxtat">
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente al <ref tip="vid"
      cel="belg.B0ujo">Belgujo</ref>, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, sude al
      <ref tip="vid" cel="franc.F0ujo">Francujo</ref>:
      <ekz>
        Fogra kolektis
        grason en deponejoj en suda Belgio, norda Francio kaj <tld/>o
        <fnt><aut>Roland ROTSAERT</aut>:
        <vrk><url ref="&Monato;/006610.html">Dioksino-krizo</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 07/1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">Lucembursko</trd>
  </snc>
  <trd lng="bg">&c_L;&c_ju;&c_k;&c_s;&c_je;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="be">&c_L;&c_ju;&c_k;&c_s;&c_e;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="br">Luksembourg</trd>
  <trd lng="ca">Luxemburg</trd>
  <trd lng="da">Luxembourg</trd>
  <trd lng="de">Luxemburg</trd>
  <trd lng="en">Luxemburg</trd>
  <trd lng="el">&Lambda;&omikron;&ypsilon;&xi;&epsilon;&my;&beta;&omikron;&ypsilon_acute;&rho;&gamma;&omikron;</trd>
  <trd lng="es">Luxemburgo</trd>
  <trd lng="fa">&a_l;&a_w;&f_k;&a_z;&a_A;&a_m;&a_b;&a_w;&a_r;&f_g;</trd>
  <trd lng="fi">Luxemburg</trd>
  <trd lng="fr">Luxembourg</trd>
  <trd lng="gd">Lucsamburg</trd>
  <trd lng="hu">Luxemburg</trd>
  <trd lng="id">Luksemburg</trd>
  <trd lng="is">L&uacute;xemborg</trd>
  <trd lng="it">Lussemburgo</trd>
  <trd lng="lt">Liuksemburgas</trd>
  <trd lng="lv">Luksemburga</trd>
  <trd lng="nl">Luxemburg</trd>
  <trd lng="pl">Luksemburg</trd>
  <trd lng="pt">Luxemburgo</trd>
  <trd lng="ru">&c_L;&c_ju;&c_k;&c_s;&c_je;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="sk">Luxemburg</trd>
  <trd lng="sl">Luksemburg</trd>
  <trd lng="sv">Luxemburg</trd>
  <trd lng="tr">L&uuml;ksenburg</trd>
</drv>
</art>
