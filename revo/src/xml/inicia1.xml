<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: inicia1.xml,v 1.13 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>iniciativ</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="inicia1.0o">
  <kap><tld/>o</kap>
  <snc mrk="inicia1.0o.iniciato">
    <dif>
      <ref tip="dif" cel="inicia.0o">Iniciato</ref>:
      <ekz>
        ne atendante, ke aliaj prenu sur sin la <tld/>on, &ccirc;iu societo
        a&ubreve; kongreso aparte decidu
        <fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kezdem&eacute;nyez&eacute;s</trd>,
      <trd>iniciat&iacute;va</trd>
    </trdgrp>
    <trd lng="pt">iniciativa<klr tip="amb"> (a&ccedil;&atilde;o)</klr></trd>
  </snc>
  <snc mrk="inicia1.0o.iniciatemo">
    <dif>
      <ref tip="dif" cel="inicia.0emo">Iniciatemo</ref>:
      <ekz>
        baras al vi la vojon <klr>[...]</klr> la manko de <tld/>o
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        li estis tro <ind><tld/>-hava</ind> kaj faris sin
        netolerebla per sia entreprenemo
        <fnt><bib>UrdHadda</bib><lok>p. 9</lok></fnt>.
        <trdgrp lng="fr">
          <trd>entreprenant<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>dynamique<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>audacieux<klr tip="ind"> (plein d'initiative)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>kezdem&eacute;nyez&odblac;k&eacute;sz</trd>,
          <trd>&ouml;tletgazdag</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="pt">
     <trd>iniciativa<klr tip="amb"> (capacidade)</klr></trd>,
     <trd>esp&iacute;rito de <ind>iniciativa</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>iniciativa</trd>,
    <trd>podnět</trd>,
    <trd>popud</trd>
  </trdgrp>
  <trd lng="en">initiative</trd>
  <trdgrp lng="fr">
    <trd>initiative<klr tip="ind"> (ts)</klr></trd>,
    <trd>entreprise<klr tip="ind"> (initiative)</klr></trd>,
    <trd>esprit d'<ind>initiative</ind></trd>,
    <trd>esprit d'<ind>entreprise</ind></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&odblac;k&eacute;szs&eacute;g</trd>
  <trd lng="nl">initiatief</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>iniciatíva</trd>,
    <trd>podnet</trd>,
    <trd>popud</trd>
  </trdgrp>
</drv>
</art>
