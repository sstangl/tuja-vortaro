<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxtomatik.xml,v 1.1 2013/01/28 11:10:10 revo Exp $">
<kap>
  <rad>a&ubreve;tomatik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="auxtomatik.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxtomatik.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Arto programi
      <ref tip="vid" cel="auxtom.0o">a&ubreve;tomatojn</ref>:
      <ekz>
        raporto pri liaj esploroj pri <tld/>o
        aperis en la revuo <ctl>Scientific American</ctl>
        en 1915
        <fnt>
         <aut>T. del Barrio</aut>,
         <vrk><url
         ref="http://groups.yahoo.com/group/per-esperanto-bazascienco/message/222">
         Leonardo Torres Quevedo</url></vrk>,
         <lok>per-esperanto-bazascienco, 2002-04-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="robotik.0o.MAS">robotiko</ref>
    </refgrp>
  </snc>
  <trd lng="fr">automatique<klr> (subst.)</klr></trd>
</drv>

<drv mrk="auxtomatik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxtomatik.0isto.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="auxtomatik.0o.MAS"><tld/>o</ref>,
      pri teorio kaj realigo de a&ubreve;tomatoj:
      <ekz>
        la amba&ubreve;flanka konverto de Laplace <klr>...</klr>
        estas uzata interalie de <tld/>istoj 
        kaj anka&ubreve; uzata en statistiko
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Laplaca_konverto">
          laplaca konverto</url>, 2013-01-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">automaticien</trd>
</drv>
</art>

