<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: piment.xml,v 1.10 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>piment</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="piment.0o">
  <kap><tld/>o</kap>
  <snc mrk="piment.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> el la
      akregustaj
      sekigitaj beroj de la <tld/>ujo:
      <ekz>
        <tld/>o fajnigas &ccirc;efe supojn, sa&ubreve;cojn,
        marina&jcirc;ojn, kukojn kaj likvorojn
        <fnt><bib>Viki</bib>, <lok>pimento</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="bg">
      <trd>&c_ja;&c_m;&c_a;&c_j;&c_k;&c_a; &c_p;&c_i;&c_p;&c_je;&c_r;</trd>,
      <trd>&c_b;&c_a;&c_h;&c_a;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Piment</trd>
    <trd lng="en">allspice</trd>
    <trd lng="hu">szegf&udblac;bors <klr
    tip="amb">(f&udblac;szer)</klr></trd>
    <trdgrp lng="nl">
      <trd>piment</trd>,
      <trd>jamaicapeper</trd>
    </trdgrp>
  </snc>
  <snc mrk="piment.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Pimenta</trd>), kies beroj estas uzataj kiel spico.
    </dif>
    <trd lng="de">Pimentbaum</trd>
    <trd lng="en">pimenta</trd>
    <trd lng="hu">szegf&udblac;bors <klr tip="amb">(fa)</klr></trd>
    <trd lng="nl">pimentboom</trd>
    <trdgrp lng="pt">
      <trd>piment&atilde;o</trd>,
      <trd>pimento</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>anglické koření</trd>,
    <trd>jamajský pepř</trd>,
    <trd>nové koření</trd>,
    <trd>piment</trd>
  </trdgrp>
  <trd lng="fr">piment de la jama&iuml;que</trd>
  <trdgrp lng="sk">
    <trd>nové korenie</trd>,
    <trd>piment</trd>
  </trdgrp>
</drv>
</art>
