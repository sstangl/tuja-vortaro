<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fendr.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap>
<rad>fendr</rad>/o
</kap>
<drv mrk="fendr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      &Scirc;irmilo, kiun oni starigas anta&ubreve; la kameno por malhelpi,
      ke la brulanta karbo falu sur la plankon.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_t;&c_y; <klr tip="amb">(&c_k;&c_a;&c_m;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_m;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">zástěna</trd>
  <trd lng="de">Kaminvorsetzer</trd>
  <trdgrp lng="fr">
    <trd>garde-feu</trd>,
    <trd>pare-&eacute;tincelles</trd></trdgrp>
  <trd lng="hu">k&aacute;lyhaellenz&odblac;</trd>
  <trd lng="nl">haardscherm</trd>
  <trd lng="pt">guarda-fogo</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_i;&c_n;&c_n;&c_a;&c_ja; &c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">plechová predložka pred pecou</trd>
</drv>
</art>
