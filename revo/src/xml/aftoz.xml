<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aftoz.xml,v 1.13 2013/10/07 15:10:47 revo Exp $">
<kap>
  <rad>aftoz</rad>/o
</kap>
<drv mrk="aftoz.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Brutara <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> kies simptomoj estas
      <ref tip="vid" cel="aft.0o.epizootia">aftoj</ref> &ccirc;e la brusto,
mamo
      kaj hufoj; afta malsano:
      <ekz>
        &ccirc;iutage miloj da e&ccirc; ne malsanaj bestoj estas mortigataj,
la&ubreve;dire
        por "preventi", ke <tld/>o disvasti&gcirc;os
        <fnt><aut>Gerrit Berveling</aut>:
        <vrk><url ref="&Monato;/007440.html">Holoka&ubreve;sto</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 05/2001</lok>.
        </fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_ja;&c_sh;&c_ch;&c_u;&c_r; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
    <trdgrp lng="ca">
      <trd>glossopeda</trd>,
      <trd>febre aftosa</trd>
    </trdgrp>
    <trd lng="de">Maul- und Klauen<ind>seuche</ind></trd>
    <trd lng="da">mund- og klovsyge</trd>
    <trdgrp lng="en">
      <trd>foot-and-mouth disease <klr>(Britujo)</klr></trd>,
      <trd>hoof-and-mouth disease <klr>(Usono)</klr></trd>
    </trdgrp>
    <trd lng="es">fiebre aftosa</trd>
    <trd lng="fr">fi&egrave;vre aphteuse</trd>
    <trd lng="he">&mem;&het;&lamed;&tav; &he;&pe;&he; &vav;&he;&tet;&lamed;&pe;&yod;&yod;&fmem;</trd>
    <trd lng="hu">sz&aacute;j- &eacute;s
k&ouml;r&ouml;mf&aacute;j&aacute;s</trd>
     <trd lng="it">afta epizootica</trd>
    <trdgrp lng="nl">
      <trd>mond- en klauwzeer</trd>,
      <trd>MKZ</trd>
    </trdgrp>
    <trd lng="pl">pryszczyca</trd>
    <trd lng="pt">febre aftosa</trd>
    <trd lng="ru">&c_ja;&c_shch;&c_u;&c_r;</trd>
    <trd lng="sk">slinta&ccaron;ka a kr&iacute;va&ccaron;ka</trd>
    <trd lng="sv">mul- och kl&ouml;vsjuka</trd>
  </snc>
</drv>
</art>
