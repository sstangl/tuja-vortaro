<vortaro>
<art mrk="$Id: etnogr.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap><ofc>2</ofc>
<rad>etnografi</rad>/o</kap>
<drv mrk="etnogr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Priskriba scienco pri konkretaj
      <ref tip="prt" cel="etn.0o">etnoj</ref>,
      iliaj moroj, vivmaniero,
      <ref tip="prt" cel="folklo.0o">folkloro</ref> ktp;
      priskriba <ref tip="super" cel="etnolo1.0o">etnologio</ref>.
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_t;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_t;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="ca">etnografia</trd>
  <trdgrp lng="cs">
    <trd>etnografie</trd>,
    <trd>národopis</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ethnographie</trd>,
    <trd>beschreibende <ind>V&ouml;lkerkunde</ind></trd>
  </trdgrp>
  <trd lng="en">ethnography</trd>
  <trd lng="es">ethnograph&iacute;a</trd>
  <trd lng="fa">&a_q;&a_w;&a_m;&zwnj;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">ethnographie</trd>
  <trdgrp lng="hu">
    <trd>n&eacute;prajz</trd>,
    <trd>etnogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="nl">etnografie</trd>
  <trdgrp lng="pl">
    <trd>etnografia</trd>,
    <trd>ludoznawstwo</trd>
  </trdgrp>
  <trd lng="pt">etnografia</trd>
  <trd
  lng="ru">&c_e;&c_t;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">národopis</trd>
  <trd lng="sv">etnografi</trd>
</drv>
</art>
