<vortaro>
<art mrk="$Id: arcx.xml,v 1.23 2015/09/16 11:10:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ar&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arcx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="arcx.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Vergeto kun stre&ccirc;itaj &ccirc;evalharoj a&ubreve; nilon&scirc;nuretoj
      por ludi sur <ref tip="malprt"
      cel="instru2.kord0o.MUZ">kordinstrumento</ref>:
      <ekz>
        se vi prenis la violonon prenu anka&ubreve; la ar&ccirc;on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_m;&c_y;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_malmol;&c_k;<klr tip="ind"> (&c_m;&c_u;&c_z;&c_i;&c_k;&c_a;)</klr></trd>
  <trd lng="de">Bogen</trd>
  <trd lng="br">gwareg<klr tip="ind"> (sonerezh)</klr></trd>
  <trd lng="en">bow</trd>
  <trd lng="es">arco <klr tip="amb">(de instrumentos de
  cuerda)</klr></trd> 
  <trd lng="fr">archet</trd>
  <trd lng="hu">von&oacute;</trd>
  <trd lng="nl">strijkstok</trd>
  <trd lng="pl">smyczek</trd>
  <trd lng="pt">arco <klr tip="amb"></klr>(de instrumentos de
  corda)</trd>
  <trd lng="ru">&c_s;&c_m;&c_y;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">sl&aacute;k</trd>
  <trd lng="sv">str&aring;ke</trd>
</drv>
</art>
