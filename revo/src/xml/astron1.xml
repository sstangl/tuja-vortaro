<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: astron1.xml,v 1.22 2015/07/04 08:37:31 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>astronomi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="astron1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la astroj:
      <ekz>
        la leganto ricevas koncizan bazan kurson pri elementa
        <tld/>o
        <fnt>
        <aut>Garbhan Macaoidh</aut>,
        <vrk>Per ardua ad astra - Tra kruta&jcirc;oj al la astroj (David
        Galadi-Enriquez kaj Amri Wandel: La kosmo kaj ni)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 6a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>astronomie</trd>,
    <trd>hvězdářství</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Astronomie</trd>,
    <trd>Sternkunde</trd>,
    <trd>Himmelskunde</trd>
  </trdgrp>
  <trd lng="en">astronomy</trd>
  <trd lng="es">astronom&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_n;&a_j;&a_w;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">astronomie</trd>
  <trdgrp lng="hu">
    <trd>csillag&aacute;szat</trd>,
    <trd>asztron&oacute;mia</trd>
  </trdgrp>
  <trd lng="it">astronomia</trd>
  <trdgrp lng="nl">
    <trd>astronomie</trd>,
    <trd>sterrenkunde</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>astron&oacute;mia</trd>,
    <trd>astronómia</trd>,
    <trd>hvezdárstvo</trd>
  </trdgrp>
  <trd lng="sv">astronomi</trd>
</drv>

<drv mrk="astron1.0a">
  <kap><tld/>a</kap>
  <snc mrk="astron1.0a.AST">
    <dif>
      Rilata al astronomio:
      <ekz>
        tre malgranda da&ubreve;ro la&ubreve; <tld/>ia temposkalo
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Planeduma_nebulozo">Planeduma
          nebulozo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="astron1.0a.KOM">
    <dif>
      Tiel grandega, kiel la kalkul-elementoj de <tld/>o:
      <ekz>
        <tld/>aj nombroj da botoj estis produktitaj
        <fnt>
          <aut>George Orwell, trad. Donald Broadribb</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">Mil
          Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>p. 30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">astronomique</trd>
</drv>

<drv mrk="astron1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="astron.0o">astronomo</ref>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
  <trd lng="cs">astronom</trd>
  <trdgrp lng="de">
    <trd>Astronom</trd>,
    <trd>Sternkundler</trd>,
    <trd>Himmelskundler</trd>
  </trdgrp>
  <trd lng="en">astronomer</trd>
  <trd lng="es">astr&oacute;nomo</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_m;&a_n;&a_j;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">astronome</trd>
  <trdgrp lng="hu">
    <trd>csillag&aacute;sz</trd>,
    <trd>asztron&oacute;mus</trd>
  </trdgrp>
  <trd lng="it">astronomo</trd>
  <trdgrp lng="nl">
    <trd>astronoom</trd>,
    <trd>sterrenkundige</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>astron&oacute;m</trd>,
    <trd>astronóm</trd>
  </trdgrp>
  <trd lng="sv">astronom</trd>
</drv>
</art>
