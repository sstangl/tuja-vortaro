<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: semajn.xml,v 1.31 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>semajn</rad>/o
</kap>

<drv mrk="semajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="semajn.0o.tagsepo">
    <dif>
      <ref tip="lst" cel="dauxr.0o" lst="voko:da&ubreve;roj">Da&ubreve;ro</ref> 
      de sep sinsekvaj <ref tip="prt" cel="tag.0o.diurn">tagoj</ref>:
      <ekz>
        post paso de unu <tld/>o la ju&gcirc;isto estis vokita Londonon
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="semajn.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="period.0o.dauxro">Periodo</ref> de sep tagoj
      la&ubreve;orde tradicie komenci&gcirc;anta je
      <ref tip="prt" cel="dimanc.0o">diman&ccirc;o</ref> a&ubreve;
      <ref tip="prt" cel="lund.0o">lundo</ref>:
      <ekz>
        la sepan tagon de la <tld/>o Dio elektis, ke &gcirc;i estu 
        pli sankta, ol la ses unuaj tagoj
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:semajntagoj"><tld/>tagoj</lstref>
    <adm>Estas kontrolinda, &ccirc;u iuj kulturoj havas oficiale semajnojn kun
    alia nombro da tagoj</adm>
  </snc>
  <snc mrk="semajn.0o.labortagoj">
    <dif>
      La labortagoj de la <tld/>o <sncref ref="semajn.0o.KAL"/>, en
      kristanaj landoj t.e. lundo &gcirc;is vendredo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trd lng="ca">setmana</trd>
  <trd lng="cs">t&yacute;den</trd>
  <trd lng="de">Woche</trd>
  <trd lng="en">week</trd>
  <trd lng="es">semana</trd>
  <trd lng="fr">semaine</trd>
  <trd lng="he">&shin;&bet;&vav;&ayin;</trd>
  <trd lng="hu">h&eacute;t<klr tip="ind"> (h&eacute;t nap)</klr></trd>
  <trdgrp lng="id">
    <trd>minggu</trd>,
    <trd>pekan</trd>
  </trdgrp>
  <trd lng="it">settimana</trd>
  <trd lng="nl">week</trd>
  <trd lng="pl">tydzie&nacute;</trd>
  <trd lng="pt">semana</trd>
  <trd lng="ro">s&abreve;pt&abreve;m&acirc;n&abreve;</trd>
  <trd lng="ru">&c_n;&c_je;&c_d;&c_je;&c_l;&c_ja;</trd>
  <trd lng="sk">t&yacute;&zcaron;de&ncaron;</trd>
  <trd lng="sv">vecka</trd>
</drv>

<drv mrk="semajn.0a">
  <kap><tld/>a</kap>
  <snc mrk="semajn.0a.KAL">
    <dif>
      Rilata al <tld/>o, precipe okazanta unufoje en <tld/>o:
      <ekz>
        hodia&ubreve; miaj gesinjoroj havas sian <tld/>an vesperkunvenon
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">setmanal</trd>
  <trd lng="cs">týdenní</trd>
  <trd lng="es">semanal</trd>
  <trd lng="fr">hebdomadaire</trd>
  <trd lng="it">settimanale</trd>
  <trd lng="pl">cotygodniowy</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
   
<trd>&c_je;&c_zh;&c_je;&c_n;&c_je;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>týždenný</trd>,
    <trd>týždňový</trd>
  </trdgrp>
</drv>

<drv mrk="semajn.0fino">
  <kap><tld/>fino</kap>
  <snc mrk="semajn.0fino.KAL">
    <dif>
      La tagoj de la semajno, dum kiuj oni kutime ne laboras.
      En multaj landoj tio estas sabato kaj diman&ccirc;o.
    </dif>
  </snc>
  <trd lng="ca">cap de setmana</trd>
  <trd lng="cs">víkend</trd>
  <trd lng="de">Wochenende</trd>
  <trd lng="en">weekend</trd>
  <trd lng="es">fin de semana</trd>
  <trd lng="id"><ind>akhir</ind> pekan</trd>
  <trd lng="it">fine <ind>settimana</ind></trd>
  <trd lng="pt">fim de <ind>semana</ind></trd>
  <trd lng="sk">koniec týždňa</trd>
</drv>

</art>
