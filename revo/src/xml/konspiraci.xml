  </snc>
  <trd lng="ca">conspiraci&oacute;</trd>
  <trd lng="fr">conspiration</trd>
  <trd lng="nl">samenzwering<klr tip="ind"> (ontr.)</klr></trd>
  <trdgrp lng="pl">
  <trd>konspiracja</trd>,
  <trd>spisek</trd>
</trdgrp>
  <trdgrp lng="pt">
    <trd>conspira&ccedil;&atilde;o</trd>,
    <trd>conjura&ccedil;&atilde;o</trd>,
    <trd>conluio</trd>,
    <trd>trama</trd>,    
    <trd>maquina&ccedil;&atilde;o</trd>
  </trdgrp>
</drv>
</art>

