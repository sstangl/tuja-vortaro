<kap>
  <rad>konspiraci</rad>/o
</kap>
<drv mrk="konspiraci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="konspiraci.0o.konspiro">
    <ref tip="sin" cel="konspi.0o">konspiro</ref>
