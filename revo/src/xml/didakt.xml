<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: didakt.xml,v 1.22 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>didaktik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="didakt.0o">
  <kap><tld/>o</kap>
  <snc mrk="didakt.0o.PED">
    <uzo tip="fak">PED</uzo>
    <ref tip="dif" cel="pedago1.0o">pedagogio</ref>
    <ekz>
      <tld/>a poemo estas versa verko, en kiu oni klarigas te&hcirc;nikan
      temon.
    </ekz>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_d;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">did&agrave;ctica</trd>
  <trdgrp lng="cs">
    <trd>didaktika</trd>,
    <trd>nauka o výchově</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Didaktik</trd>,
    <trd>Unterrichtslehre</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>didactics</trd>,
    <trd>pedagogy</trd>
  </trdgrp>
  <trd lng="es">did&aacute;ctica</trd>
  <trdgrp lng="fr">
    <trd>didactique<klr> (subst.)</klr></trd>,
    <trd>p&eacute;dagogie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>didaktika</trd>,
    <trd>oktat&aacute;stan</trd>
  </trdgrp>
  <trd lng="pl">dydaktyka</trd>
  <trd lng="pt">did&aacute;tica</trd>
  <trd lng="ru">&c_d;&c_i;&c_d;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>didaktika</trd>,
    <trd>náuka o výchove</trd>
  </trdgrp>
</drv>

<drv mrk="didakt.0a">
  <kap><tld/>a</kap>
  <snc mrk="didakt.0a.PED">
    <ref tip="dif" cel="instru.pri0a">priinstrua</ref>
    <ekz>
      dum pluraj pa&gcirc;oj la a&ubreve;toro <klr>...</klr> prezentas
      detalan tezon en <tld/>a stilo
      <fnt>
        <aut>D. Broadribb</aut>,
        <vrk>Vi ne hontos transdoni vian ekzempleron</vrk>,
        <bib>Monato</bib>,
        <lok>2002:3, p. 25-26a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_d;&c_a;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">did&agrave;ctic</trd>
  <trdgrp lng="de">
    <trd>didaktisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>didactical</trd>,
    <trd>pedagogical</trd>
  </trdgrp>
  <trd lng="es">did&aacute;ctico</trd>
  <trdgrp lng="fr">
    <trd>didactique</trd>,
    <trd>p&eacute;dagogique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>didaktikus</trd>,
    <trd>didaktikai</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dydaktyczny</trd>,
    <trd>pouczaj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="sk">didaktick&yacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_d;&c_a;&c_k;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
