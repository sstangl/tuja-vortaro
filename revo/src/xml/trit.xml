<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trit.xml,v 1.10 2015/12/26 14:10:24 revo Exp $">
<kap>
  <rad>trit</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="trit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="trit.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el tri sinsekvaj tonoj de diatona gamo.
    </dif>
    <trd lng="en">tritone</trd>
  </snc>
  <snc mrk="trit.0o.de.toniko">
    <dif>
      Tria tono de gamo.
    </dif>
    <trd lng="en">third</trd>
  </snc>
  <refgrp tip="sin">
    <ref cel="terci.0o">tercio</ref>
  </refgrp>
  <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">tercera</trd>
  <trd lng="de">Terz</trd>
  <trd lng="es">tercera</trd>
  <trdgrp lng="fr">
    <trd>tierce<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">terc</trd>
  <trd lng="nl">terts</trd>
  <trd lng="pl">tercja <klr tip="amb">(muzyka)</klr></trd>
  <trd lng="pt">ter&ccedil;a</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">interval troch t&oacute;nov</trd>
  <trd lng="cs">interval t&rcaron;&iacute; t&oacute;n&uring;</trd>
</drv>
</art>

