<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nul0.xml,v 1.45 2015/07/04 08:37:40 revo Exp $">
    <kap>
      <ofc>8</ofc>
      <rad>nul</rad>
    </kap>

    <drv mrk="nul0.0">
      <kap><ofc>8</ofc><tld/></kap>
      <mlg>0</mlg>
      <snc mrk="nul0.0.nombro">
        <dif>
          La <ref tip="super" cel="numera.0o.gra">numeralo</ref>
          esprimanta la kvanton <ref tip="sin"
          cel="neniom.0">neniom</ref>,
          la plej malgrandan nenegativan nombron:
          <ekz>
          apud la Fundamenta substantivo &#8216;<tld/>o&#8217; estas oficialigita la
          numeralo &#8216;<tld/>&#8217;<fnt>AdE</fnt>;
          </ekz>
          <ekz>
            la nombro <frm>0</frm> estas elparolata <tld/>
            <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>.
          </ekz>
        </dif>
         <ref tip="lst" cel="nombr.0o.MAT"
         lst="voko:nombroj" val="0">nombro</ref>
      </snc>
      <snc mrk="nul0.0.cifero">
        <dif>
          La nomo de la cifero <frm>0</frm> uzata &ccirc;e vo&ccirc;lego de
          numeraloj a&ubreve; numeroj:
          <ekz>
            <tld/> komo <tld/> <tld/> tri
            <klr>(t.e. <frm>0,003</frm>)</klr>;
          </ekz><ekz>
            la agento <tld/> <tld/> sep.
          </ekz>
        </dif>
        <rim>
          Eble estus pli konvene rigardi kaj skribi &ccirc;i tiun
          <tld/><sncref ref="nul0.0.cifero"/> kiel apostrofan formon de
          <ref tip="sin" cel="nul0.0o.cifero"><tld/>o<sncref/></ref>,
          do kiel <tld/>'.
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
      <trd lng="ru">&c_n;&c_o;&c_l;&c_mol;</trd>
      <trd lng="id">nol</trd>
    </drv>

    <drv mrk="nul0.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="nul0.0o.cifero">
        <dif>
          La <ref tip="super" cel="cifer.0o.MAT">cifero</ref>
          <frm>0</frm>, uzata por prezenti la nombron <tld/>,
          a&ubreve; por obligi la valoron de ciferoj, &ccirc;e kies 
dekstro &gcirc;i estas
          lokita:
          <ekz>
            en la nombroprezento <frm>001,2050</frm> la komencaj kaj
            la vostaj <tld/>oj estas sensugnifaj kaj oni povas ilin
            forigi, lasante nur la signifajn ciferojn <frm>1,205</frm>.
          </ekz>
        </dif>
        <trd lng="es">cero</trd>
        <trd lng="fr">z&eacute;ro</trd>
        <trdgrp lng="hu">
          <trd>nulla</trd>,
          <trd>z&eacute;rus</trd>,
          <trd>z&eacute;r&oacute;</trd>
        </trdgrp>
        <trd lng="pt">zero</trd>
      </snc>
      <snc mrk="nul0.0o.io_senvalora">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Io tute senvalora a&ubreve; iu tute senkapabla:
          <ekz>
            sen mono oni estas <tld/>o, kun mono sa&gcirc;ulo
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            &ccirc;iuj nacioj estas anta&ubreve; li kiel nenio, estas
            rigardataj de li kiel <tld/>o kaj senvalora&jcirc;o<fnt>Z</fnt>;
          </ekz><ekz>
            &ccirc;e manko de adeptoj la utilo de la lingvo prezentos
            <tld/>on<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>nul<klr tip="ind"> (personne incapable)</klr></trd>,
          <trd>z&eacute;ro</trd>,
          <trd>rien</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>semmi</trd>,
          <trd>nulla</trd>
        </trdgrp>
        <trd lng="pt">nada</trd>
      </snc>
      <snc mrk="nul0.0o.neuxtra">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>KompLeks</bib><lok>ne&ubreve;tra elemento</lok></fnt>
        <dif>
          <ref tip="super" cel="neuxtr.0aelemento">Ne&ubreve;tra 
elemento</ref>
          rilate al adicie signata <ref
          cel="operac.0o.MAT">operacio</ref>.
        </dif>
        <trd lng="de">Nullelement</trd>
        <trdgrp lng="en">
          <trd>null element</trd>,
          <trd>zero</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>&eacute;l&eacute;ment nul</trd>,
          <trd>z&eacute;ro</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>nulla</trd>,
          <trd>z&eacute;rus</trd>
        </trdgrp>   
        <trd lng="pl">zero</trd>
        <trd lng="ru">&c_n;&c_u;&c_l;&c_mol;</trd>
      </snc>
  <trd lng="be">&c_n;&c_u;&c_l;&c_mol;</trd>
  <trd lng="br">zero</trd>
  <trdgrp lng="cs">
    <trd>nula</trd>,
    <trd>nula <klr>(při odpočítávání)</klr></trd>
  </trdgrp>
  <trd lng="de">Null</trd>
  <trd lng="he">&alef;&pe;&samekh;</trd>
  <trd lng="id">nol</trd>
  <trd lng="nl">nul</trd>
  <trd lng="no">null</trd>
  <trdgrp lng="pt">
        <trd>zero</trd>,
        <trd>nulo</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_n;&c_u;&c_l;&c_mol;</trd>,
        <trd>&c_n;&c_o;&c_l;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>nič</trd>,
    <trd>nula</trd>
  </trdgrp>
  <trd lng="tp">ala</trd>
  <trdgrp lng="zh">
        <trd>&#38646;<klr> [l&iacute;ng]</klr></trd>,
        <trd>&#12295;</trd>
      </trdgrp>
</drv>

    <drv mrk="nul0.0a">
      <kap><tld/>a</kap>
      <snc mrk="nul0.0a.numero">
        <dif>
          Kies <ref cel="numer.0o">numero</ref> estas <frm>0</frm>:
          <ekz>
            la <tld/>a, la tria kaj la kvina vertikaloj de la vektoro
            estas lineare sendependaj.
          </ekz>
        </dif>
      </snc>
      <snc>
        <dif>
          Neefektivi&gcirc;inta; tute senvalora:
          <ekz>
            la rezultato estis <tld/>a;
          </ekz><ekz>
            la utileco estos <tld/>a<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="van.0a">vana</ref>,
          <ref cel="frukt.sen0a">senfrukta</ref>
        </refgrp>
        <trd lng="fr">nul</trd>
        <trdgrp lng="hu">
          <trd>nulla</trd>,
          <trd>semmi</trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_u;&c_l;&c_je;&c_v;&c_o;&c_j;</trd>
      </snc>
      <snc mrk="nul0.0a.MAT">
        <uzo tip="fak">MAT</uzo>
        <subsnc mrk="nul0.0a.elemento">
          <dif>
            Egala al <ref cel="nul0.0o.neuxtra"><tld/>o</ref>;
            <klr>(p.p. <ref cel="funkci.0o.MAT">funkcio</ref>)</klr>
            alprenanta la valoron <tld/>o.
          </dif>
        </subsnc>
        <subsnc mrk="nul0.0a.angulo">
          <dif>
            <klr>(p.p. <ref cel="angul.0o.figuro">angulo</ref>)</klr>
            Situanta inter du egalaj lateroj, do malplena; kies mezuro
            egalas al 0&#730;.
          </dif>
        </subsnc>
        <trd lng="de">Null-</trd>
        <trdgrp lng="en">
          <trd>null</trd>,
          <trd>zero</trd>
        </trdgrp>
        <trd lng="fr">nul</trd>
        <trd lng="hu">null-</trd>   
        <trd lng="pl">zerowy</trd>
        <trd lng="ru">&c_n;&c_u;&c_l;&c_je;&c_v;&c_o;&c_j;</trd>
      </snc>
  <trd lng="be">&c_n;&c_u;&c_l;&c_ja;&c_v;&c_y;</trd>
  <trd lng="br">null</trd>
  <trdgrp lng="cs">
    <trd>neexistující</trd>,
    <trd>nulový</trd>
  </trdgrp>
  <trd lng="es">nulo</trd>
  <trd lng="he">&alef;&pe;&samekh;&yod;</trd>
  <trd lng="nl">waardeloos</trd>
  <trd lng="no">verdil&oslash;s</trd>
  <trd lng="pt">nulo</trd>
  <trd lng="sk">nulový</trd>
</drv>

    <drv mrk="nul0.0i">
      <kap><tld/>i</kap>
      <snc mrk="nul0.0i.KOMUNE">
        <dif>
          Esti <tld/>a, esti <tld/>o.
        </dif>
        <trd lng="de">Null sein</trd>
        <trdgrp lng="hu">
          <trd>nulla</trd>,
          <trd>semmi</trd>
        </trdgrp>
      </snc>
      <snc mrk="nul0.0i.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>MatVort</bib></fnt>
        <ref tip="dif" cel="nul0.0igxi.MAT">nuli&gcirc;i.</ref>
        <rim>
          Eblas vidi nuancon inter <ctl><tld/>i</ctl> kaj
          <ctl><tld/>i&gcirc;i</ctl>, nome ke la unua koncernas nevariajn
          objektojn, dum la dua prefere apliki&gcirc;as al variabloj 
a&ubreve;
          funkcioj, kiuj en difinitaj cirkonstancoj alprenas <tld/>an
          valoron. Tia
          nuanco tamen ne tiom gravas, &ccirc;ar en amba&ubreve; okazoj 
oni ne
          hezitus uzi la saman adjektivon <ctl><tld/>a</ctl>, lasante la
          kuntekston precizigi la celitan sencon.
        </rim>
        <trdgrp lng="de">
          <trd>Null werden</trd>,
          <trd>zu <ind>Null aufgehen</ind></trd>
        </trdgrp>
        <trd lng="hu">elt&udblac;nik</trd>   
      </snc>
  <trd lng="cs">rovnat se nule</trd>
  <trd lng="sk">rovnať sa nule</trd>
</drv>

    <drv mrk="nul0.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>PIV2</bib></fnt>
      <ref tip="dif" cel="nul0.0iganto.de_funkcio"><tld/>iganto 
<sncref/></ref>
      <trdgrp lng="sk">
    <trd>miesto</trd>,
    <trd>v ktorom funkcia nadobúda hodnotu nula</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>místo</trd>,
    <trd>ve kterém funkce nabývá hodnotu nula</trd>
  </trdgrp>
</drv>

    <drv mrk="nul0.0igi">
      <kap><tld/>igi</kap>
      <snc mrk="nul0.0igi.KOMUNE">
        <dif>
          Senvalorigi, senefikigi, oficiale deklari senvalora, igi
          <tld/>a:
          <ekz>
            <tld/>igi
            ju&gcirc;on, kontrakton, baloton, anta&ubreve;an regularon.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="kaduk">kadukigi</ref>,
          <ref cel="valid">senvalidigi</ref>,
          <ref cel="parali.0i">paralizi</ref>.
        </refgrp>
        <trdgrp lng="hu">
          <trd>megsemmis&iacute;t</trd>,
          <trd>annul&aacute;l</trd>
        </trdgrp>
        <trd lng="id">[<ind>batal</ind>] membatalkan</trd>
      </snc>
      <snc mrk="nul0.0igi.MAT">
        <uzo tip="fak">MAT</uzo>
        <uzo tip="klr">(fakula &jcirc;argono)</uzo>
        <dif>
          <klr>(funkcion, polinomon, esprimon)</klr>
          Esti <tld/>iganto de &gcirc;i, doni al &gcirc;i nulan valoron:
          <ekz>
            trovi &ccirc;iujn argumentojn, kiuj <tld/>igas la funkcion
            <frm>sin(<k>x</k><sup>2</sup>+<k>x</k>)</frm>;
          </ekz><ekz>
            la funkcio alprenas maksimuman valoron, kiam ni <tld/>igas
            &gcirc;ian variablon.
          </ekz>
        </dif>
        <trd lng="en">nullify</trd>
        <trd lng="hu">elt&uuml;ntet</trd>   
        <trd lng="pl">zerowa&cacute;</trd>
        <trd lng="ru">&c_o;&c_b;&c_n;&c_u;&c_l;&c_i;&c_t;&c_mol;</trd>
      </snc>
  <trd lng="br">nulla&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>anulovat</trd>,
    <trd>derogovat</trd>,
    <trd>odřeknout</trd>,
    <trd>rušit</trd>,
    <trd>zrušit <klr>(činit neplatným)</klr></trd>
  </trdgrp>
  <trd lng="de">annullieren</trd>
  <trd lng="es">anular</trd>
  <trd lng="fr">annuler</trd>
  <trdgrp lng="he">
        <trd>&lamed;<ind>&alef;&pe;&samekh;</ind></trd>,
        <trd>&lamed;<ind>&bet;&tet;&lamed;</ind></trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>annuleren</trd>,
        <trd>te <ind>niet</ind> doen</trd>
      </trdgrp>
  <trdgrp lng="no">
        <trd>annulere</trd>,
        <trd>tilintetgj&oslash;re</trd>
      </trdgrp>
  <trd lng="pt">anular</trd>
  <trdgrp lng="ru">
        
<trd>&c_a;&c_n;&c_n;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_v;&c_je;&c_s;&c_t;&c_i; &c_k; 
&c_n;&c_u;&c_l;&c_ju;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>anulovať</trd>,
    <trd>odvolať</trd>,
    <trd>zrušiť</trd>
  </trdgrp>
</drv>

    <drv mrk="nul0.0iganto">
      <kap><tld/>iganto</kap>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <subsnc mrk="nul0.0iganto.de_funkcio">
          <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
          <dif>
            <klr>(de <ref cel="funkci.0o.MAT">funkcio</ref>)</klr>
            &Ccirc;iu elemento, kies bildo per la funkcio estas 
<frm>0</frm>:
            <ekz>
              <frm>&pi;</frm> estas <tld/>iganto de funkcio sinuso;
            </ekz><ekz>
              la polinoma funkcio <frm><k>x</k><sup>2</sup>-1</frm>
              akceptas
              <frm>1</frm> kaj <frm>-1</frm> kiel <tld/>igantojn.
            </ekz>
          </dif>
          <refgrp tip="vid">
            <ref cel="radik.0o.ekvacio">radiko de ekvacio</ref>,
            <ref cel="radik.0o.polinomo">radiko de polinomo</ref>.
          </refgrp>
          <rim>
            Ial <fnt><bib>PIV2</bib></fnt> preferas nomi tion
            <ctl><tld/>ejo</ctl>.
            Vd rimarkon sub <ref
            cel="maksim1.0iganto">maksimumiganto</ref>.
          </rim>
          <trd lng="de">Nullstelle</trd>
          <trdgrp lng="en">
            <trd>null<klr tip="ind"> (of a function)</klr></trd>,
            <trd>zero<klr tip="ind"> (of a function)</klr></trd>
          </trdgrp>
          <trdgrp lng="fr">
            <trd>z&eacute;ro <klr tip="ind">(d'une fonction)</klr></trd>
          </trdgrp>
          <trdgrp lng="pl">
            <trd>miejsce zerowe<klr tip="ind"> (funkcji)</klr></trd>,
            <trd>zero<klr tip="ind"> (wielomianu)</klr></trd>
          </trdgrp>
          <trdgrp lng="ru">
            <trd>&c_n;&c_u;&c_l;&c_mol;<klr tip="ind">
(&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_i;)</klr></trd>
          </trdgrp>
        </subsnc>
        <subsnc mrk="nul0.0iganto.de_elemento">
          <fnt><bib>PIV2</bib></fnt>
          <dif>
            <klr>(de elemento <frm><k>a</k></frm> en
              <ref cel="ring.0o.MAT">ringo</ref>)</klr>
            &Ccirc;iu elemento, kiu multiplikite per <frm><k>a</k></frm> 
donas
            <tld/>an rezulton:
            <ekz>
              en la ringo de <frm>6</frm>-modulaj restoklasoj
              elemento <frm>2</frm> akceptas <frm>0</frm> kaj
              <frm>3</frm> kiel <tld/>igantojn.
            </ekz>
          </dif>
          <ref tip="vid" cel="divizo.nul0o">nuldivizoro.</ref>
        </subsnc>
        <rim>
          Senco <sncref ref="nul0.0iganto.de_elemento"/> povas vasti&gcirc;i
          anka&ubreve; p.p. skalaro rilate al elemento de
          <ref cel="modul.0o.strukt">modulo</ref> a&ubreve; e&ccirc;
          p.p. funkcio rilate al elemento de &gcirc;ia fonto-aro:
          <ekz>
            la karakteriza polinomo de endormorfio <klr>(konsiderata
            kiel
              polinoma funkcio super la aro de endomorfioj)</klr> estas
            <tld/>iganto de tiu endomorfio.
          </ekz>
          La du subsencoj respegulas dualan aliron al la fenomeno:
          funkcio
          nuligas sian argumenton, a&ubreve; argumento nuligas la 
funkcion. Oni
          tiam atentu, ke amba&ubreve; sencoj ne koliziu, kiam 
anka&ubreve; la
          argumentoj estas funkcioj.
        </rim>
      </snc>
  <trdgrp lng="cs">
    <trd>nula</trd>,
    <trd>nulový bod</trd>
  </trdgrp>
  <trd lng="hu">z&eacute;rushely</trd>
  <trdgrp lng="pt">
        <trd>zero</trd>,
        <trd>raiz</trd>
      </trdgrp>
  <trd lng="sk">nulový bod</trd>
</drv>

    <drv mrk="nul0.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc mrk="nul0.0igxi.KOMUNE">
        <dif>
          I&gcirc;i <tld/>a, i&gcirc;i <tld/>o.
        </dif>
        <trdgrp lng="de">
          <trd>Null werden</trd>,
        </trdgrp>
        <trdgrp lng="hu">
          <trd>megsemmis&uuml;l</trd>,
          <trd>null&aacute;z&oacute;dik</trd>,
          <trd>annul&aacute;l&oacute;dik</trd>
        </trdgrp>   
        <trd lng="id">batal</trd>
        <trdgrp lng="ru">
          
<trd>&c_a;&c_n;&c_n;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_s;&c_o;&c_j;&c_t;&c_i; &c_n;&c_a; &c_n;&c_je;&c_t;</trd>
        </trdgrp>
      </snc>
      <snc mrk="nul0.0igxi.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
        <dif>
          Alpreni <tld/>an valoron.
        </dif>
        <trdgrp lng="de">
          <trd>Null werden</trd>,
          <trd>zu <ind>Null aufgehen</ind></trd>
        </trdgrp>
        <trd lng="en">vanish</trd>
        <trd lng="hu">elt&udblac;nik</trd>   
        <trd lng="nl">verdwijnen</trd>   
        <trd lng="no">forsvinne</trd>   
        <trd lng="pl">zerowa&cacute; si&eogonek;</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja; 
&c_v;
&c_n;&c_u;&c_l;&c_mol;</trd>,
          <trd>&c_o;&c_b;&c_n;&c_u;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
        </trdgrp>
      </snc>
      <trd lng="fr">s'<ind>annuler</ind></trd>
      <trdgrp lng="he">
        <trd>&lamed;<ind>&he;&tav;&alef;&pe;&samekh;</ind></trd>,
        <trd>&lamed;<ind>&he;&tav;&bet;&tet;&lamed;</ind></trd>
      </trdgrp>
      <trd lng="pt">anular-se</trd>
    </drv>
  </art>
