<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vitrin.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vitrin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vitrin.0o">
  <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Elmontra vitro&scirc;ranko.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>prosklená skříň</trd>,
    <trd>vitrína</trd>,
    <trd>zasklená nástěnka</trd>
  </trdgrp>
  <trd lng="de">Vitrine</trd>
  <trdgrp lng="el">
    <trd>&beta;&jota;&tau;&rho;&jota_ton;&ny;&alfa;</trd>,
    <trd>&pi;&rho;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
  </trdgrp>
  <trd lng="en">glass show-case</trd>
  <trd lng="fr">vitrine</trd>
  <trd lng="hu">vitrin</trd>
  <trd lng="nl">vitrine</trd>
  <trdgrp lng="pl">
    <trd>witryna</trd>,
    <trd>gablota</trd>,
    <trd>gablotka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>vitrina</trd>,
    <trd>vitrine</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">vitrína</trd>
  <trd lng="sv">vitrinsk&aring;p</trd>
</drv>
</art>
