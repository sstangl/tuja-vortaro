<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hont.xml,v 1.32 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hont</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hont.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="hont.0i.PSI">
    <dif>
      Senti malagrablan konfuzi&gcirc;on kaj humili&gcirc;on, pro konscio je
      malhonora&jcirc;o, malhonesta&jcirc;o, maldeca&jcirc;o:
      <ekz>
        vi devus <tld/>i vian teni&gcirc;on a&ubreve; pri, pro via
        teni&gcirc;o;
      </ekz>
      <ekz>
        kiu <tld/>as nenion, ne timas Dion
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke mi tiel longe prokrastis mian viziton;
      </ekz>
      <ekz>
        ne <tld/>u penti pri faro, <tld/>u persisti en eraro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la membro de unu nacio humili&gcirc;as anta&ubreve; la membro de alia
        nacio, parolas lian lingvon, <tld/>ante la sian
        <fnt><bib>Paroloj</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_m;&c_je;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">kaout <ind>mezh</ind></trd>
  <trdgrp lng="ca">
    <trd>avergonyir-se</trd>,
    <trd>apocar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>styd&ecaron;t se</trd>,
    <trd>stydět se</trd>
  </trdgrp>
  <trd lng="de">sich <ind>sch&auml;men</ind></trd>
  <trd lng="el">&ny;&tau;&rho;&epsilon_ton;&pi;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">be ashamed</trd>
  <trd lng="es">avergonzarse de</trd>
  <trdgrp lng="fr">
    <trd>avoir <ind>honte</ind> (de)</trd>,
    <trd>rougir (de)<klr tip="ind"> (par honte)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;gyenkezik</trd>,
    <trd>sz&eacute;gyenli mag&aacute;t</trd>
  </trdgrp>
  <trd lng="it">vergognarsi</trd>
  <trd lng="nl">zich <ind>schamen</ind></trd>
  <trd lng="pl">wstydzi&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
   <trd>envergonhar-se de</trd>, 
   <trd>pejar-se</trd> 
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">hanbiť sa</trd>
  <trd lng="sv">sk&auml;mmas</trd>
</drv>

<drv mrk="hont.0o">
  <kap><tld/>o</kap>
  <snc mrk="hont.0o.PSI">
    <dif>
      <ref tip="lst" cel="sent.0o.emocio" lst="voko:sentoj">Sento</ref>
      de <tld/>anto:
      <ekz>
        ru&gcirc;i&gcirc;i pro <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        kie timo, tie <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fari <tld/>on al iu.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>torbaci&oacute;</trd>,
      <trd>rubor</trd>
    </trdgrp>
    <trd lng="de">Scham</trd>
  </snc>
  <snc mrk="hont.0o.hontindajxo">
      <ref tip="dif" cel="hont.0indajxo"><tld/>inda&jcirc;o</ref>
      <ekz>
        lia <tld/>o ne elvi&scirc;i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Sen; 6:33</lok></fnt>;
      </ekz>
      <ekz>
        li estas la <tld/>o de sia familio;
      </ekz>
      <ekz>
        mi ne transvivos tiun <tld/>on;
      </ekz>
      <ekz>
        tia malpureco estas ja <tld/>o.
      </ekz>
    <refgrp tip="vid">
      <ref cel="makul.0o">makulo</ref>,
      <ref cel="glor.mal0o">malgloro</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>oprobi</trd>,
      <trd>inf&agrave;mia</trd>
    </trdgrp>
    <trd lng="de">Schande</trd>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">mezh</trd>
  <trd lng="ca">vergonya</trd>
  <trdgrp lng="cs">
    <trd>hanba</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="es">verg&uuml;enza</trd>
  <trd lng="fr">honte</trd>
  <trd lng="hu">sz&eacute;gyen</trd>
  <trd lng="it">vergogna</trd>
  <trdgrp lng="nl">
    <trd>schaamte</trd>,
    <trd>schande</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wstyd</trd>
  </trdgrp>
  <trd lng="pt">vergonha</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_y;&c_d;</trd>,
    <trd>&c_s;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hanba</trd>,
    <trd>hanblivosť</trd>,
    <trd>stud</trd>
  </trdgrp>
  <trd lng="sv">skam</trd>
</drv>

<drv mrk="hont.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="hont.0ema.PSI">
    <dif>
      Facile kaj plej ofte senka&ubreve;ze <tld/>anta:
      <ekz>
        tiu timema kaj <tld/>ema knabino ru&gcirc;i&gcirc;as anta&ubreve;
        &ccirc;iu nekonata persono.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_a;&c_m;&c_ja;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>mezhek</trd>,
    <trd>lent</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vergony&oacute;s</trd>,
    <trd>t&iacute;mid</trd>
  </trdgrp>
  <trd lng="cs">stydlivý</trd>
  <trdgrp lng="de">
    <trd>schamhaft</trd>,
    <trd>versch&auml;mt</trd>,
    <trd>sch&uuml;chtern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>honteux<klr tip="ind"> (rougissant)</klr></trd>,
    <trd>timide</trd>,
    <trd>rougissant</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;gyenl&odblac;s</trd>
  <trd lng="it">pudico</trd>
  <trd lng="nl">beschaamd</trd>
  <trd lng="pl">wstydliwy</trd>
  <trd lng="pt">envergonhado</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">hanblivý</trd>
</drv>

<drv mrk="hont.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hont.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u:
      <ekz>
        <tld/>igi mallaboreman lernanton;
      </ekz>
      <ekz>
        kunulo de mal&ccirc;astuloj <tld/>igas sian patron
        <fnt><bib>MT</bib>, <lok>&Sen; 28:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_m;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">mezhekaat<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="ca">avergonyir</trd>
  <trd lng="cs">zahanbit</trd>
  <trd lng="de">besch&auml;men</trd>
  <trd lng="es">avergonzar</trd>
  <trd lng="fr">faire <ind>honte</ind> (&agrave;)</trd>
  <trd lng="hu">megsz&eacute;gyen&iacute;t</trd>
  <trd lng="it">svergognare</trd>
  <trd lng="nl">beschamen</trd>
  <trd lng="pl">zawstydza&cacute;</trd>
  <trd lng="pt">envergonhar</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">zahanbiť</trd>
</drv>

<drv mrk="hont.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="hont.0inda.PSI">
    <dif>
      Pri kiu oni devas <tld/>i:
      <ekz>
        <klr>[se]</klr> li trovis &ccirc;e &scirc;i ion <tld/>indan,
        tiam li skribu al &scirc;i eksedzigan leteron
        <fnt><bib>MT</bib>, <lok>&Rea; 24:1</lok></fnt>;                
      </ekz>
      <ekz>
        kiu dormas en la tempo de rikolto,
        tiu estas filo <tld/>inda
        <fnt><bib>MT</bib>, <lok>&Sen; 10:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">mezhus</trd>
  <trdgrp lng="cs">
    <trd>hanebný</trd>,
    <trd>infámní</trd>,
    <trd>nestoudný</trd>,
    <trd>nestydatý</trd>
  </trdgrp>
  <trd lng="es">vergonzoso</trd>
  <trd lng="fr">honteux<klr tip="ind"> (qui fait
  honte)</klr></trd>
  <trd lng="sk">hanebný</trd>
</drv>

<drv mrk="hont.0indajxo">
  <kap><tld/>inda&jcirc;o</kap>
  <snc mrk="hont.0indajxo.PSI">
    <dif>
      Io, kion oni <tld/>as a&ubreve; devus <tld/>i:
      <ekz>
        ni fari&gcirc;is <tld/>inda&jcirc;o por niaj najbaroj
        <fnt><bib>MT</bib>, <lok>&Psa; 79:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>                         
  <trd lng="be">&c_g;&c_a;&c_n;&c_mol;&c_b;&c_a;</trd>
  <trd lng="br">mezh</trd>
  <trdgrp lng="ca">
    <trd>vergonya</trd>,
    <trd>afront</trd>
  </trdgrp>
  <trd lng="de">Schande</trd>
  <trd lng="fr">honte</trd>
  <trd lng="hu">sz&eacute;gyenletes dolog</trd>
  <trd lng="it">ignominia</trd>
  <trd lng="nl">schande</trd>
  <trd lng="es">verg&uuml;enza</trd>
  <trd lng="pt">vergonha</trd>
  <trd lng="pl">wstydliwa rzecz</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_o;&c_r;</trd>
</drv>

<drv mrk="hont.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hont.pri0i.KOMUNE">
    <dif>
      <tld lit="H"/>i pro:
      <ekz>
        nefaron de li &scirc;i pri<tld/>us
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">avoir <ind>honte</ind> de</trd>
</drv>

<drv mrk="hont.seks0emo">
  <kap>seks<tld/>emo, <var><kap>seks<tld/>o</kap></var></kap>
  <snc mrk="hont.seks0emo.PSI">
    <dif>
      <tld lit="H"/>emo pri seksaj aferoj, tiurilata
      <ref tip="vid" cel="pudor.0o.MOR">pudoro</ref>.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>elevez</trd>,
    <trd>mezh<klr tip="ind"> (elevez)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pudor</trd>,
    <trd>reserva</trd>
  </trdgrp>
  <trd lng="cs">pohlavní stud</trd>
  <trdgrp lng="es">
    <trd>pudor</trd>,
    <trd>recato</trd>
  </trdgrp>
  <trd lng="fr">pudeur</trd>
  <trd lng="hu">szem&eacute;rem</trd>
  <trd lng="it">pudore</trd>
  <trd lng="nl">preutsheid</trd>
  <trd lng="pl">pruderia</trd>
  <trd lng="pt">pudor</trd>
  <trd lng="ru">&c_s;&c_t;&c_y;&c_d;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">pohlavný stud</trd>
  <trd lng="sv">pryd</trd>
</drv>

<drv mrk="hont.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="hont.sen0a.PSI">
    <dif>
      Ne facile sentanta <tld/>on;
      <ref tip="vid" cel="cinik.0a">cinika</ref>,
      <ref tip="vid" cel="impert.0a">impertinenta</ref>:
      <ekz>
        sen<tld/>a virino, viza&gcirc;o;
      </ekz>
      <ekz>
        sen<tld/>e mensogi.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_s;&c_a;&c_r;&c_o;&c_m;&c_n;&c_y;</trd>
  <trd lng="br">divezh</trd>
  <trdgrp lng="ca">
    <trd>pocavergonya</trd>,
    <trd>barrut</trd>,
    <trd>imp&uacute;dic</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hanebný</trd>,
    <trd>infámní</trd>,
    <trd>nestoudný</trd>,
    <trd>nestydatý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schamlos</trd>,
    <trd>unversch&auml;mt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sin verg&uuml;enza</trd>,
    <trd>desvergonzado, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;hont&eacute;</trd>,
    <trd>impudent</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;gyentelen</trd>
  <trdgrp lng="it">
    <trd>spudorato</trd>,
    <trd>impudente</trd>, 
    <trd>sfrontato</trd>,
    <trd>svergognato</trd>, 
  </trdgrp>
  <trd lng="nl">schaamteloos</trd>
  <trd lng="pl">bezwstydny</trd>
  <trdgrp lng="pt">
    <trd>sem-vergonha</trd>,
    <trd>desavergonhado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_s;&c_t;&c_y;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_s;&c_t;&c_y;&c_zh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nehanblivý</trd>,
    <trd>nehanebný</trd>
  </trdgrp>
  <trd lng="sv">skaml&ouml;s</trd>
</drv>

<drv mrk="hont.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc mrk="hont.sen0ulo.PSI">
    <dif>
      Iu, kiu sen <tld/>o agas kontra&ubreve;e al moralo, deco, moroj;
      <ref tip="vid" cel="cinik.0ulo.KOMUNE">cinikulo</ref>:
      <ekz>
        &ccirc;u ci volas tiun &ccirc;i sen<tld/>ulinon?
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">un <ind>sinverg&uuml;enza</ind></trd>
  <trdgrp lng="fr">
    <trd>effront&eacute;<klr> (subst.)</klr></trd>,
    <trd>&eacute;hont&eacute;<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>
</art>
