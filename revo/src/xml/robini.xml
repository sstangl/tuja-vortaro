<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: robini.xml,v 1.6 2015/07/04 08:37:42 revo Exp $">
<kap>
    <rad>robini</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="robini.0o">
  <kap><tld/>o</kap>
  <snc mrk="robini.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Somerverda arbo a&ubreve; arbusto kun iom e&gcirc;aj tigoj, kontra&ubreve;sidaj
      elipsaj folioj
      kun apudfolioj transformitaj al dornoj, kaj kun pendantaj
      florgrapoloj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Robinia</trd>) kun specioj kreskantaj en
      Nordameriko sed
      anka&ubreve; E&ubreve;ropo.
    </dif>
  </snc>
  <ref tip="lst" cel="plant.floro0o"
  lst="voko:faboparencoj">faboparencaj floroplantoj</ref>
  <trd lng="de">Robinie</trd>
  <trd lng="fr">robinier</trd>
  <trd lng="sk">agát</trd>
  <trd lng="cs">trnovník</trd>
</drv>

<drv mrk="robini.akacia0o">
  <kap>&scirc;ajnakacia <tld/>o</kap>
  <snc mrk="robini.akacia0o.specio">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:botanikaj_specioj">Specio</ref> de <tld/>o
      (<trd lng="la">Robinia pseudoacacia</trd>)
      kun blankaj floraroj, el kiu produkti&gcirc;as bonkvalita mielo;
      pro la foliaroj kaj dornoj la arbo estas konfuzata kun
      <ref cel="akaci.0o">akacio</ref>.
    </dif>
  </snc>
 <trdgrp lng="de">
    <trd>Gew&ouml;hnliche Robinie</trd>,
    <trd>Falsche Akazie</trd>,
    <trd>Silberregen</trd>
  </trdgrp>
  <trd lng="fr">faux-acacia</trd>
</drv>

</art>

