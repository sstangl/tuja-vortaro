<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kilobitok.xml,v 1.5 2013/08/20 13:10:11 revo Exp $">
<kap>
  <rad>kilobitok</rad>/o
</kap>

<drv mrk="kilobitok.0o">
  <kap><tld/>o</kap>
  <mlg>kB</mlg>
  <snc mrk="kilobitok.0o.MEZ">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de ciferecaj 
      datumoj, <ref tip="prt" cel="bit.0oko.KOMP">bitok</ref>milo: 
      <ekz>
        <klr>[la dosiero]</klr> pezas 199 <tld/>ojn 
        <fnt>
          <vrk>[enhavtabelo]</vrk>,
          <lok>Sennaciulo, 2000-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_i;&c_l;&c_o;&c_b;&c_a;&c_j;&c_t;</trd>
  <trd lng="de">Kilobyte<klr tip="amb"> (kB)</klr></trd>
  <trd lng="en">kilobyte</trd>
  <trd lng="es">kilobyte<klr tip="amb"> (Kb=1024 byte)</klr></trd>
  <trd lng="fr">kilooctet</trd>
  <trd lng="it">kilobyte</trd>
  <trd lng="nl">kilobyte</trd>
</drv>
</art>

