</drv>

<drv mrk="est.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="est.kun0i.KOMUNE">
    <dif>
      <tld lit="E"/>i en sama loko, kune:
      <ekz>
        li residigis lin sur la teron, dancadis &ccirc;irka&ubreve; li,
        dume
        dirante 
        al li strangajn rakontojn pri &ccirc;io, kion li vidis de kiam
        li kaj
        
        maljuna Jafo laste kun<tld/>is
        <fnt><aut>D. Defo, trad. A. Krafft</aut>, <vrk>Robinsono 
        Kruso</vrk>, <lok>1908</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>sou&zcaron;&iacute;t</trd>,
    <trd>&zcaron;&iacute;t spolu</trd>
  </trdgrp>
  <trd lng="en">be with</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre ensemble</trd>,
    <trd>se <ind>tenir compagnie</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>uczestniczy&cacute;</trd>,
    <trd>dotrzymywa&cacute; <ind>towarzystwa</ind></trd>
  </trdgrp>
  <trd lng="sk">&zcaron;i&tcaron; spolu</trd>
</drv>

<drv mrk="est.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="est.mal0i.KOMUNE">
