<drv mrk="est.al0i">
  <kap>al<tld/>i, <var><kap>apud<tld/>i</kap></var></kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <uzo tip="stl">EVI</uzo>
  <snc>
  <ref tip="dif" cel="est.cxe0i">&ccirc;eesti</ref>
  <ekz>
    jen &scirc;i vidis, ke &scirc;ia Arturo estas malsana &mdash; eble
    e&ccirc;
    mortas, kaj apud li neniu al<tld/>as
    <fnt>
      <aut>V. Devjatnin</aut>
      <bib>FK</bib>,
      <lok>Arturo</lok>
    </fnt>;
  </ekz>
  <ekz>
    kaj al<tld/>is apud li &ccirc;iuj vidvinoj, plorante kaj montrante 
    tunikojn kaj vestojn, kiujn Dorkas faris, kiam &scirc;i estis
    ankora&ubreve;
    kun 
    ili
    <fnt><bib>NT</bib>, <lok>&Ago; 9:39</lok></fnt>.
  </ekz>
  <rim>
    La direkta <em>al-</em> neniel ta&ubreve;gas por la senco
    &leftquot;&ccirc;e<tld/>i&rightquot;, kaj povis aperi nur pro
    misapliko de alilingva tradicio.
    &leftquot;Apud<tld/>i&rightquot; formale estus tute ta&ubreve;ga,
    sed por pli flanka, periferia situo.
    <aut>Sergio Pokrovskij</aut>
  </rim>
  </snc>
  <trd lng="sk">by&tcaron; pr&iacute;tomn&yacute;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t p&rcaron;&iacute;tomen</trd>,
    <trd>z&uacute;&ccaron;astnit se</trd>
  </trdgrp>
</drv>

<drv mrk="est.cxe0i">
  <kap>&ccirc;e<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="est.cxe0i.KOMUNE">
    <dif>
      <tld lit="E"/>i en iu loko, kiam io okazas:
      <ekz>
        &ccirc;e<tld/>i kunvenon;
      </ekz><ekz>
        &ccirc;e<tld/>i en, &ccirc;e kunveno;
      </ekz><ekz>
        en <ind>&ccirc;e<tld/>o</ind> amata, en for<tld/>' insultata;
        <trd
        lng="be">&c_p;&c_r;&c_y;&c_s;&c_u;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
        <trdgrp lng="fr">
          <trd>assistance<klr tip="ind"> (fait d'assister)</klr></trd>,
          <trd>pr&eacute;sence</trd>
        </trdgrp>
        <trd lng="hu">jelenl&eacute;t</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz><ekz>
        multnombra <ind>&ccirc;e<tld/>antaro</ind>.
        <trd lng="fr">assistance<klr tip="ind"> (personnes)</klr></trd>
        <trd lng="hu">jelenl&eacute;v&odblac;k</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_u;&c_t;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ser present</trd>
  <trd lng="de">dabei sein</trd>
  <trdgrp lng="en">
    <trd>be present at</trd>,
    <trd>attend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>asistir</trd>,
    <trd>estar presente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assister (&agrave;)</trd>,
    <trd>&ecirc;tre pr&eacute;sent</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jelen van</trd>,
    <trd>ott van</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>hadir</trd>,
    <trd>[<ind>hadir</ind>] menghadiri</trd>
  </trdgrp>
    <trdgrp lng="it">
    <trd>presenziare</trd>,
    <trd>partecipare</trd>
    <trd>assistere</trd>
    <trd>intervenire</trd>
  </trdgrp>
  <trd lng="nl">aanwezig zijn</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>obecnym</ind></trd>,
    <trd>uczestniczy&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_p;&c_r;&c_i;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="tp">lon</trd>
</drv>

<drv mrk="est.cxe0o">
  <kap>&ccirc;e<tld/>o</kap>
