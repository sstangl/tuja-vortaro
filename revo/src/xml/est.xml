  <trdgrp lng="fr">
    <trd>faire <ind>d&eacute;faut</ind></trd>,
    <trd>manquer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nincs</trd>,
    <trd>nem <ind>l&eacute;tezik</ind></trd>
  </trdgrp>
  <trd lng="nl">ontbreken</trd>
  <trd lng="pl">nie <ind>istnie&cacute;</ind></trd>
  <trd
  lng="ru">&c_o;&c_t;&c_s;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    <klr tip="amb"> (&c_n;&c_je;
    &c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr>
  </trd>
  <trdgrp lng="sk">
    <trd>ch&yacute;ba&tcaron;</trd>,
    <trd>neby&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="est.mar0ajxo">
  <kap>mar<tld/>a&jcirc;o</kap>
