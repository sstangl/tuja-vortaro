        la maro retiri&gcirc;is de la marbordo kaj mar<tld/>a&jcirc;oj
        restis sur 
        la tero
        <fnt><bib>IntArkeol</bib>, <lok>p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">sea creature</trd>
  <trdgrp lng="fr">
    <trd>animal marin</trd>,
    <trd>cr&eacute;ature marine</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stworzenie morskie</trd>,
    <trd>zwierz&eogonek; morskie</trd>
  </trdgrp>
</drv>

<drv mrk="est.ne0o">
  <kap>ne<tld/>o</kap>
  <snc mrk="est.ne0o.KOMUNE">
