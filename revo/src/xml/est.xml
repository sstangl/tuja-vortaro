    <trd>neml&eacute;t</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nieistnienie</trd>,
    <trd>niebyt</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_b;&c_y;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">nebytie</trd>
  <trd lng="tp">weka</trd>
</drv>

<drv mrk="est.tio0as">
  <kap><ofc>*</ofc>tio <tld/>as</kap>
  <mlg>t. e.</mlg>
  <snc mrk="est.tio0as.KOMUNE">
    <dif>
      <ref tip="vid" cel="vort.ali0e">Alivorte</ref>, <ref tip="vid" 
      cel="nom.0e.te">nome<sncref/></ref>, klarigante pli precize: 
      <ekz>
        la simpla esperantisteco, t.e. la simpla lernado kaj
        uzado de Esperanto, neniun devigas ali&gcirc;i al ia
        ideo<fnt>Z</fnt>;
      </ekz><ekz>
        la akuzativo montras direkton,
        t.e. movadon al ia loko<fnt>Z</fnt>;
      </ekz><ekz>
        por la hodia&ubreve;a tago mi ricevis duoblan pagon, t. e. 
        <klr>...</klr> dek frankojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz><ekz>
        por esprimi direkton ni aldonas la
        fina&jcirc;on n, t.e. la fina&jcirc;o, kiu signas la akuzativon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>es a <ind>dir</ind> <klr>(formal i coloq.)</klr></trd>,
    <trd>o sigui<klr> (coloq.)</klr></trd>
  </trdgrp>
  <trd lng="cs">tj.. <klr>(zkratka "to jest")</klr></trd>
  <trd lng="de">das hei&szlig;t <klr>d.h.</klr></trd>
  <trdgrp lng="en">
    <trd>that is</trd>,
    <trd>i.e.</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>es <ind>decir</ind> <klr>(formal y coloq.)</klr></trd>,
    <trd>o sea<klr> (coloq.)</klr></trd>
  </trdgrp>
  <trd lng="fr">c'<ind>est-&agrave;-dire</ind></trd>
  <trdgrp lng="hu">
    <trd>azaz</trd>,
    <trd>nevezetesen</trd>
  </trdgrp>
