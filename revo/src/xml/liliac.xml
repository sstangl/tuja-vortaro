<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: liliac.xml,v 1.10 2013/08/25 14:10:29 revo Exp $">
<kap>
  <rad>liliac</rad>/oj
</kap>
<drv mrk="liliac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Liliaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_l;&c_je;&c_j;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">lili&agrave;cies</trd>
  <trd lng="de">Liliengew&auml;chse</trd>
  <trd lng="el">&kappa;&rho;&epsilon;&jota;&ny;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="fa">&a_s;&a_w;&a_s;&a_n;&f_y;&a_A;&a_n;</trd>
  <trd lng="hu">liliomf&eacute;l&eacute;k</trd>
  <trd lng="nl">lelieachtigen</trd>
  <trd lng="pt">lili&aacute;ceas</trd>
  <trd lng="ru">&c_l;&c_i;&c_l;&c_je;&c_j;&c_n;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">liljev&auml;xter</trd>
</drv>
</art>
