<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: artemi.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>artemizi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="artemi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Artemisia</trd>), el kiu diversaj specioj estas
      uzataj kiel spicherboj.
    </dif>
    <refgrp tip="sub">
      <ref cel="abrota.0o">abrotano</ref>,
      <ref cel="absint.0o">absinto</ref>,
      <ref cel="drakun.0o">drakunkolo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_y;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>pelyněk</trd>,
    <trd>černobýl</trd>
  </trdgrp>
  <trd lng="de">Beifu&szlig;</trd>
  <trd lng="fr">armoise</trd>
  <trd lng="hu">&uuml;r&ouml;m <klr tip="ind">(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="it">artemisia</trd>
  <trd lng="pl">bylica</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_y;&c_n;&c_mol;</trd>
  <trd lng="sk">palina</trd>
  <trd lng="sv"><klr>(sl&auml;ktet) </klr>mal&ouml;rt</trd>
</drv>
</art>
