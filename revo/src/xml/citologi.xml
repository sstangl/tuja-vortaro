      &Ccirc;elosciencisto.
    </dif>
  </snc>
  <trd lng="ca">cit&ograve;leg</trd>
  <trd lng="fa">&f_y;&a_A;&a_kh;&a_t;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
