<kap>
  <rad>posesiv</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="posesi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Poseda pron. a&ubreve; adj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y; &c_z;&c_a;&c_j;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_t;&c_je;&c_zh;&c_a;&c_t;&c_je;&c_l;&c_je;&c_n; &c_m;&c_je;&c_s;&c_t;&c_o;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>posesivum</trd>,
    <trd>přivlastňovací přídavné jméno</trd>,
    <trd>přivlastňovací zájmeno</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Possessivpronomen</trd>,
    <trd>besitzanzeigendes F&uuml;rwort</trd>
  </trdgrp>
  <trd lng="en">possessive pronoun</trd>
  <trd lng="es">posesivo</trd>
  <trd lng="fr">pronom possessif</trd>
  <trd lng="hu">birtokos n&eacute;vm&aacute;s</trd>
  <trd lng="nl">bezittelijk <ind>voornaamwoord</ind></trd>
  <trd lng="pt">possessivo <klr>(pronome etc.)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_t;&c_ja;&c_zh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">privlastňovacie zámeno</trd>
</drv>
</art>
