<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: leuxkoj.xml,v 1.7 2013/08/26 08:10:22 revo Exp $">
<kap>
  <rad>le&ubreve;koj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leuxkoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxkoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara bulboherbo kreskanta en E&ubreve;ropo,
      Nordafriko kaj Okcidentazio, kun mallar&gcirc;aj folioj kaj pendaj,
      klo&scirc;aj, blankaj floroj, <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="amaril1.0oj">amarilidacoj</ref> (<trd
      lng="la">Leucojum</trd>):
      <ekz>
        mi konsultis anka&ubreve; ilustritan gvidlibron pri 
        &gcirc;ardenplantoj, kaj fine trovis &gcirc;in. &Gcirc;ia nomo estas 
        <tld/>o [Leucojum aestivum] a&ubreve; ne&gcirc;ofloko
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url
        ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
        <lok>2006-03-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="leuxkoj.0o.RIM_PARONIMO">
    Ne konfuzu kun <ref tip="vid" cel="levkoj.0o">levkojo</ref>.
  </rim>
  <trd lng="de">Knotenblume</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_h;&zwnj;&a_b;&a_r;&a_f;&f_y;</trd>
  <trd lng="fr">niv&eacute;ole</trd>
  <trd lng="nl">narcisklokje</trd>
  <trd lng="pt">niv&eacute;ola</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_l;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_l;&c_i;&c_s; 
&c_a;&c_l;&c_mol;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

