<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konjak.xml,v 1.19 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konjak</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konjak.0o">
  <kap><tld/>o</kap>
  <snc mrk="konjak.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Vin<ref tip="super" cel="brand.0o.KUI">brando</ref> farita per 
      du sinsekvaj distiladoj, el samnoma franca urbo,
      kaj komunlingve &ccirc;ia simila brando:
      <ekz>
        Zaur trankvile surtabligas belan <tld/>botelon
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 14a, p. 83a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_ja;&c_k;</trd>
  <trd lng="ca">conyac</trd>
  <trd lng="cs">koňak</trd>
  <trdgrp lng="de">
    <trd>Kognac</trd>,
    <trd>Cognac</trd>,
    <trd>franz&ouml;sischer Weinbrand</trd>
  </trdgrp>
  <trd lng="en">cognac</trd>
  <trd lng="es">co&ntilde;ac</trd>
  <trd lng="fa">&f_k;&a_damma;&a_n;&f_y;&a_A;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>cognac</trd>
  </trdgrp>
  <trd lng="nl">cognac</trd>
  <trd lng="pt">conhaque</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_mol;&c_ja;&c_k;</trd>
  <trd lng="sk">koňak</trd>
</drv>
</art>
