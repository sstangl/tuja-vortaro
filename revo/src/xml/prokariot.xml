<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prokariot.xml,v 1.4 2015/03/08 20:10:16 revo Exp $">
<kap>
  <rad>prokariot</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="prokariot.0o">
  <kap><tld/>o</kap>
  <snc mrk="prokariot.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Viva&jcirc;o plej ofte unu&ccirc;ela, kiu ne havas klare
      apartigitan
      kernon: bakterio, blua algo..., kontraste al
      <ref tip="vid" cel="euxkariot.0o.BIO">e&ubreve;karioto</ref>:
      <ekz>
        <tld/>oj <klr>...</klr> havas ununuran <tld/>on
        el ringforma DNA
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Kromosomo">
          kromosomo</url>, 2010-06-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="euxkariot.0o.BIO">e&ubreve;karioto</ref>
    <trd lng="de">Prokaryot</trd>
    <trd lng="fr">procaryote</trd>
    <trd lng="pl">prokariot</trd>
  </snc>
</drv>
</art>

