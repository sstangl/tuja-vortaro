<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mustel.xml,v 1.26 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mustel</rad>/o
</kap>

<drv mrk="mustel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Tre malgranda <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">rabobesto</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="mustel1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mustela</trd>), al kiu apartenas
      la <ref cel="ermen.0o">ermeno</ref>
      kaj la plej malgranda vivanta rabobesto
      (<trd lng="la">Mustela nivalis</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="cs">lasice</trd>
  <trdgrp lng="de">
    <trd>Wiesel</trd>
    <trd>Hermelin</trd>,
    <trd>Nerz</trd>
  </trdgrp>
  <trd lng="en">weasel</trd>
  <trd lng="fr">belette</trd>
  <trd lng="hu">meny&eacute;t</trd>
  <trd lng="id">cerpelai</trd>
  <trd lng="nl">wezel</trd>
  <trd lng="pl">mustela</trd>
  <trdgrp lng="pt">
    <trd>mustela</trd>,
    <trd>doninha</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">lasica</trd>
</drv>
</art>
