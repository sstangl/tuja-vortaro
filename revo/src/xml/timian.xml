<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: timian.xml,v 1.27 2015/07/04 08:37:44 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>timian</rad>/o
</kap>
<drv mrk="timian.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Thymus</trd>);
      tre aroma <ref tip="lst" cel="arbust.0o" 
      lst="voko:lamioparencoj">arbusto</ref>
      a&ubreve; duonarbusto kun verticiloj a&ubreve; kapetoj de 
      blankaj &gcirc;is violkoloraj floroj, kies korolo estas 
      tubetoforma kun du lipoj, kies malsupra havas tri lobojn;
      pluraj specioj estas uzataj 
      kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>
      kaj kuracherbo:
      <ekz>
        ili sukcesas senmaskigi fra&ubreve;dantojn, kiuj vendas malmultekostan
        mielon &leftquot;el &ccirc;iaj floroj&rightquot; sub la nomo de
        multekosta &leftquot;mielo de
        <tld/>o&rightquot;<fnt><aut>Robert Roze</aut>: <vrk>Vivisekcio de
        polenero</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/timian.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_a; &c_m;&c_a;&c_shch;&c_je;&c_r;&c_k;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>kerti <ind>kakukkf&udblac;</ind></trd>,
      <trd>pecsenyef&udblac;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>farigola</trd>,
      <trd>timonet</trd>
    </trdgrp>
    <trd lng="la">Thymus vulgaris</trd>
  </bld>
  <trd lng="be">&c_ch;&c_a;&c_b;&c_o;&c_r;</trd>
  <trd lng="bg">&c_m;&c_a;&c_shch;&c_je;&c_r;&c_k;&c_a;</trd>
  <trd lng="ca">tim&oacute;</trd>
  <trd lng="de">Thymian</trd>
  <trd lng="en">thyme</trd>
  <trd lng="fr">thym</trd>
  <trd lng="hu">kakukkf&udblac;</trd>
  <trd lng="nl">tijm</trd>
  <trd lng="pl">tymianek</trd>
  <trdgrp lng="pt">
    <trd>tomilho</trd>,
    <trd>timo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_m;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_ch;&c_a;&c_b;&c_r;&c_je;&c_c;</trd>,
    <trd>&c_ch;&c_je;&c_b;&c_r;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="tr">kekik</trd>
  <trd lng="sk">tymián</trd>
  <trdgrp lng="cs">
    <trd>dymián</trd>,
    <trd>mateřídouška</trd>,
    <trd>tymián</trd>
  </trdgrp>
</drv>
</art>
