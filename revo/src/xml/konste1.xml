<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konste1.xml,v 1.17 2012/08/04 19:10:22 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konstern</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konste1.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Maltrankviligi, malspritigi, konfuzi, embarasi, senaplombigi
      iun, surprizante lin kaj malordigante liajn kutimojn a&ubreve;
      projektojn;
      miregigi, senkonsiligi:
      <ekz>
        tiu respondo, spektaklo <tld/>is lin;
      </ekz>
      <ekz>
        via akcepto <tld/>us e&ccirc; pli memfidajn;
      </ekz>
      <ekz>
        tio <tld/>as niajn lingvajn kutimojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        misteroj, kiuj <tld/>as e&ccirc; sa&gcirc;ulojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfuz.0i">konfuzi 3</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>torbar</trd>,
      <trd>desconcertar</trd>
    </trdgrp>
    <trd lng="de">konsternieren</trd>
  </snc>
  <snc>
    <dif>
      Premegi iun kvaza&ubreve; per katastrofo; mal&gcirc;ojegi, malvigligi,
      senkura&gcirc;igi:
      <ekz>
        la sciigo pri lia morto <tld/>is &ccirc;iujn
        liajn amikojn;
      </ekz>
      <ekz>
        io <tld/>anta kaj terura fari&gcirc;is en la lando<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">consternar</trd>
    <trdgrp lng="de">
      <trd>best&uuml;rzen</trd>,
      <trd>betroffen machen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_b;&c_ja;&c_n;&c_t;&c_e;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_r;&c_a;&c_z;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">dismay</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_h;&a_t;&zwnj;&a_z;&a_d;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_t;&a_H;&f_y;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_t;&a_A_hamza_sure;&a_th;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ahurir</trd>,
    <trd>abasourdir</trd>,
    <trd>consterner</trd>,
    <trd>d&eacute;contenancer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontstellen</trd>,
    <trd>verbluffen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atordoar</trd>,
    <trd>desconcertar</trd>,
    <trd>aniquilar</trd>,
    <trd>bestificar</trd>,
    <trd>consternar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_z;&c_a;&c_d;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; &c_z;&c_a;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>bringa ur fattningen</trd>,
    <trd>g&ouml;ra best&ouml;rt</trd>
  </trdgrp>
</drv>

<drv mrk="konste1.0o">
  <kap><tld/>o, <var><kap><tld/>i&gcirc;o</kap></var></kap>
  <snc mrk="konste1.0o.KOMUNE">
    <dif>
      Stato de iu <tld/>ita:
      <ekz>
        la malsimpleco de tiuj demandoj &jcirc;etis <tld/>on en lian spiriton;
      </ekz>
      <ekz>
        mi preska&ubreve; svenis de <tld/>i&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio klare montras la <tld/>econ de la situacio.
      </ekz>
    </dif>
    <rim>
      Plejofte la subjekto de la verbo <em>konfuzi</em> estas persono,
      kaj la subjekto de <em>konsterni</em> estas afero.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_b;&c_ja;&c_n;&c_t;&c_e;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_u;&c_b;&c_l;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
    <trdgrp lng="ca">
      <trd>torbament</trd>,
      <trd>consternaci&oacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Best&uuml;rzung</trd>,
      <trd>Betroffenheit</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_b;&a_h;&a_t;</trd>,
      <trd>&a_H;&f_y;&a_r;&a_t;</trd>,
      <trd>&a_n;&a_A;&a_b;&a_A;&a_w;&a_r;&f_y;</trd>,
      <trd>&a_t;&a_A_hamza_sure;&a_th;&a_r;</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>ahurissement</trd>,
    <trd>consternation</trd>
  </trdgrp>
  <trd lng="nl">ontsteltenis</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_je;&c_r;&c_ja;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">best&ouml;rtning</trd>
</drv>
</art>
