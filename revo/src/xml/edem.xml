<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: edem.xml,v 1.23 2015/07/04 08:37:33 revo Exp $">
<kap>
  <rad>edem</rad>/o
</kap>
<drv mrk="edem.0o">
  <kap><tld/>o</kap>
  <snc mrk="edem.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      &Scirc;velo ka&ubreve;zata de enfiltri&gcirc;o de sero:
      <ekz>
        Semira perdis la konscion kaj horojn poste mortis pro <tld/>o
        kaj
        hemoragio en la cerbo<fnt><aut>Guido Van Damme</aut>: <vrk>Sep
        en
        unu bato</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 3a, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="sxvel.akvo0o">akvo&scirc;velo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_b;&c_r;&c_a;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_y;&c_w;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_d;&c_je;&c_m;</trd>
  <trd lng="ca">edema</trd>
  <trdgrp lng="de">
    <trd>&Ouml;dem</trd>,
    <trd>Gewebewassersucht</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&f_y;&a_z;<klr tip="ind"> (&f_p;&a_z;&a_sh;&f_k;&f_y;)</klr></trd>
  <trd lng="fr">&oelig;d&egrave;me</trd>
  <trd lng="en">edema</trd>
  <trd lng="es">edema</trd>
  <trdgrp lng="hu">
    <trd>&ouml;d&eacute;ma</trd>,
    <trd>vizeny&odblac;</trd>
  </trdgrp>
  <trd lng="it">edema</trd>
   <trdgrp lng="nl">
    <trd>oedeem</trd>,
    <trd>waterzucht</trd>,
    <trd>hydropsie</trd>
  </trdgrp>
  <trd lng="pl">puchlina wodna</trd>
  <trd lng="pt">edema</trd>
  <trd lng="ru">&c_o;&c_t;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>edém</trd>,
    <trd>opuchlina <klr>(tekutinová)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>edém</trd>,
    <trd>opuchlina</trd>,
    <trd>otok</trd>
  </trdgrp>
</drv>
</art>
