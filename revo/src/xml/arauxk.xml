<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arauxk.xml,v 1.17 2013/10/26 16:10:23 revo Exp $">
<kap>
  <rad>ara&ubreve;kari</rad>/o
</kap>
<drv mrk="arauxk.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="arauxk1.0oj">ara&ubreve;kariacoj</ref>
      (<trd lng="la">Araucaria</trd>), 
      <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">konifero</ref>
      kun skvamecaj plurjaraj folioj kaj ovoformaj konusoj,
      kreskanta en tropikaj landoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_w;&c_k;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">arauc&agrave;ria</trd>
  <trdgrp lng="de">
    <trd>Zimmertanne</trd>,
    <trd>Araukarie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monkey puzzle</trd>,
    <trd>Chili pine</trd>
  </trdgrp>
  <trd lng="fr">araucaria</trd>
  <trd lng="hu">arauk&aacute;ria</trd>
  <trd lng="ru">&c_a;&c_r;&c_a;&c_u;&c_k;&c_a;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
