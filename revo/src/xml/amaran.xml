<art mrk="$Id: amaran.xml,v 1.17 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>amarant</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="amaran.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Amarantus</trd>),
      kies floroj estas malhele karminru&gcirc;aj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_ib;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_shch;&c_i;&c_r;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>amarant</trd>,
    <trd>laskavec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Amarant</trd>, <trd>Fuchsschwanz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amaranth</trd>,
    <trd>love-lies-bleeding</trd>,
    <trd>Prince's feather</trd>,
    <trd>pigweed</trd>
  </trdgrp>
  <trd lng="es">amaranto</trd>
  <trdgrp lng="fr">
    <trd>amarante</trd>,
    <trd>amaranthe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>diszn&oacute;par&eacute;j</trd>,
    <trd>amar&aacute;nt</trd>
  </trdgrp>
  <trd lng="nl">amarant</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_v;&c_je;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">l√°skavec</trd>
</drv>
