<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hipere.xml,v 1.16 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hipereben</rad>/o
</kap>
<drv mrk="hipere.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;158</lok></fnt>
  <snc>
    <dif>
      <ref cel="hipere.afina0o">Afina <tld/>o</ref> a&ubreve;
      <ref cel="hipere.vektora0o">vektora <tld/>o</ref>, la&ubreve; la
      kunteksto.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">nadrovina</trd>
  <trd lng="de">Hyperebene</trd>
  <trd lng="en">hyperplane</trd>
  <trd lng="fa">&a_A;&a_b;&a_r;&a_S;&a_f;&a_H;&a_h;</trd>
  <trd lng="fr">hyperplan</trd>
  <trd lng="hu">hipers&iacute;k</trd>
  <trd lng="it">iperpiano</trd>
  <trd lng="nl">hypervlak</trd>
  <trd lng="pl">hiperp&lstroke;aszczyzna</trd>
  <trd lng="pt">hiperplano</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">nadrovina</trd>
</drv>

<drv mrk="hipere.afina0o">
  <kap>afina <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="spac.afinasub0o">Afina subspaco</ref>,
      direktata de
      <ref cel="hipere.vektora0o">vektora <tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_ib;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_ib;&c_p;&c_e;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  <trd lng="de">affine <ind>Hyperebene</ind></trd>
  <trd lng="en">affine <ind>hyperplane</ind></trd>
  <trd lng="fa">&a_A;&a_b;&a_r;&a_S;&a_f;&a_H;&a_h;&a_hamza_sure; &a_A_madda;&a_f;&f_y;&a_n;</trd>
  <trd lng="fr">hyperplan affine</trd>
  <trd lng="hu">affin <ind>hipers&iacute;k</ind></trd>
  <trd lng="it"><ind>iperpiano</ind> affine</trd>
  <trd lng="pl">hiperp&lstroke;aszczyzna afiniczna</trd>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_i;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_i;&c_p;&c_je;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
</drv>

<drv mrk="hipere.vektora0o">
  <kap>vektora <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="spac.vektorasub0o">Vektora subspaco</ref>
      <ref cel="komple1.0a.vektorasubspaco">komplementa</ref> al
      <ref cel="rekt.vektora0o">rekto</ref>:
      <ekz>
        en tridimensiaj spacoj, la <tld/>oj estas ebenoj;
      </ekz>
      <ekz>
        en <frm><k>n</k></frm>-dimensiaj spacoj, la <tld/>oj 
        estas &ccirc;iuj (<frm><k>n</k></frm>-1)-dimensiaj
        subspacoj;
      </ekz>
      <ekz>
        por &ccirc;iu hiperebeno ekzistas iu lineara
        <ref cel="form.0o.bildigo">formo</ref>, kies
        <ref cel="kern.0o.homomorfio">kerno</ref> &gcirc;i estas.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_k;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_ib;&c_p;&c_e;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  <trd lng="de">Hyperebene</trd>
  <trd lng="en">linear <ind>hyperplane</ind></trd>
  <trd lng="fa">&a_A;&a_b;&a_r;&a_S;&a_f;&a_H;&a_h;&a_hamza_sure; &a_b;&a_r;&a_d;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">hyperplan vectoriel</trd>
  <trd lng="hu">hipers&iacute;k</trd>
  <trd lng="it"><ind>iperpiano</ind> vettoriale</trd>
  <trd lng="ru">&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_i;&c_p;&c_je;&c_r;&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
</drv>
</art>
