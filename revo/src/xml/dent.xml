<drv mrk="dent.0igi">
  <kap><tld/>igi, <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="D"/>oforme tran&ccirc;i a&ubreve; aran&gcirc;i la randon
      de io:
      <ekz>
        <ind><tld/>ita</ind> rado, muro, falbalo, folio;
        <trd
          lng="be">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_a;&c_n;&c_y;</trd>
        <trd lng="ca">dentat</trd>
        <trd lng="he">&mem;&shin;&vav;&nun;&fnun;</trd>
        <trd lng="hu">fogazott ker&eacute;k</trd>
        <trd lng="pl">z&aogonek;bkowa&cacute;
          <klr>(materia&lstroke;)</klr></trd>
        <trd
        lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
        <trd lng="es">dentado</trd>
      </ekz><ekz>
        la pintoj de la montaro segil<tld/>is la horizonton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brecx">bre&ccirc;eti</ref>,
      <ref cel="feston">festoni</ref>
    </refgrp>
  </snc>
  <trd lng="ca">dentar<klr> (tr.)</klr></trd>
  <trd lng="cs">ozubit</trd>
  <trd lng="es">dentar</trd>
  <trd lng="fr">denteler</trd>
  <trd lng="hu">fogaz</trd>
  <trdgrp lng="it">
    <trd>dentellare</trd>, <trd>merlare</trd>
  </trdgrp>
  <trd lng="nl">tanden</trd>
  <trd lng="ru">&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">ozubiť</trd>
</drv>

<drv mrk="dent.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="dent.0ingo.ANA">
