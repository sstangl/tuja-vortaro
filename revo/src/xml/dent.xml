<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dent.xml,v 1.57 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dent</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dent.0o.busxo">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara <ref tip="super" cel="ost.0o">osteto</ref>, en la <ref
        tip="malprt" cel="busx.0o.ANA">bu&scirc;o</ref> de homoj kaj
      bestoj,
      kiu servas por mordi kaj ma&ccirc;i la nutra&jcirc;on:
      <ekz>
        la <tld/>oj de leono estas akraj
        <fnt><bib>F</bib>, <lok>&FE; 7</lok></fnt>;
      </ekz>
      <ekz>
        tie estos la plorado kaj la grincado de <tld/>oj
        <fnt><bib>NT</bib>, <lok>&Mat; 8:12</lok></fnt>;
      </ekz>
      <ekz>
        kunfrapi la <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Psa; 112:10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri la <tld/>ojn
        <klr>(minaci)</klr>;
      </ekz>
      <ekz>
        nuksoj venis, kiam <tld/>oj elfalis
        <fnt><bib>PrV</bib></fnt>
        (<ref tip="vid" cel="fru.mal0a">malfrua</ref>)
      </ekz>
      <ekz>
        la <tld/>o de l' tempo formordis la piramidojn.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent</trd>,
      <trd>crochet<klr tip="ind"> (d'un serpent)</klr></trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="pl">z&aogonek;b</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;</trd>
  </snc>
  <snc mrk="dent.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elstara pinto a&ubreve; tran&ccirc;a&jcirc;o &ccirc;e la rando
      a&ubreve; la supra&jcirc;o de objekto:
      <ekz>
        <tld/>o de kombilo, rado, segilo, muro, roko, monto, folio.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
    <trd lng="ca">sortint</trd>
    <trd lng="en">tooth</trd>
    <trdgrp lng="fr">
      <trd>dent<klr tip="ind"> (d'une scie)</klr></trd>,
      <trd>encoche</trd>
    </trdgrp>
    <trd lng="nl">tand</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_je;&c_c;</trd>
  </snc>
  <trd lng="bg">&c_z;&c_malmol;&c_b;</trd>
  <trd lng="ca">dent</trd>
  <trd lng="cs">zub</trd>
  <trd lng="de">Zahn</trd>
  <trd lng="es">diente</trd>
  <trd lng="he">&shin;&fnun;</trd>
  <trd lng="hu">fog</trd>
  <trd lng="id">gigi</trd>
  <trd lng="it">dente</trd>
  <trd lng="pl">z&aogonek;b</trd>
  <trd lng="pt">dente</trd>
  <trd lng="sk">zub</trd>
</drv>

<drv mrk="dent.0a">
  <kap><tld/>a</kap>
  <snc mrk="dent.0a.rilata_al_dento">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">FON</uzo>
        <ref cel="dent.0akonsonanto"><tld/>a konsonanto</ref>;
      </ekz><ekz>
        <uzo tip="fak">ANA</uzo>
        <tld/>a emajlo.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_y;</trd>
    <trd lng="ca">dental</trd>
    <trd lng="en">dental</trd>
    <trd lng="es">dental</trd>
    <trdgrp lng="fr">
      <trd>dental<klr tip="ind"> (adj.)</klr></trd>,
      <trd>dentaire</trd>
    </trdgrp>
    <trd lng="he">&dalet;&nun;&tet;&lamed;&yod;</trd>
    <trd lng="hu">fog-</trd>
    <trd lng="it">dentale</trd>
    <trd lng="nl">tand-</trd>
    <trd lng="pl">z&eogonek;bny</trd>
    <trd lng="ru">&c_z;&c_u;&c_b;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="dent.0a.denthava">
    <dif>
      <tld lit="D"/>hava:
      <ekz>
        <ind><tld/>a rado</ind>.
        <trd lng="he">&#8503;&lamed;&#8503;&lamed;
        &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_a;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">dentat</trd>
    <trd lng="es">dentado</trd>
    <trd lng="fr">dent&eacute;</trd>
    <trd lng="he">&mem;&shin;&vav;&nun;&fnun;</trd>
    <trdgrp lng="hu">
      <trd>fogas</trd>,
      <trd>fogazott</trd>
    </trdgrp>
    <trd lng="it">dentato</trd>
    <trd lng="pl">z&eogonek;baty</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_u;&c_b;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="nl">getand</trd>
  </snc>
  <trd lng="cs">zubatý</trd>
  <trd lng="de">Zahn-</trd>
  <trd lng="sk">zubatý</trd>
</drv>

<drv mrk="dent.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="dent.0aro.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tuto de la <ref tip="prt" cel="dent.0o.busxo"><tld/>oj</ref>:
      <ekz>
        li havas falsan <tld/>aron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>dentadura</trd>,
    <trd>dentat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chrup</trd>,
    <trd>ozubení</trd>,
    <trd>zoubkování</trd>
  </trdgrp>
  <trd lng="de">Gebiss</trd>
  <trd lng="es">dentadura</trd>
  <trdgrp lng="fr">
    <trd>denture</trd>,
    <trd>dentition<klr tip="ind"> (denture)</klr></trd>
  </trdgrp>
  <trd lng="hu">fogazat</trd>
  <trd lng="it">dentatura</trd>
  <trd lng="nl">gebit</trd>
  <trd lng="pl">uz&eogonek;bienie</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_y;<klr> (&c_v;
  &c_s;&c_o;&c_v;&c_o;&c_k;&c_u;&c_p;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  <trd lng="sk">chrup</trd>
</drv>

<drv mrk="dent.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="dent.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa <ref tip="super" cel="dent.0o.busxo"><tld/>o</ref>, kiu
      eliras el la bu&scirc;o de tiaj bestoj, kiaj elefantoj, aproj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_b;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">ullal</trd>
  <trd lng="cs">kel</trd>
  <trdgrp lng="de">
    <trd>Hauer</trd>, <trd>Sto&szlig;zahn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tusk</trd>,
    <trd>tush</trd>
  </trdgrp>
  <trd lng="es">colmillo</trd>
  <trd lng="fr">d&eacute;fense<klr tip="ind"> (dent)</klr></trd>
  <trd lng="he">&nun;&yod;&bet;</trd>
  <trd lng="hu">agyar</trd>
  <trd lng="it">zanna</trd>
  <trd lng="nl">slagtand</trd>
  <trd lng="pl">kie&lstroke;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_y;&c_k;</trd>,
    <trd>&c_b;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
