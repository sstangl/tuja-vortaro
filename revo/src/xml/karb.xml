<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karb.xml,v 1.28 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karb</rad>/o
</kap>
<drv mrk="karb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola nigra substanco, konsistanta el preska&ubreve; pura 
      <ref tip="super" cel="karbon.0o">karbono</ref>,
      devenanta nature a&ubreve; arte de ligno, kaj uzata por hejti:
      <ekz>
        &scirc;ton<tld/>o a&ubreve; ter<tld/>o;
      </ekz>
      <ekz>
        <ind>konservas e&ccirc; <tld/>o la strukturon de l' arbo</ind>
        <klr>(oni konservas &ccirc;iam signojn de sia deveno)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">la <ind>caque</ind> sent toujours le hareng</trd>
        <trd lng="nl">men kan zijn <ind>afkomst</ind> niet
        verloochenen</trd>
      </ekz>
      <ekz>
        <ind>blanka <tld/>o</ind>
        <klr>(fluakvo uzata por produkti elektron)</klr>;
        <trd lng="ca">energia hidroel&egrave;ctrica</trd>
        <trd lng="fr">houille blanche</trd>
        <trd lng="nl">witte <ind>steenkool</ind></trd>
        <trd lng="pl">bia&lstroke;y w&eogonek;giel</trd>
      </ekz>
      <ekz>
        nigraj <ind><tld/>igita</ind>j traboj;
        <trd lng="ca">carbonitzat</trd>
        <trd lng="fr">carbonis&eacute;</trd>
        <trd lng="nl">verkoold</trd>
        <trd
lng="ru">&c_o;&c_b;&c_u;&c_g;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        la macoj<ind> <tld/>i&gcirc;i</ind>s en la forno<fnt>Z</fnt>.
        <trd lng="ca">carbonitzar-se</trd>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="antrac.0o">antracito</ref>,
      <ref cel="karb.ligno0o">ligno<tld/>o</ref>,
      <ref cel="karb.ter0o">ter<tld/>o</ref>,
      <ref cel="lignit.0o">lignito</ref>,
      <ref cel="torf.0o">torfo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Peco el <tld/>o:
      <ekz>
        <tld/>oj ekflamis de Dia bu&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ardantaj kaj brulantaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sidi kvaza&ubreve; sur flamantaj <tld/>oj<fnt>B</fnt>
        <klr>(senpaciencege. <ref tip="sin"
        cel="pingl.0o">pinglo</ref>)</klr>.
        <trd lng="pl">siedzie&cacute; jak na roz&zdot;arzonych
w&eogonek;glach</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_g;&c_l;&c_i;&c_shch;&c_a;</trd>
  <trd lng="br">glaou</trd>
  <trd lng="ca">carb&oacute;</trd>
  <trdgrp lng="cs">
    <trd>uhel</trd>,
    <trd>uhl&iacute;</trd>
  </trdgrp>
  <trd lng="de">Kohle</trd>
  <trd lng="en">coal</trd>
  <trd lng="es">carb&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>charbon<klr tip="ind"> (mati&egrave;re, morceau de
charbon)</klr></trd>,
    <trd>houille</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;n</trd>
  <trd lng="id">arang</trd>
  <trd lng="nl">kool<klr tip="ind"> (grondstof)</klr></trd>
  <trd lng="pl">w&eogonek;giel</trd>
  <trd lng="ro">c&abreve;rbune</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">uhlie</trd>
  <trd lng="sv">kol</trd>
</drv>

<drv mrk="karb.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <ref tip="dif" cel="bragx.0o">bra&gcirc;o</ref>
  </snc>
  <trdgrp lng="br">
    <trd>glaou bev</trd>,
    <trd>glaou tan</trd>
  </trdgrp>
  <trd lng="ca">brasa</trd>
  <trdgrp lng="cs">
    <trd>malý uhel</trd>,
    <trd>uhlík</trd>
  </trdgrp>
  <trd lng="fr">braise</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zs</trd>,
    <trd>fasz&eacute;n</trd></trdgrp>
  <trd lng="nl">stuk <ind>houtskool</ind></trd>
  <trd lng="pl">w&eogonek;gielek</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>drobné uhlie</trd>,
    <trd>uhlík</trd>
  </trdgrp>
</drv>

<drv mrk="karb.0desegnajxo">
  <kap><tld/>desegna&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegna&jcirc;o farita per <tld/>krajono <sncref
ref="karb.0krajono.krajono"/>.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>carb&oacute;</trd>,
    <trd>dibuix al carb&oacute;</trd>
  </trdgrp>
  <trd lng="de">Kohlezeichnung</trd>
  <trd lng="fr">fusain<klr tip="ind"> (dessin)</klr></trd>
  <trd lng="hu">sz&eacute;nrajz</trd>
  <trd lng="nl">houtskooltekening</trd>
  <trd lng="pl">rysunek <ind>w&eogonek;glem</ind></trd>
  <trd lng="ru">&c_r;&c_i;&c_s;&c_u;&c_n;&c_o;&c_k;
<ind>&c_u;&c_g;&c_l;&c_je;&c_m;</ind></trd>
</drv>

<drv mrk="karb.0krajono">
  <kap><tld/>krajono</kap>
  <snc mrk="karb.0krajono.krajono">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Peco da ligno<tld/>o uzata por desegni.
    </dif>
    <trd lng="br">glaouenn-dresa&ntilde;</trd>
    <trd lng="ca">carbonet</trd>
    <trdgrp lng="fr">
      <trd>charbon<klr tip="ind"> (de fusain)</klr></trd>,
      <trd>fusain<klr tip="ind"> (crayon)</klr></trd>
    </trdgrp>
    <trd lng="hu">rajzsz&eacute;n</trd>
    <trd lng="id"><ind>pensil</ind> arang</trd>
    <trd lng="nl">houtskoolpotlood</trd>
    <trd lng="pl">w&eogonek;giel<klr> rysunkowy</klr></trd>
    <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_mol;<klr tip="amb">
(&c_d;&c_l;&c_ja;
&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="karb.0krajono.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <tld lit="K"/>opeco, cilindra a&ubreve; prisma, uzata kiel pozitiva
poluso
      en elektraj aparatoj.
    </dif>
    <adm>
      Tion &ccirc;i PIV1 nomas "~pinto". Cetere la uzo estas
      pli vasta, ekz-e &ccirc;e motoroj, kaj ili estas farataj el karbono
      pli ol el karbo.
      <aut>MB</aut>
    </adm>
    <trd lng="fr">charbon<klr tip="ind">
(&eacute;lectricit&eacute;)</klr></trd>
    <trd lng="hu">an&oacute;dsz&eacute;n</trd>
    <trd lng="nl">koolstaaf</trd>
    <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_s;&c_t;&c_je;&c_r;&c_zh;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="de">Kohlestift</trd>
</drv>

<drv mrk="karb.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="brul.0igajxo">Bruliga&jcirc;o</ref>,
      produktata per malrapida kaj nekompleta brulo de ligno en senaera
      atmosfero:
      <ekz>
        en portebla fajrujo brulis amaseto da ligno<tld/>o, plenigante
        la
        &ccirc;ambron per komfortiga ardo
        <fnt><bib>I&scirc;tar</bib></fnt>;
      </ekz>
      <ekz>
        Zalmuna malmoligis la <klr>[vaksajn]</klr> cilindrojn super la
ardantaj
        ligno<tld/>oj <klr>(t.e. <tld/>etoj el ligno<tld/>o)</klr>
        <fnt><bib>I&scirc;tar</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_je;&c_n;&c_i; &c_v;&c_malmol;&c_g;&c_l;&c_i;&c_shch;&c_a;</trd>
  <trd lng="br">glaou-koad</trd>
  <trd lng="ca"><ind>carb&oacute;</ind> vegetal</trd>
  <trd lng="cs">dřevěné uhlí</trd>
  <trd lng="de">Holzkohle</trd>
  <trd lng="fr">charbon de bois</trd>
  <trd lng="hu">fasz&eacute;n</trd>
  <trd lng="id"><ind>arang</ind> kayu</trd>
  <trd lng="nl">houtskool</trd>
  <trd lng="pl">w&eogonek;giel drzewny</trd>
  <trd lng="ro">mangal</trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;
&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">drevené uhlie</trd>
  <trd lng="sv">tr&auml;kol</trd>
</drv>

<drv mrk="karb.ter0o">
  <kap>ter<tld/>o <var><kap>min<tld/>o</kap></var>
  <var><kap>&scirc;ton<tld/>o</kap></var></kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala <tld/>o, eltirata el la tera profundo por servi
      kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>.
    </dif>
  </snc>
  <ref tip="vid" cel="min.0ejo">minejo.</ref>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;
&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</trd>
  <trd lng="br">glaou-douar</trd>
  <trdgrp lng="ca">
    <trd>carb&oacute; mineral</trd>,
    <trd>hulla</trd>,
    <trd>antracita</trd>
  </trdgrp>
  <trd lng="de">Steinkohle</trd>
  <trd lng="fr">houille</trd>
  <trd lng="hu">k&odblac;sz&eacute;n</trd>
  <trd lng="id"><ind>batu</ind> bara</trd>
  <trd lng="nl">steenkool</trd>
  <trd lng="pl">w&eogonek;giel kamienny</trd>
  <trd lng="ro">huil&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;
&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">lignit</trd>
  <trd lng="cs">lignit</trd>
</drv>
</art>
