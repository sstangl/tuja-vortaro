<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eritro.xml,v 1.11 2013/08/19 13:10:23 revo Exp $">
<kap>
  <rad>eritrozin</rad>/o
</kap>

<drv mrk="eritro.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Ru&gcirc;a substanco tre koloriga, uzata en histologio kaj
      fotografio.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_y;&c_t;&c_r;&c_a;&c_z;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_je;&c_r;&c_i;&c_t;&c_r;&c_o;&c_z;&c_i;&c_n;</trd>
  <trd lng="de">Erythrosin</trd>
  <trd lng="en">erythrosine</trd>
  <trd lng="fr">&eacute;rythrosine</trd>
  <trd lng="hu">eritrozin</trd>
  <trd lng="nl">erythrosine</trd>
  <trd lng="pl">erytrozyna</trd>
  <trdgrp lng="pt">
    <trd>eritrosina</trd>,
    <trd>eritrina</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_r;&c_i;&c_t;&c_r;&c_o;&c_z;&c_i;&c_n;</trd>
</drv>
</art>
