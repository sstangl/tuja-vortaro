<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaban.xml,v 1.24 2015/07/04 08:37:36 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>kaban</rad>/o
</kap>
<drv mrk="kaban.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Malri&ccirc;a dometo, konstruita el krudaj kaj ne &ccirc;iam solidaj
       materialoj:
       <ekz>
         <tld/>o de kampulo, de montano;
       </ekz>
       <ekz>
         ter<tld/>o;
       </ekz>
       <ekz>
         bran&ccirc;o<tld/>o;
       </ekz>
       <ekz>
         <tld/>oj de sova&gcirc;uloj.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_h;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_l;&c_u;&c_p;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_t;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="br">
     <trd>kabanenn</trd>,
     <trd>loch</trd>,
     <trd>lochenn</trd>,
     <trd>log</trd>,
     <trd>logenn</trd>
   </trdgrp>
  <trd lng="ca">cabana</trd>
  <trdgrp lng="cs">
    <trd>bouda</trd>,
    <trd>chalupa</trd>,
    <trd>chata</trd>,
    <trd>chýše</trd>,
    <trd>provizorní obydlí</trd>
  </trdgrp>
  <trd lng="de">H&uuml;tte</trd>
  <trdgrp lng="en">
     <trd>cabin</trd>,
     <trd>hut</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>caba&ntilde;a</trd>,
     <trd>choza</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&a_A_madda;&a_l;&a_w;&a_n;&f_k;</trd>,
     <trd>&f_k;&a_l;&a_b;&a_h;</trd>
   </trdgrp>
  <trd lng="fi">maja</trd>
  <trdgrp lng="fr">
     <trd>cabane</trd>,
     <trd>cahute</trd>,
     <trd>hutte</trd>
   </trdgrp>
  <trd lng="hu">kunyh&oacute;</trd>
  <trd lng="nl">hut</trd>
  <trd lng="pl">chata</trd>
  <trdgrp lng="pt">
     <trd>cabana</trd>,
     <trd>barrac&atilde;o</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_h;&c_i;&c_zh;&c_i;&c_n;&c_a;</trd>,
     <trd>&c_l;&c_a;&c_ch;&c_u;&c_g;&c_a;</trd>,
     <trd>&c_i;&c_z;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>chalupa</trd>,
    <trd>chata</trd>,
    <trd>chyža</trd>
  </trdgrp>
  <trd lng="sv">hydda</trd>
  <trd lng="tr">kul&uuml;be</trd>
</drv>
</art>
