<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fontan.xml,v 1.18 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fontan</rad>/o
</kap>
<drv mrk="fontan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fontan.0o.AKV">
    <dif>
      Loko a&ubreve; konstrua&jcirc;o, el kiu artefare elfluas akvo:
      <ekz>
        per statuoj ornamita <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Konstrua&jcirc;o kun artefarita tubaran&gcirc;o, kiu alten &scirc;prucigas akvon.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>fontána</trd>,
    <trd>kašna</trd>,
    <trd>vodotrysk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Springbrunnen</trd>,
    <trd>Font&auml;ne</trd>
  </trdgrp>
  <trd lng="en">fountain</trd>
  <trd lng="es">fuente</trd>
  <trd lng="fr">fontaine</trd>
  <trd lng="hu">sz&ouml;k&odblac;k&uacute;t</trd>
  <trd lng="id"><ind>air</ind> mancur</trd>
  <trd lng="nl">fontein</trd>
  <trdgrp lng="pt">
    <trd>fonte</trd>,
    <trd>chafariz</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>fontána</trd>,
    <trd>vodomet</trd>
  </trdgrp>
</drv>
</art>
