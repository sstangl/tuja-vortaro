<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cereal.xml,v 1.24 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>cereal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cereal.0o">
  <kap><tld/>o</kap>
  <snc mrk="cereal.0o.AGR">
    <tezrad fak="AGR"/>
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="sub" cel="gren.0o.planto">Greno</ref> a&ubreve;
      grensimila vegeta&jcirc;o;
      <ref tip="sin" cel="herb.pan0o.AGR">panherbo</ref>:
      <ekz>
        sova&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        hordeo, aveno, sekalo, tritiko estas kulturaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;</trd>,
    <trd>&c_z;&c_b;&c_a;&c_zh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">cereal</trd>
  <trd lng="cs">obilovina</trd>
  <trd lng="de">Getreide</trd>
  <trd
  lng="el">&delta;&eta;&my;&eta;&tau;&rho;&jota;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">cereal</trd>
  <trd lng="es">cereal</trd>
  <trd lng="fr">c&eacute;r&eacute;ale</trd>
  <trdgrp lng="hu">
    <trd>gabonan&ouml;v&eacute;ny</trd>,
    <trd>gabonaf&eacute;le</trd>
  </trdgrp>
  <trd lng="it">cereale</trd>
  <trd lng="nl">koren</trd>
  <trd lng="pl">zbo&zdot;e</trd>
  <trd lng="pt">cereal</trd>
  <trd lng="ru">&c_z;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>obilie</trd>,
    <trd>obilniny</trd>
  </trdgrp>
</drv>
</art>
