<art mrk="$Id: terci.xml,v 1.15 2015/12/26 15:10:20 revo Exp $">
<kap>
  <rad>terci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="terci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="trit.0o">trito</ref>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_c;&c_a;<klr> (&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="ca">tercera</trd>
  <trd lng="cs">tercie</trd>
  <trd lng="de">Terz</trd>
  <trdgrp lng="en">
    <trd>third</trd>,
    <trd>tritone</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tierce<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="hu">terc</trd>
  <trd lng="nl">terts</trd>
  <trd lng="pl">tercja <klr tip="amb">(muzyka)</klr></trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">tercia</trd>
</drv>
</art>
