<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kupul.xml,v 1.9 2014/04/11 15:10:13 revo Exp $">
<kap>
  <rad>kupul</rad>/o
</kap>
<drv mrk="kupul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Malgranda folieca a&ubreve; skvameca kaliko, kiu formas la bazon de
kelkaj
      fruktoj.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Becher</trd>,
     <trd>Fruchtbecher</trd>,
     <trd>Cupula</trd>
  </trdgrp>
  <trd lng="fr">cupule</trd>
  <trd lng="hu">kupacs</trd>
  <trd lng="nl">dop<klr tip="ind"> (van eikels enz.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_je;&c_ch;&c_k;&c_a;
<klr>(&c_zh;&c_jo;&c_l;&c_u;&c_d;&c_ja; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>
</art>
