<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konson1.xml,v 1.23 2016/01/04 04:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konsonant</rad>/o
</kap>
<drv mrk="konson1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="FON"/>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Nesilabiga <ref tip="super" cel="son.parol0o">parolsono</ref>:
      <ekz>
        duonrimoj, kie la <tld/>oj samas kaj la vokaloj malsamas (beno
        &mdash; suno; monto &mdash; vento; maro &mdash; deziro)
        <klr>...</klr>
        <fnt>
          <aut>Baldur Ragnarsson</aut>:
          <vrk>Iom pri la aliteracio</vrk>.
              <lok>Norda Prismo, 58:1, p.&nbsp;44.</lok>
        </fnt>.
      </ekz>
    </dif>
  <ref tip="vid" cel="son.0anto">sonanto</ref> 
  </snc>
  <rim num="1" mrk="konson1.0o.aktiva">
    La&ubreve; la <em>aktiva organo</em> oni distingas, interalie, la
    sekvajn <tld/>ajn klasojn:
    <refgrp tip="sub">
      <ref cel="lip.0akonsonanto">lipaj <tld/>oj</ref>,
      <ref cel="lang.0a">langaj <tld/>oj</ref>,
      <ref cel="uvul.0a">uvulaj <tld/>oj</ref>
    </refgrp>.
  </rim>
  <rim num="2" mrk="konson1.0o.pasiva">
    La&ubreve; la <em>pasiva organo</em> oni distingas, interalie, la
    sekvajn <tld/>ajn klasojn:
    <refgrp tip="sub">
      <ref cel="dent.0akonsonanto">dentaj <tld/>oj</ref>,
      <ref cel="palat.0a">palataj <tld/>oj</ref>,
      <ref cel="vel.0sono">velaj <tld/>oj</ref>,
      <ref cel="glot.0akonsonanto">glotaj <tld/>oj</ref>
    </refgrp>.
  </rim>
  <rim num="3" mrk="konson1.0o.paroj">
    La&ubreve; la <em>artikulaciaj paroj</em> de la organoj
    <ctl>aktiva&mdash;pasiva</ctl>:
    <refgrp tip="sub">
      <ref cel="lip.0dentakonsonanto">lip-dentaj <tld/>oj</ref>,
      <ref cel="lip.00akonsonanto">lip-lipaj <tld/>oj</ref>,
      <ref cel="dent.lang0akonsonanto">lang-dentaj <tld/>oj</ref>
    </refgrp>.
  </rim>
  <rim num="4" mrk="konson1.0o.kvanto">
    La&ubreve; la <em>kvanto</em>: la <tld/>oj momentaj, da&ubreve;raj,
    duoblaj,
    longaj.
  </rim>
  <rim num="5" mrk="konson1.0o.vocxrolo">
    La&ubreve; la rolo de la vo&ccirc;o kaj stre&ccirc;o:
    <refgrp tip="sub">
      <ref cel="vocx.0a.FON">vo&ccirc;aj</ref>,
      <ref cel="vocx.sen0a.FON">senvo&ccirc;aj</ref>,
      <ref cel="sonor.0anto">sonorantoj</ref>,
      <ref cel="bru.0anto">bruantoj</ref> 
    </refgrp>
  </rim>
  <rim num="6" mrk="konson1.0o.maniero">
    La&ubreve; la <em>esti&gcirc;maniero</em>:
    <refgrp tip="sub">
      <ref cel="frot.ek0akonsonanto">ekfrotaj <tld/>oj</ref>,
      <ref cel="frot.0sono">frotaj <tld/>oj</ref>,
      <ref cel="eksplo.0akonsonanto">eksplodaj <tld/>oj</ref>,
      <ref cel="naz.0akonsonanto">nazaj <tld/>oj</ref>,
      <ref cel="busx.0a.FON">bu&scirc;aj <tld/>oj</ref>,
      <ref cel="flu.0akonsonanto">fluaj <tld/>oj</ref>
    </refgrp>
  </rim>
  <trd lng="be">&c_z;&c_y;&c_ch;&c_n;&c_y;
  <klr>(&c_g;&c_u;&c_k;)</klr></trd>
  <trdgrp lng="de">
    <trd>Konsonant</trd>,
    <trd>Mitlaut</trd>,
    <trd>Mitlauter</trd>
  </trdgrp>
  <trd lng="es">consonante</trd>
  <trd lng="en">consonant</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n;<klr tip="ind">
    (&a_A_madda;&a_w;&a_A;&a_sh;&a_n;&a_A;&a_s;&f_y;)</klr></trd>,
    <trd>&a_S;&a_A;&a_m;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">consonne</trd>
  <trd lng="nl">medeklinker</trd>
  <trd lng="pt">consoante <klr tip="amb">(fon.)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;<klr>
  (&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  <trd lng="sk">spoluhl&aacute;ska</trd>
  <trdgrp lng="cs">
    <trd>konsonant</trd>,
    <trd>souhl&aacute;ska</trd>
  </trdgrp>
  <trd lng="th">&#3614;&#3618;&#3633;&#3597;&#3594;&#3609;&#3632;</trd>
</drv>

<drv mrk="konson1.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        ili ofte tenas <tld/>an fina&jcirc;on, kiun la aliaj lingvoj,
        kaj e&ccirc;
        la mandarena [...] perdis
        <fnt><aut>Bradley Kendal</aut>: <vrk>La korea</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 3a, p.
        21-23a</lok></fnt>;
      </ekz><ekz>
        Ni memoru, ke la greka <klr>lingvo</klr> enkondukis en
        Esperanton multajn <tld/>ajn grupojn
        <fnt>
          <aut>Marc Bavant</aut>: <vrk>Silabo kaj silabado</vrk>.
          <lok><url
              ref="http://lingvakritiko.com/2006/12/20/silabo-kaj-silabado/"
              >Lingva kritiko</url></lok></fnt>;
      </ekz><ekz>
        la araba skribsistemo estas precipe <tld/>a
        <fnt>
          <aut>Garvan MAKAJ'</aut>:
          <vrk>Disleksio kaj literumado</vrk>.
          <bib>&c_M;&c_o;&c_n;&c_a;&c_t;&c_o;</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">conson&aacute;ntico</trd>
  <trdgrp lng="fr">
    <trd>consonantique</trd>
  </trdgrp>
  <trd lng="nl">meeklinkend</trd>
  <trd
  lng="ru">&c_k;&c_o;&c_n;&c_s;&c_o;&c_n;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">konsonant</trd>
</drv>
</art>
