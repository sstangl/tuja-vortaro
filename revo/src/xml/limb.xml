<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: limb.xml,v 1.11 2014/04/12 14:10:20 revo Exp $">
<kap>
  <rad>limb</rad>/o
</kap>
<drv mrk="limb.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Gradita rando de cirklo, &ccirc;e astronomia, geodezia a&ubreve;
      matematika instrumento.
    </dif>
    <trdgrp lng="de">
      <trd>Saum</trd>,
      <trd>Rand</trd>
    </trdgrp>
    <trd lng="en">limb</trd>
    <trd lng="fr">limbe</trd>
    <trdgrp lng="hu">
      <trd>fokbeoszt&aacute;s</trd>,
      <trd>limbusz</trd>
    </trdgrp>
    <trd lng="pt">limbo <klr>(t&eacute;cn.)</klr></trd> 
    <trd lng="ru">&c_l;&c_i;&c_m;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Membraneca maldika parto de la folio senpere insertita sur la tigo
      a&ubreve; portita sur la petiolo.
    </dif>
    <trdgrp lng="de">
      <trd>Limbus</trd>,
      <trd>Blattrand</trd>
    </trdgrp>
    <trd lng="en">limb</trd>
    <trd lng="fr">limbe</trd>
    <trd lng="pt">limbo <klr>(bot.)</klr></trd>
    <trd lng="ru">&c_v;&c_l;&c_a;&c_g;&c_a;&c_l;&c_i;&c_shch;&c_je; <klr tip="amb">(&c_l;&c_i;&c_s;&c_t;&c_a; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      La disetendita parto de petalo a&ubreve; sepalo.
    </dif>
    <trdgrp lng="de">
      <trd>Kelchsaum</trd>,
      <trd>Limbus</trd>
    </trdgrp>
    <trd lng="en">limb</trd>
    <trd lng="fr">limbe</trd>
    <trd lng="nl">bladschijf</trd>
    <trd lng="pt">limbo</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_r;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_je;&c_p;&c_je;&c_s;&c_t;&c_k;&c_a;,
    &c_l;&c_i;&c_s;&c_t;&c_a;)</klr></trd>
  </snc>
</drv>
</art>
