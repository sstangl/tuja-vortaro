<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pop.xml,v 1.16 2015/07/04 08:37:41 revo Exp $">
<kap>
<rad>pop</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="pop.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KRI</uzo>
    <dif>
      Neformala sinonimo por ortodokskristana
      <ref tip="super" cel="pastr.0o.presbitero">pastro</ref>
      (<ref tip="super" cel="presbi.0o.duagrada">presbitero</ref>).
    </dif>
    <ref tip="vid" cel="ortodo.0akristanismo">ortodoksa
    kristanismo</ref>
    <rim>Ne konfuzu kun <ref tip="vid" cel="pap.0o">papo</ref>.</rim>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_p;</trd>
  <trdgrp lng="cs">
    <trd>mll. pop</trd>,
    <trd>pop-art</trd>
  </trdgrp>
  <trd lng="de">Pope</trd>
  <trd lng="el">&pi;&alfa;&pi;&alfa_ton;&sigma_fina;</trd>
  <trd lng="en">pope<klr tip="amb"> (Eastern orthodox)</klr></trd>
  <trd lng="fr">pope</trd>
  <trd lng="hu">p&oacute;pa</trd>
  <trd lng="nl">pope</trd>
  <trd lng="pt">pope</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;</trd>
  <trd lng="sk">pop-art</trd>
</drv>
</art>
