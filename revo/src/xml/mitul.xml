<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mitul.xml,v 1.19 2013/09/16 16:10:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mitul</rad>/o
</kap>
<drv mrk="mitul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="mitule.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mytilus</trd>), konko, kies unu mara specio estas
      multe man&gcirc;ata.
    </dif>
  </snc>                     
  <trd lng="be">&c_m;&c_ib;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_i;&c_d;&c_a;</trd>
  <trd lng="br">mesklenn</trd>
  <trd lng="ca">musclo</trd>
  <trd lng="de">Miesmuschel</trd>
  <trd lng="en">common <ind>mussel</ind></trd>
  <trd lng="fr">moule</trd>
  <trd lng="hu">kagyl&oacute;<klr tip="amb"> (ehet&odblac;)</klr></trd>
  <trd lng="nl">mossel</trd>
  <trdgrp lng="pt">
   <trd>mexilh&atilde;o</trd>,
   <trd>sururu</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>
