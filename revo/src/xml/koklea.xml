<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koklea.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
<rad>kokleari</rad>/o
</kap>
<drv mrk="koklea.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Cochlearia</trd>),
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herbo</ref>,
      kies unu specio <trd lng="la">Cochlearia officinalis</trd>
      kun renformaj bazaj folioj kaj korformaj tigofolioj
      en E&ubreve;ropo estis uzata kontra&ubreve; skorbuto kaj tiucele salite
      kunportita dum marvoja&gcirc;oj.
    </dif>
  </snc>
  <trd lng="cs">lžičník</trd>
  <trd lng="de">L&ouml;ffelkraut</trd>
  <trd lng="fr">cochl&eacute;aria</trd>
  <trdgrp lng="hu">
    <trd>kal&aacute;nf&udblac;</trd>,
    <trd>kan&aacute;lf&udblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lepelblad</trd>,
    <trd>lepelkruid</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_zh;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">lyžičník</trd>
</drv>
</art>
