<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aten.xml,v 1.7 2016/04/12 10:10:19 revo Exp $">
<kap>
    <rad>Aten</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="aten.0o">
  <kap><tld/>o</kap>
  <snc mrk="aten.0o.SNC">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Nuna <ref tip="lst" cel="urb.cxef0o" 
      lst="voko:&ccirc;efurboj">&ccirc;efurbo</ref> de <ref tip="malprt" 
      cel="grek.G0ujo">Grekujo</ref>, lo&gcirc;ata jam de 5000 jaroj:
      <ekz>
        Pa&ubreve;lo atendis ilin en <tld/>o, lia spirito &ccirc;agreni&gcirc;is en li,
        kiam li vidis la urbon idolplena
        <fnt><bib>NT</bib>, <lok>La agoj 17:15</lok></fnt>
      </ekz>
      <ekz>
        li foriris el <tld/>o kaj alvenis en Korinton
        <fnt><bib>NT</bib>, <lok>La agoj 17:34</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_t;&c_e;&c_n;&c_y;</trd>
  <trd lng="cs">At&eacute;ny</trd>
  <trd lng="de">Athen</trd>
  <trd lng="el">&Alfa;&theta;&eta_ton;&ny;&alfa;</trd>
  <trd lng="en">Athens</trd>
  <trd lng="fr">Ath&egrave;nes</trd>
  <trd lng="pl">Ateny</trd>
  <trd lng="sk">At&eacute;ny</trd>
</drv>
</art>

