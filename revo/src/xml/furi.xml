<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: furi.xml,v 1.20 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>furi</rad>/o
</kap>
<drv mrk="furi.0o">
  <kap><tld/>o</kap>
  <snc mrk="furi.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Infera diino de ven&gcirc;o:
      <ekz>
        la tri <tld/>oj.
      </ekz>
     </dif>
  <trd lng="pt">F&uacute;ria</trd>
  </snc>
  <snc mrk="furi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioza virino.
    </dif>
  <trdgrp lng="pt">
    <trd>f&uacute;ria</trd>,
    <trd>mulher furiosa</trd>,
  </trdgrp>
  </snc>
  <adm>
  Uskleco. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_f;&c_u;&c_r;&c_y;&c_ja;</trd>
  <trd lng="de">Furie</trd>
  <trdgrp lng="en">
    <trd>fury</trd>,
    <trd>goddess of vengeance</trd>
  </trdgrp>
  <trd lng="es">furia</trd>
  <trd lng="fr">furie<klr tip="ind"> (divinit&eacute;, personne
  furieuse)</klr></trd>
  <trd lng="hu">f&uacute;ria</trd>
  <trd lng="nl">furie</trd>
  <trd lng="pl">furia</trd>
  <trd lng="ru">&c_f;&c_u;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>fúria</trd>,
    <trd>zlá žena</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fúrie</trd>,
    <trd>lítice</trd>,
    <trd>saň</trd>,
    <trd>zlá žena</trd>
  </trdgrp>
</drv>
</art>
