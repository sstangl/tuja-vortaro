<vortaro>
<art mrk="$Id: tabane.xml,v 1.13 2015/10/23 14:10:22 revo Exp $">
<kap>
  <rad>tabaned</rad>/oj
</kap>
<drv mrk="tabane.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Tabanidae</trd>), kies inoj su&ccirc;as sangon.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ja;&c_p;&c_n;&c_ib; 
<klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tab&agrave;nids</trd>
  <trd lng="cs">ov&aacute;dovit&iacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Bremsen<klr tip="ind"> (Familie)</klr></trd>
  <trdgrp lng="en">
    <trd>Tabanidae</trd>,
    <trd>horse and deer flies</trd>
  </trdgrp>
  <trd lng="hu">b&ouml;g&ouml;lyf&eacute;l&eacute;k</trd>
  <trdgrp lng="it">
  <trd>tafani<klr tip="ind"> (zoo-famiglia)</klr></trd>, 
  <trd>tabanidi</trd>
  </trdgrp>
  <trd lng="nl">dazen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">b&aogonek;kowate</trd>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_p;&c_n;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">ovadovit&iacute;</trd>
  <trd lng="sv">bromsar<klr tip="ind">( familj)</klr></trd>
</drv>
</art>
