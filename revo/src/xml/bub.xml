<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bub.xml,v 1.12 2015/10/06 12:10:22 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>bub</rad>/o
  </kap>

  <drv mrk="bub.0o">
    <kap>
      <ofc>*</ofc>
      <tld/>o
    </kap>
    <snc mrk="bub.0o.knabo">
      <dif>
        Petola knabeto.
      </dif>
      <trdgrp lng="de">
        <trd>Bube</trd>,
        <trd>Junge</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>imp</trd>,
        <trd>scamp</trd>, <trd>urchin</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>gamin</trd>,
        <trd>polisson</trd></trdgrp> 
      <trdgrp lng="hu">
        <trd>k&ouml;ly&ouml;k</trd>,
        <trd>csintalan <ind>k&ouml;ly&ouml;k</ind></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>brzd&aogonek;c</trd>,
        <trd>malec</trd>,
        <trd>maluch</trd>,
        <trd>smyk</trd>,
        <trd>b&aogonek;bel <klr>(pot.)</klr></trd>,
        <trd>f&aogonek;fel <klr>(pot.)</klr></trd>
      </trdgrp>
      <trd lng="pt">pirralho</trd>
      <trdgrp lng="ru">
        <trd>&c_m;&c_a;&c_l;&c_mol;&c_ch;&c_i;&c_sh;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_z;&c_n;&c_i;&c_k;</trd>,
        <trd>&c_s;&c_o;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;</trd>
      </trdgrp>
    </snc>
    <snc mrk="bub.0o.karta">
      <fnt>PIV3</fnt>
      <ref tip="dif" cel="fant.0o.karta">fanto</ref>
      <trd lng="en">jack</trd>
    </snc>
  </drv>

  <drv mrk="bub.0acxo">
    <kap><tld/>a&ccirc;o</kap>
    <snc>
      <dif>
        Malbonedukita <tld/>o.
      </dif>
      <trdgrp lng="de">
        <trd>Bengel</trd>,
        <trd>Schlingel</trd>,
        <trd>L&uuml;mmel</trd>
      </trdgrp>
      <trd lng="en">brat</trd>
      <trdgrp lng="fr">
        <trd>garnement</trd>,
        <trd>sale <ind>gosse</ind></trd></trdgrp> 
      <trdgrp lng="hu">
        <trd>csib&eacute;sz</trd>,
        <trd>g&eacute;zeng&uacute;z</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>ga&lstroke;gan</trd>,
        <trd>&lstroke;obuz</trd>,
        <trd>&lstroke;obuziak</trd>
        <trd>urwis</trd>,
        <trd>szczeniak<klr> (obra&zacute;l.)</klr></trd>,
        <trd>andrus<klr> (przest.)</klr></trd>,
        <trd>ladaco<klr> (przest.)</klr></trd>,
        <trd>wisus<klr> (przest.)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_a;&c_l;&c_o;&c_p;&c_a;&c_j;</trd>,
        <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>
      </trdgrp>
    </snc>
  </drv>
</art>

