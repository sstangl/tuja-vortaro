<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: barcx.xml,v 1.16 2015/10/02 02:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bar&ccirc;</rad>/o
</kap>
<drv mrk="barcx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Rusa acideta <ref tip="super" cel="sup.0o">supo</ref>
      el ru&gcirc;aj <ref tip="prt" cel="bet.0o">betoj</ref>:
      <ekz>
        kiun &scirc;i nutros nun per sia fama <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 27a, p.
        198a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_r;&c_sh;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Borschtsch</trd>,
    <trd>Beetensuppe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>beet <ind>soup</ind></trd>,
    <trd>borsh</trd>,
    <trd>bortsch</trd>,
    <trd>borscht</trd>
  </trdgrp>
  <trd lng="fr">borchtch</trd>
  <trdgrp lng="he">
    <trd>&samekh;&yod;&lamed;&qof;&nun;&yod;&tav;</trd>,
    <trd>&het;&mem;&yod;&tsadi;&tav; &samekh;&lamed;&qof;</trd>,
    <trd>&bet;&vav;&resh;&shin;&tet;</trd>
  </trdgrp>
  <trd lng="hu">borscs</trd>
  <trd lng="nl">borsjtsj</trd>
  <trd lng="pl">barszcz</trd>
  <trd lng="pt">borche</trd>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_shch;</trd>
  <trd lng="sk">bor&scaron;&ccaron;</trd>
</drv>
</art>
