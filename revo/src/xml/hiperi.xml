<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hiperi.xml,v 1.23 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hiperik</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="hiperi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="hiperi1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Hypericum</trd>);
      <ref tip="lst" cel="veget.0ajxo" 
      lst="voko:malpigioparencoj">vegeta&jcirc;o</ref>
      kies floroj havas po kvin (malofte kvar) kalikajn kaj korolajn
      foliojn kaj sepdek a&ubreve; pli multajn stamenojn kunigitaj
      en kvar &gcirc;is kvin faskoj;
      kaj kun <ref tip="prt" cel="kapsul.0o.BOT">kapsula</ref>
      frukto malfermi&gcirc;anta post seki&gcirc;o ellasanta multajn malgrandajn
      semojn:
      <ekz>
        la brando havis almiksa&jcirc;on de <tld/>o
        <fnt><bib>Fab3</bib>, <lok>Kolo de botelo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/hiperik.jpg">
    floroj de <tld/>o
  </bld>
  <trd lng="de">Johanniskraut</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l; &a_r;&a_A;&a_ayn;&f_y;</trd>,
    <trd>&f_g;&a_l; &a_h;&a_z;&a_A;&a_r;&f_ch;&a_sh;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">millepertuis</trd>
  <trd lng="hu">orb&aacute;ncf&udblac;</trd>
  <trd lng="it">iperico</trd>
  <trd lng="nl">hertshooi</trd>
  <trd lng="pl">dziurawiec</trd>
  <trdgrp lng="pt">
    <trd>gut&iacute;fera</trd>,
    <trd>bacurizeiro</trd>,
    <trd>bacupari</trd>,
    <trd>abric&oacute;-do-par&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_r;&c_o;&c_b;&c_o;&c_j;</trd>
  <trd lng="sk">ľubovník</trd>
  <trd lng="cs">třezalka</trd>
</drv>
</art>
