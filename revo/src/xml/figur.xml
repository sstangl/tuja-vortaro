<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: figur.xml,v 1.46 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>figur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="figur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="figur.0o.aspekto">
    <dif>
      Videbla ekstera formo de esta&jcirc;o a&ubreve; objekto:
      <ekz>
        subite virina <tld/>o ekstaris anta&ubreve; mi;
      </ekz>
      <ekz>
        &scirc;ia tuta <tld/>o estis senmova<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun &scirc;i per sia tuta <tld/>o el&scirc;ovi&gcirc;is
        en la lumon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas grava <tld/>o de la angla literaturo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="person.0o.rolanto">persono<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Figur</trd>
    </trdgrp>
    <trd lng="hu">alak</trd>
    <trdgrp lng="pl">
      <trd>figura</trd>,
      <trd>posta&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="figur.0o.BELA">
    <dif>
      Belarta prezenta&jcirc;o de objekto a&ubreve; estulo:
      <ekz>
        <tld/>oj de dioj;
      </ekz>
      <ekz>
        sankt<tld/>o<fnt>Z</fnt>;
      </ekz>
    </dif>
    <trd lng="es">imagen</trd>
    <trd lng="hu">alak</trd>
    <subsnc mrk="figur.0o.sxaka">
      <uzo tip="fak">SPO</uzo>
      <dif>
        <ref tip="super" cel="sxak.0peco">&Scirc;akpeco</ref> pli
        grava
        ol peono kaj ordinare skulptita malpli abstrakte ol peono:
        <ref tip="sub" cel="regx.0o.sxaka">re&gcirc;o</ref> (R,
        &#9812;),
        <ref tip="sub" cel="dam.0o.sxaka">damo</ref> (D, &#9813;),
        <ref tip="sub" cel="kurier.0o.sxaka">kuriero</ref> (K,
        &#9815;),
        <ref tip="sub" cel="cxeval.0o.sxaka">&ccirc;evalo</ref>
        (&Ccirc;, &#9816;),
        <ref tip="sub" cel="tur.0o.sxaka">turo</ref> (T, &#9814;):
        <ekz>
          sur la tablo estis granda &scirc;aktabulo kun mirinde
          fajne faritaj <tld/>etoj
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a, p.
          219a</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Figur</trd>,
        <trd>Schachfigur</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>b&aacute;bu</trd>,
        <trd>figura</trd>
      </trdgrp>
      <trd lng="pl">figura<klr> szachowa</klr></trd>
    </subsnc>
    <subsnc mrk="figur.0o.karta">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Altranga <ref tip="super" cel="kart.lud0o">ludkarto</ref>,
        kutime kun bildo:
        <ref tip="sub" cel="regx.0o.karta">re&gcirc;o</ref>,
        <ref tip="sub" cel="dam.0o.karta">damo</ref>,
        <ref tip="sub" cel="fant.0o">fanto</ref>,
        <ref tip="sub" cel="jxoker.0o.karto">&jcirc;okero</ref>.
      </dif>
      <trd lng="de">Bild</trd>
      <trd lng="pl">figura<klr> karciana</klr></trd>
    </subsnc>
    <trd lng="de">Zeichen</trd>
    <trdgrp lng="pl">
      <trd>figura</trd>,
      <trd>figurka</trd>
    </trdgrp>
  </snc>
  <snc mrk="figur.0o.geometria">
    <ref tip="dif" cel="figur.geometria0o">geometria figuro</ref>
    <trd lng="de">geometrische Figur</trd>
    <trd lng="es">figura geom&eacute;trica</trd>
    <trd lng="hu">alakzat</trd>
    <trd lng="pl">figura geometryczna</trd>
  </snc>
  <snc mrk="figur.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <ref tip="dif" cel="figur.stil0o">stil<tld/>o</ref>
    <ekz>
      retorikaj <tld/>oj.
    </ekz>
    <trdgrp lng="de">
      <trd>Stilfigur</trd>
    </trdgrp>
    <trd lng="es">figura literaria</trd>
    <trdgrp lng="fr">
      <trd>figure de style</trd>,
      <trd>trope</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&oacute;k&eacute;p</trd>,
      <trd>alakzat</trd>,
      <trd>tr&oacute;pus</trd>
    </trdgrp>
    <trd lng="nl">stijlfiguur</trd>
    <trdgrp lng="pl">
      <trd>figura retoryczna</trd>,
      <trd>przeno&sacute;nia</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>figura</trd>,
    <trd>obrazec</trd>,
    <trd>postava</trd>,
    <trd>útvar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Figur</trd>
  </trdgrp>
  <trd lng="es">figura</trd>
  <trd lng="fr">figure<klr tip="ind"> (tous sens sauf
  visage)</klr></trd>
  <trd lng="it">figura</trd>
  <trd lng="nl">figuur</trd>
  <trd lng="pt">figura</trd>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>diagram</trd>,
    <trd>schéma</trd>,
    <trd>značka</trd>
  </trdgrp>
</drv>

<drv mrk="figur.0a">
  <kap><tld/>a</kap>
  <snc mrk="figur.0a.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Vort<tld/>a, metafora:
      <ekz>
        vorto uzata la&ubreve; la <tld/>a senco.
      </ekz>
    </dif>
    <trd
    lng="be">&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>fig&uuml;rlich</trd>,
      <trd>&uuml;bertragen</trd>
    </trdgrp>
    <trd lng="fr">figur&eacute;</trd>
    <trd lng="nl">figuurlijk</trd>
    <trdgrp lng="hu">
      <trd>&aacute;tvitt</trd>,
      <trd>k&eacute;pes</trd>
    </trdgrp>
    <trd lng="it">figurato</trd>
    <trd lng="pl">obrazowy</trd>
    <trd
    lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>figurální</trd>,
    <trd>obrazný</trd>,
    <trd>přenesený</trd>
  </trdgrp>
  <trd lng="es">figurado</trd>
  <trdgrp lng="sk">
    <trd>figurálny</trd>,
    <trd>prenesený</trd>
  </trdgrp>
</drv>

<drv mrk="figur.0i">
  <kap><tld/>i</kap>
  <snc mrk="figur.0i.roli">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Aperi en iu formo, ludi rolon:
      <ekz>
        en la kongreso li <tld/>is kiel interpretisto
        <fnt><bib>PIV1</bib></fnt>;
      </ekz><ekz>
        en la skandalo <tld/>as gravaj politikistoj;
      </ekz><ekz>
        li <tld/>as kiel estro, sed efektive estas nur subulo.
      </ekz>
    </dif>
    <adm>
      Estas strange, ke PIV1 konsideras tiun uzon transitiva.
      <aut>MB</aut>
    </adm>
    <trdgrp lng="be">
      <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_y;&c_g;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;
        <ind>&c_r;&c_o;&c_l;&c_ju;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>eine <ind>Rolle spielen</ind></trd>,
      <trd>eine <ind>Figur abgeben</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>figurer<klr tip="ind"> (intr.)</klr></trd>,
      <trd>faire <ind>figure</ind> (de)</trd>
    </trdgrp>
    <trd lng="hu">szerepel</trd>
    <trd lng="it">figurare</trd>
    <trdgrp lng="nl">
      <trd>een <ind>rol</ind> spelen</trd>,
      <trd>figureren</trd>
    </trdgrp>
    <trd lng="pl">figurowa&cacute;</trd>
    <trd
    lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="figur.0i.bildigi">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Prezenti, reprodukti formon, <tld/>on de esta&jcirc;o
      a&ubreve; objekto:
      <ekz>
        la statuo, pentra&jcirc;o, bildo <tld/>as personon,
        leonon, floron, vazon;
      </ekz><ekz>
        <uzo tip="fak">MAT</uzo>
        oni <tld/>as per kurbo la ekvacion
        <frm><k>f</k>(<k>x</k>,<k>y</k>) = 0</frm>
        <fnt><bib>MatTerm</bib>, <lok>p. 16</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_e;&c_z;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">represent</trd>
    <trdgrp lng="de">
      <trd>bilden</trd>,
      <trd>formen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>figurer<klr tip="ind"> (tr.)</klr></trd>,
      <trd>repr&eacute;senter<klr tip="ind"> (figurer)</klr></trd>
    </trdgrp>
    <trd lng="hu">&aacute;br&aacute;zol</trd>
    <trd lng="it">rappresentare<klr> (mat. e art.)</klr></trd>
    <trdgrp lng="pl">
      <trd>przedstawia&cacute;</trd>,
      <trd>obrazowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>představovat</trd>,
    <trd>zobrazovat</trd>
  </trdgrp>
  <trd lng="es">figurar</trd>
  <trd lng="sk">zobrazovať</trd>
</drv>

<drv mrk="figur.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="figur.0anto.KIN">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <ref tip="dif" cel="statist.0o.KIN">statisto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_t;&c_y;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_t;&c_o;&c_r;
      &c_k;&c_a;&c_r;&c_d;&c_e;&c_b;&c_a;&c_l;&c_je;&c_t;&c_u;</trd>
  </trdgrp>
  <trd lng="cs">statista <klr>(div.)</klr></trd>
  <trdgrp lng="es">
    <trd>comparsa</trd>,
    <trd>extra</trd>,
    <trd>figurante</trd>
  </trdgrp>
  <trd lng="fr">figurant</trd>
  <trdgrp lng="hu">
    <trd>statiszta</trd>,
    <trd>mell&eacute;kszerepl&odblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>figurante<klr> (cin. e spettac.)</klr></trd>,
    <trd>comparsa<klr> (cin. e teat.)</klr></trd>
  </trdgrp>
  <trd lng="nl">figurant</trd>
  <trdgrp lng="pl">
    <trd>figurant</trd>,
    <trd>statysta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;(&c_k;&c_a;)
    <ind>&c_k;&c_o;&c_r;&c_d;&c_je;&c_b;&c_a;&c_l;&c_je;&c_t;&c_a;</ind></trd>;
    <trd>&c_s;&c_t;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">štatista</trd>
</drv>

<drv mrk="figur.geometria0o">
  <kap>geometria <tld/>o</kap>
  <tezrad fak="MAT"/>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib>, <lok>p. 6</lok></fnt>
  <snc>
    <dif>
      <ref cel="ar.0o.MAT">Aro</ref> da punktoj, konsiderata de la
      vidpunkto de &gcirc;iaj geometriaj ecoj:
      <ekz>
        ebena <tld/>o <klr>(inkluzivata de ebeno)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="geomet1.0o">geometrio</ref>.
    </refgrp>
    <refgrp tip="sub">
      Specifaj figuroj:
      <ref cel="punkt.0o.elementa">punkto</ref>,
      <ref cel="lini.0o.MAT">linio</ref>,
      <ref cel="surfac.0o.MAT">surfaco</ref>,
      <ref cel="solid.0o.geometrio">solido</ref>.
    </refgrp>
  </snc>
  <bld tip="svg" lok="../bld/e_elementaj_figuroj.svg.gif" alt="600"
  lrg="600">
    elementaj geometriaj <tld/>oj
  </bld>
  <trd lng="be">&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a; <klr
      tip="amb">(&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="de">Figur</trd>
  <trd lng="en">figure<klr tip="ind"> (geometric shape)</klr></trd>
  <trd lng="es">figura geom&eacute;trica</trd>
  <trd lng="fr">figure g&eacute;om&eacute;trique</trd>
  <trd lng="hu">m&eacute;rtani <ind>alakzat</ind></trd>
  <trd lng="it">figura<klr> geometrica</klr></trd>
  <trd lng="nl">figuur<klr tip="ind"> (geometrie)</klr></trd>
  <trd lng="pl">figura geometryczna</trd>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>
  <trd lng="sk">geometrický útvar</trd>
  <trd lng="cs">geometrický útvar</trd>
</drv>

<drv mrk="figur.mis0igi">
  <kap>mis<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="figur.mis0igi.KOMUNE">
    <dif>
      Malbonigi la figuron de...;
      <ref tip="vid" cel="form.mis0i.KOMUNE">misformi</ref>,
      <ref tip="vid" cel="tord.0i.misforme">tordi</ref>:
      <ekz>
        en nia vila&gcirc;o estas <klr>...</klr> knabino kies 
        viza&gcirc;on hazarde mis<tld/>igas variolumado
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">desfigurar</trd>
  <trd lng="fr">d&eacute;figurer</trd>
</drv>

<drv mrk="figur.stil0o">
  <kap>stil<tld/>o</kap>
  <tezrad fak="BELE"/>
  <snc mrk="figur.stil0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Esprimo, kiu uzas vortojn neordinare
      por atentiga a&ubreve; pla&ccirc;a efekto.
    </dif>
    <refgrp tip="sin">
      <ref cel="figur.0o.BELE">figuro</ref>,
      <ref cel="trop.0o">tropo</ref>,
      <ref cel="figur.vort0o">vort<tld/>o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a; &c_m;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;
      &c_s;&c_t;&c_y;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;
      &c_r;&c_y;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>stylistický útvar</trd>,
    <trd>řečnická ozdoba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stilfigur</trd>
  </trdgrp>
  <trd lng="es">figura literaria</trd>
  <trdgrp lng="fr">
    <trd>figure de style</trd>,
    <trd>trope</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&oacute;k&eacute;p</trd>,
    <trd>k&eacute;pes kifejez&eacute;s</trd>,
    <trd>tr&oacute;pus</trd>
  </trdgrp>
  <trd lng="it">figura<klr> stilistica</klr></trd>
  <trd lng="nl">stijlfiguur</trd>
  <trdgrp lng="pl">
    <trd>figura retoryczna</trd>,
    <trd>przeno&sacute;nia</trd>,
    <trd>paralela</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;
    &c_r;&c_je;&c_ch;&c_i;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;
      &c_r;&c_i;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_u;&c_r;&c_a;
      &c_s;&c_t;&c_i;&c_l;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rečnícka ozdoba</trd>,
    <trd>štylistický útvar</trd>
  </trdgrp>
</drv>

<drv mrk="figur.trans0igxo">
  <kap>Trans<tld/>i&gcirc;o</kap>
  <snc mrk="figur.trans0igxo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Unu el la
      <ref tip="lst" cel="fest.dekduopa0o.KRI"
      lst="voko:festoj">dekduopaj 
      festoj</ref>
      kristanaj, rememore pri la apero de
      <ref cel="jesu.0o.homo">Jesuo</ref> en glora supernatura
      aspekto sur la monto Tabor (Mateo, &ccirc;ap. 17a):
      <ekz>
        la nova pre&gcirc;ejo de Valamo, dedi&ccirc;ita al la
        trans<tld/>i&gcirc;o de Kristo, estis konstruita
        en 1975&ndash;1977
        <fnt>
          <aut>Saliko</aut>:
          <vrk>La hereda&jcirc;o de orientaj ortodoksuloj</vrk>
          <bib>Monato</bib>.
        </fnt>.
      </ekz>        
    </dif>
  </snc>
  <trd lng="en">Transfiguration</trd>
  <trd lng="es">transfiguraci&oacute;n de Jes&uacute;s</trd>
