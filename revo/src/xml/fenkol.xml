<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fenkol.xml,v 1.22 2015/10/14 05:10:19 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fenkol</rad>/o
</kap>
<drv mrk="fenkol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Foeniculum</trd>). La aroma legomo estas kulturata.
        
    </dif>
    <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_n;&c_h;&c_je;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_z;&c_je;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>fonoll</trd>,
    <trd>g&egrave;nere Foeniculum</trd>
  </trdgrp>
  <trd lng="cs">fenykl obecn&yacute;</trd>
  <trd lng="de">Fenchel</trd>
  <trd lng="en">fennel</trd>
  <trd lng="fa">&a_r;&a_A;&a_z;&f_y;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">fenouil</trd>
  <trd lng="hu">&eacute;desk&ouml;m&eacute;ny</trd>
  <trd lng="nl">venkel</trd>
  <trdgrp lng="pl">
    <trd>koper <klr>w&lstroke;oski</klr></trd>,
    <trd>fenku&lstroke; <klr>w&lstroke;oski</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>funcho</trd>,
    <trd>erva-doce</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_h;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">fenikel</trd>
</drv>
</art>
