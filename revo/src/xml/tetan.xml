<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tetan.xml,v 1.21 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tetan</rad>/o
</kap>
<drv mrk="tetan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Infekta, plej ofte mortiga <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> karakterizata per fortegaj
      konvulsioj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_u;&c_p;&c_n;&c_ja;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>t&egrave;tan</trd>,
    <trd>t&egrave;tanus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>svalová křeč</trd>,
    <trd>tetanus</trd>
  </trdgrp>
  <trd lng="de">Tetanus</trd>
  <trd lng="en">tetanus</trd>
  <trd lng="fa">&f_k;&a_z;&a_A;&a_z;</trd>
  <trd lng="fr">t&eacute;tanos</trd>
  <trd lng="hu">tetanusz</trd>
  <trd lng="id">tetanus</trd>
  <trd lng="nl">tetanus</trd>
  <trd lng="pl">t&eogonek;&zdot;ec</trd>
  <trdgrp lng="pt">
    <trd>t&eacute;tano</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_a;&c_n;&c_u;&c_s;</trd>
  <trd lng="sk">tetanus</trd>
</drv>
</art>
