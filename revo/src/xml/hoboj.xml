<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hoboj.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>hoboj</rad>/o
</kap>
<drv mrk="hoboj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika ligna <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      kun duobla langeto kaj klavoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_b;&c_o;&c_j;</trd>
  <trd lng="bg">&c_o;&c_b;&c_o;&c_j;</trd>
  <trd lng="br">oboell</trd>
  <trd lng="ca">obo&egrave;</trd>
  <trd lng="cs">hoboj</trd>
  <trd lng="de">Oboe</trd>
  <trdgrp lng="el">
    <trd>&omikron_ton;&my;&pi;&omikron;&epsilon;</trd>,
    <trd>&omikron;&xi;&ypsilon_ton;&alfa;&ypsilon;&lambda;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">oboe</trd>
  <trd lng="es">oboe</trd>
  <trd lng="fr">hautbois</trd>
  <trd lng="hu">oboa</trd>
  <trd lng="it">oboe</trd>
  <trd lng="nl">hobo</trd>
  <trd lng="pl">ob&oacute;j</trd>
  <trd lng="pt">obo&eacute;</trd>
  <trd lng="ru">&c_g;&c_o;&c_b;&c_o;&c_j;</trd>
  <trd lng="sk">hoboj</trd>
</drv>
</art>
