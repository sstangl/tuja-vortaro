<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolubr.xml,v 1.23 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolubr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolubr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolubr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nevenena <ref tip="lst" cel="serpen.0o"
      lst="voko:serpentoj">serpento</ref> kun grandaj okuloj,
      rapidmova pro plievolui&gcirc;inta pulmo
      kaj kapabla forte barakti;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="kolubr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Coluber</trd>):
      <ekz>
        la <tld/>on mi ser&ccirc;is kaj mi kredis, ke &gcirc;i ka&scirc;i&gcirc;is
        en via pajlolito
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_zh;</trd>
  <trd lng="bg">&c_s;&c_m;&c_o;&c_k;&c_o;&c_v;&c_je; &c_s;&c_t;&c_r;&c_je;&c_l;&c_c;&c_i;</trd>
  <trd lng="ca">colobra</trd>
  <trd lng="cs">užovka</trd>
  <trd lng="de">Zornnatter</trd>
  <trd lng="en">colubrid</trd>
  <trd lng="es">culebra</trd>
  <trd lng="fr">couleuvre</trd>
  <trd lng="hu">sikl&oacute;</trd>
  <trd lng="nl">toornslang</trd>
  <trd lng="pl">po&lstroke;oz <klr tip="amb">w&lstroke;a&sacute;ciwy</klr></trd>
  <trd lng="pt">cobra</trd>
  <trd lng="ru">&c_u;&c_zh;</trd>
  <trd lng="sk">užovka</trd>
</drv>

</art>

