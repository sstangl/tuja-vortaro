<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plejst.xml,v 1.12 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>plejstocen</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="plejst.0o">
  <kap><tld/>o</kap>
  <snc mrk="plejst.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="lst" cel="epok.0o.periodo"
      lst="voko:geologiaj_epokoj" val="-1806000">epoko</ref> inter <ref
      cel="plioce.0o">plioceno</ref> kaj <ref
      cel="holocen.0o">holoceno</ref>, dum kiu evolui&gcirc;is la moderna
      <ref tip="prt" cel="hom.0o.ZOO">homo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_a;&c_c;&c_e;&c_n;</trd>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_o;&c_c;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>diluvium</trd>,
    <trd>pleistocén</trd>,
    <trd>věk ledových dob</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Pleistoz&auml;n</trd>,
     <trd>Eiszeitalter</trd>
  </trdgrp>
  <trd lng="en">Pleistocene</trd>
  <trd lng="fa">&f_p;&a_l;&f_y;&a_s;&a_t;&a_w;&a_s;&a_n;</trd>
  <trd lng="fr">Pl&eacute;istoc&egrave;ne</trd>
  <trd lng="hu">pleisztoc&eacute;n</trd>
  <trd lng="nl">pleistoceen</trd>
  <trdgrp lng="pt">
    <trd>Plistoceno</trd>,
    <trd>Pleistoceno</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_j;&c_s;&c_t;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">pleistocén</trd>
</drv>
</art>
