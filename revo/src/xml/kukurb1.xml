<vortaro>
<art mrk="$Id: kukurb1.xml,v 1.10 2015/06/24 12:10:14 revo Exp $">
<kap>
  <rad>kukurbac</rad>/oj
</kap>
<drv mrk="kukurb1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="kukurb1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cucurbitaceae</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_i;&c_k;&c_v;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">cucurbit&agrave;cies</trd>
  <trd lng="de">K&uuml;rbisgew&auml;chse</trd>
  <trd lng="en">curcurbits</trd>
  <trd lng="es">cucurbit&aacute;ceas</trd>
  <trd lng="fr">curcurbitac&eacute;e</trd>
  <trd lng="hu">t&ouml;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">kalebasachtigen</trd>
  <trd lng="pt">cucurb&aacute;ceas</trd>
  <trd lng="ru">&c_t;&c_y;&c_k;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
