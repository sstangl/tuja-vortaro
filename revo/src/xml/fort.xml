<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fort.xml,v 1.41 2015/09/22 11:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fort</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fort.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="fort.0a.mano">
    <dif>
      Kapabla venki kontra&ubreve;staron kaj produkti grandajn
      efikojn materiajn a&ubreve; moralajn:
      <ekz>
        <tld/>a homo, luktisto, nacio, motoro, registaro;
      </ekz>
      <ekz>
        <tld/>a helpo, volo<fnt>B</fnt>, esprimo;
      </ekz>
      <ekz>
        kontra&ubreve; <tld/>a mano la le&gcirc;o estas vana
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li en &ccirc;io estas <tld/>a<fnt>Z</fnt> <klr>(multkapabla)
        </klr>;
      </ekz>
      <ekz>
        <tld/>korulo
        <fnt><bib>MT</bib>, <lok>&Psa; 76:5</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="potenc.0a">potenca</ref>.
    </refgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_w;&a_A;&a_n;&a_A;</trd>,
      <trd>&a_t;&a_w;&a_A;&a_n;&a_m;&a_n;&a_d;</trd>,
      <trd>&a_n;&f_y;&a_r;&a_w;&a_m;&a_n;&a_d;</trd>,
      <trd>&a_q;&a_d;&a_r;&a_t;&a_m;&a_n;&a_d;</trd>
      <trd>&a_q;&a_w;&f_y;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>silny</trd>,
      <trd>mocny</trd>,
      <trd>krzepki</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>forte</trd>,
      <trd>potente</trd>,
      <trd>rijo</trd>,
      <trd>vigoroso</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0a.intensa">
    <dif>
      Altgrada, <ref tip="sin"
        cel="intens.0a.impresa">intensa<sncref/></ref>;
      posedanta en alta grado siajn karakterizajn ecojn:
      <ekz>
        <tld/>a sento, kolero, intereso, bezono, dozo, fulmotondro,
        streko, profito, utilo, diferenco;
      </ekz>
      <ekz>
        en plej <tld/>a grado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a malsano, febro, varmo, tuso, koloro, lumo, adoro,
        vo&ccirc;o;
      </ekz>
      <ekz>
        <tld/>a kafo, biero, vino, cigaro.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_q;&a_w;&f_y;</trd>,
      <trd>&a_sh;&a_d;&f_y;&a_d;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>silny</trd>,
      <trd>mocny</trd>,
      <trd>du&zdot;y</trd>,
      <trd>t&eogonek;gi</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>forte</trd>,
      <trd>potente</trd>,
      <trd>intenso</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0a.fortika">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif"
        cel="fortik.0a.materialo">Fortika<sncref/></ref>,
        <ref tip="sin" cel="firm.0a.fiksita">firma<sncref/></ref>:
      <ekz>
        <tld/>a muro, fortika&jcirc;o, &scirc;nuro, teksa&jcirc;o,
        papero;
      </ekz>
      <ekz>
        <tld/>a sano, pozicio.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_A;&a_s;&a_t;&a_w;&a_A;&a_r;</trd>,
      <trd>&a_m;&a_H;&f_k;&a_m;</trd>,
      <trd>&a_m;&a_s;&a_t;&a_H;&f_k;&a_m;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>mocny</trd>,
      <trd>pot&eogonek;&zdot;ny</trd>,
      <trd>krzepki</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>forte</trd>,
      <trd>rijo</trd>,
      <trd>resistente</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0a.FON">
    <uzo tip="fak">FON</uzo>
    <uzo tip="klr">(pri konsonanto aux kvaza&ubreve;vokalo)</uzo>
    <dif>
      Elparolata pli intense.
    </dif>
  </snc>
  <snc mrk="fort.0a.POE">
    <uzo tip="fak">POE</uzo>
    <uzo tip="klr">(pri silabo)</uzo>
    <dif>
      Elparolata pli intense ol la najbaraj.
    </dif>
    <trdgrp lng="pt">
      <trd>forte</trd>,
      <trd>t&ocirc;nico</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_u;&c_zh;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prudk&yacute;</trd>,
    <trd>siln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>stark</trd>,
    <trd>kr&auml;ftig</trd>,
    <trd>heftig</trd>
  </trdgrp>
  <trd lng="en">strong</trd>
  <trd lng="es">fuerte</trd>
  <trd lng="fr">fort<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="id">kuat</trd>
  <trd lng="nl">sterk</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mocn&yacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>siln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fort.0e">
  <kap><tld/>e</kap>
  <snc mrk="fort.0e.maniero">
    <dif>
      En <tld/>a maniero:
      <ekz>
        <tld/>e bati.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>mocno</trd>,
      <trd>silnie</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0e.multe">
    <dif>
      Multe, tre:
      <ekz>
        <tld/>e malpura<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>e &gcirc;oji, dubi<fnt>Z</fnt>,
        deziri<fnt>Z</fnt>, voli<fnt>Z</fnt>,
        sin amuzi<fnt>Z</fnt>;
      </ekz><ekz>
        mi <tld/>e volus scii&gcirc;i, kiel<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_ib;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="de">sehr</trd>
    <trdgrp lng="pl">
      <trd>strasznie</trd>,
      <trd>okropnie</trd>,
      <trd>bardzo</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_ch;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_c;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mocn&ecaron;</trd>,
    <trd>siln&ecaron;</trd>
  </trdgrp>
  <trd lng="en">strongly</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kh;&a_t;</trd>,
    <trd>&a_sh;&a_d;&f_y;&a_d;&a_A;&a_fathatan;</trd>,
    <trd>&a_q;&a_w;&f_y;&a_A;&a_fathatan;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fortement</trd>,
    <trd>tr&egrave;s</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trd lng="sk">forte</trd>
</drv>

<drv mrk="fort.0o">
  <kap><tld/>o</kap>
  <snc mrk="fort.0o.kapablo">
    <dif>
      Kapablo venki kontra&ubreve;staron kaj produkti grandajn 
      efikojn:
      <ekz>
        <tld/>o de vivo, volo<fnt>B</fnt>, spirito;
      </ekz>
      <ekz>
        <tld/>o de la esperanta movado;
      </ekz>
      <ekz>
        kie regas la <tld/>o, tie rajto silentas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>oj de la naturo<fnt>B</fnt>;
      </ekz>
      <ekz>
        per propraj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        perdi, stre&ccirc;i la <tld/>ojn;
      </ekz>
      <ekz>
        kiu havas <tld/>on, havas rajton
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        larmoj kun neretenebla <tld/>o ver&scirc;i&gcirc;is sur
        &scirc;iaj vangoj;
      </ekz>
      <ekz>
        la malnova dialekto restas en sia plena <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        militaj, maraj <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kraft</trd>,
      <trd>St&auml;rke</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_n;&f_y;&a_r;&a_w;</trd>,
      <trd>&a_q;&a_d;&a_r;&a_t;</trd>,
      <trd>&a_t;&a_w;&a_A;&a_n;</trd>
    </trdgrp>
    <trd lng="id">[<ind>kuat</ind>] kekuatan</trd>
    <trd lng="pl">si&lstroke;a</trd>
    <trd lng="ru">&c_s;&c_i;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="fort.0o.solideco">
    <dif>
      Solideco, fortikeco, malfacila rompebleco:
      <ekz>
        <tld/>o de &scirc;nuro, de sano;
      </ekz><ekz>
        hodia&ubreve; <tld/>o, morga&ubreve; morto <fnt>
        <bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Festigkeit</trd>,
      <trd>Stabilit&auml;t</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_s;&a_t;&a_w;&a_A;&a_r;&f_y;</trd>
      <trd>&a_A;&a_s;&a_t;&a_H;&f_k;&a_A;&a_m;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>moc</trd>,
      <trd>si&lstroke;a</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;
    <klr tip="amb">
    (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <snc mrk="fort.0o.intenseco">
    <dif>
      Alta grado, intenseco:
      <ekz>
        <tld/>o de febro<fnt>B</fnt>, de vino.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;he</trd>,
      <trd>St&auml;rke</trd>,
      <trd>Intensit&auml;t</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_d;&a_t;</trd>,
      <trd>&a_gh;&a_l;&a_Z;&a_t;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>si&lstroke;a</trd>,
      <trd>moc</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;
      <klr tip="amb">
          (&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
      <trd>&c_s;&c_i;&c_l;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fort.0o.FIZ">
    <tezrad fak="FIZ"/>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      esprimanta &ccirc;ian ka&ubreve;zon kapablan produkti,
      modifi a&ubreve; nuligi movon:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de <tld/>o estas
        <frm>MLT<sup>-2</sup></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neuxto.n0o">ne&ubreve;tono</ref>.
    </refgrp>
    <trd lng="de">Kraft</trd>
    <trd lng="id">gaya</trd>
    <trd lng="fa">&a_n;&f_y;&a_r;&a_w;</trd>
    <trd lng="pl">si&lstroke;a</trd>
    <trd lng="ru">&c_s;&c_i;&c_l;&c_a;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>schopnost</trd>,
    <trd>s&iacute;la</trd>,
    <trd>v&yacute;konnost</trd>
  </trdgrp>
  <trd lng="en">strength</trd>
  <trd lng="es">fuerza</trd>
  <trd lng="fr">force</trd>
  <trdgrp lng="nl">
    <trd>sterkte</trd>,
    <trd>kracht</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>moc</trd>,
    <trd>schopnos&tcaron;</trd>,
    <trd>sila</trd>
  </trdgrp>
</drv>

<drv mrk="fort.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="fort.0ega.KOMUNE">
    <dif>
      Malmodere, senbride <tld/>a.
    </dif>
  </snc>
  <trd lng="en">mighty</trd>
  <trdgrp lng="pl">
    <trd>mocarny</trd>,
    <trd>pot&eogonek;&zdot;ny</trd>
  </trdgrp>
</drv>

<drv mrk="fort.cxiu0e">
  <kap>&ccirc;iu<tld/>e,
  <var><kap>per &ccirc;iuj <tld/>oj</kap></var>,
  <var><kap>per la tuta forto</kap></var></kap>
  <snc mrk="fort.cxiu0e.KOMUNE">
    <dif>
      Uzante sian tutan <tld/>on, intense provante:
      <ekz>
        per &ccirc;iuj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Sa2; 6:14</lok></fnt>;
      </ekz>
      <ekz>
        per la tuta <tld/>o
        <fnt><bib>MT</bib>, <lok>&Kr1; 13:8</lok></fnt>;
      </ekz>
      <ekz>
        pliboni&gcirc;on ni entreprenu &ccirc;iu<tld/>e!
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>with maximum effort</trd>,
    <trd>full out</trd>, <trd>all out</trd>, <trd>flat out</trd>
  </trdgrp>
  <trd lng="fr">de toutes ses forces</trd>
  <trdgrp lng="pl">
    <trd>z ca&lstroke;ej <ind>si&lstroke;y</ind></trd>,
    <trd>ze wszystkich <ind>si&lstroke;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="fort.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="fort.mal0a.KOMUNE">
    <dif>
      Malmulte efika, malintensa:
      <ekz>
        &ccirc;iuj vortoj estas mal<tld/>aj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en mal<tld/>a grado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a en la kalkulado<fnt>Z</fnt>, en la
        muziko<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="san.mal0a">malsana</ref>,
      <ref cel="pal.0a">pala</ref>,
      <ref cel="velk.0a">velka</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_b;&c_y;</trd>
  <trd lng="cs">slab&yacute;</trd>
  <trd lng="de">schwach</trd>
  <trd lng="en">weak</trd>
  <trd lng="es">d&eacute;bil</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_s;&a_t;</trd>,
    <trd>&a_n;&a_A;&f_ch;&f_y;&a_z;</trd>,
    <trd>&a_n;&a_A;&a_t;&a_w;&a_A;&a_n;</trd>,
    <trd>&a_D;&a_ayn;&f_y;&a_f;</trd>
  </trdgrp>
  <trd lng="fr">faible</trd>
  <trd lng="id">lemah</trd>
  <trd lng="nl">zwak</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;aby</trd>,
    <trd>w&aogonek;t&lstroke;y</trd>,
    <trd>wycie&nacute;czony</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_b;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>slab&yacute;</trd>,
    <trd>vetch&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fort.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="fort.mal0ajxo.KOMUNE">
    <dif>
      Mal<tld/>a loko, mal<tld/>a flanko,
      mal<tld/>a eco en ies karaktero:
      <ekz>
        li havis la strangan mal<tld/>a&jcirc;on ke li ne povis...
        <fnt><bib>Fab1</bib>, <lok>Malgranda Niko kaj granda
            Niko</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tikl.0a">tikla</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_b;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_je; &c_m;&c_je;&c_s;&c_c;&c_a;
    </trd>
  </trdgrp>
  <trd lng="de"><klr tip="ind">(Charakter-)</klr> Schw&auml;che
  </trd>
  <trd lng="en">weakness</trd>
  <trd lng="es">debilidad</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_q;&a_T;&a_h;&a_hamza_sure; &a_D;&a_ayn;&a_f;</trd>,
    <trd>&a_n;&a_q;&a_S;</trd>,
    <trd>&a_ayn;&f_y;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">faiblesse</trd>
  <trd lng="id">[<ind>lemah</ind>] kelemahan</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;abo&sacute;&cacute;</trd>,
    <trd>s&lstroke;aba<klr> strona</klr></trd>,
    <trd>pi&eogonek;ta Achillesa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;
    </trd>
  </trdgrp>
</drv>

<drv mrk="fort.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fort.mal0igi.KOMUNE">
    <dif>
      Igi mal<tld/>a:
      <ekz>
        malsano mal<tld/>igis lin;
      </ekz>
      <ekz>
        fremdaj elementoj mal<tld/>igis la unuecon de la
        socio<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="frost.0igi">frostigi</ref>,
      <ref cel="sang.sen0igi">sensangigi</ref>,
