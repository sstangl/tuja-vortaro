  </trdgrp>
  <trd lng="fr">force &eacute;lectromotrice</trd>
  <trd lng="id"><ind>gaya</ind> gerak listrik</trd>
  <trd lng="pl">si&lstroke;a elektromotoryczna</trd>
  <trdgrp lng="ru">
