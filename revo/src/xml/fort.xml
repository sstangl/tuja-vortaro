  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&f_y;&a_r;&a_w;&f_y; &a_m;&a_H;&a_r;&f_k;&a_h;&a_hamza_sure; &a_A;&a_l;&f_k;&a_t;&a_r;&f_y;&f_k;&f_y;</trd>,
