<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kajman.xml,v 1.8 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kajman</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kajman.0o">
  <kap><tld/>o</kap>
  <snc mrk="kajman.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="reptil.0o"
      lst="voko:krokodiluloj">Reptilio</ref> 
      granda sudamerika,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="aligat1.0oj.ZOO">aligatoredoj</ref>
      (<trd lng="la">Caiman</trd>): 
      <ekz>
        inter grandaj kamelioj el Amazonaj teroj, trovi&gcirc;is koloraj 
        ardeoj, <tld/>oj dikaj kiel porkoj, dek du klakil-serpentoj kaj 
        or-&scirc;ela testudo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_j;&c_m;&c_a;&c_n;</trd>
  <trd lng="cs">kajman</trd>
  <trd lng="da">kaiman</trd>
  <trd lng="de">Kaiman</trd>
  <trd lng="en">caiman</trd>
  <trd lng="es">caiman</trd>
  <trd lng="fr">ca&iuml;man</trd>
  <trd lng="nl">kaaiman</trd>
  <trd lng="ru">&c_k;&c_a;&c_j;&c_m;&c_a;&c_n;</trd>
  <trd lng="sk">kajman</trd>
</drv>
</art>

