<vortaro>
<art mrk="$Id: gulasx.xml,v 1.12 2012/04/09 16:10:09 revo Exp $">
<kap>
  <rad>gula&scirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gulasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="gulasx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Hungarmaniera <ref tip="super" cel="ragu.0o">raguo</ref>
      el bova&jcirc;o, distran&ccirc;ita en neegalajn grandajn
      pecojn, rostita kaj poste supigita kun legomoj kaj paprika
      sa&ubreve;co:
      <ekz>
        la edzino preparis tre gasteme hungaran <tld/>on por min satigi
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 24a, p.
        62a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_ja;&c_sh;</trd>
  <trd lng="bg">&c_g;&c_u;&c_l;&c_a;&c_sh;</trd>
  <trd lng="de">Gulasch</trd>
  <trd lng="en">goulash</trd>
  <trd lng="es">gulash</trd>  
  <trd lng="fr">goulache</trd>
  <trd lng="hu">p&ouml;rk&ouml;lt</trd>
  <trd lng="nl">goelasj</trd>
  <trd lng="pt">gulache</trd>
  <trd lng="ru">&c_g;&c_u;&c_l;&c_ja;&c_sh;</trd>
</drv>
</art>
