<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxaluz.xml,v 1.30 2016/02/26 21:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&jcirc;aluz</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jxaluz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="jxaluz.0a.PSI_KOMUNE">
    <dif>
      Sentanta doloran maltrankvilon pro la timo pri perdo a&ubreve;
      difekto 
      de iu sia poseda&jcirc;o, kiun oni pro tio fervore prizorgas:
      <ekz>
        <tld/>a pri sia famo, situacio, pri siaj rajtoj;
      </ekz>
      <ekz>
        nia Dio estas <tld/>a Dio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">zel&oacute;s</trd>
    <trd lng="nl">ongerust</trd>
    <trdgrp lng="pt">
      <trd>cioso</trd>,
      <trd>zeloso</trd>
    </trdgrp>
  </snc>
  <snc mrk="jxaluz.0a.PSI_ame">
    <dif>
      Sentanta doloran suspektoplenan maltrankvilon pri la fideleco de
      amata persono:
      <ekz>
        <tld/>a kiel tigro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu nacio <tld/>e volas konservi sian suverenecon.
      </ekz>
    </dif>
    <trd lng="ca">gel&oacute;s</trd>
    <trd lng="nl">jaloers</trd>
    <trdgrp lng="pt">
      <trd>enciumado</trd>,
      <trd>ciumento</trd>
    </trdgrp>
    <trd lng="sv">svartsjuk</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_w;&c_n;&c_ib;&c_v;&c_y;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_n;&c_i;&c_v;</trd>
  <trd lng="de">eifers&uuml;chtig</trd>
  <trd lng="en">jealous</trd>
  <trd lng="es">celoso</trd>
  <trd lng="fr">jaloux</trd>
  <trd lng="he">&qof;&nun;&alef;&yod;</trd>
  <trd lng="hu">f&eacute;lt&eacute;keny</trd>
  <trdgrp lng="id">
    <trd>cemburu</trd>,
    <trd>dengki</trd>,
    <trd>iri</trd>
  </trdgrp>
  <trd lng="pl">zazdrosny</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_n;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="it">geloso</trd>
</drv>

<drv mrk="jxaluz.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxaluz.0o.KOMUNE">
    <dif>
      Sento de iu <tld/>a.
      <ekz>
        amo kaj <tld/>o estas gefratoj<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="ca">gelosia</trd>
  <trd lng="de">Eifersucht</trd>
  <trd lng="en">jealousy</trd>
  <trd lng="es">celos</trd>
  <trd lng="fr">jalousie</trd>
  <trd lng="he">&qof;&nun;&alef;&he;</trd>
  <trd lng="hu">f&eacute;lt&eacute;kenys&eacute;g</trd>
  <trdgrp lng="id">
    <trd>[<ind>cemburu</ind>] kecemburuan</trd>,
    <trd>[<ind>dengki</ind>] kedengkian</trd>,
    <trd>[<ind>iri</ind>] keirian</trd>
  </trdgrp>
  <trd lng="nl">jalousie</trd>
  <trd lng="pl">zazdro&sacute;&cacute;</trd>
  <trdgrp lng="pt">
    <trd>ci&uacute;me</trd>,
    <trd>zelo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">svartsjuka</trd>
 <trd lng="it">gelosia</trd>
  <trd lng="th">&#3588;&#3623;&#3634;&#3617;&#3627;&#3638;&#3591;</trd>
</drv>

<drv mrk="jxaluz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="jxaluz.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        Moseo diris al li: &ccirc;u vi <tld/>as pri mi?
        <fnt><bib>MT</bib>, <lok>&Nom; 11:29</lok></fnt>;
      </ekz>
      <ekz>
        atakos lin spirito de <tld/>o kaj li <tld/>os sian edzinon
        <fnt><bib>MT</bib>, <lok>&Nom; 5:14</lok></fnt>;
      </ekz>
      <ekz>
        mian edzinon volas pereigi viaj eksedzinoj,
        kiuj <tld/>as &scirc;ian virton!
        <fnt>
          <aut>L. Zamenhof</aut>,
          <vrk>Quo vadis?</vrk>,
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i pro io, al, kontra&ubreve; iu.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="envi.0i">envii</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>tenir gels</trd>,
    <trd>envejar</trd>
  </trdgrp>
  <trd lng="de">eifers&uuml;chtig sein</trd>
  <trdgrp lng="es">
    <trd>estar <ind>celoso</ind> (de)</trd>,
    <trd>sentir <ind>celos</ind> (de)</trd>
  </trdgrp>
  <trd lng="en">be <ind>jealous</ind></trd>
  <trd lng="fr">&ecirc;tre <ind>jaloux</ind> (de)</trd>
  <trd lng="he">&lamed;<ind>&qof;&nun;&alef;</ind></trd>
  <trd lng="hu">f&eacute;lt&eacute;kenykedik</trd>
  <trd lng="nl">jaloers zijn</trd>
  <trd lng="pl">by&cacute; <ind>zazdrosnym</ind></trd>
  <trdgrp lng="pt">
    <trd>sentir ci&uacute;mes</trd>,
    <trd>ter ci&uacute;mes</trd>,
    <trd>mostrar zelo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">vara <ind>svartsjuk</ind></trd>
  <trd lng="it">essere <ind>geloso</ind></trd>
  <trd lng="th">&#3627;&#3638;&#3591;</trd>
</drv>
</art>
