<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mars.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>Mars</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mars.0o">
  <kap><tld/>o</kap>
  <snc mrk="mars.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">dio</ref> de milito.
      Analogo de helena <ref tip="vid" cel="ares.0o">Areso</ref>.
    </dif>
    <trd lng="pt">Marte <klr>(mit.)</klr></trd>
    <trd lng="zh">马尔斯 [<ind>mǎ ěr sī</ind>] <klr>(罗马神话中的战神)</klr></trd>
  </snc>
  <snc mrk="mars.0o.AST">
    <uzo tip="fak">AST</uzo>
     <mlg>&#9794;</mlg>
    <dif>
      La kvara (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>)
      <ref tip="lst" cel="planed.0o" lst="voko:planedoj">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>:
      <ekz>
        dum multaj jaroj oni kredis, ke ekzistas vivo sur la planedo
        <tld/>o
        <fnt>
        <aut>Amri Wandel</aut>,
        <vrk>&Ccirc;u ni estas solaj?</vrk>,
        <lok><url ref="http://esperanto.org/AEK/biblioteko.html">
        Virtuala Biblioteko de AEKo</url>, [vidita je 2006-01-24]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dejm.0o">Dejmo</ref>,
      <ref cel="fob.0o">Fobo</ref>
    </refgrp>
    <trdgrp lng="zh">
      <trd>火星 [<ind>huǒ xīng</ind>]</trd>
      <trd>荧惑 [<ind>yíng huò</ind>]</trd>
    </trdgrp>
    <trd lng="pt">Marte <klr>(astr.)</klr></trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_r;&c_s;</trd>
  <trd lng="ca">Mart</trd>
  <trd lng="cs">Mars</trd>
  <trd lng="de">Mars</trd>
  <trd lng="el">&Alfa_ton;&rho;&eta;&sigma_fina;</trd>
  <trd lng="en">Mars</trd>
  <trd lng="es">Marte</trd>
  <trd lng="fr">Mars</trd>
  <trd lng="hu">Mars</trd>
  <trd lng="id">Mars</trd>
  <trd lng="it">Marte</trd>
  <trd lng="nl">Mars</trd>
  <trd lng="pl">Mars</trd>
  <trd lng="pt">Marte</trd>
  <trd lng="ru">&c_M;&c_a;&c_r;&c_s;</trd>
  <trd lng="sk">Mars</trd>
</drv>

<drv mrk="mars.0a">
  <kap><tld lit="m"/>a</kap>
  <snc mrk="mars.0a.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Rilata al <tld/>o<sncref ref="mars.0o.mita"/>.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_a;&c_w;</trd>
    <trd lng="ca">de Mart</trd>
    <trd lng="pl">marsowy</trd>
    <trd lng="pt">marciano <klr>(adj.)</klr></trd>
    <trd lng="ru">&c_m;&c_a;&c_r;&c_s;&c_o;&c_v;</trd>
  </snc>
  <snc mrk="mars.0a.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Rilata al <tld/>o<sncref ref="mars.0o.AST"/>.
    </dif>
    <trd lng="be">
    &c_m;&c_a;&c_r;&c_s;&c_ib;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    <trd lng="ca">marci&agrave;<klr> (adj.)</klr></trd>
    <trd lng="pl">marsja&nacute;ski</trd>
    <trd lng="ru">
    &c_m;&c_a;&c_r;&c_s;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="de">Mars-</trd>
  <trd lng="en">Martian</trd>
  <trd lng="es">marciano</trd>
  <trd lng="fr">martien<klr> (adj.)</klr></trd>
  <trd lng="hu">Mars-</trd>  
  <trd lng="it">marziano</trd>
  <trd lng="nl">Mars-</trd>  
  <trdgrp lng="pt">
    <trd>marciano <klr>(adj.)</klr></trd>,
    <trd>marci&aacute;tico</trd>
  </trdgrp>
 </drv>

<drv mrk="mars.0ano">
  <kap><tld lit="m"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o<sncref ref="mars.0o.AST"/>
      (en sciencfikcia literaturo).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trd lng="ca">marci&agrave;<klr> (adj.)</klr></trd>
  <trd lng="de">Marsbewohner</trd>
  <trd lng="en">Martian</trd>
  <trd lng="es">marciano</trd>
  <trd lng="fr">martien<klr> (subst.)</klr></trd>
  <trd lng="hu">marslak&oacute;</trd>  
  <trd lng="it">marziano</trd>
  <trdgrp lng="nl">
     <trd>marsbewoner</trd>,
     <trd>marsmannetje</trd>
  </trdgrp>
  <trd lng="pl">marsjanin</trd>
  <trd lng="pt">marciano <klr>(subst.)</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_s;&c_i;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="zh">火星人 [<ind>huǒ xīng rén</ind>]</trd>
</drv>
</art>
