<vortaro>
<art mrk="$Id: molest.xml,v 1.12 2015/11/10 06:10:15 revo Exp $">
<kap>
  <rad>molest</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="molest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="trakt.mis0i">Mistrakti</ref> per batoj,
      supra&jcirc;aj vundadoj a&ubreve; similaj malamikaj agoj:
      <ekz>
        &ccirc;u vi pensas, <klr>...</klr> ke vi povas kaprice <tld/>i? Vi
        miskalkulas
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, volumo 2a, p.
        563a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dot&iacute;rat</trd>,
    <trd>hrub&ecaron; jednat</trd>,
    <trd>obt&ecaron;&zcaron;ovat</trd>,
    <trd>ur&aacute;&zcaron;liv&ecaron; napadat</trd>,
    <trd>&scaron;patn&ecaron; zach&aacute;zet</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to accost</trd>,
    <trd>molest</trd>
  </trdgrp>
  <trd lng="es">molestar</trd>
  <trdgrp lng="fr">
    <trd>brutaliser</trd>,
    <trd>molester</trd>,
    <trd>rudoyer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zaklat</trd>,
    <trd>moleszt&aacute;l</trd>,
    <trd>b&aacute;nt</trd>
  </trdgrp>
  <trd lng="nl">molesteren</trd>
  <trd lng="pl">molestowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>molestar</trd>,
   <trd>destratar</trd>,
   <trd>machucar</trd>,
   <trd>ferir</trd>   
   <trd>incomodar</trd>
  </trdgrp><trd lng="pt">molestar</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_b;&c_o;
&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_je;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepriate&lcaron;sky</trd>,
    <trd>ur&aacute;&zcaron;livo jedna&tcaron;</trd>,
    <trd>zaobch&aacute;dza&tcaron; <klr>(s nie&ccaron;&iacute;m, niek&yacute;m)</klr></trd>
  </trdgrp>
</drv>
</art>
