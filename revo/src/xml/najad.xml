<art mrk="$Id: najad.xml,v 1.16 2015/07/04 08:37:39 revo Exp $">
<kap>
   <rad>najad</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="najad.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Nimfo de fontoj kaj riveroj.
     </dif>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_ja;&c_d;&c_a;</trd>
  <trd lng="ca">n&agrave;iade</trd>
  <trdgrp lng="cs">
    <trd>jezerní panna</trd>,
    <trd>najáda</trd>,
    <trd>rusalka</trd>,
    <trd>vodní panna</trd>,
    <trd>vodní víla</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Najade</trd>,
     <trd>Quellnymphe</trd>,
     <trd>Flussnymphe</trd>
   </trdgrp>
  <trd lng="el">&ny;&alfa;&jota_trema;&alfa_ton;&delta;&alfa;</trd>
  <trdgrp lng="en">
     <trd>naiad</trd>,
     <trd>water nymph</trd>
   </trdgrp>
  <trd lng="es">n&aacute;yade</trd>
  <trd lng="fr">na&iuml;ade</trd>
  <trdgrp lng="hu">
     <trd>naj&aacute;d</trd>,
     <trd>v&iacute;zi <ind>t&uuml;nd&eacute;r</ind></trd>
   </trdgrp>
  <trd lng="nl">waternimf</trd>
  <trdgrp lng="pt">
     <trd>n&aacute;iade</trd>,
     <trd>n&aacute;iada</trd>
   </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_ja;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>najada</trd>,
    <trd>rusalka</trd>,
    <trd>vodná víla</trd>
  </trdgrp>
  <trd lng="sv">najad</trd>
</drv>
</art>
