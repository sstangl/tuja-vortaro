<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: toluen.xml,v 1.11 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>toluen</rad>/o
</kap>
<drv mrk="toluen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>5</sub>CH<sub>3</sub>.
      Likva senkolora <ref tip="super"
      cel="hidrok.0o">hidrokarbido</ref>,
      bolanta en temperaturo de 111&#730;C. Uzata kiel 
      <ref cel="solv.0ilo">solvilo</ref>, 
      <ref cel="reakci.0ilo">reakciilo</ref> kaj kiel konsista
      parto de iuj benzinoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_u;&c_o;&c_l;</trd>
  <trd lng="bg">&c_t;&c_o;&c_l;&c_u;&c_je;&c_n;</trd>
  <trd lng="ca">tolu&egrave;</trd>
  <trd lng="cs">toluen</trd>
  <trdgrp lng="de">
    <trd>Toluen</trd>,
    <trd>Toluol</trd>
  </trdgrp>
  <trd lng="en">toluene</trd>
  <trd lng="es">tolueno</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_w;&a_l;&a_w;&a_y_hamza;&a_n;</trd>,
    <trd>&a_m;&a_t;&f_y;&a_l; &a_b;&a_n;&a_z;&a_w;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">tolu&egrave;ne</trd>
  <trd lng="hu">toluol</trd>
  <trd lng="nl">tolueen</trd>
  <trd lng="pl">toluen</trd>
  <trd lng="pt">tolueno</trd>
  <trd lng="sk">tolu√©n</trd>
  <trd lng="sv">toluen</trd>
</drv>
</art>

