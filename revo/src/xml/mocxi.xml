<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mocxi.xml,v 1.6 2015/06/26 17:10:15 revo Exp $">
<kap>
   <rad>mo&ccirc;i</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mocxi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kuk.0o">Kuko</ref> el rizka&ccirc;o sekigita
      kaj fritita a&ubreve; rostita:
      <ekz>
        &scirc;i alportis pleton da varma <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 60a, p.
311a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mochi<klr tip="amb"> (japanischer
  Reiskuchen)</klr></trd>
  <trd lng="es">pastel de arroz</trd>
  <trd lng="fa">&a_m;&a_w;&f_ch;&f_y;</trd>
  <trd lng="fr">mochi<klr> (g&acirc;teau de riz)</klr></trd>
  <trdgrp lng="hu">
    <trd>rizsfelf&uacute;jt</trd>,
    <trd>rizspuding</trd>
  </trdgrp>
  <trd lng="nl">mochi</trd>
  <trd lng="pt">arroz <ind>mochi</ind></trd>
</drv>
</art>

