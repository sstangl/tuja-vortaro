<kap>
  <rad>kaliforni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kaliforni.0o">
  <kap><tld/>o</kap>
  <snc>
  <mlg>Cf</mlg>
  <uzo tip="fak">KEM</uzo>
  <dif>
  <ref tip="dif" cel="kalifornium.0o">kaliforniumo</ref>
  </dif>
  <trd
lng="be">&c_k;&c_a;&c_l;&c_ib;&c_f;&c_o;&c_r;&c_n;&c_ib;&c_j;</trd>
  <trd lng="br">kaliforniom</trd>
  <trd lng="ca">californi</trd>
  <trd lng="de">Californium</trd>
  <trd lng="en">californium</trd>
  <trd lng="es">californio</trd>
  <trd lng="fr">californium</trd>
  <trd lng="hu">kalifornium</trd>
  <trd lng="id">kalifornium</trd>
  <trd lng="it">californio</trd>
  <trd lng="la">Californium</trd>
  <trd lng="nl">californium</trd>
  <trd lng="pl">kaliforn</trd>
  <trd lng="pt">calif&oacute;rnio</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
  <trd lng="sv">californium</trd>
  </snc>

  <snc>
<fnt><bib>K</bib></fnt>
  <uzo tip="fak">GEOG</uzo>
  <dif>
<ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> de
<ref tip="super" cel="uson.U0o">Usono</ref>, situanta &ccirc;e la
<ref tip="super" cel="pacifik.P0o">Pacifika</ref> 
marbordo de 
<ref tip="super" cel="amerik.Nord0o">Nordameriko</ref>.
&Ccirc;efurbo estas Sakramento, la plej grava urbo estas
Los-An&gcirc;eleso.
  <ekz>
   Kiel oni scias, <tld lit="K"/>o apartenas al la lokoj tre ri&ccirc;aj
kiel per mineraloj, tiel anka&ubreve; per produkta&jcirc;oj de la regno
kreska&jcirc;a.
   <fnt>
   <aut>L. L. Zamenhof</aut>
   <vrk>Fundamenta Krestomatio</vrk>
   </fnt>
   </ekz>
  </dif>
  <trd lng="ca">Calif&ograve;rnia</trd>
  <trd lng="de">Kalifornien</trd>
  <trd lng="id">Kalifornia</trd>
  <trd lng="it">California</trd>
  <trd lng="en">California</trd>
  <trd lng="pl">Kalifornia</trd>
  <trd lng="sv">Kalifornien</trd>
  </snc>
  <trd lng="cs">kalifornium</trd>
  <trd lng="sk">kalifornium</trd>
</drv>
