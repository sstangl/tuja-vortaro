        <fnt>
          <aut>Qiong Yao, trad. Vejdo</aut>,
          <vrk><url
          ref="http://www.ipernity.com/group/115874/discuss/130589">
          En profunda korto (28)</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">meneau</trd>
</drv>
