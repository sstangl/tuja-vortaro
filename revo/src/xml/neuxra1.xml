<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: neuxra1.xml,v 1.14 2014/11/19 07:10:24 revo Exp $">
<kap>
  <rad>ne&ubreve;rasteni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="neuxra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="neuxra1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> karakterizata per malforti&gcirc;o de
      nervoj, laco,
      timemo, kapdoloro...
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_r;&c_a;&c_s;&c_t;&c_e;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_n;&c_je;&c_v;&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Neurasthenie</trd>,
    <trd>Nervenschw&auml;che</trd>
  </trdgrp>
  <trd lng="en">neurasthenia</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_s;&a_t;&a_h;&zwnj;&a_r;&a_w;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_D;&a_ayn;&a_f; &a_A;&a_ayn;&a_S;&a_A;&a_b;</trd>,
    <trd>&a_n;&a_w;&a_r;&a_A;&a_s;&a_t;&a_kasra;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">neurasth&eacute;nie</trd>
  <trdgrp lng="hu">
    <trd>ideggyenges&eacute;g</trd>,
    <trd>neuraszt&eacute;nia</trd>
  </trdgrp>
  <trd lng="id">neurastenia</trd>
  <trdgrp lng="nl">
    <trd>zenuwzwakte</trd>,
    <trd>neurastenie</trd>
  </trdgrp>
  <trd lng="pt">neurastenia</trd>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sv">neurasteni</trd>
</drv>

<drv mrk="neuxra1.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="neuxra1.0ulo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Suferanto de <tld/>o:
      <ekz>
        vi, havante aferon kun tia <tld/>ulo, kiel mi, konsentis 
        preni mian kapitalon sub vian regadon, liberiginte min de 
        aferaj pensoj, gestoj, tagoj, horoj kaj minutoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">neurasth&eacute;nique<klr> (subst.)</klr></trd>
</drv>
</art>
