<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: biplan.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>biplan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="biplan.0o">
  <kap><tld/>o</kap>
  <snc mrk="biplan.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aeropl.0o">Aeroplano</ref> havanta du parojn
      da flugiloj:
      <ekz>
        la&ubreve; la statuto de la Klubo, ajna monoplano, <tld/>o, para&scirc;uto, 
        balono a&ubreve; aerostato devas esti, por eviti ridindajn kaj 
        malfeli&ccirc;ajn akcidentojn, anticipe taksita de ekspertoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="monopl.0o">monoplano</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_p;&c_l;&c_ja;&c_n;</trd>
  <trd lng="bg">&c_b;&c_i;&c_p;&c_l;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>biplán</trd>,
    <trd>dvouplošník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Doppeldecker</trd>,
    <trd>Zweidecker</trd>
  </trdgrp>
  <trd lng="en">biplane</trd>
  <trd lng="es">biplano</trd>
  <trd lng="fr">biplan</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;tfedel&udblac;
    <ind>rep&uuml;l&odblac;g&eacute;p</ind></trd>,
    <trd>bipl&aacute;n</trd>
  </trdgrp>
  <trd lng="it">biplano</trd>
  <trd lng="nl">tweedekker</trd>
  <trdgrp lng="pl">
    <trd>dwup&lstroke;atowiec</trd>,
    <trd>dwup&lstroke;at</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_p;&c_l;&c_a;&c_n;</trd>
  <trd lng="sk">dvojplo&scaron;n&iacute;k</trd>
</drv>
</art>

