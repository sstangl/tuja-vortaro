<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: velen.xml,v 1.14 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>velen</rad>/o
</kap>
<drv mrk="velen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Por skribo preparita bovida felo, pli delikata ol pergameno.
    </dif>
    <trdgrp lng="de">
      <trd>Velin</trd>,
      <trd>Schreibpergament</trd>
    </trdgrp>
    <trd lng="hu">borj&uacute;b&odblac;r <ind>pergamen</ind></trd>
    <trd lng="nl">velijn</trd>
    <trd lng="ru">&c_v;&c_je;&c_l;&c_je;&c_n;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Glata, <tld/>simila papero.
    </dif>
    <trd lng="ca">paper <ind>vitel&middot;la</ind></trd>
    <trd lng="de">Velinpapier</trd>
    <trd lng="nl">velijnpapier</trd>
    <trd lng="ru">&c_v;&c_je;&c_l;&c_je;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_b;&c_u;&c_m;&c_a;&c_g;&c_a;</trd>
  </snc>
  <trd lng="ca">vitel&middot;la</trd>
  <trdgrp lng="cs">
    <trd>velín</trd>,
    <trd>velínový papír</trd>
  </trdgrp>
  <trd lng="fr">v&eacute;lin</trd>
  <trd lng="hu">velin(pap&iacute;r)</trd>
  <trd lng="pt">velino</trd>
  <trd lng="sk">teľacia koža upravená na písanie</trd>
</drv>
</art>
