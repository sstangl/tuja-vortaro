<vortaro>
<art mrk="$Id: majenc.xml,v 1.15 2015/09/24 11:10:22 revo Exp $">
<kap>
  <rad>Majenc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="majenc.M0o">
  <kap><tld/>o</kap>
  <snc mrk="majenc.M0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en sudokcidenta <ref
      tip="malprt" cel="german1.G0ujo">Germanujo</ref> (8&#730;16'E,
      50&#730;00'N), situanta &ccirc;e la rivero <ref tip="vid"
      cel="rejn.R0o">Rejno</ref>, regurbo de la federacia
      lando <ref tip="vid"
      cel="rejnlandp.R0o">Rejnland-Palatinato</ref>.
    </dif>
    <trd lng="be">&c_M;&c_a;&c_j;&c_n;&c_c;</trd>
    <trd lng="bg">&c_M;&c_a;&c_j;&c_n;&c_c;</trd>
    <trd lng="ca">Mag&uacute;ncia</trd>
    <trd lng="de">Mainz</trd>
    <trd lng="en">Mainz <klr>(formerly Mentz)</klr></trd>
    <trd lng="es">Maguncia</trd>
    <trd lng="fr">Mayence</trd>
    <trd lng="hu">Mainz</trd>
    <trd lng="lat">Moguntiacum</trd>
    <trd lng="nl">Mainz</trd>
    <trd lng="pl">Moguncja</trd>
    <trdgrp lng="pt">
      <trd>Mog&uacute;ncia</trd>,
      <trd>Mainz</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>Mainz</trd>,
    <trd>Mohu&ccaron;</trd>
  </trdgrp>
  <trd lng="sk">Muh&uacute;&ccaron;</trd>
</drv>
</art>
