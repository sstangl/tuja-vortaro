<vortaro>
<art mrk="$Id: apertu.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
   <ofc>6</ofc>
   <rad>apertur</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="apertu.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Malferma&jcirc;o:
       <ekz>
         la fenestra <tld/>o fermi&gcirc;is<fnt>Z</fnt>;
       </ekz>
       <ekz>
         alrulu grandajn &scirc;tonojn al la <tld/>o de la kaverno
         <fnt><bib>MT</bib>, <lok>&Jos; 10:18</lok></fnt>;
       </ekz>
       <ekz>
         gardu la <tld/>on de via bu&scirc;o
         <fnt><bib>MT</bib>, <lok>&Mih; 7:5</lok></fnt>;
       </ekz>
       <ekz>
         <tld/>o de sako
         <fnt><bib>MT</bib>, <lok>&Gen; 43:21</lok></fnt>;
       </ekz>
       <ekz>
         <tld/>o de tubo, de puto<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>o por la kapo en mezo de tuniko
         <fnt><bib>MT</bib>, <lok>&Eli; 28:32</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>,
     <trd>&c_d;&c_z;&c_ib;&c_r;&c_k;&c_a;</trd>,
     <trd>&c_zh;&c_a;&c_r;&c_a;&c_l;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>apertura</trd>,
    <trd>díra</trd>,
    <trd>jícen</trd>,
    <trd>otvor</trd>,
    <trd>průduch</trd>,
    <trd>ventil</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>&Ouml;ffnung</trd>,
        <trd>Lochweite</trd>,
        <trd>Apertur</trd>
   </trdgrp>
  <trd lng="en">aperture</trd>
  <trd lng="es">abertura</trd>
  <trdgrp lng="fr">
     <trd>ouverture</trd>,
     <trd>entr&eacute;e</trd>
     <trd>orifice</trd>
   </trdgrp>
  <trd lng="hu">ny&iacute;l&aacute;s</trd>
  <trd lng="it">apertura</trd>
  <trd lng="nl">opening</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">stupeň otvorenia clony</trd>
  <trd lng="sv">&ouml;ppning</trd>
</drv>
</art>
