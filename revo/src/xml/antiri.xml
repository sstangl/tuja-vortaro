<vortaro>
<art mrk="$Id: antiri.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>antirin</rad>/o
</kap>
<drv mrk="antiri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="skrofu1.0oj">skrofulariacoj</ref>
      (<trd lng="la">Antirrhinum</trd>), kies floro similas iomete la
      bu&scirc;egon de leono.
    </dif>
  </snc>
  <trd lng="ca">antirr&iacute;num</trd>
  <trd lng="cs">hledík</trd>
  <trd lng="de">L&ouml;wenmaul</trd>
  <trd lng="en">snapdragon</trd>
  <trdgrp lng="fr">
    <trd>muflier</trd>,
    <trd>gueule-de-loup</trd>
  </trdgrp>
  <trd lng="hu">oroszl&aacute;nsz&aacute;j</trd>
  <trd lng="nl">leeuwenbek</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_r;&c_r;&c_i;&c_n;&c_u;&c_m;</trd>,
    <trd>&c_l;&c_mol;&c_v;&c_i;&c_n;&c_y;&c_j; &c_z;&c_je;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">papuľka</trd>
</drv>
</art>
