<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klin.xml,v 1.23 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klin</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klin.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="klin.0i.MOV">
    <dif>
      Oblikvigi ion vertikalan; fleksi malsupren a&ubreve; flanken:
      <ekz>
	<tld/>i la kapon, la frunton anta&ubreve; iu;
      </ekz>
      <ekz>
	sin salute <tld/>i;
      </ekz>
      <ekz>
	gracia <ind><tld/>o</ind> de la kapo<fnt>Z</fnt>;
	<trd lng="fr"><klr tip="ind">(mouvement d') </klr>inclinaison</trd>
      </ekz>
      <ekz>
	mi <tld/>as min anta&ubreve; vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
	la ne&gcirc;o <tld/>is al la tero la bran&ccirc;ojn de l' abioj<fnt>B</fnt>;
      </ekz>
      <ekz>
	<ind>sin <tld/>i</ind> malanta&ubreve;en kaj rigardi la plafonon;
	<trdgrp lng="fr">
	  <trd>s'<ind>incliner</ind></trd>,
	  <trd>c&eacute;der</trd>,
	  <trd>capituler<klr tip="ind"> (c&eacute;der)</klr></trd>,
	  <trd>fl&eacute;chir<klr tip="ind"> (intr., c&eacute;der)</klr></trd>,
	  <trd>plier<klr tip="ind"> (intr., c&eacute;der)</klr></trd>,
	  <trd>se <ind>plier</ind><klr tip="ind"> (c&eacute;der)</klr></trd>,
	  <trd>ob&eacute;ir<klr tip="ind"> (c&eacute;der)</klr></trd>,
	  <trd>se <ind>rendre</ind><klr tip="ind"> (c&eacute;der)</klr></trd>,
	  <trd>renoncer<klr tip="ind"> (c&eacute;der)</klr></trd>
	</trdgrp>
      </ekz>
      <ekz>
	<tld/>ita sub la &scirc;ar&gcirc;o de la jaroj;
      </ekz>
      <ekz>
	arbo <tld/>ita de la vento;
      </ekz>
      <ekz>
	<ind>(al)<tld/>i la orelon al iu</ind><fnt>Z</fnt>.
	<trd lng="fr">pr&ecirc;ter l'<ind>oreille</ind> &agrave; qn</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>beugen</trd>,
      <trd>neigen</trd>,
      <trd>kippen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>agachar</trd>,
      <trd>bajar</trd>
    </trdgrp>
  </snc>
  <snc mrk="klin.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Emigi, inklinigi, cedigi:
      <ekz>
	<tld/>i al pardonemo la animon de la re&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
	mi <tld/>is mian koron por plenumi viajn le&gcirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
	plena sako &ccirc;iun mastron al vi <tld/>os
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
	devi sin <tld/>i <klr>(cedi)</klr> anta&ubreve; la cirkonstancoj<fnt>Z</fnt>.


      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_d;&c_a; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trdgrp lng="de">
      <trd>beugen</trd>,
    </trdgrp>
    <trd lng="es">humillar</trd>
    <trd lng="ru">&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>chýlit</trd>,
    <trd>klonit</trd>,
    <trd>klopit</trd>,
    <trd>nachýlit</trd>,
    <trd>nahnout</trd>,
    <trd>nahýbat</trd>,
    <trd>naklánět</trd>,
    <trd>schýlit</trd>,
    <trd>sehnout</trd>,
    <trd>sklonit</trd>,
    <trd>sklánět</trd>,
    <trd>sklápět</trd>
  </trdgrp>
  <trd lng="en">bend</trd>
  <trdgrp lng="fr">
    <trd>incliner</trd>,
    <trd>faire <ind>c&eacute;der</ind><klr tip="ind"> (en faveur
    de...)</klr></trd>,
    <trd>pencher<klr tip="ind"> (tr., incliner)</klr></trd>,
    <trd>faire <ind>pencher</ind><klr tip="ind"> (en faveur
    de...)</klr></trd>,


    <trd>plier<klr tip="ind"> (tr., incliner)</klr></trd>,
    <trd>faire <ind>plier</ind><klr tip="ind"> (inciter &agrave;
    c&eacute;der)</klr></trd>,


    <trd>fl&eacute;chir<klr tip="ind"> (tr., incliner)</klr></trd>,
    <trd>faire <ind>fl&eacute;chir</ind><klr tip="ind"> (inciter &agrave;
c&eacute;der)</klr></trd>,
    <trd>inciter</trd>,
    <trd>porter<klr tip="ind"> (inciter)</klr></trd>,
    <trd>pousser<klr tip="ind"> (inciter)</klr></trd>,
    <trd>pr&eacute;diposer</trd>
  </trdgrp>
  <trd lng="nl">buigen</trd>
  <trdgrp lng="sk">
    <trd>nakláňať</trd>,
    <trd>skloniť</trd>,
    <trd>skláňať</trd>
  </trdgrp>
</drv>

<drv mrk="klin.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="klin.0igxi.MOV">
    <dif>
      Sin <tld/>i, i&gcirc;i <tld/>ita:
      <ekz>
	<tld/>i&gcirc;i &gcirc;is la tero anta&ubreve; iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<ind>ador<tld/>i&gcirc;i</ind><fnt>Z</fnt>;
	<trdgrp lng="fr">
	  <trd>se <ind>prosterner</ind></trd>,
	  <trd>adorer<klr tip="ind"> (en se prosternant)</klr></trd>
	</trdgrp>
      </ekz>
      <ekz>
	<tld/>i&gcirc;intaj krucoj<fnt>B</fnt>;
      </ekz>
      <ekz>
	<ind><tld/>i&gcirc;inta</ind> turo de Pizo;
	<trd lng="fr">pench&eacute;</trd>
      </ekz>
      <ekz>
	la veturilo <tld/>i&gcirc;is kaj trafis en kavon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_k;&c_l;&c_a;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sich <ind>beugen</ind></trd>,
      <trd>sich <ind>neigen</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="klin.0igxi.FIG_mallevigxi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallevi&gcirc;i, kiel io <tld/>i&gcirc;anta; 
      proksimi&gcirc;i al sia fini&gcirc;o:
      <ekz>
	la suno <tld/>i&gcirc;as al la horizonto;
      </ekz>
      <ekz>
	la jaro <tld/>i&gcirc;is al sia fino<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<tld/>i&gcirc;i al la tombo<fnt>Z</fnt>;
      </ekz>
      <ekz>
	la tago jam forte <tld/>i&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
	la imperio <tld/>i&gcirc;is al sia ruino.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaduk.0a">kaduka</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_h;&c_ib;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="klin.0igxi.FIG_cedi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedi:
      <ekz>
	li <tld/>i&gcirc;is respekte anta&ubreve; la le&gcirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
	en ju&gcirc;o ne <tld/>i&gcirc;u al la opinio de la plimulto<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<tld/>i&gcirc;ado anta&ubreve; &scirc;ajnaj a&ubreve;toritatoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
    <trd lng="ru">&c_s;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (intr., s'incliner, c&eacute;der)</klr></trd>,
    <trd>capituler<klr tip="ind"> (c&eacute;der)</klr></trd>,
    <trd>c&eacute;der</trd>,
    <trd>incliner<klr tip="ind"> (intr.)</klr></trd>,
    <trd>s'<ind>incliner</ind></trd>,
    <trd>pencher<klr tip="ind"> (intr., s'incliner)</klr></trd>,
    <trd>se <ind>pencher</ind></trd>,
    <trd>plier<klr tip="ind"> (intr., s'incliner, c&eacute;der)</klr></trd>,
    <trd>se <ind>plier</ind><klr tip="ind"> (s'incliner,
    c&eacute;der)</klr></trd>,
    <trd>se <ind>rendre</ind><klr tip="ind"> (&agrave; une opinion,
c&eacute;der)</klr></trd>,
    <trd>renoncer<klr tip="ind"> (c&eacute;der)</klr></trd>,
    <trd>toucher<klr tip="ind"> (&agrave; son terme)</klr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>buigen</ind></trd>
</drv>

<drv mrk="klin.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Flankenigi, deturni, deteni:
      <ekz>
	ili vane penos de<tld/>i nin de nia vojo<fnt>Z</fnt>;
      </ekz>
      <ekz>
	ka&scirc;itajn donacojn akceptas malvirtulo, por de<tld/>i la vojon
	de la justeco<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<ind>de<tld/>i la koron de</ind> la Izraelidoj de la transiro en la
	landon<fnt>Z</fnt>.
	<trdgrp lng="fr">
	  <trd>d&eacute;courager</trd>,
	  <trd>dissuader</trd>
	</trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odchýlit</trd>,
    <trd>odklonit</trd>,
    <trd>vychýlit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>biegen</trd>,
    <trd>zur Seite <ind>neigen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner<klr tip="ind"> (qn de sa voie)</klr></trd>,
    <trd>dissuader</trd>,
    <trd>emp&ecirc;cher</trd>,
    <trd>faire <ind>d&eacute;vier</ind></trd>,
    <trd>faire <ind>&eacute;viter</ind></trd>
  </trdgrp>
  <trd lng="nl">afbuigen</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">odkloniť</trd>
</drv>

<drv mrk="klin.de0igxi">
  <kap>de<tld/>i&gcirc;i</kap>
  <snc mrk="klin.de0igxi.MOV">
    <dif>
      Flankeni&gcirc;i de normala sinteno, de ia &gcirc;usta vojo...:
      <ekz>
	&ccirc;iuj arbegoj, rozujoj kaj dornoj de<tld/>i&gcirc;is, kaj li pasis
	<fnt>
	  <aut>C. Perrault, trad. P. Sarpy</aut>,
	  <vrk>La Belulino dormanta en Arbaro</vrk>, 
	  <lok><url
ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">Rakontoj
	  pri feinoj</url>, Hachette, 1904</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>wegbeugen</ind></trd>,
      <trd>sich zur Seite <ind>neigen</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="klin.de0igxi.FIG">
    <dif>
      Flankeni&gcirc;i de vero a&ubreve; justo:
      <ekz>
	de<tld/>i&gcirc;i de la bona konduto, de la reguloj<fnt>Z</fnt>,
      </ekz>
      <ekz>
	de sia &jcirc;uro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;tourner</ind><klr tip="ind"> (de sa
    voie)</klr></trd>,
    <trd>s'<ind>&eacute;carter</ind><klr tip="ind"> (de sa voie)</klr></trd>,
    <trd>s'<ind>&eacute;loigner</ind><klr tip="ind"> (de sa voie)</klr></trd>,
    <trd>d&eacute;vier<klr tip="ind"> (intr.)</klr></trd>,
  </trdgrp>
  <trd lng="nl">afwijken</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="klin.de0igxo">
  <kap>de<tld/>i&gcirc;o</kap>
  <snc mrk="klin.de0igxo.MOV">
    <dif>
      Flankeni&gcirc;o:
      <ekz>
	de<tld/>i&gcirc;o de la magneta montrilo.
      </ekz>
    </dif>
    <trd lng="en">declination</trd>
  </snc>
  <snc mrk="klin.de0igxo.FIG">
    <dif>
      Malobservado de la reguloj a&ubreve; de la moralo:
      <ekz>
	&ccirc;iuj liaj de<tld/>i&gcirc;oj estos pardonitaj;
      </ekz>
      <ekz>
	per nede<tld/>i&gcirc;a irado rekte anta&ubreve;en<fnt>Z</fnt>;
      </ekz>
      <ekz>
	li sende<tld/>i&gcirc;e iris al la elektita celo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;cart<klr tip="ind"> (par rapport &agrave; la r&egrave;gle)</klr></trd>,
    <trd>d&eacute;clinaison<klr tip="ind"> (&eacute;cart)</klr></trd>,
    <trd>d&eacute;viance</trd>,
    <trd>transgression</trd>,
  </trdgrp>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="klin.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malproksimigi de si klinante, deturni al alia direkto:
      <ekz>
	for<tld/>i la kapon<fnt>Z</fnt>,
      </ekz>
      <ekz>
	sian orelon<fnt>Z</fnt>,
      </ekz>
      <ekz>
	pilkon;
      </ekz>
      <ekz>
	<uzo tip="stl">FIG</uzo>
	for<tld/>i ies koleron, ripro&ccirc;ojn, koron<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<uzo tip="stl">FIG</uzo>
	for<tld/>i la rajton de fremdulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
	<uzo tip="stl">FIG</uzo>
	for<tld/>i&gcirc;i de la virto<fnt>Z</fnt>,
      </ekz>
      <ekz>
	<uzo tip="stl">FIG</uzo>
	de la ordonoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">odklonit</trd>
  <trd lng="es">desviar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner<klr tip="ind"> (de soi)</klr></trd>,
    <trd>&eacute;carter<klr tip="ind"> (de soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afweren</trd>,
    <trd>ombuigen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odkloniť</trd>
</drv>

<drv mrk="klin.ador0igxi">
  <kap>ador<tld/>i&gcirc;i</kap>
  <snc mrk="klin.ador0igxi.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Klini&gcirc;i anta&ubreve;en, eventuale &gcirc;is tu&scirc;o de la grundo, elmontrante
      <ref tip="vid" cel="ador.0i">adoron</ref>:
      <ekz>
	li iros kaj servos al aliaj dioj, kaj ador<tld/>i&gcirc;os
	anta&ubreve; ili a&ubreve; anta&ubreve; la suno a&ubreve; anta&ubreve; la luno
	<fnt>
	  <bib>MT</bib>,
	  <lok>&Rea; 17:3</lok>
	</fnt>.
      </ekz>
    </dif>    
  </snc>
  <trd lng="fr">se <ind>prosterner</ind></trd>
</drv>
</art>
