<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kasxta.xml,v 1.36 2015/05/07 06:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ka&scirc;tan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kasxta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kasxta.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla frukto de <tld/>arbo.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>   
    <trd lng="bg">&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;</trd>
    <trd lng="br">kistinenn<klr tip="ind"> (frouezhenn)</klr></trd>
    <trdgrp lng="de">
      <trd>Kastanie</trd>, <trd>Marone</trd>
    </trdgrp>
    <trd lng="en">chestnut</trd>
    <trd lng="es">casta&ntilde;a <klr> (fruto del casta&ntilde;o)</klr></trd>
    <trdgrp lng="fr">
      <trd>ch&acirc;taigne</trd>,
      <trd>marron<klr tip="ind"> (ch&acirc;taigne)</klr></trd>
    </trdgrp>
    <trd lng="hu">gesztenye</trd>
    <trd lng="nl">kastanje</trd>
    <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
    <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
    <trd lng="tr">kestane</trd>
  </snc>
  <snc mrk="kasxta.0o.hipokasxtano">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="hipoka.0o">hipoka&scirc;tano</ref>   
    <trd lng="bg">&c_k;&c_o;&c_n;&c_s;&c_k;&c_i; <ind>&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;</ind></trd>
    <trd lng="br">kistinenn-Indez<klr tip="ind">
    (frouezhenn)</klr></trd>
    <trd lng="en">horse <ind>chestnut</ind></trd>
    <trd lng="de">Rosskastanie</trd>
    <trd lng="es">falso casta&ntilde;o<klr> (de Indias)</klr></trd>
    <trd lng="fr">marron<klr tip="ind"> (d'Inde)</klr></trd>
    <trdgrp lng="hu">
      <trd>szel&iacute;dgesztenye</trd>,
      <trd>mar&oacute;ni</trd>
    </trdgrp>
    <trd lng="nl">paardekastanje</trd>
  </snc>
  <trd lng="fa">&a_sh;&a_A;&a_h;&zwnj;&a_b;&a_l;&a_w;&a_T;</trd>
  <trd lng="pl">kasztan</trd>
</drv>

<drv mrk="kasxta.0a">
  <kap><tld/>a, <var><kap><tld/>kolora</kap></var></kap>
  <snc mrk="kasxta.0a.KOL">
    <dif>
      De sama, ru&gcirc;e
      <ref tip="vid" cel="brun.0a">bruna</ref>
      koloro, kiel la interna &scirc;elo de <tld/>oj:
      <ekz>
        hel<tld/>aj haroj
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u ni kunvenis 
        vane?</vrk>, <lok>1982</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">gell-kistin</trd>
    <trd lng="es">casta&ntilde;o<klr> (color)</klr></trd>
    <trd lng="fr">ch&acirc;tain</trd>
    <trd lng="nl">kastanjebruin</trd>
  </snc>
</drv>

<drv mrk="kasxta.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="kasxta.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fagac.0oj">fagacoj</ref>
      (<trd lng="la">Castanea</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>   
  <trd lng="bg">&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-gistin</trd>,
    <trd>kistinenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Esskastanie</trd>,
    <trd>Edelkastanie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chestnut</trd>,
    <trd>chestnut tree</trd>
  </trdgrp>
  <trd lng="es">casta&ntilde;o<klr> (&aacute;rbol)</klr></trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_sh;&a_A;&a_h;&zwnj;&a_b;&a_l;&a_w;&a_T;</ind></trd>
  <trd lng="fr">ch&acirc;taignier</trd>
  <trd lng="hu">gesztenyefa</trd>
  <trd lng="nl">kastanjeboom</trd>
  <trd lng="pl">kasztan</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="tr">kestane a&gbreve;ac&inodot;</trd>
</drv>

<drv mrk="kasxta.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kasxta.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Man&gcirc;ebla marbesto ekstere simila al <tld/>o: globa, kun
      pikiloj; 
      genro el <ref tip="super"
      cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>
      (<trd lng="la">Echino</trd>):
      <ekz>
        mar<tld/>a&jcirc;o estas flavru&gcirc;a ovario de
        marka&scirc;tano
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url
          ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2007-04-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekin.0o">ekino</ref>,
      <ref cel="ehxin.0o">e&hcirc;ino</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; &c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fa">&a_t;&a_w;&a_t;&f_y;&a_A;&f_y; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="nl">zee-egel</trd>
  <trd lng="pl">je&zdot;owiec</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j;
  &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
  <trd lng="tr">deniz kestanesi</trd>
</drv>
</art>
