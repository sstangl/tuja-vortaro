<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fascxi.xml,v 1.13 2013/04/01 01:10:08 revo Exp $">
<kap>
  <rad>fas&ccirc;in</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fascxi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fascxi.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Fasko el bran&ccirc;etoj por fari barojn, &scirc;topplenigi
      fosa&jcirc;ojn ktp,
      precipe uzata en militarto:
      <ekz>
        vi ja estus povinta ordigi la brul-<tld/>ojn
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="gabi.0o">gabio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_sh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Faschine</trd>
  <trdgrp lng="en">
    <trd>fascine</trd>,
    <trd>faggot (of brushwood)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fagot</trd>,
    <trd>fascine</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&odblac;zsek&ouml;teg</trd>,
    <trd>r&odblac;zsenyal&aacute;b</trd>
  </trdgrp>
  <trd lng="nl">fascine</trd>
  <trd lng="pl">faszyna</trd>
  <trd lng="pt">faxina <klr tip="amb">(feixe de ramos ou
  paus)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
