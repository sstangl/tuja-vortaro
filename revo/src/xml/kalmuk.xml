<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kalmuk.xml,v 1.13 2015/07/04 08:37:36 revo Exp $">
    <kap>
      <rad>kalmuk</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="kalmuk.0o">
      <kap><tld/>o</kap>
      <snc mrk="kalmuk.0o.ETN">
        <dif>
          Ano de <ref tip="super" cel="mongoled.0a">mongoleda</ref>
          popolo vivanta en Ruslando, Mongolujo kaj &Ccirc;inujo:
          <ekz>
            la Ruslandaj <tld/>oj estas la sola tradicie
            <ref cel="buda.0ismo">budaisma</ref> popolo de E&ubreve;ropo;
          </ekz><ekz>
            anka&ubreve; la sova&gcirc;a <tld/>o kaj mongolo posedas forton
            <fnt><bib>PatrojFiloj</bib>,
              <lok>&ccirc;apitro 10a, p. 55a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_m;&c_y;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_m;&c_i;&c_k;</trd>
  <trd lng="cs">kalmuk</trd>
  <trd lng="de">Kalm&uuml;cke</trd>
  <trd lng="en">kalmyk</trd>
  <trd lng="fa">&f_k;&a_A;&a_l;&a_m;&a_w;&f_k;</trd>
  <trd lng="fr">Kalmouk<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="it">
        <trd>calmucco<klr> (sost.)</klr></trd>,
        <trd>calmicco<klr> (sost.)</klr></trd>
      </trdgrp>
  <trd lng="nl">kalmuk</trd>
  <trd lng="pl">Ka&lstroke;muk</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_m;&c_y;&c_k;</trd>
  <trd lng="sk">kalmuk</trd>
</drv>

    <drv mrk="kalmuk.0a">
      <kap><tld/>a</kap>
      <snc mrk="kalmuk.0a.ETN">
        <dif>
          Rilata al la <tld/>oj.
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_a;&c_l;&c_m;&c_y;&c_c;&c_k;&c_ib;</trd>
      <trd lng="bg">&c_k;&c_a;&c_l;&c_m;&c_i;&c_k;&c_s;&c_k;&c_i;</trd>
      <trd lng="de">kalm&uuml;ckisch</trd>
      <trd lng="en">kalmyk</trd>
      <trd lng="fr">kalmouk<klr tip="ind"> (adj.)</klr></trd>
      <trdgrp lng="it">
        <trd>calmucco<klr> (agg.)</klr></trd>,
        <trd>calmicco<klr> (agg.)</klr></trd>
      </trdgrp>
      <trd lng="nl">kalmuks</trd>
      <trd lng="pl">ka&lstroke;mucki</trd>
      <trd lng="pt">calmuco</trd>
      <trd lng="ru">&c_k;&c_a;&c_l;&c_m;&c_y;&c_c;&c_k;&c_i;&c_j;</trd>
    </drv>
  </art>

