<art mrk="$Id: kalend2.xml,v 1.22 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kalendul</rad>/o
</kap>
<drv mrk="kalend2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Calendula</trd>), kun oran&gcirc;flava floraro.
    </dif>
  </snc>
  <bld lok="../bld/kalendul.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="ca">boixac</trd>
    <trd lng="la">Calendula officinalis</trd>
  </bld>
  <trd lng="be">&c_n;&c_a;&c_g;&c_o;&c_t;&c_k;&c_a;</trd>          
  <trd lng="br">rozinilenn</trd>
  <trdgrp lng="ca">
    <trd>cal&egrave;ndula</trd>,
    <trd>boixac</trd>
  </trdgrp>
  <trd lng="de">Ringelblume</trd>
  <trd lng="en">marigold</trd>
  <trd lng="es">cal&eacute;ndula</trd>
  <trd lng="fa">&a_h;&a_m;&f_y;&a_sh;&a_h;&zwnj;&a_b;&a_h;&a_A;&a_r;</trd>
  <trd lng="fr">souci<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="hu">k&ouml;r&ouml;mvir&aacute;g</trd>
  <trd lng="nl">goudsbloem</trd>
  <trd lng="pt">cal&ecirc;ndula</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_g;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_je;&c_n;&c_d;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mesiačik</trd>
  <trd lng="cs">měsíček <klr>(rostl.)</klr></trd>
</drv>
</art>
