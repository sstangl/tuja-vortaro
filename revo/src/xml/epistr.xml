<vortaro>
<art mrk="$Id: epistr.xml,v 1.12 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>epistrof</rad>/o
</kap>
<drv mrk="epistr.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<snc>
<dif>
Nomo de la dua kola vertebro, havanta vertikalan apofizon k sur kiu
turni&gcirc;as kranio.  
</dif>
</snc>
  <trd lng="ca">epistrofeu</trd>
  <trdgrp lng="cs">
    <trd>axis</trd>,
    <trd>druhý krční obratel</trd>,
    <trd>epistrofeus</trd>,
    <trd>čepovec</trd>
  </trdgrp>
  <trd lng="de">Drehwirbel</trd>
  <trdgrp lng="hu">
  <trd>forg&oacute;csigolya</trd>,
  <trd>m&aacute;sodik <ind>nyakcsigolya</ind></trd>,
  <trd>epistropheus</trd></trdgrp>
  <trd lng="it">epistrofeo</trd>
  <trd lng="pl">kr&eogonek;g szyjny d&lstroke;ugi</trd>
  <trd lng="pt">epistrofeu</trd>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_o;&c_j; &c_sh;&c_je;&c_j;&c_n;&c_y;&c_j; &c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>axis</trd>,
    <trd>druhý krčný stavec</trd>
  </trdgrp>
</drv>
</art>
