<vortaro>
<art mrk="$Id: viktuali.xml,v 1.10 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>viktuali</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="viktuali.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Proviza&jcirc;o de <ref tip="super" cel="nutr.0ajxo">nutra&jcirc;oj</ref>
      rezerve tenata a&ubreve; kunportata:
      <ekz>
        ne tre valora <tld/>o por tiel brava kavaliro
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 10a, p.
        79a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_s; &c_je;&c_zh;&c_y;</trd>
  <trd lng="ca">vituallas</trd>
  <trdgrp lng="cs">
    <trd>potraviny</trd>,
    <trd>viktuálie</trd>,
    <trd>zásoby potravin</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proviant</trd>,
    <trd>Viktualie<klr tip="amb"> (veraltet)</klr></trd>
  </trdgrp>
  <trd lng="es">vituallas</trd>
  <trd lng="fa">&a_A_madda;&a_dh;&a_w;&a_q;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>provision de bouche</trd>,
    <trd>victuaille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;lelmiszerk&eacute;szlet</trd>,
    <trd>&uacute;traval&oacute;</trd>,
    <trd>elem&oacute;zsia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proviand</trd>,
    <trd>mondvoorraad</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>potraviny</trd>,
    <trd>viktuálie</trd>
  </trdgrp>
</drv>
</art>

