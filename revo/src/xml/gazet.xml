<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gazet.xml,v 1.35 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gazet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="gazet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gazet.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref cel="period.0a">Perioda</ref> <ref tip="super"
      cel="pres.0ajxo">presa&jcirc;o</ref> sciiganta 
      la <ref cel="aktual.0a">aktuala&jcirc;ojn</ref>:
      <ekz>
        duonmonata, semajna <tld/>o;
      </ekz>
      <ekz>
        mensogas kiel <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bulten.0o">bulteno</ref>,
      <ref cel="revu.0o">revuo</ref>
    </refgrp>
    <ref tip="super" cel="inform.0ilo">informilo</ref>
  </snc>
  <trd lng="ar">
  &a_j;&a_fatha;&a_r;&a_kasra_y;&a_d;&a_fatha;&a_t_marbuta;&a_dammatan; &a_j;
  &a_j;&a_fatha;&a_r;&a_fatha_A;&a_y_hamza;&a_kasra;&a_d;&a_damma;</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_je;
    &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_e;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_s;&c_o;&c_p;&c_ib;&c_s;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;asopis</trd>,
    <trd>list</trd>,
    <trd>věstník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zeitung</trd>,
    <trd>Zeitschrift</trd>
  </trdgrp>
  <trd lng="en">gazette</trd>
  <trd lng="es">revista</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_j;&a_l;&a_h;</trd>,
    <trd>&a_n;&a_sh;&a_r;&f_y;&a_h;</trd>,
    <trd>&a_r;&a_w;&a_z;&a_n;&a_A;&a_m;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gazette</trd>,
    <trd>journal</trd>,
    <trd>p&eacute;riodique<klr tip="ind"> (publication)</klr></trd>,
    <trd>revue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rlap</trd>,
    <trd>&uacute;js&aacute;g</trd>
  </trdgrp>
  <trd lng="id">majalah</trd>
  <trdgrp lng="nl">
    <trd>krant</trd>,
    <trd>tijdschrift</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gazeta</trd>,
    <trd>bibu&lstroke;a<klr> (nielegalna gazeta)</klr></trd>,
    <trd>szmat&lstroke;awiec<klr> (obra&zacute;liwie)</klr></trd>,
    <trd>dziennik</trd>,
    <trd>periodyk</trd>,
    <trd>magazyn</trd>,
    <trd>&zdot;urnal</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gazeta</trd>,
    <trd>folha<klr tip="ind"> (gazeta)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">časopis</trd>
  <trd lng="tr">gazete</trd>
</drv>

<drv mrk="gazet.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="gazet.0aro.INF">
    <dif>
      Tuto de la <tld/>oj en iu loko a&ubreve; lando:
      <ekz>
        la <tld/>aro amike akceptis la aperon de nia revuo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_a;
  <klr>(&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_y;&c_ja;
  &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>noviny</trd>,
    <trd>tisk</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Presse</trd>,
    <trd>Zeitungslandschaft</trd>
  </trdgrp>
  <trd lng="es">prensa</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_T;&a_b;&a_w;&a_ayn;&a_A;&a_t;</trd>,
    <trd>&a_j;&a_r;&a_A;&f_y;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">presse<klr tip="ind"> (les journaux)</klr></trd>
  <trd lng="hu">sajt&oacute;<klr tip="ind"> (az &uacute;js&aacute;gok)
  </klr></trd>
  <trd lng="nl">pers<klr tip="ind"> (de kranten)</klr></trd>
  <trd lng="pl">prasa</trd>
  <trd lng="pt">imprensa<klr tip="amb"> (os jornais)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>noviny</trd>,
    <trd>tlač</trd>
  </trdgrp>
  <trd lng="tr">bas&inodot;n</trd>
</drv>

<drv mrk="gazet.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="gazet.0isto.INF">
    <dif>
      <tld lit="G"/>a
      <ref tip="super" cel="jxurna.0isto">&jcirc;urnalisto</ref>:
      <ekz>
        &ccirc;iuj malbonaj poetoj kaj poetinoj, komponistoj,
        <tld/>istoj kaj konataj artistoj, &ccirc;iuj, kiuj estas tute
        senta&ubreve;gaj, rajdas en la Novjara nokto tra la aero
        al la insulo Amager
        <fnt><bib>Fab3</bib>, <lok>Turgardisto Ole</lok></fnt>;
      </ekz>
      <ekz>
        komentis kun beda&ubreve;ro <tld/>isto en unu el la
        noblaj &#8208; kaj sendependaj &#8208;
        diman&ccirc;o-&jcirc;urnaloj:
        britoj estas <ctl>kondamnitaj sen&ccirc;ese partoprenadi
        karikaturan version de la dua mondomilito</ctl>
        <fnt>
        <aut>P. Gubbins</aut>, <vrk>Komediistoj senkomediaj
        </vrk>
        <bib>Monato</bib>.
        </fnt>;
      </ekz>
      <ekz>
        la pianon oni &scirc;losis, la &jcirc;azbando
        disiris, kelkaj <tld/>istoj forveturis &ccirc;iu en
        sian redaktejon por verki nekrologon
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="cs">novinář</trd>
  <trdgrp lng="de">
    <trd>Journalist</trd>,
    <trd>Zeitungsschreiber</trd>
  </trdgrp>
  <trd lng="es">periodista</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&a_z;&a_n;&a_A;&a_m;&a_h;&zwnj;&a_n;&f_g;&a_A;&a_r;</trd>,
    <trd>&f_zh;&a_w;&a_r;&a_n;&a_A;&a_l;&f_y;&a_s;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">journaliste de presse</trd>
  <trdgrp lng="hu">
    <trd>&uacute;js&aacute;g&iacute;r&oacute;</trd>,
    <trd>h&iacute;rlap&iacute;r&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>redacteur</trd>,
    <trd>journalist</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziennikarz<klr> gazety</klr></trd>,
    <trd>redaktor<klr> gazety</klr></trd>,
    <trd>&zdot;urnalista</trd>,
    <trd>pismak <klr> (obra&zacute;liwie, potocznie)</klr></trd>
  </trdgrp>
  <trd lng="pt">jornalista</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">novinár</trd>
  <trd lng="tr">gazeteci</trd>
</drv>

<drv mrk="gazet.0istaro">
  <kap><tld/>istaro</kap>
  <snc mrk="gazet.0istaro.INF">
    <dif>
      Tuto de la <tld/>istoj en iu loko a&ubreve; lando.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_a; <klr>(&c_k;&c_o;&c_l;&c_a;
  &c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_a;&c_w;)</klr></trd>
  <trd lng="es">prensa</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&a_z;&a_n;&a_A;&a_m;&a_h;&zwnj;&a_n;&f_g;&a_A;&a_r;&a_A;&a_n;</trd>,
    <trd>&a_A;&a_r;&a_b;&a_A;&a_b; <ind>&a_j;&a_r;&a_A;&f_y;&a_d;</ind></trd>
  </trdgrp>
  <trd lng="fr">presse<klr tip="ind"> (les journalistes)</klr></trd>
  <trd lng="hu">sajt&oacute;<klr tip="ind">
  (az &uacute;js&aacute;g&iacute;r&oacute;k)</klr></trd>
  <trd lng="nl">pers<klr tip="ind"> (de journalisten)</klr></trd>
  <trd lng="pt">imprensa<klr tip="amb"> (os jornalistas)</klr></trd>
</drv>

<drv mrk="gazet.klacx0o">
  <kap>kla&ccirc;<tld/>o</kap>
  <snc mrk="gazet.klacx0o.INF">
    <dif>
      Gazeto entenanta precipe kla&ccirc;ojn, raportojn pri
      la&ubreve;dire skandalaj agoj de famuloj:
      <ekz>
        pri krueleco &mdash; kiel pretendas kelkaj kla&ccirc;<tld/>oj
        &mdash; tute ne temas
        <fnt>
          <aut>M. A. Clerk</aut>,
          <vrk><url
          ref="http://esperanto.org.uk/lbe/arkivo/953/08.html">
          El taglibro de najbarino</url></vrk>,
          <lok>La Brita Esperantisto, 2002, a&ubreve;tuno</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>journal &agrave; scandale</trd>,
    <trd>tablo&iuml;de</trd>
  </trdgrp>
</drv>

<drv mrk="gazet.mur0o">
  <kap>mur<tld/>o</kap>
  <snc mrk="gazet.mur0o.INF">
    <dif>
      <tld lit="G"/>o, kiun oni afi&scirc;as sur muro por publika
      legado:
      <ekz>
        anka&ubreve; &ccirc;ijarfine kaj venontjarkomence la Internacia
        Seminario havos mur<tld/>on kun la nomo
        <ctl>La Blinda Gardisto</ctl>
        <fnt>
        <aut>T. Sch&ouml;nberner, G. Fischer</aut>,
        <vrk><url
        ref="http://www.esperanto.de/is/bg/9899/is42salo.htm">Saluton
        karaj!</url></vrk>,
        <lok>1998-11-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nástěnka</trd>,
    <trd>nástěnné noviny</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_w;&a_z;&a_n;&a_A;&a_m;&a_h;&a_hamza_sure; &a_d;&f_y;&a_w;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">journal mural</trd>
  <trd lng="sk">nástenné noviny</trd>
</drv>

<drv mrk="gazet.tag0o">
  <kap>tag<tld/>o</kap>
  <snc mrk="gazet.tag0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      &Ccirc;iutaga <tld/>o:
      <ekz>
        la renoma sveda tag<tld/>o <ctl>Svenska Dagbladet</ctl>
        <fnt>
        <aut>B. Englund</aut>, <vrk>Benu modere Usonon!</vrk>
        <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        Pri la skandalo, kiu mondimprese ||
        La postan tagon sekvis kun furoro, ||
        Kaj pri l' pli postaj eksedzi&gcirc;o-pledoj, ||
        Vi legis en la anglaj tag<tld/>oj
        <fnt>
        <aut>Lord Byron, trad. W. Auld</aut>,
        <vrk>El la I-a kanto de &leftquot;Don Johano&rightquot;</vrk>,
        <lok><url ref=
"http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5702/johanou.
html">
        Norda Prismo, 57:2</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="jxurna.0o">&jcirc;urnalo</ref>
  <trd lng="de">Tageszeitung</trd>
  <trd lng="fa">&a_r;&a_w;&a_z;&a_n;&a_A;&a_m;&a_h;</trd>
  <trd lng="fr">quotidien<klr tip="amb"> (presse)</klr></trd>
  <trd lng="hu">napilap</trd>
  <trd lng="pl">dziennik</trd>
  <trd lng="pt">jornal</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_je;&c_t;&c_a;<klr>
  (&c_je;&c_zh;&c_je;&c_d;&c_n;&c_je;&c_v;&c_n;&c_a;&c_ja;)</klr></trd>
</drv>

<drv mrk="gazet.viva0o">
  <kap>viva <tld/>o</kap>
  <snc mrk="gazet.viva0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Improviza teatro, sceniganta aktuala&jcirc;ojn, plej ofte satire.
    </dif>
  </snc>
  <trd lng="es"><ind>revista</ind> teatral</trd>
  <trd lng="fa">&a_w;&a_A;&a_r;&f_y;&a_t;&a_h;<klr tip="amb"> (&a_n;&a_m;&a_A;&f_y;&a_sh;)</klr></trd>
  <trd lng="pt">teatro-revista</trd>
</drv>
</art>
