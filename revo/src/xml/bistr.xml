<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bistr.xml,v 1.12 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bistr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="bistr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Kolorilo nigrebruna, farita el fulgo.
    </dif>
  </snc>
  <trd lng="cs">bistro</trd>
  <trdgrp lng="en">
    <trd>brown wash</trd>,
    <trd>bistre</trd>
  </trdgrp>
  <trd lng="fr">bistre <klr tip="ind">(subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>biszter</trd>,
    <trd>barnalakk</trd>
  </trdgrp>
  <trd lng="pl">bistr <klr>(barwnik)</klr></trd>
  <trd lng="ru">&c_b;&c_i;&c_s;&c_t;&c_r; <klr>(&c_k;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_r;&c_a;&c_s;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">bister</trd>
</drv>

<drv mrk="bistr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bruna kiel <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">bistre <klr tip="ind">(adj.)</klr></trd>
  <trdgrp lng="en">
    <trd>bistre brown</trd>
  </trdgrp>
  <trd lng="hu">mang&aacute;nbarna</trd>
  <trdgrp lng="pl">
    <trd>barwy <ind>bistru</ind></trd>,
    <trd>ciemnobrunatny</trd>,
    <trd>ciemnobr&aogonek;zowy</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_jo;&c_m;&c_n;&c_o;-&c_k;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_je;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
