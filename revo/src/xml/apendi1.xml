<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apendi1.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>apendicit</rad>/o
</kap>
<drv mrk="apendi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de <ref cel="apendi.0o.ANA">apendico</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_n;&c_d;&c_y;&c_c;&c_y;&c_t;</trd>
  <trd lng="ca">apendicitis</trd>
  <trdgrp lng="cs">
    <trd>apendicitida</trd>,
    <trd>apendicitis</trd>,
    <trd>zánět slepého střeva</trd>,
    <trd>zánět červovitého přívěsku <klr>(slepého střeva)</klr></trd>
  </trdgrp>
  <trd lng="de">Blinddarmentz&uuml;ndung</trd>
  <trd lng="en">appendicitis</trd>
  <trd lng="es">apendicitis</trd>
  <trd lng="fr">appendicite</trd>
  <trd lng="hu">f&eacute;regny&uacute;lv&aacute;ny-gyullad&aacute;s</trd>
  <trdgrp lng="id">
    <trd>apendisitis</trd>,
    <trd><ind>radang</ind> usus buntu</trd>
  </trdgrp>
  <trd lng="pl">zapalenie wyrostka <klr>robaczkowego</klr></trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_n;&c_d;&c_i;&c_c;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>z&aacute;pal <ind>slep&eacute;ho &ccaron;reva</ind></trd>,
    <trd>zápal slepého čreva</trd>
  </trdgrp>
  <trd lng="sv">blindtarmsinflammation</trd>
</drv>
</art>
