<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dekt.xml,v 1.3 2007/07/19 16:31:27 revo Exp $">
<kap>
  <rad>dekt</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="dekt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="dekt.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el dek sinsekvaj tonoj de diatona gamo.
    </dif>
    <trdgrp lng="hu">
      <trd>tizedhangk&ouml;z</trd>,
      <trd>decima<klr> (hangk&ouml;z)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="dekt.0o.de.toniko">
    <dif>
      Deka tono de gamo.
    </dif>
    <trdgrp lng="hu">
      <trd>tizedik hang<klr tip="ind"> (hangsorban)</klr></trd>,
      <trd>decima<klr tip="ind"> (tizedik hang)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>dixi&egrave;me<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="pt">d&#233;cima</trd>
</drv>
</art>

