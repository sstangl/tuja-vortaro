<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rafan.xml,v 1.20 2016/04/11 19:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rafan</rad>/o
</kap>
<drv mrk="rafan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Raphanus</trd>), 
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herbo</ref>,
      kies dika tubera
      diverskolora radiko estas man&gcirc;ebla.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_d;&c_z;&c_mol;&c_k;&c_a;</trd>
  <trd lng="cs">&rcaron;edkev</trd>
  <trd lng="de">Rettich</trd>
  <trd lng="en">radish</trd>
  <trd lng="es">r&aacute;bano</trd>
  <trd lng="hu">retek</trd>
  <trd lng="nl">ramenas</trd>
  <trd lng="pl">rzodkiew</trd>
  <trd lng="pt">r&aacute;bano</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_mol;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>re&dcaron;kev</trd>,
    <trd>re&dcaron;kovka</trd>
  </trdgrp>
</drv>

<drv mrk="rafan.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Varia&jcirc;o de <tld/>o kun malgranda ekstere ru&gcirc;a radiko.
    </dif>
  </snc>
  <trd lng="cs">&rcaron;edkvi&ccaron;ka</trd>
  <trd lng="de">Radieschen</trd>
  <trd lng="fr">radis</trd>
  <trd lng="hu">h&oacute;napos <ind>retek</ind></trd>
  <trd lng="nl">radijs</trd>
  <trd lng="pl">rzodkiewka</trd>
  <trd lng="pt">rabanete</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_i;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>re&dcaron;kev</trd>,
    <trd>re&dcaron;kovka</trd>
  </trdgrp>
</drv>
</art>
