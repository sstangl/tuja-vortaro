<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: herman.xml,v 1.8 2015/10/23 14:10:21 revo Exp $">
<kap>
  <rad>Herman</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herman.0o">
  <kap><tld/>o</kap>
  <snc mrk="herman.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        la granda pupo de via fratino <klr>...</klr> aspektas kiel viro
        kaj estas nomata <tld/>o
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Vendredo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hermann</trd>
  <trd lng="en">Herman</trd>
  <trd lng="fr">Hermann</trd>
  <trd lng="hu">Herman</trd> 
   <trdgrp lng="it">
   <trd>Ermano</trd>, 
   <trd>Ermanno</trd> 
  </trdgrp>
  <trd lng="nl">Herman</trd>
  <trdgrp lng="pt">
   <trd>Hermano</trd>, 
   <trd>Germano</trd> 
  </trdgrp>
</drv>
</art>

