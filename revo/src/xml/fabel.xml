<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fabel.xml,v 1.28 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fabel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fabel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fabel.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Fantazia rakonto pri mirigaj aventuroj:
      <ekz>
        <tld/>oj de Andersen, de Perrault.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>kontadenn</trd>,
      <trd>mojenn<klr tip="ind"> (kontadenn)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ap&ograve;leg</trd>,
      <trd>par&agrave;bola</trd>
    </trdgrp>
    <trd lng="en">fairy tale</trd>
    <trdgrp lng="pl">
      <trd>bajka</trd>,
      <trd>ba&sacute;&nacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fabel.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mensoga rakonto:
      <ekz>
        anka&ubreve; tio estas <tld/>o... senhonta <tld/>o!... &mdash;
        ekkriis 
        Tutmozis
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>brozenn</trd>,
      <trd>bidenno&ugrave;<klr> [pl.]</klr></trd>,
      <trd>mojenn<klr tip="ind"> (gaou)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>invenci&oacute;</trd>,
      <trd>farsa</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>fairy tale</trd>,
      <trd>bedtime <ind>story</ind></trd>
    </trdgrp>
    <trd lng="pl">bajka</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="fabl.0o">fablo</ref>,
    <ref cel="legend.0o">legendo</ref>,
    <ref cel="rakont.mir0o">mirrakonto</ref>,
    <ref cel="mit.0o">mito</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="ca">faula</trd>
  <trd lng="cs">poh&aacute;dka</trd>
  <trdgrp lng="de">
    <trd>M&auml;rchen</trd>,
    <trd>Sage</trd>
  </trdgrp>
  <trd lng="es">f&aacute;bula</trd>
  <trdgrp lng="fr">
    <trd>conte</trd>,
    <trd>fable<klr tip="ind"> (l&eacute;gende, mensonge)</klr></trd>,
    <trd>l&eacute;gende</trd>
  </trdgrp>
  <trd lng="hu">mese</trd>
  <trd lng="id">dongeng</trd>
  <trd lng="nl">sprookje</trd>
  <trd lng="pt">f&aacute;bula</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="sv">saga</trd>
  <trd lng="sk">rozprávka</trd>
</drv>

<drv mrk="fabel.0a">
  <kap><tld/>a</kap>
  <snc mrk="fabel.0a.BELE">
    <dif>
      Apartenanta al <tld/>o:
      <ekz>
        <tld/>aj vortoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <tld/>a estulo.
      </ekz>
    </dif>
    <trd lng="br">mojennel</trd>
    <trd lng="en">fabulous</trd>
    <trd lng="hu">mesebeli</trd>
    <trdgrp lng="pl">
      <trd>bajkowy</trd>,
      <trd>ba&sacute;niowy</trd>
    </trdgrp>
    <trd lng="sv">sago-</trd>
  </snc>
  <snc mrk="fabel.0a.FIG">
    <dif>
      Eksterordinara, nekredebla:
      <ekz>
        <tld/>a ri&ccirc;eco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mirakl">mirakla</ref>,
      <ref cel="mir">miriga</ref>
    </refgrp>
    <trd lng="br">marzhus</trd>
    <trd lng="en">fabulous</trd>
    <trd lng="hu">mes&eacute;s</trd>
    <trd lng="pl">bajeczny</trd>
    <trd lng="sv">sagolik</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_je;&c_n;</trd>
  <trd lng="ca">fabul&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>báječný <klr>(skvělý)</klr></trd>,
    <trd>pohádkový</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&auml;rchenhaft</trd>,
    <trd>sagenhaft</trd>
  </trdgrp>
  <trd lng="es">fabuloso</trd>
  <trdgrp lng="fr">
    <trd>fabuleux</trd>,
    <trd>l&eacute;gendaire</trd>
  </trdgrp>
  <trd lng="nl">fabelachtig</trd>
  <trd
  lng="ru">&c_s;&c_k;&c_a;&c_z;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">rozprávkový</trd>
</drv>

<drv mrk="fabel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="fabel.0aro.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="super" cel="kolekt.0o.libr">Kolekto</ref> de <tld/>oj:
      <ekz>
        &ccirc;i tiu libro donas al ni entute 19 ekzemplerojn el la
        <tld/>aro 
        de Miyazawa
        <fnt>
          <aut>Don Harlow</aut>,
          <vrk><url ref="http://www.esperanto-usa.org/en/node/826">
          Recenzo: Goo&scirc; la '&ccirc;elisto</url></vrk>,
          <lok>2007-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">recull de faules</trd>
  <trdgrp lng="cs">
    <trd>kniha pohádek</trd>,
    <trd>sbírka pohádek</trd>
  </trdgrp>
  <trd lng="de">M&auml;rchensammlung</trd>
  <trd lng="fr">recueil de contes</trd>
  <trd lng="sk">zbierka rozprávok</trd>
</drv>
</art>
