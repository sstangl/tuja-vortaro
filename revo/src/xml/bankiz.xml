<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bankiz.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bankiz</rad>/o
</kap>
<drv mrk="bankiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Tre vasta tavolego da glacio en polusaj maroj.
    </dif>
    <trd lng="ca">banquisa</trd>
    <trdgrp lng="de">
      <trd>Packeis</trd>,
      <trd>Packeisfeld</trd>,
      <trd>Schelfeis</trd>
    </trdgrp>
    <trd lng="en">ice cap</trd>
    <trd lng="fr">banquise</trd>
    <trd lng="hu">j&eacute;gmez&odblac;</trd>
    <trd lng="pl">pokrywa lodowa</trd>
    <trdgrp lng="ru">
       <trd>&c_t;&c_o;&c_r;&c_o;&c_s;</trd>,
       <trd>&c_p;&c_r;&c_i;&c_p;&c_a;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ledová kra</trd>,
    <trd>polární ledovec</trd>
  </trdgrp>
  <trd lng="sk">ľadová vrstva <klr>(na póle)</klr></trd>
</drv>
</art>
