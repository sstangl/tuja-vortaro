<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: laplac.xml,v 1.12 2015/07/04 08:37:38 revo Exp $">
<kap>
<rad>Laplac</rad>/o
</kap>
<drv mrk="laplac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Franclingve: Pierre Simon de <trd lng="fr">Laplace</trd>,
      1749-1827. 
      Franca matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_a;&c_p;&c_l;&c_a;&c_s;</trd>
  <trd lng="bg">&c_L;&c_a;&c_p;&c_l;&c_a;&c_s;</trd>
  <trd lng="ca">Laplace</trd>
  <trd lng="cs">Laplace Pierre Simon de</trd>
  <trd lng="de">Laplace</trd>
  <trd lng="en">Laplace</trd>
  <trd lng="es">Laplace</trd>
  <trd lng="hu">Laplace</trd>
  <trd lng="it">Laplace</trd>
  <trd lng="nl">Pierre Simon de <ind>Laplace</ind></trd>
  <trd lng="pl">Laplace</trd>
  <trd lng="pt">Laplace</trd>
  <trd lng="ru">&c_L;&c_a;&c_p;&c_l;&c_a;&c_s;</trd>
  <trd lng="sk">Laplace Pierre Simon de</trd>
</drv>

<drv mrk="laplac.l0a">
  <kap><tld lit="l"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="operat.laplaca0o"><tld lit="l"/>a operatoro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_L;&c_a;&c_p;&c_l;&c_a;&c_s;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_l;&c_a;&c_s;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">laplaci&agrave;</trd>
  <trd lng="de">Laplacesch</trd>
  <trd lng="en">Laplace['s]</trd>
  <trd lng="es">de Laplace</trd>
  <trdgrp lng="fr">
    <trd>de <ind>Laplace</ind></trd>,
    <trd>laplacien<klr tip="ind"> (adj.)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>Laplace-</trd>,
    <trd>Laplace-f&eacute;le</trd>
  </trdgrp> 
  <trd lng="it">de <ind>Laplace</ind></trd>
  <trd lng="nl">laplace-</trd>
  <trd lng="pl">Laplace'a</trd>
  <trd lng="ru">&c_L;&c_a;&c_p;&c_l;&c_a;&c_s;&c_a;</trd>
</drv>
</art>

