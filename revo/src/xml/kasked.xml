<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kasked.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kasked</rad>/o
</kap>
<drv mrk="kasked.0o">
  <kap><tld/>o</kap>
  <snc mrk="kasked.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="cxap.0o">&Ccirc;apo</ref> kun viziero:
      <ekz>
        li &jcirc;etis la <tld/>on kun la gantoj sur tablon
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_r;&c_a;&c_zh;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kašket</trd>,
    <trd>čepice se štítkem</trd>
  </trdgrp>
  <trd lng="de">Schirmm&uuml;tze</trd>
  <trd lng="en">cap (with visor)</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_kasra;&f_p;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_s;&f_k;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">casquette</trd>
  <trd lng="hu">ellenz&odblac;s <ind>sapka</ind></trd>
  <trd lng="nl">pet<klr tip="ind"> (met klep)</klr></trd>
  <trd lng="ru">&c_f;&c_u;&c_r;&c_a;&c_zh;&c_k;&c_a;</trd>
  <trd lng="sk">čiapka <klr>(s tienidlom)</klr></trd>
</drv>
</art>
