<vortaro>
<art mrk="$Id: sebastian.xml,v 1.7 2015/02/26 19:10:57 revo Exp $">
<kap>
  <rad>Sebastian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sebastian.0o">
  <kap><tld/>o</kap>
  <snc mrk="sebastian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        la portugalo klarigis, ke ne, ke la nomo de <klr>la</klr> 
        nigruleto estas <tld/>o
        <fnt>
          <aut>S. Ponte Preta, trad. P. S. Viana</aut>,
          <vrk><url
          ref="http://www.kke.org.br/eo/literatura/la_miraklo">
          La Miraklo</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_S;&c_je;&c_v;&c_a;&c_s;&c_t;&c_i;&c_a;&c_n;</trd>
    <trd lng="de">Sebastian</trd>
    <trd lng="en">Sebastian</trd>
    <trd lng="fr">S&eacute;bastien</trd>
    <trd lng="nl">Sebastiaan</trd>
    <trd lng="no">Sebastian</trd>
    <trd lng="pl">Sebastian</trd>
    <trd lng="pt">Sebasti&atilde;o</trd>
  </snc>
</drv>
</art>

