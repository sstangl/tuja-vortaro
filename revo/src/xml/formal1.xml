<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: formal1.xml,v 1.16 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>formaldehid</rad>/o
</kap>
<drv mrk="formal1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      H<sub>2</sub>CO.
      Organika kombina&jcirc;o, senkolora gaso kun neagrabla incitanta odoro:
      <ekz><tld/>o estas forta desinfektilo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_d;&c_e;&c_g;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_d;&c_je;&c_h;&c_i;&c_d;</trd>
  <trd lng="ca">formaldehid</trd>
  <trdgrp lng="cs">
    <trd>formaldehyd</trd>,
    <trd>metanal</trd>,
    <trd>metanol</trd>,
    <trd>mravenčí aldehyd</trd>
  </trdgrp>
  <trd lng="de">Formaldehyd</trd>
  <trd lng="es">formaldeh&iacute;do</trd>
  <trd lng="fr">formald&eacute;hyde</trd>
  <trd lng="hu">formaldehid</trd>
  <trd lng="nl">formaldehyde</trd>
  <trd lng="pl">formaldehyd</trd>
  <trdgrp lng="pt">
    <trd>alde&iacute;do f&oacute;rmico</trd>,
    <trd>formalde&iacute;do</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_d;&c_je;&c_g;&c_i;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>formaldehyd</trd>,
    <trd>metanal</trd>
  </trdgrp>
  <trd lng="sv">formaldehyd</trd>
</drv>
</art>
