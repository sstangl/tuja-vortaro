<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: soft.xml,v 1.13 2015/09/22 10:10:26 revo Exp $">
<kap>
  <rad>soft</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="soft.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="soft.0a.SON">
    <dif>
      Milde <ref tip="super" cel="lauxt.mal0a">malla&ubreve;ta</ref>:
      <ekz>
        mia vo&ccirc;o fari&gcirc;as <tld/>a kaj diskreta
        <fnt><bib>Kiso</bib>, <lok>G&auml;rda</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>,
    <trd>&c_m;&c_ja;&c_k;&c_k;&c_ib; <klr tip="amb">(&c_g;&c_u;&c_k;,
&c_g;&c_o;&c_l;&c_a;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jemn&yacute;</trd>,
    <trd>m&ecaron;kk&yacute;</trd>,
    <trd>slab&yacute;</trd>,
    <trd>tichounk&yacute;</trd>
  </trdgrp>
  <trd lng="en">quiet</trd>
  <trd lng="es">bajo<klr> (sonido)</klr></trd>
  <trd lng="fr">doux<klr> (son)</klr></trd>
  <trd lng="hu">l&aacute;gy hang&uacute;</trd>
  <trd lng="nl">zacht<klr tip="ind"> (van geluid)</klr></trd>
  <trdgrp lng="pl">
    <trd>mi&eogonek;kki <klr>(o d&zacute;wi&eogonek;ku)</klr></trd>,
    <trd>delikatny <klr>(o d&zacute;wi&eogonek;ku)</klr></trd>,
    <trd>cichy</trd>,
    <trd>ledwie <ind>s&lstroke;yszalny</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_l;&c_u;&c_sh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;<klr tip="amb">
(&c_z;&c_v;&c_u;&c_k;, &c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mierny</trd>,
    <trd>m&auml;kk&yacute;</trd>,
    <trd>ne&zcaron;n&yacute;</trd>,
    <trd>tich&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="soft.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="soft.0i.SON">
    <dif>
      <ref tip="super" cel="son.0i.SON">Soni</ref> malla&ubreve;te, karese:
      <ekz>
        &leftquot;tio, kaj amuza kaj kortu&scirc;a&rightquot; <tld/>is
&scirc;ia vo&ccirc;o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 58a, volumo 2a, p.
        289a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">to sound softly</trd>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_h;&c_a; &c_g;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_a; &c_g;&c_a;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>doux</ind><klr> (son)</klr></trd>
  </trdgrp>
  <trd lng="hu">l&aacute;gy hangon sz&oacute;l</trd>
  <trd lng="nl">zacht zijn<klr tip="ind"> (van stem)</klr></trd>
  <trdgrp lng="pl">
    <trd>brzemi&cacute; <ind>mi&eogonek;kko</ind> <klr>(o d&zacute;wi&eogonek;ku)</klr></trd>,
    <trd>brzmie&cacute; <ind>delikatnie</ind> <klr>(o d&zacute;wi&eogonek;ku)</klr></trd>,
    <trd>by&cacute; <ind>cichym</ind></trd>,
    <trd>by&cacute; ledwie <ind>s&lstroke;yszalnym</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_u;&c_ch;&c_a;&c_t;&c_mol; &c_t;&c_i;&c_h;&c_o;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol; &c_t;&c_i;&c_h;&c_o;</trd>
  </trdgrp>
</drv>
</art>
