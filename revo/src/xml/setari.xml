<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: setari.xml,v 1.8 2015/11/20 06:10:19 revo Exp $">
<kap>
  <rad>setari</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="setari.0o">
  <kap><tld/>o</kap>
  <snc mrk="setari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Neda&ubreve;ra herbo kun rigidaj haroj, <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="poac.0oj.BOT">poacoj</ref>
      (<trd lng="la">Setaria</trd>):
      <ekz>
        &ccirc;iutage &scirc;i man&gcirc;is riza&jcirc;on kun <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;iun tagon por mortigi
        tiun memoron&rightquot;, p. 236a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">b&eacute;r siv&yacute;</trd>
  <trd lng="de">Borstenhirse</trd>
  <trdgrp lng="en">
    <trd>Setaria</trd>,
    <trd>bristlegrass</trd>, <trd>foxtail millet</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>millet<klr tip="amb"> (&agrave; grappes, d'italie...)</klr></trd>,
    <trd>s&eacute;taire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>muhar</trd>,
    <trd>mohar</trd>
  </trdgrp>
  <trd lng="it">setaria</trd>
  <trd lng="pl">w&lstroke;o&sacute;nica <klr>(ro&sacute;lina)</klr></trd>
  <trd lng="sk">moh&aacute;r siv&yacute;</trd>
</drv>
</art>

