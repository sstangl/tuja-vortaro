<vortaro>
<art mrk="$Id: preska.xml,v 1.21 2015/03/17 07:10:16 revo Exp $">
<kap><ofc>*</ofc> <rad>preska&ubreve;</rad></kap>
<drv mrk="preska.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
       Adv. signifanta &leftquot;ne tute, sed proksimume&rightquot;:
       <ekz>
         li estas <tld/> mia frato: nepo de onklo de kuzo de
         konato<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/> &ccirc;iuj;
       </ekz>
       <ekz>
         li estis <tld/> dis&scirc;irita de la popolo;
       </ekz>
       <ekz>
         ili estas <tld/> sama&gcirc;aj;
       </ekz>
       <ekz>
         estis <tld/> nokto.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="rand.0o">rando</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_a;&c_m;&c_a;&c_l;&c_mol;</trd>
   <trdgrp lng="ca">
     <trd>quasi</trd>,
     <trd>gaireb&eacute;</trd>
   </trdgrp>
   <trdgrp lng="de">
      <trd>fast</trd>,
      <trd>beinahe</trd>,
      <trd>nahezu</trd>
   </trdgrp>
   <trd lng="en">almost</trd>
   <trd lng="es">casi</trd>
   <trdgrp lng="fi">
     <trd>melkein</trd>,
     <trd>l&auml;hes</trd>,
     <trd>miltei</trd>
   </trdgrp>
   <trd lng="fr">presque</trd>
   <trd lng="he">&kaf;&mem;&ayin;&tet;</trd>
   <trdgrp lng="hu">
     <trd>majdnem</trd>,
     <trd>szinte</trd>
   </trdgrp>
   <trd lng="id">hampir</trd>
   <trd lng="nl">bijna</trd>
   <trd lng="oc">quasi, gaireben</trd>
   <trdgrp lng="pl">
      <trd>bez ma&lstroke;a</trd>,
      <trd>niemal</trd>,
      <trd>prawie</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>quase</trd>,
     <trd>perto de</trd>,
     <trd>por <ind>pouco</ind></trd>
   </trdgrp>
   <trd lng="ro">de <ind>aproape</ind></trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_ch;&c_t;&c_i;</trd>,
     <trd>&c_p;&c_o;&c_ch;&c_t;&c_i; &c_ch;&c_t;&c_o;</trd>,
     <trd>&c_ch;&c_u;&c_t;&c_mol; &c_n;&c_je;</trd>
   </trdgrp>
   <trd lng="sv">n&auml;stan</trd>
   <trd lng="vo">ti</trd>
</drv>
</art>
