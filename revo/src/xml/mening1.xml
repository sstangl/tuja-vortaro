  </snc>
  <trd lng="be">&c_m;&c_e;&c_n;&c_ib;&c_n;&c_g;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_m;&c_je;&c_n;&c_i;&c_n;&c_g;&c_i;&c_t;</trd>
