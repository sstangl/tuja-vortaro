  <trd lng="es">meningitis</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_n;&a_n;&f_zh;&f_y;&a_t;</trd>,
    <trd>&a_A;&a_l;&a_t;&a_h;&a_A;&a_b; &f_p;&a_r;&a_d;&a_h;&zwnj;&a_h;&a_A;&f_y; &a_m;&a_gh;&a_z;</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;ningite</trd>
  <trdgrp lng="hu">
      <trd>agyh&aacute;rtyagyullad&aacute;s</trd>,
      <trd>meningitis</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>meningitis</trd>,
    <trd><ind>radang</ind> selaput otak</trd>
  </trdgrp>
  <trd lng="nl">hersenvliesontsteking</trd>
  <trd lng="pt">meningite</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_i;&c_n;&c_g;&c_i;&c_t;</trd>
</drv>
