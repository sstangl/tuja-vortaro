<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: likv.xml,v 1.23 2015/07/04 08:37:38 revo Exp $">
    <kap>
      <ofc>8</ofc>
      <rad>likv</rad>/o
    </kap>

    <drv mrk="likv.0o">
      <kap><tld/>o</kap>
      <snc mrk="likv.0o.FIZ">
        <uzo tip="fak">FIZ</uzo>
        <dif>
          <ref tip="super" cel="flu.0ajxo">Flua&jcirc;o</ref>, substanco
          kapabla (simile al <ref tip="vid" cel="gas.0o">gaso</ref>)
          flui kaj akcepti la formon de ujo &gcirc;in entenanta, sed
          kies
          volumeno tre malmulte dependas je temperaturo a&ubreve; premo
          (kiel &ccirc;e la <ref tip="vid"
          cel="solid.0o.FIZ">solidoj</ref>):
          <ekz>
            kvanton de <tld/>o (akvo, lakto, oleo, vino) oni povas
            esprimi en <ref cel="litr.0o">litroj</ref>.
          </ekz>
        </dif>
      </snc>
  <trd
      lng="be">&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">l&iacute;quid</trd>
  <trdgrp lng="cs">
    <trd>kapalná látka</trd>,
    <trd>tekutina</trd>
  </trdgrp>
  <trd lng="de">Fl&uuml;ssigkeit</trd>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron_ton;</trd>
  <trd lng="en">liquid</trd>
  <trd lng="es">l&iacute;quido</trd>
  <trd lng="fr">liquide<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">folyad&eacute;k</trd>
  <trd lng="nl">vloeistof</trd>
  <trd lng="pl">ciecz</trd>
  <trdgrp lng="pt">
        <trd>l&iacute;quido <klr>(subst.)</klr></trd>,
        <trd>l&iacute;q&uuml;&iacute;do <klr>(subst.)</klr></trd>
      </trdgrp>
  <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kvapalina</trd>,
    <trd>tekutina</trd>
  </trdgrp>
  <trd lng="tp">telo</trd>
</drv>

    <drv mrk="likv.0a">
      <kap><tld/>a</kap>
      <snc mrk="likv.0a.FIZ">
        <uzo tip="fak">FIZ</uzo>
        <dif>
          Rilata al <tld/>o, kiu estas en la stato de <tld/>o:
          <ekz>
            <tld/>a azoto (<frm>-195,79&ring;C</frm>)
            <ctl>brulvundas</ctl> la
            histojn kiel varmego.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_k;&c_ib;</trd>
  <trd lng="ca">l&iacute;quid</trd>
  <trdgrp lng="cs">
    <trd>kapalný</trd>,
    <trd>tekutý</trd>
  </trdgrp>
  <trd lng="de">fl&uuml;ssig</trd>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">liquid</trd>
  <trd lng="es">l&iacute;quido</trd>
  <trd lng="fr">liquide<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
        <trd>foly&eacute;kony</trd>,
        <trd>cseppfoly&oacute;s</trd>
      </trdgrp>
  <trd lng="nl">vloeibaar</trd>
  <trd lng="pl">ciek&lstroke;y</trd>
  <trdgrp lng="pt">
        <trd>l&iacute;quido <klr>(adj.)</klr></trd>,
        <trd>l&iacute;q&uuml;ido <klr>(adj.)</klr></trd>
      </trdgrp>
  <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kvapalný</trd>,
    <trd>tekutý</trd>
  </trdgrp>
</drv>

    <drv mrk="likv.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="likv.0ajxo.likvo">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="likv.0o.FIZ">likvo</ref>

        <trd

        lng="be">&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
        <trd lng="ca">l&iacute;quid</trd>
        <trd lng="de">Fl&uuml;ssigkeit</trd>
        <trd lng="el">&ypsilon;&gamma;&rho;&omikron_ton;</trd>
        <trd lng="en">liquid</trd>
        <trd lng="es">l&iacute;quido</trd>
        <trd lng="fr">liquide<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="hu">folyad&eacute;k</trd>
        <trd lng="nl">vloeistof</trd>
        <trd lng="pl">ciecz</trd>
        <trdgrp lng="pt">
          <trd>l&iacute;quido <klr>(subst.)</klr></trd>,
          <trd>l&iacute;q&uuml;&iacute;do <klr>(subst.)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
        <trd lng="tp">telo</trd>
      </snc>
    </drv>

    <drv mrk="likv.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="likv.0igi.FIZ">
        <dif>
          Meti en <tld/>an staton:
        </dif>
        <refgrp tip="sin">
          <ref cel="fand.0i.likvigi">fandi</ref>,
          kondensi
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_w;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;
        &c_u;
        <ind>&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
        <trd>&c_z;&c_v;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trd lng="ca">liquar</trd>
  <trd lng="cs">zkapalňovat</trd>
  <trd lng="de">verfl&uuml;ssigen</trd>
  <trdgrp lng="el">
        <trd>&tau;&eta_ton;&kappa;&omega;</trd>,
        <trd>&ypsilon;&gamma;&rho;&omikron;&pi;&omikron;&jota;&omega_ton;</trd>
      </trdgrp>
  <trdgrp lng="en"><trd>liquefy</trd>, <trd>liquify</trd></trdgrp>
  <trd lng="es">licuar</trd>
  <trd lng="fr">liqu&eacute;fier</trd>
  <trdgrp lng="hu">
        <trd>cseppfoly&oacute;s&iacute;t</trd>,
        <trd>elfoly&oacute;s&iacute;t</trd>
      </trdgrp>
  <trd lng="nl">vloeibaar maken</trd>
  <trdgrp lng="pl">
        <trd>skrapla&cacute;</trd>,
        <trd>topi&cacute;</trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>l&iacute;equefazer</trd>,
        <trd>l&iacute;q&uuml;&iacute;efazer</trd>
      </trdgrp>
  <trd lng="ru">&c_s;&c_zh;&c_i;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>skvapalniť</trd>,
    <trd>urobiť tekutým</trd>
  </trdgrp>
</drv>
  </art>
