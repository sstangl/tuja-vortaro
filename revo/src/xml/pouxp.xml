<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pouxp.xml,v 1.16 2015/12/10 02:10:17 revo Exp $">
<kap>
  <rad>po&ubreve;p</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pouxp.0o">
  <kap><tld/>o</kap>
  <snc mrk="pouxp.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <ref tip="dif" cel="pob.0o.MAR">pobo</ref>
    <ekz>
      tiam la ondoj venis kontra&ubreve; nian <tld/>on
      <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
      <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
    </ekz>
    <ref tip="malprt" cel="sxip.0o.MAR">&scirc;ipo</ref>
    <ref tip="ant" cel="pru.0o">pruo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_m;&c_a;
  <klr>(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>
  <trd lng="ca">popa</trd>
  <trd lng="de">Achternschiff</trd>
  <trdgrp lng="en">
    <trd>stern</trd>,
    <trd>poop</trd>
  </trdgrp>
  <trd lng="es">popa</trd>
  <trd lng="fr">poupe</trd>
  <trdgrp lng="hu">
    <trd>tat</trd>,
    <trd>haj&oacute;far</trd>
  </trdgrp>
  <trd lng="nl">achtersteven</trd>
  <trd lng="pt">popa</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_a;</trd>
</drv>
</art>
