<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: turnir.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>turnir</rad>/o
</kap>
<drv mrk="turnir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e la mezepoko, lukto de kavaliroj en publika festa
      vetbatalo.
    </dif>
    <trd lng="ca">justa</trd>
    <trdgrp lng="en">
      <trd>tournament</trd>
      <trd>tourney</trd>
    </trdgrp>
    <trd lng="es">torneo</trd>
    <trd lng="hu">lovagi <ind>torna</ind></trd>
    <trd lng="pl">turniej</trd>
    <trd lng="pt">torneio</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Granda publika <ref tip="super" cel="konkur2.0o">konkurso</ref>:
      <ekz>
        la trubaduraj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>&scirc;ak<tld/></ind>o.
        <trd lng="ca">torneig d'escacs</trd>
        <trd lng="es"><ind>campeonato</ind> de ajedrez</trd>
        <trd lng="hu">sakktorna</trd>
        <trd lng="pl">turniej szachowy</trd>
        <trd lng="pt"><ind>campeonato</ind> de xadrez</trd>
      </ekz>
    </dif>
    <trd lng="ca">competici&oacute;</trd>
    <trd lng="en">tournament</trd>
    <trdgrp lng="es">
      <trd>competici&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>torna</trd>,
      <trd>verseny</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>turniej</trd>,
      <trd>liga</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>torneio</trd>,
      <trd>campeonato</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_n;&c_ib;&c_r;</trd>
  <trd lng="ca">torneig</trd>
  <trd lng="cs">rytířský turnaj</trd>
  <trd lng="de">Turnier</trd>
  <trd lng="es">torneo</trd>
  <trd lng="fr">tournoi</trd>
  <trd lng="nl">toernooi</trd>
  <trdgrp lng="pt">
   <trd>torneio</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_i;&c_r;</trd>
  <trd lng="sk">rytiersky turnaj</trd>
  <trd lng="sv">turnering</trd>
</drv>
</art>
