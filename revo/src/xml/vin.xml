<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vin.xml,v 1.39 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Alkohola trinka&jcirc;o, ekstraktita el fermentintaj vinberoj:
      <ekz>
        mi ne volis trinki la <tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        blanka, ru&gcirc;a, juna<fnt>Z</fnt> vino;
      </ekz>
      <ekz>
        la vino gajigas la koron de la homo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="most.0o">mosto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_o;</trd>
  <trd lng="bg">&c_v;&c_i;&c_n;&c_o;</trd>
  <trd lng="ca">vi</trd>
  <trd lng="cs">v&iacute;no</trd>
  <trd lng="de">Wein</trd>
  <trd lng="el">&kappa;&rho;&alfa;&sigma;&jota_ton;</trd>
  <trd lng="en">wine</trd>
  <trd lng="es">vino</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_r;&a_A;&a_b;</trd>,
    <trd>&a_m;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">vin</trd>
  <trd lng="he">&yod;&yod;&fnun;</trd>
  <trd lng="hu">bor</trd>
  <trd lng="hr">vino</trd>
  <trd lng="nl">wijn</trd>
  <trd lng="pl">wino</trd>
  <trd lng="pt">vinho</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;</trd>
  <trd lng="sk">v&iacute;no</trd>
  <trd lng="sv">vin</trd>
  <trd lng="tr">&scedil;arap</trd>
  <trd lng="zh">酒 [<ind>jiǔ</ind>]</trd>
</drv>

<drv mrk="vin.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="vin.0isto.KOMUNE">
    <dif>
      Homo kies metio estas fari a&ubreve; doni <tld/>on:
      <ekz>
        post tiuj okazinta&jcirc;oj la <tld/>isto de la re&gcirc;o de Egiptujo
        kaj la bakisto kulpi&gcirc;is anta&ubreve; sia sinjoro
        <fnt><bib>MT</bib>, <lok>&Gen; 40:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">vinater</trd>
  <trd lng="cs">číšník ve vinárně</trd>
  <trd lng="en">vintner</trd>
  <trd lng="fa">&a_m;&a_w;&f_k;&a_A;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>sommelier</trd>,
    <trd>viticulteur</trd>
  </trdgrp>
  <trd lng="hu">bor&aacute;sz</trd>
  <trdgrp lng="pt">
    <trd>vinhateiro</trd>,
    <trd>viticultor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_o;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_a;&c_r;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_ch;&c_je;&c_r;&c_p;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">čašník vo vinárni</trd>
</drv>

<drv mrk="vin.palmo0o">
  <kap>palmo<tld/>o, <var><kap>palm<tld/>o</kap></var></kap>
  <snc mrk="vin.palmo0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Alkohola trinka&jcirc;o farita el fermentinta suko de kokosarbo 
a&ubreve; 
      alia palmo:
      <ekz>
        plenkarafo da fermentinta palmo<tld/>o
        <fnt><aut>J. Ribillard</aut>, <vrk>Vagado sub palmoj</vrk>, 
        <lok>UEA, 1956</lok></fnt>;
      </ekz>
      <ekz>
        post dek ok kilometroj ni alvenas &ccirc;e Daya, vila&gcirc;eto kie 
&ccirc;iu 
        ebrias pro palmo<tld/>o
        <fnt>
          <aut>E. Kokmeijer</aut>,
          <vrk><url
          ref="http://www.estherkokmeijer.nl/resumo-de-la-mondo-la-vojago">
          Resumo de la mondo</url></vrk>,
          <lok>[2008?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca"><ind>vi</ind> de palma</trd>
  <trd lng="fr">vin de palme</trd>
</drv>

<drv mrk="vin.sxauxm0o">
  <kap>&scirc;a&ubreve;m<tld/>o</kap>
  <snc mrk="vin.sxauxm0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="vin.0o.KUI"><tld lit="V"/>o</ref> entenanta
      karbondioksidon, kiu aperas kiel &scirc;a&ubreve;mo sur la 
trinka&jcirc;o:
      <ekz>
        mi proponis provizi A&ubreve;stralian &scirc;a&ubreve;m<tld/>on por la 
festo
        <fnt>
        <aut>R. Harry</aut>,
        <vrk><url ref="http://www.luin.se/inko/X24-6.pdf">La Diplomato
        Kiu Ridis</url></vrk>,
        <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca"><ind>vi</ind> escum&oacute;s</trd>
  <trd lng="fa">&a_sh;&a_r;&a_A;&a_b; &f_g;&a_A;&a_z;&a_d;&a_A;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>mousseux<klr> (vin)</klr></trd>,
    <trd>vin mousseux</trd>
  </trdgrp>
  <trd lng="hu">habz&oacute;bor</trd>

</drv>

<drv mrk="vin.0berarbo0berujo">
  <kap><tld/>berarbo, <var><kap><tld/>berujo</kap></var></kap>
  <snc mrk="vin.0berarbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="vitac.0oj">vitacoj</ref>
      (<trd lng="la">Vitis</trd>), 
      <ref tip="lst" cel="arb.0eto" lst="voko:vitoparencoj">arbeto</ref>
      produktanta <tld/>berojn.
    </dif>
     <ref tip="sin" cel="vit.0o">vito</ref>
  </snc>
  <trd lng="ca">vinya</trd>
  <trdgrp lng="cs">
    <trd>kmen révy</trd>,
    <trd>vinná réva</trd>
  </trdgrp>
  <trd lng="de">Weinrebe</trd>
  <trd 
lng="el">&kappa;&lambda;&eta;&my;&alfa;&tau;&alfa;&rho;&jota;&alfa_ton;</trd>
  <trd lng="en">grapevine</trd>
  <trdgrp lng="fa">
    <trd><klr tip="amb">(&a_d;&a_r;&a_kh;&a_t;) </klr>&a_m;&a_w;</trd>,
    <trd>&a_t;&a_A;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">cep de vigne</trd>
  <trd lng="he">&gimel;&pe;&fnun;</trd>
  <trd lng="hu">sz&odblac;l&odblac;t&odblac;ke</trd>
  <trd lng="nl">wijnstok</trd>
  <trd lng="pt">parreira</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>vini&ccaron;</trd>,
    <trd>vinič hroznorodý</trd>
  </trdgrp>
  <trd lng="tr">asma</trd>
</drv>

<drv mrk="vin.0bergxardeno">
  <kap><tld/>ber&gcirc;ardeno</kap>
  <snc mrk="vin.0bergxardeno.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Loko plantita per <tld/>berujoj:
      <ekz>
        Noa plantis <tld/>ber&gcirc;ardenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">vinyar</trd>
  <trd lng="de">Weinberg</trd>
  <trd lng="en">vineyard</trd>
  <trd lng="fa">&a_t;&a_A;&f_k;&a_s;&a_t;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>vigne</trd>,
    <trd>vignoble</trd>
  </trdgrp>
  <trd lng="he">&kaf;&resh;&fmem;</trd>
  <trd lng="hu">sz&odblac;l&odblac;skert</trd>
  <trd lng="nl">wijngaard</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">vinohrad</trd>
</drv>
</art>
