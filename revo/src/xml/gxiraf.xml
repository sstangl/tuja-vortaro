<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxiraf.xml,v 1.29 2014/05/30 08:10:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;iraf</rad>/o
</kap>

<drv mrk="gxiraf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="gxiraf1.0oj"><tld/>edoj</ref>
      (<trd lng="la"><ind>Giraffa</ind> camelopardalis</trd>), vivanta
      sude 
      de Saharo, kun makulita felo kaj tre longaj kruroj kaj kolo.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_r;&c_a;&c_f;</trd>
  <trd lng="bg">&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>
  <trd lng="br">jirafenn</trd>
  <trd lng="de">Giraffe</trd>
  <trd lng="el">
&kappa;&alfa;&my;&ypsilon;&lambda;&omikron;&pi;&alfa_ton;&rho;&delta;&alfa;&lambda;&eta;
</trd>
  <trd lng="en">giraffe</trd>
  <trd lng="es">jirafa</trd>
  <trd lng="fa">&a_z;&a_r;&a_A;&a_f;&a_h;</trd>
  <trd lng="fr">girafe</trd>
  <trd lng="hu">zsir&aacute;f</trd>
  <trd lng="id">jerapah</trd>
  <trd lng="it">giraffa</trd>
  <trd lng="nl">giraf</trd>
  <trd lng="pl">&zdot;yrafa</trd>
  <trd lng="pt">girafa</trd>
  <trd lng="ru">&c_zh;&c_i;&c_r;&c_a;&c_f;</trd>
  <trd lng="tr">z&uuml;rafa</trd>
  <trd lng="sv">giraff</trd>
</drv>

<drv mrk="gxiraf.Gx0o">
  <kap><tld lit="&Gcirc;"/>o</kap>
  <mlg>Cam</mlg>
  <snc>
    <dif>
      Nord-hemisfera stelfiguro (<trd lng="la">Camelopardalis</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_Zh;&c_i;&c_r;&c_a;&c_f;<klr tip="amb"> 
(&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="de">Giraffe</trd>
  <trd lng="en">Camelopardalis</trd>
  <trd lng="fa">&a_z;&a_r;&a_A;&a_f;&a_h;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="hu">Zsir&aacute;f</trd>
  <trd lng="it">Giraffa</trd>
  <trd lng="es">Jirafa</trd>
  <trd lng="pl">&Zdot;yrafa</trd>
</drv>
</art>

