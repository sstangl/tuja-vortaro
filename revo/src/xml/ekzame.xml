<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ekzame.xml,v 1.25 2015/07/04 08:37:33 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>ekzamen</rad>/i
    </kap>

    <drv mrk="ekzame.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="ekzame.0i.esplori">
        <dif>
          Zorgeme rigardi, detale esplori:
          <ekz>
            <tld/>i demandon, problemon,
            aferon;
          </ekz><ekz>
            <tld/>i ne devigas preni
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="rigard.pri0i">pririgardi</ref>
        <trdgrp lng="be">
          <trd>&c_p;&c_r;&c_a;&c_v;&c_ja;&c_r;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_b;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>untersuchen</trd>,
          <trd>pr&uuml;fen</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>vizsg&aacute;l</trd>,
          <trd>megvizsg&aacute;l</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>bada&cacute;</trd>,
          <trd>rozpatrywa&cacute;</trd>,
          <trd>dochodzi&cacute;<klr> (kryminal.)</klr></trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>examinar</trd>,
          <trd>ensaiar</trd>,
          <trd>inspecionar</trd>,
          <trd>revistar</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_o;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_r;&c_a;&c_s;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
        </trdgrp>
        <trd lng="sv">unders&ouml;ka</trd>
      </snc>
      <snc mrk="ekzame.0i.PED">
      <uzo tip="fak">PED</uzo>
        <dif>
          Provi per demandoj la sciojn de kandidato:
          <ekz>
            la kandidato estis <tld/>ita pri la &hcirc;emio.
          </ekz>
        </dif>
        <trd
        lng="be">&c_e;&c_k;&c_z;&c_a;&c_m;&c_e;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        <trdgrp lng="de">
          <trd>pr&uuml;fen</trd>,
          <trd>examinieren</trd>
        </trdgrp>
        <trd lng="hu">vizsg&aacute;ztat</trd>
        <trd lng="pl">egzaminowa&cacute;</trd>
        <trd lng="pt">examinar</trd>
        <trdgrp lng="ru">
          <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
          <trd>&c_e;&c_k;&c_z;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
        <trd lng="sv">examinera</trd>
      </snc>
  <trd lng="bg">&c_i;&c_z;&c_p;&#1117;&c_t;&c_a;&c_m;</trd>
  <trd lng="ca">examinar</trd>
  <trdgrp lng="cs">
    <trd>examinovat</trd>,
    <trd>prověřovat</trd>,
    <trd>testovat</trd>,
    <trd>vyslýchat</trd>,
    <trd>vyšetřovat</trd>,
    <trd>zjišťovat pravost</trd>,
    <trd>zkoumat</trd>,
    <trd>zkoušet <klr>(škol.)</klr></trd>
  </trdgrp>
  <trd lng="en">examine</trd>
  <trd lng="es">examinar</trd>
  <trd lng="fr">examiner<klr tip="ind"> (un probl&egrave;me, un
      candidat)</klr></trd>
  <trdgrp lng="sk">
    <trd>preskúšať</trd>,
    <trd>vyšetriť</trd>
  </trdgrp>
</drv>

    <drv mrk="ekzame.0o">
      <kap><tld/>o</kap>
      <snc mrk="ekzame.0o.esploro">
        <dif>
          Detala esploro.
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_r;&c_a;&c_v;&c_je;&c_r;&c_k;&c_a;</trd>,
          <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
        </trdgrp>
        <trdgrp lng="ca">
          <trd>inspecci&oacute;</trd>,
          <trd>exploraci&oacute;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Untersuchung</trd>,
          <trd>Pr&uuml;fung</trd>
        </trdgrp>
        <trd lng="hu">vizsg&aacute;lat</trd>
        <trdgrp lng="pl">
          <trd>badanie</trd>,
          <trd>dochodzenie<klr> (kryminal.)</klr></trd>,
          <trd>&sacute;ledztwo</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_k;&c_a;</trd>,
          <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
        <trd lng="sv">unders&ouml;kning</trd>
      </snc>
      <snc mrk="ekzame.0o.PED">
        <uzo tip="fak">PED</uzo>
        <dif>
          Kontrolo de la scioj de kandidato:
          <ekz>
            trapasi, elteni <tld/>on;
          </ekz><ekz>
            fari sian <tld/>(i&gcirc;)on<sup>Z</sup>;
          </ekz><ekz>
            sukcese trapasi, plenumi <tld/>on;
          </ekz><ekz>
            sukcesi <tld/>on;
          </ekz><ekz>
            sin prezenti al <tld/>o.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_e;&c_k;&c_z;&c_a;&c_m;&c_e;&c_n;</trd>,
          <trd>&c_ib;&c_s;&c_p;&c_y;&c_t;</trd>
        </trdgrp>
        <trdgrp lng="ca">
          <trd>control</trd>,
          <trd>prova</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Pr&uuml;fung</trd>,
          <trd>Examen</trd>
        </trdgrp>
        <trd lng="hu">vizsga</trd>
        <trd lng="pl">egzamin</trd>
        <trd lng="ru">&c_e;&c_k;&c_z;&c_a;&c_m;&c_je;&c_n;</trd>
        <trdgrp lng="sv">
          <trd>prov</trd>,
          <trd>tentamen</trd>
        </trdgrp>
      </snc>
  <trd lng="bg">&c_i;&c_z;&c_p;&c_i;&c_t;</trd>
  <trd lng="ca">examen</trd>
  <trdgrp lng="cs">
    <trd>examen</trd>,
    <trd>průba <klr>(slang.)</klr></trd>,
    <trd>zjišťování schopností a znalostí</trd>,
    <trd>zkou&scaron;ka</trd>,
    <trd>šetření</trd>
  </trdgrp>
  <trd lng="es">ex&aacute;men</trd>
  <trd lng="fr">examen</trd>
  <trd lng="id"><ind>uji</ind>an</trd>
  <trdgrp lng="sk">
    <trd>skúška</trd>,
    <trd>vyšetrenie</trd>
  </trdgrp>
</drv>
  </art>
