<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kupe.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kupe</rad>/o
</kap>

<drv mrk="kupe.0o">
  <kap><tld/>o</kap>
  <snc mrk="kupe.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Parto de veturilo, de <ref tip="malprt"
      cel="vagon.0o.FER">vagono</ref>, apartigita de aliaj, kun propra
      pordo:
      <ekz>
        <tld/>o por fumantoj, por sinjorinoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_e;</trd>
  <trd lng="bg">&c_k;&c_u;&c_p;&c_je;<klr tip="amb"> (&c_n;&c_a; &c_v;&c_l;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">compartiment</trd>
  <trdgrp lng="cs">
    <trd>box</trd>,
    <trd>kupé</trd>,
    <trd>odděl. prostor</trd>
  </trdgrp>
  <trd lng="de">Abteil</trd>
  <trd lng="en">compartment</trd>
  <trd lng="es">compartimento</trd>
  <trd lng="fa">&f_k;&a_w;&f_p;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>coup&eacute;<klr tip="amb"> (compartiment)</klr></trd>,
    <trd>compartiment<klr tip="amb"> (chemin de fer...)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&uuml;lke</trd>,
    <trd>kup&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compartiment</trd>,
    <trd>coup&eacute;</trd>
  </trdgrp>
  <trd lng="pl">przedzia&lstroke;</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>kupé</trd>,
    <trd>oddelenie</trd>
  </trdgrp>
</drv>
</art>
