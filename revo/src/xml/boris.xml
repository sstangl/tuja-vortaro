<kap>
  <rad>Boris</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="boris.0o">
  <kap><tld/>o</kap>
  <snc mrk="boris.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> slavdevena:
      <ekz>
        en la jaro 1918 <klr>...</klr> <tld/>o la 3a fari&gcirc;as la
        bulgara caro
        <fnt>
        <bib>Viki</bib>,
        <lok>Boriso la 3a, 2005-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_B;&c_o;&c_r;&c_i;&c_s;</trd>
  <trd lng="ca">Bor&iacute;s</trd>
  <trd lng="de">Boris</trd>
  <trd lng="fr">Boris</trd>
  <trd lng="it">Boris</trd>
  <trd lng="nl">Boris</trd>
  <trd lng="pl">Borys</trd>
  <trd lng="ru">&c_B;&c_o;&c_r;&c_i;&c_s;</trd>
</drv>
</art>

