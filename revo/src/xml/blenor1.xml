<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blenor1.xml,v 1.16 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>blenore</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="blenor1.0o">
  <kap><tld/>o</kap>
  <snc mrk="blenor1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kronika blenoragio, venerea <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> karakterizata per inflamo kun
      elfluo &ccirc;e la seksorganoj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_je;&c_n;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="ca">blennorrea</trd>
  <trd lng="cs">kapavka</trd>
  <trd lng="es">blenorrea</trd>
  <trd lng="fa">&a_s;&a_w;&a_z;&a_A;&f_k;</trd>
  <trd lng="fr">blennorrh&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>gennycsorg&aacute;s</trd>,
    <trd>blennorrhea</trd>
  </trdgrp>
  <trd lng="id">blenorea</trd>
  <trdgrp lng="pl">
    <trd>rze&zdot;&aogonek;czka</trd>,
    <trd>tryper<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">kvapavka</trd>
</drv>
</art>
