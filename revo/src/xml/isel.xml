<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: isel.xml,v 1.10 2013/09/06 19:10:18 revo Exp $">
<kap>
  <rad>Isel</rad>/o
</kap>

<drv mrk="isel.I0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> al
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>,
      kie &gcirc;i fluas en la <tld/>an Lagon:
      <ekz>
        la <tld/>a Lago [estas] nomita
        la&ubreve; la rivero <tld/>o, de kiu &gcirc;i ricevas dol&ccirc;an akvon.
        <fnt><aut>Wouter PILGER</aut>:
        <vrk>Nederlando kaj la maro</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Issel</trd>,
    <trd>Ijssel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>IJssel</trd>,
    <trd>Ijssel</trd>
  </trdgrp>
  <trd lng="hu">Ijssel</trd>
  <trd lng="nl">IJssel</trd>
  <trdgrp lng="pt">
    <trd>Ijsel</trd>,
    <trd>Yssel</trd>
  </trdgrp>
</drv>

<drv mrk="isel.I0aLago">
  <kap><tld/>a Lago</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lag.0o">Lago</ref> en la nordo de
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>. Anta&ubreve; 1932
      &gcirc;i
      nomi&gcirc;is Suda Maro, kiun oni decidis fermi per granda digo:
      <ekz>
        la lasta
        granda poldero en la <tld/>a Lago seki&gcirc;is en 1968.
        <fnt><aut>Wouter PILGER</aut>:
        <vrk>Nederlando kaj la maro</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">IJsselmeer</trd>
  <trdgrp lng="fr">
    <trd>Lac d'IJssel</trd>,
    <trd>Lac d'Ijssel</trd>,
    <trd>IJsselmeer</trd>
  </trdgrp>
  <trd lng="hu">Ijssel-t&oacute;</trd>
  <trd lng="nl">IJsselmeer</trd>
</drv>
</art>
