<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dehisk.xml,v 1.16 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>dehisk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dehisk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dehisk.0i.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malfermi&gcirc;i per la natura disi&gcirc;o de la valvoj:
      <ekz>
        <ind><tld/>a</ind> frukto.
        <trd lng="ca">dehiscent</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">obrir-se naturalment</trd>
  <trd lng="cs">puknout <klr>(bot.)</klr></trd>
  <trdgrp lng="de">
       <trd>aufplatzen</trd>,
       <trd>aufspringen</trd>
    </trdgrp>
  <trd lng="en">dehisce</trd>
  <trdgrp lng="hu">
       <trd>felny&iacute;lik<klr tip="ind"> (pl. r&uuml;gy)</klr></trd>,
       <trd>kipattan<klr tip="ind"> (pl. r&uuml;gy)</klr></trd>,
       <trd>kifakad<klr tip="ind"> (pl. r&uuml;gy)</klr></trd>
    </trdgrp>
  <trd lng="pl">p&eogonek;ka&cacute;</trd>
  <trd lng="pt">apresentar <ind>deisc&ecirc;ncia</ind></trd>
  <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_je;&c_s;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_o; &c_p;&c_l;&c_o;&c_d;&c_a;&c_h;)</klr></trd>
    </trdgrp>
  <trd lng="sk">roztvoriť sa</trd>
</drv>

<drv mrk="dehisk.0ado">
  <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
    <snc mrk="dehisk.0ado.BOT">
      <dif>
        Ago, per kiu <ref tip="vid" cel="perika1.0o">perikarpo</ref>
        malfermi&gcirc;as por eligi grajnon, anteron, polenon.  
      </dif>
    </snc>
  <trd lng="ca">dehisc&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>puknutí <klr>(plodu)</klr></trd>,
    <trd>rozpuk</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Aufplatzen</trd>,
       <trd>Aufspringen</trd>
    </trdgrp>
  <trd lng="en">dehiscence</trd>
  <trd lng="fr">d&eacute;hiscence</trd>
  <trd lng="pt">deisc&ecirc;ncia</trd>
  <trdgrp lng="sk">
    <trd>pukanie <klr>(plodu)</klr></trd>,
    <trd>rozpuk</trd>
  </trdgrp>
</drv>
</art>
