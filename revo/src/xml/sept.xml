<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sept.xml,v 1.9 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sept</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="sept.0o">
  <kap><tld/>o</kap>
  <snc mrk="sept.0o.intervalo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el sep sinsekvaj tonoj de diatona gamo.
    </dif>
    <refgrp tip="sin">
      <ref cel="septim.0o">septimo</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_e;&c_p;&c_t;&c_y;&c_m;&c_a;</trd>
    <trd lng="ca">s&egrave;ptima<klr> (interval)</klr></trd>
    <trd lng="de">Septime</trd>
    <trdgrp lng="fr">
      <trd>septi&egrave;me<klr> (intervalle musical)</klr></trd>
    </trdgrp>
    <trd lng="hu">szeptim <klr tip="amb">(hangk&ouml;z)</klr></trd>
    <trd lng="it">settima<klr> (mus.)</klr></trd>
    <trd lng="nl">septime</trd>
    <trd lng="ru">&c_s;&c_je;&c_p;&c_t;&c_i;&c_m;&c_a;</trd>
  </snc>
  <snc mrk="sept.0o.de.toniko">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sepa tono de gamo.
    </dif>
    <refgrp tip="sin">
      <ref cel="septim.0o">septimo</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_e;&c_p;&c_t;&c_y;&c_m;&c_a;</trd>
    <trd lng="ca">s&egrave;ptima<klr> (to)</klr></trd>
    <trd lng="de">Septime</trd>
    <trdgrp lng="fr">
      <trd>septi&egrave;me<klr> (intervalle musical)</klr></trd>
    </trdgrp>
    <trd lng="hu">szeptim <klr tip="amb">(hetedik hang)</klr></trd>
    <trd lng="it">settima<klr> (mus.)</klr></trd>
    <trd lng="nl">septime</trd>
    <trd lng="ru">&c_s;&c_je;&c_p;&c_t;&c_i;&c_m;&c_a;</trd>
  </snc>
  <snc mrk="sept.0o.vando">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="vand.0o">Vando</ref>
    <trd lng="ca">septe</trd>
    <trd lng="it">setto</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>přepážka</trd>,
    <trd>příčka</trd>,
    <trd>stěna</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>priehradka</trd>,
    <trd>priečka</trd>,
    <trd>stena</trd>
  </trdgrp>
</drv>
</art>
