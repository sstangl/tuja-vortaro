<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: siluet.xml,v 1.19 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>siluet</rad>/o
</kap>
<drv mrk="siluet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Bildo formata de ombro de profila viza&gcirc;o.
    </dif>
    <trd lng="hu">&aacute;rnyk&eacute;p</trd>
    <trd lng="pt">silhueta</trd>
  </snc>
  <snc>
    <dif>
      Desegno montranta nur la eksterajn konturojn.
    </dif>
    <trd lng="hu">&aacute;rnyrajz</trd>
    <trd lng="pt">silhueta</trd>
  </snc>
  <snc>
    <dif>
      Vida&jcirc;o de io, kio nete kontrastas el la fono:
      <ekz>
        de tie oni ekvidas la pitoreskan <tld/>on de la urbo;
      </ekz>
      <ekz>
        la turoj grandioze <tld/>i&gcirc;as kontra&ubreve; la ru&gcirc;a fono de l'
        okcidento.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>sziluett</trd>,
      <trd>k&ouml;rvonal</trd>
    </trdgrp>
    <trd lng="pt">silhueta</trd>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_l;&c_ju;&c_e;&c_t;</trd>
  <trd lng="ca">silueta</trd>
  <trdgrp lng="cs">
    <trd>kontura</trd>,
    <trd>obrys</trd>,
    <trd>silueta</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Umriss</trd>,
      <trd>Silhouette</trd>
  </trdgrp>
  <trd lng="en">silhouette</trd>
  <trdgrp lng="fa">
    <trd>&a_D;&a_d;&a_shadda;&a_n;&a_w;&a_r;</trd>,
    <trd>&a_s;&f_y;&a_l;&a_w;&a_y_hamza;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">silhouette</trd>
  <trdgrp lng="it">
      <trd>profilo<klr> (figura)</klr></trd>,
      <trd>sagoma<klr> (disegno)</klr></trd>, 
      <trd>silhouette</trd>
    </trdgrp>
  <trd lng="nl">silhouet</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_u;&c_e;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>obrys</trd>,
    <trd>silueta</trd>
  </trdgrp>
</drv>
</art>
