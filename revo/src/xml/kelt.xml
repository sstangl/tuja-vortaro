<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kelt.xml,v 1.10 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kelt</rad>/o<fnt>PIV1</fnt>
</kap>
<drv mrk="kelt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de <tld/>lingva nacio.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_l;&c_mol;&c_t;</trd>
  <trd lng="cs">Kelt</trd>
  <trd lng="de">Kelte</trd>
  <trdgrp lng="en">
    <trd>Celt</trd>, <trd>Kelt</trd>
  </trdgrp>
  <trd lng="es">celta</trd>
  <trd lng="fr">Celte</trd>
  <trd lng="hu">kelta</trd>
  <trd lng="nl">Kelt</trd>
  <trd lng="ru">&c_k;&c_je;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">Kelt</trd>
</drv>

<drv mrk="kelt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, ilia historio kaj lingvoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_l;&c_mol;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">keltisch</trd>
  <trd lng="en">celtic</trd>
  <trd lng="es">c&eacute;ltico</trd>
  <trdgrp lng="fr">
    <trd>celte<klr tip="ind"> (adj.)</klr></trd>,
    <trd>celtique<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kelta</trd>
  <trd lng="nl">Keltisch</trd>
  <trd lng="ru">&c_k;&c_je;&c_l;&c_mol;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kelt.0a.lingvo">
  <kap><tld/>a lingvo</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Lingvo apartenanta al la <tld/>a bran&ccirc;o de la
      <ref tip="malprt" cel="hind.0euxropa">hinde&ubreve;ropa</ref>
      lingvofamilio: la ejra, la kimra, la ga&ubreve;la ktp.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_l;&c_mol;&c_c;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="de">Keltisch</trd>
  <trd lng="en">celtic language</trd>
  <trdgrp lng="fr">
    <trd>celte<klr tip="ind"> (langue)</klr></trd>,
    <trd>celtique<klr tip="ind"> (langue)</klr></trd>,
    <trd>langue <ind>celtique</ind></trd>
  </trdgrp>
  <trd lng="hu">kelta nyelv</trd>
  <trd lng="nl">Keltisch</trd>
  <trd lng="ru">&c_k;&c_je;&c_l;&c_mol;&c_t;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
</drv>
</art>
