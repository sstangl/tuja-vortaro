<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: proces1.xml,v 1.18 2016/03/16 14:10:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>procesi</rad>/o
</kap>
<drv mrk="proces1.0o">
  <kap><tld/>o</kap>
  <snc mrk="proces1.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Festa mar&scirc;ado sur vojoj por elmontrado de sankta&jcirc;oj:
      <ekz>
	mar&scirc;i kiel <tld/>o
	<klr>(malrapide kaj solene)</klr>;
      </ekz>
      <ekz>
	<ind>funebra <tld/>o</ind>.
	<trd lng="hu">gy&aacute;szmenet</trd>
      </ekz>
    </dif>
    <trd lng="th">&#3585;&#3634;&#3619;&#3649;&#3627;&#3656;&#3626;&#3636;&#3656;&#3591;&#3624;&#3633;&#3585;&#3626;&#3636;&#3607;&#3608;&#3636;&#3660;</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ca">process&oacute;</trd>
  <trdgrp lng="cs">
    <trd>proces&iacute;</trd>,
    <trd>slavn. pr&uring;vod</trd>
  </trdgrp>
  <trd lng="de">Prozession</trd>
  <trd lng="en">procession</trd>
  <trdgrp lng="fr">
    <trd>cort&egrave;ge</trd>,
    <trd>procession</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;rmenet</trd>
  <trdgrp lng="nl">
    <trd>processie</trd>,
    <trd>stoet</trd>,
    <trd>optocht</trd>
  </trdgrp>
  <trd lng="pt">prociss&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; &c_h;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">sprievod</trd>
</drv>

<drv mrk="proces1.0i">
  <kap><tld/>i</kap>
  <snc mrk="proces1.0i.KOMUNE">
    <dif>
      Pa&scirc;i grupe, solene sur vojoj, kiel <tld/>o:
      <ekz>
	la homoj de la vitra urbo faris spacon por ili kaj <tld/>is post ili
	<fnt><bib>DOz</bib>, <lok>&leftquot;la vitra
	urbo&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="defil.0i.MIL">defili <sncref/></ref>,
      <ref cel="defil.0i.modo">defili <sncref/></ref>,
      <ref cel="manife1.0o">manifestacii</ref>
    </refgrp>
  </snc>
  <trd lng="ca">fer una process&oacute;</trd>
  <trdgrp lng="cs">
    <trd>j&iacute;t v pr&uring;vodu</trd>,
    <trd>&uacute;&ccaron;astnit se pochodu</trd>,
    <trd>&uacute;&ccaron;astnit se pr&uring;vodu</trd>
  </trdgrp>
  <trd lng="en">go in procession</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;filer</trd>,
    <trd>faire une <ind>procession</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>defileren</trd>,
    <trd>in <ind>stoet</ind> optrekken</trd>
  </trdgrp>
  <trd lng="sk">&iacute;s&tcaron; v sprievode</trd>
</drv>
</art>
