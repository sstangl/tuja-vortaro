    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Boraginaceae</trd>).
    </dif>
  </snc>
  <trd lng="ca">borragin&agrave;cies</trd>
  <trd lng="de">Boretschgew&auml;chse</trd>
  <trd lng="fr">boraginac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>bor&aacute;g&oacute;f&eacute;l&eacute;k</trd>,
    <trd>&eacute;rdeslevel&udblac;ek</trd></trdgrp>
  <trd
lng="ru">&c_b;&c_u;&c_r;&c_a;&c_ch;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">str&auml;vbladiga v&auml;xter</trd>
</drv>
</art>
