<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trust.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>trust</rad>/o
</kap>

<drv mrk="trust.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grupi&gcirc;o da potencaj komercistoj a&ubreve; fabrikantoj
      kuni&gcirc;intaj por
      akapari iujn varojn a&ubreve; monopoligi iujn produktojn kaj tiele
      povi
      la&ubreve;pla&ccirc;e altigi la prezojn.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_s;&c_t;</trd>
  <trd lng="bg">&c_t;&c_r;&c_malmol;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>trast</trd>,
    <trd>trust</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Konzern</trd>
    <trd>Trust<klr tip="amb"> (Wirtschaft)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>business trust<klr tip="amb"> (with intent to
    monopolize, as in "antitrust laws")</klr></trd>
  </trdgrp>
  <trd lng="hu">tr&ouml;szt</trd>
  <trd lng="nl">trust</trd>
  <trd lng="pl">trust</trd>
  <trd lng="pt">truste</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>trast</trd>,
    <trd>trust</trd>
  </trdgrp>
</drv>
</art>
