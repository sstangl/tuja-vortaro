</kap>
<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">Na&gcirc;birdo</ref> 
      el la familio
      <ref tip="malprt" cel="anased.0oj"><tld/>edoj</ref>, de
      diversaj genroj.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
    <trd lng="br">houad</trd>
    <trd lng="cs">kachna</trd>
    <trd lng="de">Ente</trd>
    <trd lng="en">duck</trd>
    <trd lng="es">pato</trd>
    <trd lng="fr">canard</trd>
    <trd lng="hu">kacsa</trd>
    <trd lng="id">bebek</trd>
    <trd lng="it">anatra</trd>
    <trd lng="nl">eend</trd>
    <trd lng="no">and</trd>
    <trd lng="pl">kaczka</trd>
    <trd lng="pt">pato</trd>
    <trd lng="ru">&c_u;&c_t;&c_k;&c_a;</trd>
    <trd lng="sk">ka&ccaron;ica</trd>
    <trdgrp lng="sv">
      <trd>and</trd>,
      <trd>anka</trd>
    </trdgrp>
    <trd lng="tr">&ouml;rdek</trd>
  </snc>

  <snc mrk="anas.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="anased.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anas</trd>);
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">na&gcirc;birdo</ref>,
      kies viro havas plumaron belan kaj tute apartan de la virino:
      <ekz>
        trapasante la facilajn rapidfluejojn de Enco, ni vidis plurfoje
        <tld/>ojn kun belaj oran&gcirc;aj bekoj
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/anas.jpg">
      <ind>platbeka <tld/>o</ind> &#8208; paro
      <trd lng="br">houad-korz</trd>
      <trd lng="cs">kachna divok&aacute;</trd>
      <trd lng="de">Stockente</trd>
      <trd lng="en">mallard</trd>
      <trd lng="fr">colvert</trd>
      <trd lng="he">&bet;&resh;&kaf;&yod;&yod;&he;</trd>
      <trd lng="hu">t&odblac;k&eacute;s r&eacute;ce</trd>
      <trd lng="la">Anas platyrhynchos</trd>
      <trd lng="pl">krzy&zdot;&oacute;wka</trd>
      <trd lng="ru">&c_k;&c_r;&c_ja;&c_k;&c_v;&c_a;</trd>
    </bld>
  </snc>
  <trd lng="he">&bet;&resh;&vav;&vav;&zayin;</trd>
</drv>

