<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: polu.xml,v 1.16 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>polu</rad>/i
  <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="polu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polu.0i.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Malpurigi vivesencan elementon de la naturo:
      <ekz>
        la urino kaj feka&jcirc;oj de intense breditaj bestoj <tld/>as 
riverojn
        <fnt>
        <aut>Krys Ungar</aut>,
        <vrk><url ref="&Monato;/004583.html">Vivu vigle kaj 
vegetare!</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 07/1995</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd 
lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">znečišťovat život. prostř.</trd>
  <trd lng="de">verschmutzen</trd>
  <trd lng="en">pollute</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>polluer</trd>
  </trdgrp>
  <trd lng="hu">szennyez <klr>(k&ouml;rnyezetet)</klr></trd>
  <trdgrp lng="nl">
    <trd>verontreinigen</trd>,
    <trd>vervuilen</trd>
  </trdgrp>
  <trd lng="pt">poluir</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_ja;&c_t;&c_mol; 
<klr>(&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_u;&c_ju; 
&c_s;&c_r;&c_je;&c_d;&c_u;)</klr></trd>
  <trd lng="sk">znečisťovať životné prostredie</trd>
  <trd lng="tp">jaki</trd>
</drv>

<drv mrk="polu.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polu.0ado.EKOL">
    <dif>
      La ago <tld/>i:
      <ekz>
        la poleno informas pri la atmosfera <tld/>ado
        <fnt>
        <aut>Robert Roze</aut>,
        <vrk><url ref="&Monato;/005775.html">Vivisekcio de 
polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 11/1997</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="poluci.0o.medi">polucio</ref>
  </refgrp>
  <trd 
lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Verschmutzung</trd>
  <trd lng="en">pollution</trd>
  <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_d;&a_h;&zwnj;&a_s;&a_A;&a_z;&f_y;</trd>
  <trd lng="fr">pollution</trd>
  <trd lng="hu">k&ouml;rnyezetszennyez&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>vervuiling</trd>,
    <trd>pollutie</trd>,
    <trd>verontreiniging</trd>
  </trdgrp>
  <trd 
lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_je;&c_n;&c_i;&c_je; 
&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_je;&c_j; 
&c_s;&c_r;&c_je;&c_d;&c_y;</trd>
</drv>
</art>
