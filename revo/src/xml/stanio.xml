<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stanio.xml,v 1.14 2015/10/27 10:10:24 revo Exp $">
<kap>
  <rad>staniol</rad>/o
</kap>
<drv mrk="stanio.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Alojo el stano kaj aluminio, uzata en folioj por envolvi
      &ccirc;okoladon,
      bombonojn ks (= stanfolio).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_ib;&c_jo;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_o;&c_l;&c_mol;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>c&iacute;nov&aacute; f&oacute;lie</trd>,
    <trd>staniol</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stanniol</trd>,
    <trd>Silberpapier</trd>
  </trdgrp>
  <trd lng="en">tin foil</trd>
  <trd lng="fr">staniole</trd>
  <trdgrp lng="hu">
    <trd>sztaniol</trd>,
    <trd>ez&uuml;stpap&iacute;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>staniol</trd>,
    <trd>zilverpapier</trd>
  </trdgrp>
  <trd lng="pl">staniol</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_i;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_o;&c_l;&c_mol;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">staniol</trd>
</drv>
</art>
