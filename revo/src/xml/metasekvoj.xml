<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: metasekvoj.xml,v 1.6 2013/10/26 16:10:30 revo Exp $">
<kap>
  <rad>metasekvoj</rad>/o
</kap>
<drv mrk="metasekvoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="metasekvoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref> kun pingloformaj folioj en
      a&ubreve;tuno
      flave kaj brune velkantaj, el centra &Ccirc;inujo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="cipres1.0oj">cipresacoj</ref>
      (<trd lng="la">Metasequoia</trd>)
      kun nur unu vivanta specio (<trd lng="la">Metasequoia
      glyptostroboides</trd>):
      <ekz>
        mi vidis te&gcirc;ardenojn <klr>...</klr> valorajn <tld/>ojn
        kaj sekvojojn <klr>...</klr> starantajn amba&ubreve;flanke de la vojo
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk>&Ccirc;ina ta&ubreve;isma sankta monto Wudang (1)</vrk>,
        <lok>2005-02-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">metasequoia</trd>
  <trdgrp lng="de">
    <trd>Urweltmammutbaum</trd>,
    <trd>Chinesisches Rotholz</trd>,
    <trd>Metasequoie</trd>,
    <trd>Wassertanne</trd>
  </trdgrp>
  <trd lng="fr">M&eacute;tas&eacute;quoia</trd>
  <trd lng="pt">metassequ&oacute;ia</trd>
</drv>
</art>

