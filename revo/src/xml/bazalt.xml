      <trd lng="fr">basalte</trd>
      <trd lng="hu">bazalt</trd>
      <trd lng="pl">bazalt</trd>
      <trd lng="ru">&c_b;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_t;</trd>
      <trd lng="it">basalto</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>bazalt</trd>,
    <trd>훾edi훾</trd>
  </trdgrp>
  <trd lng="sk">훾adi훾</trd>
</drv>
</art>
