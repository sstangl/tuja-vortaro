<art mrk="$Id: bazalt.xml,v 1.18 2015/07/04 08:37:31 revo Exp $">
  <kap>
    <ofc>2</ofc>
    <rad>bazalt</rad>/o
  </kap>
  <drv mrk="bazalt.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MIN</uzo>
    <snc>
      <dif>
        Tre malmola nigra vulkana &scirc;tono.
