<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: manegx.xml,v 1.10 2006/07/29 16:32:26 revo Exp $">
<kap>
  <rad>mane&gcirc;</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="manegx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ekzerco, per kiu oni dresas &ccirc;evalon.
    </dif>
    <trd lng="be">&c_v;&c_y;&c_je;&c_z;&c_d;&c_k;&c_a;</trd>
    <trd lng="de">Manege</trd>
    <trd lng="en">manege</trd>
    <trdgrp lng="hu">
      <trd>m&udblac;lovagl&aacute;s</trd>,
      <trd>lovagl&oacute;m&udblac;v&eacute;szet</trd>,
      <trd>lovagl&oacute;iskola</trd></trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_je;&c_z;&c_d;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Konstruo, kie oni lernas la rajdarton; rajdlernejo.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_zh;</trd>
    <trd lng="en">manege</trd>
    <trdgrp lng="hu">
      <trd>man&eacute;zs</trd>,
      <trd>lovarda</trd></trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_n;&c_je;&c_zh;</trd>
  </snc>
  <trd lng="nl">manege</trd>
  <trd lng="pt">manejo</trd>
</drv>

<drv mrk="manegx.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      <tld/>o 2.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_zh;</trd>
  <trd lng="nl">manege</trd>
  <trd lng="pt">manejo</trd>
</drv>
</art>
