<kap>
<ofc>2</ofc>
<rad>dekametr</rad>/o
</kap>
<drv mrk="dekame.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, egala
      al
      dek <ref tip="prt" cel="metr.0o.unuo">metroj</ref>; simb. dam.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">dec&agrave;metre</trd>
  <trdgrp lng="de">
     <trd>Dekameter</trd>,
     <trd>zehn <ind>Meter</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decameter</trd>,
    <trd>decametre</trd>
  </trdgrp>  <trd lng="fa">&a_d;&f_k;&a_A;&a_m;&a_t;&a_r;</trd>
  <trd lng="fr">d&eacute;cam&egrave;tre</trd>
  <trdgrp lng="hu">
     <trd>t&iacute;z <ind>m&eacute;ter</ind></trd>,
     <trd>dekam&eacute;ter</trd>
  </trdgrp>
  <trd lng="nl">dekameter</trd>
  <trd lng="pl">dekametr</trd>
  <trd lng="pt">dec&acirc;metro</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
