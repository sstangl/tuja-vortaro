  <trd lng="ru">&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;&c_i;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">tetrovovité</trd>
