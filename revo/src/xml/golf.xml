<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: golf.xml,v 1.24 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>golf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="golf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="golf.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vastega parto de maro eni&gcirc;anta en la teron:
      <ekz>
        <tld/>o de Botnio, de Finlando.
      </ekz>
    </dif>
  </snc>
  <rim mrk="golf.0o.RIM_HOM">
    <ref tip="hom" cel="golf1.0o">Golfo</ref>
    estas anka&ubreve; nomo de sporto.
  </rim>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_ib;&c_w;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_l;&c_i;&c_v;</trd>
  <trd lng="ca">golf</trd>
  <trdgrp lng="de">
    <trd>Golf</trd>,
    <trd>Meerbusen</trd>
  </trdgrp>
  <trd lng="cs">z&aacute;toka</trd>
  <trd lng="en">gulf</trd>
  <trd lng="es">golfo</trd>
  <trd lng="fr">golfe</trd>
  <trd lng="hu">tenger&ouml;b&ouml;l</trd>
  <trd lng="id">teluk</trd>
  <trd lng="nl">golf</trd>
  <trd lng="ru">&c_z;&c_a;&c_l;&c_i;&c_v;</trd>
  <trd lng="pt">golfo</trd>
  <trd lng="tr">k&ouml;rfez</trd>
  <trdgrp lng="sk">
    <trd>záliv</trd>,
    <trd>zátoka</trd>
  </trdgrp>
  <trd lng="cs">záliv</trd>
</drv>

<drv mrk="golf.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="golf.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de maro eni&gcirc;anta en la teron, pli malvasta ol <tld/>o
      <klr>(longa &gcirc;is cent kilometroj, proksimume)</klr>:
      <ekz>
        la <tld/>eto de Napolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_h;&c_t;&c_a;</trd>
  <trd lng="ca">badia</trd>
  <trd lng="cs">minigolf</trd>
  <trd lng="de">Bucht</trd>
  <trd lng="en">bay</trd>
  <trd lng="es">bah&iacute;a</trd>
  <trd lng="fr">baie</trd>
  <trd lng="hu">&ouml;b&ouml;l<klr tip="ind"> (kisebb)</klr></trd>
  <trd lng="id">teluk</trd>
  <trdgrp lng="pt">
     <trd>ba&iacute;a</trd>,
     <trd>enseada</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_h;&c_t;&c_a;</trd>
  <trd lng="sk">minigolf</trd>
</drv>
</art>
