<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dipsak.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
<rad>dipsak</rad>/o
</kap>
<drv mrk="dipsak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="dipsak1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Dipsacus</trd>), kies dornojn oni uzas por kardi
      &scirc;tofon.
    </dif>
  </snc>
  <trd lng="ca">card&oacute;</trd>
  <trd lng="cs">štětka</trd>
  <trd lng="de">Karde</trd>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_j;&a_h;&zwnj;&a_b;&a_A;&a_sh;&f_y;</trd>
  <trd lng="fr">card&egrave;re</trd>
  <trd lng="hu">m&aacute;csonya</trd>
  <trd lng="pl">szcze&cacute;</trd>
  <trdgrp lng="pt">
    <trd>d&iacute;psaco</trd>,
    <trd>cardo</trd>,
    <trd>cardo-cardador</trd>,
    <trd>cardo-penteador</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">štetka súkennícka</trd>
</drv>

</art>
