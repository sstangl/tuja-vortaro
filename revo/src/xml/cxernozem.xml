<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxernozem.xml,v 1.3 2013/04/03 10:10:19 revo Exp $">
<kap>
  <rad>&ccirc;ernozem</rad>/o
</kap>
<drv mrk="cxernozem.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxernozem.0o.GRU">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Nigra, fruktodona
      <ref tip="super" cel="grund.0o.KOMUNE">grundo</ref>
      grandparte huma, trovebla en vasta nordazia areo,
      precipe en Ukrajnujo, kaj anka&ubreve; 
      en Nordameriko kaj en Nordorienta &Ccirc;inujo:
      <ekz>
        la <tld/>a zono de Sovetio etendi&gcirc;as de Nigra
        Maro &gcirc;is Man&ccirc;urio
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="cxernozem.0o.RIM_SIN">
    PIV2 proponas la formon <em>&ccirc;ernozio</em>.
  </rim>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_a;&c_z;&c_jo;&c_m;</trd>
  <trdgrp lng="fr">
    <trd>tchernozem</trd>,
    <trd>tchernoziom</trd>
  </trdgrp>
  <trd lng="pl">czarnoziem</trd>
</drv>
</art>

