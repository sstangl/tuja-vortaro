<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cikad.xml,v 1.22 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>cikad</rad>/o
</kap>
<drv mrk="cikad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="super" cel="cikad.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_c;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">skrilh-wez</trd>
  <trd lng="ca">cigala</trd>
  <trd lng="cs">cikáda</trd>
  <trdgrp lng="de">
    <trd>Zikade</trd>,
    <trd>Zirpe</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&tau;&zeta;&jota;&tau;&zeta;&jota_ton;&kappa;&jota;</trd>,
    <trd>&tau;&zeta;&jota_ton;&tau;&zeta;&jota;&kappa;&alfa;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">cicada</trd>
  <trd lng="fr">cigale</trd>
  <trd lng="hu">kab&oacute;ca</trd>
  <trd lng="it">cicala</trd>
  <trd lng="nl">cicade</trd>
  <trdgrp lng="pl">
    <trd>cykada</trd>,
    <trd>piewnik</trd>
  </trdgrp>
  <trd lng="pt">cigarra</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">cikáda</trd>
</drv>
<drv mrk="cikad.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subordo de la ordo <ref tip="super"
      cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Auchenorhyncha</trd>), kun triparta rostro,
      saltkapablaj
      malanta&ubreve;aj kruroj kaj mallongaj tentakloj. La maskloj ofte
      kapabla la&ubreve;te &ccirc;irpi per abdomenaj membranoj.
    </dif>
  </snc>
  <trd lng="hu">sz&iacute;nkab&oacute;c&aacute;k</trd>
  <trdgrp lng="it">
    <trd>Auchenorrinchi</trd>,
    <trd>cicale</trd>,
    <trd>cicaline</trd>
  </trdgrp>
  <trd lng="nl">cicaden</trd>
  <trdgrp lng="pl">
    <trd>cykadowate</trd>,
    <trd>piewnikowate</trd>
  </trdgrp>
  <trd lng="pt">cican&iacute;deos</trd>
</drv>
</art>
