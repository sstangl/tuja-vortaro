<vortaro>
<art mrk="$Id: marmel.xml,v 1.21 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>marmelad</rad>/o
</kap>
<drv mrk="marmel.0o">
  <kap><tld/>o</kap>
  <snc mrk="marmel.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Speco de konfita&jcirc;o el fruktoj kuiritaj kun sukero sufi&ccirc;e longe,
      por
      ke ili konsistigu densan ka&ccirc;on.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_e;&c_l;&c_ja;&c_d;</trd>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_m;&c_a;&c_l;&c_a;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>melmelada</trd>,
    <trd>compota</trd>,
    <trd>confitura</trd>
  </trdgrp>
  <trd lng="cs">marmeláda</trd>
  <trd lng="de">Marmelade</trd>
  <trdgrp lng="en">
    <trd>marmelade</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mermelada</trd>,
    <trd>jalea</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_r;&a_b;&a_A;</trd>,
    <trd>&a_m;&a_A;&a_r;&a_m;&a_A;&a_l;&a_A;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiture<klr> (marmelade)</klr></trd>,
    <trd>marmelade</trd>
  </trdgrp>
  <trd lng="hu">lekv&aacute;r</trd>
  <trdgrp lng="nl">
    <trd>marmelade</trd>,
    <trd>moes</trd>,
    <trd>jam</trd>
  </trdgrp>
  <trd lng="pt">gel&eacute;ia de frutas</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_m;&c_je;&c_l;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>džem</trd>,
    <trd>lekvár</trd>
  </trdgrp>
  <trd lng="sv">marmelad</trd>
  <trd lng="tr">re&ccedil;el</trd>
</drv>
</art>
