<vortaro>
<art mrk="$Id: frugil.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>frugileg</rad>/o
</kap>
<drv mrk="frugil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="korv.0o.genro">korvo</ref>
      (<trd lng="la">Corvus frugilegus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_p;&c_o;&c_s;&c_je;&c_v;&c_n;&c_a; <ind>&c_v;&c_r;&c_a;&c_n;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>havran polni</trd>,
    <trd>havran polní</trd>
  </trdgrp>
  <trd lng="de">Saatkr&auml;he</trd>
  <trd lng="en">rook</trd>
  <trd lng="fr">freux</trd>
  <trd lng="hu">vet&eacute;si varj&uacute;</trd>
  <trd lng="nl">roek</trd>
  <trd lng="pl">gawron</trd>
  <trdgrp lng="pt">
    <trd>gralha-calva</trd>,
    <trd>frouva</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_ch;</trd>
  <trd lng="sk">havran poľný</trd>
</drv>
</art>
