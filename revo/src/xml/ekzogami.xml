<kap>
  <rad>ekzogami</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekzogami.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekzogami.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Moro, la&ubreve; kiu oni geedzi&gcirc;as al aligentanoj,
      kontraste al <ref tip="ant" 
      cel="endogami.0o.GEN">endogamio</ref>:
      <ekz>
        <klr>[en Nov-Gvineo]</klr> estas forta <tld/>a tradicio, la&ubreve; 
        kiu inoj estis aligentaj kaj alilingvaj
        <fnt>
          <aut>LingvaFestivalo</aut>,
          <vrk><url
          ref="http://wiki.lingvafestivalo.org:8080/index.php?title=Dania_lingvo">
          Dania lingvo</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">exog&agrave;mia</trd>
  <trd lng="de">Exogamie</trd>
  <trd lng="en">exogamy</trd>
  <trd lng="fr">exogamie</trd>
  <trd lng="hu">exog&aacute;mia</trd>
</drv>
</art>

