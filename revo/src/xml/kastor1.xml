   </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_r;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cast&ograve;rids</trd>
  <trd lng="cs">bobrovití</trd>
  <trd lng="de">Biber</trd>
  <trd lng="en">beavers</trd>
  <trd lng="fr">castorid&eacute;s</trd>
  <trd lng="hu">h&oacute;df&eacute;l&eacute;k</trd>
  <trd lng="nl">bevers</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;&c_o;&c_v;&c_y;&c_je; <klr 
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">bobrovité</trd>
</drv>
</art>
