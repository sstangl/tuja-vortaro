          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Marto_2009.html">
          taglibro</url></vrk>,
          <lok>2009-03-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Louisiana</trd>
  <trd lng="en">Louisiana</trd>
  <trd lng="fr">Louisiane</trd>
  <trd lng="sk">Louisiana</trd>
</drv>
</art>

