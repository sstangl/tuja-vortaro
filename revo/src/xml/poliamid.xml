<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: poliamid.xml,v 1.6 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>poliamid</rad>/o 
</kap>
<drv mrk="poliamid.0o">
  <kap><tld/>o</kap>
  <snc mrk="poliamid.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Plur<ref tip="prt" cel="amid.0o.KEM">amido</ref>;
      <ref tip="super" cel="polimer.0o.molekulo">polimero</ref>
      enhavanta plurajn amidojn, ekzemple
      <ref tip="sub" cel="nilon.0o.TEKS">nilono</ref>,
      <ref tip="sub" cel="kevlar.0o.KEM">kevlaro</ref>:
      <ekz>
        proteinoj estas animalaj kaj kreska&jcirc;aj, sintezaj kondensa&jcirc;oj 
        estas <tld/>aj kaj poliesteraj
        <fnt>
          <aut>M. B&aacute;rtovsk&aacute;</aut>,
          <vrk><url
          ref="http://www.freeweb.hu/eventoj/steb/malpeza_industrio/sxtofoj.htm">
          Nomoj de &scirc;tofoj</url></vrk>,
          <lok>AEST '81</lok>
        </fnt>;
      </ekz>
      <ekz>
        nuntempe grimpo&scirc;nuroj kutime konsistas el
        la plasto <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Grimpo%C5%9Dnuro">
          grimpo&scirc;nuro</url>, 2006-03-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">poliamida</trd>
  <trd lng="cs">polyamid</trd>
  <trd lng="de">Polyamid</trd>
  <trd lng="fr">polyamide</trd>
  <trd lng="it">poliamide</trd>
  <trd lng="sk">polyamid</trd>
</drv>
</art>

