<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: barbakan.xml,v 1.8 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>barbakan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="barbakan.0o">
  <kap><tld/>o</kap>
  <snc mrk="barbakan.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Anta&ubreve;<ref tip="malprt"
      cel="fortik.0ajxo.ARKI">fortika&jcirc;o</ref> 
      sole staranta, &scirc;irmanta pordon, ponton, pasejon...:
      <ekz>
        &ccirc;e la fino de la vojo ja videblas nebula konturo de
        <tld/>oj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/1/2003/12/24/1@1895.htm">
          La antikva urbo Jiaohe...</url></vrk>,
          <lok>2003-12-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iun angulon de la fortika&jcirc;o protektas <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Kastelo de Sankta
        An&gcirc;elo&rightquot;, 
        2008-01-23</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">barbacana</trd>
    <trd lng="de">Barbakane</trd>
    <trd lng="en">barbican</trd>
    <trd lng="fr">barbacane</trd>
    <trdgrp lng="it">
      <trd>barbacana</trd>,
      <trd>barbacane</trd>
    </trdgrp>
    <trd lng="pl">barbakan</trd>
    <trd lng="pt">barbac&atilde;</trd>
  </snc>
  <trd lng="cs">barbak√°n</trd>
  <trd lng="sk">barbakan</trd>
</drv>
</art>

