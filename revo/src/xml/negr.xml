<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: negr.xml,v 1.22 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>negr</rad>/o
</kap>
<drv mrk="negr.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">PERS</uzo>
    <dif>
      Homo de nigra raso; nigrulo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_g;&c_a;&c_r;</trd>
  <trd lng="bg">&c_n;&c_je;&c_g;&c_malmol;&c_r;</trd>
  <trd lng="br">morian</trd>
  <trd lng="ca">negre <klr>(melanoderm)</klr></trd>
  <trdgrp lng="cs">
    <trd>negr</trd>,
    <trd>černoch</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwarzer</trd>,
    <trd>Neger</trd>
  </trdgrp>
  <trd lng="en">Negro</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_p;&a_w;&a_s;&a_t;</trd>
  <trd lng="fr">n&egrave;gre</trd>
  <trdgrp lng="he">
    <trd>&kaf;&vav;&shin;&yod;</trd>,
    <trd>&shin;&het;&vav;&resh;<klr tip="ind">
(&kaf;&vav;&shin;&yod;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;ger</trd>,
    <trd>fekete</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwarte</trd>,
    <trd>neger</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>murzyn</trd>,
    <trd>czarnuch <klr>(obra&zacute;liwie)</klr></trd>
  </trdgrp>
  <trd lng="pt">negro</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;</trd>
  <trd lng="sk">černoch</trd>
  <trd lng="sv">neger</trd>
  <trd lng="tr">zenci</trd>
</drv>
</art>
