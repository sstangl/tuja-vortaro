<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lumbri.xml,v 1.21 2012/08/02 19:10:16 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>lumbrik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lumbri.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="lumbri1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lumbricus</trd>), relative granda vermo vivanta en
      tero, surtere videbla post pluvo.
    </dif>
    <ref tip="sin" cel="verm.ter0o">tervermo</ref>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_zh;&c_d;&c_zh;&c_a;&c_v;&c_y; &c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_zh;&c_d;&c_o;&c_v;&c_je;&c_n; &c_ch;&c_je;&c_r;&c_v;&c_je;&c_j;</trd>
  <trdgrp lng="ca">
    <trd><ind>cuc</ind> de terra</trd>,
    <trd>llombric</trd>
  </trdgrp>
  <trd lng="de">Regenwurm</trd>
  <trd lng="el">&alfa;&sigma;&kappa;&alfa;&rho;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">earthworm</trd>
  <trdgrp lng="fr">
    <trd>ver de terre</trd>,
    <trd>lombric</trd>
  </trdgrp>
  <trd lng="hu">f&ouml;ldigiliszta</trd>
  <trd lng="nl">regenworm</trd>
  <trd lng="pt">lombriga</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_o;&c_j; &c_ch;&c_je;&c_r;&c_v;&c_mol;</trd>
  <trd lng="sv">daggmask</trd>
</drv>
</art>
