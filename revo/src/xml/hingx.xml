<kap>
  <rad>hin&gcirc;</rad>/o
</kap>
<drv mrk="hingx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="pord.0hoko">pordhoko</ref>
  </snc>
