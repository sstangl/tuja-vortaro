  <trdgrp lng="de">
    <trd>Angel</trd>,
    <trd>T&uuml;rangel</trd>,
    <trd>T&uuml;rhaken</trd>
  </trdgrp>
  <trd lng="fr">gond</trd>
  <trd lng="hu">sarokvas<klr tip="ind"> (ajt&oacute;n)</klr></trd>
  <trd lng="nl">scharnier</trd>
  <trd lng="pl">zawiasa</trd>
  <trdgrp lng="pt">
   <trd>gonzo</trd>, 
   <trd>dobradi&ccedil;a</trd> 
  </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_je;&c_r;&c_n;&c_o;&c_j; &c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
