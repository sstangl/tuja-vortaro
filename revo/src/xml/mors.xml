<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mors.xml,v 1.9 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>Mors</rad>/o
</kap>

<drv mrk="mors.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve: Samuel <trd lng="en">Morse</trd>, 1791-1872.
      Usona artisto kaj inventisto.
    </dif>
  </snc>
  <trd lng="bg">&c_M;&c_o;&c_r;&c_z;</trd>
  <trd lng="cs">S. Morse</trd>
  <trd lng="de">Morse</trd>
  <trd lng="fr">Morse</trd>
  <trd lng="hu">Morse</trd>
  <trd lng="it">Morse</trd>
  <trd lng="pt">Samuel <ind>Morse</ind></trd>
  <trd lng="ru">&c_M;&c_o;&c_r;&c_z;&c_je;</trd>
  <trd lng="sk">S. Morse</trd>
</drv>

<drv mrk="mors.m0a">
  <kap><tld lit="m"/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ref cel="kod.morsa0o"><tld lit="m"/>a kodo</ref>.
      </ekz>
    </dif>
    <trdgrp lng="it">
      <trd>codice <ind>Morse</ind></trd>,
      <trd>alfabeto <ind>Morse</ind></trd>
    </trdgrp>
    <trd lng="pt">c&oacute;digo <ind>Morse</ind></trd>
  </snc>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_z;&c_o;&c_v;</trd>
  <trd lng="de">Morse-</trd>
  <trd lng="fr">de <ind>Morse</ind></trd>
  <trd lng="hu">morze</trd>
  <trd lng="it">di <ind>Morse</ind></trd>
  <trd lng="pt">de <ind>Morse</ind></trd>
</drv>

<drv mrk="mors.m0i">
  <kap><tld lit="m"/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sendi per <ref cel="kod.morsa0o"><tld lit="m"/>a kodo</ref>.
    </dif>
  </snc>
  <trd lng="de">morsen</trd>
  <trdgrp lng="fr">
    <trd>envoyer en <ind>morse</ind></trd>,
    <trd>manipuler<klr tip="ind"> (envoyer du morse)</klr></trd>
  </trdgrp>
  <trd lng="hu">morz&eacute;zik</trd>
  <trd lng="it">trasmettere in <ind>Morse</ind></trd>  
  <trd lng="ru">&c_v;&c_y;&c_s;&c_t;&c_u;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;
  <ind>&c_m;&c_o;&c_r;&c_z;&c_ja;&c_n;&c_k;&c_o;&c_j;</ind></trd>
</drv>

</art>

