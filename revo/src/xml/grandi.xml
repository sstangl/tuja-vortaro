<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grandi.xml,v 1.19 2015/12/01 06:10:17 revo Exp $">
<kap><ofc>1</ofc>
<rad>grandioz</rad>/a</kap>
<drv mrk="grandi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Frapante, impone granda:
      <ekz>
        <tld/>a vida&jcirc;o, monumento, ceremonio, manifestacio,
        rezultato, servo, elpensa&jcirc;o, stilo;
      </ekz>
      <ekz>
        <tld/>a iluminado;
      </ekz>
      <ekz>
        <tld/>a diverseco de niaj ideoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        eldoni <tld/>an pazigrafian provon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="majest.0a">majesta</ref>,
      <ref cel="pomp.0a">pompa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>grandi&oacute;s</trd>,
    <trd>fastu&oacute;s</trd>,
    <trd>espl&egrave;ndid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>impozantn&iacute;</trd>,
    <trd>velkolep&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grandios</trd>,
    <trd>gro&szlig;artig</trd>,
    <trd>gewaltig</trd>,
    <trd>&uuml;berw&auml;ltigend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>grandiose</trd>,
    <trd>grand</trd>,
    <trd>magnificent</trd>,
    <trd>splendid</trd>
  </trdgrp>
  <trd lng="es">grandioso</trd>
  <trd lng="fr">grandiose</trd>
  <trdgrp lng="hu">
    <trd>nagyszer&udblac;</trd>,
    <trd>pomp&aacute;s</trd>,
    <trd>leny&udblac;g&ouml;z&odblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>groots</trd>,
    <trd>schitterend</trd>,
    <trd>grandioos</trd>,
    <trd>enorm</trd>,
    <trd>reusachtig</trd>,
    <trd>prachtig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wspania&lstroke;y</trd>,
    <trd>okaza&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">grandioso<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_a;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mohutn&yacute;</trd>,
    <trd>skvel&yacute;</trd>,
    <trd>ve&lcaron;kolep&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="grandi.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Tro&scirc;pare, nenoble malgranda:
      <ekz>
        mal<tld/>a festo;
      </ekz>
      <ekz>
        mal<tld/>aj anta&ubreve;ju&gcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>miserable</trd>,
    <trd>esquifit</trd>,
    <trd>escarransit</trd>
  </trdgrp>
  <trd lng="en">squalid</trd>
  <trdgrp lng="fr">
    <trd>mesquin<klr tip="ind"> (minable)</klr></trd>,
    <trd>minable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sek&eacute;lyes</trd>,
    <trd>hitv&aacute;ny</trd>,
    <trd>trivi&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>deerniswekkend</trd>,
    <trd>erbarmelijk</trd>,
    <trd>beklagenswaardig</trd>,
    <trd>miserabel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mizerny</trd>,
    <trd>skromny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_z;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
