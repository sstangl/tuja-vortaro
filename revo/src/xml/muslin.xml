<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: muslin.xml,v 1.19 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>muslin</rad>/o
</kap>

<drv mrk="muslin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="muslin.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Delikata maldensa kaj malpeza <ref tip="super"
cel="sxtof.0o.TEKS">&scirc;tofo</ref> el kotono, lano a&ubreve; silko:
      <ekz>
        en unu angulo staris malgranda lito sub kurteno <tld/>a
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 8a, p.
        38a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_mol;&c_l;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_m;&c_u;&c_s;&c_je;&c_l;&c_i;&c_n;</trd>
  <trd lng="ca">mussolina</trd>
  <trd lng="cs">mušelín</trd>
  <trd lng="de">Musselin</trd>
  <trd lng="en">muslin</trd>
  <trd lng="fr">mousseline</trd>
  <trd lng="hu">muszlin</trd>
  <trd lng="nl">mousseline</trd>
  <trdgrp lng="pl">
    <trd>mu&sacute;lin</trd>,
    <trd>szyfon</trd>
  </trdgrp>
  <trd lng="pt">musselina <klr tip="amb">(t&ecirc;xt.)</klr></trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">mušelín</trd>
</drv>
</art>
