<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sterku.xml,v 1.10 2013/08/25 16:11:19 revo Exp $">
<kap>
  <rad>sterkuliac</rad>/oj
</kap>
<drv mrk="sterku.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      tropikaj plantoj (<trd lng="la">Sterculiaceae</trd>).
    </dif>
    <ref tip="sub" cel="kaka.0ujo">kakaujo</ref>
  </snc>
  <trd lng="de">Sterkuliengew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>kaka&oacute;f&eacute;l&eacute;k</trd>,
    <trd>kaka&oacute;faf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">colaboomfamilie</trd>
  <trd
lng="ru">&c_s;&c_t;&c_je;&c_r;&c_k;&c_u;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
