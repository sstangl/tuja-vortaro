<kap>
<ofc>1</ofc>
<rad>epiderm</rad>/o
</kap>
<drv mrk="epider.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<snc>
<dif>
Ekstera preska&ubreve; travidebla tavoleto de la ha&ubreve;to.  
</dif>
</snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_r;&c_m;&c_ib;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>epiderm</trd>,
    <trd>epidermis</trd>,
    <trd>pokožka</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Oberhaut</trd>,
     <trd>Epidermis<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">epidermis</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_w;&f_p;&a_w;&a_s;&a_t;</trd>,
    <trd>&a_b;&a_fatha;&a_sh;&a_fatha;&a_r;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;piderme</trd>
  <trdgrp lng="hu">
    <trd>b&odblac;rh&aacute;m</trd>,
    <trd>felh&aacute;m</trd>,
    <trd>epidermisz</trd>
  </trdgrp>
  <trd lng="it">epidermide</trd>
  <trd lng="pl">nask&oacute;rek</trd>
  <trd lng="pt">epiderme</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_p;&c_i;&c_d;&c_je;&c_r;&c_m;&c_i;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j; &c_s;&c_l;&c_o;&c_j; &c_k;&c_o;&c_zh;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">pokožka</trd>
</drv>
</art>
