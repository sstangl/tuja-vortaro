<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skvam.xml,v 1.27 2016/04/11 20:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skvam</rad>/o
</kap>
<drv mrk="skvam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skvam.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la kornecaj lamenetoj, kiuj tegas la korpon de plej multaj
      fi&scirc;oj kaj rampuloj:
      <ekz>
        belaj fi&scirc;oj kun ar&gcirc;entaj kaj oraj <tld/>oj na&gcirc;is
post la boato
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Mardo.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">schub</trd>
  </snc>
  <snc mrk="skvam.0o.FIG">
    <dif>
      Io similanta <tld/>on:
      <ekz>
        &ccirc;e kelkaj vegeta&jcirc;oj <tld/>oj anstata&ubreve;as foliojn;
      </ekz>
      <ekz>
        skarlatinaj <tld/>oj de la ha&ubreve;to;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ho, se la <tld/>oj defalus de viaj okuloj<fnt>Z</fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        per <tld/>ita kiraso li estis vestita
        <fnt><bib>MT</bib>, <lok>&Sa1; 17:5</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">esquama <klr>(vegetals)</klr></trd>
    <trd lng="nl">schilfer</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_v;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_ju;&c_s;&c_p;&c_a;</trd>
  <trd lng="ca">escata</trd>
  <trdgrp lng="cs">
    <trd>z&aacute;voj</trd>,
    <trd>&scaron;upina</trd>
  </trdgrp>
  <trd lng="de">Schuppe</trd>
  <trd lng="en">scale<klr> (of fish, etc)</klr></trd>
  <trd lng="es">escama</trd>
  <trd lng="fr">&eacute;caille</trd>
  <trd lng="hu">pikkely</trd>
  <trd lng="pl">&lstroke;uska</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_sh;&c_u;&c_ja;</trd>,
    <trd>&c_ch;&c_je;&c_sh;&c_u;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;upina <klr>(na ryb&aacute;ch)</klr></trd>
  <trd lng="sv">fj&auml;ll<klr tip="amb"> (p&aring; fiskar t.ex.)</klr></trd>
</drv>

<drv mrk="skvam.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="skvam.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref>
      de <ref tip="malprt" cel="reptil.0oj">reptilioj</ref>
      (<trd lng="la">Squamata</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_ju;&c_s;&c_p;&c_je;&c_s;&c_t;&c_i;</trd>
  <trd lng="ca">escamosos</trd>
  <trd lng="de">Schuppenkriechtiere</trd>
  <trd lng="fr">squamates</trd>
  <trd lng="hu">pikkelyes <ind>h&uuml;ll&odblac;k</ind></trd>
  <trd lng="nl">squamata</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;uskosk&oacute;re</trd>,
    <trd>&lstroke;uskono&sacute;ne</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_sh;&c_u;&c_j;&c_ch;&c_a;&c_t;&c_y;&c_je; <klr
tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
