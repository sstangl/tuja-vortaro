<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monume.xml,v 1.19 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>monument</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="monume.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="monume.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="konstr.0ajxo.domo">Konstrua&jcirc;o</ref> grava, 
      impona, ofte memoriga pri iu a&ubreve; io:
      <ekz>
        tiuj kavernoj estas interesaj <tld/>oj de la anta&ubreve;historia
        pratempo;
      </ekz>
      <ekz>
        Partenono estas la plej admirinda <tld/>o en Ateno;
      </ekz>
      <ekz>
        <tld/>a arkitekturo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sign">memorsigno</ref>,
      <ref cel="statu.0o">statuo</ref>
    </refgrp>
  </snc>
  <snc mrk="monume.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Da&ubreve;re grava verko:
      <ekz>
        tiu libro estas senkompara scienca <tld/>o;
      </ekz>
      <ekz>
        Iliado estas eble la plej bela literatura <tld/>o de la
        antikveco;
      </ekz>
      <ekz>
        per sia verko Zamenhof konstruis al si mem neforigeblan 
        <tld/>on en la historio de la homara evoluo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_o;&c_n;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">monument</trd>
  <trdgrp lng="cs">
    <trd>monument</trd>,
    <trd>památnost</trd>,
    <trd>pomník</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monument</trd>,
    <trd>Denkmal</trd>
  </trdgrp>
  <trd lng="en">monument</trd>
  <trd lng="es">monumento</trd>
  <trd lng="fr">monument</trd>
  <trd lng="id">monumen</trd>
  <trd lng="nl">monument</trd>
  <trd lng="pl">pomnik</trd>
  <trd lng="pt">monumento</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pamätník</trd>,
    <trd>pomník</trd>
  </trdgrp>
</drv>

<drv mrk="monume.tombo0o">
  <kap>tombo<tld/>o, <var><kap>tomba <tld/>o</kap></var></kap>
  <snc mrk="monume.tombo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Luksa <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref> 
      &ccirc;irka&ubreve; la <ref tip="dif" cel="tomb.0o">tombo</ref> de 
      mortinto:
      <ekz>
        tio estas la tomba <tld/>o de Ra&hcirc;el &gcirc;is la nuna tago
        <fnt><bib>MT</bib>, <lok>&Gen; 35:20</lok></fnt>;
      </ekz>
      <ekz>
        la filmo montras la tombo<tld/>on de Mwl Nyerere
        <fnt>
          <aut>N. Vessella</aut>,
          <vrk><url ref="http://www.vessella.it/video.htm">
          Changamano</url></vrk>,
          <lok>1996-1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mauxzo.0o.ARKI">ma&ubreve;zoleo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_w;&c_z;&c_a;&c_l;&c_je;&c_j;</trd>
  <trd lng="bg">&c_m;&c_a;&c_v;&c_z;&c_o;&c_l;&c_je;&c_j;</trd>
  <trd lng="br">mozoleon</trd>
  <trd lng="ca">mausoleu</trd>
  <trd lng="de">Mausoleum</trd>
  <trd lng="en">mausoleum</trd>
  <trd lng="es">mausoleo</trd>  
  <trd lng="fr">mausol&eacute;e</trd>
  <trd lng="hu">mauz&oacute;leum</trd>
  <trd lng="id">mausoleum</trd>
  <trd lng="nl">mausoleum</trd>
  <trdgrp lng="pl">
    <trd>pomnik <klr>(pot.)</klr></trd>,
    <trd>nagrobek</trd>
  </trdgrp>
  <trd lng="pt">mausol&eacute;u</trd>
  <trd lng="ru">&c_m;&c_a;&c_v;&c_z;&c_o;&c_l;&c_je;&c_j;</trd>
</drv>
</art>
