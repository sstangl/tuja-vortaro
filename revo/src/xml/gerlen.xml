<art mrk="$Id: gerlen.xml,v 1.12 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gerlen</rad>/o
</kap>
<drv mrk="gerlen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;nurego konsistanta el kelkaj <ref tip="prt"
      cel="hauxser.0o">ha&ubreve;seroj</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_u;&c_k;&c_s;&c_i;&c_r;&c_n;&c_o; &c_v;&c_malmol;&c_zh;&c_je;</trd>
  <trd lng="cs">táhlo</trd>
  <trdgrp lng="de">
    <trd>Greling</trd>,
    <trd>Ankerseil</trd>,
    <trd>kleines Kabeltau</trd>
  </trdgrp>
  <trd lng="en">hawser</trd>
  <trd lng="fr">grelin</trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;k&ouml;t&eacute;l</trd>,
    <trd>kik&ouml;t&odblac;k&ouml;t&eacute;l</trd>
  </trdgrp>
  <trd lng="nl">tros<klr tip="ind"> (zeewezen)</klr></trd>
  <trdgrp lng="pt">
   <trd>calabre</trd>, 
   <trd>amarra</trd> 
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_j;&c_n;&c_o;&c_j; &c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>prívesný</trd>,
    <trd>tiahlo</trd>,
    <trd>závesný povraz</trd>
  </trdgrp>
</drv>
</art>
