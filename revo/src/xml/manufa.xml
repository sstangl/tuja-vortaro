<kap>
  <ofc>2</ofc>
  <rad>manufaktur</rad>/o
</kap>
<drv mrk="manufa.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Granda laborejo, kie oni produktas pogrande komerca&jcirc;ojn el krudaj
      materialoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="fabrik.0o">fabriko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_u;&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>                   
  <trd lng="br">labouradeg</trd>
  <trdgrp lng="ca">
    <trd>manufactura</trd>,
    <trd>f&agrave;brica</trd>
  </trdgrp>
  <trd lng="de">Manufaktur</trd>
  <trdgrp lng="en">
    <trd>factory</trd>,
    <trd>manufactory</trd>,
    <trd>works</trd>
  </trdgrp>
  <trdgrp lng="fr">
  <trd>manufacture</trd>,
  <trd>fabrique</trd>
</trdgrp>
  <trd lng="hu">manufakt&uacute;ra</trd>
  <trd lng="nl">fabriek</trd>
  <trdgrp lng="pt">
    <trd>manufatura</trd>,
    <trd>f&aacute;brica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_u;&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
