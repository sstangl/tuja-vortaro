<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pedago1.xml,v 1.17 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>pedagogi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pedago1.0o">
  <kap><tld/>o</kap>
  <snc mrk="pedago1.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Scienco pri instruado kaj edukado,
      arto instrui kaj eduki precipe infanojn;
      <ref tip="sin" cel="didakt.0o.PED">didaktiko</ref>:
      <ekz>
        lernejo, kiu edukas la&ubreve; Steiner-<tld/>o, malfermis siajn
        pordojn por senpaperuloj
        <fnt><aut>G. Van Damme</aut>: <vrk>Sep en unu
        bato</vrk>, <bib>Monato</bib>, <lok>1999:3, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">pedagogia</trd>
  <trdgrp lng="cs">
    <trd>pedagogie</trd>,
    <trd>pedagogika</trd>,
    <trd>vychovatelství</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>P&auml;dagogig</trd>,
    <trd>Didaktik</trd>
  </trdgrp>
  <trd lng="en">pedagogy</trd>
  <trdgrp lng="fr">
    <trd>didactique<klr> (subst.)</klr></trd>,
    <trd>p&eacute;dagogie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pedag&oacute;gia</trd>,
    <trd>nevel&eacute;stan</trd>,
    <trd>nevel&eacute;studom&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkunde</trd>,
    <trd>pedagogie</trd>
  </trdgrp>
  <trd lng="pt">pedagogia</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">pedagogika</trd>
</drv>

<drv mrk="pedago1.0a">
  <kap><tld/>a</kap>
  <snc mrk="pedago1.0a.PED">
    <ref tip="dif" cel="instru.pri0a">priinstrua</ref>
    <ekz>
      tro malofte oni kritike konsideras ties enhavon, aspekton, valoron
      &ndash; sciencan, <tld/>an, lingvan
      <fnt>
      <aut>W. M. A. De Smet</aut>,
      <vrk>Ekzotaj arboj en Esperantujo</vrk>,
      <bib>Monato</bib>,
      <lok>1997:10, p. 19a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_d;&c_a;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">pedag&ograve;gic</trd>
  <trdgrp lng="cs">
    <trd>pedagogický</trd>,
    <trd>výchovný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>p&auml;dagogisch</trd>,
    <trd>didaktisch</trd>
  </trdgrp>
  <trd lng="en">pedagogical</trd>
  <trd lng="es">did&aacute;ctico</trd>
  <trdgrp lng="fr">
    <trd>didactique</trd>,
    <trd>p&eacute;dagogique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pedag&oacute;giai</trd>,
    <trd>nevel&eacute;stani</trd>,
    <trd>nevel&eacute;studom&aacute;nyi</trd>
  </trdgrp>
  <trd lng="nl">didactisch</trd>
  <trdgrp lng="sk">
    <trd>pedagogický</trd>,
    <trd>výchovný</trd>
  </trdgrp>
</drv>

<drv mrk="pedago1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pedago1.0isto.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Sciencisto studanta la <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g; <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_je;&c_c;)</klr></trd>
  <trd lng="ca">pedagog</trd>
  <trd lng="cs">pedagog</trd>
  <trdgrp lng="de">
    <trd>P&auml;dagoge</trd>
  </trdgrp>
  <trd lng="en">pedagogue</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;dagogue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nevel&eacute;studom&aacute;nyi szakember</trd>,
    <trd>pedag&oacute;gus <klr tip="amb">(tud&oacute;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;
  <klr>(&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">pedagóg</trd>
</drv>
</art>
