<kap>
  <rad>sublim</rad>/a
  <fnt>N</fnt>
</kap>
<drv mrk="sublim.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Belega, superbela.
    </dif>
    <trdgrp lng="de">
      <trd>sublim</trd>,
      <trd>gro&szlig;artig</trd>
    </trdgrp>
    <trd lng="fr">sublime</trd>
    <trdgrp lng="hu">
      <trd>gy&ouml;ny&ouml;r&udblac;</trd>,
      <trd>k&aacute;pr&aacute;zatos</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Noblega, supernobla.
    </dif>
    <trdgrp lng="de">
      <trd>sublim</trd>,
      <trd>majest&auml;tisch</trd>,
      <trd>erhaben</trd>
    </trdgrp>
    <trdgrp lng="fr">
    <trd>noble</trd>,
    <trd>majestueux</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fens&eacute;ges</trd>,
      <trd>magasztos</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_mol;&c_n;&c_jo;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>grandiózní</trd>,
    <trd>skvělý</trd>,
    <trd>vznešený</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>subliem</trd>,
    <trd>verheven</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_d;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>grandiózny</trd>,
    <trd>skvelý</trd>,
    <trd>vznešený</trd>,
    <trd>šľachtený</trd>
  </trdgrp>
</drv>
