<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gajl.xml,v 1.19 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gajl</rad>/o
</kap>
<drv mrk="gajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Tubero kreskanta sur la folioj de kelkaj vegeta&jcirc;oj,
      precipe de kverko kaj rozujo, kaj ka&ubreve;zita de piko de insekto:
      <ekz>
        el <tld/>oj <klr>(kverkaj)</klr> oni produktas inkon
        <fnt><bib>VdE</bib>, <lok></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cassanella</trd>,
    <trd>gala</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>duběnka</trd>,
    <trd>hálka</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>Galle</trd>,
   <trd>Gallapfel</trd></trdgrp>
  <trdgrp lng="en">
    <trd>gall</trd>,
    <trd>oak-apple</trd>,
    <trd>oak apple</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_A;&a_z;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>galle</trd>,
    <trd>noix de galle</trd></trdgrp>
  <trd lng="hu">gubacs</trd>
  <trdgrp lng="nl">
    <trd>gal<klr tip="ind"> (galnoot)</klr></trd>,
    <trd>galnoot</trd>,
    <trd>galappel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>(noz-de-)galha</trd>,
    <trd>bugalho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">dubienka <klr>(bot)</klr></trd>
  <trd lng="sv">gall&auml;pple</trd>
</drv>
</art>
