<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: efekti.xml,v 1.32 2016/02/27 21:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>efektiv</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="efekti.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="efekti.0a.KOMUNE">
    <dif>
      Reale ekzistanta, estanta ne nur &scirc;ajna:
      <ekz>
        en okazo de <tld/>a neceso;
      </ekz>
      <ekz>
        vi faris al mi <tld/>an surprizon;
      </ekz>
      <ekz>
        li estas <tld/>a <klr>(funkcianta)</klr> generalo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fakt.0a">fakta</ref>,
      <ref cel="real.0a">reala</ref>,
      <ref cel="ver.0a">vera</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_ib;&c_s;&c_n;&c_u;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_r;&c_e;&c_ch;&c_a;&c_ib;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>efectiu</trd>,
    <trd>tangible</trd>,    
    <trd>veritable</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efektivn&iacute;</trd>,
    <trd>re&aacute;ln&yacute;</trd>,
    <trd>skute&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirklich</trd>,
    <trd>tats&auml;chlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>real</trd>,
    <trd>actual</trd>,
    <trd>true</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>efectivo</trd>,
     <trd>eficaz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>effectif<klr tip="ind"> (adj.)</klr></trd>,
    <trd>r&eacute;el</trd>,
    <trd>v&eacute;ritable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;nyleges</trd>,
    <trd>val&oacute;s&aacute;gos</trd>,
    <trd>effekt&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">werkelijk</trd>
  <trdgrp lng="pl">
    <trd>prawdziwy</trd>,
    <trd>faktyczny</trd>,
    <trd>rzeczywisty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>efetivo</trd>,
    <trd>real</trd>,
    <trd>verdadeiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>efekt&iacute;vny</trd>,
    <trd>skuto&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="sv">verklig</trd>
</drv>

<drv mrk="efekti.0e">
  <kap><tld/>e</kap>
  <snc mrk="efekti.0e.KOMUNE">
    <dif>
      Verfakte, reale:
      <ekz>
        mi dubas, &ccirc;u li estas <tld/>e rajtigita.
      </ekz>
    </dif>
    <trd lng="ca">en efecte</trd>
    <trdgrp lng="pl">
      <trd>prawdziwie</trd>,
      <trd>faktycznie</trd>,
      <trd>rzeczywi&sacute;cie</trd>,
      <trd>w&lstroke;a&sacute;ciwie</trd>
    </trdgrp>
  </snc>
  <snc mrk="efekti.0e.ARG">
    <dif>
      Kiel montras la fakto ke...,
      kion pruvas tio ke...:
      <!--
      anta&ubreve;a difino:
      Adverbo anoncanta, ke la sekvanta frazo konfirmas
      a&ubreve; motivas la anta&ubreve;an &jcirc;usan aserton
      -->
      <ekz>
        Volap&uuml;k ne povis sukcesi,
        <tld/>e &gcirc;i postulis tro grandan penadon por la memoro
        <fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxar.0">&ccirc;ar</ref>,
      <ref cel="pruv.0e">pruve</ref>
    </refgrp>
    <trd lng="ca">de fet</trd>
    <trdgrp lng="pl">
      <trd><klr>na </klr>serio</trd>,
      <trd>w rzeczywisto&sacute;ci</trd>,
      <trd>na <ind>dobre</ind></trd>,
      <trd>zaiste</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_y;</trd>,
    <trd>&c_n;&c_a; &c_s;&c_a;&c_m;&c_o;&c_j;
    &c_s;&c_p;&c_r;&c_a;&c_v;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">efectivament</trd>
  <trdgrp lng="cs">
    <trd>doopravdy</trd>,
    <trd>efektivn&ecaron;</trd>,
    <trd>opravdu</trd>,
    <trd>skute&ccaron;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirklich</trd>,
    <trd>tats&auml;chlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>actually</trd>,
    <trd>in fact</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>effectivement</trd>,
    <trd>en <ind>effet</ind></trd>,
    <trd>r&eacute;ellement</trd>,
    <trd>vraiment</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;nylegesen</trd>,
    <trd>val&oacute;j&aacute;ban</trd>
  </trdgrp>
  <trd lng="nl">inderdaad</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_v; &c_s;&c_a;&c_m;&c_o;&c_m; &c_d;&c_je;&c_l;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>efekt&iacute;vne</trd>,
    <trd>skuto&ccaron;ne</trd>
  </trdgrp>
  <trd lng="sv">verkligen</trd>
</drv>

<drv mrk="efekti.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="efekti.0ajxo.KOMUNE">
    <dif>
      Ia reala&jcirc;o:
      <ekz>
        anstata&ubreve; niaj belaj revoj ni spertis
        teruran <tld/>a&jcirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fakt.0o">fakto</ref>,
      <ref cel="real.0a">reala</ref>,
      <ref cel="ver.0a">vera</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ch;&c_a;&c_ib;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>fet</trd>,
    <trd>realitat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tatsache</trd>,
    <trd>Wirklichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>reality</trd>,
    <trd>fact</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;alit&eacute;</trd>
  <trd lng="hu">val&oacute;s&aacute;g</trd>
  <trdgrp lng="pl">
    <trd>fakt</trd>,
    <trd>rzeczywisto&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="efekti.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="efekti.0eco.KOMUNE">
    <dif>
      Eco de tio, kio estas reala, fakte ekzistas:
      <ekz>
        en <tld/>eco
        <fnt><bib>MT</bib>, <lok>&Jeh; 33:18</lok></fnt>
        <klr>(<tld/>e)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_r;&c_e;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>facticitat</trd>,
    <trd>realitat</trd>,
    <trd>autenticitat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skute&ccaron;nost</trd>,
    <trd>&uacute;&ccaron;innost</trd>
  </trdgrp>
  <trd lng="de">Wirklichkeit</trd>
  <trdgrp lng="en">
    <trd>reality</trd>,
    <trd>actuality</trd>,
    <trd>truth</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mat&eacute;rialit&eacute;<klr tip="ind">
    (r&eacute;alit&eacute;)</klr></trd>,
    <trd>r&eacute;alit&eacute;<klr tip="ind">
    (mat&eacute;rialit&eacute;)</klr></trd></trdgrp>
  <trd lng="hu">realit&aacute;s</trd>
  <trd lng="nl">realiteit</trd>
  <trdgrp lng="pl">
    <trd>rzeczywisto&sacute;&cacute;</trd>,
    <trd>materialno&sacute;&cacute;</trd>,
    <trd>namacalno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>efekt&iacute;vnos&tcaron;</trd>,
    <trd>&uacute;&ccaron;innos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="efekti.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="efekti.0igi.KOMUNE">
    <dif>
      Realigi, <ref tip="vid" cel="plen.0umi">plenumi</ref>:
      <ekz>
        <tld/>igi intencon, ideon, planon la pacon;
      </ekz>
      <ekz>
        tio estas <ind>ne<tld/>igebla</ind> utopio<fnt>Z</fnt>
        <trd lng="ca">irrealitzable</trd>
        <trdgrp lng="de">
          <trd>nicht <ind>machbar</ind></trd>,
          <trd>unmachbar</trd>,
          <trd>nicht <ind>realisierbar</ind></trd>,
          <trd>unrealisierbar</trd>
        </trdgrp>
        <trd lng="fr">irr&eacute;alisable</trd>
        <trd lng="hu">megval&oacute;s&iacute;thatatlan</trd>
        <trdgrp lng="pl">
          <trd>nieziszczalny</trd>,
          <trd>nieosi&aogonek;galny</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ating.0i">atingi</ref>,
      <ref cel="akir.0i">akiri</ref>,
      <ref cel="far.el0i">elfari</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_w;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dur a terme</trd>,
    <trd>acomplir</trd>,
    <trd>realitzar</trd>,
    <trd>efectuar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podniknout</trd>,
    <trd>prov&eacute;st</trd>,
    <trd>splnit</trd>,
    <trd>uskute&ccaron;nit</trd>,
    <trd>vykonat</trd>
  </trdgrp>
  <trd lng="de">verwirklichen</trd>
  <trdgrp lng="en">
    <trd>realize</trd>,
    <trd>accomplish</trd>,
    <trd>bring about</trd>
  </trdgrp>
  <trd lng="es">tener efectividad</trd>
  <trdgrp lng="fr">
    <trd>effectuer</trd>,
    <trd>mat&eacute;rialiser</trd>,
    <trd>ex&eacute;cuter</trd>,
    <trd>r&eacute;aliser<klr tip="ind"> (effectuer)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>megval&oacute;s&iacute;t</trd>,
    <trd>realiz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">realiseren</trd>
  <trdgrp lng="pl">
    <trd>dokonywa&cacute;</trd>,
    <trd>realizowa&cacute;</trd>,
    <trd>zrealizowa&cacute;</trd>,
    <trd>osi&aogonek;ga&cacute;</trd>,
    <trd>sprawia&cacute;</trd>,
    <trd>urzeczywistnia&cacute;</trd>,
    <trd>uskutecznia&cacute;</trd>,
    <trd>zdzia&lstroke;a&cacute;</trd>,
    <trd>ziszcza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>splni&tcaron;</trd>,
    <trd>uskuto&ccaron;ni&tcaron;</trd>,
    <trd>vykona&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rverkliga</trd>
</drv>

<drv mrk="efekti.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="efekti.0igxi.KOMUNE">
    <dif>
      Reali&gcirc;i, <ref tip="vid" cel="plen.0umigxi">plenumi&gcirc;i</ref>:
      <ekz>
        la kongreso fine <tld/>i&gcirc;os
        tiun &ccirc;i jaron;
      </ekz>
      <ekz>
        la bela revo de l' homaro ne tuj <tld/>i&gcirc;os;
      </ekz>
      <ekz>
        kiel hirundo forflugas, tiel senka&ubreve;za malbeno ne
        <tld/>i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Sen; 26:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>esdevindre's</trd>,
    <trd>realitzar-se</trd>,
    <trd>oc&oacute;rrer, tenir lloc</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr tip="amb">sich </klr>verwirklichen</trd>,
    <trd>wahr <ind>werden</ind></trd>,
    <trd>sich <ind>erf&uuml;llen</ind></trd>,
    <trd>wirksam <ind>werden</ind></trd>,
    <trd>zum Einsatz <ind>kommen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>come to pass</trd>,
    <trd>come true</trd>
  </trdgrp>
  <trd lng="es">ser efectivo</trd>
  <trdgrp lng="fr">
    <trd>avoir <ind>lieu</ind></trd>,
    <trd>se <ind>r&eacute;aliser</ind></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>megval&oacute;sul</trd>,
    <trd>realiz&aacute;l&oacute;dik</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>werkelijkheid worden</trd>,
    <trd>in <ind>vervulling</ind> gaan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dokonywa&cacute; si&eogonek;</trd>,
    <trd>realizowa&cacute; si&eogonek;</trd>,
    <trd>zrealizowa&cacute; si&eogonek;</trd>,
    <trd>urzeczywistnia&cacute; si&eogonek;</trd>,
    <trd>uskutecznia&cacute; si&eogonek;</trd>,
    <trd>ziszcza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rverkligas</trd>
</drv>
</art>
