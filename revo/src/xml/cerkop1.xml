  <rad>cerkopiteked</rad>/oj
</kap>
<drv mrk="cerkop1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="simi.0oj">simioj</ref>
      (<trd lng="la">Cercopithecidae</trd>).
    </dif>
  </snc>
  <trd
  lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
