  <trd lng="ca">cercopit&egrave;cids</trd>
  <trd lng="nl">meerkat</trd>
  <trdgrp lng="de">
    <trd>Meerkatzen</trd>,
    <trd>Hundsaffen</trd>
  </trdgrp>
  <trd lng="en">old world <ind>monkeys</ind></trd>
  <trd lng="fr">cercopith&egrave;que</trd>
  <trd lng="hu">cerk&oacute;fmajomf&eacute;l&eacute;k</trd>
  <trd lng="it">cercopitecini</trd>
  <trdgrp lng="pl">
    <trd>w&aogonek;skonose<klr> (nadrodzina)</klr></trd>,
    <trd>ma&lstroke;py Starego &Sacute;wiata<klr>
    (nadrodzina)</klr></trd>,
    <trd>makakowate<klr> (rodzina)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
