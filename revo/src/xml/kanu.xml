<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanu.xml,v 1.7 2015/07/04 08:37:36 revo Exp $">
<kap>
    <rad>kanu</rad>/o
    <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kanu.0o">
  <kap><tld/>o</kap>
   <snc mrk="kanu.0o.SHI">
     <uzo tip="fak">SHI</uzo>
     <uzo tip="fak">SPO</uzo>
     <dif>
       Malpeza <ref tip="super" cel="boat.0o">boato</ref> kun rigida
       &scirc;elo,
       kondukata per simpla <ref tip="vid" cel="pagaj.0o">pagajo</ref>
       kiajn
       tradicie uzis indianoj.  En <tld/>o oni a&ubreve; <ref
       cel="genu.0i">genuas</ref>
       a&ubreve; sidas sur benko,
     </dif>
     <ref tip="vid" cel="kajak.0o.SHI">kajako</ref>
     <trd lng="be">&c_k;&c_a;&c_n;&c_o;&c_e;</trd>
     <trd lng="bg">&c_k;&c_a;&c_n;&c_u;</trd>
     <trd lng="ca">canoa</trd>
     <trd lng="da">kano</trd>
     <trd lng="de">Kanu</trd>
     <trd lng="en">canoe</trd>
     <trd lng="fr">cano&euml;</trd>
     <trd lng="hr">kanu</trd>
     <trd lng="hu">kenu</trd>
     <trd lng="id">kano</trd>
     <trd lng="nl">kano</trd>
     <trdgrp lng="pl">
       <trd>kanadyjka</trd>,
       <trd>kanu</trd>,
       <trd>canoe</trd>
     </trdgrp>
     <trd lng="sv">kanot</trd>
     <trd lng="tr">kano</trd>
   </snc>
  <trd lng="cs">kanoe</trd>
  <trd lng="sk">kanoe</trd>
</drv>
</art>

