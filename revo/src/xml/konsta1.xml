<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konsta1.xml,v 1.12 2006/11/30 18:43:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konstat</rad>/i
</kap>
<drv mrk="konsta1.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rekoni fakton kiel senduban:
      <ekz>
        la kuracisto povis nur <tld/>i la morton<fnt>B</fnt>;
      </ekz>
      <ekz>
        la protokolo <tld/>as, ke li forestis en la momento de la
        krimo;
      </ekz>
      <ekz>
        estas <tld/>ite, ke ...
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_s;&c_t;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">constatar</trd>
  <trdgrp lng="de">
     <trd>feststellen</trd>,
     <trd>konstatieren</trd>,
     <trd>best&auml;tigen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>acertain to be true <klr>by experiment or
    investigation</klr></trd>,
    <trd>establish to be a fact</trd>
  </trdgrp>
  <trd lng="es">constatar</trd>
  <trd lng="fr">constater</trd>
  <trd lng="nl">constateren</trd>
  <trdgrp lng="pt">
    <trd>constatar</trd>,
    <trd>verificar</trd>,
    <trd>reconhecer</trd>,
    <trd>achar</trd>,
    <trd>averiguar</trd>,    
    <trd>apurar</trd>            
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_t;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
