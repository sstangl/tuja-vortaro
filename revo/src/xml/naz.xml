<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: naz.xml,v 1.38 2015/07/04 08:37:40 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>naz</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="naz.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="naz.0o.homo">
        <uzo tip="fak">ANA</uzo>
        <dif>
          Flarorgano elstaranta el la kapo inter frunto kaj bu&scirc;o:
          <ekz>
            rekta, kurba <tld/>o;
          </ekz><ekz>
            <tld/>loboj;
          </ekz><ekz>
            purigi la <tld/>on;
          </ekz><ekz>
            <tld/>e, tra la <tld/>o paroli;
          </ekz><ekz>
            <tld/>asona vo&ccirc;o<fnt>Z</fnt>;
          </ekz><ekz>
            <ref cel="naz.0sono"><tld/>aj parolsonoj</ref>,
          </ekz><ekz>
            <ref cel="naz.0avokalo"><tld/>aj vokaloj</ref>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>ne &scirc;ovu la <tld/>on en fremdan vazon</ind>
            <trd lng="fr">m&ecirc;le-toi de ce qui te regarde</trd>
            <trd lng="ru">&c_n;&c_je; &c_s;&c_u;&c_j; &c_s;&c_v;&c_o;&c_j; &c_n;&c_o;&c_s; &c_v; &c_ch;&c_u;&c_zh;&c_o;&c_j; &c_v;&c_o;&c_p;&c_r;&c_o;&c_s;</trd>
            <klr>(ne enmiksu vin en aferojn, kiuj vin ne koncernas.
              <refgrp tip="vid">
                <ref cel="en.0igxi">eni&gcirc;i</ref>,
                <ref cel="zorg.0i">zorgi</ref>
              </refgrp>)</klr>
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            <ind>vidi nur &gcirc;is la pinto de sia <tld/>o</ind>
            <trd lng="fr">ne pas voir plus loin que le bout de son
              nez</trd>
            <klr>(esti tute nekapabla anta&ubreve;vidi kaj anta&ubreve;zorgi.
              <ref tip="vid" cel="miop.0a">miopa</ref>)</klr>
            <fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            rigardu la faktojn, kiuj sin trovas anta&ubreve; via
            <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            mi havis bonan <tld/>on<fnt>Z</fnt>
            <klr>(mi &gcirc;uste divenis, konjektis, anta&ubreve;suspektis)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            fari longan <tld/>on<fnt>Z</fnt>,
            foriri kun longa <tld/>o<fnt>Z</fnt>
            <klr>(montri konsternitan mienon pro elrevi&gcirc;o,
              ne atinginte la esperitan rezulton)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            teni la <tld/>on supren
            <klr>(esti arogante fiera)</klr>
            <fnt>Z</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            konduki iun per<fnt>B</fnt>, &ccirc;e, je la nazo<fnt>Z</fnt>
            <klr>(la&ubreve; sia arbitro,
            <ref tip="vid"
            cel="laux.0.io_obeata">la&ubreve;<sncref/></ref>)</klr>.
          </ekz>
        </dif>
        <trd lng="bg">&c_n;&c_o;&c_s;</trd>
        <trd lng="ca">nas</trd>
        <trd lng="el">&my;&ypsilon_ton;&tau;&eta;</trd>
        <trd lng="fr">nez</trd>
        <trd lng="pt">nariz</trd>
        <trd lng="sv">n&auml;sa</trd>
      </snc>
      <snc mrk="naz.0o.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <ref tip="dif" cel="naz.0ego.ZOO"><tld/>ego</ref>
        <trd lng="ca">musell</trd>
        <trd lng="fr">museau</trd>
        <trd lng="pt">focinho</trd>
        <trd lng="sv">nos</trd>
      </snc>
      <snc mrk="naz.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Anta&ubreve;a, elstara parto de objekto pli malpli simila al <tld/>o;
          <ref tip="vid" cel="bek.0o.FIG">beko<sncref/></ref>:
          <ekz>
            &scirc;ip<tld/>o
            <klr>(anta&ubreve;a&jcirc;o, pruo)</klr>;
          </ekz><ekz>
            <tld/>o de plugilo;
          </ekz><ekz>
            <tld/>eto de kru&ccirc;o.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>punta</trd>,
          <trd>sortint</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>nez <klr>(d'un objet)</klr></trd>,
          <trd>pointe</trd>
        </trdgrp>
        <trdgrp lng="it">
          <trd>becco<klr> (di oggetto)</klr></trd>,
          <trd>beccuccio</trd>
        </trdgrp>
        <trd lng="pt">nariz</trd>
        <trd lng="sv">nos</trd>
      </snc>
  <trd lng="be">&c_n;&c_o;&c_s;</trd>
  <trd lng="bg">&c_n;&c_o;&c_s;</trd>
  <trd lng="br">fri</trd>
  <trd lng="ca">nas</trd>
  <trd lng="cs">nos</trd>
  <trd lng="de">Nase</trd>
  <trd lng="en">nose</trd>
  <trd lng="es">nariz</trd>
  <trd lng="hu">orr</trd>
  <trd lng="id">hidung</trd>
  <trd lng="it">naso</trd>
  <trd lng="nl">neus</trd>
  <trd lng="pl">nos</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;</trd>
  <trd lng="sk">nos</trd>
  <trd lng="tr">burun</trd>
</drv>

    <drv mrk="naz.0a">
      <kap><tld/>a</kap>
      <snc mrk="naz.0a.gxenerale">
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <uzo tip="fak">ANA</uzo>
            la <tld/>aj kavoj.
          </ekz>
        </dif>
      </snc>
      <snc mrk="naz.0a.FON">
        <uzo tip="fak">FON</uzo>
        <dif>
          <ref cel="naz.0sono">Nazsona</ref>:
          <ekz>
            <ref cel="naz.0akonsonanto"><tld/>a konsonanto</ref>,
          </ekz><ekz>
            <ref cel="naz.0avokalo"><tld/>a vokalo</ref>.
          </ekz>
        </dif>
        <ref tip="ant" cel="busx.0a.FON">bu&scirc;a<sncref/></ref>
      </snc>
  <trdgrp lng="cs">
    <trd>nosní</trd>,
    <trd>nosový</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_s;o&c_v;&c_o;&c_j;</trd>
  <trd lng="sk">nosný</trd>
</drv>

    <drv mrk="naz.0ego">
      <kap><tld/>ego</kap>
      <snc mrk="naz.0ego.ZOO">
        <dif>
          Dika <tld/>o de besto;
          <ref tip="sub" cel="muzel.0o.ZOO">muzelo</ref>:
          <ekz>
            <tld/>ego de porko
            <fnt>
              <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
              <vrk>Montara Vila&gcirc;o</vrk>,
              <lok>1984</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">musell</trd>
  <trdgrp lng="cs">
    <trd>čenich</trd>,
    <trd>čumák</trd>
  </trdgrp>
  <trdgrp lng="fr">
        <trd>groin</trd>,
        <trd>mufle</trd>
        <trd>museau</trd>
      </trdgrp>
  <trd lng="pt">focinho</trd>
  <trdgrp lng="ru">
        <trd>&c_n;&c_o;&c_s;&c_i;&c_shch;&c_je;</trd>,
        <trd>&c_r;&c_y;&c_l;&c_o;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>raťafák</trd>,
    <trd>ňufák</trd>
  </trdgrp>
  <trd lng="sv">nos</trd>
</drv>

    <drv mrk="naz.0umo">
      <kap><tld/>umo</kap>
      <snc mrk="naz.0umo.FOT">
        <uzo tip="fak">FOT</uzo>
        <dif>
          <ref tip="super" cel="vitr.okul0oj">Okulvitroj</ref> fiksitaj
          sur la <tld/>o per pin&ccirc;a risorto;
          <ref tip="sin" cel="pincx.naz0ilo">nazpin&ccirc;ilo</ref>:
          <ekz>
            li elprenis <tld/>umon el ora ingo, vi&scirc;is &gcirc;in per ne&gcirc;blanka 
            naztuko, metis &gcirc;in sur la nazon kaj kun vigle memkontenta 
            rideto &ccirc;irka&ubreve;rigardis &ccirc;iujn sidantojn
            <fnt>
              <aut>I. G. &Scirc;irjaev</aut>,
              <vrk>Sen titolo</vrk>, 
              <lok>[1898?]</lok></fnt>. 
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_n;&c_e;</trd>
  <trd lng="br">lunedo&ugrave;-fri</trd>
  <trd lng="ca">pin&ccedil;an&agrave;s</trd>
  <trdgrp lng="cs">
    <trd>cvikr</trd>,
    <trd>skřipec</trd>
  </trdgrp>
  <trd lng="de">Kneifer</trd>
  <trd lng="en">pince-nez</trd>
  <trd lng="fr">pince-nez</trd>
  <trdgrp lng="hu">
        <trd>cs&iacute;ptet&odblac;<klr tip="amb">(szem&uuml;vegfajta)</klr></trd>,
        <trd>cvikker</trd>
      </trdgrp>
  <trd lng="it">occhiali <ind>pince-nez</ind></trd>
  <trd lng="nl">neusknijper</trd>
  <trd lng="pl">binokle</trd>
  <trd lng="pt">pincen&ecirc;</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_n;&c_je;</trd>
  <trd lng="sk">cviker</trd>
</drv>

    <drv mrk="naz.alt0a">
      <kap>alt<tld/>a</kap>
      <uzo tip="stl">FRAZ</uzo>
      <snc mrk="naz.alt0a.FIG">
        <dif>
          Tro memfiera,
          <ref tip="vid" cel="arogan.0a">aroganta</ref>,
          <ref tip="vid" cel="modest.mal0a.PSI">malmodesta</ref>:
          <ekz>
            la pse&ubreve;dofilozofo estas <klr>...</klr>
            despota kaj alt<tld/>a 
            <fnt>
              <aut>V. O&scaron;lak</aut>,
              <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
              <lok>2003</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">pr&eacute;tentiard</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_z;&c_a;&c_d;&c_i;&c_r;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_n;&c_o;&c_s;</trd>
      </trdgrp>
    </drv>

    <drv mrk="naz.kusp0a">
      <kap>kusp<tld/>a</kap>
      <snc mrk="naz.kusp0a.ANA">
        <dif>
          Kies nazo (plejparte, mallonga) &scirc;ajnas turnita supren:
          <ekz>
            Sokrato estis kusp<tld/>a;
          </ekz><ekz>
            Ofelia Pepita estis <klr>(...)</klr> eble iomete tro
            kusp<tld/>a, sed ankora&ubreve; ne estas decidite, &ccirc;u tiu propreco
            estas makulo de beleco
            <fnt>
              <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>:
              <vrk><url
                  ref="http://www.freeweb.hu/verkoj/fredlamalpura.htm">
                  Fred la malpura intervenas</url></vrk>,
              <lok>2002</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">de nas arreveixinat</trd>
      <trd lng="en">snub-nosed</trd>
      <trd lng="fr">au <ind>nez en trompette</ind></trd>
      <trd lng="it">camuso</trd>
      <trd lng="ru">&c_k;&c_u;&c_r;&c_n;&c_o;&c_s;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="naz.stump0a">
      <kap>stump<tld/>a</kap>
      <snc mrk="naz.sxtump0a.kuspa">
        <uzo tip="stl">EVI</uzo>
        <ref tip="dif" cel="naz.kusp0a.ANA">kusp<tld/>a</ref>
        <rim>
          La metaforo surbaze de stump- estas sufi&ccirc;e internacia, tamen
          &gcirc;i estas evitinda &ccirc;ar aliaj similaj kombina&jcirc;oj ofte indikas
          rezulton de kripligo (stumpa vosto, orelo ktp).
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="it">camuso</trd>
      </snc>
  <trd lng="cs">s tupým nosem <klr>(adj.)</klr></trd>
  <trd lng="sk">s tupým nosom</trd>
</drv>

    <drv mrk="naz.0sono">
      <kap><tld/>sono</kap>
      <snc mrk="naz.0sono.FON">
        <uzo tip="fak">FON</uzo>
        <dif>
          <ref tip="super" cel="son.parol0o">Parolsono</ref> dum kies
          artikulacio la velo estas mallevita kaj interkomunikigas
          la bu&scirc;on kun la <tld/>a kavo:
          <ekz>
            la franca lingvo vere ne bezonas iri trans la
            pura<klr>n</klr>
            rimo<klr>n</klr>, &gcirc;ian rimadon ja enviinde helpas la
            <tld/>sonoj,
            kiuj englutas post si &ccirc;iujn konsonantojn
            <fnt>
              <aut>K. Kalocsay</aut>:
              <vrk>Kiel verki kaj traduki poemojn?</vrk>.
              <lok>Budapest: HEA, 1979.</lok>
            </fnt>.
          </ekz>
        </dif>
        <refgrp tip="sub">
          <ref cel="naz.0akonsonanto"><tld/>a konsonanto</ref>,
          <ref cel="naz.0avokalo"><tld/>a vokalo</ref>
        </refgrp>
      </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_v;&c_y; &c_g;&c_u;&c_k;</trd>
  <trd lng="ca">nasal <klr>(fon.)</klr></trd>
  <trd lng="cs">nosovka</trd>
  <trdgrp lng="de">
        <trd>Nasenlaut</trd>,
        <trd>Nasal</trd>
      </trdgrp>
  <trd lng="en">nasal</trd>
  <trd lng="es">nasal</trd>
  <trd lng="fr">nasale<klr> (subst., consonne ou
      voyelle)</klr></trd>
  <trdgrp lng="hu">
        <trd>orrhang</trd>,
        <trd>naz&aacute;lis</trd>
      </trdgrp>
  <trd lng="nl">neusklank</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  <trd lng="sk">nosovka</trd>
</drv>

    <drv mrk="naz.0akonsonanto">
      <kap><tld/>a konsonanto</kap>
      <snc>
        <uzo tip="fak">FON</uzo>
        <dif>
          <ref tip="super" cel="konson1.0o">Konsonanto</ref>
          kiu estas <ref tip="super" cel="naz.0sono"><tld/>sono</ref>:
          <ekz>
            du fonemoj esperantaj estas <tld/>aj konsonantoj:
            /m/ kaj /n/.
          </ekz><ekz>
            N estas denta <tld/>a konsonanto
            <fnt><bib>PMEG</bib>,
              <lok><url
 ref="http://bertilow.com/pmeg/skribo_elparolo/elparolo/bazaj_reguloj.html#i-nol"
                  >Konsonanta variado</url></lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">nasal consonant</trd>
      <trd lng="fr">consonne nasale</trd>
      <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska nosowa</trd>
      <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="naz.0avokalo">
      <kap><tld/>a vokalo</kap>
      <snc>
        <uzo tip="fak">FON</uzo>
        <dif>
          <ref tip="super" cel="vokal.0o">Vokalo</ref>
          kiu estas <ref tip="super" cel="naz.0sono"><tld/>sono</ref>:
          <ekz>
            en la malnovslava ekzistis <tld/>aj vokaloj...
            <fnt>
              <aut>Joh&aacute;n Valano</aut>:
              <vrk>&Ccirc;u ni kunvenis vane?</vrk>.
              <lok><url
 ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0042/0017.php"
                  >&Ccirc;ap. 17&#170;</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Nasalvokal</trd>
      <trd lng="en">nasal vowel</trd>
      <trd lng="fr">voyelle nasale</trd>
      <trd lng="pl">samog&lstroke;oska nosowa</trd>
      <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_v;&c_o;&c_j; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
    </drv>

  </art>
