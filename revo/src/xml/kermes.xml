<vortaro>
<art mrk="$Id: kermes.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kermes</rad>/o
</kap>
<drv mrk="kermes.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda publika jara <ref tip="lst" cel="fest.0o"
      lst="voko:festoj">festo</ref>, origine 
      el <ref cel="flandr.F0ujo.geo">Flandrujo</ref> kaj okazanta je
      la tago de iu loke grava sanktulo.
    </dif>
  </snc>
  <trd lng="cs">rostlinný kermes</trd>
  <trdgrp lng="de">
    <trd>Kirmes</trd>,
    <trd>Kirchweih</trd>,
    <trd>Kirtag</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kermis</trd>,
    <trd>country fair</trd>,
    <trd>village fair</trd>
  </trdgrp>
  <trd lng="fr">kermesse</trd>
  <trd lng="hu">b&uacute;cs&uacute;</trd>
  <trd lng="nl">kermis</trd>
  <trd lng="pt">quermesse</trd>
  <trdgrp lng="sk">
    <trd>hody</trd>,
    <trd>púť <klr>(slávnosť)</klr></trd>
  </trdgrp>
</drv>
</art>
