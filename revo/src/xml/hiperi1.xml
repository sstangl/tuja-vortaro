<vortaro>
<art mrk="$Id: hiperi1.xml,v 1.11 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hiperikac</rad>/oj
</kap>
<drv mrk="hiperi1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Hypericaceae</trd>).
    </dif>
  </snc>
  <trd lng="cs">třezalkovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Johanniskrautgew&auml;chse</trd>
  <trd lng="fr">hypericac&eacute;es</trd>
  <trd lng="hu">orb&aacute;ncf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="it">ipericacee</trd>
  <trd lng="nl">hertshooifamilie</trd>
  <trdgrp lng="pt">
    <trd>gut&iacute;feras</trd>,
    <trd>hiperic&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_r;&c_o;&c_b;&c_o;&c_j;&c_n;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">ľubovníkovité</trd>
</drv>
</art>
