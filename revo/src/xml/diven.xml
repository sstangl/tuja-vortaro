<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diven.xml,v 1.23 2015/09/16 14:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>diven</rad>/i
</kap>
<drv mrk="diven.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkovri per supozoj, rezonoj a&ubreve; hazarde ion, kion oni ne
      jam scias:
      <ekz>
        <tld/>i &scirc;aradon, enigmon, penson, intencojn, sekreton;
      </ekz>
      <ekz>
        mi ne povas <tld/>i la ka&ubreve;zon;
      </ekz>
      <ekz>
        por tion <tld/>i ne necesas peni.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hipote2.0i">hipotezi</ref>,
      <ref cel="konjek.0i">konjekti</ref>,
      <ref cel="supoz.0i">supozi</ref>,
      <ref cel="suspek.0i">suspekti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dovt&iacute;pit se</trd>,
    <trd>h&aacute;dat</trd>,
    <trd>lu&scaron;tit</trd>,
    <trd>rozpoznat</trd>,
    <trd>uhodnout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>raten</trd>,
    <trd>erraten</trd>,
    <trd>herausbekommen</trd>
  </trdgrp>
  <trd lng="en">guess</trd>
  <trd lng="es">adivinar</trd>
  <trd lng="fr">deviner</trd>
  <trdgrp lng="hu">
    <trd>kital&aacute;l</trd>,
    <trd>megfejt</trd>
  </trdgrp>
  <trd lng="nl">raden</trd>
  <trd lng="no">gjette</trd>
  <trdgrp lng="pl">
    <trd>zgadywa&cacute;</trd>,
    <trd>odgadywa&cacute;</trd>,
    <trd>domy&sacute;la&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adivinhar</trd>,
    <trd>acertar</trd>,
    <trd>decifrar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">uh&aacute;dnu&tcaron;</trd>
</drv>

<drv mrk="diven.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <ref tip="dif" cel="auxgur.0isto">a&ubreve;guristo</ref>
      <ekz>
        la re&gcirc;o mem, via patro, faris lin &ccirc;efo de la
        astrologoj,
        sor&ccirc;istoj, &Hcirc;aldeoj, kaj <tld/>istoj
        <fnt><bib>MT</bib>, <lok>&Dan; 5:11</lok></fnt>
      </ekz>
  </snc>
  <trdgrp lng="en">
    <trd>diviner</trd>,
    <trd>fortuneteller</trd>, <trd>soothsayer</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;&c_a;&c_zh;&c_b;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hada&ccaron;</trd>,
    <trd>v&ecaron;&scaron;tec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Augur</trd>,
    <trd>Orakelpriester</trd>
  </trdgrp>
  <trd lng="es">adivino</trd>
  <trdgrp lng="fr">
    <trd>augure <klr tip="ind">(devin)</klr></trd>,
    <trd>devin</trd>
  </trdgrp>
  <trd lng="hu">j&oacute;s</trd>
  <trdgrp lng="pl">
    <trd>wr&oacute;&zdot;biarz</trd>,
    <trd>wr&oacute;&zdot;bita</trd>,
    <trd>prorok</trd>
  </trdgrp>
  <trd lng="pt">adivinho</trd>
  <trd lng="ru">&c_g;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">ve&scaron;tec</trd>
</drv>

<drv mrk="diven.0prove">
  <kap><tld/>prove</kap>
  <snc>
    <dif>
      Fidante al la hazardo, la&ubreve; la &scirc;anco de l' trafo;
      trafe maltrafe:
      <ekz>
        mi adresos divenprove strato Po&scirc;toficeja
        <fnt><bib>Revizoro</bib>, <lok>4:9</lok></fnt>;
      </ekz>
      <ekz>
        divenprove oni telefonis al la Komisiono pri Beletro
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 49a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>prove elekti<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>by chance</trd>,
    <trd>haphazardly</trd>, <trd>randomly</trd>, <trd>arbitrarily</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_w;&c_g;&c_a;&c_d;</trd>,
    <trd>&c_n;&c_a;&c_w;&c_d;&c_a;&c_l;&c_u;&c_ju;</trd>,
    <trd>&c_n;&c_a;&c_w;&c_d;&c_a;&c_ch;&c_u;</trd>
  </trdgrp>
  <trd lng="cs">nam&aacute;tkou</trd>
  <trdgrp lng="de">
    <trd>auf Risiko</trd>,
    <trd>auf gut Gl&uuml;ck</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; tout hasard</trd>,
    <trd>au <ind>hasard</ind></trd>
  </trdgrp>
  <trd lng="hu">tal&aacute;lomra</trd>
  <trdgrp lng="pl">
    <trd>na <ind>chybi&lstroke;-trafi&lstroke;</ind></trd>,
    <trd>licz&aogonek;c na &lstroke;ut
    <ind>szcz&eogonek;&sacute;cia</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_u;&c_g;&c_a;&c_d;</trd>
  <trd lng="sk">naver&iacute;mboha</trd>
</drv>

<drv mrk="diven.kart0isto">
  <kap>kart<tld/>isto</kap>
  <snc>
    <dif>
    <ref tip="super" cel="diven.0isto"><tld lit="D"/>isto</ref>, kiu
    uzas kartojn 
    por diveni:
      <ekz>
      en la anta&ubreve;a tago &scirc;i konsultis kart<tld/>istinon
      <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
      <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
      2005a, p. 
      9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>card reader</trd>,
    <trd>cartomancer</trd>
  </trdgrp>
  <trd lng="pt">cartomante</trd>
</drv>
</art>
