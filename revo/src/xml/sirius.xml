<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sirius.xml,v 1.7 2015/07/04 08:37:43 revo Exp $">
<kap>
<rad>Sirius</rad>/o
</kap>
<drv mrk="sirius.0o">
  <kap><tld/>o</kap>
  <snc mrk="sirius.0o.mit">
  <uzo tip="fak">MIT</uzo>
    <dif>
Fidela hundo de 
<ref tip="vid" cel="orion.0o.mit">Oriono</ref>.
    </dif>
  </snc>

  <snc mrk="sirius.0o.ast">
  <uzo tip="fak">AST</uzo>
    <dif>
Tre brila stelo de la stelaro de la
<ref tip="vid" cel="hund.Cxas0oj.AST">&Ccirc;ashundoj</ref>, 
apud la konstelacio de 
<ref tip="vid" cel="orion.0o.AST">Oriono</ref>.
    </dif>
    <trd lng="fa">&a_sh;&a_b;&a_A;&a_h;&a_n;&f_g;<klr tip="ind"> (&a_s;&a_t;&a_A;&a_r;&a_h;)</klr></trd>
  </snc>
  <trd lng="bg">&c_S;&c_i;&c_r;&c_i;&c_u;&c_s;</trd>
  <trd lng="cs">Sírius</trd>
  <trd lng="de">Sirius</trd>
  <trd lng="en">Sirius</trd>
  <trd lng="fr">Sirius</trd>
  <trd lng="it">Sirio</trd>
  <trd lng="la">Sirius</trd>
  <trd lng="nl">Sirius</trd>
  <trd lng="pl">Syriusz</trd>
  <trd lng="sk">Sírius</trd>
</drv>
</art>

