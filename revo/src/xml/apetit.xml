<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apetit.xml,v 1.29 2016/01/03 10:10:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>apetit</rad>/o
</kap>
<drv mrk="apetit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Deziro man&gcirc;i:
      <ekz>
        bona <tld/>o estas signo de sano;
      </ekz>
      <ekz>
        <ind>dum la man&gcirc;ado venas <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="fr">
          <trd>l'<ind>app&eacute;tit</ind> vient en mangeant</trd>
        </trdgrp>
        <trd lng="pl">apetyt ro&sacute;nie w miar&eogonek; jedzenia</trd>
      </ekz>
      <ekz>
        <ind>bonan <tld/>on!</ind>
        <klr>(bondeziro dirita anta&ubreve; man&gcirc;o)</klr>
        <trdgrp lng="de">
          <trd>guten <ind>Appetit!</ind></trd>,
          <trd>gesegnete Mahlzeit!</trd>,
          <trd>Mahlzeit!</trd>
        </trdgrp>
        <trd lng="fa">&a_n;&a_w;&a_sh; &a_j;&a_A;&a_n;!</trd>
        <trd lng="fr">bon app&eacute;tit</trd>
        <trd lng="es">buen apetito</trd>
        <trd lng="hu">j&oacute; &eacute;tv&aacute;gyat</trd>
        <trdgrp lng="lat">
          <trd>bene sapiat</trd>, <trd>bonam orexin</trd>
        </trdgrp>
        <trd lng="pl">smacznego!</trd>
        <trd lng="ro">poft&abreve; bun&abreve;!</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_t;&c_y;&c_t;</trd>
  <trd lng="bg">&c_a;&c_p;&c_je;&c_t;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>apetyt</trd>,
    <trd>apet&yacute;t</trd>,
    <trd>chu&tcaron;</trd>
  </trdgrp>
  <trd lng="de">Appetit</trd>
  <trd lng="en">appetite</trd>
  <trd lng="es">apetito</trd>
  <trd lng="fa">&a_A;&a_sh;&a_t;&a_h;&a_A;</trd>
  <trd lng="fr">app&eacute;tit</trd>
  <trd lng="hu">&eacute;tv&aacute;gy</trd>
  <trd lng="it">appetito</trd>
  <trd lng="pl">apetyt</trd>
  <trd lng="ro">apetit</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>apet&iacute;t</trd>,
    <trd>chu&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="apetit.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="apetit.0veka"><tld/>veka</ref>
    <ekz>
      &scirc;i anka&ubreve; estas tre <tld/>a, tute ne malbela
      <fnt><bib>Revizoro</bib><lok>4:13</lok></fnt>.
    </ekz>
  </snc>
</drv>

<drv mrk="apetit.0iga">
  <kap><tld/>iga</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="apetit.0veka"><tld/>veka</ref>
  </snc>
  <trd lng="cs">vzbuzuj&iacute;c&iacute; chu&tcaron; k j&iacute;dlu</trd>
  <trd lng="sk">vzbudzuj&uacute;ci chu&tcaron; k jedlu</trd>
</drv>

<drv mrk="apetit.0veka">
  <kap><tld/>veka<fnt>Z</fnt></kap>
  <snc mrk="apetit.0veka.propre">
    <dif>
      Ekscitanta la <tld/>on.
    </dif>
  </snc>
  <snc mrk="apetit.0veka.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alloga, dezirinda:
      <ekz>
        <tld/>veka virino.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_e;&c_t;&c_y;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>appetitanregend</trd>, <trd>appetitlich</trd>
  </trdgrp>
  <trd lng="en">appetizing</trd>
  <trd lng="es">apetitoso</trd>
  <trd lng="fa">&a_A;&a_sh;&a_t;&a_h;&a_A;&a_A_madda;&a_w;&a_r;</trd>
  <trd lng="fr">app&eacute;tissant</trd>
  <trd lng="hu">&eacute;tv&aacute;gygerjeszt&odblac;</trd>
  <trdgrp lng="it">
    <trd>appetitoso,</trd>
    <trd>appetibile</trd>
  </trdgrp>
  <trd lng="pl">apetyczny</trd>
  <trd lng="ro">apetisant</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="apetit.0vekilo">
  <kap><tld/>vekilo</kap>
  <snc>
    <dif>
      &Ccirc;io ekscitanta la <tld/>on:
      <ekz>
        <klr>[aperitivo estas]</klr> trinka&jcirc;o, ordinare alkohola,
        trinkata kiel <tld/>vekilo anta&ubreve; man&gcirc;o
        <fnt><bib>9OA</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ap&eacute;ritif</trd>
</drv>
</art>
