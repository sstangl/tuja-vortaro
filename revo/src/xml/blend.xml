<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blend.xml,v 1.15 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>blend</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="blend.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Natura zinka sulfido. ZnS.
    </dif>
    <trdgrp lng="ca">
      <trd>blenda</trd>,
      <trd>esfalerita</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sphalerit</trd>,
      <trd>Zinkblende</trd>,
      <trd>Zinksulfid</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>blende</trd>,
      <trd>zinc sulfate</trd>,
      <trd>zinc sulphate</trd>,
      <trd>sphalerite</trd>
    </trdgrp>
    <trd lng="fr">blende</trd>
    <trdgrp lng="hu">
      <trd>szfalerit</trd>,
      <trd>cinkf&eacute;nyle</trd>,
      <trd>cinkszulfid</trd>,
    </trdgrp>
    <trd lng="pl">blenda cynkowa</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_f;&c_a;&c_l;&c_je;&c_r;&c_i;&c_t;</trd>,
      <trd>&c_c;&c_i;&c_n;&c_k;&c_o;&c_v;&c_a;&c_ja;
      &c_o;&c_b;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_l;&c_je;&c_n;&c_d;&c_a;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="hom">
    Ne miksu kun <ref cel="blend1.0o">blendo</ref> (kiraso).
  </refgrp>
  <trdgrp lng="sk">
    <trd>sfalerit</trd>,
    <trd>sulfid zinočnatý</trd>
  </trdgrp>
  <trd lng="cs">pancéřový kryt</trd>
</drv>
</art>
