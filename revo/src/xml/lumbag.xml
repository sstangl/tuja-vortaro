<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lumbag.xml,v 1.9 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lumbag</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lumbag.0o">
  <kap><tld/>o</kap>
  <snc mrk="lumbag.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Akuta <ref tip="malprt" cel="dolor.lumbo0o">lumbodoloro</ref>,
      plejofte pro fortostre&ccirc;o.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>lumbago</trd>,
    <trd>lumb&agrave;lgia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bolest v zádech</trd>,
    <trd>houser</trd>,
    <trd>lumbago</trd>,
    <trd>ústřel</trd>
  </trdgrp>
  <trd lng="de">Hexenschuss</trd>
  <trdgrp lng="fr">
    <trd>lumbago</trd>,
    <trd>tour de rein</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>lumbago</trd>,
    <trd><ind>nyeri</ind> pinggang</trd>
  </trdgrp>
  <trd lng="sk">úsad</trd>
</drv>
</art>

