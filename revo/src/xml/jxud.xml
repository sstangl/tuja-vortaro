<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jxud.xml,v 1.16 2015/11/28 11:10:15 revo Exp $">
<kap>
  <ofc>9</ofc><rad>&jcirc;ud</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="jxud.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Japan-devena lukto-<ref tip="super" cel="sport.0o">sporto</ref>
      sindefenda, en kiu oni kaptas, for&jcirc;etas, a&ubreve; senmovigas
      kontra&ubreve;ulon.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ju;&c_d;&c_o;</trd>
  <trd lng="bg">&c_d;&c_zh;&c_u;&c_d;&c_o;</trd>
  <trd lng="en">judo</trd>
  <trd lng="de">Judo</trd>
  <trd lng="es">judo</trd>
  <trd lng="fr">judo</trd>
  <trdgrp lng="hu">
    <trd>cselg&aacute;ncs</trd>,
    <trd>dzs&uacute;d&oacute;</trd>
  </trdgrp>
  <trd lng="nl">judo</trd>
  <trd lng="pt">jud&ocirc;</trd>
  <trd lng="ru">&c_d;&c_z;&c_ju;&c_d;&c_o;</trd>
  <trdgrp lng="it">
    <trd>judo</trd>, 
    <trd>jud&ograve;</trd>
  </trdgrp>
</drv>

<drv mrk="jxud.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Praktikanto de <tld/>o:
      <ekz>
        &ccirc;iuj <tld/>istoj portas sur si diverskolorajn zonojn por montri
        sian klason a&ubreve; rangon
        <fnt><bib>EncJap</bib>, <lok>artikolo &leftquot;&jcirc;udo&rightquot;, 
         p.78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ju;&c_d;&c_a;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_d;&c_zh;&c_u;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="de">Judoka</trd>
  <trd lng="en">judo wrestler</trd>
  <trd lng="fr">judoka</trd>
  <trdgrp lng="hu">
    <trd>cselg&aacute;ncsoz&oacute;</trd>,
    <trd>dzs&uacute;d&oacute;&oacute;s</trd>
  </trdgrp>
  <trd lng="nl">judoka</trd>
  <trd lng="ru">&c_d;&c_z;&c_ju;&c_d;&c_o;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="it">
    <trd>judoka</trd>,
    <trd>lottatore di <ind>judo</ind></trd>
  </trdgrp>
</drv>
</art>
