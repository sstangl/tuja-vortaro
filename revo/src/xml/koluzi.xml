<art mrk="$Id: koluzi.xml,v 1.14 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>koluzi</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koluzi.0o">
  <kap><tld/>o</kap>
  <snc mrk="koluzi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Sekreta, plej ofte kondamninda, interkonsento:
      <ekz>
        &leftquot;ne necesas tempo por privataj aran&gcirc;oj
        kaj negocado pri vo&ccirc;oj.&rightquot;
        &leftquot;Ne temas pri <tld/>o,&rightquot; Vanderlieb protestis.
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>tk-stafeto, 1982</lok></fnt>.
      </ekz>  
    </dif>
    <refgrp tip="vid">
      <ref cel="komplo.0o">komploto</ref>,
      <ref cel="konspi.0o">konspiro</ref>
    </refgrp>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_je;&c_m;&c_n;&c_a;&c_je; <ind>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="cs">koluze</trd>
  <trdgrp lng="de">
      <trd>Geheimabsprache</trd>,
      <trd>geheime <ind>Absprache</ind></trd>,
      <trd>Gemauschel</trd>
   </trdgrp>
  <trd lng="en">collusion</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_b;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_z;&a_d;&a_w;&a_b;&a_n;&a_d;</trd>,
    <trd>&a_s;&a_A;&a_kh;&a_t;&zwnj;&a_w;&f_p;&a_A;&a_kh;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>collusion</trd>,
    <trd>intelligence<klr tip="ind"> (entente secr&egrave;te)</klr></trd>,
    <trd>entente<klr tip="ind"> (collusion)</klr></trd>
  </trdgrp>
  <trd lng="nl">geheime <ind>afspraak</ind></trd>
  <trdgrp lng="pt">
    <trd>colus&atilde;o</trd>,
    <trd>conluio</trd>,    
    <trd>acordo secreto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_je; &c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">tajn√Ω dohovor</trd>
</drv>
</art>
