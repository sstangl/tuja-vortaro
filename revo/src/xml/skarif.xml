<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skarif.xml,v 1.13 2015/10/22 05:10:27 revo Exp $">
<kap>
  <rad>skarif</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skarif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skarif.0i.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="skarifik.0i.MED">skarifiki</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      violkolora fulmo <tld/>is la sufoke koaguli&gcirc;intan aeron
      <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
      <lok>tk-stafeto, 1982</lok></fnt>      
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pou&scaron;t&ecaron;t &zcaron;ilou</trd>,
    <trd>skarifikovat</trd>
  </trdgrp>
  <trd lng="de">skarifizieren</trd>
  <trd lng="en">to scarify</trd>
  <trd lng="fr">scarifier</trd>
  <trd lng="nl">schrapen</trd>
  <trd lng="pl">skaryfikowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nareza&tcaron; ko&zcaron;u</trd>,
    <trd>skarifikova&tcaron;</trd>,
    <trd>urobi&tcaron; z&aacute;rez</trd>
  </trdgrp>
</drv>
</art>
