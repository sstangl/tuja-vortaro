<art mrk="$Id: akr1.xml,v 1.15 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akr</rad>/o <fnt><bib>ZR</bib></fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="akr1.0o.MEZ">
    <ref tip="dif" cel="akre.0o.MEZ">akreo</ref>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>akr</trd>,
    <trd>jitro <klr>(0,817 ha)</klr></trd>
  </trdgrp>
  <trd lng="de">Morgen</trd>
  <trd lng="en">acre</trd>
  <trd lng="es">acre</trd>
  <trd lng="fa">&a_A_madda;&f_k;&a_r;<klr> (&a_w;&a_A;&a_H;&a_d; &a_s;&a_T;&a_H;)</klr></trd>
  <trd lng="fr">acre<klr> (mesure de surface)</klr></trd>
  <trd lng="hu">hold<klr> (f&ouml;ldm&eacute;rt&eacute;k)</klr></trd>
  <trd lng="pt">acre</trd>
  <trd lng="ru">&c_a;&c_k;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>aker</trd>,
    <trd>aker <klr>(plošná miera 40 árov)</klr></trd>
  </trdgrp>
</drv>
</art>
