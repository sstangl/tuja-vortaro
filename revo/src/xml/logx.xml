<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: logx.xml,v 1.26 2015/11/27 07:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lo&gcirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="logx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="logx.0i.propre">
    <dif>
      Da&ubreve;re restadi en loko, kie oni kutime vivadas:
      <ekz>
        li <tld/>as ekster la urbo
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        la rusoj <tld/>as en Rusujo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i en bela domo, en pensiono;
      </ekz>
      <ekz>
        se la tero estus <tld/>ata <klr>...</klr> de sa&gcirc;uloj
        <fnt><bib>Lanti</bib>, <lok>publika letero al Romen
        Rolan</lok></fnt>.
      </ekz>
      <ekz>
        arbaro <tld/>ata de sennombraj birdoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lando de via fremde<tld/>ado
        <fnt><bib>MT</bib>, <lok>&Gen; 28:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>loko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">wohnen</trd>
    <trd lng="nl">wonen</trd>
    <trd lng="pl">mieszka&cacute;</trd>
    <trdgrp lng="pt">
      <trd>morar</trd>,
      <trd>habitar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="logx.0i.gasti">
    <ref tip="dif" cel="gast.0i">gasti</ref>
      <ekz>
        <tld/>i &ccirc;e amiko, en hotelo dum du tagoj.
      </ekz>
    <trdgrp lng="pl">
      <trd>mieszka&cacute;</trd>,
      <trd>zamieszkiwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">hospedar-se</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_v;&c_je;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">bydlet</trd>
  <trdgrp lng="de">
    <trd>wohnen</trd>,
    <trd>gastieren</trd>,
    <trd>&uuml;bernachten</trd>
  </trdgrp>
  <trd lng="nl">logeren</trd>
  <trdgrp lng="en">
    <trd>dwell</trd>,
    <trd>inhabit</trd>,
    <trd>live</trd>,
    <trd>reside</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>habitar</trd>,
    <trd>vivir</trd>,
    <trd>residir</trd>,
    <trd>morar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>habiter</trd>,
    <trd>loger</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lakik</trd>,
    <trd>&eacute;l <klr tip="ind">(lakik)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as ie:
      <ekz>
        tiu &ccirc;i urbo havas milionon da <tld/>antoj
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        alia <tld/>anto de la strato
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 5, metamorfozo
        de la
        skribisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Bewohner</trd>
  <trdgrp lng="en">
    <trd>dweller</trd>,
    <trd>inhabitant</trd>,
    <trd>resident</trd>
  </trdgrp>
  <trd lng="es">habitante</trd>
  <trdgrp lng="fr">
    <trd>habitant</trd>,
    <trd>occupant<klr> (d'une habitation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lak&oacute;</trd>,
    <trd>lakos</trd>
  </trdgrp>
  <trd lng="nl">bewoner</trd>
  <trd lng="pl">mieszkaniec</trd>
  <trdgrp lng="pt">
    <trd>morador</trd>,
    <trd>habitante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0antaro">
  <kap><tld/>antaro</kap>
  <snc mrk="logx.0antaro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la homoj <tld/>antaj en loko, urbo, provinco, lando...:
      <ekz>
        bruis la <tld/>antaro de la granda urbo sur la vasta trotuaro
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">population</trd>
  <trdgrp lng="fr">
    <trd>habitants</trd>,
    <trd>nombre d'habitants</trd>,
    <trd>population</trd>
  </trdgrp>
  <trd lng="pl">mieszka&nacute;cy</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="logx.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Kie oni povas <tld/>i:
      <ekz>
        <tld/>ebla lando<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_y;</trd>
  <trd lng="de">bewohnbar</trd>
  <trd lng="en">habitable</trd>
  <trd lng="es">habitable</trd>
  <trd lng="fr">habitable</trd>
  <trd lng="hu">lakhat&oacute;</trd>
  <trd lng="nl">bewoonbaar</trd>
  <trd lng="pl">mieszkalny</trd>
  <trd lng="pt">habit&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja; &c_zh;&c_i;&c_l;&c_mol;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0ejo">
  <kap><ofc>*</ofc><tld/>ejo</kap>
  <snc mrk="logx.0ejo.ARKI">
    <dif>
      Domo a&ubreve; &ccirc;ambraro, kie oni <tld/>as:
      <ekz>
        &scirc;i prenis la plej belan ar&gcirc;entan vazon, kiu estis en la
        <tld/>ejo
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_l;&c_mol;&c_l;&c_jo;</trd>,
    <trd>&c_zh;&c_y;&c_t;&c_l;&c_o;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_t;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abode</trd>,
    <trd>dwelling</trd>,
    <trd>house</trd>,
    <trd>residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appartement</trd>,
    <trd>habitation</trd>,
    <trd>logement</trd>
  </trdgrp>
  <trd lng="hu">lak&aacute;s</trd>
  <trd lng="nl">woning</trd>
  <trdgrp lng="pl">
    <trd>mieszkanie</trd>,
    <trd>lokal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_zh;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havigi <tld/>ejon al iu:
      <ekz>
        volonte mi <tld/>igos vin en mia palaco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 18a, maltrankvilo en la
        re&gcirc;a
        palaco</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi laboristojn en barako.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">beherbergen</trd>
  <trdgrp lng="en">
    <trd>accomodate</trd>,
    <trd>give a home to</trd>,
    <trd>put up</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>acoger</trd>,
    <trd>hospedar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&eacute;berger</trd>,
    <trd>loger<klr> (quelqu'un)</klr></trd>
  </trdgrp>
  <trd lng="hu">elsz&aacute;ll&aacute;sol</trd>
  <trd lng="nl">huisvesten</trd>
  <trdgrp lng="pl">
    <trd>da&cacute; <ind>mieszkanie</ind></trd>,
    <trd>zakwaterowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">hospedar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.amas0ejo">
  <kap>amas<tld/>ejo</kap>
  <snc mrk="logx.amas0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Loko aran&gcirc;ita por densa <tld/>ado
      de multaj homoj, kie privateco mankas:
      <ekz>
        <uzo tip="fak">ESP</uzo>
        kutime en UK-j amas<tld/>ejo
        estas ekzemple gimnastejo de lernejo
        <fnt>
          <aut>Nori</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/E-istajVegetaranoj/message/610">
          Amaslo&gcirc;ejo dum la UK</url></vrk>,
          <lok>Esperantistaj Vegetaranoj, 2007-01-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj labortempoj, malsanigaj amas<tld/>ejoj, infanlaboro, 
        mallonga vivda&ubreve;ro, mankanta laborprotekto estis kelkaj el la 
        mankoj kiuj per klasbatalo estis iom post iom forigitaj
        <fnt>
          <aut>T. Bergmann</aut>,
          <vrk><url ref="http://mas-eo.org/spip.php?article309">
          La socialisma disvolvo-strategio de la PR &Ccirc;inio</url></vrk>,
          <lok>Monda Asembleo Socia, 2008-08-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>dormitory (dorm)</trd>,
    <trd>barracks</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grand ensemble</trd>,
    <trd>logement collectif</trd>
  </trdgrp>
</drv>

<drv mrk="logx.ek0i">
  <kap>ek<tld/>i</kap>
  <snc>
    <dif>
      Aran&gcirc;i sian restadon en nova loko:
      <ekz>
        ne iru en Egiptujon, por tie ek<tld/>i
        <fnt><bib>MT</bib>, <lok>&Jer; 43:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>settle in</trd>,
    <trd>take up residence</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>installer</ind><klr> (pour habiter)</klr></trd>
  <trdgrp lng="pl">
    <trd>zamieszka&cacute;</trd>,
    <trd>urz&aogonek;dzi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="logx.en0igxi">
  <kap>en<tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Preni, ekokupi <tld/>ejon:
      <ekz>
        mi en<tld/>i&gcirc;is en la mansardo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">einziehen</trd>
  <trdgrp lng="en">
    <trd>settle in</trd>,
    <trd>take up residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emm&eacute;nager</trd>,
    <trd>s'<ind>installer</ind><klr> (emm&eacute;nager)</klr></trd>
  </trdgrp>
  <trd lng="hu">bek&ouml;lt&ouml;zik</trd>
  <trd lng="nl">zijn <ind>intrek</ind> nemen</trd>
  <trdgrp lng="pl">
    <trd>zamieszka&cacute;</trd>,
    <trd>wprowadzi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.kun0anto">
  <kap>kun<tld/>anto</kap>
  <snc mrk="logx.kun0anto.KOMUNE">
    <dif>
      <tld lit="L"/>anto de sama <tld/>ejo:
      <ekz>
        miaj kun<tld/>antoj jam klarigis min kiu estas Bj&ouml;rk
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/eng/sentences/show/815078">
          [frazo 815078]</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dom.0ano.ajna">domano</ref>
    </refgrp>
  </snc>
  <trd lng="en">roommate</trd>
  <trdgrp lng="fr">
    <trd>commensal</trd>,
    <trd>colocataire</trd>
  </trdgrp>
  <trd lng="pl">wsp&oacute;&lstroke;lokator</trd>
</drv>

<drv mrk="logx.pas0anto">
  <kap>pas<tld/>anto</kap>
  <snc>
    <dif>
      Portempa restanto en loko, en lando:
      <ekz>
        fremdulo kaj pas<tld/>anto mi estas &ccirc;e vi
        <fnt><bib>MT</bib>, <lok>&Gen; 23:4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gast.0o">gasto</ref>,
      <ref cel="migr.0i">migri</ref>,
      <ref cel="nomad.0o">nomado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>sojourner</trd>,
    <trd>temporary dweller</trd>,
    <trd>temporary resident</trd>
  </trdgrp>
  <trd lng="fr">h&ocirc;te de passage</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="logx.pra0anto">
  <kap>pra<tld/>anto</kap>
  <snc>
    <dif>
      Iu el la plej antikve konataj <tld/>antoj de lando:
      <ekz>
        la Akkadai (a&ubreve; montanoj), la pra<tld/>antoj en la orienta,
        monta
        parto de la Babela regno
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a, Lemuel da&ubreve;rigas sian
        esploron</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indigx.0o">indi&gcirc;eno</ref>
    </refgrp>
  </snc>
  <trd lng="en">aboriginal</trd>
  <trd lng="fr">indig&egrave;ne<klr> (subst.)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_n;&c_je;&c_j;&c_sh;&c_i;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="logx.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>anto, resti en:
      <ekz>
        kiam Jerusalem estis prilo&gcirc;ata kaj bonstata
        <fnt><bib>MT</bib>, <lok>&Zeh; 7:7</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okup.0i">okupi</ref>
    </refgrp>
  </snc>
  <trd lng="en">inhabit</trd>
  <trd lng="fr">occuper<klr> (un lieu, y r&eacute;sider)</klr></trd>
  <trdgrp lng="pl">
    <trd>zamieszkiwa&cacute;</trd>,
    <trd>zajmowa&cacute;</trd>,
    <trd>zasiedla&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="logx.trans0igxi">
  <kap>trans<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      &Scirc;an&gcirc;i sian <tld/>ejon, kaj okupi alian:
      <ekz>
        poste ni trans<tld/>i&gcirc;is Moskvon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">umziehen</trd>
  <trdgrp lng="en">
    <trd>change one's dwelling</trd>,
    <trd>move</trd>,
    <trd>relocate</trd>
  </trdgrp>
  <trd lng="es">mudarse</trd>
  <trd lng="fr">d&eacute;m&eacute;nager</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;lt&ouml;zik</trd>,
    <trd>&aacute;tk&ouml;lt&ouml;zik</trd>
  </trdgrp>
  <trd lng="nl">verhuizen</trd>
