<vortaro>
<art mrk="$Id: muz.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>muz</rad>/o
</kap>
<drv mrk="muz.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIT</uzo>
    <dif>
      &Ccirc;iu el la na&ubreve; grekaj diinoj, kiuj personigis belartojn kaj
      sciencojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_a;</trd>
  <trd lng="bg">&c_m;&c_u;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>Múza</trd>,
    <trd>bohyně umění</trd>,
    <trd>múza</trd>
  </trdgrp>
  <trd lng="de">Muse</trd>
  <trd lng="en">muse</trd>
  <trd lng="fa">&a_A;&a_l;&a_A;&a_h;&a_h;&a_hamza_sure; &a_h;&a_n;&a_r;</trd>
  <trd lng="fr">muse</trd>
  <trd lng="hu">m&uacute;zsa</trd>
  <trd lng="nl">muze</trd>
  <trd lng="pt">musa</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>bohyňa umenia</trd>,
    <trd>m&uacute;za</trd>
  </trdgrp>
</drv>
</art>
