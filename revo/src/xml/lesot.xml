<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lesot.xml,v 1.9 2014/06/23 14:10:24 revo Exp $">
<kap>
  <rad>Lesot</rad>/o
</kap>

<drv mrk="lesot.L0o">
  <kap>
    <tld lit="L"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="lesot.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Maseruo.
      Teritorio de Re&gcirc;lando Lesotho.
      ISO-3166 kodo: LS.
      &Gcirc;i estas <ref tip="vid" cel="enklav.0o.POL">enklavo</ref> en
      <ref tip="vid" cel="afrik.SudA0o.GEOG">Sud-Afriko</ref>.
    </dif>
    </snc>
    <trd lng="bg">&c_L;&c_je;&c_s;&c_o;&c_t;&c_o;</trd>
    <trd lng="de">Lesotho</trd>
    <trd lng="en">Lesotho</trd>
    <trdgrp lng="fr">
      <trd>Lesotho</trd>,
      <trd>Royaume du Lesotho</trd>
    </trdgrp>
    <trd lng="id">Lesotho</trd>
    <trd lng="nl">Lesotho</trd>
    <trdgrp lng="pl">
      <trd>Lesotho</trd>,
      <trd>Kr&oacute;lestwo <ind>Lesotho</ind></trd>
    </trdgrp>
    <trd lng="pt">Lesoto</trd>
</drv>

</art>

