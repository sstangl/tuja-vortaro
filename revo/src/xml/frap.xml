<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frap.xml,v 1.29 2016/02/28 23:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frap</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="frap.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frap.0i.bati">
    <dif>
      <ref tip="dif" cel="bat.0i.ektusxegi">Bati<sncref/></ref>:
      <ekz>
        <tld/>i iun per bastono, per kuglo;
      </ekz>
      <ekz>
        <tld/>ita de la fulmo;
      </ekz>
      <ekz>
        <tld/>i vazon por &gcirc;in rompi;
      </ekz>
      <ekz>
        <tld/>i al si la kapon, la ventron;
      </ekz>
      <ekz>
        <tld/>i al iu la vangon;
      </ekz>
      <ekz>
        li <tld/>is la birdon sur la bekon
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de
        feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        malseka aero <tld/>is iliajn viza&gcirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ita de la radioj de la suno, li &gcirc;uis trankvilan
        plezuron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>uderza&cacute;<klr> w co&sacute;, kogo&sacute;</klr></trd>,
      <trd>da&cacute; <ind>kuksa&nacute;ca</ind><klr>
      komu&sacute;</klr></trd>,
      <trd>stukn&aogonek;&cacute;<klr> (w) co&sacute;,
      kogo&sacute;</klr></trd>,
      <trd>szturcha&cacute;<klr> co&sacute;, kogo&sacute;</klr></trd>,
      <trd>paln&aogonek;&cacute;<klr> co&sacute;, kogo&sacute;
      (pot.)</klr></trd>,
      <trd>r&aogonek;bn&aogonek;&cacute;<klr> w co&sacute;,
      kogo&sacute;</klr></trd>,
      <trd>trzasn&aogonek;&cacute;<klr> w co&sacute;,
      kogo&sacute;</klr></trd>,
      <trd>wali&cacute;<klr> w co&sacute;, kogo&sacute;</klr></trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_r;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="frap.0i.produktiSonon">
    <dif>
      Batante produkti sonon:
      <ekz>
        <tld/>i tamburon, cimbalon;
      </ekz>
      <ekz>
        <tld/>i &ccirc;e<fnt>Z</fnt>,
        je la pordo<fnt>Z</fnt>,
        sur la pordon de iu;
      </ekz>
      <ekz>
        oni <tld/>as;
      </ekz>
      <ekz>
        kiu <tld/>as, al tiu oni malfermas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la maro <tld/>as &ccirc;e la rokoj;
      </ekz>
      <ekz>
        lia koro <tld/>is kun granda rapideco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia pulso, la tempioj forte <tld/>is pro emocio;
      </ekz>
      <ekz>
        kun<tld/>i la manojn <klr>(klaki per la manoj)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplaux.0i">apla&ubreve;di</ref>,
      <ref cel="bat.0i.movi">bati<sncref/></ref>,
      <ref cel="plauxd.0i">pla&ubreve;di</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>uderza&cacute;<klr> w co&sacute;</klr></trd>,
      <trd>bi&cacute;<klr> w co&sacute;</klr></trd>,
      <trd>trzaska&cacute;<klr> czym&sacute;, w co&sacute;</klr></trd>
      <trd>wali&cacute;<klr> w co&sacute;</klr></trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="frap.0i.mirigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite kaj forte atentigi, mirigi:
      <ekz>
        <tld/>i la atenton, la okulojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>(ant)a</ind> ekzemplo, argumento, simileco;
        <trdgrp lng="fr">
          <trd>frappant</trd>,
          <trd>saisissant<klr tip="ind"> (frappant)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>ante bela, pala<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="pl">uderza&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="frap.0i.tusxegi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmilde trafi, subite tu&scirc;egi:
      <ekz>
        kvaza&ubreve; tiuj vortoj
        <tld/>us &scirc;in per ia rememoro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        penso, kiu <tld/>is &scirc;ian koron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teruro lin <tld/>is
        <fnt><bib>MT</bib>, <lok>&Jes; 21:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>ita de blindeco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="emoci.0i">emocii</ref>,
      <ref cel="fal.0i">fali sur</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="sku.0i">skui</ref>.
    </refgrp>
    <trd lng="pl">uderza&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="frap.0i.punbati">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Punbati, puni:
      <ekz>
        ili estis <tld/>itaj de la Dia kolero;
      </ekz>
      <ekz>
        la Eternulo <tld/>is la popolon per granda <tld/>o
        <fnt><bib>MT</bib>, <lok>&Sa1; 6:19</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bat.0i.puni">bati<sncref/></ref>,
      <ref cel="plag.0i">plagi</ref>,
      <ref cel="vip.0i">vipi</ref>.
    </refgrp>
    <trd lng="pl">uderza&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>zabouchat</trd>,
    <trd>zabu&scaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>klopfen</trd>,
    <trd>schlagen</trd>,
    <trd>treffen</trd>,
    <trd>pochen</trd>,
    <trd>frappieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hit</trd>,
    <trd>knock</trd>,
    <trd>rap</trd>,
    <trd>slap</trd>,
    <trd>smack</trd>,
    <trd>strike</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>golpear</trd>,
    <trd>percutir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>frapper</trd>,
    <trd>atteindre<klr tip="ind"> (frapper)</klr></trd>,
    <trd>cogner</trd>,
    <trd>heurter</trd>,
    <trd>percuter</trd>,
    <trd>saisir<klr tip="ind"> (frapper)</klr></trd>,
    <trd>taper</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>ketok</ind>] mengetok</trd>,
    <trd>[<ind>ketuk</ind>] mengetuk</trd>
  </trdgrp>
  <trd lng="nl">slaan</trd>
  <trdgrp lng="pt">
    <trd>bater</trd>,
    <trd>golpear</trd>,
    <trd>percutir</trd>
  </trdgrp>
  <trd lng="sk">zab&uacute;cha&tcaron;</trd>
  <trd lng="tp">utala</trd>
</drv>

<drv mrk="frap.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="frap.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>o;
      <ref tip="vid" cel="bat.0.SON">bat</ref>:
      <ekz>
        signalo estis trifoja <tld/>o sur plej alta &scirc;tupo
        <klr>...</klr> - <tld/>! - <tld/>! - <tld/>!
        <fnt>
          <aut>J. Mahen, trad. Moraviaj Esperanto-Pioniroj</aut>,
          <vrk><url
          ref="http://eo.wikisource.org/wiki/La_koboldo_Ondra">
          La koboldo Ondra</url></vrk>,
          <lok>1921</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pan !</trd>
  <trd lng="id">tok !</trd>
</drv>


<drv mrk="frap.0o">
  <kap><tld/>o</kap>
  <snc mrk="frap.0o.KOMUNE">
    <dif>
      Ago de <tld/>anto, a&ubreve; rezulto de tiu ago:
      <ekz>
        oni a&ubreve;dis tri <tld/>ojn sur la muro;
      </ekz>
      <ekz>
        <ind>vango<tld/>o</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>claque<klr tip="ind"> (soufflet)</klr></trd>,
          <trd>soufflet<klr tip="ind"> (claque)</klr></trd>
        </trdgrp>
        <trd lng="nl">kaakslag</trd>
      </ekz>
      <ekz>
        tiu morto estis por li neatendita <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klep</trd>,
    <trd>klepnut&iacute;</trd>,
    <trd>&uacute;der</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hit</trd>,
    <trd>knock</trd>,
    <trd>strike</trd>,
    <trd>stroke</trd>
  </trdgrp>
  <trd lng="es">golpe</trd>
  <trd lng="fr">coup</trd>
  <trdgrp lng="id">
    <trd>[<ind>ketok</ind>] ketokan</trd>,
    <trd>[<ind>ketuk</ind>] ketukan</trd>
  </trdgrp>
  <trd lng="nl">slag</trd>
  <trdgrp lng="pl">
    <trd>uderzenie</trd>,
    <trd>palni&eogonek;cie</trd>,
    <trd>r&aogonek;bni&eogonek;cie</trd>,
    <trd>stukni&eogonek;cie</trd>,
    <trd>trza&sacute;ni&eogonek;cie</trd>,
    <trd>walni&eogonek;cie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_k;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klepnutie</trd>,
    <trd>&uacute;der</trd>
  </trdgrp>
</drv>

<drv mrk="frap.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="frap.0ado.KOMUNE">
    <dif>
      Ago de <tld/>anto:
      <ekz>
        almozula <tld/>ado &ccirc;e multaj pordoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>klep&aacute;n&iacute;</trd>,
    <trd>tep&aacute;n&iacute;</trd>,
    <trd>tlukot</trd>,
    <trd>tlu&ccaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="nl">het <ind>slaan</ind></trd>
  <trdgrp lng="pl">
    <trd>uderzanie</trd>,
    <trd>r&aogonek;banie</trd>,
    <trd>stukanie</trd>,
    <trd>trzaskanie</trd>,
    <trd>walenie</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_k;</trd>
  <trd lng="sk">klopanie</trd>
</drv>

<drv mrk="frap.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="frap.0ilo.KOMUNE">
    <dif>
      &Ccirc;io por <tld/>i:
      <ekz>
        <tld/>ilo de pordo, horlo&gcirc;o, sonorilego
        <klr>(svingilo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">klepadlo</trd>
  <trdgrp lng="de">
    <trd>T&uuml;rklopfer</trd>,
    <trd>Schlagwerk <klr tip="ind">(Uhr)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>knocker</trd>,
    <trd>striker</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>battant<klr tip="ind"> (de cloche)</klr></trd>,
    <trd>heurtoir</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>klopper</trd>,
    <trd>klepel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ko&lstroke;atka</trd>,
    <trd>m&lstroke;otek<klr> (w zegarze, pianinie</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>klopadlo</trd>,
    <trd>klop&aacute;&ccaron;</trd>
  </trdgrp>
</drv>

<drv mrk="frap.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="frap.0igxi.KOMUNE">
    <dif>
      Ektu&scirc;egi, pu&scirc;tu&scirc;egi kontra&ubreve; io:
      <ekz>
        <tld/>i&gcirc;i kontra&ubreve; tablo, muro;
      </ekz>
      <ekz>
        &ccirc;u vi falis a&ubreve; <tld/>i&gcirc;is?<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sich <ind>sto&szlig;en</ind></trd>
  <trdgrp lng="fr">
    <trd>se <ind>cogner</ind><klr tip="ind"> (contre qc)</klr></trd>,
    <trd>se <ind>heurter</ind><klr tip="ind"> (&agrave; qc)</klr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>stoten</ind></trd>
  <trdgrp lng="pl">
    <trd>uderzy&cacute; si&eogonek;</trd>,
    <trd>r&aogonek;bn&aogonek;&cacute; si&eogonek;</trd>,
    <trd>waln&aogonek;&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="frap.inter0igxi">
  <kap>inter<tld/>i&gcirc;i, <var><kap>kun<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="frap.inter0igxi.KOMUNE">
    <dif>
      Esti malmilde trafita, malmilde intertu&scirc;egi&gcirc;i:
      <ekz>
        liaj dentoj inter<tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zusammenschlagen <klr tip="ind">(Z&auml;hne)</klr></trd>
  <trd lng="en">clap</trd>
  <trdgrp lng="fr">
    <trd>se <ind>heurter</ind></trd>,
    <trd>se <ind>tamponner</ind></trd>
  </trdgrp>
  <trd lng="nl">klapperen</trd>
  <trdgrp lng="pl">
    <trd>zderza&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;
    &c_d;&c_r;&c_u;&c_g; &c_o; &c_d;&c_r;&c_u;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="frap.okul0a">
  <kap>okul<tld/>a</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="frap.okul0a.KOMUNE">
    <dif>
      La&ubreve; sia aspekto atentiga, kiun oni nepre rimarkas:
      <ekz>
        tre malmultaj atentis pri <ctl>Vanessa polychloros</ctl>,
        iu <klr>...</klr> ne tre okul<tld/>a taga papilio 
        <fnt>
          <aut>P. T. Justesen</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5702/papiliou.html">
          Papilio</url></vrk>,
          <lok>Malgranda Revuo, 1943</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>n&aacute;padn&yacute;</trd>,
    <trd>okat&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flagrant</trd>,
    <trd>voyant</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krik&lcaron;av&yacute;</trd>,
    <trd>n&aacute;padn&yacute;</trd>,
    <trd>okat&yacute;</trd>,
    <trd>zjavn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="frap.orel0a">
  <kap>orel<tld/>a</kap>
  <snc mrk="frap.orel0a.KOMUNE">
    <dif>
      Kiun oni rimarkas pro &gcirc;ia aparta, rimarkinda sono:
      <ekz>
        Espo Despo estas internacia muzikgrupo kiu kreadas kaj 
        prezentas proprajn ritmajn &ndash; ofte dancigajn kaj
        orel<tld/>ajn kantojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperantodesperado.dk/">
          Pri Espo Despo</url></vrk>,
          <lok>[vidita en 2016]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;clatant<klr> (son)</klr></trd>
</drv>

<drv mrk="frap.sun0o">
  <kap>sun<tld/>o</kap>
  <snc mrk="frap.sun0o.hauxta">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ha&ubreve;ta inflamo ka&ubreve;zita de suna varmego.
    </dif>
    <trd lng="de">Sonnenbrand</trd>
    <trd lng="en">sunburn</trd>
    <trdgrp lng="fr">
      <trd>coup de soleil</trd>,
      <trd>h&eacute;liodermite aigu&euml;</trd>
    </trdgrp>
    <trd lng="nl">zonnebrand</trd>
    <trd lng="pl">pora&zdot;enie s&lstroke;oneczne</trd>
    <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_o;&c_zh;&c_o;&c_g;</trd>
  </snc>
  <snc mrk="frap.sun0o.cerba">
    <uzo tip="fak">MED</uzo>
    <dif>
      Cerba <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref> ka&ubreve;zita de suna varmego.
    </dif>
    <adm>
      &Ccirc;u vere &leftquot;cerba&rightquot;? La&ubreve; mi temas pri malsaneca
      stato tu&scirc;anta preska&ubreve; la tutan organismon.
      <aut>MB</aut>
    </adm>
    <trd lng="de">Sonnenstich</trd>
    <trd lng="en">sunstroke</trd>
    <trdgrp lng="fr">
      <trd>coup de chaleur</trd>,
      <trd>insolation</trd>
    </trdgrp>
    <trd lng="nl">zonnesteek</trd>
    <trd lng="pl">udar s&lstroke;oneczny</trd>
    <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_u;&c_d;&c_a;&c_r;</trd>
  </snc>
  <trd lng="cs">sp&aacute;lenina || &uacute;&zcaron;eh</trd>
  <trdgrp lng="sk">
