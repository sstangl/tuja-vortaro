<vortaro>
<art mrk="$Id: velur.xml,v 1.23 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>velur</rad>/o
</kap>
<drv mrk="velur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="velur.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Silka, kotona a&ubreve; lana <ref tip="super"
      cel="sxtof.0o.TEKS">&scirc;tofo</ref>, prezentanta unuflanke densan
      teksa&jcirc;on, kaj aliflanke molan supra&jcirc;on konsistantan el multaj
      mallongaj, rekte starantaj haroj:
      <ekz>
        <ind><tld/>a</ind> &ccirc;apelo
        <fnt><bib>Marta</bib></fnt>;
        <trd lng="ca">vellutat</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>aj herbejoj
        <klr>(molaj kiel <tld/>o)</klr>;
      </ekz>
      <ekz>
        <ind>sur la ventro <tld/>o, en la ventro murmuro</ind>
        <klr>(ri&ccirc;a aspekto ka&scirc;as efektivan malri&ccirc;on)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">b&aacute;rsony mell&eacute;ny, &uuml;res has</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_s;&c_a;&c_m;&c_ib;&c_t;</trd>
  <trd lng="ca">vellut</trd>
  <trdgrp lng="cs">
    <trd>aksamit</trd>,
    <trd>samet</trd>,
    <trd>velur</trd>
  </trdgrp>
  <trd lng="de">Samt</trd>
  <trd lng="el">&beta;&epsilon;&lambda;&omikron;&ypsilon_ton;&delta;&omikron;</trd>
  <trd lng="en">velvet</trd>
  <trd lng="es">terciopelo</trd>
  <trd lng="fa">&a_m;&a_kh;&a_m;&a_l;</trd>
  <trd lng="fr">velours</trd>
  <trd lng="hu">b&aacute;rsony</trd>
  <trdgrp lng="id">
    <trd>beledu</trd>,
    <trd>beludru</trd>
  </trdgrp>
  <trd lng="nl">fluweel</trd>
  <trdgrp lng="pl">
    <trd>welur</trd>,
    <trd>welwet</trd>,
    <trd>aksamit</trd>,
    <trd>sztruks</trd>
  </trdgrp>
  <trd lng="pt">veludo</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_r;&c_h;&c_a;&c_t;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ju;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vel√∫r</trd>,
    <trd>zamat</trd>
  </trdgrp>
  <trd lng="tr">kadife</trd>
</drv>
</art>
