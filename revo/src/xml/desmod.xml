<vortaro>
<art mrk="$Id: desmod.xml,v 1.13 2013/08/30 14:10:49 revo Exp $">
<kap>
<rad>desmodi</rad>/o
</kap>
<drv mrk="desmod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Desmodium</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_s;&c_m;&c_o;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trd lng="de">Telegraphenpflanze</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;velycsom&oacute;</trd>,
    <trd>koldusgyom</trd>
  </trdgrp>
  <trd lng="nl">desmodium</trd>
  <trd lng="pl">desmodium</trd>
  <trdgrp lng="pt">
    <trd>desm&oacute;dio</trd>,
    <trd>carrapicho</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_m;&c_o;&c_d;&c_i;&c_u;&c_m;</trd>
</drv>
</art>

