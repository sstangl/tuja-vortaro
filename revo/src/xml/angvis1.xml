<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: angvis1.xml,v 1.9 2015/09/21 13:10:20 revo Exp $">
<kap>
  <rad>angvised</rad>/oj
</kap>
<drv mrk="angvis1.0oj">
  <kap>
    <tld/>oj
  </kap>
    <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Anguidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Schleichen</trd>
  <trdgrp lng="en">
    <trd>Anguidae</trd>,
    <trd>anguids</trd>, <trd>alligator lizards</trd>
  </trdgrp>
  <trd lng="fr">anguilles<klr> (famille des)</klr></trd>
  <trd lng="hu">l&aacute;batlan <ind>gy&iacute;kf&eacute;l&eacute;k</ind></trd>
  <trd lng="pl">abronia</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_je;&c_t;&c_je;&c_n;&c_i;&c_c;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

