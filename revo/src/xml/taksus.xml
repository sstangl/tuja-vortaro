<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: taksus.xml,v 1.23 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>taksus</rad>/o
</kap>

<drv mrk="taksus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="taksus1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Taxus</trd>), arbo el
      <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref>
      kun &ccirc;iamverdaj pinglofolietoj
      kaj brilru&gcirc;aj dol&ccirc;aj beroj kun venena grajno.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ib;&c_s;</trd>
  <trd lng="bg">&c_t;&c_i;&c_s;</trd>
  <trd lng="ca">teix</trd>
  <trd lng="cs">tis</trd>
  <trd lng="de">Eibe</trd>
  <trd lng="en">yew</trd>
  <trd lng="fr">if</trd>
  <trd lng="hu">tiszafa</trd>
  <trd lng="nl">taxus</trd>
  <trd lng="pl">cis</trd>
  <trdgrp lng="pt">
   <trd>teixo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_s;&c_s;</trd>
  <trd lng="sk">tis</trd>
  <trd lng="sv">idegran</trd>
</drv>
</art>
