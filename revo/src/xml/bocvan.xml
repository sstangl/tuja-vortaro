<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bocvan.xml,v 1.6 2015/10/01 14:10:25 revo Exp $">
<kap>
  <rad>bocvan</rad>/o
</kap>

<drv mrk="bocvan.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bocvan.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda Afriko kun &ccirc;efurbo Gaberono.
      Teritorio de Respubliko Bocvano.
      &Gcirc;is 1966 nomata Be&ccirc;uanalando.
      ISO-3166 kodo: BW.
      &Gcirc;i limas kun
      <ref tip="vid" cel="namibi.N0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="zambi.Z0o.GEOG">Zambio</ref>,
      <ref tip="vid" cel="zimbabv.Z0o.GEOG">Zimbabvo</ref>
      kaj Sud-Afriko.
    </dif>
    </snc>
    <trdgrp lng="en">
      <trd>Botswana</trd>,
      <trd>Republic of Botswana</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Botswana</trd>,
      <trd>R&eacute;publique du Botswana</trd>
    </trdgrp>
    <trd lng="id">Botswana</trd>
    <trdgrp lng="pl">
      <trd>Botswana</trd>,
      <trd>Republika <ind>Botswany</ind></trd>
    </trdgrp>
    <trd lng="pt">Botswana</trd>
</drv>

</art>

