<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bocvan.xml,v 1.8 2016/02/02 23:10:18 revo Exp $">
<kap>
  <rad>Bocvan</rad>/o
</kap>

<drv mrk="bocvan.B0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="bocvan.B0o.GEOG">
    <mlg kod="ISO-3166">BW</mlg>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda 
      <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo 
      <ref tip="prt" cel="gaberon.G0o">Gaberono</ref>,
      teritorio de Respubliko <tld/>o,
      &gcirc;is 1966 nomata Be&ccirc;uanalando,
      limanta kun
      <ref tip="vid" cel="namibi.N0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="zambi.Z0o.GEOG">Zambio</ref>,
      <ref tip="vid" cel="zimbabv.Z0o.GEOG">Zimbabvo</ref>
      kaj Sud-Afriko.
    </dif>
    </snc>
    <trdgrp lng="en">
      <trd>Botswana</trd>,
      <trd>Republic of Botswana</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Botswana</trd>,
      <trd>R&eacute;publique du Botswana</trd>
    </trdgrp>
    <trd lng="id">Botswana</trd>
    <trd lng="nl">Botswana</trd>
    <trd lng="no">Botswana</trd>
    <trdgrp lng="pl">
      <trd>Botswana</trd>,
      <trd>Republika <ind>Botswany</ind></trd>
    </trdgrp>
    <trd lng="pt">Botswana</trd>
</drv>

</art>

