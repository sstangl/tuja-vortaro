<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mahome.xml,v 1.22 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>Mahomet</rad>/o
  <fnt>PIV</fnt>
</kap>

<drv mrk="mahome.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ISL</uzo>
      <ref tip="dif" cel="muhama.0o.profeto">Muhamado</ref>
  </snc>
  <trd lng="ca">Mahoma</trd>
  <trdgrp lng="cs">
    <trd>Mohamed</trd>,
    <trd>Muammad</trd>
  </trdgrp>
  <trd lng="de">Mohamed</trd>
  <trdgrp lng="en">
      <trd>Mohammed</trd>,
      <trd>Muhammad</trd>
    </trdgrp>
  <trd lng="es">Mahoma</trd>
  <trd lng="fr">Mahomet</trd>
  <trd lng="hu">Mohamed</trd>
  <trd lng="nl">Mohammed</trd>
  <trd lng="pt">Maom&eacute;</trd>
  <trd lng="ru">&c_M;&c_a;&c_g;&c_o;&c_m;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>Mohamed</trd>,
    <trd>Muammad</trd>
  </trdgrp>
</drv>

<drv mrk="mahome.0ano">
  <kap><tld lit="m"/>ano<fnt>Z</fnt></kap>
  <uzo tip="stl">EVI</uzo>
  <snc>
  <uzo tip="fak">ISL</uzo>
      <ref tip="dif" cel="islam.0ano">Islamano</ref>
    <rim>
      Kvankam Zamenhofa kaj PIV-a, tiu formo estas evitenda
      en ne&ubreve;trala kunteksto, &ccirc;ar la islamanoj opinias &gcirc;in ofenda
      (kp <ctl>papistoj</ctl> por romkatolikoj).
    </rim>
  </snc>
    <trdgrp lng="ca">
      <trd>mahomet&agrave;</trd>,
      <trd>musulm&agrave;</trd>
    </trdgrp>
    <trd lng="de">Mohamedaner</trd>
    <trd lng="en">Mohammedan</trd>
    <trd lng="es">mahometano</trd>
    <trdgrp lng="fr">
      <trd>mahom&eacute;tan</trd>,
      <trd>mahom&eacute;tane</trd>
    </trdgrp>
    <trd lng="hu">mohamed&aacute;n</trd>
    <trd lng="sk">mohamed&aacute;n</trd>
    <trd lng="nl">mohammedaan<klr tip="ind"> (ontr.)</klr></trd>
</drv>
</art>
