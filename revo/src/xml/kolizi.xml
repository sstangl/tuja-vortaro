<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolizi.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>kolizi</rad>/i
</kap>
<drv mrk="kolizi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Renkonti&gcirc;i kaj fortege tu&scirc;egi sin reciproke:
      <ekz>
        la ekspresa vagonaro <tld/>is kun vara trajno.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="karamb.0i.FIG">karamboli</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_a;&c_p;&c_h;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_e;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zusammensto&szlig;en</trd>,
      <trd>kollidieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>collide with</trd>,
      <trd>crash into</trd>,
      <trd>run into</trd>,
    </trdgrp>
  <trdgrp lng="pt">
    <trd>colidir</trd>,
    <trd>ir de <ind>encontro</ind> a</trd>,    
    <trd>chocar-se</trd>,    
    <trd>abalroar</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Komenci interbatali:
      <ekz>
        la trupoj <tld/>is kontra&ubreve; la ribelantoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>angreifen</trd>,
      <trd>treffen (auf)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>engage</trd>,
      <trd>clash</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      (p.p. cirkonstancoj a&ubreve; sentoj).
      Neakordi&gcirc;i, kontra&ubreve;i, konflikti:
      <ekz>
        tia entrepreno <tld/>us kun la le&gcirc;o;
      </ekz>
      <ekz>
        la honoro <tld/>is en li kun la timo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</ind></trd>,
      <trd>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
    </trdgrp>
    <trdgrp lng="de">
      <trd>in Konflikt geraten</trd>,
      <trd>versto&szlig;en (gegen)</trd>,
      <trd>kollidieren</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kolidovat</trd>,
    <trd>srazit se</trd>
  </trdgrp>
  <trd lng="es">colisionar</trd>
  <trdgrp lng="fr">
    <trd>entrer en <ind>collision</ind></trd>
    <trd>se <ind>caramboler</ind></trd>,
    <trd>emboutir<klr tip="ind"> (entrer en collision)</klr></trd>,
    <trd>heurter</trd>,
    <trd>se <ind>heurter</ind> (&agrave;)</trd>,
    <trd>percuter<klr tip="ind"> (heurter)</klr></trd>,
    <trd>rentrer dans<klr tip="ind"> (entrer en collision
    avec)</klr></trd>,
    <trd>tamponner<klr tip="ind"> (heurter)</klr></trd>,
    <trd>t&eacute;lescoper<klr tip="ind"> (heurter)</klr></trd>
  </trdgrp>
  <trd lng="nl">botsen</trd>
  <trdgrp lng="sk">
    <trd>kolidovať</trd>,
    <trd>nezhodovať sa</trd>,
    <trd>zraziť sa</trd>
  </trdgrp>
</drv>

<drv mrk="kolizi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o de vagonaroj, de partianoj, de interesoj, de sentoj;
      </ekz>
      <ekz>
        la eterna kaj freneziga <tld/>o de vortoj kaj formoj en la
        materialo de artefarita lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj de gentoj en unu lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la formoj venas en <tld/>on unu kun la alia<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_t;&c_y;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kolize</trd>,
    <trd>rozpor</trd>,
    <trd>srážka</trd>,
    <trd>střet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zusammensto&szlig;</trd>,
    <trd>Kollision</trd>
  </trdgrp>
  <trd lng="en">collision</trd>
  <trd lng="es">colisi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>collision</trd>,
    <trd>antagonisme<klr tip="ind"> (heurt)</klr></trd>
    <trd>carambolage<klr tip="ind"> (heurt)</klr></trd>,
    <trd>choc<klr tip="ind"> (heurt)</klr></trd>,
    <trd>conflit<klr tip="ind"> (heurt)</klr></trd>,
    <trd>heurt</trd>,
    <trd>opposition<klr tip="ind"> (heurt)</klr></trd>,
    <trd>tamponnement<klr tip="ind"> (heurt)</klr></trd>,
    <trd>t&eacute;lescopage</trd>
  </trdgrp>
  <trd lng="nl">botsing</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_l;&c_i;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolízia</trd>,
    <trd>zrážka</trd>
  </trdgrp>
</drv>
</art>
