<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teler.xml,v 1.23 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>teler</rad>/o
</kap>

<drv mrk="teler.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="teler.0o.mangxi">
    <dif>
      Iom kava, plata, malprofunda vazo, el kiu oni man&gcirc;as.
    </dif>
  </snc>
  <snc mrk="teler.0o.similante">
    <dif>
      Io similanta <tld/>on:
      <ekz>
        <ind>pesil<tld/>o</ind>
        <klr>(pesiltaso)</klr>.
        <trd lng="bg">&c_b;&c_l;&c_ju;&c_d;&c_o; &c_n;&c_a; &c_v;&c_je;&c_z;&c_n;&c_a;</trd>
        <trd lng="de">Waagschale</trd>
        <trd lng="fa">&f_k;&a_f;&a_h;&a_hamza_sure; &a_t;&a_r;&a_A;&a_z;&a_w;</trd>
        <trdgrp lng="hu">
          <trd>m&eacute;rlegserpeny&odblac;</trd>,
          <trd>m&eacute;rlegt&aacute;ny&eacute;r</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plad.0o">plado</ref>,
      <ref cel="kalik.0o">kaliko</ref>,
      <ref cel="tas.0o">taso</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_je;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_i;&c_n;&c_i;&c_ja;</trd>
  <trd lng="ca">plat</trd>
  <trdgrp lng="cs">
    <trd>tal&iacute;&rcaron;</trd>,
    <trd>talíř</trd>
  </trdgrp>
  <trd lng="de">Teller</trd>
  <trd lng="en">plate</trd>
  <trd lng="es">plato</trd>
  <trd lng="fa">&a_b;&a_sh;&a_q;&a_A;&a_b;</trd>
  <trd lng="fr">assiette</trd>
  <trd lng="hu">t&aacute;ny&eacute;r</trd>
  <trd lng="id">piring</trd>
  <trd lng="nl">bord <klr>(eten)</klr></trd>
  <trd lng="pl">talerz</trd>
  <trdgrp lng="pt">
    <trd>prato</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">tanier</trd>
  <trd lng="sv">tallrik</trd>
  <trd lng="tr">tabak</trd>
</drv>
</art>
