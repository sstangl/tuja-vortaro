<vortaro>
<art mrk="$Id: lukan.xml,v 1.17 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>lukan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lukan.0o">
  <kap><tld/>o</kap>
  <snc mrk="lukan.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="skarab.cervo0o.ZOO">cervoskarabo</ref>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_k;-&c_a;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_b;&c_r;&c_malmol;&c_m;&c_b;&c_a;&c_r;-&c_r;&c_o;&c_g;&c_a;&c_ch;</trd>
  <trd lng="cs">obyvatel Lukanie</trd>
  <trd lng="de">Hirschk&auml;fer</trd>
  <trd lng="en">stag beetle</trd>
  <trd lng="hu">szarvasbog&aacute;r</trd>
  <trd lng="nl">vliegend <ind>hert</ind></trd>
  <trd lng="pt">lucan&iacute;deo</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_u;&c_k;-&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_g;&c_a;&c_ch; <klr tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="sk">roháč <klr>(chrobák)</klr></trd>
</drv>
</art>
