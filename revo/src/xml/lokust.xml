<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lokust.xml,v 1.18 2013/09/01 13:10:35 revo Exp $">
<kap>
  <rad>lokust</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lokust.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="akride.0oj">akridedoj</ref>
      (<trd lng="la">Locusta</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Wanderheuschrecke</trd>
  <trdgrp lng="en">
    <trd>locust</trd>,
    <trd>grasshopper</trd>
  </trdgrp>
  <trd lng="es">langosta</trd>
  <trd lng="fr">sauterelle</trd>
  <trd lng="hu">sz&ouml;cske</trd>
  <trd lng="nl">sprinkhaan</trd>
  <trdgrp lng="pl">
    <trd>pasikonik</trd>,
    <trd>konik polny</trd>,
    <trd>szara&nacute;cza</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gafanhoto</trd>,
    <trd>locusta</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_z;&c_n;&c_je;&c_ch;&c_i;&c_k;</trd>
</drv>
</art>
