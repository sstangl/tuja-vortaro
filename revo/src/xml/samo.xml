<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: samo.xml,v 1.10 2014/06/24 10:10:18 revo Exp $">
<kap>
  <rad>samo</rad>/o
</kap>

<drv mrk="samo.S0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="samo.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insularo en la <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>.
      Teritorio de &scirc;tato Samoo.
    </dif>
    <trd lng="fr">Samoa</trd>
    <trdgrp lng="pl">
      <trd>Wyspy Samoa</trd>
    </trdgrp>
  </snc>
  <snc mrk="samo.S0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la samnoma insularo kun &ccirc;efurbo Apio.
      ISO-3166 kodo: WS.
    </dif>
    <trd lng="fr">Etat ind&eacute;pendant des Samoa</trd>
    <trdgrp lng="pl">
      <trd>Niezale&zdot;ne Pa&nacute;stwo <ind>Samoa</ind></trd>,
      <trd>Samoa Zachodnie<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_S;&c_a;&c_m;&c_o;&c_a;</trd>
  <trd lng="de">Samoa</trd>
  <trd lng="en">Samoa</trd>
  <trd lng="fa">&a_s;&a_A;&a_m;&a_w;&a_A_madda;</trd>
  <trd lng="id">Samoa</trd>
  <trd lng="it">Samoa</trd>
  <trd lng="nl">Samoa</trd>
  <trd lng="pt">Samoa</trd>
</drv>

<drv mrk="samo.OkcidentaS0o">
  <kap>
    Okcidenta <tld lit="S"/>o
    <fnt>
      <vrk>
        <url ref="http://www.esperanto.be/fel/jar/landoj.php">La landoj
        de l' tero</url>
      </vrk>
   </fnt>
  </kap>
  <snc mrk="samo.OkcidentaS0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Iama nomo de <ref tip="dif" cel="samo.S0o.sxtato">Samoo</ref>.
    </dif>
    <trd lng="de">Westsamoa</trd>
    <trd lng="fa">&a_s;&a_A;&a_m;&a_w;&a_A_madda;&f_y; &a_gh;&a_r;&a_b;&f_y;</trd>
    <trd lng="fr"><ind>Samoa</ind> occidentales</trd>
    <trd lng="nl">West-Samoa<klr tip="ind"> (arch.)</klr></trd>
    <trd lng="id"><ind>Samoa</ind> Barat</trd>
    <trd lng="it"><ind>Samoa</ind> Occidentali</trd>
    <trdgrp lng="pl">
      <trd>Samoa Zachodnie<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>

