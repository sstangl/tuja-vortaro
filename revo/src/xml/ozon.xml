<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ozon.xml,v 1.21 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>2</ofc> <rad>ozon</rad>/o
</kap>
<drv mrk="ozon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Triopa <ref tip="super" cel="oksige.0o">oksigeno</ref>,
      produktebla el la duopa ekzemple per efiko de elektro
      kaj diferenca de tiu gaso per karakteriza odoro kaj pli forta
      oksidiga kapablo:
      <ekz>
        en dense lo&gcirc;ataj regionoj la nivelo de <tld/>o en la
        surfacproksima aerzono regule superas la limvaloron permesitan
        de
        EU<fnt><aut>Roland Rotsaert</aut>: <vrk>Medio-indulgo de la
        &ccirc;efaj
        ekonomiaj sektoroj</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero
        3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_o;&c_n;</trd>
  <trd lng="bg">&c_o;&c_z;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>kyslík s troj atomovou molekulou</trd>,
    <trd>ozón</trd>
  </trdgrp>
  <trd lng="de">Ozon</trd>
  <trd lng="el">&omikron_ton;&zeta;&omikron;&ny;</trd>
  <trd lng="en">ozone</trd>
  <trdgrp lng="fr">
    <trd>ozone</trd>
  </trdgrp>
  <trd lng="hu">&oacute;zon</trd>
  <trd lng="id">ozon</trd>
  <trd lng="nl">ozon</trd>
  <trd lng="pt">oz&ocirc;nio</trd>
  <trd lng="ru">&c_o;&c_z;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>O3</trd>,
    <trd>ozón</trd>
  </trdgrp>
  <trd lng="sv">ozon</trd>
</drv>

<drv mrk="ozon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elektrizi la oksigenon de l' aero, por &gcirc;in aliigi en <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_z;&c_o;&c_n;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">působit ozónem</trd>
  <trd lng="de">ozonisieren</trd>
  <trd lng="en">ozonize</trd>
  <trd lng="hu">&oacute;zoniz&aacute;l</trd>
  <trd lng="nl">ozoniseren</trd>
  <trd lng="ru">&c_o;&c_z;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ozonizovať</trd>,
    <trd>pôsobiť ozónom</trd>
  </trdgrp>
</drv>
</art>
