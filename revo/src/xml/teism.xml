<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teism.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>teism</rad>/o
</kap>
<drv mrk="teism.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Doktrina kredo je ekzisto de iu Dio, kreinto de la universo.
    </dif>
    <refgrp tip="sin">
      <ref cel="deism.0o">deismo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">teisme</trd>
  <trd lng="cs">závislost na čaji</trd>
  <trd lng="de">Theismus</trd>
  <trd lng="en">theism</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_d;&a_A;&f_p;&a_r;&a_s;&a_t;&f_y;</trd>,
    <trd>&a_kh;&a_d;&a_A;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  </trdgrp>
  <trd lng="hu">teizmus</trd>
  <trd lng="nl">the&iuml;sme</trd>
  <trd lng="pl">teizm</trd>
  <trdgrp lng="pt">
    <trd>teismo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">závislosť na čaji</trd>
</drv>
</art>
