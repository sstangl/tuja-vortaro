<kap>
  <rad>statik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="statik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Parto de la mekaniko, kiu pritraktas la ekvilibron de la fortoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="cs">statika</trd>
  <trd lng="de">Statik</trd>
  <trd lng="en">statics</trd>
  <trd lng="es">est&aacute;tica <klr tip="ind"> (rama de la mec&aacute;nica) </klr>
  </trd>
  <trdgrp lng="fa">
    <trd>&a_A;&f_y;&a_s;&a_t;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_A;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">statique
    <klr tip="ind"> (branche de la m&eacute;canique)
    </klr>
  </trd>
  <trd lng="hu">statika</trd>
  <trd lng="nl">statica</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">statika</trd>
  <trd lng="sv">statik</trd>
</drv>
</art>
