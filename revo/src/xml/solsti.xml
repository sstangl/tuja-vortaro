<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: solsti.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>solstic</rad>/o
</kap>
<drv mrk="solsti.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">AST</uzo>
    <dif>
      Tempo, kiam la suno estas plej malproksime de la ekvatoro:
      <ekz>
        somera, vintra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_n;&c_c;&c_a;&c_s;&c_t;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>slunovrat</trd>,
    <trd>solsticium</trd>
  </trdgrp>
  <trd lng="de">Sonnenwende</trd>
  <trd lng="fr">solstice</trd>
  <trd lng="hu">napfordul&oacute;</trd>
  <trdgrp lng="nl">
    <trd>solstitium</trd>,
    <trd>dag-en-nachtevening</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">slnovrat</trd>
</drv>

<drv mrk="solsti.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj:
      <ekz>
        <tld/>aj punktoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">obratníkový</trd>
  <trd lng="fr">solsticial</trd>
  <trd lng="sk">obratníkový</trd>
</drv>
</art>
