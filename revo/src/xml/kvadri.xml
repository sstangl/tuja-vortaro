<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvadri.xml,v 1.18 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kvadril</rad>/o
</kap>
<drv mrk="kvadri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Franca malnova <ref tip="lst" cel="danc.0o"
      lst="voko:dancoj">danco</ref>, en kiu du
      grupoj da du paroj dancas unu kontra&ubreve; alia:
      <ekz>
        alkuris adjutanto kaj invitis &scirc;in al <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 14a, p.
        75a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_r;&c_y;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kadril</trd>,
    <trd>kadrila</trd>,
    <trd>kvadrila</trd>,
    <trd>čtverylka</trd>
  </trdgrp>
  <trd lng="de">Quadrille</trd>
  <trd lng="en">quadrille</trd>
  <trd lng="fr">quadrille</trd>
  <trdgrp lng="hu">
    <trd>francia n&eacute;gyes</trd>,
    <trd>quadrille</trd>
  </trdgrp>
  <trd lng="nl">quadrille</trd>
  <trd lng="pl">kwadryl</trd>
  <trd lng="pt">quadrilha</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_r;&c_i;&c_l;&c_mol;</trd>
  <trd lng="sk">štvorylka</trd>
  <trd lng="sv">kadrilj</trd>
</drv>
</art>
