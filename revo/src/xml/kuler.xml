<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kuler.xml,v 1.24 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kuler</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kuler.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kuler.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ilo konsistanta
      el tenilo kaj kava, ovoforma a&ubreve; ronda ekstremo,
      per kiu oni &ccirc;erpas flua&jcirc;ojn a&ubreve;
      duonsolidajn nutra&jcirc;ojn:
      <ekz>
        <tld/>o el ligno;
      </ekz>
      <ekz>
        pli allogas <tld/>o da mielo ol da vinagro
        barelo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>el malplena telero vane &ccirc;erpas <tld/>o</ind>
        <klr>(kontra&ubreve; neesto ne helpas protesto)</klr>
        <trd lng="fr">la plus belle fille du monde ne peut donner
        que ce qu'elle a</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_y;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_o;&c_n;&c_ib;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_s;&c_u;&c_p;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_malmol;&c_zh;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">cullera</trd>
  <trd lng="cs">l&zcaron;&iacute;ce</trd>
  <trd lng="de">L&ouml;ffel</trd>
  <trd lng="en">spoon</trd>
  <trd lng="es">cuchara</trd>
  <trd lng="fa">&a_q;&a_A;&a_sh;&a_q;</trd>
  <trd lng="fr">cuill&egrave;re</trd>
  <trdgrp lng="he">
    <trd>&kaf;&fpe;</trd>,
    <trd>&kaf;&pe;&yod;&tav;</trd>
  </trdgrp>
  <trd lng="hu">kan&aacute;l</trd>
  <trd lng="id">sendok</trd>
  <trd lng="nl">lepel</trd>
  <trd lng="pl">&lstroke;y&zdot;ka</trd>
  <trd lng="pt">colher (&eacute;) <klr tip="ind">(subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_zh;&c_k;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_je;&c_d;&c_y;)</klr></trd>,
    <trd>&c_ch;&c_je;&c_r;&c_p;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_ja;
    &c_s;&c_u;&c_p;&c_a;)</klr></trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">lyžica</trd>
  <trd lng="sv">sked</trd>
  <trd lng="tr">ka&scedil;&inodot;k</trd>
</drv>

<drv mrk="kuler.sup0o">
  <kap>sup<tld/>o</kap>
  <snc mrk="kuler.sup0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ordinara
      <ref tip="super" cel="kuler.0o.KUI"><tld/>o</ref>
      uzebla por man&gcirc;ado,
      kun proksimume 15-mililitra enhaveco:
      <ekz>
        Leporido man&gcirc;is el telero semolan ka&ccirc;on. Kaj ne simple 
        man&gcirc;is, sed man&gcirc;is per sup<tld/>o
        <fnt>
          <aut>M. Plackovskij, trad. M. Povorin</aut>,
          <vrk><url
          ref="http://mpovorin.narod.ru/porinfanajxoj/la_suneto_por_memoro/erinaceto_kiun_oni_povis_glatumi/erinaceto_kiun_oni_povis_glatumi.html">
          Erinaceto, kiun oni povis glatumi</url></vrk>,
          <lok>en: La Suneto por Memoro, [vidita 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">polévková lžíce</trd>
  <trd lng="fr">cuill&egrave;re &agrave; soupe</trd>
  <trd lng="id"><ind>sendok</ind> makan</trd>
  <trd lng="sk">polievková lyžica</trd>
</drv>

<drv mrk="kuler.te0o">
  <kap>te<tld/>o</kap>
  <snc mrk="kuler.te0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda
      <ref tip="super" cel="kuler.0o.KUI"><tld/>o</ref>
      uzata por dozado,
      man&gcirc;ado de delikata&jcirc;oj, kun proksimume
      5-mililitra enhaveco:
      <ekz>
        1 te<tld/>o da vanilekstrakto
        <fnt>
          <aut>Esp&eacute;ranto-Jeunes</aut>,
          <vrk><url
          ref="http://fagot.alain.free.fr/kuirado/Rpf01/html/CxS18.php">
          &Ccirc;okoladlakto la&ubreve; malnova modo</url></vrk>,
          <lok>1996-2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kávová lžička</trd>
  <trd lng="fr">cuill&egrave;re &agrave; caf&eacute;</trd>
  <trd lng="id"><ind>sendok</ind> teh</trd>
  <trd lng="sk">čajová lyžička</trd>
</drv>
</art>
