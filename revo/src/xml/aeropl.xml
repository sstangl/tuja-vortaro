<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aeropl.xml,v 1.27 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>aeroplan</rad>/o
</kap>
<drv mrk="aeropl.0o">
  <kap><tld/>o</kap>
  <snc mrk="aeropl.0o.AVI">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">AVI</uzo>
    <dif>
      Aparato kapabla sin levi kaj movi en la aero, ne estante pli
      malpeza
      ol &gcirc;i, dank' al la premo de la vento sur &gcirc;iaj
      surfacoj:
      <ekz>
        la blankuloj bombardas ilin de <tld/>oj
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova
        &scirc;akproblemo, 3</lok></fnt>;
      </ekz>
      <ekz>
        en la aero zumas <tld/>oj kaj blankaj reflektoroj lumigas la
        defendantajn sin stratojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="super">
    <ref cel="avi.0o">avio</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_e;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_a;&c_m;&c_o;&c_l;&c_je;&c_t;</trd>
  <trd lng="de">Flugzeug</trd>
  <trdgrp lng="en">
    <trd>airplane</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aeroplano</trd>,
    <trd>avi&oacute;n</trd>
  </trdgrp>
  <trd lng="fa">&a_h;&a_w;&a_A;&f_p;&f_y;&a_m;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>a&eacute;roplane</trd>,
    <trd>avion</trd>
  </trdgrp>
  <trd lng="he">&alef;&vav;&yod;&resh;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>rep&uuml;l&odblac;g&eacute;p</trd>,
    <trd>aeropl&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aeroplano</trd>,
    <trd>aereo</trd>
  </trdgrp>
  <trd lng="nl">vliegtuig</trd>
  <trdgrp lng="pl">
    <trd>aeroplan<klr> (przest.)</klr></trd>,
    <trd>samolot</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aeroplano</trd>,
    <trd>avi&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_a;&c_e;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">lietadlo</trd>
  <trd lng="sv">flygplan</trd>
  <trd lng="tr">u&ccedil;ak</trd>
  <trd lng="sk">aeropolán, lietadlo</trd>
  <trd lng="sk">aeropolán, lietadlo</trd>
  <trdgrp lng="cs">
    <trd>aero</trd>,
    <trd>aeroplán</trd>,
    <trd>motorové letadlo</trd>
  </trdgrp>
</drv>
</art>
