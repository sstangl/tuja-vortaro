<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: centia.xml,v 1.17 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>centiar</rad>/o
</kap>
<drv mrk="centia.0o">
  <kap><tld/>o</kap>
  <mlg>ca</mlg>
  <snc>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:areoj">mezurunuo</ref> de
      areo,
      centono de <ref tip="malprt" cel="ar1.0o">aro</ref>, egala al
      unu kvadrata <ref cel="metr.0o.unuo">metro</ref>.
    </dif>
    <trd lng="ca">centi&agrave;rea</trd>
    <trdgrp lng="en">
      <trd>centiare</trd>,
      <trd>centare</trd>
    </trdgrp>
    <trd lng="es">centi&aacute;rea</trd>
    <trd lng="fr">centiare</trd>
    <trd lng="hu">centi&aacute;r</trd>
    <trd lng="id">sentiare</trd>
    <trd lng="it">centiara</trd>
    <trd lng="nl">centiare</trd>
    <trd lng="pt">centiare</trd>
    <trd lng="ru">&c_s;&c_a;&c_n;&c_t;&c_i;&c_a;&c_r;</trd>
  </snc>
  <trd lng="cs">metr čtverečný</trd>
  <trd lng="sk">štvorcový meter</trd>
</drv>
</art>
