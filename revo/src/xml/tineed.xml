<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tineed.xml,v 1.12 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tineed</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tineed.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="tineed.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="lepido1.0oj">lepidopteroj</ref>
      (<trd lng="la">Tineidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_m;&c_o;&c_l;&c_ib;</ind> <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tineids</trd>
  <trd lng="cs">moli</trd>
  <trd lng="de"><klr>(Echte)</klr> Motten<klr tip="ind">
  (Familie)</klr></trd>
  <trd lng="fr">mites</trd>
  <trd lng="hu">val&oacute;di <ind>molyok</ind><klr tip="ind">
  (csal&aacute;d)</klr></trd>
  <trd lng="nl">motten<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">molowate</trd>
  <trdgrp lng="pt">
    <trd>tine&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_m;&c_o;&c_l;&c_i;</ind>
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">mole</trd>
</drv>
</art>
