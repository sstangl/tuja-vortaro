<art mrk="$Id: taoism.xml,v 1.15 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>taoism</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="taoism.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="daux.0ismo">da&ubreve;ismo</ref>
  </snc>
  <rim mrk="taoism.0o.RIM">
    &leftquot;Tao&rightquot; &leftquot;taoismo&rightquot; kaj
    &leftquot;taoisto&rightquot; estas pli uzataj, sed malpli regulaj
    formoj ol &leftquot;da&ubreve;o&rightquot;,
    &leftquot;da&ubreve;ismo&rightquot;,
    &leftquot;da&ubreve;isto&rightquot;.
    <aut>&jcirc;v</aut>
  </rim>
  <trd lng="ca">taoisme</trd>
  <trd lng="fa">&a_t;&a_A;&a_y_hamza;&a_w;&a_y_hamza;&f_y;&a_s;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>tao&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">taoizmus</trd>
  <trd lng="pl">taoizm</trd>
  <trdgrp lng="pt">
   <trd>taoismo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_o;&c_s;&c_i;&c_z;&c_m;</trd>
  <trd lng="zh">&#36947;&#25945;<klr> [d&agrave;oji&#257;o]</klr></trd>
  <trd lng="sk">taoizmus</trd>
  <trd lng="cs">taoizmus</trd>
</drv>
</art>
