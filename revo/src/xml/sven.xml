<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sven.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sven</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sven.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sven.0i.PSI">
    <dif>
      Perdi dum kelkaj momentoj la konscion kaj sinmovon:
      <ekz>
        rigardante la preparojn al la operacio, li <tld/>is<fnt>B</fnt>;
      </ekz>
      <ekz>
        fali <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        homoj kaj bestoj <tld/>i&gcirc;adis de la varmego<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_l;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ohnm&auml;chtig werden</trd>,
      <trd>in <ind>Ohnmacht</ind> fallen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'&eacute;vanouir</trd>,
      <trd>perdre <ind>connaissance</ind></trd>
    </trdgrp>
    <trd lng="hu">el&aacute;jul</trd>
    <trdgrp lng="it">
      <trd>svenire</trd>,
      <trd>perdere i <ind>sensi</ind></trd>, 
      <trd>perdere <ind>conoscienza</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bezwijmen</trd>,
      <trd>flauwvallen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol; <ind>&c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</ind></trd>,
      <trd>&c_u;&c_p;&c_a;&c_s;&c_t;&c_mol; &c_v; <ind>&c_o;&c_b;&c_m;&c_o;&c_r;&c_o;&c_k;</ind></trd>
    </trdgrp>
    <trd lng="sv">svimma</trd>
  </snc>
  <snc mrk="sven.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperi, fori&gcirc;i, disperdi&gcirc;i:
      <ekz>
        la odoro <tld/>is;
      </ekz>
      <ekz>
        la tago <tld/>as;
      </ekz>
      <ekz>
        la &ccirc;armo de la originalo ne <tld/>as en la traduko;
      </ekz>
      <ekz>
        la vortoj duon<tld/>e forsonis preter liaj oreloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>verschwinden</trd>,
      <trd>sich <ind>verfl&uuml;chtigen</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>disparaitre</trd>,
      <trd>s'&eacute;vanouir<klr> (disparaitre)</klr></trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elt&udblac;nik</trd>,
      <trd>elsz&aacute;ll</trd>,
      <trd>elhal</trd>,
      <trd>megsemmis&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>svanire</trd>,
      <trd>sfumare<klr> (svanire)</klr></trd>, 
      <trd>languire<klr> (fig.)</klr></trd>, 
    </trdgrp>
    <trd lng="nl">verdwijnen</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_r;&c_je;&c_t;&c_mol; <klr tip="amb">(&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">omdlít</trd>
  <trd lng="en">faint</trd>
  <trdgrp lng="sk">
    <trd>omdlieť</trd>,
    <trd>upadnúť do mdlôb</trd>,
    <trd>zamdlieť</trd>
  </trdgrp>
</drv>

<drv mrk="sven.0o">
  <kap><tld/>o</kap>
  <snc mrk="sven.0o.PSI">
    <dif>
      Momenta senkonscii&gcirc;o;
      <ref tip="sin" cel="sinkop.0o.sveno">sinkopo<sncref/></ref>.
    </dif>
    <trd lng="be">&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>&eacute;vanouissement</trd>
      <trd>syncope</trd>
    </trdgrp>
    <trd lng="hu">&aacute;jul&aacute;s</trd>
    <trd lng="it">svenimento</trd>
    <trdgrp lng="nl">
      <trd>flauwte</trd>,
      <trd>syncope</trd>,
      <trd>flauwvallen<klr tip="ind"> (zn.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_m;&c_o;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_m;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja; &c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sven.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malapero:
      <ekz>
        la for<tld/>o de la lasta vibrado de la sonoriloj.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="fr">disparition</trd>
    <trdgrp lng="hu">
      <trd>elt&udblac;n&eacute;s</trd>,
      <trd>elhal&aacute;s</trd>,
      <trd>megsemmis&uuml;l&eacute;s</trd>
    </trdgrp>
    <trd lng="nl">het in <ind>rook</ind> opgaan</trd>
    <trd lng="ru">&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bezvědomí</trd>,
    <trd>mdloba</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mdloba</trd>,
    <trd>zamdlenie</trd>
  </trdgrp>
</drv>

<drv mrk="sven.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="sven.0emo.PSI">
    <dif>
      Tia sento de korpa malforteco, mensa nebuleco, kia
      anta&ubreve;as <tld/>on:
      <ekz>
        spitante sufokan <tld/>emon li iris al la pordo
        <fnt>
          <aut>J. I. Francis</aut>,
          <vrk><url
          ref="http://www.autodidactproject.org/other/francis_deka.html">
          Okazos je la Deka</url></vrk>,
          <lok>en: Vitralo, 1960</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">faiblesse<klr> (tendance &agrave; l'&eacute;vanouissement)</klr></trd>
</drv>

<drv mrk="sven.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sven.0igi.PSI">
    <dif>
      Ka&ubreve;zi <tld/>on al:
      <ekz>
        duone <tld/>igita per la bato, li falis<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bat.sven0i.KOMUNE">svenbati</ref>
    </refgrp>
  </snc>
  <trd lng="cs">omráčit</trd>
  <trdgrp lng="fr">
    <trd>assommer</trd>,
    <trd>faire perdre <ind>connaissance</ind></trd>
  </trdgrp>
  <trd lng="sk">omráčiť</trd>
</drv>
</art>
