<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elektr12.xml,v 1.15 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>elektromagnetism</rad>/o
</kap>
<drv mrk="elektr12.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="fizik.0o">fiziko</ref>,
      kiu pritraktas la interagojn inter elektraj kaj magnetaj kampoj,
      kaj iliaj fontoj, nome elektraj &scirc;argoj kaj kurentoj.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_m;&c_a;&c_g;&c_n;&c_e;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_m;&c_a;&c_g;&c_n;&c_je;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="cs">elektromagnetizmus</trd>
  <trd lng="de">Elektromagnetismus</trd>
  <trd lng="es">electromagnetismo</trd>
  <trd lng="fr">&eacute;lectromagn&eacute;tisme</trd>
  <trdgrp lng="hu">
    <trd>elektrom&aacute;gnesess&eacute;g</trd>,
    <trd>elektromagnetizmus</trd>
  </trdgrp>
  <trd lng="pl">elektromagnetyzm</trd>
  <trd lng="pt">eletromagnetismo</trd>
  <trd
  lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_m;&c_a;&c_g;&c_n;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">elektromagnetizmus</trd>
  <trd lng="sv">elektromagnetism</trd>
</drv>
</art>
