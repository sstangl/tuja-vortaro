<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elastan.xml,v 1.6 2012/07/15 08:10:24 revo Exp $">
<kap>
  <rad>elastan</rad>/o
</kap>

<drv mrk="elastan.0o">
  <kap><tld/>o</kap>
  <snc mrk="elastan.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tre elasta sinteza fibro precipe el poliuretano, el kiu oni teksas
      <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofon</ref> interalie
      por
      vestoj:
      <ekz>
        komunlingve oni ofte aludas <tld/>on per la nomo Likrao
        <klr>...</klr>, sed tio estas registrita marko
        <fnt>
        <bib>Viki</bib>,
        <lok>elastano, 2005-08-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>elast&agrave;</trd>,
    <trd>esp&agrave;ndex</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Elasthan</trd>,
  	<trd>Lycra</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>spandex</trd>,
    <trd>elastane</trd>,
    <trd>lycra</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;lastane</trd>,
    <trd>lycra</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elastan</trd>,
    <trd>lycra</trd>,
    <trd>lajkra</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>elastano</trd>,
    <trd>lycra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_a;&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_l;&c_a;&c_j;&c_k;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>

