<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: snuf.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>snuf</rad>/i
  <fnt>N</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="snuf.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="snuf.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de forta enspiro:
      <ekz>
        la filino respondas <tld/>etante kun kulpo-konscio:
        mi scias patro, <tld/>, <tld/>
        <fnt>
          <aut>Neciklopedio</aut>,
          <vrk><url
          ref="http://neciklopedio.wikia.com/wiki/%E2%80%A0_Kristanismo_%E2%80%A0">
          Kristanismo</url></vrk>,
          <lok>2013-08-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">snif !</trd>
</drv>

<drv mrk="snuf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forte enspiri en la nazon.
    </dif>
    <refgrp tip="vid">
      <ref cel="flar.0i">flari</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_c;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>frkat</trd>,
    <trd>funět</trd>,
    <trd>větřit</trd>,
    <trd>čmuchat</trd>,
    <trd>šňupat tabák</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schn&uuml;ffeln</trd>,
    <trd>schnuppern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>snuff</trd>
  </trdgrp>
  <trd lng="fr">renifler</trd>
  <trdgrp lng="hu">
    <trd>szimatol</trd>,
    <trd>szagl&aacute;szik</trd>
  </trdgrp>
  <trd lng="nl">snuiven</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_je;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vetriť</trd>,
    <trd>ňuchať</trd>
  </trdgrp>
</drv>

<drv mrk="snuf.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="snuf.0egi.KOMUNE">
    <dif>
      Forte, brue <tld/>i, kiel iuj grandaj hufbestoj:
      <ekz>
        ta&ubreve;re ili <tld/>egis kaj baraktadis
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url
          ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ren&acirc;cler</trd>
</drv>
</art>
