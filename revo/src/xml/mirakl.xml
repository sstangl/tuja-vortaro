<vortaro>
<art mrk="$Id: mirakl.xml,v 1.19 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mirakl</rad>/o
</kap>
<drv mrk="mirakl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eksterordinara fenomeno kontra&ubreve;a al la le&gcirc;oj de naturo.
    </dif>
    <trdgrp lng="de">
      <trd>&uuml;bernat&uuml;rliches Ereignis</trd>,
      <trd>Wunder</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mirindega farita&jcirc;o:
      <ekz>
        la <tld/>oj de la scienco.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>prodigi</trd>,
      <trd>meravella</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Wunderwerk</trd>,
      <trd>Wunder</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_d;</trd>
  <trd lng="bg">&c_ch;&c_u;&c_d;&c_o;</trd>
  <trd lng="ca">miracle</trd>
  <trd lng="cs">zázrak</trd>
  <trd lng="en">miracle</trd>
  <trdgrp lng="es">
    <trd>milagro</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_ayn;&a_j;&a_z;&a_h;</trd>
  <trd lng="fr">miracle</trd>
  <trd lng="hu">csoda</trd>
  <trdgrp lng="nl">
    <trd>wonder</trd>,
    <trd>mirakel</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_u;&c_d;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>div</trd>,
    <trd>z&aacute;zrak</trd>,
    <trd>čudo</trd>
  </trdgrp>
</drv>
</art>
