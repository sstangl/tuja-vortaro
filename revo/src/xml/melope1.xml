<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: melope1.xml,v 1.13 2014/10/25 08:10:13 revo Exp $">
<kap>
  <rad>melopep</rad>/o
</kap>
<drv mrk="melope1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukurb.0o">kukurbo</ref>
      (<trd lng="la">Cucurbita melopepo</trd>), kulturata
      en Ameriko.
    </dif>
  </snc>
  <trd lng="de">Turbank&uuml;rbis</trd>
  <trdgrp lng="en">
    <trd>squash</trd>,
    <trd>summer squash</trd>,
    <trd>yellow squash</trd>,
    <trd>spaghetti squash</trd>
  </trdgrp>
  <trd lng="fr">courge spaghetti</trd>
  <trd lng="hu">koron&aacute;s <ind>t&ouml;k</ind></trd>
  <trd lng="nl">tulbandkalebas</trd>
  <trdgrp lng="pt">
    <trd>ab&oacute;bora</trd>,
    <trd>jerimum</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_t;&c_i;&c_s;&c_s;&c_o;&c_n;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_t;&c_y;&c_k;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
