<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: helike.xml,v 1.8 2013/08/31 16:10:32 revo Exp $">
<kap>
  <rad>heliked</rad>/oj
</kap>

<drv mrk="helike.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la klaso <ref
      tip="malprt" cel="gastro2.0oj">
      gastropodoj</ref>
      (<trd lng="la">Helicidae</trd>), kun spirala konko, kiun ili
      fermas &ccirc;e frosto a&ubreve; sekeco.
    </dif>
  </snc>
  <trd lng="de">Schnirkelschnecken</trd>
  <trd lng="hu">&eacute;ticsigaf&eacute;l&eacute;k</trd>
  <trd lng="it">elicidi</trd>
  <trd lng="pl">&sacute;limakowate</trd>
  <trdgrp lng="pt">
    <trd>helic&iacute;deos</trd>,
    <trd>hel&iacute;cidas</trd>
  </trdgrp>
</drv>
</art>

