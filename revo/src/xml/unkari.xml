<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: unkari.xml,v 1.10 2013/08/29 09:10:22 revo Exp $">
<kap>
   <rad>unkari</rad>/o
</kap>
<drv mrk="unkari.0o">
   <kap><tld/>o</kap>
   <snc mrk="unkari.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="rubi.0oj">rubiacoj</ref>
       (<trd lng="la">Uncaria</trd>) kun precipe aziaj arbustaj specioj:
       <ekz>
         Ora haste sendis a&ccirc;eti iom da <tld/>o
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 84a, volumo 3a, p.
         50a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="ca">
     <trd>unc&agrave;ria</trd>,
     <trd>g&egrave;nere Uncaria</trd>
   </trdgrp>
   <trd lng="de">Klimmstrauch</trd>
   <trdgrp lng="en">
     <trd>gambir</trd>,
     <trd>gambier</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>Uncaria<klr> (kasvisuku, mm. gambiirikasvi ja
perunkynsiliaani)</klr></trd>
   </trdgrp>
   <trd lng="fr">gambier</trd>
   <trdgrp lng="hu">
     <trd>gambir(cserje)</trd>,
     <trd>kambir</trd>
   </trdgrp>
   <trd lng="it">uncaria</trd>
   <trd lng="nl">uncaria<klr tip="ind"> (geslacht)</klr></trd>
   <trd lng="pl">czepota</trd>
   <trd lng="pt">gambir</trd>
</drv>
</art>

