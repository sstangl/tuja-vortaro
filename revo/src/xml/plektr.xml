<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plektr.xml,v 1.12 2015/07/04 08:37:41 revo Exp $">
<kap>
<rad>plektr</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="plektr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Mane tenata a&ubreve; &ccirc;efingre fiksita ilo,
      per kiu oni vibrigas la kordojn de
      <ref tip="malprt" cel="instru2.kord0o">kordinstrumento</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>plektrum</trd>,
    <trd>trsátko</trd>
  </trdgrp>
  <trd lng="de">Plektron</trd>
  <trdgrp lng="en">
    <trd>plectrum</trd>,
    <trd>pick</trd>
  </trdgrp>
  <trd lng="es">plectro</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;diator</trd>,
    <trd>plectre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>penget&odblac;</trd>,
    <trd>plektrum</trd>
  </trdgrp>
  <trd lng="nl">plectrum</trd>
  <trdgrp lng="pt">
    <trd>palheta <klr tip="amb">(m&uacute;s.)</klr></trd>,
    <trd>plectro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_k;&c_t;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">brnkadlo</trd>
</drv>

<drv mrk="plektr.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Sonigi muzikilon per plektro.
    </dif>
  </snc>
  <trd lng="cs">brnkat trsátkem</trd>
  <trd lng="en">pick<klr tip="amb"> (stringed instrument)</klr></trd>
  <trd lng="hu">penget</trd>
  <trd lng="nl">met een <ind>plectrum</ind> bespelen</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; <ind>&c_m;&c_je;&c_d;&c_i;&c_a;&c_t;&c_o;&c_r;&c_o;&c_m;</ind></trd>
  <trd lng="sk">brnkať brnkadlom</trd>
</drv>
</art>
