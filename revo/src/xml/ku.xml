<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ku.xml,v 1.7 2015/10/30 11:10:19 revo Exp $">
<kap>
    <ofc>9</ofc><rad>ku</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ku.0o">
  <kap><tld/>o</kap>
  <snc mrk="ku.0o.SNC">
    <uzo tip="fak">TIP</uzo>
    <dif>
    Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref> (q,
    Q).
    </dif>
    <trdgrp lng="fr">
    <trd>q</trd>,
    <trd>Q</trd>
    </trdgrp>
  </snc>
  <snc>
  <dif>
  <ref tip="dif" cel="grafem.0o">Grafemo</ref>, uzata i.a. kiel:
  </dif>
  <trd lng="fr">q<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (maj. kaj grase) simbolo por la aro de &ccirc;iuj racionalaj nombroj:
  <ekz><em>Q</em></ekz>;
  </dif>
  <trd lng="fr">Q<klr> (ensemble des nombres rationnels)</klr></trd>
  </subsnc>  
  </snc>
  <trdgrp lng="cs">
    <trd>Q <klr>(neesperantsk&eacute; p&iacute;smeno a souhl&aacute;ska)</klr></trd>,
    <trd>q</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Q</trd>,
    <trd>q</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>Q</trd>,
    <trd>q</trd>
  </trdgrp>
</drv>
</art>

