<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: martel.xml,v 1.25 2014/09/14 08:10:12 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>martel</rad>/o
</kap>

<drv mrk="martel.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <snc>
     <dif>
       Batilo konsistanta el peza ferpeco kun ligna tenilo kaj servanta
       por
       enigi najlojn, for&gcirc;i feron ktp.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_l;&c_a;&c_t;&c_o;&c_k;</trd>,
     <trd>&c_m;&c_o;&c_l;&c_a;&c_t;</trd>
   </trdgrp>
   <trd lng="bg">&c_ch;&c_u;&c_k;</trd>
   <trd lng="br">morzhol</trd>
   <trd lng="de">Hammer</trd>
   <trd lng="en">hammer</trd>
   <trd lng="es">martillo</trd>
   <trd lng="fa">&f_ch;&f_k;&a_sh;</trd>
   <trd lng="hu">kalap&aacute;cs</trd>
   <trdgrp lng="id">
     <trd>martil</trd>,
     <trd>palu</trd>
   </trdgrp>
   <trd lng="it">martello</trd>
   <trd lng="nl">hamer</trd>
   <trdgrp lng="pl">
     <trd>m&lstroke;ot</trd>,
     <trd>m&lstroke;otek</trd>
   </trdgrp>
   <trd lng="pt">martelo</trd>
   <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_l;&c_o;&c_t;</trd>,
     <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>
   </trdgrp>
   <trd lng="sv">hammare</trd>
     <trd lng="fr">marteau</trd>
</drv>

<drv mrk="martel.0i">
   <kap>
     <tld/>i
   </kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Batadi per <tld/>o.
     </dif>
     <trd lng="be">&c_b;&c_ib;&c_c;&c_mol; &c_m;&c_o;&c_l;&c_a;&c_t;&c_a;&c_m;</trd>
     <trd lng="bg">&c_ch;&c_u;&c_k;&c_a;&c_m;</trd>
     <trd lng="br">morzholat</trd>
     <trd lng="de">h&auml;mmern</trd>
     <trd lng="en">hammer</trd>
     <trdgrp lng="fa">
      <trd>&f_ch;&f_k;&a_sh;&zwnj;&f_k;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>&a_komo;
      <trd>&f_ch;&f_k;&a_sh; &a_z;&a_d;&a_n;</trd>
     </trdgrp>
     <trd lng="hu">kalap&aacute;l</trd>
     <trd lng="it">martellare</trd>
     <trd lng="id">[<ind>palu</ind>] memalu</trd>
     <trdgrp lng="pl">
       <trd>bi&cacute; <ind>m&lstroke;otkiem</ind></trd>,
       <trd>ku&cacute;</trd>,
       <trd>m&lstroke;otkowa&cacute;</trd>
     </trdgrp>
     <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_m;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m;</trd>
     <trd lng="sv">hamra</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Aparte kaj forte akcenti:
       <ekz>
         <tld/>i &ccirc;iun vorton<fnt>Z</fnt>
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_a;&c_k;&c_c;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
     </trdgrp>
     <trd lng="it">marcare<klr> (accentuare, scandire)</klr></trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Komprenigi, memorigi ion per ripetata penado:
       <ekz>
         <tld/>i ion en la
         memoron de iu (<ref tip="sin"
         cel="cerb.en0igi">encerbigi</ref>);
       </ekz>
       <ekz>
         <tld/>i per reklamo en la cerbojn de la publiko la
         efikecon de iu medikamento
       </ekz>
     </dif>
     <trd lng="be">&c_u;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
     <trd lng="hu">belever</trd>
     <trd lng="it">fissare<klr> (far/memorizzare)</klr></trd>
     <trdgrp lng="pl">
       <trd>ku&cacute;</trd>,
       <trd>wkuwa&cacute;</trd>,
       <trd>zakuwa&cacute;</trd>
     </trdgrp>
     <trd lng="ru">&c_v;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
   </snc>
   <trd lng="fr">marteler</trd>
   <trd lng="nl">hameren</trd>
   <trd lng="pt">martelar</trd>
</drv>

<drv mrk="martel.pneuxmata0o">
  <kap>pne&ubreve;mata <tld/>o</kap>
  <snc mrk="martel.pneuxmata0o.MASXINO">
    <dif>
      Pne&ubreve;mata <tld/>o martelbatas per impulso de la premita aero tra
      pi&scirc;to,
      reguligita per ventilo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_n;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; <ind>&c_m;&c_o;&c_l;&c_a;&c_t;</ind></trd>
  <trd lng="bg">&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_n; &c_ch;&c_u;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Drucklufthammer</trd>,
    <trd>Presslufthammer</trd>
  </trdgrp>
  <trd lng="en">jackhammer</trd>
  <trd lng="fr">marteau pneumatique</trd>
  <trd lng="it"><ind>martello</ind> pneumatico</trd>
  <trd lng="fa">&f_ch;&f_k;&a_sh; &a_b;&a_A;&a_d;&f_y;</trd>
  <trd lng="pl">m&lstroke;ot pneumatyczny</trd>
</drv>
</art>
