<kap>
<ofc>*</ofc>
<rad>aln</rad>/o
</kap>
<drv mrk="aln.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt"
       cel="betula.0oj">betulacoj</ref>
       (<trd lng="la">Alnus</trd>).
     </dif>
   </snc>
   <bld lok="../bld/aln.jpg">masklaj floroj kaj fruktokonusoj</bld>
   <trdgrp lng="be">
     <trd>&c_a;&c_l;&c_je;&c_sh;&c_y;&c_n;&c_a;</trd>, 
     <trd>&c_v;&c_o;&c_l;&c_mol;&c_h;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="br">
     <trd>gwernenn</trd>,
     <trd>gwezenn-wern</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>vern</trd>,
     <trd>g&egrave;nere Alnus</trd>
   </trdgrp>
   <trd lng="de">Erle</trd>
   <trdgrp lng="en">
     <trd>alder</trd>,
     <trd>elder</trd>
   </trdgrp>
   <trd lng="es">aliso</trd> 
   <trd lng="fr">aulne</trd>
   <trd lng="hu">&eacute;ger(fa)</trd>
   <trd lng="nl">els<klr tip="ind"> (boom)</klr></trd>
   <trd lng="pt">amieiro</trd>
   <trd lng="ru">&c_o;&c_l;&c_mol;&c_h;&c_a;</trd>
   <trd lng="sk">jel&scaron;a</trd>
   <trd lng="sv">al</trd>
  <trd lng="cs">ol≈°e</trd>
</drv>
