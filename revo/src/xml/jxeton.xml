<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jxeton.xml,v 1.10 2014/07/30 06:10:11 revo Exp $">
<kap>
 <rad>&jcirc;eton</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="jxeton.0o">
 <kap><tld/>o</kap>
 <snc mrk="jxeton.0o.KOMUNE">
   <dif>
     Kvaza&ubreve;-monero sen a&ubreve;tentiga marko, valida nur en
iu loko, en ludo
     a&ubreve; por iuspecaj ma&scirc;inoj:
     <ekz>
       dek-poenta <tld/>o;
     </ekz>
     <ekz>
       oni pagis la diversajn &gcirc;ojveturadojn kaj
distra&jcirc;ojn per plastaj
       <tld/>oj
       <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
     </ekz>
   </dif>
   <ref tip="vid" cel="poent.0o.SPO">poento</ref>
 </snc>
 <trd lng="de">Jeton<klr> (Spielmarke)</klr></trd>
 <trdgrp lng="en">
   <trd>token</trd>,
   <trd>chip</trd>,
   <trd>disk</trd>
 </trdgrp>
 <trd lng="es"><ind>ficha</ind> de juego</trd>
 <trd lng="fa">&f_zh;&a_t;&a_w;&a_n;</trd>
 <trd lng="fr">jeton</trd>
 <trdgrp lng="he">
   <trd>&alef;&samekh;&yod;&mem;&vav;&fnun;</trd>,
   <trd>&zayin;'&yod;&tet;&vav;&fnun;</trd>
 </trdgrp>
 <trd lng="hu">zseton</trd>
 <trdgrp lng="nl">
   <trd>penning<klr tip="ind"> (fiche)</klr></trd>,
   <trd>munt<klr tip="ind"> (fiche)</klr></trd>,
   <trd>fiche</trd>
 </trdgrp>
 <trd lng="pt">ficha</trd>
 <trdgrp lng="it">
   <trd>gettone</trd>,
   <trd>fiche</trd>
 </trdgrp>
</drv>
</art>

