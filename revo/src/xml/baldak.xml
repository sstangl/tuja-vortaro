<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: baldak.xml,v 1.18 2015/10/01 12:10:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>baldaken</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="baldak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="baldak.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tegmenta tapi&scirc;o sur stangoj super trono, altaro, lito,
      a&ubreve; 
      portata &scirc;irme super altranguloj dum solena&jcirc;oj:
      <ekz>
        la&ubreve;longe de alia muro staris &scirc;ia lito, kovrita per
        rozkolora 
        atlaskovrilo kaj &scirc;irmita per silka <tld/>o
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen 
        titolo</vrk>, <lok>&ccirc;apitro 15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_l;&c_d;&c_a;&c_h;&c_ib;&c_n;</trd>,
    <trd>&c_t;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>baldach&yacute;n</trd>,
    <trd>nebesa postele</trd>,
    <trd>tr&uring;nu</trd>
  </trdgrp>
  <trd lng="de">Baldachin</trd>
  <trd lng="en">baldachin</trd>
  <trd lng="fa">&a_A_madda;&a_s;&a_m;&a_A;&a_n;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>baldaquin</trd>,
    <trd>dais</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>baldachin</trd>,
    <trd>d&iacute;sztet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">baldakijn</trd>
  <trd lng="pl">baldachim</trd>
  <trd lng="pt">baldaquino</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_l;&c_d;&c_a;&c_h;&c_i;&c_n;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">baldach&yacute;n</trd>
</drv>
</art>
