<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mojos.xml,v 1.9 2015/11/04 13:10:19 revo Exp $">
<kap>
    <rad>mojos</rad>/a
    <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="mojos.0a">
  <kap><tld/>a</kap>
  <snc mrk="mojos.0a.junstila">
       <dif>
       <ref tip="super" cel="akronim.0o">Akronimo</ref>
       de <em>modernjunstila</em>, t.e. <ctl>mjs-a</ctl>;
       impresanta a&ubreve; pla&ccirc;anta al gejunuloj:
      <ekz>menciinda estas la mojosa &jcirc;urnalo La NASKa Fasko
        <fnt><bib>LOdE</bib>, <lok>2013 N&deg;8-9 (226-227)</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cool</trd>
  <trdgrp lng="es">
    <trd>chulo</trd>,
    <trd>majo</trd>,
    <trd>genial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>relax</trd>,
    <trd>sympa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>toll</trd>,
    <trd>genial</trd>,
    <trd>geil <klr tip="ind">(cool)</klr></trd>,
    <trd>abgefahren <klr tip="ind">(cool)</klr></trd>,
    <trd>angesagt <klr tip="ind">(cool)</klr></trd>,
    <trd><klr>(voll)</klr> krass <klr tip="ind">(cool)</klr></trd>,
    <trd><klr>(voll)</klr> schick <klr tip="ind">(cool)</klr></trd>,
    <trd>total inn <klr tip="ind">(cool)</klr></trd>,
  </trdgrp>
  <trd lng="pl">fajny</trd>
</drv>
<drv mrk="mojos.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
       Stato de tio, kio estas <tld/>a.
    </dif>
  </snc>
  <trd lng="en">coolness</trd>
  <trd lng="es">algo fant&aacute;stico</trd>
</drv>

<drv mrk="mojos.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld lit="M"/>a renkontejo, renkonto, retpa&gcirc;o; gejunulejo:
      <ekz>
         MOJOsejo, alternativa semajno por gejunuloj
         <klr>(gejunula renkonto en kastelo Gresiljono)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cool place</trd>
  <trd lng="es">sitio de reuni&oacute;n de moda</trd>
</drv>
</art>

