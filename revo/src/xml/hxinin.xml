<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id: hxinin.xml,v 1.10 2013/02/28 16:10:11 revo Exp $">
    <kap>
      <rad>&hcirc;inin</rad>/o
      <fnt><bib>BoER</bib>, <lok>p. 219</lok></fnt>
    </kap>

    <drv mrk="hxinin.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MED</uzo>
        <dif>
          Senkolora kristala substanco
          <frm>C<sub>20</sub>H<sub>24</sub>O<sub>2</sub>N<sub>2</sub></frm>,
          ekstraktita el la &scirc;elo de
          <ref cel="kinkon.0o">kinkono</ref>,
          kaj uzata kontra&ubreve; <ref cel="malari.0o">malario</ref>.  
        </dif>
        <ref tip="vid" cel="kinkon1.0o">kinkonino</ref>
        <rim>
          Por &ccirc;i tiu signifo la Akademio oficialigis (en 4OA)
          la vorton
          <ref tip="sin"
          cel="kinin.0o.kontrauxmalaria">kinino<sncref/></ref>,
          sen atenti la ekziston de la homonimo
          <ref tip="sin" cel="kinin.0o.hormono">kinino<sncref/></ref>.
        </rim>
      </snc>
      <trd lng="be">&c_h;&c_ib;&c_n;&c_ib;&c_n;</trd>
      <trd lng="bg">&c_h;&c_i;&c_n;&c_i;&c_n;</trd>
      <trd lng="ca">quinina</trd>
      <trd lng="cs">chinin</trd>
      <trd lng="de">Chinin</trd>
      <trd lng="en">quinine</trd>
      <trd lng="es">quinina</trd>
      <trd lng="fa">&f_k;&f_y;&a_n;&f_y;&a_n;</trd>
      <trd lng="hu">kinin</trd>
      <trd lng="la">chininum</trd>
      <trd lng="nl">kinine</trd>
      <trd lng="pl">chinina</trd>
      <trd lng="pt">quinina</trd>
      <trd lng="ru">&c_h;&c_i;&c_n;&c_i;&c_n;</trd>
      <trd lng="it">chinino</trd>
    </drv>
  </art>
  
  
