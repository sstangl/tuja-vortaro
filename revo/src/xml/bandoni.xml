<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bandoni.xml,v 1.10 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>bandoni</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="bandoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Balga, klavara <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      pli eta ol <ref tip="vid" cel="akordi.0o">akordiono</ref>,
      kun ses-angulaj ekstremoj, melankoli-sona
      populara en sudamerika muziko.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bandoneon</trd>,
    <trd>bandonika</trd>,
    <trd>bandonion</trd>,
    <trd>bandonium</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bandoneon</trd>,
    <trd>Bandonion</trd>
  </trdgrp>
  <trd lng="en">bandoneon</trd>
  <trd lng="es">bandone&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>bandon&eacute;on</trd>
  </trdgrp>
  <trd lng="hu">bandoneon</trd>
  <trdgrp lng="it">
    <trd>bandoneon</trd>,
    <trd>bandonion</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_d;&c_o;&c_n;&c_je;&c_o;&c_n;</trd>
  <trd lng="sk">bandonion</trd>
</drv>
</art>

