<drv mrk="gejsx.0o">
  <kap><tld/>o</kap>
  <snc mrk="gejsx.0o.KOMUNE">
    <dif>
      Japana elegantulino, klera pri tradiciaj muziko, danco, kanto,
      la&ubreve;mende akompananta en bankedoj a&ubreve; privataj festoj:
      <ekz>
        ili kunlo&gcirc;as kun kelkaj aliaj <tld/>oj kiel 
&leftquot;fratinoj&rightquot; sub la
        dommastrino kiel &leftquot;patrino&rightquot;
        <fnt><bib>EncJap</bib>, <lok>artikolo
&leftquot;gej&scirc;o&rightquot;, 
p. 47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">geisha</trd>
  <trd lng="de">Geisha</trd>
  <trd lng="fr">geisha</trd>
  <trd lng="hu">g&eacute;sa</trd>
  <trd lng="nl">geisha</trd>
  <trd lng="pl">gejsza</trd>
  <trd lng="pt">gueixa</trd>
  <trd lng="ru">&c_g;&c_je;&c_j;&c_sh;&c_a;</trd>
</drv>
</art>

