<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: latv.xml,v 1.13 2015/07/04 08:37:38 revo Exp $">
<kap>
    <ofc>8</ofc>
    <rad>latv</rad>/o
</kap>

<drv mrk="latv.0o">
  <kap><tld/>o</kap>
  <snc mrk="latv.0o.ETNO">
    <dif>
      Ano de la <tld/>a etno.
    </dif>
  </snc>
  <snc mrk="latv.0o.ANO">
    <ref tip="dif" cel="latv.0ujano"><tld lit="L"/>ujano</ref>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_je;&c_c;</trd>
  <trdgrp lng="cs">
    <trd>Lotyš</trd>,
    <trd>Lotyšec</trd>
  </trdgrp>
  <trd lng="de">Lette</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="fr">Letton<klr> (subst.)</klr></trd>
  <trd lng="nl">Let</trd>
  <trd lng="pl">&Lstroke;otysz</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
  <trdgrp lng="sk">
    <trd>Lotyš</trd>,
    <trd>Lotyš,</trd>
  </trdgrp>
</drv>

<drv mrk="latv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, ilia etno a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">lotyšský</trd>
  <trd lng="de">lettisch</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="fr">letton<klr> (adj.)</klr></trd>
  <trd lng="nl">Lets</trd>
  <trd lng="pl">&lstroke;otewski</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="sk">lotyšský</trd>
</drv>

<drv mrk="latv.L0ujo">
  <kap><tld lit="L"/>ujo</kap>
  <snc mrk="latv.L0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato situanta en nordoriento de E&ubreve;ropo kaj limanta
      al Balta Maro, Litovujo, Belorusujo, Rusujo kaj Estonujo.
    </dif>
  <ref tip="sin" cel="latvi.L0o">Latvio</ref>
  </snc>
  <trd lng="bg">&c_L;&c_a;&c_t;&c_v;&c_i;&c_ja;</trd>
  <trd lng="de">Lettland</trd>
  <trd lng="en">Latvia</trd>
  <trdgrp lng="fr">
    <trd>Lettonie</trd>,
    <trd>R&eacute;publique de Lettonie</trd>
  </trdgrp>
  <trd lng="id">Latvia</trd>
  <trd lng="nl">Letland</trd>
  <trd lng="pl">&Lstroke;otwa</trd>
  <trdgrp lng="pt">
    <trd>Let&ocirc;nia</trd>,
    <trd>Let&oacute;nia</trd>
  </trdgrp>
</drv>

<drv mrk="latv.0ujano">
  <kap><tld/>ujano</kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld lit="L"/>ujo sendepende de etno.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_je;&c_c;</trd>
  <trd lng="de">Lette</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="nl"><ind>Lets</ind> staatsburger</trd>
  <trd lng="pl">&Lstroke;otysz</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
</drv>

<drv mrk="latv.0uja">
  <kap><tld/>uja</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld lit="L"/>ujo.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">lettisch</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="nl">Lets<klr tip="ind"> (betr.de staat)</klr></trd>
  <trd lng="pl">&lstroke;otewski</trd>
</drv>

</art>

