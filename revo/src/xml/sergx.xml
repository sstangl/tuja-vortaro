<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sergx.xml,v 1.12 2013/09/04 18:10:13 revo Exp $">
<kap>
  <rad>ser&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sergx.0o">
  <kap><tld/>o</kap>
  <snc mrk="sergx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tute a&ubreve; parte lana <ref tip="super"
      cel="sxtof.0o.TEKS">&scirc;tofo</ref>
      densa, malmola, kun oblikva neegala reliefo, uzata ordinare por
      survestoj:
      <ekz>
        <klr>...</klr> se oni donacus al mi sutanon el Florenca
        <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 6a, p.
        58a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gabardin.0o.TEKS_sxtofo">gabardino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_zh;&c_a;</trd>
  <trd lng="de">Serge</trd>
  <trd lng="en">serge</trd>
  <trd lng="es">sarga</trd>
  <trd lng="fa">&a_s;&a_r;&f_zh;&a_h;</trd>
  <trd lng="fr">serge<klr> (tissu)</klr></trd>
  <trd lng="hu">szerzs</trd>
  <trdgrp lng="it"><trd>saia</trd>, <trd>sargia</trd></trdgrp>
  <trd lng="nl">serge</trd>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_zh;&c_a;</trd>
</drv>
</art>
