<art mrk="$Id: raned.xml,v 1.8 2015/07/04 08:37:42 revo Exp $">
<kap>
   <rad>raned</rad>/oj
</kap>
<drv mrk="raned.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO"
       lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
       <ref tip="malprt" cel="vost.sen0uloj">senvostuloj</ref>
       (<trd lng="la">Ranidae</trd>), kun senveruka ha&ubreve;to.
     </dif>
   </snc>
  <trd lng="cs">žáby <klr>(čeleď)</klr></trd>
  <trd lng="de">Fr&ouml;sche</trd>
  <trd lng="en">frog</trd>
  <trd lng="fr">ranid&eacute;s</trd>
  <trd lng="hu">val&oacute;di <ind>b&eacute;kaf&eacute;l&eacute;k</ind></trd>
  <trd lng="nl">kikvorsachtigen</trd>
  <trd lng="pt">ran&iacute;deos</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; 
<ind>&c_l;&c_ja;&c_g;&c_u;&c_sh;&c_k;&c_i;</ind> <klr 
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">žaby</trd>
</drv>
</art>

