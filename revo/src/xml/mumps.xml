<vortaro>
<art mrk="$Id: mumps.xml,v 1.7 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mumps</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mumps.0o">
  <kap><tld/>o</kap>
  <snc mrk="mumps.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Konta&gcirc;a virusa infan<ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>, kiu inflamigas kaj
      &scirc;veligas salivoglandojn, ka&ubreve;zas febron kaj
      dolorojn:
      <ekz>
        en tiu sama jaro trafis min <klr>...</klr>
        <tld/>o
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>galteres</trd>,
    <trd>parotiditis</trd>
  </trdgrp>
  <trd lng="cs">příušnice</trd>
  <trdgrp lng="de">
    <trd>Mumps</trd>,
    <trd>Ziegenpeter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oreillons</trd>,
    <trd>parotidite virale</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>beguk</trd>,
    <trd>gondong</trd>,
    <trd>parotitis</trd>
  </trdgrp>
  <trd lng="sk">mumps</trd>
</drv>
</art>

