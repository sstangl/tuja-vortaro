<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: liknid.xml,v 1.15 2013/08/27 07:10:25 revo Exp $">
<kap>
  <rad>liknid</rad>/o
</kap>
<drv mrk="liknid.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kariof1.0oj">kariofilacoj</ref>
      (<trd lng="la">Lychnis</trd>).
    </dif>
    <trd lng="hu">kakukkszegf&udblac;szeg</trd>
  </snc>
  <bld lok="../bld/liknid.jpg">
    <ind>kukola <tld/>o</ind>
    <trdgrp lng="hu">
      <trd>r&eacute;ti kakukkszegf&udblac;szeg</trd>,
      <trd>sz&uacute;nyogszegf&udblac;szeg</trd>,
      <trd>sz&uacute;nyogvir&aacute;g</trd>
    </trdgrp>
    <trd lng="la">Lychnis flos-cuculi</trd>
    <trd lng="pt">flor-de-cuco</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_k;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pechnelke</trd>,
    <trd>Lichtnelke</trd>
  </trdgrp>
  <trd lng="en">ragged robin</trd>
  <trd lng="nl">koekoeksbloem</trd>
  <trd lng="pt">l&iacute;quinis</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_h;&c_n;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_o;&c_r;&c_mol;&c_k;&c_a; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
