<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kupl.xml,v 1.13 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kupl</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kupl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kupl.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Funkciigi kune du a&ubreve; plurajn similajn partojn de ma&scirc;ino, ma&scirc;inojn
      a&ubreve;
      elektrajn elementojn por la sama kaj komuna laboro.
    </dif>
    <trd lng="ca">acoblar</trd>
    <trdgrp lng="de">
      <trd>kuppeln</trd>,
      <trd>koppeln</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>couple</trd>,
      <trd>unite</trd>,
      <trd>join up</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kupl.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Trapasigi la radioelektran energion de unu cirkvito en alian per
      alproksimigo de tiuj cirkvitoj a&ubreve; de ilia interkonekto.
    </dif>
    <refgrp tip="vid">
      <ref cel="komut.0i">komuti</ref>,
      <ref cel="konekt.0i">konekti</ref>,
      <ref cel="kun">kun(igi)</ref>,
      <ref cel="lig">(kun)ligi</ref>
    </refgrp>
    <trd lng="ca">connectar</trd>
    <trd lng="de">koppeln</trd>
    <trd lng="en">couple</trd>
    <trd lng="ru">&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>pářit <klr>(se)</klr></trd>,
    <trd>přivěsit</trd>,
    <trd>spojovat</trd>,
    <trd>spřáhnout</trd>,
    <trd>řadit rychlost</trd>
  </trdgrp>
  <trd lng="fr">coupler</trd>
  <trdgrp lng="hu">
    <trd>csatol</trd>,
    <trd>&ouml;sszekapcsol<klr tip="ind"> (energia&aacute;tvitelre)</klr></trd>
  </trdgrp>
  <trd lng="nl">koppelen</trd>
  <trdgrp lng="sk">
    <trd>pripojiť</trd>,
    <trd>privesiť</trd>,
    <trd>spojiť</trd>
  </trdgrp>
</drv>

<drv mrk="kupl.0o">
  <kap><tld/>o</kap>
  <snc mrk="kupl.0o.TEK">
    <dif>
      Ago <tld/>i kaj la rezulto de tiu ago:
      <ekz>
        artika, elasta, fiksita, movebla <tld/>o;
      </ekz>
      <ekz>
        bendo<tld/>o, bros<tld/>o, ktp.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">acoblament</trd>
  <trdgrp lng="cs">
    <trd>páření</trd>,
    <trd>spínání</trd>,
    <trd>spřažení</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kuppeln</trd>,
    <trd>Koppelung</trd>
  </trdgrp>
  <trd lng="fr">couplage</trd>
  <trdgrp lng="hu">
    <trd>csatol&aacute;s</trd>,
    <trd>&ouml;sszekapcsol&aacute;s<klr tip="ind"> (energia&aacute;tvitelre)</klr></trd>
  </trdgrp>
  <trd lng="nl">koppeling</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>párenie</trd>,
    <trd>spriahanie</trd>,
    <trd>spínanie</trd>
  </trdgrp>
</drv>

<drv mrk="kupl.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="kupl.0ilo.TEK">
    <dif>
      Aparato por <tld/>i.
    </dif>
  </snc>
  <trd lng="ca">acoblador</trd>
  <trdgrp lng="cs">
    <trd>spojka</trd>,
    <trd>spínací zařízení</trd>,
    <trd>spřáhlo</trd>
  </trdgrp>
  <trd lng="de">Kupplung</trd>
  <trdgrp lng="en">
    <trd>coupler</trd>,
    <trd>coupling</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatol&oacute;</trd>,
    <trd>tengelykapcsol&oacute;</trd>,
    <trd>kuplung</trd>
  </trdgrp>
  <trd lng="nl">koppelstang</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je; <klr>(&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja; &c_m;&c_u;&c_f;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spojka</trd>,
    <trd>spriahadlo</trd>
  </trdgrp>
</drv>
</art>
