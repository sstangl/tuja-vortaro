<vortaro>
<art mrk="$Id: kamfor.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kamfor</rad>/o
</kap>
<drv mrk="kamfor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blanka aroma substanco, vapori&gcirc;ema, ekstraktata el la
      <tld/>arbo, kaj uzata en medicino kaj kontra&ubreve; tineoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;</trd>
  <trd lng="br">ka&ntilde;fr</trd>
  <trd lng="ca">c&agrave;mfora</trd>
  <trd lng="cs">kafr</trd>
  <trd lng="de">Kampfer</trd>
  <trd lng="en">camphor</trd>
  <trd lng="fa">&f_k;&a_A;&a_f;&a_w;&a_r;</trd>
  <trd lng="fr">camphre</trd>
  <trd lng="hu">k&aacute;mfor</trd>
  <trd lng="nl">kamfer</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_m;&c_f;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">g√°for</trd>
</drv>

<drv mrk="kamfor.0ujo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="cinam.0ujo">cinamujo</ref>
      (<trd lng="la">Cinnamomum camphora</trd>),
      kreskanta en orienta Azio kaj ri&ccirc;a je <tld/>oleo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_o;&c_v;&c_o; &c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
  <trd lng="br">gwezenn-ga&ntilde;fr</trd>
  <trd lng="ca">camforer</trd>
  <trd lng="de">Kampferbaum</trd>
  <trd lng="en">camphor tree</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&f_k;&a_A;&a_f;&a_w;&a_r;</ind></trd>
  <trd lng="fr">camphrier</trd>
  <trd lng="hu">k&aacute;mforfa</trd>
  <trd lng="nl">kamferboom</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
