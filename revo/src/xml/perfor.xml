<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: perfor.xml,v 1.5 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>perfor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="perfor.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="perfor.0i.KOMUNE">
    <ref tip="dif" cel="tru.tra0i.KOMUNE">tratrui</ref>
    <ekz>
      - fiksi inter&scirc;tofon kaj stabiligajn materialojn
      - <tld/>i &scirc;upartojn
      - elmatrici ornamojn 
      <fnt>
        <aut>M. Thorenz</aut>,
        <vrk><url
        ref="http://ourworld.compuserve.com/homepages/profcon/e_shufa.htm">
        &Scirc;ufarado</url></vrk>,
        <lok>Eventoj, 1998-03, numero 144a</lok>
      </fnt>.
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      klaveceno bruon <tld/>is /
      &gcirc;i estas forta instrumento 
      <fnt>
        <aut>J. S. Skorupski</aut>,
        <vrk>Soneto &ccirc;ifoje johana</vrk>,
        <lok><url ref="http://skorupski.bitflux.ch/sonets/lang=eo">
        Sonetado</url>, 2006</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="cs">perforovat</trd>
  <trd lng="fr">perforer</trd>
  <trd lng="nl">perforeren<klr tip="ind"> (ontr.)</klr></trd>
  <trdgrp lng="sk">
    <trd>perforovať</trd>,
    <trd>predierkovať</trd>
  </trdgrp>
</drv>
</art>

