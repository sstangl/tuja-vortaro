<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flik.xml,v 1.22 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flik</rad>/i
</kap>
<drv mrk="flik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flik.0i.ripari">
    <dif>
      <ref tip="super" cel="ripar.0i">Ripari</ref> almetante pecon por
      kovri truon, ekzemple en vesto:
      <ekz>
	<tld/>i &scirc;iritan robon, difektitan &scirc;uon;
      </ekz>
      <ekz>
	<tld/>i truon en tegmento<fnt>Z</fnt>;
      </ekz>
      <ekz>
	multpece <tld/>ita pantalono;
      </ekz>
      <ekz>
	surkudri pecon por <tld/>i mantelon.
      </ekz>
    </dif>
    <trd lng="de">flicken</trd>
    <trd lng="hu">foltoz</trd>	  
  </snc>
  <snc mrk="flik.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte, nur parte ripari ion:
      <ekz>
	<tld/>i rompitan amikecon;
      </ekz>
      <ekz>
	<tld/>i la karton de E&ubreve;ropo, la pacon.
      </ekz>
    </dif>
    <trd lng="de">zusammenflicken</trd>
    <trd lng="hu">&ouml;sszefoltoz</trd>	
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>aparracar</trd>,
    <trd>apeda&ccedil;ar</trd>,
    <trd>reparar <klr>(apeda&ccedil;ar)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>flikovat</trd>,
    <trd>látat</trd>,
    <trd>spravit</trd>,
    <trd>zašít</trd>,
    <trd>zašívat</trd>,
    <trd>záplatovat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>patch</trd>,
    <trd>darn</trd>,
    <trd>mend</trd>
  </trdgrp>
  <trd lng="es">parchar</trd>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>racommoder</trd>,
    <trd>rapi&eacute;cer</trd>
  </trdgrp>
  <trd lng="nl">oplappen</trd>
  <trd lng="pl">&lstroke;ata&cacute;</trd>
  <trdgrp lng="pt">
    <trd>remendar</trd>,
    <trd>consertar</trd>,
    <trd>reparar</trd>,
    <trd>atamancar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">plátať</trd>
</drv>

<drv mrk="flik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Riparo farita per surkudrita a&ubreve; surmetita peco:
      <ekz>
	neniu alkudras <tld/>a&jcirc;on el nefulita drapo sur malnovan veston
	<fnt><bib>NT</bib>, <lok>&Mar;, 2:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="pt">peda&ccedil;</trd>
  <trd lng="de">Flicken</trd>
  <trd lng="en">patch</trd>
  <trd lng="es">parche</trd>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce<klr tip="ind"> (pour r&eacute;parer un trou)</klr></trd>,
    <trd>rustine</trd>
  </trdgrp>
  <trd lng="hu">folt</trd>  
  <trd lng="nl">oplapping</trd>
  <trd lng="pl">&lstroke;ata</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
</drv>

<drv mrk="flik.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu metie <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>správkař</trd>,
    <trd>záplatář</trd>
  </trdgrp>
  <trd lng="fr">rapi&eacute;ceur</trd>
  <trd lng="hu">foltoz&oacute;</trd>
  <trd lng="nl">oplapper</trd>
  <trd lng="sk">záplatár</trd>
</drv>

<drv mrk="flik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senzorge, mallerte <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
  <trd>zusammenmurksen</trd>,
  <trd>pfuschen</trd>
  </trdgrp>
  <trd lng="fr">rapetasser</trd>
  <trd lng="hu">&ouml;sszetoldoz</trd>  
</drv>
</art>
