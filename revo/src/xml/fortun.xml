<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fortun.xml,v 1.18 2015/11/22 21:10:34 revo Exp $">
<kap>
  <rad>fortun</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fortun.0o">
  <kap><tld/>o</kap>
  <snc mrk="fortun.0o.KOMUNE">
    <dif>
      Bona sorto. 
    </dif>
    <refgrp tip="vid">
      <ref cel="destin.0o">destino</ref>,
      <ref cel="hazard.0o">hazardo</ref>,
      <ref cel="fat.0o">fato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_malmol;&c_s;&c_m;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>fortuna</trd>,
    <trd>&uacute;sp&ecaron;ch</trd>,
    <trd>&scaron;t&ecaron;st&iacute;</trd>,
    <trd>&scaron;t&ecaron;st&ecaron;na</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gl&uuml;ck</trd>,
    <trd>Gunst des Schicksals</trd>
  </trdgrp>
  <trd lng="en">fortune</trd>
  <trd lng="es">fortuna</trd>
  <trdgrp lng="fr">
    <trd>fortune<klr tip="ind"> (chance)</klr></trd>,
    <trd>chance</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szerencse</trd>,
    <trd>j&oacute; <ind>sors</ind></trd>
  </trdgrp>
  <trd lng="nl">fortuin</trd>
  <trdgrp lng="pl">
    <trd>fortuna</trd>,
    <trd>szcz&eogonek;&sacute;cie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fortuna</trd>,
    <trd>sorte</trd>,
    <trd>acaso</trd>,
    <trd>felicidade</trd>,
    <trd>destino</trd>,
    <trd>fado</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_z;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fort&uacute;na</trd>,
    <trd>&uacute;spech</trd>,
    <trd>&scaron;&tcaron;astena</trd>
  </trdgrp>
</drv>

<drv mrk="fortun.f0o">
  <kap><tld lit="F"/>o</kap>
  <snc mrk="fortun.f0o.PERS">
    <dif>
      Diino a&ubreve; personigo de la &scirc;anco. 
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_a;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>fortuna</trd>,
    <trd>&uacute;sp&ecaron;ch</trd>,
    <trd>&scaron;t&ecaron;st&iacute;</trd>,
    <trd>&scaron;t&ecaron;st&ecaron;na</trd>
  </trdgrp>
  <trd lng="bg">&c_F;&c_o;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>
  <trd lng="de">Fortuna</trd>
  <trd lng="en">Fortuna</trd>
  <trd lng="fr">Fortune</trd>
  <trd lng="hu">Fortuna</trd>
  <trd lng="nl">Fortuna</trd>
  <trd lng="pl">Fortuna</trd>
  <trd lng="pt">Fortuna</trd>
  <trd lng="ru">&c_F;&c_o;&c_r;&c_t;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>fort&uacute;na</trd>,
    <trd>&uacute;spech</trd>,
    <trd>&scaron;&tcaron;astena</trd>
  </trdgrp>
</drv>

<drv mrk="fortun.0i">
  <kap><tld/>i</kap>
  <snc mrk="fortun.0i.al_iu">
    <dif>
      <ref tip="vid" cel="prospe1.0i.ntr">Prosperi</ref>,
      brile <ref tip="vid" cel="sukces.0i">sukcesi</ref>
      pro bon&scirc;anco:
      <ekz>
        kial al li tiel <tld/>is, &mdash; pensis mi, &mdash; kial?
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&iacute;t &scaron;t&ecaron;st&iacute;</trd>
  <trd lng="fr">r&eacute;ussir<klr> (&agrave; qqun)</klr></trd>
  <trd lng="sk">ma&tcaron; &scaron;&tcaron;astie</trd>
</drv>
</art>
