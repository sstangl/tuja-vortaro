<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: buldog.xml,v 1.20 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>buldog</rad>/o
</kap>

<drv mrk="buldog.0o">
  <kap><tld/>o</kap>
  <snc mrk="buldog.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="hund.0o.dombesto">hundo</ref> el raso
      havanta
      mallongan, lar&gcirc;an kapon kaj elstarajn makzelojn:
      <ekz>
        la grandaj <tld/>oj, kiuj aspektis kvaza&ubreve; &ccirc;iu el
        ili povus engluti
        homon, eksaltis alten, tamen sen bojado
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino, fabelo en
        sep rakontoj,
        kvina rakonto, malgranda rabistinfano</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_g;</trd>
    <trd lng="bg">&c_b;&c_u;&c_l;&c_d;&c_o;&c_g;</trd>
    <trd lng="de">Bulldogge</trd>
    <trd lng="en">French <ind>bulldog</ind></trd>
    <trd lng="es">bulldog</trd>
    <trd lng="fr">bouledogue</trd>
    <trd lng="hu">bulldog</trd>
    <trd lng="pl">buldog</trd>
    <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_g;</trd>
    <trd lng="sk">buldog</trd>
  </snc>
  <trd lng="cs">buldok</trd>
</drv>
</art>
