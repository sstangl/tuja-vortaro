<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bezon.xml,v 1.31 2015/10/05 04:10:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bezon</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bezon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bezon.0i.suferi">
    <dif>
      Malhavi ion necesan:
      <ekz>
        <tld/>i monon, ripozon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dezir.0i">deziri</ref>.
    </refgrp>
  </snc>
  <snc mrk="bezon.0i.postuli">
    <dif>
      Esti en stato, kiu postulas ion kiel necesan, la&ubreve;celan 
      a&ubreve; utilan:
      <ekz>
        <tld/>i ripozi, iri, trinki;
      </ekz>
      <ekz>
        ni anta&ubreve; &ccirc;io <tld/>as plej severan
        unuecon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la klasikaj verkistoj ofte <tld/>as
        komentariojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pri tio ni ne <tld/>as paroli<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ne <tld/>as aldoni sufikson<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ne <tld/>as timi, ke oni ilin reciproke
        konfuzos<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ne <tld/>as decidi, ke oni uzu
        ekskluzive nur la nominativon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio <tld/>igas multe da tempo;
      </ekz>
      <ekz>
      la vorto "<tld/>esti" estas kunmetita tute
      regule<fnt><bib>LR</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.mal0i">malhavi</ref>,
      <ref cel="neces.0igi">necesigi</ref>,
      <ref cel="postul.0i">postuli</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;
    </trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_u;
    </trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;&c_ja; &c_s;&c_je;</trd>,
    <trd>&c_i;&c_m;&c_a;&c_m; &c_n;&c_u;&c_zh;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="br">kaout ezhomm</trd>
  <trd lng="ca">necessitar</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;t zapot&rcaron;eb&iacute;</trd>,
    <trd>pot&rcaron;ebovat</trd>,
    <trd>pot&rcaron;ebovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ofc>*</ofc>brauchen</trd>,
    <trd>ben&ouml;tigen</trd>
  </trdgrp>
  <trd lng="en"><ofc>*</ofc>need</trd>
  <trd lng="es">necesitar</trd>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>avoir <ind>besoin</ind> (de)</trd>,
    <trd>n&eacute;cessiter</trd></trdgrp>
  <trd lng="hu">sz&uuml;ks&eacute;ge van<klr tip="amb"> (vmire)
  </klr></trd>
  <trd lng="oc">aver <ind>besonh</ind></trd>
  <trd lng="pl">potrzebowa&cacute;</trd>
  <trd lng="pt">necessitar</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; 
    &c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">potrebova&tcaron;</trd>
  <trd lng="sv">beh&ouml;va</trd>
  <trd lng="tp">wile</trd>
</drv>

<drv mrk="bezon.0o">
  <kap><tld/>o</kap>
  <snc mrk="bezon.0o.sufero">
    <dif>
      Sufero pro manko de io necesa:
      <ekz>
        <tld/>o le&gcirc;on ne obeas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o estas plej forta ordono<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&uuml;ks&eacute;g</trd>
  </snc>
  <snc mrk="bezon.0o.postulo">
    <dif>
      Stato, kiu postulas ion, kiel necesan, la&ubreve;celan 
      a&ubreve; utilan:
      <ekz>
        klareco estas <tld/>o de normala spirito;
      </ekz>
      <ekz>
        en okazo de <tld/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        en okazo de sen<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">sz&uuml;ks&eacute;g</trd>
  </snc>
  <snc mrk="bezon.0o.manko">
    <ref tip="dif" cel="bezon.0ajxo.KOMUNE"><tld/>a&jcirc;o</ref>
    <ekz>
      havi grandajn, malmultajn, moderajn <tld/>ojn.
    </ekz>
    <trd lng="hu">sz&uuml;ks&eacute;glet</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_a;</trd>
  <trdgrp lng="br">
    <trd>ezhomm</trd>,
    <trd>c'hoant</trd>
  </trdgrp>
  <trd lng="ca">necessitat</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;le&zcaron;itost <klr>(co je nutn&eacute;)</klr></trd>,
    <trd>pot&rcaron;eba</trd>,
    <trd>pot&rcaron;ebn&aacute; v&ecaron;c</trd>,
    <trd>po&zcaron;adavek</trd>
  </trdgrp>
  <trd lng="de">Bed&uuml;rfnis</trd>
  <trdgrp lng="en">
    <trd>need</trd>,
    <trd>want</trd></trdgrp>
  <trd lng="es">necesidad</trd>
  <trdgrp lng="fr">
    <trd>besoin</trd>,
    <trd>n&eacute;cessit&eacute;</trd></trdgrp>
  <trd lng="oc">besonh</trd>
  <trdgrp lng="pl">
     <trd>potrzeba</trd>,
     <trd>zapotrzebowanie</trd>
   </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_r;&c_je;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="sk">potrebn&aacute; vec</trd>
  <trd lng="sv">behov</trd>
  <trd lng="tp">wile</trd>
</drv>

<drv mrk="bezon.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc mrk="bezon.0a.bezonata">
    <dif>
      <tld lit="B"/>ata, mankanta sed necesa:
      <ekz>
        por vino bona &scirc;ildo <klr>(reklamo)</klr>
        ne <tld/>a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        5 jaroj estis <tld/>aj por venki ilin.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avid.0i">avidi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>rekis</trd>,
    <trd>ezhommek<klr tip="ind"> (trao&ugrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nutn&yacute;</trd>,
    <trd>pot&rcaron;ebn&yacute;</trd>
  </trdgrp>
  <trd lng="de">n&ouml;tig</trd>
  <trdgrp lng="en">
    <trd>needed</trd></trdgrp>
  <trd lng="es">necesario</trd>
  <trd lng="fr">n&eacute;cessaire</trd>
  <trd lng="hu">sz&uuml;ks&eacute;ges</trd>
  <trdgrp lng="pl">
    <trd>potrzebny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_y;&c_j;
    </trd>,
    <trd>
    &c_t;&c_r;&c_je;&c_b;&c_u;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nutn&yacute;</trd>,
    <trd>potrebn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bezon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bezon.0ajxo.KOMUNE">
    <dif>
      Tio, kies mankon oni forte, sufere sentas;
      <ref tip="sin" cel="bezon.0o.manko"><tld/>o<sncref/></ref>:
      <ekz>
        vintraj <tld/>a&jcirc;oj
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_n;&c_a;&c_je;
      <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_h;&c_o;&c_d;&c_n;&c_a;&c_je;
      <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="br">ezhomm</trd>
  <trd lng="en">need</trd>
  <trd lng="es">necesidad</trd>
  <trd lng="fr">besoin</trd>
  <trd lng="hu">sz&uuml;ks&eacute;glet</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_o;&c_je;
      <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_n;&c_u;&c_zh;&c_n;&c_o;&c_je; <klr>(&c_s;&c_u;&c_shch;.)
    </klr></trd>
  </trdgrp>
</drv>

<drv mrk="bezon.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="bezon.sen0a.KOMUNE">
    <dif>
      Ne<tld/>ata, nenecesa:
      <ekz>
        sen<tld/>a radiko;
      </ekz>
      <ekz>
        tio &ccirc;i estas sen<tld/>a timo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i sen<tld/>e ligus nin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malpermesi la uzadon de tiu &ccirc;i formo estas tute
        sen<tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_p;&c_a;&c_t;&c_r;&c_e;&c_b;&c_n;&c_y;
  </trd>
  <trd lng="br">diezhomm</trd>
  <trdgrp lng="cs">
    <trd>nepot&rcaron;ebn&yacute;</trd>,
    <trd>zbyte&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">unn&ouml;tig</trd>
  <trdgrp lng="en">
    <trd>needless</trd>,
    <trd>unnecessary</trd></trdgrp>
  <trd lng="es">innecesario</trd>
  <trdgrp lng="fr">
    <trd>inutile</trd>,
    <trd>superflu</trd>,
    <trd>ne <ind>servant</ind> &agrave; rien</trd></trdgrp>
  <trd lng="hu">sz&uuml;ks&eacute;gtelen</trd>
  <trdgrp lng="pl">
   <trd>niepotrzebny</trd>,
   <trd>zb&eogonek;dny</trd>
 </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_n;&c_u;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nepotrebn&yacute;</trd>,
    <trd>zbyto&ccaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bezon.viv0ajxo">
  <kap>viv<tld/>a&jcirc;o</kap>
  <snc mrk="bezon.viv0ajxo.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="neces.viv0ajxo.EKON">vivnecesa&jcirc;o</ref>
      <ekz>
        la Komerca Ministerio da&ubreve;re reguligas la merkaton
        de viv<tld/>a&jcirc;oj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://www.youtube.com/watch?v=Qt97P1xEbLE">
          Panorame</url></vrk>,
          <lok>2010-12-24</lok>
        </fnt>.
      </ekz>
    <refgrp tip="vid">
      <ref cel="viv.por0ajxo.KOMUNE">porviva&jcirc;o</ref>,
      <ref cel="pan.viv0o.FIG">vivpano</ref>
    </refgrp>
  </snc>
  <trd lng="es">art&iacute;culo de primera necesidad</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;cessaire<klr> (le ~, subst.)</klr></trd>,
    <trd>ressource vitale</trd>
  </trdgrp>
</drv>
</art>
