<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: agarik1.xml,v 1.9 2013/04/12 10:10:16 revo Exp $">
<kap>
  <rad>agarikac</rad>/oj
</kap>
<drv mrk="agarik1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="fung.klab0oj">klabfungoj</ref>
      (<trd lng="la">Agaricaceae</trd>),
      kies ombrelforma fruktokorpo prezentas de sube
      alcentrajn lamenojn.
    </dif>
    <ref tip="prt" cel="fung.lamen0o">lamenfungo</ref>
    <refgrp tip="sub">
      <ref cel="cxampinjon.0o">&ccirc;ampinjono</ref>,
      <ref cel="amanit.0o">amanito</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_r;&c_y;&c_k;&c_a;&c_v;&c_y;&c_ja;</trd>
  <trd lng="de">Lamellenpilze</trd>
  <trd lng="en">Agrucis family</trd>
  <trd lng="fr">agaricac&eacute;es</trd>
  <trd lng="hu">csiperkef&eacute;l&eacute;k
<klr tip="ind">(csal&aacute;dja)</klr></trd>
  <trd lng="it">agaricacee</trd>
  <trd lng="nl">agaricaceae</trd>
  <trd lng="pt">agaric&aacute;ceas</trd>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_r;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>
</drv>
</art>

