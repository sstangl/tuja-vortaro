<kap><rad>grum</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="grum.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juna lakeo, precipe en hoteloj kaj kafejoj.
    </dif>
  </snc>
  <trd lng="ca">grum</trd>
  <trdgrp lng="cs">
    <trd>pikolík</trd>,
    <trd>podkoní</trd>,
    <trd>čeledín</trd>
  </trdgrp>
  <trd lng="de">Page</trd>
  <trdgrp lng="en">
    <trd>groom</trd>,
    <trd>stable boy</trd>,
    <trd>stable lad</trd>,
    <trd>servant</trd>,
    <trd>page boy</trd>,
    <trd>bellhop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>groom</trd>,
    <trd>chasseur<klr tip="ind"> (groom)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;llodainas</trd>,
    <trd>liftes fi&uacute;</trd>,
    <trd>boy</trd>,
    <trd>kifut&oacute;fi&uacute;</trd>,
    <trd>pikol&oacute;<klr tip="ind"> (pinc&eacute;rfi&uacute;)</klr></trd>,
    <trd>pinc&eacute;rfi&uacute;</trd>,
    <trd>pinc&eacute;rtanul&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>picollo</trd>,
    <trd>hoteljongen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>trintan&aacute;rio</trd>,
    <trd>groom</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_m;</trd>
  <trd lng="sk">mladý hotelový sluha</trd>
</drv>
</art>
