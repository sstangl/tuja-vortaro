<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dig.xml,v 1.22 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dig</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dig.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Murego a&ubreve; teramaso starigita por &scirc;irmi lokon
      kontra&ubreve; superakvo el
      rivero a&ubreve; maro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ilia sango estas tro bolanta, rompanta
        ofte <tld/>on de prudento<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_e;&c_b;&c_l;&c_ja;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_d;&c_i;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_i;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dic</trd>,
    <trd>escullera</trd>
  </trdgrp>
  <trd lng="cs">hráz</trd>
  <trdgrp lng="de">
    <trd>Damm</trd>,
    <trd>Deich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dam</trd>,
    <trd>dike</trd>
  </trdgrp>
  <trd lng="es">dique</trd>
  <trd lng="fr">digue</trd>
  <trdgrp lng="hu">
    <trd>g&aacute;t</trd>,
    <trd>v&eacute;d&odblac;g&aacute;t</trd>,
    <trd>t&ouml;lt&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">dijk</trd>
  <trdgrp lng="pl">
    <trd>grobla</trd>,
    <trd>nasyp</trd>,
    <trd>tama</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dique</trd>,
    <trd>represa</trd>,
    <trd>barreira</trd>,
    <trd>obst&aacute;culo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hrádza</trd>
</drv>

<drv mrk="dig.en0igi">
  <kap><tld/>i<fnt>Z</fnt>, <var><kap>en<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dig.en0igi.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Bari per <tld/>o.
    </dif>
    <trd
    lng="be">&c_p;&c_je;&c_r;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    &c_p;&c_l;&c_a;&c_c;&c_ib;&c_n;&c_a;&c_j;</trd>
    <trd lng="ca">posar un dic</trd>
    <trdgrp lng="de">
      <trd>eind&auml;mmen</trd>,
      <trd>eindeichen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>dam</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>g&aacute;ttal elz&aacute;r</trd>,
      <trd>g&aacute;tak k&ouml;z&eacute; szor&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zatamowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_g;&c_r;&c_a;&c_d;&c_i;&c_t;&c_mol;
      &c_p;&c_l;&c_o;&c_t;&c_i;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dig.en0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malakceli, haltigi.
    </dif>
    <refgrp tip="vid">
      <ref cel="brid.0i">bridi</ref>,
      <ref cel="star.kontraux0i">kontra&ubreve;stari</ref>,
      <ref cel="help.mal0i">malhelpi</ref>,
      <ref cel="moder.0igi">moderigi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_mol;
      &c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">detenir</trd>
    <trdgrp lng="de">
      <trd>eind&auml;mmen</trd>,
      <trd>begrenzen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stop</trd>,
      <trd>halt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megf&eacute;kez</trd>,
      <trd>korl&aacute;tok k&ouml;z&eacute; szor&iacute;t</trd>,
      <trd>megzabol&aacute;z</trd>
    </trdgrp>
    <trd lng="pl">tamowa&cacute;</trd>
    <trd
    lng="ru">&c_p;&c_r;&c_je;&c_p;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>aturar</trd>,
    <trd>retenir</trd>
  </trdgrp>
  <trd lng="fr">endiguer</trd>
  <trd lng="nl">indijken</trd>
</drv>
</art>
