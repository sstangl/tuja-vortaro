<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prunt.xml,v 1.24 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prunt</rad>/i
</kap>
<drv mrk="prunt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disponigi al iu ion redonotan post difinita tempo; prunte doni:
      <ekz>
        mi redonas al vi la monon, kiun vi <tld/>is al mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i al iu vestojn<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &ccirc;ambron<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &ccirc;evalon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">k&ouml;lcs&ouml;nad</trd>
    <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_v;&c_z;&c_a;&c_j;&c_m;&c_y;</trd>
    <trdgrp lng="en">
      <trd>loan</trd>,
      <trd>lend</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>prestar</trd>,
      <trd>dejar</trd>
    </trdgrp>
    <trd lng="fr">pr&ecirc;ter</trd>
  <trdgrp lng="pt">
    <trd>emprestar</trd>,  
    <trd>dar por <ind>empr&eacute;stimo</ind></trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      Fari, ke iu disponigu al ni ion redonotan post difinita tempo;
      prunte preni:
      <ekz>
        redonu al mi la monon, kiun vi <tld/>is de mi;
      </ekz>
      <ekz>
        vila&gcirc;ano kreditas, vila&gcirc;estro <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">k&ouml;lcs&ouml;nvesz</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_v;&c_z;&c_a;&c_j;&c_m;&c_y;</trd>,
      <trd>&c_z;&c_a;&c_i;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">emprunter</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&uring;j&ccaron;it</trd>,
    <trd>půjčit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>leihen</trd>,
    <trd>borgen</trd>
  </trdgrp>
  <trd lng="en">borrow</trd>
  <trd lng="hu">k&ouml;lcs&ouml;n&ouml;z</trd>
  <trd lng="nl">lenen</trd>
  <trdgrp lng="pt">
    <trd>emprestar</trd>,  
    <trd>tomar por <ind>empr&eacute;stimo</ind></trd>
  </trdgrp>
  <trd lng="sk">požičať</trd>
  <trd lng="sv">l&aring;na</trd>
</drv>

<drv mrk="prunt.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kun kondi&ccirc;o redoni post difinita tempo:
      <ekz>
        mi ne <tld/>edonis procentege, mi ne prenis <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        perdi&gcirc;as per <tld/>edono amiko kaj mono
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; <ind>&c_p;&c_a;&c_z;&c_y;&c_k;&c_u;</ind></trd>
  <trd lng="de">leihweise</trd>
  <trd lng="en">on <ind>loan</ind></trd>
  <trd lng="fr">en <ind>pr&ecirc;t</ind></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;lcs&ouml;n</trd>,
    <trd>k&ouml;lcs&ouml;nbe</trd>
  </trdgrp>
  <trd lng="nl">te <ind>leen</ind></trd>
  <trd lng="ru">&c_v;&c_z;&c_a;&c_j;&c_m;&c_y;</trd>
</drv>

<drv mrk="prunt.0o">
  <kap><tld/>o</kap>
  <snc mrk="prunt.0o.pruntado">
    <ref tip="dif" cel="prunt.0ado"><tld/>ado</ref>
  </snc>
  <snc mrk="prunt.0o.pruntajxo">
    <ref tip="dif" cel="prunt.0ajxo"><tld/>a&jcirc;o</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>přenos řádu při odčítání</trd>,
    <trd>záporný přenos</trd>
  </trdgrp>
  <trd lng="en">loan</trd>
  <trd lng="es">pr&eacute;stamo</trd>
  <trd lng="sk">záporný prenos dát</trd>
</drv>

<drv mrk="prunt.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago doni <tld/>e:
      <ekz>
        <tld/>o amikon forpelas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">pr&ecirc;t</trd>
    <trd lng="hu">k&ouml;lcs&ouml;nad&aacute;s</trd>
  </snc>
  <snc>
    <dif>
      Ago preni <tld/>e.
    </dif>
    <trd lng="fr">emprunt</trd>
    <trd lng="hu">k&ouml;lcs&ouml;nv&eacute;tel</trd>
  </snc>
  <trd lng="cs">půjčování</trd>
  <trd lng="en">loan</trd>
  <trd lng="hu">k&ouml;lcs&ouml;nz&eacute;s</trd>
  <trd lng="ru">&c_z;&c_a;&c_i;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">požičiavanie</trd>
</drv>

<drv mrk="prunt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>e donita.
    </dif>
  </snc>
  <snc>
    <dif>
      Io <tld/>e prenita.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Leihgabe</trd>
  <trd lng="en">loan</trd>
  <trd lng="hu">k&ouml;lcs&ouml;n</trd>
  <trd lng="nl">leengoed</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_i;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_jo;&c_m;</trd>,
  </trdgrp>
</drv>

<drv mrk="prunt.al0i">
  <kap>al<tld/>i, <var><kap><tld/>edoni</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>e doni.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_m;&c_u;&c_s;&c_mol;&c_c;&c_ib; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verleihen</trd>,
    <trd>verborgen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>loan</trd>,
    <trd>lend</trd>
  </trdgrp>
  <trd lng="es">prestar</trd>
  <trd lng="fr">pr&ecirc;ter</trd>
  <trd lng="hu">k&ouml;lcs&ouml;nad</trd>
  <trd lng="nl">uitlenen</trd>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_mol; &c_v;&c_z;&c_a;&c_j;&c_m;&c_y;</trd>
</drv>

<drv mrk="prunt.de0i">
  <kap>de<tld/>i, <var><kap><tld/>epreni</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>e preni.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol; <klr>(&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib; &c_w; &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd>ausleihen</trd>,
    <trd>ausborgen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>borrow</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>coger prestado</trd>,
    <trd>tomar prestado</trd>
  </trdgrp>
  <trd lng="fr">emprunter</trd>
  <trd lng="hu">k&ouml;lcs&ouml;nvesz</trd>
  <trd lng="nl">te <ind>leen</ind> krijgen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_ja;&c_t;&c_mol; &c_v;&c_z;&c_a;&c_j;&c_m;&c_y;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_a;&c_i;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
