<drv mrk="vimen.0o">
  <kap><tld/>o</kap>
   <snc mrk="vimen.0o.BOT">
   <uzo tip="fak">BOT</uzo>
    <dif>
      Speco de saliketo, kies flekseblaj trunketoj servas por fari
      korbojn
      ks.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_r;&c_b;&c_a; &c_n;&c_ib;&c_c;&c_a;&c_ja;</trd>, 
    <trd>&c_l;&c_a;&c_z;&c_a;</trd> 
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_k;&c_i;&c_t;&c_a;</trd>
  <trd lng="ca">vimetera</trd>
  <trd lng="cs">vrba košíkářská</trd>
  <trd lng="de">Korbweide</trd>
  <trdgrp lng="en">
    <trd>osier</trd>,
    <trd>willow</trd>,
    <trd>wicker</trd>
  </trdgrp>
  <trd lng="fr">osier</trd>
  <trdgrp lng="hu">
    <trd>kos&aacute;rk&ouml;t&odblac; <ind>f&udblac;z</ind></trd>,
    <trd>nemes <ind>f&udblac;z</ind></trd>,
    <trd>k&ouml;t&odblac;f&udblac;z</trd>,
    <trd>kenderf&udblac;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>teenwilg</trd>,
    <trd>katwilg</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_v;&c_a;
&c_p;&c_r;&c_u;&c_t;&c_o;&c_v;&c_i;&c_d;&c_n;&c_a;&c_ja;</trd>
  <trd lng="sk">košikárska vŕba</trd>
</drv>
