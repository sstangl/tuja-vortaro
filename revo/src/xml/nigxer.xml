<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nigxer.xml,v 1.9 2014/06/04 16:10:45 revo Exp $">
<kap>
  <rad>ni&gcirc;er</rad>/o
</kap>

<drv mrk="nigxer.N0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="nigxer.N0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta Afriko kun &ccirc;efurbo Niameo.
      Teritorio de <em>Respubliko Ni&gcirc;ero</em>.
      ISO-3166-1 kodo: NE.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="benin.B0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref> kaj
      <ref tip="vid" cel="nigxeri.N0o.GEOG">Ni&gcirc;erio</ref>.
    </dif>
    <trd lng="bg">&c_N;&c_i;&c_g;&c_je;&c_r;</trd>
    <trd lng="de">Niger</trd>
    <trd lng="en">Niger</trd>
    <trdgrp lng="fr">
      <trd>Niger<klr> (pays)</klr></trd>,
      <trd>R&eacute;publique du Niger</trd>
    </trdgrp>    
    <trd lng="id">Niger</trd>
    <trd lng="it">Nigeria</trd>
    <trd lng="nl">Niger</trd>
    <trdgrp lng="pl">
      <trd>Niger</trd>,
      <trd>Republika <ind>Nigru</ind></trd>
    </trdgrp>
    <trd lng="pt">N&iacute;ger</trd>
  </snc>
</drv>

</art>

