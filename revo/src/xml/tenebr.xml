<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tenebr.xml,v 1.11 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tenebr</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tenebr.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Densa <ref tip="super" cel="lum.mal0o">mallumo</ref>:
      <ekz>
        estingi&gcirc;is la lampo de la gastejestro, kaj en la <tld/>o
        ili batis sin reciproke<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 16a, p. 114a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_r;&c_o;&c_k;</trd>
  <trd lng="ca">tenebrositat</trd>
  <trdgrp lng="cs">
    <trd>temnota</trd>,
    <trd>tma</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tenebrosity</trd>,
    <trd>tenebrousness</trd>
  </trdgrp>
  <trd lng="es">tenebrosidad</trd>
  <trd lng="fr">t&eacute;n&egrave;bre</trd>
  <trd lng="hu">koroms&ouml;t&eacute;t</trd>
  <trdgrp lng="pl">
    <trd>ciemno&sacute;&cacute;</trd>,
    <trd>ciemno&sacute;ci</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>treva</trd>,
    <trd>trevas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_mol;&c_m;&c_a;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>temnota</trd>,
    <trd>tma</trd>
  </trdgrp>
</drv>
</art>

