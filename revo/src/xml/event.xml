<art mrk="$Id: event.xml,v 1.18 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>event</rad>/o
</kap>
<drv mrk="event.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Difinita, sufi&ccirc;e grava
      <ref tip="super" cel="okaz.0ajxo">okaza&jcirc;o</ref>:
      <ekz>
        popolo, kiu pere de kreo kaj partopreno en kulturaj <tld/>oj
        klopodas sekurigi sian propran estadon kaj
        estontecon<fnt><aut>Guido Van Damme</aut>: <vrk>Bruliga libro
        (Senad &Ccirc;oli&ccirc;: Spite al &ccirc;io Bosnio)</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 6a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_z;&c_je;&c_ja;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jev</trd>,
    <trd>událost <klr>(význačná)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ereignis</trd>,
    <trd>Begebenheit</trd>
  </trdgrp>
  <trd lng="en">event</trd>
  <trd lng="es">evento</trd>
  <trd lng="fr">&eacute;v&eacute;nement</trd>
  <trd lng="hu">esem&eacute;ny</trd>
  <trd lng="pl">wydarzenie</trd>
  <trd lng="pt">evento</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_b;&c_y;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_i;&c_s;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">udalosť</trd>
  <trd lng="tp">kama</trd>
</drv>
</art>
