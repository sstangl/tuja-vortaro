<vortaro>
<art mrk="$Id: akceso.xml,v 1.20 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>akcesor</rad>/a
</kap>
<drv mrk="akceso.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ne&ccirc;efa, malprecipa, flanka:
      <ekz>
        <tld/>a demando, profito.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_b;&c_o;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_zh;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_v;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Neben-</trd>,
      <trd>zus&auml;tzlich</trd>,
      <trd>Zusatz-</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accessory</trd>,
      <trd>secondary</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>accesorio</trd>,
      <trd>secundario</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accessoire</trd>,
      <trd>secondaire</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bijkomstig</trd>,
      <trd>secundair</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dodatkowy</trd>,
      <trd>uboczny</trd>,
      <trd>poboczny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>acess&oacute;rio</trd>,
      <trd>secund&aacute;rio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_t;&c_o;&c_r;&c_o;&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_s;&c_p;&c_o;&c_m;&c_o;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">ved&lcaron;aj&scaron;&iacute;</trd>
    <trdgrp lng="sv">
      <trd>bi-</trd>,
      <trd>till&auml;ggs-</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>akcesorický</trd>,
    <trd>akcesorní</trd>,
    <trd>nepodstatný</trd>,
    <trd>průvodní</trd>,
    <trd>vedlejší <klr>(stojící vedle hlavního)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepodstatný</trd>,
    <trd>nepodstatný, vedľajší</trd>
  </trdgrp>
</drv>
</art>
