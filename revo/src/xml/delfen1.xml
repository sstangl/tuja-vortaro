<kap>
  <rad>delfened</rad>/oj
</kap>
<drv mrk="delfen1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref>
      el la ordo <ref tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Delphinidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Delphine <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">delphinid&eacute;s</trd>
  <trd lng="hu">delfinf&eacute;l&eacute;k</trd>
  <trd lng="pt">delfin&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

