<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kamion.xml,v 1.25 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>kamion</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kamion.0o">
  <kap><tld/>o</kap>
  <snc mrk="kamion.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Fortika, platplanka
      <ref tip="super" cel="auxt.0o">a&ubreve;to</ref>
      por pezaj &scirc;ar&gcirc;oj kun 4, 6 a&ubreve; pli da radoj;
      <ref tip="sin" cel="auxt.sxargx0o.AUT">&scirc;ar&gcirc;a&ubreve;to</ref>:
      <ekz>
        poste oni transportis lignon per fervojo kaj
        <tld/>oj
        <fnt>
          <aut>W. Klag</aut>,
          <vrk>Vakeroj de Alpoj</vrk>,
          <bib>Monato</bib>,
          <lok>2001:1, p. 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        videblis nek a&ubreve;to, nek <tld/>o
        <fnt>
          <aut>J. Modest</aut>,
          <vrk>La ikono</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_o;&c_n;</trd>
  <trd lng="ca">cami&oacute;</trd>
  <trdgrp lng="cs">
    <trd>kamion</trd>,
    <trd>nákladní automobil</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lastwagen</trd>,
    <trd>Lkw</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lorry</trd>,
    <trd>truck</trd>
  </trdgrp>
  <trd lng="es">cami&oacute;n</trd>
  <trd lng="fr">camion</trd>
  <trdgrp lng="hu">
    <trd>kamion</trd>,
    <trd>teheraut&oacute;</trd>
  </trdgrp>
  <trd lng="it">camion</trd>
  <trd lng="nl">vrachtwagen</trd>
  <trd lng="oc">camion</trd>
  <trdgrp lng="pl">
    <trd>ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d ci&eogonek;&zdot;arowy</trd>
    <trd>tir<klr> (zwyczajowo o ci&aogonek;gniku siod&lstroke;owym z
naczep&aogonek; wieloosiow&aogonek; 
    i napisem TIR)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">nákladné auto</trd>
</drv>

<drv mrk="kamion.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="kamion.0eto.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Malgranda <ref tip="super" cel="kamion.0o.AUT"><tld/>o</ref>,
      kun du- a&ubreve; triloka &scirc;oforejo kaj plejofte fermita &scirc;ar&gcirc;ejo:
      <ekz>
        feli&ccirc;e alveturis <tld/>eto kaj per &gcirc;i ni atingis la
        vila&gcirc;on
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">camioneta</trd>
  <trdgrp lng="cs">
    <trd>dodávka</trd>,
    <trd>dodávkové auto</trd>
  </trdgrp>
  <trd lng="de">Lieferwagen</trd>
  <trd lng="en">van</trd>
  <trdgrp lng="es">
    <trd>camioneta</trd>,
    <trd>furgoneta</trd>
  </trdgrp>
  <trd lng="fr">camionnette</trd>
  <trd lng="hu">kisteheraut&oacute;</trd>
  <trdgrp lng="it">
    <trd>camionetta</trd>,
    <trd>furgone</trd>
  </trdgrp>
  <trd lng="nl">bestelwagen</trd>
  <trd lng="oc">camioneta</trd>
  <trdgrp lng="pl">
    <trd>p&oacute;&lstroke;ci&eogonek;&zdot;ar&oacute;wka</trd>,
    <trd>samoch&oacute;d dostawczy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_a;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dodávka</trd>,
    <trd>dodávkové auto</trd>
  </trdgrp>
</drv>

<drv mrk="kamion.baskul0o">
  <kap>baskul<tld/>o</kap>
  <snc mrk="kamion.baskul0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="kamion.0o.AUT">Kamiono</ref>
      kun platformo, kiu baskule klini&gcirc;as por el&scirc;uto
      de la enhavo:
      <ekz>
        baskul<tld/>oj faligis en la riveron &scirc;tonojn kaj gruzon
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Novosibirska_hidroelektra_centralo">
          Novosibirska hidroelektra centralo</url>, 2013-04-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sklápěcí auto</trd>
  <trdgrp lng="es">
    <trd>volquete</trd>,
    <trd>cami&oacute;n de obras</trd>
  </trdgrp>
  <trd lng="fr">camion &agrave; benne basculante</trd>
  <trd lng="sk">vyklápacie auto</trd>
</drv>
</art>
