<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: artur.xml,v 1.11 2015/02/26 20:11:06 revo Exp $">
<kap>
  <rad>Artur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="artur.0o">
  <kap><tld/>o</kap>
  <snc mrk="artur.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> keltdevena:
      <ekz>
        via nomo estas <tld/>o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
        Devjatnin), 2</lok></fnt>?!
      </ekz>
    </dif>
  </snc>
  <trd lng="br">Arzhur</trd>
  <trd lng="ca">Artur</trd>
  <trd lng="de">Artur</trd>
  <trd lng="en">Arthur</trd>
  <trd lng="es">Arturo</trd>
  <trd lng="hu">Art&uacute;r</trd>
  <trd lng="it">Arturo</trd>
  <trd lng="fr">Arthur</trd>
  <trd lng="nl">Arthuur</trd>
  <trd lng="pl">Artur</trd>
  <trd lng="ru">&c_A;&c_r;&c_t;&c_u;&c_r;</trd>
</drv>
</art>

