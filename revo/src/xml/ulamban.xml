<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ulamban.xml,v 1.8 2015/07/04 08:37:45 revo Exp $">
<kap>
     <rad>ulamban</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ulamban.0o">
     <kap><tld/>o</kap>
     <snc mrk="ulamban.0o.BUD">
       <uzo tip="fak">BUD</uzo>
       <uzo tip="fak">KAL</uzo>
       <dif>
         Budaisma <ref tip="lst" cel="fest.0o"
         lst="voko:festoj">festo</ref>
          por mortintaj prauloj:
         <ekz>
           Sansa Danco estas unu el dancadoj en <tld/>o
           <fnt>
             <aut>Sasaki M.</aut>,
             <vrk><url
ref="http://morioka.cool.ne.jp/ihatov-esperanto/sansa.html">Kio
             estas Sansa Danco?</url></vrk>,
             <lok>2004-08-09</lok>
           </fnt>;
         </ekz>
         <ekz>
           <tld/>a akrido <klr>...</klr> aperas ofte
           &ccirc;irka&ubreve; la tagoj de <tld/>o (t.e. en 
a&ubreve;gusto)
           <fnt>
           <aut>Vastalto</aut>,
           <vrk><url
           ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
           <lok>2006-08-03</lok>
           </fnt>.
         </ekz>
       </dif>
     </snc>
  <trd lng="cs">japonský svátek mrtvých</trd>
  <trdgrp lng="fi">
         <trd>ullambana</trd>,
         <trd><klr tip="amb">(buddhalainen) </klr> esi-isien
         juhla</trd>,
         <trd>obon-juhla <klr> (Japanissa)</klr></trd>
     </trdgrp>
  <trd lng="fr">ullambana<klr> (toussaint bouddhique)</klr></trd>
  <trdgrp lng="it">
         <trd>ullambana</trd>,
         <trd>y&uacute;l&aacute;np&eacute;n<klr> (cina)</klr></trd>,
         <trd>obon<klr> (giappone)</klr></trd>
     </trdgrp>
  <trd lng="sk">japonský sviatok mŕtvych</trd>
</drv>
</art>

