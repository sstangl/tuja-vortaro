<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kerkedul.xml,v 1.8 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kerkedul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kerkedul.0o">
  <kap><tld/>o</kap>
  <snc mrk="kerkedul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda na&gcirc;obirdo de nordaj Europo kaj Azio,
      specio de la genro <ref tip="super"
      cel="anas.0o.ZOO">anaso</ref> (<trd lng="la">Anas
      querquedula</trd>), vivanta en densaj kreska&jcirc;oj apud stagnaj
      akvejoj :
      <ekz>
        ha&ubreve;taj pecoj de <tld/>aj kapoj
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 105a, volumo 3a, p. 367a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="anas.marcx0o">mar&ccirc;anaso</ref>
    <trdgrp lng="ca">
      <trd>xarrasclet</trd>,
      <trd>roncadell</trd>
    </trdgrp>
    <trd lng="en">garganey</trd>
    <trd lng="fr">sarcelle d'&eacute;t&eacute;</trd>
    <trd lng="hu">b&odblac;jti <ind>r&eacute;ce</ind></trd>
    <trd lng="nl">zomertaling</trd>
    <trd lng="pl">cyranka</trd>
    <trd lng="sv">&aring;rta</trd>
  </snc>
  <trd lng="cs">Čírka modrá</trd>
  <trd lng="sk">kačica chrapačka</trd>
</drv>
</art>

