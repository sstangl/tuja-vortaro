<vortaro>
<art mrk="$Id: hipoka1.xml,v 1.11 2013/08/25 16:10:34 revo Exp $">
<kap>
  <rad>hipoka&scirc;tanac</rad>/oj
</kap>
<drv mrk="hipoka1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Hippocastanaceae</trd>).
    </dif>
  </snc>
  <trd lng="ca">hipocastan&agrave;cies</trd>
  <trd lng="de">Rosskastaniengew&auml;chse</trd>
  <trd lng="fr">hippocastanac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>vadgesztenyef&eacute;l&eacute;k</trd>,
    <trd>l&oacute;gesztenyef&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="it">ippocastanacee</trd>
  <trd lng="nl">paardekastanjefamilie</trd>
  <trd lng="pt">hipocastan&aacute;ceas</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_k;&c_o;&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
