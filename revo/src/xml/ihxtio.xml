<vortaro>
<art mrk="$Id: ihxtio.xml,v 1.11 2006/05/15 16:31:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>i&hcirc;tiokol</rad>/o
</kap>
<drv mrk="ihxtio.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Gluo fabrikata el flosveziko de kelkaj fi&scirc;oj.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Hausenblase</trd>,
     <trd>Fischleim</trd>
  </trdgrp>
  <trd lng="fr">ichtyocolle</trd>
  <trd lng="hu">halenyv</trd>
  <trd lng="nl">vislijm</trd>
  <trd lng="pl">karuk<klr> (klej rybi)</klr></trd>
  <trdgrp lng="pt">
   <trd>ictiocola</trd>, 
   <trd>cola-de-peixe</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_h;&c_t;&c_i;&c_o;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_r;&c_y;&c_b;&c_i;&c_j; &c_k;&c_l;&c_je;&c_j;</trd>
  </trdgrp>
</drv>
</art>
