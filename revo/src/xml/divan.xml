<art mrk="$Id: divan.xml,v 1.17 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>divan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="divan.0o">
  <kap><tld/>o</kap>
  <snc mrk="divan.0o.MEBL">
    <dif>
      Turkdevena <ref tip="super" cel="sof.0o.MEBL">sofo</ref>;
      benko sen brak- a&ubreve; dorsapogiloj, garnita per kusenoj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="kanap.0o">kanapo</ref>
    </refgrp>
    <trdgrp lng="de">
       <trd>Diwan</trd>,
       <trd>Ruhebett</trd>,
       <trd>niedriges <ind>Liegesofa</ind></trd>
    </trdgrp>
    <trd lng="en">ottoman</trd>
    <trd lng="es">div&aacute;n</trd>
    <trd lng="fr">divan<klr tip="ind"> (si&egrave;ge)</klr></trd>
    <trdgrp lng="hu">
       <trd>d&iacute;v&aacute;ny</trd>,
       <trd>kerevet</trd>,
       <trd>pamlag</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>otomana</trd>
    </trdgrp>
    <trd lng="pt">div&atilde;</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_o;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="divan.0o.POL">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Turka &scirc;tata konsilantaro.  
    </dif>
    <trd lng="de">Diwan</trd>
    <trd lng="en">oriental council</trd>
    <trd lng="fr">divan<klr tip="ind"> (salle du Conseil
    turc)</klr></trd>
    <trd lng="hu">d&iacute;v&aacute;n</trd>
    <trd lng="pt">div&atilde;</trd>
    <trd lng="pl">Dywan<klr> (rada turecka)</klr></trd>
    <trd lng="ru">&c_d;&c_i;&c_v;&c_a;&c_n;</trd>
  </snc>
  <trd lng="ca">divan</trd>
  <trdgrp lng="cs">
    <trd>divan</trd>,
    <trd>pohovka</trd>
  </trdgrp>
  <trd lng="en">divan</trd>
  <trdgrp lng="sk">
    <trd>div√°n</trd>,
    <trd>pohovka</trd>
  </trdgrp>
</drv>
</art>
