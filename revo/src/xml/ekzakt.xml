<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ekzakt.xml,v 1.17 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>ekzakt</rad>/a
</kap>
<drv mrk="ekzakt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Science &gcirc;usta kaj severe preciza:
      <ekz><tld/>a difino, senco, kalkulo;</ekz>
      <ekz><tld/>aj sciencoj <klr>(matematikoj)</klr>;</ekz>
      <ekz><tld/>a rakonto, reprodukto.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erar">senerara</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="preciz.0a">preciza</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>
  <trd lng="ca">exacte</trd>
  <trdgrp lng="cs">
    <trd>dokonalý</trd>,
    <trd>exaktní</trd>,
    <trd>vědecky přesný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>genau</trd>,
    <trd>exakt</trd>
  </trdgrp>
  <trd lng="en">exact</trd>
  <trd lng="es">exacto</trd>
  <trd lng="fr">exact<klr tip="ind"> (v&eacute;ridique, juste,
  pr&eacute;cis)</klr></trd>
  <trd lng="hu">egzakt</trd>
  <trd lng="nl">exact</trd>
  <trdgrp lng="pl">
    <trd>dok&lstroke;adny</trd>,
    <trd>&sacute;cis&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">exato</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bezchybný</trd>,
    <trd>dôkladný</trd>,
    <trd>vedecky presný</trd>
  </trdgrp>
</drv>
</art>
