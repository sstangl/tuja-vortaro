<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: salm.xml,v 1.29 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salm</rad>/o
</kap>
<drv mrk="salm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Mara fi&scirc;o</ref> 
      kun rozkolora bongusta viando,
      kiu kontra&ubreve;fluen migras en la riverojn por fraji;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="salmed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Salmo</trd>), 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_o;&c_s;&c_mol;</trd>; 
    <trd>&c_s;&c_jo;&c_m;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_mol;&c_o;&c_m;&c_g;&c_a;</trd>
  <trd lng="ca">salm&oacute;</trd>
  <trd lng="cs">losos</trd>
  <trd lng="de">Lachs</trd>
  <trd lng="en">salmon</trd>
  <trd lng="es">salm&oacute;n</trd>
  <trd lng="fr">saumon</trd>
  <trd lng="hu">lazac</trd>
  <trd lng="id">salmon</trd>
  <trd lng="it">salmone</trd>
  <trd lng="nl">zalm</trd>
  <trd lng="pl">&lstroke;oso&sacute;<klr> szlachetny</klr></trd>
  <trdgrp lng="pt">
   <trd>salm&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;
    <ind>&c_l;&c_o;&c_s;&c_o;&c_s;&c_mol;</ind> <klr
    tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>,
    <trd>&c_l;&c_o;&c_s;&c_o;&c_s;&c_mol; <klr>(unu el
    specioj)</klr></trd>,
    <trd>&c_s;&c_jo;&c_m;&c_g;&c_a; <klr>(unu el specioj, =
    &c_l;&c_o;&c_s;&c_o;&c_s;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="sk">losos</trd>
</drv>

<drv mrk="salm.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> de
      <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Salmoniformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">salmoniformes</trd>
  <trd lng="de">Lachsfische</trd>
  <trd lng="fr">salmoniformes</trd>
  <trd lng="hu">lazacalak&uacute;ak</trd>
  <trd lng="it">salmoniformi</trd>
  <trd lng="pl">&lstroke;ososiokszta&lstroke;tne</trd>
  <trd
  lng="ru">&c_l;&c_o;&c_s;&c_o;&c_s;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
