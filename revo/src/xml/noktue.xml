<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: noktue.xml,v 1.9 2013/08/31 16:10:18 revo Exp $">
<kap>
  <rad>noktued</rad>/oj
</kap>
<drv mrk="noktue.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="lepido1.0oj">lepidopteroj</ref>
      (<trd lng="la">Noctuidae</trd>), de noktaj papilioj kun
      strigsimila
      koloraran&gcirc;o sur la anta&ubreve;aj flugiloj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_w;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">noctuids</trd>
  <trd lng="de">Eulenschmetterlinge</trd>
  <trdgrp lng="hu">
    <trd>bagolypill&eacute;k<klr tip="ind"> (csal&aacute;d)</klr></trd>,
    <trd>bagolylepk&eacute;k<klr tip="ind"> (csal&aacute;d)</klr></trd></trdgrp>
  <trd lng="nl">nachtuiltjes<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">noctu&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_b;&c_a;&c_b;&c_o;&c_ch;&c_je;&c_k;)</klr></trd>,
    <trd>&c_n;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_b;&c_a;&c_b;&c_o;&c_ch;&c_je;&c_k;)</klr></trd>
  </trdgrp>
</drv>
</art>
