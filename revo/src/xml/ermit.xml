<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ermit.xml,v 1.19 2012/04/21 16:10:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ermit</rad>/o
</kap>
<drv mrk="ermit.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="ermit.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Piulo vivanta en soleco.
    </dif>
    <trd
    lng="be">&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>ermit&agrave;</trd>,
      <trd>anacoreta</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Eremit</trd>,
      <trd>Klausner</trd>,
      <trd>Einsiedler</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>anachor&egrave;te</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>eremita</trd>,
      <trd>pustelnik</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>eremita</trd>,
      <trd>ermit&atilde;o</trd>,
      <trd>anacoreta</trd>
    </trdgrp>
    <trd
    lng="ru">&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="ermit.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo, kiu sisteme evitas la aliajn, kaj izole vivas.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_m;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_ib;&c_n;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Einsiedler</trd>,
      <trd>Einzelg&auml;nger</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>samotnik</trd>,
      <trd>pustelnik</trd>
    </trdgrp>
  </snc>
  <trd lng="en">hermit</trd>
  <trdgrp lng="fr">
    <trd>ermite</trd>,
    <trd>solitaire<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">remete</trd>
  <trdgrp lng="pt">
    <trd>eremita</trd>,
    <trd>ermit&atilde;o</trd>,
    <trd>solit&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eremiet</trd>,
    <trd>heremiet</trd>,
    <trd>kluizenaar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_v;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_h;&c_o;&c_r;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ermita&ntilde;o</trd>,
    <trd>anacoreta</trd>
  </trdgrp>
</drv>

<drv mrk="ermit.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie vivas unu a&ubreve; pluraj <tld/>oj.
    </dif>
    <trdgrp lng="de">
      <trd>Eremitage</trd>,
      <trd>Klause</trd>,
      <trd>Einsiedelei</trd>
    </trdgrp>
    <trd lng="en">hermitage</trd>
    <trd lng="fr">ermitage</trd>
    <trd lng="hu">remetelak</trd>
    <trdgrp lng="nl">
      <trd>kluizenaarswoning</trd>,
      <trd>ermitage</trd>,
      <trd>hermitage</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pustelnia</trd>,
      <trd>ustronie</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>eremit&eacute;rio</trd>,
      <trd>ermit&eacute;rio</trd>,
      <trd>ermit&aacute;gio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_s;&c_t;&c_o;
      &c_u;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>;
      <trd>&c_s;&c_k;&c_i;&c_t;</trd>,
      <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">ermita</trd>
  </snc>
</drv>

<drv mrk="ermit.0ludo">
  <kap>
    <tld/>ludo<fnt><bib>PIV1</bib></fnt>
  </kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Ludo, kiun oni ludas sola.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_ja;&c_n;&c_s;</trd>
    <trd lng="en">solitaire</trd>
    <trd lng="fr">solitaire<klr tip="ind"> (jeu)</klr></trd>
    <trd lng="hu">paszi&aacute;nsz</trd>
    <trd lng="nl">patience</trd>
    <trd lng="pl">samotnik</trd>
    <trd lng="ru">&c_p;&c_a;&c_s;&c_mol;&c_ja;&c_n;&c_s;</trd>
    <trd lng="es">solitario</trd>
    <rim>
      &Ccirc;u temas &gcirc;enerale pri ludoj kiuj oni ludas solaj,
      &ccirc;u pri ludo en kiu oni transsaltas unu peonon per alia tiel
      ke restu nur unu?
      <aut>SC</aut>
    </rim>
  </snc>
</drv>
</art>
