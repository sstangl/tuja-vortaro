    <trd>&c_p;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>enteresa</trd>,
    <trd>completesa</trd>,
    <trd>totalitat</trd>
  </trdgrp>
  <trd lng="cs">celistvost</trd>
  <trdgrp lng="en">
    <trd>entirety</trd>,
    <trd>completeness</trd>,
