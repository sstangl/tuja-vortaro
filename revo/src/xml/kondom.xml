<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kondom.xml,v 1.12 2009/07/01 17:23:12 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kondom</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kondom.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ka&ubreve;&ccirc;uka protektilo de la <ref tip="vid"
      cel="penis.0o">peniso</ref>,
      uzata dum <ref tip="vid" cel="koit.0o">koito</ref> por preventi
      infekti&gcirc;on a&ubreve; gravedi&gcirc;on:
      <ekz>
        multjara uzado de <tld/>oj fare de la prostituitinoj, kondukis
        al redukti&gcirc;o de aidoso
        <fnt><aut>Vladimir Lemelev</aut>: <vrk>Grava
        bazo por preventi aidoson</vrk>, <bib>Monato</bib><lok>jaro
        2001a,
        numero 5a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="koncip.kontraux0ilo">kontra&ubreve;koncipilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_d;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_e;&c_z;&c_e;&c_r;&c_v;&c_a;&c_t;&c_y;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>preservatiu</trd>,
    <trd>condom</trd>,
    <trd>cond&oacute;</trd>,
    <trd>profil&agrave;ctic</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kondom</trd>,
    <trd>Pariser</trd>
  </trdgrp>
  <trd lng="en">condom</trd>
  <trdgrp lng="es">
    <trd>preservativo</trd>,
    <trd>cond&oacute;n</trd>,
    <trd>profil&aacute;ctico</trd>,
    <trd>goma<klr> (coloquial)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pr&eacute;servatif</trd>,
    <trd>capote<klr tip="ind"> (pr&eacute;servatif)</klr></trd>,
    <trd>capote anglaise</trd>,
    <trd>condom</trd>
  </trdgrp>
  <trd lng="nl">condoom</trd>
  <trdgrp lng="pl">
    <trd>kondom</trd>,
    <trd>prezerwatywa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>preservativo</trd>,
    <trd>camisa-de-v&ecirc;nus</trd>,
    <trd>camisinha</trd>,
    <trd>preventivo</trd>,        
    <trd>condom</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_d;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_a;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
</drv>
</art>
