  <trd lng="hu">bibliogr&aacute;fia</trd>
  <trd lng="mk">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&#1112;&c_a;</trd>
  <trd lng="nl">bibliografie</trd>
  <trd lng="pl">bibliografia</trd>
  <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
