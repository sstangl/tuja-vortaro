    <trd>bibliografie</trd>,
    <trd>seznam literatury</trd>
  </trdgrp>
  <trd lng="de">Bibliografie</trd>
  <trd lng="en">bibliography</trd>
  <trd lng="es">bibliograf&iacute;a</trd>
  <trd lng="fa">&f_k;&a_t;&a_A;&a_b;&a_n;&a_A;&a_m;&a_h;</trd>
