        ja neniu <tld/>o estas kompleta<fnt><aut>Paul Peeraerts</aut>:
        <vrk>Kiu sekvos? (William Auld kaj E. G. Evans, Bibliografio de
        tradukoj el la angla lingvo)</vrk>, <bib>Monato</bib><lok>jaro
        1996a, numero 6a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="biblio2.0o">bibliografo</ref>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
