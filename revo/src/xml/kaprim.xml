<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaprim.xml,v 1.14 2011/12/06 23:10:18 revo Exp $">
<kap>
  <rad>kaprimulg</rad>/o
</kap>
<drv mrk="kaprim.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Caprimulgus</trd>),
      birdo kun lar&gcirc;a beko, grandaj okuloj kaj arb&scirc;elkolora
      plumaro.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="hirund.nokto0o">noktohirundo</ref>
  </refgrp>                 
  <trd lng="be">&c_l;&c_ja;&c_l;&c_ja;&c_k;</trd>
  <trd lng="br">adren</trd>
  <trd lng="ca">g&egrave;nere Caprimulgus</trd>
  <trd lng="cs">lelek</trd>
  <trdgrp lng="de">
    <trd>Ziegenmelker</trd>,
    <trd>Nachtschwalbe</trd>
  </trdgrp>
  <trd lng="en">nightjar</trd>
  <trdgrp lng="fr">
    <trd>engoulevent</trd>
    <trd><ind>crapaud</ind> volant<klr tip="ind">
    (engoulevent)</klr></trd>
  </trdgrp>
  <trd lng="hu">kecskefej&odblac;</trd>
  <trdgrp lng="nl">
    <trd>geitenmelker<klr tip="ind"> (zwaluw)</klr></trd>,
    <trd>nachtzwaluw</trd>
  </trdgrp>
  <trd lng="pl">lelek kozod&oacute;j</trd>
  <trd lng="pt">noitib&oacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_o;&c_d;&c_o;&c_j;</trd>
</drv>
</art>
