<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kasero.xml,v 1.14 2009/08/27 16:32:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kaserol</rad>/o
</kap>
<drv mrk="kasero.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Metala kuirpoto kun tenilo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>      
  <trd lng="br">kastelodenn</trd>
  <trdgrp lng="ca">
     <trd>casserola (amb m&agrave;nec)</trd>,
     <trd>cassola (amb anses)</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kasserolle</trd>,
     <trd>Schmorpfanne</trd>
  </trdgrp>
  <trd lng="en">stewpan</trd>
  <trdgrp lng="es">
     <trd>cacerola</trd>,
     <trd>cazo</trd>
  </trdgrp>
  <trd lng="fr">casserole</trd>
  <trd lng="hu">serpeny&odblac;</trd>
  <trd lng="nl">pan</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_ja; <klr>(&c_s; &c_o;&c_d;&c_n;&c_o;&c_j;
  &c_r;&c_u;&c_ch;&c_k;&c_o;&c_j;)</klr></trd>
  <trd lng="tr">sapl&inodot; tencere</trd>
</drv>
</art>
