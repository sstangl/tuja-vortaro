<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bremen.xml,v 1.19 2015/11/06 15:10:30 revo Exp $">
<kap><rad>Bremen</rad>/o</kap>

<drv mrk="bremen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <fnt><bib>ElektFab</bib></fnt>
  <fnt><bib>PMEG</bib></fnt>
  <snc mrk="bremen.0o.urbo">
    <dif>
      Havena <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      en norda <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> 
      (8&#730;71'E, 53&#730;21'N), situanta &ccirc;e la rivero Vesero, regurbo de la
      federacia lando <ref tip="dif"
      cel="bremen.0o.subsxtato">Bremeno<sncref/></ref>:
      <ekz>
        mi iras <tld/>on
        <fnt><bib>ElektFab</bib> <lok>Bremenaj muzikistoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a biero
        <fnt><bib>Fab1</bib> <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bremen</trd>
    <trd lng="lat">Brema</trd>
  </snc>
  <snc mrk="bremen.0o.subsxtato">
    <dif>
      La plej malgranda el la federaciaj &scirc;tatoj de <ref
      tip="malprt"
      cel="german1.G0ujo">Germanujo</ref> kun
      regurbo kaj plej granda urbo <ref tip="dif"
      cel="bremen.0o.urbo">Bremeno<sncref/></ref>
      kaj dua plej granda urbo Bremerhaveno.
    </dif>
    <trd lng="de"><klr>(Stadtstaat) </klr>Bremen</trd>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_e;&c_m;&c_e;&c_n;</trd>
  <trd lng="bg">&c_B;&c_r;&c_je;&c_m;&c_je;&c_n;</trd>
  <trd lng="de">Bremen</trd>
  <trd lng="hu">Br&eacute;ma</trd>
  <trd lng="nl">Bremen</trd>
  <trd lng="fr">Br&ecirc;me</trd>
  <trd lng="pl">Brema</trd>
  <trd lng="ru">&c_B;&c_r;&c_je;&c_m;&c_je;&c_n;</trd>
</drv>

<drv mrk="bremen.b0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      Civitano de Bremeno.
    </dif>
  </snc>
  <trd lng="de">Bremer</trd>
  <trd lng="hu">br&eacute;mai (lakos)</trd>
  <trd lng="nl">Bremer</trd>
  <trd lng="pl">breme&nacute;czyk</trd>
  <trd lng="ru">&c_b;&c_r;&c_je;&c_m;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
