<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vina.xml,v 1.9 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vina</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="vina.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sudhinda <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      granda liuto kun longa kolo,
      4 &ccirc;efaj, 3 ritmaj, pin&ccirc;e sonigataj kordoj, 24 fretoj,
      du sonkestoj, el kiuj la pli eta fiksitas supre &ccirc;e la kolo.
    </dif>
  </snc>
  <trd lng="ca">vina</trd>
  <trd lng="cs">vína</trd>
  <trdgrp lng="de">
    <trd>Vina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sarasvat&icirc; <ind>v&icirc;n&acirc;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vina</trd>,
    <trd>cs&odblac;citera</trd>
  </trdgrp>
  <trd lng="nl">vina</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_a;<klr tip="amb"> (&c_m;&c_u;&c_z;.
  &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sk">vína</trd>
</drv>
</art>

