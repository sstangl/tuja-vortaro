<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: delfin.xml,v 1.17 2013/08/30 14:10:48 revo Exp $">
<kap>
  <rad>delfini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="delfin.0o">
  <kap><tld/>o</kap>
  <snc mrk="delfin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara vegeta&jcirc;o kun bluaj spronhavaj floroj,
      el kiu pluraj specioj estas &gcirc;ardene
      kulturataj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Delphinium</trd>, <trd lng="la">Consolida</trd>).
    </dif>
    <ref tip="sin" cel="spron.kavalir0o">kavalirsprono</ref>
  </snc>
  <bld lok="../bld/delfini.jpg">
    <ind>kampa <tld/>o</ind>
    <trd lng="la">Consolida regalis</trd>
    <trdgrp lng="hu">
      <trd>mezei <ind>szarkal&aacute;b</ind></trd>,
      <trd>vet&eacute;si <ind>szarkal&aacute;b</ind></trd>
  </trdgrp>
  </bld>
  <trd lng="be">&c_r;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_k;&c_ib;</trd>
  <trd lng="de">Rittersporn</trd>
  <trd lng="en">larkspur</trd>
  <trd lng="fr">dauphinelle</trd>
  <trdgrp lng="hu">
    <trd>szarkal&aacute;b</trd>,
    <trd>sarkvir&aacute;g</trd>
  </trdgrp>
  <trd lng="pt">delf&iacute;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_o;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_p;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
