<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: barakt.xml,v 1.20 2015/07/04 08:37:31 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>barakt</rad>/i
</kap>
<drv mrk="barakt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Forte skui, movi la membrojn, luktante por sin liberigi:
      <ekz>
        <tld/>i en &scirc;limo, en akvo;
      </ekz>
      <ekz>
        la kaptito furioze <tld/>is<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; la morto;
      </ekz>
      <ekz>
        <tld/>i kiel fi&scirc;o ekster la akvo<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="tord">tordi&gcirc;i</ref>,
      <ref cel="star">kontra&ubreve;stari</ref>,
      <ref cel="ced.mal0i">malcedi</ref>,
      <ref cel="rezist.0i">rezisti</ref>,
      <ref cel="lukt.0i">lukti</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd><ind>difreta&ntilde;</ind>, en em zifreta&ntilde;</trd>,
      <trd>diskrapa&ntilde;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>verg&odblac;dik</trd>,
      <trd>kap&aacute;l&oacute;dzik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>worstelen</trd>,
      <trd>vechten</trd>
    </trdgrp>
    <trd lng="pl">szamota&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_r;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte penadi:
      <ekz>
        <tld/>i kontra&ubreve; malfacila&jcirc;oj,
        kontra&ubreve; la cirkonstancoj;
      </ekz>
      <ekz>
        li <tld/>as en kontra&ubreve;diroj;
      </ekz>
      <ekz>
        multaj projektoj <tld/>is en lia kapo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nun en &scirc;ia kapo <tld/>is nova demando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la internacia vivo <tld/>as por sin organizi.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&uuml;szk&ouml;dik</trd>,
      <trd>tusakodik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kampen</trd>,
      <trd>zwoegen</trd>,
      <trd>zich <ind>uitsloven</ind></trd>
    </trdgrp>
    <trd lng="pl">boryka&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>debater-se</trd>,
      <trd>estrebuchar</trd>,
      <trd>espernear</trd>,
      <trd>sacudir-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bojovat</trd>,
    <trd>mávat</trd>,
    <trd>ohánět se</trd>,
    <trd>třepetat se</trd>,
    <trd>zmítat se</trd>,
    <trd>zápasit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zappeln</trd>,
    <trd>strampeln</trd>,
    <trd>k&auml;mpfen</trd>
  </trdgrp>
  <trd lng="en">struggle</trd>
  <trdgrp lng="es">
    <trd>Debatirse</trd>,
    <trd>sacudirse</trd>,
    <trd>luchar por liberarse</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_q;&a_l;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_d;&a_s;&a_t; &a_w; &f_p;&a_A; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;battre</ind></trd>,
    <trd>se <ind>d&eacute;mener</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>boriť sa</trd>,
    <trd>zmietať sa</trd>,
    <trd>zápasiť<klr>(s niečím)</klr></trd>
  </trdgrp>
</drv>
</art>
