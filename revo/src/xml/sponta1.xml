<kap>
  <ofc>3</ofc>
  <rad>spontane</rad>/a
</kap>
<drv mrk="sponta1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Fari&gcirc;anta per si mem, sen ia ekstera influo; propramova,
      propravola, meminiciata:
          <ekz>
            ili kutimi&gcirc;is al zorge preparitaj prelegoj
            anstata&ubreve;
            <tld/>a diskuto kia okazas nun
	    <fnt>
	      <aut>Platono, tr. D. Broadribb</aut>:
	      <vrk>La Respubliko</vrk>. <lok>Parto 6</lok>.
	    </fnt>;
          </ekz><ekz>
            la mutacioj povas esti <tld/>aj, sen videblaj a&ubreve;
            rekoneblaj eksteraj instigoj
            <fnt>
              <vrk><url
 ref="http://www.fw.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm"
                  >Enkonduko en la biologion</url></vrk>
            </fnt>.
          </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;</trd>, 
    <trd>&c_s;&c_a;&c_m;&c_a;&c_a;&c_d;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    
    <trd>&c_s;&c_t;&c_y;&c_h;&c_ib;&c_j;&c_n;&c_y;</trd> 
  </trdgrp>
  <trd lng="bg">&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>samovoln&yacute;</trd>,
    <trd>spont&aacute;nn&iacute;</trd>
  </trdgrp>
  <trd lng="de">spontan</trd>
  <trd lng="en">spontaneous</trd>
  <trd lng="fr">spontan&eacute;</trd>
  <trdgrp lng="hu">
    <trd>spont&aacute;n</trd>,
    <trd>&ouml;nk&eacute;nytelen</trd>
  </trdgrp>
  <trd lng="nl">spontaan</trd>
  <trd lng="pl">spontaniczny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_n;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_h;&c_i;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>samovo&lcaron;n&yacute;</trd>,
    <trd>spont&aacute;nny</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3595;&#3638;&#3656;&#3591;&#3648;&#3611;&#3655;&#3609;&#3652;&#3611;&#3605;&#3634;&#3617;&#3608;&#3619;&#3619;&#3617;&#3594;&#3634;&#3605;&#3636;</trd>,
    <trd>&#3648;&#3585;&#3636;&#3604;&#3586;&#3638;&#3657;&#3609;&#3648;&#3629;&#3591;</trd>
  </trdgrp>
</drv>
