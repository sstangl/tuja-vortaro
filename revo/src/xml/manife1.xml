<kap>
  <ofc>3</ofc>
  <rad>manifestaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="manife1.0o">
  <kap><tld/>o</kap>
  <snc mrk="manife1.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Amasa publika, plej ofte surstrata, elmontrado de opinio, precipe
      por politikaj celoj:
      <ekz>
        la laboristoj <tld/>is por la okhora laboro, kontra&ubreve; la
        kondamno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit">agitado</ref>,
      <ref cel="protes.amas0o.POL">amasprotesto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_ib;&c_f;&c_e;&c_s;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_d;&c_e;&c_m;&c_a;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd
  lng="bg">&c_m;&c_a;&c_n;&c_i;&c_f;&c_je;&c_s;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="br">manifestadeg</trd>
  <trd lng="ca">manifestaci&oacute;</trd>
  <trd lng="cs">manifestace</trd>
  <trd lng="de">Demonstration</trd>
  <trd lng="en">demonstration</trd>
  <trd lng="es">manifestaci&oacute;n</trd>
  <trd lng="fr">manifestation</trd>
  <trdgrp lng="nl">
    <trd>betoging</trd>,
    <trd>manifestatie</trd>,
    <trd>demonstratie</trd>,
    <trd>protestmars</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>manifestacja</trd>,
    <trd>demonstracja</trd>
  </trdgrp>
  <trd lng="pt">manifesta&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_n;&c_i;&c_f;&c_je;&c_s;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(zhromaždenie)</klr></trd>,
    <trd>manifestácia</trd>
  </trdgrp>
</drv>
