<drv mrk="sxijaism.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxijaism.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      La dua, neortodoksa formo de
      <ref tip="super" cel="islam.0o">islamo</ref>, ne
      agnoskanta <ref tip="vid" cel="suna.0o">Sunaon</ref>.
    </dif>
    <ref tip="vid" cel="suna.0ismo">sunaismo</ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_i;&c_z;&c_m;</trd>
  <trd lng="ca">xiisme</trd>
  <trd lng="en">shi'iteism</trd>
  <trd lng="fr">chiisme</trd>
  <trd lng="hu">siitizmus</trd>
  <trd lng="id">Syiah</trd>
  <trd lng="it">sciismo</trd>
  <trd lng="nl">sjiisme</trd>
  <trd lng="pt">xiismo</trd>
  <trd lng="ru">&c_sh;&c_i;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
