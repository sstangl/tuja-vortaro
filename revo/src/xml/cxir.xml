<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: cxir.xml,v 1.9 2014/06/03 12:10:17 revo Exp $">
    <kap>
      <rad>&ccirc;ir</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="cxir.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">BOT</uzo>
        <dif>
          Maldika spirale volvi&gcirc;anta
          <ref tip="sin" cel="krocx.0ilo.cxiro">kro&ccirc;ilo</ref>
          de grimpa planto, ekz-e de
          <ref cel="vin.0berarbo0berujo">vinberujo</ref>.
        </dif>
        </snc>
        <trd lng="be">&c_v;&c_u;&c_s;&c_ib;&c_k; <klr tip="amb">(&c_u;
        &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;)</klr></trd>
        <trd lng="ca">circell</trd>
        <trdgrp lng="en">
          <trd>tendril</trd>,
          <trd>runner</trd>        
        </trdgrp>
        <trd lng="fr">vrille</trd>
        <trd lng="hu">kacs</trd>
        <trd lng="it">viticcio</trd>
        <trd lng="pl">w&aogonek;s<klr tip="amb"> czepny</klr></trd>
        <trd lng="ru">&c_u;&c_s;&c_i;&c_k; <klr>(&c_b;&c_o;&c_t;.)</klr></trd>
    </drv>
  </art>
