<vortaro>
<art mrk="$Id: katamaran.xml,v 1.8 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>katamaran</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="katamaran.0o">
  <kap><tld/>o</kap>
  <snc mrk="katamaran.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="sxip.0o.MAR">&Scirc;ipo</ref> kun du
      paralelaj &scirc;eloj:
      <ekz>
        la <tld/>o estis pliakutiganta la angulon al la vento
        <fnt>
        <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij Finkel</aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis1.html">parto
        
1a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">katamarán</trd>
  <trd lng="de">Katamaran</trd>
  <trd lng="fr">catamaran</trd>
  <trd lng="hu">katamar&aacute;n</trd>
  <trdgrp lng="nl">
    <trd>catamaran</trd>,
    <trd>multirompsboot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>katamaran</trd>,
    <trd>dwukad&lstroke;ubowiec</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="sk">katamarán</trd>
</drv>
</art>

