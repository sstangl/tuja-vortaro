<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mosk.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mosk</rad>/o
</kap>
<drv mrk="mosk.0o">
  <kap><tld/>o</kap>
  <snc mrk="mosk.0o.PAR">
    <dif>
      Tre odora substanco uzata por fabriki parfumojn, sekreciita de iuj
      bestoj, precipe de iu rema&ccirc;ulo, analoga al kapreolo.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>mošus</trd>,
    <trd>pižmo</trd>
  </trdgrp>
  <trd lng="de">Moschus</trd>
  <trd lng="en">musk</trd>
  <trd lng="fr">musc</trd>
  <trd lng="nl">muscus</trd>
  <trd lng="pt">alm&iacute;scar</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trd lng="sk">pižmo</trd>
</drv>

<drv mrk="mosk.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="mosk.0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="ondatr.0o">Ondatro</ref>:
      <ekz>
        dek <tld/>ulaj feloj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_t;&c_r;&c_a;</trd>
  <trdgrp lng="de"><trd>Bisamratte</trd>,
  <trd>Moschusratte</trd></trdgrp>
  <trd lng="fr">rat musqu&eacute;</trd>
  <trd lng="nl">muskusrat</trd>
  <trdgrp lng="pt">
   <trd>rato-almiscarado</trd>,
    <trd>Ondatra zibethicus</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_n;&c_d;&c_a;&c_t;&c_r;&c_a;</trd>
</drv>
</art>
