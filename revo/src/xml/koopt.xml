<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koopt.xml,v 1.3 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>koopt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="koopt.0i">
  <kap><tld/>i</kap>
  <snc mrk="koopt.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="vid" cel="elekt.al0i">Alelekti</ref>
      per komuna decido de jamaj anoj:
      <ekz>
        la Estraro <klr>...</klr> &ccirc;e rezigno de estrarano dum 
        balotperiodo rajtas <tld/>i novan membron
        <fnt>
         <aut>-</aut>,
         <vrk><url
         ref="http://w3.forst.tu-muenchen.de/~quednau/klerigofico/statutoj/sk.txt">
         Statuto de la Slovakia AIS</url></vrk>,
         <lok>[2006?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kooptovat</trd>,
    <trd>přibrat za člena</trd>
  </trdgrp>
  <trd lng="fr">coopter</trd>
  <trdgrp lng="sk">
    <trd>kooptovať</trd>,
    <trd>pribrať za člena</trd>
  </trdgrp>
</drv>
</art>

