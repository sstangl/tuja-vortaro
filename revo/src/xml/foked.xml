<art mrk="$Id: foked.xml,v 1.13 2015/10/27 11:10:33 revo Exp $">
<kap><rad>foked</rad>/oj</kap>
<drv mrk="foked.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Phocidae</trd>), vivantaj en maro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_c;&c_ju;&c_l;&c_je;&c_n;&c_ib;</ind> <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_malmol;&c_shch;&c_i;&c_n;&c_s;&c_k;&c_i; <ind>&c_t;&c_ju;&c_l;&c_je;&c_n;&c_i;</ind></trd>
  <trd lng="cs">tule&ncaron;ovit&iacute;</trd>
  <trd lng="de">Hundsrobben</trd>
  <trd lng="en">earless <ind>seals</ind></trd>
  <trd lng="fr">phocid&eacute;s</trd>
  <trd lng="hu">f&oacute;kaf&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>robachtigen</trd>,
    <trd>zeehondachtigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>foki</trd>,
    <trd>fokowate</trd>
  </trdgrp>
  <trd lng="pt">foc&iacute;deos</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_t;&c_ju;&c_l;&c_je;&c_n;&c_i;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">tule&ncaron;ovit&eacute;</trd>
</drv>
</art>
