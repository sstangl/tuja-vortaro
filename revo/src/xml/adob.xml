<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: adob.xml,v 1.10 2015/07/04 08:37:29 revo Exp $">
    <kap>
      <rad>adob</rad>/o <fnt><bib>SPV</bib></fnt>
    </kap>

    <drv mrk="adob.0o">
      <kap><tld/>o</kap>
      <snc mrk="adob.0o.KON">
        <uzo tip="fak">KON</uzo>
        <dif>
          Nebakita <ref tip="super" cel="brik.0o.bakita">briko</ref>
          el argilo sekigita:
          <ekz>
            tiuj, kiuj lo&gcirc;as en kaverndomo, dormas sur terlito
            konstruita el
            <tld/>oj
            <fnt>
              <aut>&Ccirc;ina Radio Internacia</aut>,
              <vrk><url ref="http://es.chinabroadcast.cn">lo&gcirc;kutimoj de
                  &ccirc;inoj</url></vrk>,
              <lok>2005-12-20</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_c;&c_e;&c_g;&c_l;&c_a; <klr>(&c_n;&c_je;&c_a;&c_b;&c_p;&c_a;&c_l;&c_je;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;</trd>
  <trdgrp lng="cs">
    <trd>nepálená cihla</trd>,
    <trd>vepřovice</trd>
  </trdgrp>
  <trd lng="de">Lehmziegel</trd>
  <trd lng="en">adobe</trd>
  <trd lng="fr">adobe</trd>
  <trd lng="it">adobe<klr> (mattone)</klr></trd>
  <trdgrp lng="pl">
        <trd>ceg&lstroke;a surowa</trd>,
        <trd>ceg&lstroke;a suszona</trd>,
        <trd>ceg&lstroke;a adobe</trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>adobe</trd>,
        <trd>adobo</trd>,
        <trd>tijolo cru</trd>
      </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;-&c_s;&c_y;&c_r;&c_je;&c_c;</trd>
  <trd lng="sk">nepálená tehla</trd>
</drv>
  </art>

