  <trd lng="be">&c_l;&c_u;&c_b;&c_ib;&c_n;</trd>
  <trd lng="de">Lupine</trd>
  <trdgrp lng="en">
    <trd>lupin</trd>,
    <trd>lupine</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_r;&a_m;&a_s;</trd>
  <trd lng="fr">lupin</trd>
  <trdgrp lng="hu">
    <trd>csillagf&uuml;rt</trd>,
    <trd>farkasbab</trd></trdgrp>
  <trd lng="nl">lupine</trd>
  <trd lng="ru">&c_l;&c_ju;&c_p;&c_i;&c_n;</trd>
  <trd lng="sv">lupin</trd>
</drv>
