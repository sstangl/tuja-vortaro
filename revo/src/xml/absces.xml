<kap>
  <ofc>*</ofc>
  <rad>absces</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="absces.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="absces.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Amaso da puso, kolekti&gcirc;inta en kava&jcirc;o
      meze de &ccirc;irka&ubreve;antaj histoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_c;&c_e;&c_s;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_y;&c_w;</trd>
  </trdgrp>
  <trd lng="br">gor</trd>
  <trdgrp lng="cs">
    <trd>absces</trd>,
    <trd>bolák</trd>,
    <trd>hlíza</trd>,
    <trd>nežit</trd>,
    <trd>vřed</trd>
  </trdgrp>
  <trd lng="de">Abszess</trd>
  <trd lng="en">abcess</trd>
  <trd lng="es">absceso</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_s;&a_h;</trd>,
    <trd>&a_d;&a_m;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">abc&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;lyog</trd>,
    <trd>kel&eacute;s</trd>
  </trdgrp>
  <trd lng="it">ascesso</trd>
  <trdgrp lng="nl">
    <trd>abces</trd>,
    <trd>etterbuil</trd>,
    <trd>ettergezwel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ropie&nacute;</trd>,
    <trd>wrz&oacute;d</trd>
 </trdgrp>
  <trdgrp lng="pt">
    <trd>abscesso</trd>,
    <trd>apostema</trd>
  </trdgrp>
  <trd lng="ro">peptic</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_r;&c_y;&c_v;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_c;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ložisko hnisu</trd>,
    <trd>vred</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>b&ouml;ld</trd>,
    <trd>bulnad</trd>
  </trdgrp>
</drv>
</art>
