<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tamari1.xml,v 1.22 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>tamarik</rad>/o
</kap>
<drv mrk="tamari1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
    <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="tamari2.0oj"><tld/>acoj</ref>
      (<trd lng="la">Tamarix</trd>), arbeto kun malgrandaj folietoj kaj
      floretoj, kreskanta en la mediteraneaj regionoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_m;&c_a;&c_r;&c_y;&c_s;&c_k;</trd>
  <trd lng="ca">tamariu</trd>
  <trd lng="cs">tamaryšek</trd>
  <trd lng="de">Tamariske</trd>
  <trdgrp lng="fr">
    <trd>tamarix</trd>,
    <trd>tamaris</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tamariska</trd>,
    <trd>tamariszkusz</trd>
  </trdgrp>
  <trd lng="nl">tamarisk</trd>
  <trd lng="pl">tamaryszek</trd>
  <trdgrp lng="pt">
   <trd>tamargueira</trd>,
   <trd>tamariz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_k;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">tamariška</trd>
  <trd lng="sv">kl&aring;dris</trd>
</drv>
</art>
