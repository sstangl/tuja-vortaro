<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: meti.xml,v 1.24 2014/06/06 15:10:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>meti</rad>/o
</kap>
<drv mrk="meti.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Manlabora profesio:
      <ekz>&ccirc;iu klopodu nur en sia <tld/>o, tiam al la urbo mankos
        nenio
      <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bala.0i">balai</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="okup.0o">okupo</ref>,
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="specia">specialo</ref>,
      <ref cel="fak.0o">fako</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_ja;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="br">micher dorn</trd>
  <trd lng="cs">&rcaron;emeslo</trd>
  <trdgrp lng="de">
    <trd>Handwerk</trd>,
    <trd>Gewerbe</trd>
  </trdgrp>
  <trd lng="el">&chi;&epsilon;&jota;&rho;&omega;&ny;&alfa;&kappa;&tau;&jota;&kappa;&omikron_ton; &epsilon;&pi;&alfa_ton;&gamma;&gamma;&epsilon;&lambda;&my;&alfa;</trd>
  <trd lng="en">blue-collar <ind>profession</ind></trd>
  <trd lng="es">profesi&oacute;n manual</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;tier</trd>,
    <trd><ind>profession</ind> manuelle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mesters&eacute;g</trd>,
    <trd>k&eacute;zm&udblac;ves szakma</trd>,
    <trd>k&eacute;zm&udblac;ipar</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>lavoro da <ind>operaio</ind></trd>,
    <trd>lavoro manuale</trd>
  </trdgrp>
  <trd lng="nl">ambacht</trd>
  <trd lng="pl">rzemios&lstroke;o</trd>
  <trdgrp lng="pt">
   <trd>of&iacute;cio <klr>(profiss&atilde;o)</klr></trd>,
   <trd>mister <klr>(profiss&atilde;o)</klr></trd>,
   <trd>arte <klr>(profiss&atilde;o)</klr></trd>,
   <trd>ind&uacute;stria <klr>(profiss&atilde;o)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">remeslo</trd>
</drv>

<drv mrk="meti.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, &ccirc;ambro destinita por manlaboro.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_n;&c_ja;</trd>
  <trd lng="br">labouradeg</trd>
  <trd lng="de">Werkstatt</trd>
  <trdgrp lng="en">
    <trd>workplace</trd>,
    <trd>jobsite</trd>
  </trdgrp>
  <trd lng="fr">atelier</trd>
  <trd lng="hu">m&udblac;hely</trd>  
  <trd lng="it">luogo di lavoro</trd>
  <trd lng="nl">werkplaats</trd>
  <trd lng="pl">warsztat rzemie&sacute;lniczy</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  <trd lng="sk">diel&ncaron;a</trd>
</drv>

<drv mrk="meti.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Homo profesie manlaboranta.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="br">
    <trd>artizan</trd>,
    <trd>dornwezhour</trd>
  </trdgrp> 
  <trd lng="de">Handwerker</trd>
  <trd lng="en">blue-collar <ind>worker</ind></trd>
  <trd lng="fr">artisan</trd>
  <trdgrp lng="hu">
    <trd>mesterember</trd>,
    <trd>k&eacute;zm&udblac;ves</trd>,
    <trd>iparos</trd>
  </trdgrp>
  <trd lng="it">operaio</trd>
  <trd lng="nl">ambachtsman</trd>
  <trd lng="pl">rzemie&sacute;lnik</trd>
  <trd lng="ru">&c_r;&c_je;&c_m;&c_je;&c_s;&c_l;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">remeseln&iacute;k</trd>
</drv>

<drv mrk="meti.ne0ulo">
  <kap>
    ne<tld/>ulo
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="amator.0o">Amatoro</ref> 2.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_y;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Laie</trd>
  <trd lng="en">layman</trd>
  <trdgrp lng="fr">
    <trd>amateur</trd>,
    <trd>bricoleur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nem <ind>szakmabeli</ind></trd>,
    <trd>kont&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>profano</trd>,
    <trd>non <ind>specialista</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>amator</trd>,
    <trd>dyletant</trd>
  </trdgrp>    
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_l;&c_je;&c_t;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
