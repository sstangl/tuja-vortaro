<kap>
  <rad>halucinaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="halucinaci.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="haluci.0o">halucino</ref>
  <ekz>
    li eksentis komence kapdolorojn, kiuj ofte faris ian ajn laboron
    neebla, poste dum vesperoj aperis al li <tld/>oj
    <fnt>
    <aut>Ivan G. &Scirc;irjaev</aut>,
    <vrk>Sen titolo</vrk>,
    <lok>&ccirc;apitro 8a</lok>
    </fnt>.
  </ekz>
  <trd lng="be">
  &c_g;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Halluzination</trd>,
     <trd>Sinnest&auml;uschung</trd>,
     <trd>Wahrnehmungsst&ouml;rung</trd>,
     <trd>Wahnvorstellung</trd>,
     <trd>krankhafte <ind>Einbildung</ind></trd>
  </trdgrp>
  <trd lng="fr">hallucination</trd>
  <trdgrp lng="hu">
     <trd>hallucin&aacute;ci&oacute;</trd>,
     <trd>&eacute;rz&eacute;kcsal&oacute;d&aacute;s</trd>,
     <trd>r&eacute;ml&aacute;t&aacute;s</trd>,
     <trd>beteges <ind>k&eacute;pzel&odblac;d&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="it">allucinazione</trd>
  <trd lng="nl">hallucinatie</trd>
  <trd lng="pl">halucynacje</trd>
  <trd lng="pt">alucina&ccedil;&atilde;o</trd>
  <trd lng="ru">
  &c_g;&c_a;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>halucinácia</trd>,
    <trd>prelud</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>halucinace</trd>,
    <trd>přelud</trd>
  </trdgrp>
</drv>
</art>

