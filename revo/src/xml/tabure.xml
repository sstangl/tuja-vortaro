<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tabure.xml,v 1.22 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>taburet</rad>/o
</kap>
<drv mrk="tabure.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eta <ref tip="super" cel="segx.0o.meblo">se&gcirc;o</ref> sen
      dorsapogilo:
      <ekz>
        avino ordonis, ke oni alportu <tld/>ojn por la oldulinoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 43a, volumo 2a, p.
        28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_u;&c_r;&c_e;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>escambell</trd>,
    <trd>tamboret</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>stolička bez opěradla</trd>,
    <trd>taburet</trd>
  </trdgrp>
  <trd lng="de">Hocker</trd>
  <trd lng="en">stool</trd>
  <trd lng="fa">&f_ch;&a_h;&a_A;&a_r;&f_p;&a_A;&f_y;&a_h;</trd>
  <trd lng="fr">tabouret</trd>
  <trdgrp lng="hu">
    <trd>zs&aacute;moly</trd>,
    <trd>&uuml;l&odblac;ke</trd>,
    <trd>s&aacute;mli</trd>
  </trdgrp>
  <trd lng="nl">krukje</trd>
  <trd lng="pl">taboret</trd>
  <trdgrp lng="pt">
   <trd>tamborete</trd>,
   <trd>mocho</trd>,
   <trd>banco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">taburetka</trd>
  <trd lng="sv">pall<klr> (att sitta p&aring;)</klr></trd>
</drv>
</art>
