<vortaro>
<art mrk="$Id: laring1.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>laringit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="laring1.0o">
  <kap><tld/>o</kap>
  <snc mrk="laring1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la laringo:
      <ekz>
        <tld/>o akuta, kronika.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="inflam.gorgx0o">gor&gcirc;inflamo</ref>
  </refgrp>
  <trd lng="be">&c_l;&c_ja;&c_r;&c_y;&c_n;&c_g;&c_ib;&c_t;</trd>
  <trd lng="ca">laringitis</trd>
  <trdgrp lng="de">
    <trd>Kehlkopfentz&uuml;ndung</trd>,
    <trd>Laryngitis<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&rho;&ypsilon;&gamma;&gamma;&jota_ton;&tau;&jota;&delta;&alfa;</trd>
  <trd lng="en">laryngitis</trd>
  <trd lng="es">laringitis</trd>
  <trdgrp lng="fr">
    <trd>laryngite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>laryngitis</trd>,
    <trd>g&eacute;gegyullad&aacute;s</trd>
  </trdgrp>
  <trd lng="id">laringitis</trd>
  <trd lng="nl">keelontsteking</trd>
  <trd lng="pt">laringite</trd>
  <trd lng="ru">&c_l;&c_a;&c_r;&c_i;&c_n;&c_g;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>laryngitída</trd>,
    <trd>zápal hrtanu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laryngitida</trd>,
    <trd>zánět hrtanu</trd>
  </trdgrp>
</drv>
</art>
