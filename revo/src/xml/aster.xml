  <trd lng="de">Aster</trd>
  <trd lng="en">aster</trd>
  <trdgrp lng="fr">
    <trd>aster</trd>,
    <trd>vendangeuse<!-- PLI --></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&odblac;szir&oacute;zsa</trd>,
    <trd>gerebcsin</trd>,
    <trd>aszter</trd>
  </trdgrp>
  <trd lng="it">astro<klr> (bot.)</klr></trd>
  <trd lng="pl">aster</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>astra</trd>,
    <trd>hviezdica</trd>
  </trdgrp>
  <trd lng="sv">aster</trd>
</drv>
