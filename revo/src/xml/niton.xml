<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: niton.xml,v 1.14 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>niton</rad>/o
</kap>

<drv mrk="niton.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      Malnova nomo de la &hcirc;emia elemento <ref tip="sin"
      cel="radon.0o">radono</ref>, tiama simbolo Nt.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">niton</trd>
  <trd lng="de">Niton</trd>
  <trd lng="fr">niton</trd>
  <trd lng="hu">niton</trd>
  <trd lng="nl">radon</trd>
  <trd lng="pl">niton</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">niton <klr>(Nt)</klr></trd>
</drv>
</art>
