<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bosk.xml,v 1.13 2015/07/04 08:37:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bosk</rad>/o
</kap>
<drv mrk="bosk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita arbareto en parko:
      <ekz>
        el figa <tld/>o elglitis homo en nigra mantelo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">arbreda</trd>
  <trdgrp lng="cs">
    <trd>háj</trd>,
    <trd>lesík</trd>,
    <trd>parkový háj</trd>
  </trdgrp>
  <trd lng="de">Boskett</trd>
  <trdgrp lng="en">
    <trd>grove</trd>,
    <trd>copse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bosquet</trd>,
    <trd>bois <klr>(dans un parc)</klr></trd>,
    <trd>massif <klr>(d'arbres)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gaj</trd>,
    <trd>gaik</trd>,
    <trd>zagajnik</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_shch;&c_a; <klr>(&c_v;
  &c_p;&c_a;&c_r;&c_k;&c_je;)</klr></trd>
  <trd lng="sk">háj</trd>
</drv>
</art>
