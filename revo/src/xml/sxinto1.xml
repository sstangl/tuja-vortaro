<vortaro>
<art mrk="$Id: sxinto1.xml,v 1.9 2013/04/10 09:10:13 revo Exp $">
<kap>
  <rad>&scirc;into</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sxinto1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxinto1.0o.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="sxinto.0o">&scirc;intoismo</ref>
  </snc>
  <trd lng="de">Schintoismus</trd>
  <trd lng="en">Shintoism</trd>
  <trd lng="es">sinto&iacute;smo</trd>
  <trdgrp lng="fr">
    <trd>shint&ocirc;</trd>,
    <trd>shinto&iuml;sme</trd>
  </trdgrp>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_t;&c_a;&c_ib;&c_z;&c_m;</trd>
  <trd lng="hu">sinto</trd>
  <trdgrp lng="it">
    <trd>shinto</trd>,
    <trd>shintoismo</trd>,
    <trd>scintoismo</trd>
  </trdgrp>
  <trd lng="nl">sjinto&iuml;sme</trd>
  <trdgrp lng="pl">
    <trd>szintoism</trd>,
    <trd>sintoizm</trd>,
    <trd>shintoizm</trd>
  </trdgrp>
  <trd lng="pt">xintoismo</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_t;&c_o;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
