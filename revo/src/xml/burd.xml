<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: burd.xml,v 1.22 2015/10/07 11:10:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>burd</rad>/o
</kap>

<drv mrk="burd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="burd.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="abeled.0oj">abeledoj</ref>
      (<trd lng="la">Bombus</trd>), pli peza ol abelo:
      <ekz>
        grafoj kaj princoj tie sin pu&scirc;as kaj zumas, kiel <tld/>oj,
        oni nur a&ubreve;das &leftquot;&jcirc; ... &jcirc; ... &jcirc;
        ...&rightquot; 
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 6a</lok></fnt>;
      </ekz>
      <ekz>  
        kelkaj <tld/>oj, kviete gustumintaj flornektaron, levi&gcirc;as
        super la bedo
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 14a, p.
        80a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_m;&c_je;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_je;&c_m;&c_n;&c_a;
  &c_p;&c_ch;&c_je;&c_l;&c_a;</trd>
  <trd lng="cs">&ccaron;mel&aacute;k</trd>
  <trd lng="de">Hummel</trd>
  <trd lng="en">bumblebee</trd>
  <trd lng="es">abejorro</trd>
  <trd lng="fr">bourdon<klr tip="ind"> (insecte)</klr></trd>
  <trd lng="hu">poszm&eacute;h</trd>
  <trd lng="nl">hommel</trd>
  <trd lng="pl">trzmiel</trd>
  <trd lng="pt">zang&atilde;o</trd>
  <trd lng="ru">&c_sh;&c_m;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">&ccaron;meliak</trd>
  <trd lng="sv">humla</trd>
</drv>
</art>
