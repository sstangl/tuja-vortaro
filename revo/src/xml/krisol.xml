<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krisol.xml,v 1.14 2010/05/02 16:31:03 revo Exp $">
<kap>
  <rad>krisol</rad>/o
</kap>
<drv mrk="krisol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Vazo el fajrorezista tero, en kiu oni fandas mineralojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="vaz.fand0o">fandvazo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">gresol</trd>
  <trd lng="de">Schmelztiegel</trd>
  <trd lng="en">crucible</trd>
  <trd lng="es">crisol</trd>
  <trd lng="fa"><klr tip="ind"> (&a_Z;&a_r;&a_f; &a_n;&a_s;&a_w;&a_z;)</klr>&a_b;&a_w;&a_t;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>creuset</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;nt&odblac;&uuml;st</trd>,
    <trd>olvaszt&oacute;t&eacute;gely</trd>
  </trdgrp>
  <trd lng="nl">smeltkroes</trd>
  <trd lng="pl">tygiel</trd>
  <trdgrp lng="pt">
    <trd>crisol</trd>,
    <trd>cadinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>
