<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: precip.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>precip</rad>/e
    </kap>

    <drv mrk="precip.0e">
      <kap><ofc>*</ofc><tld/>e</kap>
      <snc>
        <dif>
          Adverbo, signifanta &leftquot;anta&ubreve; &ccirc;io, &ccirc;efe, pli ol &ccirc;io a&ubreve;
          &ccirc;iu alia&rightquot;:
          <ekz>
            sur tiuj &ccirc;i kampoj pa&scirc;tas sin grandaj brutaroj, <tld/>e
            aroj da
            bellanaj &scirc;afoj<fnt>Z</fnt>;
          </ekz><ekz>
            la kuraciston maltrankviligis <tld/>e la febro<fnt>B</fnt>;
          </ekz><ekz>
            internacian lingvon bezonas <tld/>e komercistoj kaj
            scienculoj<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>,
        <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y;&c_m; &c_ch;&c_y;&c_n;&c_a;&c_m;</trd>
      </trdgrp>
  <trd lng="ca">principalment</trd>
  <trdgrp lng="cs">
    <trd>hlavně</trd>,
    <trd>jmenovitě</trd>,
    <trd>obzvláště</trd>,
    <trd>především</trd>,
    <trd>zejména</trd>,
    <trd>zvl&aacute;&scaron;t&ecaron;</trd>,
    <trd>zvláště</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>besonders</trd>,
        <trd>insbesondere</trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>chiefly</trd>,
        <trd>mainly</trd>,
        <trd>principally</trd>
      </trdgrp>
  <trd lng="es">principalmente</trd>
  <trdgrp lng="fr">
        <trd>avant <ind>tout</ind></trd>,
        <trd>essentiellement</trd>,
        <trd>principalement</trd>,
        <trd>particuli&egrave;rement</trd>,
        <trd>en <ind>particulier</ind></trd>,
        <trd>surtout</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>f&odblac;leg</trd>,
        <trd>k&uuml;l&ouml;n&ouml;sen<klr tip="ind"> (f&odblac;leg)</klr></trd>
      </trdgrp>
  <trd lng="nl">vooral</trd>
  <trdgrp lng="pt">
    <trd>principalmente</trd>,
    <trd>mormente</trd>,
    <trd>em <ind>especial</ind></trd>,
    <trd>sobretudo</trd>        
  </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_v; &c_o;&c_s;&c_n;&c_o;&c_v;&c_n;&c_o;&c_m;</trd>,
        <trd>&c_o;&c_s;&c_o;&c_b;&c_je;&c_n;&c_n;&c_o;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;</trd>,
        <trd>&c_g;&c_l;&c_a;&c_v;&c_n;&c_y;&c_m; &c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_m;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>najmä</trd>,
    <trd>obzvlášť</trd>,
    <trd>predovšetkým</trd>
  </trdgrp>
</drv>

    <drv mrk="precip.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Plej grava; &ccirc;efa:
          <ekz>
            la <tld/>a rolo, motivo;
          </ekz><ekz>
            la <tld/>aj punktoj de parolado.
          </ekz>
        </dif>
      </snc>
      <rim>
        Pri sinonimoj vd sub <ref cel="specia.0a.SIN">Speciala</ref>.
      </rim>
      <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>,
        <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y;</trd>,
        <trd>&c_n;&c_a;&c_j;&c_v;&c_a;&c_zh;&c_n;&c_je;&c_j;&c_sh;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>principal</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>besondere</trd>,
        <trd>haupts&auml;chlich</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>chief</trd>,
        <trd>main</trd>,
        <trd>principal</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>principal</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>essentiel</trd>,
        <trd>principal</trd>
      </trdgrp>
      <trd lng="hu">f&odblac;</trd>
      <trd lng="nl">voornaamste</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_a;&c_zh;&c_n;&c_je;&c_j;&c_sh;&c_i;&c_j;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_v;&c_a;&c_l;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
        <trd>&c_g;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trd lng="sk">hlavný</trd>
  <trdgrp lng="cs">
    <trd>hlavní</trd>,
    <trd>přední</trd>
  </trdgrp>
</drv>
  </art>
