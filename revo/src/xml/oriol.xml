<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oriol.xml,v 1.16 2015/07/04 08:37:40 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>oriol</rad>/o
</kap>
<drv mrk="oriol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="oriole.0oj"><tld/>edoj</ref>
      (<trd lng="la">Oriolus</trd>), kun flava nigra plumaro kaj kun
      agrabla   
      kanto.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_v;&c_a;&c_l;&c_g;&c_a;</trd>
  <trd lng="ca">oriol</trd>
  <trdgrp lng="cs">
    <trd>&zcaron;luva</trd>,
    <trd>žluva hajní</trd>,
    <trd>žluva obecná</trd>
  </trdgrp>
  <trd lng="de">Pirol</trd>
  <trd lng="en">oriole</trd>
  <trd lng="fr">loriot</trd>
  <trd lng="hu">s&aacute;rgarig&oacute;</trd>
  <trd lng="nl">wielewaal</trd>
  <trd lng="pl">wilga</trd>
  <trd lng="ru">&c_i;&c_v;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="sk">žlna</trd>
</drv>

</art>
