<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vahabism.xml,v 1.4 2014/06/03 10:10:45 revo Exp $">
<kap>
  <rad>Vahabism</rad>/o
</kap>
<drv mrk="vahabism.0o">
  <kap><tld/>o</kap>
  <snc mrk="vahabism.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      <ref tip="super" cel="salafism.0o.ISL">Salafisma</ref>
      movado nomata la&ubreve; predikinto de la 18a jarcento,
      kiu disvastigis &gcirc;in precipe en Sauda Arabujo:
      <ekz>
        Sa&ubreve;darabiaj pintaj klerikoj <klr>...</klr> samkiel la
        re&gcirc;a familio estas anoj de la malcedema <tld/>a tendenco
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.eventeo.net/web/index.php/mezoriento/socio/618-regxo-de-arabio-parolas-pri-toleremo">
          Re&gcirc;o de Arabio...</url></vrk>,
          <lok>eventeo, 2008-06-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        teatroj kaj kinejoj estas malpermesitaj, &ccirc;ar la
        tradicio de <tld/>o konsideras tiujn instituciojn
        nekongruajn kun Islamo  
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Sauda_Arabio">
          Sauda Arabio</url>, 2011-06-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_h;&c_a;&c_b;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">wahhabisme</trd>
  <trd lng="fr">Wahhabisme</trd>
  <trd lng="id">Wahabi</trd>
</drv>

<drv mrk="vahabism.0ano">
  <kap><tld lit="v"/>ano</kap>
  <snc mrk="vahabism.0ano.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Ano de
      <ref tip="vid" cel="vahabism.0o.ISL"><tld/>o</ref>;
      <ref tip="sin" cel="vahabit.0o.ISL">vahabito</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_h;&c_a;&c_b;&c_ib;&c_t;</trd>
  <trd lng="ca">wahhabita</trd>
  <trd lng="fr">wahhabite<klr> (subst.)</klr></trd>
</drv>
</art>

