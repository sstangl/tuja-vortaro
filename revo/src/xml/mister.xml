<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mister.xml,v 1.18 2015/10/13 11:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mister</rad>/o
</kap>
<drv mrk="mister.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sekreta doktrino kaj kulto &ccirc;e idolanoj.
    </dif>
    <trdgrp lng="en">
      <trd>mystery</trd>,
      <trd>mystic doctrine</trd>
    </trdgrp>
    <trd lng="hu">miszt&eacute;rium</trd>
    <trd lng="pl">misterium</trd>
  </snc>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      Dogmo neesplorebla kaj nekomprenebla de la homa intelekto, sed
      senpere malka&scirc;ita de Dio.
    </dif>
    <trdgrp lng="en">
      <trd>mystery</trd>,
      <trd>mysticism</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hittitok</trd>,
      <trd>miszt&eacute;rium</trd></trdgrp>
    <trd lng="pl">misterium</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fakto, afero, kiu ne povas esti klarigata kaj kies ka&ubreve;zoj restas
      nekonataj:
      <ekz>
        la <tld/>oj de la naturo, de nekonata lando, de la politiko
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="enigm.0o">enigmo</ref>.
    </refgrp>
    <trd lng="en">mystery</trd>
    <trd lng="hu">rejt&eacute;ly</trd>
    <trdgrp lng="pl">
      <trd>tajemnica</trd>,
      <trd>zagadka</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_j;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_ja;&c_m;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">misteri</trd>
  <trdgrp lng="cs">
    <trd>myst&eacute;rie</trd>,
    <trd>myst&eacute;rium</trd>
  </trdgrp>
  <trd lng="de">Mysterium</trd>
  <trd lng="fr">myst&egrave;re</trd>
  <trd lng="it">mistero</trd>
  <trd lng="nl">mysterie</trd>
  <trd lng="pt">mist&eacute;rio</trd>
  <trd lng="ru">&c_t;&c_a;&c_j;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>myst&eacute;rium</trd>,
    <trd>taj</trd>,
    <trd>tajomstvo</trd>,
    <trd>z&aacute;hada</trd>
  </trdgrp>
</drv>

<drv mrk="mister.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Enhavanta ion ka&scirc;itan, sekretan, malklaran:
      <ekz>
        <tld/>aj signoj;
      </ekz>
      <ekz>
        <tld/>aj por la amaso teknikaj terminoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a krimo;
      </ekz>
      <ekz>
        <tld/>a ordono, sciigo;
      </ekz>
      <ekz>
        fine la <tld/>a pordo malfermi&gcirc;is;
      </ekz>
      <ekz>
        la senlima <tld/>a maro;
      </ekz>
      <ekz>
        <tld/>a forto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_ja;&c_m;&c_n;&c_ib;&c_ch;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">misteri&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>mysteri&oacute;zn&iacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>,
    <trd>tajemn&yacute;</trd>,
    <trd>tajupln&yacute;</trd>,
    <trd>z&aacute;hadn&yacute;</trd>
  </trdgrp>
  <trd lng="de">mysteri&ouml;s</trd>
  <trd lng="fr">myst&eacute;rieux</trd>
  <trdgrp lng="hu">
    <trd>rejt&eacute;lyes</trd>,
    <trd>misztikus</trd>
  </trdgrp>
  <trd lng="it">misterioso</trd>
  <trdgrp lng="nl">
    <trd>mysterieus</trd>,
    <trd>raadselachtig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tajemniczy</trd>,
    <trd>zagadkowy</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>tajomn&yacute;</trd>,
    <trd>z&aacute;hadn&yacute;</trd>
  </trdgrp>
</drv>
</art>
