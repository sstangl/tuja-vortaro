<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: etap.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap>
<ofc>8</ofc>
<rad>etap</rad>/o
</kap>
<drv mrk="etap.0o">
<kap><tld/>o</kap>

 <snc>
  <dif>
   <refgrp tip="dif">
     <ref cel="stadi.0o.haltejo">Stadio (haltejo).</ref>
   </refgrp>
  </dif>
  <trdgrp lng="de">
     <trd>Rastplatz</trd>,
     <trd>Etappenort</trd>,
     <trd>Etappenziel</trd>,
     <trd>Etappe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>halting place</trd>,
    <trd>stopover</trd>
  </trdgrp>
  <trd lng="hu">pihen&odblac;hely</trd>
  <trd lng="pl">etap</trd>
  <trd lng="pt">etapa</trd>
  <trd lng="ro">etap&abreve;</trd>
 </snc>

 <snc>
  <dif>
   <refgrp tip="dif">
     <ref cel="stadi.0o.interhalteja_distanco">Stadio
       (interhalteja distanco)</ref>
   </refgrp>:
   <ekz>la regimento faris hodia&ubreve; longan <tld/>on.</ekz>
  </dif>
  <trd lng="de">Tagesmarsch</trd>
  <trdgrp lng="en">
    <trd>distance covered</trd>,
    <trd>lap</trd>,
    <trd>length covered</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>menetszakasz</trd>,
    <trd>&uacute;tszakasz</trd>,
    <trd>etap</trd>
  </trdgrp>
  <trd lng="pl">etap</trd>
  <trd lng="pt">etapa</trd>
  <trd lng="ro">etap&abreve;</trd>
 </snc>

  <snc>
    <dif>
     <refgrp tip="dif">
       <ref cel="stadi.0o.fazo">Stadio (fazo)</ref>.
     </refgrp>
    </dif>
    <trd lng="de">Etappe</trd>
    <trdgrp lng="en">
      <trd>stage</trd>,
      <trd>period</trd>,
      <trd>phase</trd>
    </trdgrp>
    <trd lng="hu">szakasz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_t;&c_a;&c_p;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_d;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_je;&c_t;&c_a;&c_p;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_d;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&ecaron;h</trd>,
    <trd>běh</trd>,
    <trd>etapa</trd>,
    <trd>fáze</trd>,
    <trd>kolo závodu</trd>,
    <trd>stádium</trd>,
    <trd>turnus</trd>,
    <trd>úsek cesty</trd>,
    <trd>část závodu</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tape</trd>
  <trdgrp lng="nl">
    <trd>stadium</trd>,
    <trd>etappe</trd>
  </trdgrp>
  <trd lng="pl">etap</trd>
  <trd lng="pt">etapa</trd>
  <trd lng="ro">etap&abreve;</trd>
  <trdgrp lng="ru">
  <trd>&c_e;&c_t;&c_a;&c_p;</trd>,
  <trd>&c_s;&c_t;&c_a;&c_d;&c_i;&c_ja;</trd>
</trdgrp>
  <trdgrp lng="sk">
    <trd>fáza</trd>,
    <trd>úsek</trd>
  </trdgrp>
</drv>
</art>
