<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: viand.xml,v 1.30 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>viand</rad>/o
    </kap>

    <drv mrk="viand.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Muskola parto de animala <ref cel="karn.0o">karno</ref>,
          do escepte de ha&ubreve;to, graso, <ref cel="trip.0o">tripo</ref>;
          kaj anka&ubreve; escepte de la karno de fi&scirc;oj, moluskoj kaj aliaj
          senvertebruloj, rigardata kiel nutra&jcirc;o:
          <ekz>
            la &scirc;afid<tld/>o estas tre delikata;
          </ekz><ekz>
            <klr>(pilafo)</klr> estas preparata el kokina <tld/>o kaj
            rizo
            <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
          </ekz><ekz>
            la tran&ccirc;ilo estis tiel malakra, ke mi ne povis
            tran&ccirc;i per &gcirc;i la <tld/>on
            <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            &gcirc;i estas <ind>nek <tld/>o, nek fi&scirc;o</ind>
            <klr>(= &gcirc;i ne havas difinitan karakteron, ne estas
              tiel a&ubreve; alie klasebla)</klr>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="ca"><klr>[no &eacute;sser]</klr> ni carn ni peix</trd>
            <trd lng="hu">se hideg, se meleg</trd>
            <trd lng="pl">ni pies, ni wydra</trd>
            <trd lng="ru">&c_n;&c_i; &c_r;&c_y;&c_b;&c_a;, &c_n;&c_i; &c_m;&c_ja;&c_s;&c_o;</trd>
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            <ind>li havis <tld/>on</ind>, mi havis nur oston
            &#8208; li havis la &gcirc;uon, mi pagis la koston
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="ru">&c_v; &c_ch;&c_u;&c_zh;&c_o;&c_m; &c_p;&c_i;&c_r;&c_u;
              &c_p;&c_o;&c_h;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_m;&c_ja;&c_s;&c_a;</trd>
  <trd lng="bg">&c_m;&c_je;&c_s;&c_o;</trd>
  <trd lng="br">kig</trd>
  <trd lng="ca">carn</trd>
  <trd lng="cs">maso</trd>
  <trd lng="de">Fleisch</trd>
  <trd lng="en">meat</trd>
  <trd lng="es">carne</trd>
  <trdgrp lng="fr">
        <trd>viande</trd>
      </trdgrp>
  <trd lng="hu">h&uacute;s</trd>
  <trd lng="id">daging</trd>
  <trd lng="nl">vlees<klr tip="ind"> (om te eten)</klr></trd>
  <trd lng="oc">carn</trd>
  <trd lng="pl">mi&eogonek;so</trd>
  <trd lng="pt">carne</trd>
  <trd lng="ru">&c_m;&c_ja;&c_s;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>m&auml;so</trd>,
    <trd>mäso</trd>
  </trdgrp>
  <trd lng="sv">k&ouml;tt</trd>
</drv>

    <drv mrk="viand.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          <tld lit="V"/>ovendejo.
        </dif>
      </snc>
  <trd lng="ca">carnisseria</trd>
  <trdgrp lng="cs">
    <trd>prodejna masa</trd>,
    <trd>řeznictví</trd>
  </trdgrp>
  <trd lng="hu">h&uacute;sbolt</trd>
  <trdgrp lng="pl">
        <trd>sklep <ind>mi&eogonek;sny</ind></trd>,
        <trd><klr tip="amb">sklep </klr><ind>mi&eogonek;sny</ind><klr>
            (pot.)</klr></trd>,
        <trd>masarnia</trd>
      </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_a;&c_ja;
        &c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">mäsiarstvo</trd>
</drv>

    <drv mrk="viand.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          <tld lit="V"/>ovendisto.
        </dif>
      </snc>
  <trd lng="ca">carnisser</trd>
  <trdgrp lng="cs">
    <trd>prodavač masa</trd>,
    <trd>řezník</trd>
  </trdgrp>
  <trd lng="hu">hentes</trd>
  <trdgrp lng="pl">
        <trd>masarz</trd>,
        <trd>sprzedawca w<klr> sklepie </klr>mi&eogonek;snym</trd>
      </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_s;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">mäsiar</trd>
</drv>

    <drv mrk="viand.0omangxanto">
      <kap><tld/>oman&gcirc;anto</kap>
      <snc>
        <dif>
          Persono uzanta <tld/>on en sia nutrado:
          <ekz>
            la <ref tip="ant" cel="vegeta2.0ano">vegetarano</ref>
            &gcirc;uas pli bonan sanon, ne estas tiel forte
            elmetita al malsanoj a&ubreve; elportas tiujn &ccirc;i multe pli
            bone, ol la <tld/>oman&gcirc;anto
	    <fnt>
	      <vrk>Kio estas vegetarismo?</vrk>. <bib>FK</bib>
	    </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">carn&iacute;vor<klr> (en oposici&oacute; a vegetari&agrave;)</klr></trd>
      <trd lng="hu">h&uacute;sev&odblac;</trd>
      <trd lng="ru">&c_m;&c_ja;&c_s;&c_o;&c_je;&c_d;</trd>
    </drv>

  </art>
