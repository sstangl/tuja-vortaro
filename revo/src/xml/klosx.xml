<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klosx.xml,v 1.13 2007/07/22 16:30:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klo&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klosx.0o.formo">
    <dif>
      Objekto kun duongloba supro kaj pli lar&gcirc;a malsupro, ordinare kava:
      <ekz>
        la jasmenaj floroj malfermis siajn blankajn <tld/>ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <subsnc mrk="klosx.0o.ujo">
      <dif>
        Ujo sonorilforma, &gcirc;enerale vitra, por kovri kaj &scirc;irmi
        delikatan objekton: vegeta&jcirc;on, man&gcirc;a&jcirc;on, 
        horlo&gcirc;on...:
        <ekz>
          &scirc;i bone atentis, ke buteroj kaj froma&gcirc;oj trovi&gcirc;u sub siaj
          respektivaj <tld/>oj
          <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="en">
        <trd>cloche</trd>,
        <trd>bell jar</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="klosx.0o.MUZ">
      <fnt><bib>MuzTerm</bib></fnt>
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Pli lar&gcirc;a, sonoron ellasanta ekstremo de pluraj
        <ref tip="malprt" cel="instru2.blov0o">blovinstrumentoj</ref>.
      </dif>
    </subsnc>
  </snc>
  <trd lng="de">Glocke</trd>
  <trdgrp lng="fr">
    <trd>cloche<klr tip="ind"> (couvercle)</klr></trd>,
    <trd>clochette<klr tip="ind"> (fleur)</klr></trd>,
    <trd>pavillon<klr tip="ind"> (d'instrument de musique)</klr></trd>
  </trdgrp>
  <trd lng="nl">stolp</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;</trd>
  </trdgrp>
</drv>
</art>
