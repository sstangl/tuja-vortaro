<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxore.xml,v 1.17 2015/04/21 02:10:15 revo Exp $">
<kap>
      <rad>&hcirc;ore</rad>/o<fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hxore.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Nerva <ref tip="lst" cel="san.mal0o.MED"
       lst="voko:malsanoj">malsano</ref>, karakterizata per sen&ccirc;esaj
       konvulsiaj senvolaj movoj:
       <ekz>
         elektra, falsa, fibretara, hereda, histeria, paraliza <tld/>o.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="ca">
     <trd>corea</trd>,
     <trd>ball de Sant Vito</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Chorea</trd>,
     <trd>Veitstanz</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>(Sydenham's) <ind>chorea</ind></trd>,
     <trd>St. Vitus' dance</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>corea</trd>,
     <trd>baile de San Vito</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>tanssitauti</trd>,
     <trd>chorea</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>chor&eacute;e</trd>,
     <trd>danse de Saint-Guy</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>chorea</trd>,
     <trd>vitust&aacute;nc</trd>
   </trdgrp>
   <trd lng="nl">St.-Vitusdans</trd>
   <trdgrp lng="pl">
     <trd>pl&aogonek;sawica</trd>,
     <trd>chorea</trd>,
     <trd>choroba &sacute;w. Wita<klr> (przest.)</klr></trd>
   </trdgrp>
   <trd lng="pt">cor&eacute;ia<klr tip="amb"> (medicina)</klr></trd>
   <trdgrp lng="ru">
     <trd>&c_h;&c_o;&c_r;&c_je;&c_ja;</trd>,
     <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a; &c_s;&c_v;&c_ja;&c_t;&c_o;&c_g;&c_o; &c_V;&c_i;&c_t;&c_t;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>corea<klr> (patologia)</klr></trd>,
     <trd>ballo di San Vito</trd>
   </trdgrp>
  <trd lng="la">chorea</trd>
</drv>
</art>
