<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kauxte.xml,v 1.16 2013/08/08 15:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ka&ubreve;teriz</rad>/i
</kap>
<drv mrk="kauxte.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Apliki fajron a&ubreve; ka&ubreve;stika&jcirc;on sur histon por kuraca celo (koaguli
      sangon en vundo, seninfektigi vundon ktp):
      <ekz>
        mi devas ... <tld/>i malgrandan vundon
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 27a, p.
        194a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_b;&c_g;&c_a;&c_r;&c_ja;&c_m;</trd>
  <trd lng="ca">cauteritzar</trd>
  <trd lng="de">kauterisieren</trd>
  <trd lng="en">cauterize</trd>
  <trd lng="es">cauterizar</trd>
  <trdgrp lng="fr">
    <trd>caut&eacute;riser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&eacute;get</trd>,
    <trd>kauteriz&aacute;l</trd>,
    <trd>kauterez</trd>,
    <trd>kauteroz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cauteriseren</trd>,
    <trd>uitbranden</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_zh;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kauxte.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Me&hcirc;anika rimedo por brulkuraci karnon.
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t; &c_z;&c_a; <ind>&c_o;&c_b;&c_g;&c_a;&c_r;&c_ja;&c_n;&c_je;</ind></trd>
  <trd lng="ca">cauteri</trd>
  <trd lng="de">Kauter</trd>
  <trd lng="en">cautery</trd>
  <trd lng="es">cauterio</trd>
  <trdgrp lng="fr">
    <trd>caut&egrave;re<klr tip="ind"> (instrument)</klr></trd>
  </trdgrp>
  <trd lng="hu">kauter</trd>
  <trd lng="nl">brandijzer</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_u;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kauxte.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      &Hcirc;emia rimedo por sama celo.
    </dif>
  </snc>
  <trd lng="de">Kauterisierungsmittel</trd>
  <trd lng="en">cautery</trd>
  <trdgrp lng="fr">
    <trd>caut&egrave;re<klr tip="ind"> (substance)</klr></trd>
  </trdgrp>
  <trd lng="hu">kauteriz&aacute;l&oacute;szer</trd>
  <trd lng="nl">cauteriserende stof</trd>
  <trd lng="ru">&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o; &c_d;&c_l;&c_ja; <ind>&c_p;&c_r;&c_i;&c_zh;&c_i;&c_g;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
</drv>
</art>
