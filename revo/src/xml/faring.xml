<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: faring.xml,v 1.18 2015/07/04 08:37:34 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>faring</rad>/o
</kap>
<drv mrk="faring.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Kava parto inter la bu&scirc;o kaj la ezofaga tubo.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">faringe</trd>
  <trdgrp lng="cs">
    <trd>farynx</trd>,
    <trd>hltan</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schlundkopf</trd>,
     <trd>Rachen</trd>,
     <trd>Pharynx</trd></trdgrp>
  <trd lng="en">pharynx</trd>
  <trd lng="es">faringe</trd>
  <trd lng="fr">pharynx</trd>
  <trd lng="hu">garat</trd>
  <trdgrp lng="id">
    <trd>esofagus</trd>,
    <trd>kerongkongan</trd>
  </trdgrp>
  <trd lng="it">faringe</trd>
  <trdgrp lng="nl">
   <trd>keelholte</trd>,
   <trd>farynx</trd>
</trdgrp>
  <trd lng="pl">gardziel</trd>
  <trd lng="pt">faringe</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">hltan</trd>
</drv>
</art>
