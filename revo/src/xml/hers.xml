<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hers.xml,v 1.12 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hers</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hers.0o">
  <kap><tld/>o</kap>
  <snc mrk="hers.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Vertikale mallevebla plato a&ubreve; krado, per kiu oni povas bari
      pasejon, ekzemple pordon de <ref tip="malprt" cel="fortik.0ajxo">
      fortika&jcirc;o</ref>, <ref tip="malprt" cel="kluz.0o.MAR">kluzon
      </ref>:
      <ekz>
        kastelo kun profunda foso, turoj, <tld/>o kaj levoponto
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 71a, p.
        797a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klap.0ego">klapego</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>padací brána</trd>,
    <trd>padací mříž</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fallgatter</trd>,
    <trd>Fallgitter</trd>
  </trdgrp>
  <trd lng="es">rastrillo</trd>
  <trd lng="fr">herse</trd>
  <trdgrp lng="hu">
    <trd>zsilip</trd>,
    <trd>csap&oacute;r&aacute;cs</trd>,
    <trd>kapurost&eacute;ly</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>chiudenda<klr> (fortificazione)</klr></trd>,
    <trd>saracinesca<klr> (fortificazione)</klr></trd>
  </trdgrp>
  <trd lng="nl">valhek</trd>
  <trdgrp lng="pl">
    <trd>brona</trd>,
    <trd>krata<klr> forteczna</klr></trd>,
    <trd>brama zwodzona</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>grade</trd>,
    <trd>rastelo</trd>
  </trdgrp>
  <trd lng="ru">
  &c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_a;&c_ja;
  &c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;
  </trd>
  <trdgrp lng="sk">
    <trd>mreža</trd>,
    <trd>padacia brána</trd>
  </trdgrp>
</drv>
</art>
