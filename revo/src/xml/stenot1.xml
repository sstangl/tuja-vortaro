<vortaro>
<art mrk="$Id: stenot1.xml,v 1.8 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>stenotipi</rad>/o
</kap>
<drv mrk="stenot1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Arto stenotipi:
      <ekz>
        la <tld/>o akiras &ccirc;iam pli multajn adeptojn kaj konkuras forte kun
        la stenografio.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>psaní na těsnopisném stroji</trd>,
    <trd>stenotypie</trd>
  </trdgrp>
  <trd lng="hu">g&eacute;pi <ind>gyors&iacute;r&aacute;s</ind></trd>
  <trd lng="nl">stenotypie</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">písanie rýchlopisom</trd>
</drv>
</art>
