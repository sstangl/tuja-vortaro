<kap>
  <rad>referendum</rad>/o
</kap>
<drv mrk="refere1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Vo&ccirc;dono de tuta popolo en la afero de akcepto a&ubreve;
      malakcepto de iu le&gcirc;dona projekto:
      <ekz>
        paca interkonsento subskribita de la brita kaj la irlanda
        registaroj, vaste aprobita en <tld/>o de la tuta insulanaro
        <fnt>
          <aut>Garbhan MacAoidh</aut>:
          <vrk>Polico ne plu re&gcirc;a</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2002a, numero 1a, p. 11a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_f;&c_e;&c_r;&c_e;&c_n;&c_d;&c_u;&c_m;</trd>
  <trd lng="bg">&c_r;&c_je;&c_f;&c_je;&c_r;&c_je;&c_n;&c_d;&c_u;&c_m;</trd>
  <trd lng="ca">refer&egrave;ndum</trd>
  <trdgrp lng="cs">
    <trd>plebiscit</trd>,
    <trd>referendum</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Referendum</trd>,
    <trd>Volksabstimmung</trd>
  </trdgrp>
  <trd lng="en">referendum</trd>
  <trd lng="es">refer&eacute;ndum</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_h;&zwnj;&f_p;&a_r;&a_s;&f_y;</trd>,
    <trd>&a_r;&a_f;&a_r;&a_A;&a_n;&a_d;&a_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;f&eacute;rendum</trd>,
    <trd>votation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;pszavaz&aacute;s</trd>,
    <trd>referendum</trd>
  </trdgrp>
  <trd lng="nl">referendum</trd>
  <trdgrp lng="pt">
    <trd>referendo</trd>,
    <trd>referendum</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_f;&c_je;&c_r;&c_je;&c_n;&c_d;&c_u;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>celonárodné hlasovanie</trd>,
    <trd>referendum</trd>
  </trdgrp>
</drv>
