<vortaro>
<art mrk="$Id: decxif.xml,v 1.20 2012/07/15 08:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>de&ccirc;ifr</rad>/i
</kap>
<drv mrk="decxif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="cxifr.mal0i">mal&ccirc;ifri.</ref>
    <trd lng="bg">&c_d;&c_je;&c_sh;&c_i;&c_f;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>
    <trdgrp lng="de">
      <trd>entschl&uuml;sseln</trd>,
      <trd>dechiffrieren</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megfejt<klr tip="ind">
      (titkos&iacute;r&aacute;st)</klr></trd>,
      <trd>dek&oacute;dol</trd>,
      <trd>desifr&iacute;roz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>deszyfrowa&cacute;</trd>,
      <trd>dekodowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pene legi malbone skribitan tekston a&ubreve; malklare esprimitan
      penson:
      <ekz>
        <tld/>i papiruson;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur lia indiferenta viza&gcirc;o &scirc;i penis <tld/>i lian
        deziron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diven.0i">diveni</ref>,
      <ref cel="konjek.0i">konjekti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>entziffern</trd>
    </trdgrp>
    <trd lng="hu">kisilabiz&aacute;l</trd>
    <trdgrp lng="pl">
      <trd>odszyfrowa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_f;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">desxifrar</trd>
  <trd lng="en">decipher</trd>
  <trd lng="es">descifrar</trd>
  <trd lng="fr">d&eacute;chiffrer</trd>
  <trd lng="it">decifrare</trd>
  <trd lng="nl">ontcijferen</trd>
  <trd lng="pt">decifrar</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_f;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">l&uacute;&scaron;ti&tcaron;</trd>
</drv>
</art>
