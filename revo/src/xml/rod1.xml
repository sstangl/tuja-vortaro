<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rod1.xml,v 1.18 2015/07/04 08:37:42 revo Exp $">
<kap>
   <rad>rod</rad>/i
</kap>
<drv mrk="rod1.0i">
   <kap><tld/>i</kap>
   <uzo tip="fak">ZOO</uzo>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="rongx.0i">ron&gcirc;i</ref>
     <ekz>
       la ratoj <tld/>as meblojn;
     </ekz>
     <ekz>
       <uzo tip="stl">FIG</uzo>
       la ven&gcirc;o <tld/>is en lia koro.
     </ekz>
   </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="ca">rosegar</trd>
  <trdgrp lng="cs">
    <trd>hlodat</trd>,
    <trd>hryzat</trd>
  </trdgrp>
  <trd lng="de">nagen</trd>
  <trdgrp lng="en">
     <trd>gnaw</trd>,
     <trd>nibble</trd>
   </trdgrp>
  <trd lng="fr">ronger</trd>
  <trd lng="hu">r&aacute;g</trd>
  <trd lng="nl">knagen</trd>
  <trd lng="ru">&c_g;&c_r;&c_y;&c_z;&c_t;&c_mol;</trd>
  <trd lng="sk">hloda≈•</trd>
</drv>

<drv mrk="rod1.0uloj">
   <kap><tld/>uloj</kap>
   <uzo tip="fak">ZOO</uzo>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="rongx.0uloj">ron&gcirc;uloj</ref>
   </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_y;</trd>
  <trd lng="ca">rosegadors</trd>
  <trd lng="cs">hlodavci</trd>
  <trd lng="de">Nagetiere</trd>
  <trd lng="en">rodents</trd>
  <trd lng="fr">rongeurs</trd>
  <trd lng="hu">r&aacute;gcs&aacute;l&oacute;k</trd>
  <trd lng="nl">knaagdieren</trd>
  <trd lng="pt">roer</trd>
  <trd lng="ru">&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_y; <klr
tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">hlodavce</trd>
</drv>
</art>
