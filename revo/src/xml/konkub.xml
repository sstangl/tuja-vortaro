<vortaro>
<art mrk="$Id: konkub.xml,v 1.12 2014/10/22 07:10:12 revo Exp $">
<kap>
  <rad>konkubin</rad>/o
  <fnt>B</fnt>
</kap>
<drv mrk="konkub.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Virino vivanta kun viro, kun kiu &scirc;i ne estas edzinigita.
    </dif>
    <trdgrp lng="de">
       <trd>Lebensgef&auml;hrtin</trd>,
       <trd>Partnerin</trd>
    </trdgrp>
    <trd lng="es">concubina</trd>
    <trd lng="fr">concubine</trd>
    <trdgrp lng="nl">
      <trd>levensgezellin</trd>,
      <trd>partner<klr tip="ind"> (vr.)</klr></trd>
    </trdgrp>
    <trd lng="pt">concubina</trd>
  </snc>
  <snc>
    <dif>
      Kromvirino.
    </dif>
    <trdgrp lng="de">
      <trd>Konkubine</trd>,
      <trd>Kebse</trd>,
      <trd>Nebenfrau</trd>
    </trdgrp>
    <trd lng="en">concubine</trd>
    <trd lng="nl">concubine</trd>
    <trdgrp lng="pt">
      <trd>concubina</trd>,
      <trd>amante</trd>,    
      <trd>am&aacute;sia</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">concubina</trd>
  <trd lng="ru">&c_s;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
