<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stampf.xml,v 1.12 2015/07/04 08:37:44 revo Exp $">
<kap>
   <rad>stampf</rad>/i
</kap>
<drv mrk="stampf.0i">
   <kap><tld/>i</kap>
   <uzo tip="stl">ARK</uzo>
   <snc>
     <ref tip="dif" cel="stamf.0i">stamfi</ref>
   </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
   <ind>&c_k;&c_a;&c_p;&c_y;&c_t;&c_a;&c_m;&c_ib;</ind></trd>
  <trdgrp lng="cs">
    <trd>dupat mocně kopytem</trd>,
    <trd>dusat</trd>,
    <trd>hrabat nohou <klr>(zvl. o koni)</klr></trd>,
    <trd>pěchovat</trd>
  </trdgrp>
  <trd lng="de">stampfen</trd>
  <trdgrp lng="fr">
     <trd>piaffer</trd>,
     <trd>taper du pied</trd>,
     <trd>tr&eacute;pigner</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>r&uacute;gkap&aacute;l</trd>,
     <trd>toporz&eacute;kol</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>stampvoeten</trd>,
     <trd>trappelen</trd>
   </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_y;&c_t;&c_a;&c_m;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>dupkať</trd>,
    <trd>hrabať nohou</trd>,
    <trd>udupávať</trd>
  </trdgrp>
</drv>
</art>
