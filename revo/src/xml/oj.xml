<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oj.xml,v 1.9 2016/03/18 20:10:15 revo Exp $">
<kap>
  <rad>oj</rad> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="oj.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc>
    <dif>
      Dolore, beda&ubreve;rinde, korpreme:
      <ekz>
        <tld/>, Pjotr Ivanovi&ccirc;, Pjotr Ivanovi&ccirc;, vi
        piedpremis mian piedon
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">ay<klr tip="amb"> (interjecci&oacute;n de dolor)</klr></trd>
  <trdgrp lng="fr">
    <trd>ah<klr> (regret, douleur)</klr></trd>,
    <trd>houla</trd>
  </trdgrp>
  <trd lng="hu">jaj</trd>
  <trd lng="nl">aj</trd>
  <trdgrp lng="pl">
    <trd>aj!</trd>
    <trd>oj!</trd>,
    <trd>ojej!</trd>
  </trdgrp>
  <trd lng="pt">ai!<klr tip="amb"> (interj. de dor)</klr></trd>
  <trd lng="ru">&c_o;&c_j;</trd>
  <trd lng="tp">a</trd>
</drv>
</art>

