<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaldro.xml,v 1.27 2015/07/04 08:37:36 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kaldron</rad>/o
</kap>

<drv mrk="kaldro.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="kaldro.0o.boligilo">
     <uzo tip="fak">KUI</uzo>
     <dif>
       Granda metala <ref tip="super" cel="vaz.0o">vazo</ref> por kuiri,
       boligi ktp:
       <ekz>
         el la <tld/>o, en kiu sin trovas bolanta akvo, eliras vaporo
         <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FRAZ</uzo>
         <ind><tld/>o ridas pro poto, kaj mem estas kota</ind>
         <trd lng="ru">&c_n;&c_a; &c_s;&c_je;&c_b;&c_ja; 
&c_p;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_i;<klr> 
(&c_p;&c_o;&c_s;&c_l;.)</klr></trd>
         <trd lng="fi">pata kattilaa soimaa<klr> (sananl.)</klr></trd>
         <trd lng="nl">de <ind>pot</ind> verwijt de ketel dat hij zwart
         ziet</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FRAZ</uzo>
         ne batalu pot' el tero kontra&ubreve; <tld/>o el fero
         <klr>(ne ataku pli fortajn ol vi)</klr>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <ctl>La granda <tld/>o</ctl>
         <klr>(romano de John Francis)</klr>
         <fnt><aut>Francis John</aut>, <vrk>La granda kaldrono. -
         Antverpeno, La Laguna: Stafeto, 1978</vrk></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_k;&c_a;&c_c;&c_jo;&c_l;</trd>,
       <trd>&c_b;&c_a;&c_k; <klr>(&c_n;&c_a;&c_p;&c_r;., 
&c_d;&c_l;&c_ja; 
&c_k;&c_ib;&c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
     </trdgrp>
     <trd lng="br">kaoter<klr tip="ind"> (kegin)</klr></trd>
     <trdgrp lng="ca">
       <trd>cassola</trd>,
       <trd>olla</trd>
     </trdgrp>
     <trd lng="de">Kessel</trd>
     <trd lng="en">cauldron</trd>
     <trd lng="es">caldero</trd>
     <trdgrp lng="fi">
       <trd>pata</trd>,
       <trd><klr tip="amb">(iso)</klr> kattila</trd>
     </trdgrp>
     <trd lng="fr">chaudron</trd>
     <trdgrp lng="hu">
       <trd>&uuml;st</trd>,
       <trd>bogr&aacute;cs</trd>,
       <trd>kond&eacute;r</trd>
     </trdgrp>
     <trd lng="nl">ketel</trd>
     <trd lng="pt">caldeir&atilde;o</trd>
     <trdgrp lng="ru">
       <trd>&c_k;&c_o;&c_t;&c_jo;&c_l;</trd>,
       <trd>&c_b;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; 
&c_k;&c_i;&c_p;&c_ja;&c_ch;&c_je;&c_n;&c_i;&c_ja;, 
&c_v;&c_a;&c_r;&c_k;&c_i;)</klr></trd>
     </trdgrp>
     <trd lng="sv">kittel</trd>
   </snc>
   <snc mrk="kaldro.0o.GEOG">
     <uzo tip="fak">GEOG</uzo>
     <dif>
       Ronda valo.
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_ib;&c_z;&c_ib;&c_n;&c_a;</trd>,
       <trd>&c_l;&c_a;&c_g;&c_ch;&c_y;&c_n;&c_a; 
<klr>(&c_k;&c_r;&c_u;&c_g;&c_l;&c_a;&c_ja;)</klr></trd>
     </trdgrp>
     <trd lng="br">toull<klr tip="ind"> (torosennadur)</klr></trd>
     <trdgrp lng="ca">
       <trd>circ</trd>,
       <trd>caldera</trd>,
       <trd>depressi&oacute;</trd>
     </trdgrp>
     <trd lng="de">Talkessel</trd>
     <trd lng="en">hollow</trd>
     <trd lng="fi">kattilalaakso</trd>
     <trd lng="fr">cuvette<klr tip="ind"> (g&eacute;og.)</klr></trd>
     <trd lng="hu">v&ouml;lgykatlan</trd>
     <trd lng="nl">keteldal</trd>
     <trd lng="ru">&c_k;&c_o;&c_t;&c_l;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>kotel</trd>,
    <trd>kotlík</trd>
  </trdgrp>
  <trd lng="de">Kessel</trd>
  <trd lng="sk">kotlina</trd>
</drv>

<drv mrk="kaldro.0ego">
   <kap><tld/>ego</kap>
   <uzo tip="fak">TEK</uzo>
   <snc mrk="kaldro.0ego.TEK">
     <dif>
       Granda ujo, kie oni vaporigas akvon, por funkciigi 
vaporma&scirc;inojn.
     </dif>
     <rim>
       Tiu vortformo estas malkonvena por ne tro antikvaj
       aran&gcirc;a&jcirc;oj, kiuj balda&ubreve; perdis duonsferan 
formon kaj i&gcirc;is
       tuba&jcirc;oj (kiel en varmocentraloj). Tial la formo de la angla
       termino estas pli rekomendinda kaj ta&ubreve;ga:
       <ref tip="super" cel="bol.0igilo">boligilo</ref>.
       <aut>Sergio Pokrovskij</aut>
     </rim>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_jo;&c_l; 
<klr>(&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;)</klr></trd>
  <trd lng="br">kaoter<klr tip="ind"> (tommerezh)</klr></trd>
  <trd lng="ca">caldera</trd>
  <trd lng="cs">velký kotel</trd>
  <trd lng="de">Kessel <klr>(grosser)</klr></trd>
  <trd lng="en">(steam) <ind>boiler</ind></trd>
  <trd lng="es">caldera</trd>
  <trd lng="fi">(h&ouml;yrykoneen) <ind>kattila</ind></trd>
  <trd lng="fr">chaudi&egrave;re</trd>
  <trd lng="hu">kaz&aacute;n</trd>
  <trd lng="nl">stoomketel</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_l; 
<klr>(&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; 
&c_m;&c_a;&c_sh;&c_i;&c_n;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">veľký kotol</trd>
</drv>

<drv mrk="kaldro.vapor0o">
   <kap>vapor<tld/>o</kap>
   <uzo tip="fak">TEK</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <ref tip="dif" cel="kaldro.0ego"><tld/>ego</ref>
     <ekz>
       en la <ind>vapor<tld/>ejo</ind> <klr>[de la elektrostacio]</klr>
       oni klarigas al mi, ke dudek vagonoj da &ccirc;iutage
       venanta karbo estas mal&scirc;ar&gcirc;ataj &ccirc;i
       tie de unu homo dum du kaj duono minutoj
       <fnt><bib>Metrop</bib><lok>&ccirc;ap. 1</lok></fnt>.
       <trd lng="br">kambr ar gaoter</trd>
       <trd lng="fr">chaufferie</trd>
     </ekz>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_jo;&c_l; 
<klr>(&c_m;&c_a;&c_sh;&c_y;&c_n;&c_y;)</klr></trd>
  <trd lng="br">kaoter<klr tip="ind"> (tommerezh)</klr></trd>
  <trd lng="ca">caldera</trd>
  <trd lng="cs">parní kotel</trd>
  <trd lng="de">Dampfkessel</trd>
  <trd lng="fi">h&ouml;yrykattila<klr> (tekn.)</klr></trd>
  <trd lng="fr">chaudi&egrave;re</trd>
  <trd lng="nl">stoomketel</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; 
<ind>&c_k;&c_o;&c_t;&c_jo;&c_l;</ind></trd>
  <trd lng="sk">parný kotol</trd>
</drv>
</art>
