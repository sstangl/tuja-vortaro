<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkolog.xml,v 1.1 2012/10/12 00:10:11 revo Exp $">
<kap>
  <rad>konkolog</rad>/o
</kap>
<drv mrk="konkolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkolog.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="konkologi.0o.ZOO">konkologio</ref>:
      <ekz>
        spertaj <tld/>oj nun mallevas multajn el la tre
        fajnaj specioj de Dorbini (D'Orbigny) kaj aliaj
        al la rango de varia&jcirc;oj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>conchyliologiste</trd>,
    <trd>conchyliologue</trd>
  </trdgrp>
</drv>
</art>

