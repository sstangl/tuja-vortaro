<vortaro>
<art mrk="$Id: sardon.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
   <rad>sardon</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sardon.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="sarkas.0a">sarkasma</ref>
     <ekz>
       <tld/>a rido a&ubreve;di&gcirc;is
       <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 67a, p. 441a</lok></fnt>.
     </ekz>
   </snc>
  <trdgrp lng="be">
     <trd>&c_s;&c_a;&c_r;&c_k;&c_a;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
     <trd>&c_s;&c_a;&c_r;&c_d;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>sardonický</trd>,
    <trd>trpký</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>sarcastic</trd>,
     <trd>sardonic</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>sarc&aacute;stico</trd>,
     <trd>sard&oacute;nico</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>sarcastique</trd>,
     <trd>sardonique</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>g&uacute;nyos</trd>,
     <trd>szardonikus</trd>,
     <trd>g&ouml;rcs&ouml;s</trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>sarcastico</trd>,
     <trd>sardonico</trd>
   </trdgrp>
  <trdgrp lng="pt">
   <trd>sard&ocirc;nico</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_s;&c_a;&c_r;&c_d;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_s;&c_a;&c_r;&c_k;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>sardonický</trd>,
    <trd>trpný</trd>
  </trdgrp>
</drv>
</art>
