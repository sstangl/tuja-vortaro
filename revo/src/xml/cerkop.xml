<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cerkop.xml,v 1.14 2015/10/07 14:10:25 revo Exp $">
<kap>
  <rad>cerkopitek</rad>/o
</kap>
<drv mrk="cerkop.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Mezgranda <ref tip="lst" cel="simi.0o"
      lst="voko:simioj">simio</ref>,
      kun longaj kruroj kaj vosto, diverskolora felo kaj hela
      viza&gcirc;desegno,
      vivanta en Afriko sude de Saharo sur arboj nutranta sin precipe de
      fruktoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="cerkop1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cercopithecus</trd>).
    </dif>
  <trd lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;</trd>
  <trd lng="ca">cercopitec</trd>
  <trd lng="de">Meerkatze</trd>
  <trdgrp lng="en">
    <trd>Cercopithecus</trd>,
    <trd>guenon</trd>
  </trdgrp>
  <trd lng="fr">cercopith&egrave;que</trd>
  <trd lng="hu">cerk&oacute;f</trd>
  <trd lng="it">cercopiteco</trd>
  <trdgrp lng="nl">
    <trd>meerkat</trd>,
    <trd>makako</trd>
  </trdgrp>
  <trd lng="pl">koczkodan</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
</drv>
</art>
