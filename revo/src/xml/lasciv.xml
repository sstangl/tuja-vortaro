<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lasciv.xml,v 1.9 2016/04/21 11:10:26 revo Exp $">
<kap>
  <rad>lasciv</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lasciv.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Kondute provoka al seksa volupto.
    </dif>
    <ref tip="ant" cel="prud.0a">pruda</ref>
    <refgrp tip="vid">
      <ref cel="dibocx.0a">dibo&ccirc;a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ju;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_zh;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chl&iacute;pn&yacute;</trd>,
    <trd>lascivn&iacute;</trd>,
    <trd>necudn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lascivious</trd>,
    <trd>lustful</trd>,
    <trd>lewd</trd>
  </trdgrp>
  <trd lng="fr">lascif</trd>
  <trdgrp lng="hu">
    <trd>buja</trd>,
    <trd>&eacute;rz&eacute;ki</trd>,
    <trd>k&eacute;js&oacute;v&aacute;r</trd>,
    <trd>k&eacute;jv&aacute;gy&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wellustig<klr tip="ind"> (zeldz.)</klr></trd>,
    <trd>lascief<klr tip="ind"> (zeldz.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lascivo</trd>,
    <trd>libidinoso</trd>,
    <trd>l&uacute;brico</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_h;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlipn&yacute;</trd>,
    <trd>lasc&iacute;vny</trd>,
    <trd>zmyseln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="lasciv.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Sinteno sugestanta mal&ccirc;aston, provanta instigi &gcirc;in &ccirc;e aliaj:
      <ekz>
        timi la sintrudon de amoremuloj kaj la atencojn de la
        <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 11a, p.
        82a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ju;&c_r;</trd>,
    <trd>&c_ju;&c_r;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_zh;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lascivita</trd>,
    <trd>lascivnost</trd>,
    <trd>necudnost</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lascivet&eacute;</trd>,
    <trd>lascivit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bujas&aacute;g</trd>,
    <trd>&eacute;rz&eacute;kis&eacute;g</trd>,
    <trd>k&eacute;jv&aacute;gy</trd>
  </trdgrp>
  <trd lng="nl">wellustigheid<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_h;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">necudnos&tcaron;</trd>
</drv>
</art>

