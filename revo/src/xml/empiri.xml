<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: empiri.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>empiri</rad>/o
</kap>
<drv mrk="empiri.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <dif>
      Sperto konsiderata kiel bazo de filozofio kaj scienco.
    </dif>
    <trdgrp lng="en">
      <trd>empiry</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>emp&iacute;ria</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">empiria</trd>
  </snc>
  <snc>
    <dif>
      Nescienca rutina metodo bazita sur nura praktika nesufi&ccirc;e
      kontrolita
      sperto.
    </dif>
    <trdgrp lng="hu">
      <trd>j&aacute;rtass&aacute;g</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">do&sacute;wiadczenie</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>empirie</trd>,
    <trd>zkušenost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Empirie</trd>,
    <trd>Erfahrungswissen</trd>,
    <trd>Erfahrung</trd>
  </trdgrp>
  <trd lng="fr">empirique<klr> (subst.)</klr></trd>
  <trd lng="pt">empiria</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_y;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">skúsenosť</trd>
</drv>

<drv mrk="empiri.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur metoda scienca sperto.
    </dif>
    <refgrp tip="sin">
      <ref cel="eksper">posteksperimenta</ref>,
      <ref cel="aposte.0a">aposteriora</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="spekul2.0a">spekulativa</ref>,
      <ref cel="teori.0o">teorio</ref>.
    </refgrp>
    <trd lng="hu">empirikus</trd>
    <trd lng="pl">do&sacute;wiadczalny</trd>
  </snc>
  <snc>
    <dif>
      Bazita sur senscienca rutina sperto.
    </dif>
    <trdgrp lng="hu">
      <trd>empirikus</trd>,
      <trd>tapasztalati</trd>
    </trdgrp>
    <trd lng="pl">empiryczny</trd>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>mastičkářský</trd>,
    <trd>šarlatánský</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>empirisch</trd>,
    <trd>auf Erfahrung beruhend</trd>,
    <trd>aus Erfahrung</trd>,
    <trd>Erfahrungs-</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empirical</trd>,
    <trd>objective</trd>
  </trdgrp>
  <trd lng="fr">empirique<klr> (adj.)</klr></trd>
  <trd lng="pt">emp&iacute;rico</trd>
  <trd
  lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mastičkársky</trd>,
    <trd>šarlatánsky</trd>
  </trdgrp>
</drv>

<drv mrk="empiri.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Filozofia doktrino, la&ubreve; kiu &ccirc;iuj ideoj devenas de la
      sperto
    </dif>
    <refgrp tip="vid">
      <ref cel="raci.0ismo">raciismo</ref>.
    </refgrp>
    <trd lng="hu">empirizmus</trd>
    <trd lng="pl">empiryzm</trd>
    <trd lng="pt">empirismo</trd>
  </snc>
  <snc>
    <dif>
      Empirio 2.
    </dif>
    <trdgrp lng="hu">
      <trd>j&aacute;rtass&aacute;g</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">do&sacute;wiadczenie</trd>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_ib;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">empirizmus</trd>
  <trdgrp lng="de">
    <trd>Empirismus</trd>,
    <trd>Erfahrungswissenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empiricism</trd>,
    <trd>objectivism</trd>
  </trdgrp>
  <trd lng="fr">empirisme</trd>
  <trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">empirizmus</trd>
</drv>

<drv mrk="empiri.0acxa">
  <kap><tld/>a&ccirc;a</kap>
  <snc>
    <dif>
      Empiria 2.
    </dif>
  </snc>
</drv>

<drv mrk="empiri.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Sekvanto de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>empirista</trd>,
    <trd>hlasatel empirizmu</trd>,
    <trd>praktik</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Empiriker</trd>,
    <trd>Erfahrungswissenschaftler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empiricist</trd>,
    <trd>objectivist</trd>
  </trdgrp>
  <trd lng="fr">empiriste</trd>
  <trdgrp lng="hu">
    <trd>empirista</trd>,
    <trd>empirikus filoz&oacute;fus</trd>
  </trdgrp>
  <trd lng="pl">empirysta</trd>
  <trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>empirista</trd>,
    <trd>praktik</trd>
  </trdgrp>
</drv>

<drv mrk="empiri.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo fidanta la rutinan kaj krudan sperton.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_t;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>empirik</trd>,
    <trd>člověk věřící na rutinní zkušenost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Praktiker</trd>,
    <trd>Erfahrungsmensch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empirikus<klr tip="ind"> (ember)</klr></trd>,
    <trd>gyakorlatias ember</trd>
  </trdgrp>
  <trd lng="fr">empiriste</trd>
  <trdgrp lng="pl">
    <trd>empiryk</trd>,m
    <trd>praktyk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">empirik</trd>
</drv>
</art>
