<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ovaci.xml,v 1.18 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ovaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ovaci.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="ovaci.0i.MOR">
     <dif>
       Entuziasme amase honore akcepti:
       <ekz>
         oni <tld/>is min
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">zujubeln</trd>
   <trd lng="es">ovacionar</trd>
   <trd lng="fr">ovationner</trd>
   <trd lng="hu">&eacute;ljenez</trd>
   <trdgrp lng="nl">
     <trd>een <ind>ovatie</ind> geven</trd>,
     <trd>iem. <ind>toejuichen</ind></trd>
   </trdgrp>
   <trd lng="pt">ovacionar</trd>
</drv>

<drv mrk="ovaci.0o">
  <kap><tld/>o</kap>
   <snc mrk="ovaci.0o.MOR">
     <dif>
       Entuziasma amasa honora akcepto al iu.
     </dif>
     <trd lng="pt">ova&ccedil;&atilde;o</trd>
   </snc>
   <snc mrk="ovaci.0o.HIS">
     <uzo tip="fak">HIS</uzo>
     <dif>
       &Ccirc;e antikvaj romianoj, ceremonio malpli solena ol
       <ref tip="vid" cel="triumf.0o.HIS">triumfo</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>oslava</trd>,
    <trd>ovace</trd>,
    <trd>přivítání</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ovation</trd>,
     <trd>Huldigung</trd>
   </trdgrp>
  <trd lng="en">ovation</trd>
  <trd lng="es">ovaci&oacute;n</trd>
  <trd lng="fr">ovation</trd>
  <trdgrp lng="hu">
     <trd>ov&aacute;ci&oacute;</trd>,
     <trd>&uuml;dvrivalg&aacute;s</trd>,
     <trd>&eacute;ljenz&eacute;s</trd>
   </trdgrp>
  <trd lng="nl">ovatie</trd>
  <trd lng="ru">&c_o;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>oslava</trd>,
    <trd>ovácia</trd>,
    <trd>privítanie</trd>
  </trdgrp>
</drv>
</art>
