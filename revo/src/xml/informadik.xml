<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: informadik.xml,v 1.6 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>informadik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="informadik.0o">
  <kap><tld/>o</kap>
  <snc mrk="informadik.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="komputik.0o.KOMP">komputiko</ref>
    <ekz>
      Tutmonda Asocio pri Kibernetiko, <tld/>o kaj Sistemiko
      <fnt>
        <aut>R. F&ouml;ssmeier</aut>,
        <vrk>Grupo de UNIX-uzantoj</vrk>,
        <lok><url 
        ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
        1992-03-16</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>informatika</trd>,
    <trd>nauka o informac√≠ch</trd>
  </trdgrp>
  <trd lng="de">Informatik</trd>
  <trd lng="fr">informatique<klr> (subst.)</klr></trd>
  <trd lng="pl">informatyka</trd>
  <trd lng="pt">inform&aacute;tica</trd>
  <trd lng="sk">informatika</trd>
</drv>

<drv mrk="informadik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="informadik.0isto.SCI">
    <ref tip="dif" cel="komput.0isto.KOMP">komputisto</ref>
    <ekz>
      dum la laboro de niaj <tld/>istoj estis perdita nia adreslibro
      <fnt>
      <aut>Radio Polonia</aut>,
      <vrk><url
      ref="http://abonu.com/archive/info.polonia/200305/02211339.html">Programoj
      de Esperanto-redakcio de Pola Radio</url></vrk>,
      <lok>2005-03</lok></fnt>
    </ekz>
  </snc>
  <trd lng="cs">informatik</trd>
  <trd lng="de">Informatiker</trd>
  <trd lng="fr">informaticien</trd>
  <trd lng="pl">informatyk</trd>
  <trd lng="sk">informatik</trd>
</drv>
</art>

