<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kiloka.xml,v 1.6 2009/08/26 16:30:51 revo Exp $">
<kap>
  <rad>kilokalori</rad>/o
</kap>

<drv mrk="kiloka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Malnova komunuza
      <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de varmo, egala
      al
      mil <ref tip="prt" cel="kalori.0o">kalorioj</ref>; simb. kcal.
    </dif>
    <ref tip="sin" cel="kalori.granda0o">granda kalorio</ref>
  </snc>
  <trd lng="de">Kilokalorie</trd>
  <trd lng="es">kilocalor&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>kilocalorie</trd>,
    <trd>grande <ind>calorie</ind></trd>
  </trdgrp>
  <trd lng="nl">kilocalorie</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_k;&c_a;&c_l;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>

