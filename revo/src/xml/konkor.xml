<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkor.xml,v 1.17 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>konkord</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konkor.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkor.0o.KOMUNE">
    <dif>
      Kora a&ubreve; morala plenakordo; harmonio de sentoj kaj opinioj
      inter pluraj personoj:
      <ekz>
        kie regas <tld/>o, regas ordo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o malgranda&jcirc;on kreskigas, mal<tld/>o granda&jcirc;on
        ruinigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsen.0o">konsento</ref>,
      <ref cel="simpat.0o">simpatio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_a;&c_je;&c_m;&c_a;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">conc&ograve;rdia</trd>
  <trdgrp lng="cs">
    <trd>konkordance</trd>,
    <trd>shoda</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Eintracht</trd>,
     <trd>Einm&uuml;tigkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>concord</trd>,
    <trd>accord</trd>,
    <trd>agreement</trd>,
    <trd>harmony</trd>
  </trdgrp>
  <trd lng="es">concordia</trd>
  <trdgrp lng="fr">
    <trd>concorde</trd>,
    <trd>pleine <ind>entente</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyet&eacute;rt&eacute;s</trd>,
    <trd>&ouml;sszhang</trd>
  </trdgrp>
  <trd lng="nl">eendracht</trd>
  <trd lng="pt">conc&oacute;rdia</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>,
    <trd>&c_l;&c_a;&c_d;</trd>,
    <trd>&c_m;&c_i;&c_r;</trd>,
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_p;&c_o;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jednota</trd>,
    <trd>svornosť</trd>
  </trdgrp>
</drv>

<drv mrk="konkor.0a">
  <kap><tld/>a</kap>
  <snc mrk="konkor.0a.KOMUNE">
    <dif>
      En grupa interakordo:
      <ekz>
        <tld/>a kunvivado
        <fnt>
          <aut>G. M. Escayola</aut>,
          <vrk><url ref="http://www.gazetoteko.com/kajeroj/k45soci.htm">
          Socia evoluo</url></vrk>,
          <lok>Kajeroj el la Sudo, 1999-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jednomyslný</trd>,
    <trd>svorný</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concordant</trd>,
    <trd>harmonieux</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jednomyseľný</trd>,
    <trd>svorný</trd>
  </trdgrp>
</drv>

<drv mrk="konkor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konkor.0i.KOMUNE">
    <dif>
      Interakordi, interkonsenti, harmonii:
      <ekz>
        en mia propra kampo psikiatria <klr>...</klr>
        necesas ordigi terminojn, kies uzado ne <tld/>as
        inter diversaj lingvoj
        <fnt>
          <aut>V. Banaitis</aut>,
          <vrk>en: <url
          ref="http://www.liberafolio.org/2014/la-universo-nun-alireblas-en-esperanto">
          La universo nun alireblas en Esperanto</url></vrk>,
          <lok>libera folio, 2014-03-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>být v jednotě</trd>,
    <trd>být ve svornosti</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concorder</trd>,
    <trd>s'<ind>harmoniser</ind></trd>
  </trdgrp>
  <trd lng="sk">byť svorný</trd>
</drv>

<drv mrk="konkor.0ekun">
  <kap><tld/>e kun,
  <var><kap><tld/>e al</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="konkor.0ekun.KOMUNE">
    <dif>
      Harmonie kun,
      <ref tip="vid" cel="akord.0ekun.KOMUNE">akorde kun</ref>...:
      <ekz>
        <tld/>e al tiu bruo ekmu&gcirc;is la urbanoj
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url
          ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kun la mona&hcirc;eja regulo ili zorgadis
        pri malri&ccirc;uloj kaj malsanuloj
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.gdynia.pl/esp/tourism/5600_53205.html">
          Mallonga promenado tra historiaj monumentoj de Gdynia
          (Gdinio)</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">en accord avec</trd>
</drv>

<drv mrk="konkor.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="konkor.mal0a.KOMUNE">
    <dif>
      Kiu ne enhavas interkonsenton, harmonion,
      unuecon:
      <ekz>
        kiel preska&ubreve; &ccirc;iam okazas, la minoritatajn
        mal<tld/>ajn vo&ccirc;ojn silentigis <klr>...</klr>
        plimulto
        <fnt>
          <aut>L. E. M&aacute;rquez, trad. T. del Barrio</aut>,
          <vrk><url ref="http://www.delbarrio.eu/spiritismo.htm">
          Infanina Ludo</url></vrk>,
          <lok>[2010?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>v nejednotě</trd>,
    <trd>ve sváru</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;suni</trd>,
    <trd>discordant</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nejednotný</trd>,
    <trd>nesvorný</trd>,
    <trd>neznášanlivý</trd>
  </trdgrp>
</drv>

<drv mrk="konkor.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="konkor.mal0o.KOMUNE">
    <dif>
      Diseco de sentoj, opinioj, agadoj,
      spirita
      <ref tip="vid" cel="unu.mal0eco.KOMUNE">malunueco</ref>:
      <ekz>
        nenia afero estas pli malbenita, ol la dispartii&gcirc;o kaj 
        mal<tld/>o
        <fnt>
          <bib>SatirRak</bib>,
          <lok>Kain kaj Abel</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nejednotnost</trd>,
    <trd>neshoda</trd>,
    <trd>nesoulad</trd>,
    <trd>nesvornost</trd>,
    <trd>rozkol</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>discorde</trd>,
    <trd>dissentiment</trd>,
    <trd>m&eacute;sentente</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nejednotnosť</trd>,
    <trd>nesvornosť</trd>,
    <trd>neznášanlivosť</trd>,
    <trd>rozkol</trd>
  </trdgrp>
</drv>
</art>
