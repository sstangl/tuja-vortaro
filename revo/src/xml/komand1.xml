<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komand1.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>komandit</rad>/o
</kap>
<drv mrk="komand1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="komand1.0o.mono">
    <dif>
      Sumo, disponigata al komerca societo de ano, kiu ne partoprenas
      en la administrado.
    </dif>
    <trdgrp lng="de">
      <trd>stille <ind>Beteiligung</ind></trd>,
      <trd>stille <ind>Einlage</ind></trd>
    </trdgrp>
    <trd lng="fr">commandite<klr tip="ind"> (fonds)</klr></trd>
  </snc>
  <snc mrk="komand1.0o.sistemo">
    <dif>
      Sistemo, en kiu parto de la anoj de societo, ne partoprenante
      en ties administrado, riskas nur sian monon kaj ne respondas
      pri la funkciado de la societo.
    </dif>
    <trd lng="en">limited <ind>partnership</ind></trd>
    <trd lng="fr">commandite<klr tip="ind"> (syst&egrave;me)</klr></trd>
    <trdgrp lng="pt">
      <trd>comandita</trd>,
      <trd>sociedade em comandita</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">komanditní společnost</trd>
  <trd lng="sk">komanditná spoločnosť</trd>
</drv>

<drv mrk="komand1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Funkcianta la&ubreve; <tld/>o <sncref ref="komand1.0o.sistemo"/>:
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <ind><tld/>a societo</ind>.
        <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_y;&c_t;&c_n;&c_a;&c_je; <ind>&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;&c_s;&c_t;&c_v;&c_a;</ind></trd>
        <trd lng="de">Kommanditgesellschaft</trd>
        <trdgrp lng="fr">
          <trd>soci&eacute;t&eacute; en <ind>commandite</ind></trd>
        </trdgrp>
        <trd lng="nl">commanditaire vennootschap</trd>
        <trd lng="ru">&c_k;&c_o;&c_m;&c_m;&c_a;&c_n;&c_d;&c_i;&c_t;&c_n;&c_o;&c_je; &c_t;&c_o;&c_v;&c_a;&c_r;&c_i;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="komand1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disponigi <tld/>on <sncref ref="komand1.0o.mono"/>.
    </dif>
  </snc>
  <trd lng="cs">investovat peníze do komanditní společnosti</trd>
  <trd lng="fr">commanditer<klr tip="ind"> (investir en commandite)</klr></trd>
  <trd lng="sk">byť účastníkom komanditnej spoločnosti</trd>
</drv>

<drv mrk="komand1.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tiu, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_y;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">podílník</trd>
  <trdgrp lng="de">
    <trd>Kommanditist</trd>,
    <trd>stiller <ind>Gesellschafter</ind></trd>,
    <trd>stiller <ind>Teilhaber</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sleeping <ind>partner</ind></trd>,
    <trd>silent <ind>partner</ind></trd>
  </trdgrp>
  <trd lng="fr">commanditaire<klr tip="ind"> (d'une commandite)</klr></trd>
  <trd lng="nl">stille <ind>vennoot</ind></trd>
  <trd lng="sk">podielnik</trd>
</drv>
</art>
