<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: feniko.xml,v 1.22 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fenikopter</rad>/o
  <fnt>Z</fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feniko.0o">
  <kap><tld/>o</kap>
  <snc mrk="feniko.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda longakrura birdo kun rozkoloraj kaj ru&gcirc;aj plumoj,
      ofte staranta sur nur unu piedo,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el <ref tip="malprt"
      cel="feniko1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoenicopterus</trd>).
    </dif>
  </snc>
  <bld lok="../bld/feniko.jpg">
    <tld/>oj
  </bld>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_g;&c_a;</trd>
  <trd lng="bg">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="cs">plame&ncaron;&aacute;k</trd>
  <trd lng="de">Flamingo</trd>
  <trd lng="en"><klr>(greater) </klr>flamingo</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_m;&f_y;&a_n;&f_g;&a_w;</trd>
  <trd lng="fr">flamant</trd>
  <trd lng="hu">flaming&oacute;</trd>
  <trd lng="nl">flamingo</trd>
  <trdgrp lng="pl">
    <trd>czerwonak</trd>,
    <trd>flaming<klr> (r&oacute;&zdot;owy)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>flamingo</trd>,
    <trd>flamengo</trd>,
    <trd>fenic&oacute;ptero</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="sv">flamingo</trd>
  <trd lng="sk">plameniak</trd>
</drv>

<drv mrk="feniko.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="feniko.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Phoenicopteriformes</trd>).
    </dif>
  </snc>
  <trd lng="cs">plameňáci</trd>
  <trd lng="de">Flamingov&ouml;gel</trd>
  <trd lng="fr">ph&oelig;nicopt&eacute;riformes</trd>
  <trd lng="hu">flaming&oacute;alak&uacute;ak</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;<klr tip="amb">
  (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">plameniaci</trd>
</drv>
</art>
