<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: salik.xml,v 1.24 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salik</rad>/o
</kap>
<drv mrk="salik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="salikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Salix</trd>) de
      <ref tip="lst" cel="arb.0o"
      lst="voko:malpigioparencoj">arboj</ref>
      kaj arbedoj kun <ref tip="prt" cel="ament.0o">amentoj</ref>;
      preferantaj malsekan grundon;
      la flekseblaj vergoj de kelkaj specioj estas uzataj en korbfarado:
      <ekz>
        &scirc;ajnis al li, ke son&gcirc;e li
        vidis la junulinon sub la bran&ccirc;oj de la olda <tld/>o, kiu
        kreskis
        &ccirc;e la fonto<fnt><aut>Julian Modest</aut>: <vrk>La ikono</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_b;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_r;&c_b;&c_a;</trd>
  <trd lng="cs">jíva</trd>
  <trd lng="de">Weide</trd>
  <trd lng="en">willow</trd>
  <trd lng="es">sauce</trd>
  <trd lng="fa"><klr>(&a_d;&a_r;&a_kh;&a_t;) </klr>&a_b;&f_y;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>saule</trd>
  </trdgrp>
  <trd lng="hu">f&udblac;z(fa)</trd>
  <trd lng="it">salice</trd>
  <trd lng="nl">wilg</trd>
  <trd lng="pl">wierzba</trd>
  <trdgrp lng="pt">
   <trd>salgueiro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_v;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>,
    <trd>&c_v;&c_je;&c_r;&c_b;&c_a; <klr>(kelkaj specioj)</klr></trd>
  </trdgrp>
  <trd lng="sk">vŕba</trd>
</drv>
</art>
