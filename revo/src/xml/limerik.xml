<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: limerik.xml,v 1.10 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>limerik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="limerik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Angladevena, &scirc;erca, kvinversa poemeto kutime priskribanta la
      strangan konduton de homo lo&gcirc;anta en iu nomata urbo a&ubreve; lando.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_e;&c_r;&c_y;&c_k;</trd>
  <trd lng="cs">limerik</trd>
  <trd lng="de">Limerick</trd>
  <trdgrp lng="fr">
    <trd>limerick</trd>
  </trdgrp>
  <trd lng="hu">limerick</trd>
  <trd lng="nl">limerick</trd>
  <trd lng="pl">limeryk</trd>
  <trd lng="pt">limerick</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_je;&c_r;&c_i;&c_k;</trd>
  <trd lng="sk">limerik</trd>
</drv>
</art>

