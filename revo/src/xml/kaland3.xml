<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaland3.xml,v 1.9 2015/07/04 08:37:36 revo Exp $">
   <kap>
     <rad>kalandr</rad>/o
   </kap>
<drv mrk="kaland3.0o">
   <kap><tld/>o</kap>
     <uzo tip="fak">ZOO</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Mediteranea birdo (<trd lng="la">Melanocorypha calandra</trd>)
       el la familo <ref cel="alauxd1.0oj">ala&ubreve;dedoj</ref>,
       kun bela kanto.
     </dif>
   </snc>
   <refgrp tip="hom">
     Ne miksu kun:
     <ref cel="kaland.0o">kalandro</ref> (grenskarabo),
     <ref cel="kaland1.0o">kalandro</ref> (ago kalandri).
   </refgrp>
   <trd lng="es">calandria</trd>
   <trd lng="fi">arokiuru</trd>
   <trd lng="fr">calandre<klr tip="ind"> (oiseau)</klr></trd>
   <trdgrp lng="pt">
     <trd>calandra <klr tip="ind">(zool.)</klr></trd>,
     <trd>calhandra <klr tip="ind">(zool.)</klr></trd>
   </trdgrp>
   <trd lng="nl">kalanderleeuwerik</trd>
   <trd lng="ru">&c_s;&c_t;&c_je;&c_p;&c_n;&c_o;&c_j; 
&c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">mangľovanie bielizne</trd>
  <trdgrp lng="cs">
    <trd>kalandrování prádla</trd>,
    <trd>mandlování prádla</trd>
  </trdgrp>
</drv>
</art>

