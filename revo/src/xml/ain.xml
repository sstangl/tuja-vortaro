<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ain.xml,v 1.9 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>ain</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ain.0o">
  <kap><tld lit="A"/>o</kap>
  <snc mrk="ain.0o.ETN">
    <dif>
      Ano de indi&gcirc;ena <ref tip="lst" cel="popol.0o"
      lst="voko:popolanoj">popolo</ref> de Japanujo
      ankora&ubreve; lo&gcirc;anta en insulo <ctl>Hokkaido</ctl>
      kaj en insularo <ctl>Kuriri</ctl>:
      <ekz>
        la <tld/>oj estas indi&gcirc;enoj de Japanujo, nun &ccirc;. 13.000,
        lo&gcirc;antaj en Hokkaido, la plej norda insulo de la lando
        <fnt>
          <aut>MIKAMI Teruo</aut>,
          <vrk>La Zamenhofa Esperanto - simpozio pri
          -ata/-ita</vrk>,
          <lok>Kio estas aspekto?</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">ainu<klr> (subst.)</klr></trd>
  <trd lng="cs">Aino</trd>
  <trd lng="de">Ainu</trd>
  <trd lng="en">Ainu</trd>
  <trd lng="fr">A&iuml;nou<klr> (subst.)</klr></trd>
  <trd lng="id">Ainu</trd>
  <trd lng="it">Ainu</trd>
  <trd lng="sk">Aino</trd>
</drv>

<drv mrk="ain.0a">
  <kap><tld/>a</kap>
  <snc mrk="ain.0a.ETN">
    <dif>
      Rilatanta al la <tld lit="A"/>oj;
      <ekz>
        <tld/>a <ref tip="lst" cel="lingv.0o.homa"
        lst="voko:lingvoj">lingvo</ref>;
      </ekz>
      <ekz>
        <tld/>aj dancoj;
      </ekz>
      <ekz>
        <tld/>aj kutimoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">ainu<klr> (adj.)</klr></trd>
  <trd lng="de">Ainu-</trd>
  <trd lng="en">Ainu</trd>
  <trd lng="fr">a&iuml;nou<klr> (adj.)</klr></trd>
  <trd lng="it">ainu</trd>
</drv>

</art>

