<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: indig.xml,v 1.21 2015/07/04 08:37:36 revo Exp $">
<kap>
   <ofc>4</ofc>
   <rad>indig</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="indig.0o">
   <kap><tld/>o</kap>
   <snc mrk="indig.0o.tinkturo">
     <dif>
       Malhele blua kolorilo, ekstraktata  unuatempe el la <tld/>ujo.
     </dif>
   </snc>
   <snc mrk="indig.0o.KOL">
     <dif>
       Malhela bluo kun viola nuanco:
       <ekz>
         la &ccirc;iela lazuro malheli&gcirc;is &gcirc;is <tld/>o, en kiu steloj ekaperis 
         <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
         <lok>tk-stafeto, 1982</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_g;&c_a;
   <klr tip="amb">(&c_f;&c_a;&c_r;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>indigo</trd>,
    <trd>modř</trd>
  </trdgrp>
  <trd lng="de">Indigo</trd>
  <trd lng="en">indigo</trd>
  <trd lng="es">&iacute;ndigo</trd>
  <trd lng="fr">indigo<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">indig&oacute;</trd>
  <trd lng="nl">indigo</trd>
  <trdgrp lng="pt">
    <trd>&iacute;ndigo<klr tip="amb"> (subst&acirc;ncia)</klr></trd>, 
    <trd>anil<klr tip="amb"> (subst&acirc;ncia)</klr></trd> 
   </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_g;&c_o;
   <klr>(&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>indigo</trd>,
    <trd>modré farbivo</trd>
  </trdgrp>
  <trd lng="sv">indigo</trd>
</drv>

<drv mrk="indig.0ujo">
   <kap><tld/>ujo</kap>
   <snc mrk="indig.0ujo.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       Arbo el tropikaj landoj produktanta bluan kolorilon.
     </dif>
   </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_g;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="cs">indigovník</trd>
  <trd lng="de">Indigopflanze</trd>
  <trd lng="en">indigo</trd>
  <trd lng="fr">indigotier</trd>
  <trd lng="hu">indig&oacute;cserje</trd>
  <trd lng="nl">indigoplant</trd>
  <trdgrp lng="pt">
    <trd>indig&oacute;fera<klr tip="amb"> (planta)</klr></trd>, 
    <trd>anileira<klr tip="amb"> (planta)</klr></trd> 
   </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_g;&c_o; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">indigovník</trd>
  <trd lng="sv">indigov&auml;xt</trd>
</drv>

<drv mrk="indig.0a">
   <kap><tld/>a , <var><kap><tld/>okolora</kap></var></kap>
   <snc mrk="indig.0a.KOL">
     <dif>
       Havanta la bluan koloron de <tld/>o:
       <ekz>
         tiu estis junulino, kiu surhavis hele <tld/>an senmanikan
         veston
         <fnt><bib>LaFamilio</bib>,
         <lok>&ccirc;apitro 15a, p. 186a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; <ind>&c_ib;&c_n;&c_d;&c_y;&c_g;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>indigové barvy</trd>,
    <trd>indigový</trd>
  </trdgrp>
  <trd lng="en">indigo</trd>
  <trd lng="fr">indigo<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">indig&oacute;sz&iacute;n&udblac;</trd>
  <trd lng="nl">indigokleurig</trd>
  <trdgrp lng="pt">
     <trd>anil<klr tip="ind"> (adj.)</klr></trd>, 
     <trd>&iacute;ndigo<klr tip="ind"> (adj.)</klr></trd> 
   </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t;&c_a; &c_i;&c_n;&c_d;&c_i;&c_g;&c_o;</trd>
  <trd lng="sk">indigovej farby</trd>
  <trdgrp lng="sv">
     <trd>indigobl&aring;</trd>,
     <trd>indigof&auml;rgad</trd>
   </trdgrp>
</drv>
</art>
