<kap><rad>frekvencometr</rad>/o</kap>
<drv mrk="frekve1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Aparato por mezuri principe la ondlongon kaj fakte la frekvencon de
      radioelektraj ondoj.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;&c_m;&c_je;&c_r;</trd>
  <trd lng="cs">měřič frekvence</trd>
  <trd lng="de">Frequenzmesser</trd>
  <trd lng="es">frecuenci&oacute;metro</trd>
  <trd lng="fr">fr&eacute;quencem&egrave;tre</trd>
  <trd lng="hu">frekvenciam&eacute;r&odblac;</trd>
  <trd lng="nl">frequentiemeter</trd>
  <trdgrp lng="pt">
    <trd>freq&#252;enc&#237;metro</trd>,
    <trd>freq&#252;enci&#244;metro</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_o;&c_t;&c_o;&c_m;&c_je;&c_r;</trd>
  <trd lng="sk">merač frekvencie</trd>
</drv>
</art>
