<vortaro>
<art mrk="$Id: pajac.xml,v 1.15 2015/10/14 08:10:16 revo Exp $">
<kap>
  <rad>pajac</rad>/o
</kap>
<drv mrk="pajac.0o">
  <kap><tld/>o</kap>
  <snc mrk="pajac.0o.KOMUNE">
    <dif>
      Foira <ref tip="super" cel="amuz.0isto.KOMUNE">amuzisto</ref>:
      <ekz>
        kun via &ccirc;apelo vi similas <tld/>on, &ndash; ne metu vian &ccirc;apelon
        sur unu
        orelon
        <fnt><bib>ElektFab</bib>, <lok>sesope oni trairas la tutan
        mondon</lok></fnt>;
      </ekz>
      <ekz>
        li &jcirc;etis sin denove en siajn <tld/>a&jcirc;ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ja;&c_c;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_z;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pallasso</trd>,
    <trd>buf&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bajazzo</trd>,
    <trd>pan&aacute;k</trd>,
    <trd>pa&ncaron;&aacute;ca</trd>,
    <trd>&scaron;a&scaron;ek</trd>
  </trdgrp>
  <trd lng="de">Possenrei&szlig;er</trd>
  <trdgrp lng="en">
    <trd>clown</trd>,
    <trd>jester</trd>,
    <trd>fool</trd>,
    <trd>buffoon</trd>
  </trdgrp>
  <trd lng="fr">paillasse<klr tip="amb"> (bateleur)</klr></trd>
  <trd lng="hu">boh&oacute;c</trd>
  <trd lng="nl">paljas</trd>
  <trdgrp lng="pl">
    <trd>pajac</trd>,
    <trd>b&lstroke;azen</trd>
  </trdgrp>
  <trd lng="pt">palha&ccedil;o</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_ja;&c_c;</trd>,
    <trd>&c_sh;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>paj&aacute;c</trd>,
    <trd>&scaron;a&scaron;o</trd>
  </trdgrp>
</drv>
</art>
