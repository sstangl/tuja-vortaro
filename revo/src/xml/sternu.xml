<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sternu.xml,v 1.15 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>sternum</rad>/o
</kap>
<drv mrk="sternu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La anta&ubreve;brusta osto, &ccirc;e kiu turni&gcirc;as la ripoj kaj la
klavikloj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hrudní kost</trd>,
    <trd>sternum</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brustbein</trd>,
    <trd>Sternum</trd>
  </trdgrp>
  <trd lng="en">sternum</trd>
  <trd lng="es">estern&oacute;n</trd>
  <trd lng="fr">sternum</trd>
  <trdgrp lng="hu">
    <trd>szegycsont</trd>,
    <trd>mellcsont</trd>
  </trdgrp>
  <trd lng="id"><ind>tulang</ind> dada</trd>
  <trd lng="it">sterno</trd>
  <trdgrp lng="nl">
    <trd>borstbeen</trd>,
    <trd>sternum</trd>
  </trdgrp>
  <trd lng="pl">mostek <klr>(anatomia)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_d;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrudná kosť</trd>,
    <trd>mostík</trd>
  </trdgrp>
</drv>
</art>
