<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grafit.xml,v 1.18 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>grafit</rad>/o
</kap>
<drv mrk="grafit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Grizenigra, mola speco de 
      <ref tip="super" cel="karbon.0o">karbono</ref>, 
      uzata precipe por fabriki krajonojn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_f;&c_i;&c_t;</trd>
  <trd lng="ca">grafit</trd>
  <trdgrp lng="cs">
    <trd>grafit</trd>,
    <trd>tuha</trd>
  </trdgrp>
  <trd lng="de">Graphit</trd>
  <trd lng="en">graphite</trd>
  <trd lng="es">grafito</trd>
  <trd lng="fr">graphite</trd>
  <trd lng="hu">grafit</trd>
  <trd lng="mk">&c_g;&c_r;&c_a;&c_f;&c_i;&c_t;</trd>
  <trd lng="nl">grafiet</trd>
  <trd lng="pl">grafit</trd>
  <trdgrp lng="pt">
   <trd>grafite<klr tip="amb"> (tipo de carbono)</klr></trd>, 
   <trd>grafita<klr tip="amb"> (tipo de carbono)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_t;</trd>
  <trd lng="sk">tuha</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_f;&c_ib;&c_t;</trd>
</drv>
</art>
