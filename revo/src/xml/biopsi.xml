<vortaro>
<art mrk="$Id: biopsi.xml,v 1.7 2015/11/12 06:10:17 revo Exp $">
<kap>
  <rad>biopsi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="biopsi.0o">
  <kap><tld/>o</kap>
  <snc mrk="biopsi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malgranda eltran&ccirc;a&jcirc;o de organo
      por mikroskopa esploro:
      <ekz>
        en sep monatoj la kanceroj tute malaperis,
        kaj la <tld/>oj renormali&gcirc;is
        <fnt>
          <aut>W. Lee, trad. L. De Cooman</aut>,
          <vrk><url
          ref="http://www.eventoj.hu/steb/medicino/angiogenezo.htm">
          Angiogenezo</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_i;&c_o;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">bi&ograve;psia</trd>
  <trd lng="cs">biopsie</trd>
  <trd lng="de">Biopsie</trd>
  <trd lng="en">biopsy</trd>
  <trd lng="fr">biopsie</trd>
  <trd lng="pl">biopsja</trd>
  <trd lng="sk">biopsia</trd>
</drv>
</art>

