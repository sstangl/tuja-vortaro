<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elim.xml,v 1.17 2015/10/09 15:10:17 revo Exp $">
<kap>
<rad>elim</rad>/o
</kap>
<drv mrk="elim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Elymus</trd>):
      <ekz>
        estas menciita nur unu arbospecio (la kverko) kaj du
        plantospecioj, nome stelario kaj <tld/>o, la unua kiel
        brula&jcirc;o,
        kaj la dua kiel fura&gcirc;o por la &ccirc;evaloj
        <fnt><aut>Garbhan MACAOIDH</aut>,
        <vrk><url ref="http://www.esperanto.be/fel/2004/008323.html">
        Le&gcirc;oj landon prosperigas</url></vrk>, <bib>Monato</bib>, 
        <lok>2004/7-8, p. 27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Quecke</trd>
  <trd lng="en">Elymus</trd>
  <trd lng="fr">&eacute;lymus</trd>
  <trdgrp lng="hu">
    <trd>tarackb&uacute;za</trd>,
    <trd>hajperje</trd>
  </trdgrp>
  <trd lng="nl">kweekgras</trd>
  <trd lng="pl">wydmuchrzyca</trd>
  <trd lng="pt">elim&iacute;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_n;&c_ja;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_s;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
<drv mrk="elim.hunda0o">
   <kap>hunda <tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
   <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> de <tld/>o (<trd
      lng="la">Elymus caninus</trd>).
    </dif>
    <ref tip="sin" cel="herb.hund0o">hundherbo</ref>
    </snc>
    <trdgrp lng="en">
  <trd>bearded wheatgrass</trd>,
  <trd>bearded couch grass</trd>
</trdgrp>
</drv>
<drv mrk="elim.sabla0o">
   <kap>sabla <tld/>o</kap>
     <uzo tip="fak">BOT</uzo>
     <snc>
   <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> de <tld/>o (<trd
      lng="la">Elymus arenosus</trd>).
    </dif>
    </snc>
    <trdgrp lng="en">
  <trd>Leymus arenarius</trd>,
  <trd>Elymus arenarius</trd>, <trd>sand ryegrass</trd>
</trdgrp>
</drv>
</art>
