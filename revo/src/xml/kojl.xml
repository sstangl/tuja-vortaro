<vortaro>
<art mrk="$Id: kojl.xml,v 1.15 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kojl</rad>/o
</kap>
<drv mrk="kojl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la dika intesto malanta&ubreve; la cekumo.  
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_b;&c_o;&c_d;&c_n;&c_o; &c_ch;&c_je;&c_r;&c_v;&c_o;</trd>
  <trd lng="ca">c&ograve;lon</trd>
  <trdgrp lng="cs">
    <trd>dutina břišní</trd>,
    <trd>tračník</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kolon</trd>,
     <trd>Grimmdarm</trd>
  </trdgrp>
  <trd lng="en">colon</trd>
  <trd lng="es">colon</trd>
  <trd lng="fr">c&ocirc;lon</trd>
  <trd lng="hu">vastagb&eacute;l</trd>
  <trd lng="it">colon</trd>
  <trdgrp lng="nl">
    <trd>karteldarm</trd>,
   <trd>colon<klr tip="ind"> (karteldarm)</klr></trd>
  </trdgrp>
  <trd lng="pl">okr&eogonek;&zdot;nica</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_d;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>brušná dutina</trd>,
    <trd>tračník</trd>
  </trdgrp>
</drv>
</art>
