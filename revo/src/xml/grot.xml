<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grot.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>grot</rad>/o
</kap>
<drv mrk="grot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Artefarita a&ubreve; kurioza kaverno.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_t;</trd>
  <trd lng="bg">&c_i;&c_z;&c_k;&c_u;&c_s;&c_t;&c_v;&c_je;&c_n;&c_a; <ind>&c_p;&c_je;&c_shch;&c_je;&c_r;&c_a;</ind></trd>
  <trd lng="ca">gruta</trd>
  <trdgrp lng="cs">
    <trd>grota</trd>,
    <trd>jeskyně</trd>,
    <trd>sluj</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Grotte</trd>,
     <trd>Felsenh&ouml;hle</trd></trdgrp>
  <trd lng="en">grotto</trd>
  <trd lng="es">gruta</trd>
  <trd lng="fr">grotte</trd>
  <trdgrp lng="hu">
     <trd>barlang</trd>,
     <trd>grotta</trd>
  </trdgrp>
  <trd lng="nl">grot</trd>
  <trd lng="pt">gruta</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_t;</trd>
  <trd lng="sk">jaskyňa</trd>
</drv>
</art>
