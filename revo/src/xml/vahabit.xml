<kap>
  <rad>vahabit</rad>/o
</kap>
<drv mrk="vahabit.0o">
  <kap><tld/>o</kap>
  <snc mrk="vahabit.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <ref tip="dif" cel="vahabism.0ano.ISL">vahabismano</ref>
      <ekz>
        <tld/>oj akceptas la Koranon kaj la Haditojn, kaj
        anka&ubreve; kelkajn komentajn verkojn pri ili
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Vahabismo">
          Vahabismo</url>, 2011-05-17</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_h;&c_a;&c_b;&c_ib;&c_t;</trd>
  <trd lng="ca">wahhabita</trd>
  <trd lng="fr">wahhabite<klr> (subst.)</klr></trd>
</drv>
</art>

