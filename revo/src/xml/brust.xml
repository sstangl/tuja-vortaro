<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brust.xml,v 1.25 2015/10/06 11:10:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brust</rad>/o
</kap>
<drv mrk="brust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la korpo, ekde kolo &gcirc;is ventro, enhavanta la pulmojn k la
      koron.
    </dif>
    <refgrp tip="vid">
      <ref cel="bust.0o">busto</ref>,
      <ref cel="sin.0o">sino</ref>,
      <ref cel="tali.0o">talio</ref>,
      <ref cel="tors.0o">torso</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;</trd>
  <trd lng="br">bruched</trd>
  <trdgrp lng="cs">
    <trd>hru&dcaron;</trd>,
    <trd>poprs&iacute;</trd>,
    <trd>prsa</trd>,
    <trd>&ncaron;adra</trd>
  </trdgrp>
  <trd lng="de">Brust</trd>
  <trdgrp lng="en">
    <trd>breast</trd>,
    <trd>chest</trd>
  </trdgrp>
  <trd lng="es">pecho</trd>
  <trd lng="fr">poitrine</trd>
  <trd lng="he">&het;&zayin;&he;</trd>
  <trd lng="hu">mell</trd>
  <trd lng="id">dada</trd>
  <trd lng="it">petto<klr> (anat.)</klr></trd>
  <trd lng="nl">borst</trd>
  <trd lng="pl">pier&sacute;</trd>
  <trd lng="pt">peito</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_d;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hru&dcaron;</trd>,
    <trd>hru&dcaron;</trd>,
    <trd>poprsie</trd>,
    <trd>prsia</trd>,
    <trd>&ncaron;adr&aacute;</trd>
  </trdgrp>
  <trd lng="tr">g&ouml;&gbreve;&uuml;s</trd>
</drv>

<drv mrk="brust.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;</trd>    
  <trd lng="br">bruched<klr tip="ind"> (boued)</klr></trd>
  <trd lng="es">pechuga</trd>
  <trd lng="fr">poitrine<klr tip="ind"> (viande)</klr></trd>
  <trd lng="he">&het;&zayin;&he;<klr tip="ind"> (&bet;&shin;&resh;)</klr></trd>
  <trdgrp lng="hu">
    <trd>melle h&uacute;sa</trd>,
    <trd>szegy</trd>
  </trdgrp>
  <trd lng="it">petto<klr> (alim.)</klr></trd>
  <trd lng="pl">pier&sacute;</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_d;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="brust.cxe0ocxe0ajxo">
  <kap>&ccirc;e<tld/>o, <var><kap>&ccirc;e<tld/>a&jcirc;o</kap></var></kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Parto de vesto, situanta &ccirc;e la <tld/>o:
      <ekz>
        Levio ser&ccirc;is en la &ccirc;e<tld/>o kaj eligis pergamenrulon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 26</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estis vestita per vesto tre simila al tiu, kiun portis
        la Betra&hcirc;anino, krom tio, ke &gcirc;i ne havis broditan
        kolombon sur la &ccirc;e<tld/>a&jcirc;o
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>                                                       
  <trd lng="br">bruched<klr tip="ind"> (dilhad)</klr></trd>
  <trd lng="fr">poitrine<klr tip="ind"> (v&ecirc;tement)</klr></trd>
  <trd lng="it">petto<klr> (abigl.)</klr></trd>
</drv>
</art>
