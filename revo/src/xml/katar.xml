<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katar.xml,v 1.23 2015/11/20 07:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="katar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="katar.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Muka fluo pro <ref tip="lst" cel="inflam.0o"
      lst="voko:malsanoj">inflamo</ref> de muka membrano:
      <ekz>
        <tld/>o de l' nazo, de l' intesto, ktp.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="varm.mal0umi">malvarmumi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_l;&c_n;&c_o; &c_v;&c_malmol;&c_z;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>katar</trd>,
    <trd>z&aacute;n&ecaron;t sliznice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Katarrh</trd>,
    <trd>Schleimhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">catarrh</trd>
  <trd lng="es">catarro</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_r;&f_y;&a_z;&a_sh;</trd>
  <trd lng="fr">catarrhe<klr tip="ind"> (de muqueuse)</klr></trd>
  <trd lng="hu">hurut</trd>
  <trd lng="nl">catarre</trd>
  <trd lng="pl">katar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_o;&c_j; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>katar</trd>,
    <trd>z&aacute;pal sliznice</trd>
  </trdgrp>
</drv>

<drv mrk="katar.naz0o">
  <kap>naz<tld/>o</kap>
  <snc mrk="katar.naz0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="K"/>o de la nazo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_r;&c_je;&c_m;&c_a;</trd>
  <trd lng="br">friad<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
  <trd lng="ca">rinitis</trd>
  <trdgrp lng="cs">
    <trd>katar nosn&iacute; sliznice</trd>,
    <trd>r&yacute;ma</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnupfen</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&f_k;&a_A;&a_m;</trd>,
    <trd>&a_A_madda;&a_b;&a_r;&f_y;&a_z;&a_sh; &a_b;&f_y;&a_n;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coryza</trd>,    
    <trd>rhinite</trd>, 
    <trd>rhume de cerveau</trd>
  </trdgrp>
  <trd lng="hu">n&aacute;tha</trd>
  <trd lng="nl">verkoudheid</trd>
  <trdgrp lng="pl">
    <trd>katar</trd>,
    <trd>nie&zdot;yt nosa</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_m;&c_o;&c_r;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>katar nosnej sliznice</trd>,
    <trd>n&aacute;dcha</trd>
  </trdgrp>
</drv>
</art>
