<drv mrk="fal.dis0i">
  <kap><ofc>*</ofc>dis<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fal.dis0i.KOMUNE">
    <dif>
      Diseri&gcirc;ante ruini&gcirc;i, perei:
      <ekz>
	pro la tertremo multaj domoj dis<tld/>is;
      </ekz>
      <ekz>
	<ind>de guto post guto dis<tld/>as granito</ind>
	<trd lng="pl">kropla dr&aogonek;&zdot;y ska&lstroke;&eogonek;
	</trd>
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
	la kadavro jam dis<tld/>is en putra&jcirc;on;
      </ekz>
      <ekz>
	dis<tld/>o de societo, entrepreno, regno;
      </ekz>
      <ekz>
	ni devas eviti la dis<tld/>on de la lingvo en
	dialektojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argume.0o">argumento</ref>,
      <ref cel="er.dis0igxi">diseri&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;
    </trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hroutit se</trd>,
    <trd>rozpadnout se</trd>,
    <trd>sesout se</trd>,
    <trd>spadnout</trd>,
    <trd>z&rcaron;&iacute;tit se <klr>(stavba)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zerfallen</trd>,
    <trd>auseinander fallen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;composer</ind></trd>,
    <trd>se <ind>d&eacute;sagr&eacute;ger</ind></trd>,
    <trd>se <ind>disloquer</ind></trd>,
    <trd>se <ind>morceler</ind></trd>,
    <trd>tomber en <ind>morceau</ind>x</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tesik</trd>,
    <trd>sz&eacute;thullik</trd>
  </trdgrp>
  <trd lng="nl">uiteenvallen</trd>
  <trd lng="pl">rozpada&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;
    </trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozpadn&uacute;&tcaron; sa</trd>,
    <trd>zr&uacute;ti&tcaron; sa</trd>
  </trdgrp>
  <trd lng="tp">pakala</trd>
</drv>

<drv mrk="fal.el0i">
  <kap><ofc>*</ofc>el<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fal.el0i.KOMUNE">
    <dif>
      <tld lit="F"/> i el la interno de io:
      <ekz>
	dum &scirc;i parolis, el<tld/>is el &scirc;ia bu&scirc;o multego
	da diamantoj
	<fnt><bib>F</bib><lok>&FE; 17, la feino</lok></fnt>;
      </ekz>
      <ekz>
	nuksoj venis, kiam dentoj el<tld/>is
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
	se &ccirc;e iu el<tld/>is la haroj sur la kapo <klr>[...]</klr>
	<fnt><bib>MT</bib><lok>&Lev; 13:14</lok></fnt>;
      </ekz>
      <ekz>
	A&hcirc;azja el<tld/>is tra la krado de sia supra &ccirc;ambro
	en Samario, kaj malsani&gcirc;is
	<fnt><bib>MT</bib><lok>&Re2; 1:2</lok></fnt>;
      </ekz>
      <ekz>
	anta&ubreve; ol venis al &scirc;i la
	aku&scirc;i&gcirc;aj suferoj, &scirc;i el<tld/>igis filon
	<fnt><bib>MT</bib><lok>&Jes; 66:7</lok></fnt>;
      </ekz>
      <ekz>
	mi el<tld/>igos viajn sagojn el via dekstra mano
	<fnt><bib>MT</bib><lok>&Jeh; 39:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vypadnout</trd>,
    <trd>vypad&aacute;vat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>herausfallen</trd>
  </trdgrp>
  <trd lng="fr">tomber<klr tip="ind"> (de l'int&eacute;rieur de)</klr>
  </trd>
  <trdgrp lng="hu">
    <trd>kiesik</trd>,
    <trd>kihullik</trd>
  </trdgrp>
  <trd lng="nl">uit iets <ind>vallen</ind></trd>
  <trd lng="pl">wypada&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vypadn&uacute;&tcaron;</trd>
</drv>

<drv mrk="fal.en0i">
  <kap><ofc>*</ofc>en<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i en ion profundan; penetri <tld/>ante:
      <ekz>
	en<tld/>i en kava&jcirc;on, truon, riveron;
      </ekz>
      <ekz>
	sablero en<tld/>is en mian okulon
	<fnt><bib>F</bib><lok>&FE; 41</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">beleesik</trd>
    <trd lng="pl">wpada&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;
      </trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="F"/>i <sncref ref="fal.0i.en_ion"/>:
      <ekz>
	<ind>lupo kaptas, sed li anka&ubreve; en<tld/>as</ind>
	<trd lng="pl">nosi&lstroke; wilk razy kilka, ponie&sacute;li i
	wilka</trd>
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
	en<tld/>i en malsanon<fnt>Z</fnt>,
	dan&gcirc;eron, mizeron<fnt>Z</fnt>,
	malesperon<fnt>Z</fnt>,
	deklamadon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>beleesik</trd>,
      <trd>ker&uuml;l<klr tip="ind"> (vhova)</klr></trd>,
      <trd>jut<klr tip="ind"> (vhova)</klr></trd>
    </trdgrp>
    <trd lng="nl">vervallen</trd>
    <trdgrp lng="pl">
      <trd>wpada&cacute;<klr> (w rozpacz)</klr></trd>,
      <trd>popada&cacute;<klr> (w rozpacz)</klr></trd>,
      <trd>zapada&cacute;<klr> (na zdrowiu)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Rapide sin &jcirc;eti en ion:
      <ekz>
	la knaboj tumulte en<tld/>is en la salonon.
      </ekz>
    </dif>
    <trd lng="hu">beleveti mag&aacute;t</trd>
    <refgrp tip="vid">
      <ref cel="entrep.0i">entrepreni</ref>,
      <ref cel="invad.0i">invadi</ref>.
    </refgrp>
    <trd lng="nl">binnenvallen</trd>
    <trd lng="pl">wpada&cacute;</trd>
    <trd lng="ru">&c_v;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      <klr>(pri surfaco de io)</klr>
      Kavi&gcirc;i pro manko de subteno:
      <ekz>
	<ind>en<tld/>inta brusto</ind><fnt>B</fnt>;
	<trd lng="hu">behorpadt mell</trd>
	<trd lng="pl">zapadni&eogonek;ta klatka piersiowa</trd>
	<trd lng="ru">
	&c_v;&c_p;&c_a;&c_l;&c_a;&c_ja; &c_g;&c_r;&c_u;&c_d;&c_mol;
	</trd>
      </ekz>
      <ekz>
	&scirc;iaj nigraj, belaj, iom <ind>en<tld/>intaj okuloj</ind>
	<trd lng="pl">zapadni&eogonek;te oczy</trd>
	<fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>beesik</trd>,
      <trd>behorpad</trd>
    </trdgrp>
    <trd lng="nl">invallen</trd>
    <trd lng="pl">zapada&cacute; <klr>si&eogonek;</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">vpadnout</trd>
  <trdgrp lng="fr">
    <trd>tomber</trd>,
    <trd>s'<ind>affaisser</ind></trd>,
    <trd>se <ind>creuser</ind></trd>,
    <trd>d&eacute;bouler<klr tip="ind"> (dans un lieu)</klr></trd>,
    <trd>envahir</trd>,
    <trd>foncer<klr tip="ind"> (dans un lieu)</klr></trd>,
    <trd>se <ind>jeter</ind><klr tip="ind"> (dans un lieu)</klr></trd>,
    <trd>se <ind>pr&eacute;cipiter</ind><klr tip="ind"> (dans un
lieu)</klr></trd>,
    <trd>se <ind>ruer</ind><klr tip="ind"> (dans un lieu)</klr></trd>
  </trdgrp>
  <trd lng="sk">padn&uacute;&tcaron; do nie&ccaron;oho</trd>
</drv>

<drv mrk="fal.en0ilo">
  <kap>en<tld/>ilo<fnt>Z</fnt>,
  <var><kap>en<tld/>ejo</kap></var>,
  <var><kap>en<tld/>ujo<fnt>Z</fnt></kap></var></kap>
  <snc mrk="fal.en0ilo.CXAS">
    <dif>
      <ref tip="super" cel="kapt.0ilo">Kaptilo</ref> konsistanta el
      ka&scirc;ita fosa&jcirc;o, en kiun iu en<tld/>os.
    </dif>
    <refgrp tip="sin">
      <ref cel="fal.en0ilo">en<tld/>ejo</ref>,
      <ref cel="fal.en0ilo">en<tld/>ujo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="cs">past</trd>
  <trdgrp lng="fr">
    <trd>fosse<klr tip="ind"> (pi&egrave;ge)</klr></trd>,
    <trd>pi&egrave;ge<klr tip="ind"> (fosse)</klr></trd>
  </trdgrp>
  <trd lng="hu">csapda</trd>
  <trd lng="nl">val<klr tip="ind"> (v..dieren bijv.)</klr></trd>
  <trdgrp lng="pl">
    <trd>zapadnia</trd>,
    <trd>wilczy d&oacute;&lstroke;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_d;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pasca</trd>
</drv>

<drv mrk="fal.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fal.en0igi.KOMUNE">
    <dif>
      <tld lit="F"/>igi en kava&jcirc;on, en profunda&jcirc;on;
      en malbonan situacion:
      <ekz>
        Dio povas forpreni kiel anka&ubreve; doni la ri&ccirc;a&jcirc;ojn, se ni ne helpos 
        al la malri&ccirc;uloj, kiujn Li en<tld/>igis en la staton de 
        mizeruloj
        <fnt>
          <aut>-, trad. G. Cox</aut>,
          <vrk><url
          ref="http://cindymckee.com/librejo/Blindulo_baba_abdala.pdf">
          La Blindulo Baba Abdala</url></vrk>,
          <lok>[1908?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire tomber</trd>,
    <trd>pi&eacute;ger</trd>
  </trdgrp>
</drv>

<drv mrk="fal.0pusxigxi">
  <kap><tld/>pu&scirc;i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Renkonti obstaklon per la piedo kaj tial perdi
      la ekvilibron; <klr>(metafore)</klr> peki:
      <ekz>
	sur la manoj ili vin portos,
	por ke vi ne <tld/>pu&scirc;i&gcirc;u sur &scirc;tono per via
	piedo
	<fnt><bib>MT</bib><lok>&Psa; 91:12</lok></fnt>;
      </ekz>
      <ekz>
	la vojo de malpiuloj estas kiel mallumo:
	ili ne scias, je kio ili <tld/>pu&scirc;i&gcirc;os
	<fnt><bib>MT</bib><lok>&Sen; 4:19</lok></fnt>;
      </ekz>
      <ekz>
	se iu ne <tld/>pu&scirc;i&gcirc;as parole, tiu estas perfekta
	homo, kapabla bridi anka&ubreve; la tutan korpon
	<fnt><bib>NT</bib><lok>&Jak; 3:1</lok></fnt>;
      </ekz>
      <ekz>
	mi metos anta&ubreve; &ccirc;i tiu popolo <tld/>pu&scirc;ilon,
	kaj sur &gcirc;i <tld/>pu&scirc;i&gcirc;os kune patroj kaj filoj
	<fnt><bib>MT</bib><lok>&Jer; 6:21</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Preska&ubreve;a sinonimo:
      <ref cel="fal.0eti">faleti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_t;&c_y;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>achopper<klr tip="ind"> (buter du pied)</klr></trd>,
    <trd>buter</trd>,
    <trd>faillir<klr tip="ind"> (commettre un faute)</klr></trd>,
    <trd>heurter</trd>,
    <trd>tr&eacute;bucher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elbotlik</trd>,
    <trd>megbotlik</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>stoten</ind></trd>
  <trdgrp lng="pl">
    <trd>potyka&cacute; si&eogonek;</trd>,
    <trd>potkn&aogonek;&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;
    </trd>,
    <trd>&c_s;&c_p;&c_o;&c_t;&c_y;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    </trd>
  </trdgrp>
</drv>

<drv mrk="fal.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fal.for0i.KOMUNE">
    <dif>
      <tld lit="F"/>i kaj malaperi, <tld/>i kaj perei:
      <ekz>
	la esperojn de mia vivo mi vidis for<tld/>i en la
	abismon<fnt>Z</fnt>;
      </ekz>
      <ekz>
	mi pensis pri tiuj miloj da kadavroj for<tld/>intaj sur la
	batalkampo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
 
<trd>&c_a;&c_d;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odpadnout</trd>,
    <trd>padnout v boji</trd>,
    <trd>zapadnout</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>ab&icirc;mer</ind><klr tip="ind">
  (tomber, dispara&icirc;tre)</klr></trd>
  <trdgrp lng="hu">
    <trd>elbukik</trd>,
    <trd>elhullik</trd>
  </trdgrp>
  <trd lng="nl">wegvallen</trd>
  <trdgrp lng="pl">
    <trd>polega&cacute; <klr>(np. w walce)</klr></trd>,
    <trd>polec <klr>(np. w walce)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odpadn&uacute;&tcaron;</trd>,
    <trd>padn&uacute;&tcaron; v boji</trd>
  </trdgrp>
</drv>

<drv mrk="fal.piedbare0igi">
  <kap>piedbare <tld/>igi</kap>
  <snc mrk="fal.piedbare0igi.KOMUNE">
    <dif>
      <tld lit="F"/>igi iun &scirc;ovante sian piedon anta&ubreve; lian pa&scirc;on:
      <ekz>
        tio ne okazis honeste. Li piedbare <tld/>igis vin
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url
          ref="http://www.gutenberg.org/files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">podtrhnout nastaven&iacute;m nohy</trd>
  <trd lng="fr">faire un <ind>croc-en-jambe</ind></trd>
  <trd lng="sk">podtrhn&uacute;&tcaron; nastaven&iacute;m nohy</trd>
</drv>

<drv mrk="fal.rul0i">
  <kap>rul<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fal.rul0i.KOMUNE">
    <dif>
      Ruli&gcirc;i la&ubreve; deklivo malsupren:
      <ekz>
	la sono de rul<tld/>anta &scirc;tono
	<fnt><bib>Far2</bib><lok>&ccirc;apitro 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>skut&aacute;let se</trd>,
    <trd>valit se</trd>,
    <trd>&rcaron;&iacute;tit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hinunterrollen</trd>,
    <trd>purzeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rouler<klr tip="ind"> (vers le bas)</klr></trd>,
    <trd>d&eacute;bouler<klr tip="ind"> (rouler vers le bas)</klr>
    </trd>,
    <trd>d&eacute;gringoler<klr tip="ind"> (rouler vers le bas)</klr>
    </trd>
  </trdgrp>
  <trd lng="hu">felbukfencezik</trd>
  <trd lng="id">[<ind>gelinding</ind>] menggelinding</trd>
  <trd lng="pl">stacza&cacute; si&eogonek;</trd>
  <trd lng="sk">kot&uacute;&lcaron;avo pada&tcaron;</trd>
</drv>

<drv mrk="fal.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fleksi&gcirc;i, mallevi&gcirc;i sub io tro peza:
      <ekz>
	la bran&ccirc;oj sub<tld/>is sub la ne&gcirc;o.
      </ekz>
    </dif>
    <trd lng="hu">al&aacute;ker&uuml;l</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne kapabli kontra&ubreve;stari al, cedi al, esti subpremita,
      venkita de:
      <ekz>
	li sub<tld/>is sub la malfeli&ccirc;o;
      </ekz>
      <ekz>
	mi ne sub<tld/>is al la malbona sorto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&ouml;sszeroskad</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">padnout pod</trd>
  <trd lng="fr">ployer<klr tip="ind"> (intr)</klr></trd>
  <trd lng="nl">vallen<klr tip="ind"> (door druk van iets)</klr></trd>
  <trdgrp lng="pl">
    <trd>upada&cacute; <klr>(pod ci&eogonek;&zdot;arem ...)</klr></trd>,
    <trd>ugina&cacute; si&eogonek; <klr>(pod ci&eogonek;&zdot;arem ...)
    </klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klesn&uacute;&tcaron;</trd>,
    <trd>podda&tcaron; sa</trd>,
    <trd>pod&lcaron;ahn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fal.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fal.super0i.KOMUNE">
    <ref tip="dif" cel="fal.sur0i.KOMUNE">sur<tld/>i</ref>
    <ekz>
      teruro super<tld/>os lin kiel akvo
      <fnt><bib>MT</bib><lok>&Ijo; 27:20</lok></fnt>;
    </ekz>
    <ekz>
      dum la bovoj estis plugantaj <klr>[...]</klr>
      super<tld/>is ilin la &Scirc;ebaanoj, kaj forprenis ilin
      <fnt><bib>MT</bib><lok>&Ijo; 1:15</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">p&rcaron;epadnout <klr>(p&rcaron;es)</klr></trd>
  <trdgrp lng="fr">
    <trd>s'<ind>abattre</ind><klr tip="ind"> (sur qn)</klr></trd>,
    <trd>assaillir</trd>,
    <trd>attaquer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rajta&uuml;t</trd>,
    <trd>megt&aacute;mad</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanvallen</trd>,
    <trd>overvallen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spada&cacute; <klr>na</klr></trd>,
    <trd>napada&cacute; <klr>na</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">prepadn&uacute;&tcaron; <klr>(cez)</klr></trd>
</drv>

<drv mrk="fal.sur0i">
