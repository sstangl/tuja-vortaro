    <trd>straucheln</trd>,
    <trd>stolpern</trd>
  </trdgrp>
  <trd lng="en">stumble</trd>
  <trdgrp lng="fr">
    <trd>broncher<klr tip="ind"> (tr&eacute;bucher)</klr></trd>,
    <trd>tr&eacute;bucher</trd>
  </trdgrp>
  <trd lng="hu">megbotlik</trd>
  <trd lng="id">[<ind>sandung</ind>] tersandung</trd>
  <trd lng="nl">struikelen</trd>
