  <trd lng="hu">hullad&eacute;k</trd>
  <trd lng="id">limbah</trd>
  <trd lng="nl">afval</trd>
  <trd lng="pl">odpadek</trd>
  <trd lng="ru">&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_y;</trd>
