<drv mrk="fal.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Renversi, igi <tld/>inta:
      <ekz>
	<tld/>igi arbon, muron, infanon;
      </ekz>
      <ekz>
	unu hako kverkon ne <tld/>igas
	<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
	Esperanto <tld/>igas la lingvobarojn;
      </ekz>
      <ekz>
	la krizo <tld/>igis agrikulturon en malbonan situacion.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>feld&ouml;nt</trd>,
      <trd>led&ouml;nt</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przewraca&cacute;</trd>,
      <trd>powala&cacute;</trd>,
      <trd>zwala&cacute;</trd>,
      <trd>obala&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Lasi <tld/>i:
      <ekz>
	<uzo tip="fak">MAR</uzo>
	<ind><tld/>igi ankron</ind>;
	<trd lng="fr">jeter l'<ind>ancre</ind></trd>
	<trd lng="hu">horgonyt <ind>vet</ind></trd>
	<trd lng="nl">het <ind>anker</ind> uitwerpen</trd>
	<trd lng="pl"><ind>rzuca&cacute;</ind> kotwic&eogonek;</trd>
	<trd lng="ru">
	&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;
	&c_ja;&c_k;&c_o;&c_r;&c_mol;
	</trd>
      </ekz>
      <ekz>
	senespere <ind><tld/>igi la brakojn</ind>.
	<trdgrp lng="fr">
	  <trd><ind>baisser</ind> les bras</trd>,
	  <trd>perdre <ind>courage</ind></trd>
	</trdgrp>
	<trd lng="pl">opuszcza&cacute; r&eogonek;ce<klr>
	(np. z bezsilno&sacute;ci)</klr></trd>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>leenged</trd>,
      <trd>leereszt</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      De<tld/>igi ion, pluvigi, &scirc;uti:
      <ekz>
	<tld/>igi pomojn de arbo;
      </ekz>
      <ekz>
	<tld/>igi kuglojn sur la malamikojn.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>ler&aacute;z</trd>,
      <trd>z&aacute;poroz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zwala&cacute;</trd>,
      <trd>spycha&cacute;</trd>,
      <trd>zepchn&aogonek;&cacute;</trd>,
      <trd>zrzuca&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&aacute;cet</trd>,
    <trd>porazit</trd>,
    <trd>por&aacute;&zcaron;et</trd>,
    <trd>povalit</trd>,
    <trd>sk&aacute;cet</trd>,
    <trd>svalit</trd>,
    <trd>upustit</trd>,
    <trd>vyvracet</trd>,
    <trd>vyvr&aacute;tit</trd>,
    <trd>zbavit moci</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>tomber</ind></trd>,
    <trd>jeter<klr tip="ind"> (l&acirc;cher)</klr></trd>,
    <trd>l&acirc;cher<klr tip="ind"> (ne plus emp&ecirc;cher la chute)
    </klr></trd>,
    <trd>faire <ind>pleuvoir</ind><klr tip="ind">
    (des balles, des ennuis)</klr></trd>,
    <trd>renverser<klr tip="ind"> (faire tomber)</klr></trd>
  </trdgrp>
  <trd lng="id">[<ind>jatuh</ind>] menjatuhkan</trd>
  <trdgrp lng="nl">
    <trd>doen <ind>vallen</ind></trd>,
    <trd>vellen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zrazi&tcaron;</trd>,
    <trd>zvali&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">d&uuml;&scedil;&uuml;rmek</trd>
</drv>

