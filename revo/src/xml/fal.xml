</drv>

<drv mrk="fal.sxtonode0pusxigxo">
  <kap>&scirc;tono de <tld/>pu&scirc;i&gcirc;o<fnt><bib>PrV</bib></fnt></kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="fal.sxtonode0pusxigxo.KOMUNE">
    <ref tip="dif" cel="stumbl.0ilo.KOMUNE">stumblilo</ref>
    <ekz>
      la unua &scirc;tono de <tld/>pu&scirc;i&gcirc;o kiun mi renkontis estis
      <klr>[la vorto]</klr> <ctl>pliko</ctl>
        <fnt>
          <aut>P. Desmet'</aut>,
          <vrk><url
          ref="http://www.ipernity.com/blog/petro_desmet/135911">
          Fundamentaj &Scirc;tonoj de Falpu&scirc;i&gcirc;o!</url></vrk>,
          <lok>2009-03-13</lok>
        </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">pierre d'achoppement</trd>
  <trd lng="hu">buktat&oacute;</trd>
  <trd lng="nl">steen des aanstoots</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol; &c_p;&c_r;&c_je;&c_t;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="fal.veterdis0o">
  <kap>veterdis<tld/>o</kap>
  <snc mrk="fal.veterdis0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Efiko de erozio, &hcirc;emiaj reakcioj kaj viva&jcirc;oj sur
      &scirc;tonoj, ka&ubreve;zanta supra&jcirc;an diseri&gcirc;on kaj kolor&scirc;an&gcirc;on:
      <ekz>
        bakterioj &ccirc;eestantaj en la tero agas kiel katalizilo en la 
        procezo de veterdis<tld/>o de la petroj
        <fnt>
          <aut>Kvantumo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/kvantumo/291771">
          Hipotezo de Gaja...</url></vrk>,
          <lok>2010-11-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">weathering</trd>
  <trd lng="fr">m&eacute;t&eacute;orisation</trd>
  <trd lng="it">meteorizzazione</trd>
</drv>
