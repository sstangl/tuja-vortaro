<drv mrk="fal.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i de loko, kie oni pendas:
      <ekz>
	la folioj<fnt>Z</fnt>, fruktoj, haroj, dentoj de<tld/>as;
      </ekz>
      <ekz>
	montgrimpante ili de<tld/>is de deklivo;
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>herabfallen</trd>,
      <trd>ausfallen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>leesik</trd>,
      <trd>lehull</trd>
    </trdgrp>
    <trd lng="nl">afvallen<klr tip="ind"> (bladeren bijv.)</klr></trd>
    <trdgrp lng="pl">
      <trd>spada&cacute;</trd>,
      <trd>odpada&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_s;&c_t;&c_mol; <klr>
      (&c_o; &c_z;&c_u;&c_b;&c_a;&c_h;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="F"/>i <sncref ref="fal.0i.en_ion"/> de tre favora
      situacio en malfavoran:
      <ekz>
	de<tld/>i de siaj esperoj, pretendoj;
      </ekz>
      <ekz>
	la prospereco de la regno multe de<tld/>is;
      </ekz>
      <ekz>
	la <ind>de<tld/>o</ind> de la romana imperio estis malrapida.
	<trdgrp lng="fr">
	  <trd>d&eacute;ch&eacute;ance</trd>,
	  <trd>d&eacute;clin</trd>
	</trdgrp>
	<trdgrp lng="hu">
	  <trd>buk&aacute;s</trd>,
	  <trd>hanyatl&aacute;s</trd>
	</trdgrp>
	<trd lng="nl">val<klr tip="ind"> (v.e.rijk bijv.)</klr></trd>
	<trd lng="pl">upadek</trd>
      </ekz>
      <ekz>
	poste vualo de<tld/>is de miaj okuloj
	<fnt>
	  <aut>L. Lukina, E. Lukin, trad. J. Finkel</aut>,
	  <vrk>La misiistoj</vrk>,
	  <lok>2000</lok>
	</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_n;&c_ja;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trdgrp lng="hu">
      <trd>bukik</trd>,
      <trd>hanyatlik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>popada&cacute; <klr>
      (np. w nie&lstroke;ask&eogonek;)</klr></trd>,
      <trd>spada&cacute; <klr>
      (o znaczeniu, wa&zdot;no&sacute;ci)</klr></trd>
      <trd>upada&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_v; <ind>&c_u;&c_p;&c_a;&c_d;&c_o;&c_k;</ind></trd>
    <refgrp tip="vid">
      <ref cel="degene.0i">degeneri</ref>,
      <ref cel="degrad">degradi&gcirc;i</ref>,
      <ref cel="dekade.0i">dekadenci</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sitel.0o">sitelo</ref>.
    </refgrp>
  </snc>
  <snc mrk="fal.de0i.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Malfideli&gcirc;i, apostati&gcirc;i, malani&gcirc;i, forlasi
      partion, deturni&gcirc;i de, ribeli kontra&ubreve;:
      <ekz>
	la Izraelidoj de<tld/>is de la Eternulo<fnt>Z</fnt>;
      </ekz>
      <ekz>
	de<tld/>i de la religio de siaj patroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
	Moab de<tld/>is de Izrael post la morto de A&hcirc;ab
	<fnt><bib>MT</bib>, <lok>&Re2; 1:1</lok></fnt>;
      </ekz>
      <ekz>
	revenu, de<tld/>intaj infanoj!
	<fnt><bib>MT</bib>, <lok>&Jer; 3:14</lok></fnt>;
      </ekz>
      <ekz>
	tiu popolo havas koron de<tld/>eman kaj neobeeman
	<fnt><bib>MT</bib>, <lok>&Jer; 5:23</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>elszakad</trd>,
      <trd>elp&aacute;rtol</trd>,
      <trd>h&udblac;tlenn&eacute; v&aacute;lik</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
    <refgrp tip="vid">
      <ref cel="voj.de0igxi.KOMUNE">devoji&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="cs">odpadnout</trd>
  <trdgrp lng="fr">
    <trd>tomber<klr tip="ind"> (de l&agrave; o&ugrave; l'on pendait)
    </klr></trd>,
    <trd>d&eacute;chanter</trd>,
    <trd>d&eacute;cliner<klr tip="ind">
    (perdre une position avantageuse)</klr></trd>,
    <trd><ind>tomber</ind> de haut<klr tip="ind">
    (d&eacute;chanter)</klr></trd>,
    <trd><ind>renier</ind> sa foi</trd>,
    <trd>revenir<klr tip="ind"> (de ses illusions)</klr></trd>
  </trdgrp>
  <trd lng="nl">afvallen<klr tip="ind"> (v.e.godsdienst)</klr></trd>
  <trd lng="sk">poklesn&uacute;&tcaron;</trd>
</drv>

<drv mrk="fal.de0ajxo">
