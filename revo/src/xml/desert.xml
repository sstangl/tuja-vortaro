<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: desert.xml,v 1.20 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>desert</rad>/o
</kap>

<drv mrk="desert.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Lasta parto de e&ubreve;ropaj man&gcirc;oj, konsistanta el
      froma&gcirc;o, fruktoj a&ubreve; dol&ccirc;a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_s;&c_e;&c_r;&c_t;</trd>
  <trd lng="bg">&c_d;&c_je;&c_s;&c_je;&c_r;&c_t;</trd>
  <trd lng="ca">postres</trd>
  <trdgrp lng="cs">
    <trd>dezert</trd>,
    <trd>zákusek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Nachtisch</trd>,
     <trd>Nachspeise</trd>
  </trdgrp>
  <trd lng="en">dessert</trd>
  <trd lng="es">postre</trd>
  <trd lng="fr">dessert</trd>
  <trdgrp lng="hu">
    <trd>desszert</trd>,
    <trd>csemege</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dessert</trd>,
    <trd>nagerecht</trd>
  </trdgrp>
  <trd lng="pl">deser</trd>
  <trd lng="pt">sobremesa</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_je;&c_r;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>dezert</trd>,
    <trd>zákusok</trd>
  </trdgrp>
  <trd lng="tr">tatl&inodot;<klr>(yemek sonunda yenen)</klr></trd>
</drv>
</art>
