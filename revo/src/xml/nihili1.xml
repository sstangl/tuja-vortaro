<kap>
  <ofc>2</ofc>
  <rad>nihilist</rad>/o
</kap>
<drv mrk="nihili1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Adepto de <ref tip="vid" cel="nihili.0o">nihilismo</ref>:
      <ekz>
        <tld/>o estas homo, kiu klinas la kapon anta&ubreve; neniu a&ubreve;toritato
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_g;&c_ib;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">nihilista</trd>
  <trd lng="de">Nihilist</trd>
  <trd lng="el">&my;&eta;&delta;&epsilon;&ny;&jota;&sigma;&tau;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">nihilist</trd>
  <trd lng="es">nihilista</trd>
  <trd lng="fr">nihiliste</trd>
  <trd lng="hu">nihilista</trd>
  <trd lng="nl">nihilist</trd>
  <trd lng="pt">niilista</trd>
  <trd lng="ru">&c_n;&c_i;&c_g;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
