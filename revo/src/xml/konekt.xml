<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konekt.xml,v 1.21 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>9</ofc><rad>konekt</rad>/i
</kap>

<drv mrk="konekt.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">KOMP</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Tiamaniere kunigi, interligi objektojn per ia
      <ref cel="konduk.0ilo.FIZ_ELE">kondukilo</ref>
      a&ubreve; tubo, ke ili funkciu kiel partoj
      de unu tuto (de cirkvito, de komputila a&ubreve; telefona reto ktp.):
      <ekz>
        li <tld/>is la komputilon kun la po&scirc;telefono per kablo;
      </ekz>
      <ekz>
        &ccirc;i tiu kablo <tld/>as partojn de la cirkvito;
      </ekz>
      <ekz>
        la paciento estas <tld/>ita al artefarita reno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komut.0i">komuti</ref>,
      <ref cel="kupl.0i">kupli</ref>,
      <ref cel="munt.0i">munti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>koppeln</trd>,
    <trd>kuppeln</trd>,
    <trd>verbinden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>connecter</trd>,
    <trd>brancher</trd>,
    <trd>raccorder</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatlakoztat</trd>,
    <trd>&ouml;sszekapcsol<klr tip="ind"> (villamosan)</klr></trd>
  </trdgrp>
  <trd lng="nl">verbinden</trd>
  <trd lng="es">conectar</trd>
  <trdgrp lng="pt">
    <trd>conectar</trd>,
    <trd>ligar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; stato esti <tld/>ita:
      <ekz>
        mi havas <klr>(ret)</klr><tld/>on;
      </ekz>
      <ekz>
        <tld/>o mankas;
      </ekz>
      <ekz>
        la <tld/>o perdi&gcirc;is;
      </ekz>
      <ekz>
        a&ubreve;tomata <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Verbindung</trd>
  <trdgrp lng="fr">
    <trd>connexion</trd>,
    <trd>branchement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatlakoz&aacute;s<klr tip="ind"> (villamos)</klr></trd>,
    <trd>&ouml;sszekapcsol&aacute;s<klr tip="ind"> (villamos)</klr></trd>,
    <trd>kapcsolat<klr tip="ind"> (villamos)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbinding</trd>,
    <trd>contact</trd>
  </trdgrp>
  <trd lng="es">conexi&oacute;n</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">KOMP</uzo>
  <uzo tip="fak">TEK</uzo>
  <fnt><bib>PV</bib><lok>&jcirc;ako</lok></fnt>
  <snc>
    <dif>
      Ilo sur objekto, al kiu eblas alfiksi kondukilon
      por <tld/>i kun &gcirc;i aliajn objektojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konektor</trd>,
    <trd>spojovací článek</trd>
  </trdgrp>
  <trd lng="en">connector</trd>
  <trdgrp lng="es">
    <trd>conector</trd>,
    <trd>enchufe</trd>,
    <trd>puerto</trd>
  </trdgrp>
  <trd lng="fr">connecteur</trd>
  <trdgrp lng="nl">
    <trd>stekker</trd>,
    <trd>steker</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_n;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_jo;&c_m;</trd>,
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konektor</trd>,
    <trd>spojovací článok</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.0ite">
  <kap><tld/>ite</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc mrk="konekt.0ite.KOMP">
    <dif>
      <ref tip="super" cel="regxim.0o.TEK">Re&gcirc;imo</ref>
      de komputa sistemo a&ubreve; ties parto (ekz-e desegnilo),
      &ccirc;e kiu &gcirc;ian funkciadon stiras &ccirc;efa komputilo
      (= mala&ubreve;tonome).
    </dif>
    <ref tip="ant" cel="konekt.sen0e">sen<tld/>e</ref>
    <rim>
      Oni anka&ubreve; aplikas tiun vorton al homo uzanta komputilon
      <tld/>itan al la <ref cel="ret.inter0o.la">Interreto</ref>:
      <ekz>
        hiera&ubreve; &scirc;i pasigis la tutan tagon <tld/>ite.
      </ekz>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_n;&c_je;&c_a;&c_w;&c_t;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;&c_m;</ind> &c_r;&c_e;&c_zh;&c_y;&c_m;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_w;&c_t;&c_a;&c_n;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_o;&c_n;-&c_l;&c_a;&c_j;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verbunden</trd>,
    <trd>online</trd>
  </trdgrp>
  <trd lng="en">on-line</trd>
  <trdgrp lng="fr">
    <trd><klr>(en mode) </klr><ind>connect&eacute;</ind></trd>,
    <trd>en <ind>connexion</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csatlakozva</trd>,
    <trd>on-line</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conectado, -a</trd>,
    <trd>en linea</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verbonden</trd>,
    <trd>online</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; <ind>&c_o;&c_p;&c_je;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_m;</ind> &c_r;&c_je;&c_zh;&c_i;&c_m;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_v;&c_t;&c_o;&c_n;&c_o;&c_m;&c_n;&c_o;</trd>,
    <trd>&c_o;&c_n;-&c_l;&c_a;&c_j;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.mal0i">
  <kap><ofc>9</ofc>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">KOMP</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      &Ccirc;esigi, rompi <tld/>on:
      <ekz>
        mal<tld/>i aparaton.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;connecter</trd>,
    <trd>d&eacute;brancher</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desconectar</trd>,
    <trd>desenchufar</trd>
  </trdgrp>
  <trd lng="nl">het <ind>contact</ind> verbreken</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="konekt.sen0e">
  <kap>sen<tld/>e</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc mrk="konekt.sen0e.KOMP">
    <dif>
      <ref tip="super" cel="regxim.0o.TEK">Re&gcirc;imo</ref>
      de aparato funkcianta sendepende je la &ccirc;efa
      komputilo.
    </dif>
    <ref tip="sin" cel="auxton.0a.TEK">a&ubreve;tonome</ref>
    <ref tip="sub" cel="ret.ekster0e">eksterrete</ref>
    <ref tip="ant" cel="konekt.0ite"><tld/>ite</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_a;&c_w;&c_t;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;&c_m;</ind> &c_r;&c_e;&c_zh;&c_y;&c_m;&c_je;</trd>,
    <trd>&c_a;&c_w;&c_t;&c_a;&c_n;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_o;&c_f;-&c_l;&c_a;&c_j;&c_n;</trd>
  </trdgrp>
  <trd lng="en">off-line</trd>
  <trdgrp lng="fr">
    <trd><klr>(en mode) </klr><ind>d&eacute;connect&eacute;</ind></trd>,
    <trd>hors <ind>connexion</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapcsolat n&eacute;lk&uuml;l</trd>,
    <trd>off-line</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aut&oacute;nomamente</trd>,
    <trd>fuera de linea</trd>,
    <trd>sin conexi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>niet <ind>verbonden</ind></trd>,
    <trd>offline</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_v;&c_t;&c_o;&c_n;&c_o;&c_m;&c_n;&c_o;</trd>,
    <trd>&c_o;&c_f;&c_f;-&c_l;&c_a;&c_j;&c_n;</trd>
  </trdgrp>
</drv>
</art>
