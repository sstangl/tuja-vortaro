<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: estr.xml,v 1.23 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  -<rad>estr</rad>/
</kap>
<subart>
<drv mrk="estr.0">
  <kap>-<tld/></kap>
  <snc>
  <dif>
  Sufikso signifanta tiun, kiu efektive direktas, funkciigas la aferon,
  montritan de la radiko:
  <ekz>
  &scirc;ip<tld/>o, ofic<tld/>o, hotel<tld/>o, urb<tld/>o,
  regn<tld/>o<fnt>Z</fnt>.
  </ekz>
  </dif>
  </snc>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="estr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu, kiu estas &ccirc;e supera kaj unua rango, kaj posedas
      efektivan
      ordonpovon:
      <ekz>
        el la <tld/>o vi fari&gcirc;os sklavo;
      </ekz>
      <ekz>
        lernej<tld/>o, labor<tld/>o<fnt>Z</fnt>,
        cent<tld/>o<fnt>Z</fnt>, gardist<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxef.0o">&ccirc;efo</ref>
    </refgrp>
    <ref tip="super" cel="an.0o">ano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&rcaron;editel</trd>,
    <trd>náčelník</trd>,
    <trd>předseda</trd>,
    <trd>představený</trd>,
    <trd>vedouc&iacute;</trd>,
    <trd>šéf</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leiter</trd>,
    <trd>Vorsitzender</trd>
  </trdgrp>
  <trd lng="en">leader</trd>
  <trdgrp lng="es">
    <trd>jefe</trd>,
    <trd>dirigente</trd>,
    <trd>superior<klr tip="ind"> (jefe)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chef</trd>,
    <trd>dirigeant</trd>,
    <trd>sup&eacute;rieur<klr tip="ind"> (chef)</klr></trd>,
    <trd>responsable<klr tip="ind"> (chef)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vezet&odblac;</trd>,
    <trd>ir&aacute;ny&iacute;t&oacute;</trd>,
    <trd>f&odblac;n&ouml;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szef</trd>,
    <trd>kierownik</trd>,
    <trd>zwierzchnik</trd>,
    <trd>naczelnik</trd>,
    <trd>zawiadowca</trd>,
    <trd>dow&oacute;dca</trd>,
    <trd>w&oacute;dz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>chefe</trd>,
    <trd>diretor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predstavený</trd>,
    <trd>vedúci</trd>
  </trdgrp>
</drv>

<drv mrk="estr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>o super, regi:
      <ekz>
        <tld/>i firmon;
      </ekz>
      <ekz>
        en sia hejmo <tld/>as karbisto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estrigis lin super la tuta Egipta lando
        <fnt><bib>MT</bib>, <lok>&Gen; 41:43</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komand.0i">komandi</ref>,
      <ref cel="mastr.0i">mastri</ref>,
      <ref cel="direkt.0i">direkti</ref>,
      <ref cel="gvid.0i">gvidi</ref>,
      <ref cel="reg.0i">regi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_m;</ind></trd>,
    <trd>&c_v;&c_ja;&c_r;&c_sh;&c_e;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>být představeným</trd>,
    <trd>ovládat</trd>,
    <trd>velet</trd>,
    <trd>vést</trd>
  </trdgrp>
  <trd lng="de">leiten</trd>
  <trdgrp lng="fr">
    <trd>commander<klr tip="ind"> (diriger)</klr></trd>,
    <trd>diriger<klr tip="ind"> (exercer le commandement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vezet&odblac;</trd>,
    <trd>ir&aacute;ny&iacute;t&oacute;</trd>,
    <trd>f&odblac;n&ouml;k</trd>,
    <trd>vez&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zarz&aogonek;dza&cacute;</trd>,
    <trd>rz&aogonek;dzi&cacute;</trd>,
    <trd>kierowa&cacute;</trd>,
    <trd>przewodzi&cacute;</trd>,
    <trd>szefowa&cacute;<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_g;&c_l;&c_a;&c_v;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>byť na čele</trd>,
    <trd>ovládať</trd>
  </trdgrp>
  <trd lng="tp">lawa</trd>
</drv>

<drv mrk="estr.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
    <rim>
      Ne konfuzu kun estrad-o.
    </rim>
  </snc>
  <trd
  lng="be">&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>estráda</trd>,
    <trd>pódium</trd>
  </trdgrp>
  <trd lng="de">Leitung</trd>
  <trd lng="fr">direction<klr tip="ind"> (action de diriger)</klr></trd>
  <trdgrp lng="hu">
    <trd>vezet&eacute;s</trd>,
    <trd>ir&aacute;ny&iacute;t&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zarz&aogonek;dzanie</trd>,
    <trd>rz&aogonek;dy</trd>,
    <trd>kierownictwo</trd>,
    <trd>przewodnictwo</trd>,
    <trd>zwierzchnictwo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;<klr
    tip="ind"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;<klr
    tip="amb"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vedenie</trd>,
    <trd>velenie</trd>
  </trdgrp>
</drv>

<drv mrk="estr.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Ordonpovo de <tld/>o:
      <ekz>
        la kadukulejo, kiu estas sub lia <tld/>eco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_v;&c_ja;&c_r;&c_sh;&c_e;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>vedoucí postavení</trd>,
    <trd>vůdcovství</trd>
  </trdgrp>
  <trd lng="de">Leitung</trd>
  <trdgrp lng="fr">
    <trd>autorit&eacute;</trd>,
    <trd>direction<klr tip="ind"> (pouvoir de diriger)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vezet&eacute;s</trd>,
    <trd>ir&aacute;ny&iacute;t&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w&lstroke;adza</trd>,
    <trd>rz&aogonek;dy</trd>,
    <trd>zwierzchno&sacute;&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_g;&c_l;&c_a;&c_v;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">vedúce postavenie</trd>
</drv>

<drv mrk="estr.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Aro da <tld/>oj:
      <ekz>
        la <tld/>aro de kongreso, de firmo, de societo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>předsednictvo</trd>,
    <trd>vedení</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorstand</trd>,
    <trd>Leitung</trd>
  </trdgrp>
  <trd lng="fr">direction<klr tip="ind"> (les dirigeants)</klr></trd>
  <trdgrp lng="hu">
    <trd>vezet&odblac;s&eacute;g</trd>,
    <trd>f&odblac;n&ouml;ks&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zarz&aogonek;d</trd>,
    <trd>dyrekcja</trd>,
    <trd>kierownictwo</trd>,
    <trd>zwierzchnictwo</trd>,
    <trd>w&lstroke;adza<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">diretoria<klr tip="amb"> (os dirigentes)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predsedníctvo</trd>,
    <trd>predstavenstvo</trd>,
    <trd>vrchnosť</trd>
  </trdgrp>
</drv>
</subart>
</art>
