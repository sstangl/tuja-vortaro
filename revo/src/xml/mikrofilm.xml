<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mikrofilm.xml,v 1.9 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mikrofilm</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="mikrofilm.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrofilm.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="film.0o.FOT">Filmo</ref> portanta seriajn
      bildetojn de dokumento poste rigardeblajn per projekciilo:
      <ekz>
        La genealogia servo de mormonoj provizas siajn <tld/>ojn anka&ubreve;
        por
        nemembroj
        <fnt>
        <aut>J. Pietil&auml;inen</aut>,
        <vrk>Iom da aldono al la demandoj en Eventoj nr-o. 3</vrk>,
        <lok><url
        ref="http://www.eventoj.hu/arkivo/eve-008.htm">Eventoj,
        1992-06, numero 8a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">microfilm</trd>
  <trd lng="cs">mikrofilm</trd>
  <trd lng="de">Mikrofilm</trd>
  <trd lng="fr">microfilm</trd>
  <trd lng="hu">mikrofilm</trd>
  <trd lng="nl">microfilm</trd>
  <trdgrp lng="pt">
    <trd>microfilme</trd>
  </trdgrp>
  <trd lng="sk">mikrofilm</trd>
</drv>

<drv mrk="mikrofilm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mikrofilm.0i.TIP">
    <dif>
      Registri sur <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">microfilmar</trd>
  <trd lng="de">verfilmen auf <ind>Mikrofilm</ind></trd>
  <trd lng="fr">microfilmer</trd>
  <trd lng="hu">mikrofilmez</trd>
  <trd lng="nl">microfilmen</trd>
  <trdgrp lng="pt">
    <trd>microfilmar</trd>
  </trdgrp>
</drv>
</art>

