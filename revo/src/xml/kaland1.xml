<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaland1.xml,v 1.17 2015/07/04 08:37:36 revo Exp $">
<kap>
   <rad>kalandr</rad>/i
   <fnt>Z</fnt>
</kap>
<drv mrk="kaland1.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">TEK</uzo>
   <snc>
     <dif>
       Glatigi tola&jcirc;ojn, &scirc;tofon, paperon per specialaj
       rulcilindroj, por doni al ili brilecon.
     </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>hladit</trd>,
    <trd>kalandrovat</trd>,
    <trd>leštit</trd>,
    <trd>vyrovnávat</trd>
  </trdgrp>
  <trd lng="de">mangeln</trd>
  <trd lng="en">mangle</trd>
  <trdgrp lng="es">
     <trd>satinar</trd>,
     <trd>calandrar</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd>mankeloida</trd>,
     <trd>kalanteroida <klr> (paperon en fabriko)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>calandrer</trd>,
     <trd>lustrer<klr tip="ind"> (&agrave; la calandre)</klr></trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>m&aacute;ngorol</trd>,
     <trd>kalanderez <klr tip="ind">(sz&ouml;vetet)</klr></trd>
   </trdgrp>
  <trd lng="nl">kalanderen</trd>
  <trd lng="pt">calandrar</trd>
  <trdgrp lng="ru">
     <trd>&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol; 
<klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>,
     <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol; 
<klr>(&c_b;&c_u;&c_m;&c_a;&c_g;&c_u;)</klr></trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>hladiť</trd>,
    <trd>kalandrovať</trd>,
    <trd>leštiť</trd>,
    <trd>mangľovať</trd>
  </trdgrp>
  <trd lng="sv">mangla</trd>
</drv>

<drv mrk="kaland1.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Ago <tld/>i.
     </dif>
   </snc>
   <refgrp tip="hom">
     Ne miksu kun:
     <ref cel="kaland3.0o">kalandro</ref> (birdo),
     <ref cel="kaland.0o">kalandro</ref> (insekto).
   </refgrp>
   <trd lng="de">Mangeln</trd>
   <trd lng="en">mangle</trd>
   <trd lng="fi">mankelointi</trd>
   <trdgrp lng="fr">
     <trd>calandrage</trd>,
     <trd>lustrage<klr tip="ind"> (&agrave; la calandre)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>m&aacute;ngorl&aacute;s</trd>,
     <trd>kalanderez&eacute;s <klr 
tip="ind">(sz&ouml;vet&eacute;)</klr></trd>
   </trdgrp>
   <trd lng="nl">kalanderen<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="sk">mangľovanie bielizne</trd>
  <trdgrp lng="cs">
    <trd>kalandrování prádla</trd>,
    <trd>mandlování prádla</trd>
  </trdgrp>
</drv>

<drv mrk="kaland1.0ilo">
   <kap><tld/>ilo</kap>
   <uzo tip="fak">TEK</uzo>
   <snc>
     <dif>
       Ma&scirc;ino por glatigi kaj brilantigi &scirc;tofojn, glaceigi 
paperon:
       <ekz>
         <tld/>iloj estas formitaj el tri
         <ref tip="prt" cel="cilind.rul0o">rulcilindroj</ref>:
         du kartonaj kaj unu metala.
       </ekz>
     </dif>
   </snc>
  <trd lng="cs">kalndr</trd>
  <trd lng="de">Mangel</trd>
  <trd lng="en">mangle</trd>
  <trdgrp lng="fi">
     <trd>mankeli</trd>,
     <trd>kalanteri <klr> (en paperfabriko)</klr></trd>
   </trdgrp>
  <trd lng="fr">calandre<klr tip="ind"> (machine pour
   lustrer)</klr></trd>
  <trdgrp lng="hu">
     <trd>m&aacute;ngorl&oacute;(g&eacute;p)</trd>,
     <trd>kalander <klr tip="ind">(sz&ouml;vethez)</klr></trd>
   </trdgrp>
  <trd lng="nl">kalander<klr tip="ind"> (toestel)</klr></trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sk">mangeľ</trd>
  <trd lng="sv">mangel</trd>
</drv>
</art>
