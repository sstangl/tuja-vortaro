<vortaro>
<art mrk="$Id: felis.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>felis</rad>/o
</kap>
<drv mrk="felis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Felis</trd>), al kiu apartenas ekzemple la
      <ref tip="sub" cel="kat.0o">kato</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_t; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="cs">kočka</trd>
  <trd lng="de">Katze</trd>
  <trd lng="es">felino</trd>
  <trd lng="fr">chat<klr tip="ind"> (genre)</klr></trd>
  <trd lng="hu">macska<klr tip="ind"> (nem)</klr></trd>
  <trd lng="pt">f&eacute;lis</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">mačka</trd>
</drv>
</art>
