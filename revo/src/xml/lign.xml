<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lign.xml,v 1.22 2011/12/08 11:10:24 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>lign</rad>/o
</kap>

<drv mrk="lign.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <snc>
     <dif>
       Malmola substanco el kiu konsistas radikoj, trunko kaj 
       bran&ccirc;oj de arboj:
       <ekz>
         <ind>brul<tld/>o</ind>;
         <trd lng="ca">llenya</trd>
         <trd lng="fa">&a_h;&f_y;&a_z;&a_m;</trd>
         <trd lng="hu">t&uuml;zifa</trd>
       </ekz>
       <ekz>
         <ind>seg<tld/>o</ind>;
         <trd lng="hu">f&udblac;r&eacute;sz&aacute;ru</trd>         
       </ekz>
       <ekz>
         konstru<tld/>o;
       </ekz>
       <ekz>
         <tld/>a benko, &scirc;uo.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_r;&c_e;&c_v;&c_a; <klr>
     (&c_m;&c_a;&c_t;&c_a;&c_r;'&c_ja;&c_l;)</klr></trd>,
     <trd>&c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>
   </trdgrp>
   <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
   <trd lng="ca">fusta</trd>
   <trd lng="cs">d&rcaron;evo</trd>
   <trd lng="de">Holz</trd>
   <trd lng="el">&xi;&ypsilon_ton;&lambda;&omikron;</trd>
   <trd lng="en">wood</trd>
   <trdgrp lng="es">
     <trd>madera</trd>,
     <trd>le&ntilde;o</trd>
   </trdgrp>
   <trd lng="fa">&f_ch;&a_w;&a_b;</trd>
   <trd lng="fr">bois</trd>
   <trd lng="hu">fa</trd>   
   <trd lng="id">kayu</trd>
   <trd lng="nl">hout</trd>
   <trd lng="pl">drewno</trd>
   <trdgrp lng="pt">
     <trd>madeira</trd>,
     <trd>lenha</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_n;&c_a;</trd>,
     <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">
     (&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;)</klr></trd>
   </trdgrp>
  <trd lng="tp">kasi</trd>
</drv>

<drv mrk="lign.0ajxisto">
   <kap>
     <tld/>a&jcirc;isto
   </kap>
   <snc>
     <dif>
       Metiisto, kiu prilaboras <tld/>ajn objektojn.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_r;&c_e;&c_v;&c_a;&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>,
     <trd>&c_c;&c_ja;&c_s;&c_mol;&c_l;&c_ja;&c_r;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_ja;&c_r;</trd>
   </trdgrp>
   <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_o;&c_d;&c_je;&c_l;&c_je;&c_c; &c_m;&c_je;&c_b;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
   <trd lng="ca">fuster</trd>
   <trd lng="de">Schreiner</trd>
   <trd lng="el">&xi;&ypsilon;&lambda;&omikron;&ypsilon;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
   <trd lng="es">carpintero</trd>
   <trdgrp lng="fa">
     <trd>&a_n;&a_j;&a_A;&a_r;</trd>,
     <trd>&a_d;&a_r;&a_w;&a_d;&f_g;&a_r;</trd>
   </trdgrp>
   <trd lng="fr">menuisier</trd>
   <trd lng="hu">asztalos</trd>   
   <trd lng="nl">schrijnwerker</trd>
   <trdgrp lng="pl">
     <trd>stolarz</trd>,
     <trd>ciesla</trd>,
     <trd>szkutnik</trd>
    </trdgrp>
   <trd lng="pt">carpinteiro</trd>
   <trdgrp lng="ru">
     <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_d;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
     <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_ch;&c_i;&c_k;</trd>,
     <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
     <trd>&c_s;&c_t;&c_o;&c_l;&c_ja;&c_r;</trd>
   </trdgrp>
</drv>
</art>
