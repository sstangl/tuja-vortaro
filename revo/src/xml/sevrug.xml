<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sevrug.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sevrug</rad>/o
</kap>
<drv mrk="sevrug.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="lst" cel="acipenser.0o"
      lst="voko:fi&scirc;oj">acipensero</ref>
      (<trd lng="la">Acipenser stellatus</trd>),
      kun brune ru&gcirc;a koloro,
      vivanta en Nigra, Asova kaj Kaspia Maroj kaj frajanta 
      en la alfluaj riveroj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_w;&c_r;&c_u;&c_g;&c_a;</trd>
  <trd lng="cs">jeseter hvězdnatý</trd>
  <trdgrp lng="de">
    <trd>Scherg</trd>, <trd>Sternhausen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>starry sturgeon</trd>,
    <trd>stellate sturgeon</trd>,
    <trd>stellated sturgeon</trd>
  </trdgrp>
  <trd lng="hu">s&odblac;regtok</trd>
  <trd lng="nl">stersteur</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_r;&c_ju;&c_g;&c_a;</trd>
  <trd lng="sk">jeseter hviezdnatý</trd>
</drv>
</art>
