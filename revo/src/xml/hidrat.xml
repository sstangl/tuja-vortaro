<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hidrat.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hidrat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hidrat.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrat.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Ccirc;ia substanco, kiu povas esti rigardata kiel
      kombina&jcirc;o de akvo kun alia substanco.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">hidrat</trd>
  <trdgrp lng="cs">
    <trd>hydrát</trd>,
    <trd>sůl s krystalickou vodou</trd>
  </trdgrp>
  <trd lng="de">Hydrat</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota_ton;&delta;&jota;&omikron;</trd>
  <trd lng="fr">hydrate</trd>
  <trd lng="hu">hidr&aacute;t</trd>
  <trd lng="it">idrato<klr> (sost.)</klr></trd>
  <trd lng="nl">hydraat</trd>
  <trdgrp lng="pl">
    <trd>hydrat</trd>,
    <trd>wodzian</trd>
  </trdgrp>
  <trd lng="pt">hidrato</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">hydrát</trd>
</drv>

<drv mrk="hidrat.0a">
  <kap><tld/>a</kap>
  <snc mrk="hidrat.0a.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kombinita kun akvo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>hidratat</trd>,
    <trd>aqu&oacute;s</trd>
  </trdgrp>
  <trd lng="de">w&auml;ssrig</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota;&delta;&omega;&my;&epsilon_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="fr">hydrat&eacute;<klr tip="ind"> (chim.)</klr></trd>
  <trd lng="hu">vizes</trd>
  <trd lng="it">idrato<klr> (sgg.)</klr></trd>
  <trd lng="nl">waterig</trd>
  <trd lng="pt">hidratado</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="hidrat.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="hidrat.0igi.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Formi <tld/>on, kombini substancon kun akvo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">hidratar</trd>
  <trdgrp lng="cs">
    <trd>hasit</trd>,
    <trd>hydratovat</trd>,
    <trd>sloučit s vodou</trd>
  </trdgrp>
  <trd lng="de">hydratisieren</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&chi;&epsilon;&jota;&delta;&omega_ton;&ny;&omega;</trd>
  <trd lng="fr">hydrater<klr tip="ind"> (chim., transformer en
  hydrate)</klr></trd>
  <trd lng="hu">hidr&aacute;t&aacute;l</trd>
  <trd lng="it">idratare</trd>
  <trd lng="nl">hydrateren</trd>
  <trdgrp lng="pl">
    <trd>hydratyzowa&cacute;</trd>, <trd>uwadnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">hidratar</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hydrátovať</trd>,
    <trd>zlučovať s vodou</trd>
  </trdgrp>
</drv>

<drv mrk="hidrat.karbon0o">
  <kap>karbon<tld/>o</kap>
  <snc mrk="hidrat.karbon0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Sukero a&ubreve; simila <tld/>a substanco
      funkcianta kiel provizanto
      de energio al viva&jcirc;oj;
      <ref tip="sin" cel="glucid.0o.KEM">glucido</ref>:
      <ekz>
        la karbon<tld/>oj estas la fonto de la muskolforto    
        <fnt>
          <aut>A. Wander</aut>,
          <vrk><url
          ref="http://arkivo.esperanto-france.org/divers/ovomalt/ovo-p5.htm">
          La Plej Esenca pri Ovomaltino</url></vrk>,
          <lok>1910</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gl&uacute;cid</trd>,
    <trd>gl&iacute;cid</trd>
  </trdgrp>
  <trd lng="cs">uhlovodík</trd>
  <trd lng="en">carbohydrate</trd>
  <trd lng="fr">glucide</trd>
  <trdgrp lng="pt">
    <trd>glic&iacute;dio</trd>,
    <trd>gluc&iacute;dio</trd>
  </trdgrp>
  <trd lng="sk">uhľovodík</trd>
</drv>
</art>
