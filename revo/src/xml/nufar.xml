<kap>
  <rad>nufar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nufar.0o">
  <kap><tld/>o</kap>
  <snc mrk="nufar.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="lili.akvo0o.BOT">akvolilio</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Teichrose</trd>,
    <trd>Mummel</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;nuphar</trd>
  <trdgrp lng="hu">
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zit&ouml;k</trd>
  </trdgrp>
  <trd lng="nl">gele <ind>plomp</ind></trd>
  <trd lng="pl">nenufar</trd>
  <trdgrp lng="pt">
    <trd>g&oacute;lf&atilde;o</trd>,
    <trd>aguap&eacute;</trd>,    
    <trd>gigoga</trd>,
    <trd>golfo</trd>,
    <trd>jigoga</trd>,
    <trd>l&iacute;rio-d'&aacute;gua</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_y;&c_sh;&c_k;&c_a; <klr
    tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_jo;&c_l;&c_t;&c_a;&c_ja;
    <ind>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
</drv>
</art>
