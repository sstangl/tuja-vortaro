<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ceruz.xml,v 1.16 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>ceruz</rad>/o<fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ceruz.0o">
  <kap><tld/>o</kap>
  <snc mrk="ceruz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Venena plumba karbonato kaj hidroksido, pasinte uzata en
      pentrado kaj farbado kiel blanka kolorilo;
      simbole <frm>2 PbCO<sub>3</sub>, Pb(OH)<sub>2</sub></frm>:
      <ekz>
        <tld/>a blanko
        <fnt>
          <aut>trad. C.M.C.</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Perle.htm">
          La junulino kun la perlo</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      La vorto "hidroksido" estas enkondukenda. <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="cs">běloba olovnatá</trd>
  <trd lng="de">Bleiwei&szlig;</trd>
  <trd lng="en">white lead</trd>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&a_A;&a_b;</trd>
  <trd lng="fr">c&eacute;ruse</trd>
  <trd lng="hu">&oacute;lomfeh&eacute;r</trd>
  <trd lng="it">biacca</trd>
  <trdgrp lng="nl">
    <trd>loodwit</trd>,
    <trd>kremserwit</trd>,
    <trd>schelpwit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cerusyt</trd>,
    <trd>w&eogonek;glan o&lstroke;owiu</trd>,
    <trd>biel o&lstroke;owiana</trd>,
    <trd>biel kremska</trd>,
    <trd>kit miniowy</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_y;&c_je; &c_b;&c_je;&c_l;&c_i;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>PbCO2</trd>,
    <trd>olovnatá bieloba</trd>
  </trdgrp>
</drv>
</art>
