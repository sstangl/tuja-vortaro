<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxl.xml,v 1.14 2012/07/17 10:10:21 revo Exp $">
<kap>
  <rad>a&ubreve;l</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="auxl.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">PED</uzo>
    <dif>
      Solenejo, festo&ccirc;ambrego &ccirc;e universitato:
      <ekz>
        la gimnazianoj en militserva a&gcirc;o estas kunvokitaj akurate je la
        8a kaj duono, en la granda <tld/>o
        <fnt><bib>KielAkvo</bib>, <lok>2:2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="auxdit.0o">a&ubreve;ditorio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_t;&c_a;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;-&c_z;&c_a;&c_l;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aula magna</trd>,
    <trd>paranimf</trd>,
    <trd>auditori</trd>
  </trdgrp>
  <trd lng="de">Aula</trd>
  <trdgrp lng="en">
    <trd>hall</trd>,
    <trd>auditorium</trd>
  </trdgrp>
  <trd lng="fr">salle de r&eacute;ception</trd>
  <trdgrp lng="hu">
    <trd>aula</trd>,
    <trd>d&iacute;szterem</trd>
  </trdgrp>
  <trd lng="it"><ind>aula</ind> magna</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_o;&c_v;&c_y;&c_j; &c_z;&c_a;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;-&c_z;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">aula</trd>
</drv>
</art>
