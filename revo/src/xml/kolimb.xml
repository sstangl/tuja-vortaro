<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolimb.xml,v 1.23 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolimb</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kolimb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolimb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Na&gcirc;obirdoj kun kruroj proksimaj al la vosto
      kaj na&gcirc;ha&ubreve;to inter la piedfingroj,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="kolimb1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gavia</trd>):
      <ekz>
        nur sola <tld/>o, balanci&gcirc;ante sur ondetoj,
        estis rigardanta al mi per la nigra okulo,
        pensante, eble, ke mi kaptis fi&scirc;on
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_g;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>pot&aacute;plice</trd>,
    <trd>potápka</trd>
  </trdgrp>
  <trd lng="de">Seetaucher</trd>
  <trdgrp lng="en">
    <trd>diver</trd>,
    <trd>loon</trd>
  </trdgrp>
  <trd lng="fr">plongeon<klr tip="amb"> (oiseau)</klr></trd>
  <trd lng="hu">b&uacute;v&aacute;r(mad&aacute;r)</trd>
  <trd lng="nl">zeeduiker</trd>
  <trd lng="pl">nur</trd>
  <trdgrp lng="pt">
    <trd>colimb&iacute;deo</trd>,
    <trd>colimbo</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;&c_r;&c_a;</trd>
  <trd lng="sk">potápka</trd>
</drv>

<drv mrk="kolimb.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="kolimb.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gaviiformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_g;&c_a;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr
  tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="de">Seetaucher<klr tip="ind"> (Ordnung)</klr></trd>
  <trd lng="fr">gaviiformes</trd>
  <trd lng="nl">zeeduikers<klr tip="ind"> (orde)</klr></trd>
  <trd lng="hu">b&uacute;v&aacute;ralak&uacute;ak</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>
