<kap>
  <rad>heksaedr</rad>/o
</kap>
<drv mrk="heksae.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <ref tip="dif" cel="edr.n0o">sesedro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_k;&c_s;&c_a;&c_e;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_g;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hexaeder</trd>,
    <trd>Sechsflach</trd>,
    <trd>Sechsfl&auml;chner</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&xi;&alfa_ton;&epsilon;&delta;&rho;&omikron;</trd>
  <trd lng="en">hexahedron</trd>
  <trd lng="es">hexaedro</trd>
  <trd lng="fr">hexa&egrave;dre</trd>
  <trdgrp lng="hu">
    <trd>hexa&eacute;der</trd>,
    <trd>hatlap</trd>,
    <trd>kocka</trd>
  </trdgrp>
  <trd lng="it">esaedro</trd>
  <trdgrp lng="nl">
    <trd>hexa&euml;der</trd>,
    <trd>zesvlak</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>heksaedr</trd>,
    <trd>sze&sacute;cian<klr> (niekoniecznie foremny)</klr></trd>
  </trdgrp>
  <trd lng="pt">hexaedro</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_k;&c_s;&c_a;&c_e;&c_d;&c_r;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_i;&c_g;&c_r;&c_a;&c_n;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
