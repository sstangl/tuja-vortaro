      <trd>assembler<klr tip="ind"> (harmoniser)</klr></trd>
      <trd>assortir<klr tip="ind"> (harmoniser)</klr></trd>
      <trd>associer<klr tip="ind"> (harmoniser)</klr></trd>
      <trd>concilier</trd>
      <trd>harmoniser<klr tip="ind"> (mettre en harmonie)</klr></trd>
      <trd>marier<klr tip="ind"> (harmoniser)</klr></trd>
    </trdgrp>
    <trd lng="nl">doen <ind>overeenstemmen</ind></trd>
    <trdgrp lng="pt">
      <trd>p&ocirc;r de acordo</trd>,
      <trd>concordar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_k; <ind>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_ju;</ind>
      <klr>(&c_l;&c_ju;&c_d;&c_je;&c_j;)</klr></trd>,
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
    <trd lng="sk">zladi&tcaron;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ladit</trd>,
    <trd>seřizovat</trd>,
    <trd>urovnat <klr>(spor)</klr></trd>,
