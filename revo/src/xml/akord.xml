      <trd>convention<klr tip="ind"> (juridique)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kompatibilnost</trd>,
    <trd>shoda</trd>
  </trdgrp>
  <trd lng="sk">zhoda</trd>
</drv>

<drv mrk="akord.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="akord.mal0o.malsamopinieco">
    <ref tip="ant" cel="akord.0o.samopinieco"><tld/>o<sncref/></ref>
    <ekz>
      juneca aventuramo igis min alpreni <klr>[tiun profesion]</klr>,
      kaj mal<tld/>o kun parencoj, kiu jam ne ekzistas
      <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 9a</lok></fnt>;
    </ekz>
    <ekz>
      en Vilvoorde <klr>[...]</klr> ekestis mal<tld/>o inter liberaluloj
      kaj la kristandemokrata partio
      <fnt>
      <aut>G. Van Damme</aut>,
      <vrk>Demokratio blokita</vrk>,
      <lok><url
      ref="http://www.esperanto.be/fel/mon/007259.html">Monato</url></lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="konfli.0o">konflikto</ref>,
      <ref cel="akord.mis0o">mis<tld/>o</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;saccord</trd>,
      <trd>conflit</trd>,
      <trd>contradiction</trd>,
      <trd>diff&eacute;rend</trd>,
      <trd>discordance</trd>,
      <trd>discorde</trd>,
      <trd>dissension</trd>,
      <trd>dissentiment</trd>,
      <trd>divergence de <ind>vue</ind>s</trd>,
      <trd>friction<klr tip="ind"> (divergence de vues)</klr></trd>,
      <trd>litige</trd>,
      <trd>m&eacute;sentente</trd>,
      <trd>opposition</trd>,
      <trd>tiraillement<klr tip="ind"> (divergence de vues)</klr></trd>,
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>konflikt</trd>,
    <trd>rozpor</trd>,
    <trd>rozvrat</trd>,
    <trd>úplná neshoda</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konflikt</trd>,
    <trd>nezhoda</trd>,
    <trd>rozpor</trd>,
    <trd>rozvrat</trd>
  </trdgrp>
</drv>

<drv mrk="akord.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akord.mal0i.KOMUNE">
    <ref tip="ant" cel="akord.0i"><tld/>i</ref>
    <ekz>
      via rakonto, profesoro, estas tre interesa, kvankam &gcirc;i
      tute mal<tld/>as kun la Evangelioj
      <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 3a</lok></fnt>.
    </ekz>
    <ref tip="vid" cel="konfli.0i">konflikti</ref>
  </snc>
  <trd lng="cs">být v rozporu</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>d&eacute;saccord</ind></trd>,
    <trd>&ecirc;tre en <ind>conflit</ind></trd>,
    <trd>&ecirc;tre en <ind>contradiction</ind></trd>,
    <trd>contredire</trd>,
    <trd>avoir un <ind>diff&eacute;rend</ind></trd>,
    <trd>avoir un <ind>litige</ind></trd>,
  </trdgrp>
  <trd lng="sk">byť v rozpore</trd>
</drv>

<drv mrk="akord.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="akord.mal0a.KOMUNE">
    <ref tip="ant" cel="akord.0a"><tld/>a</ref>
    <ekz>
      la elementoj estas kunmiksitaj, la animoj mal<tld/>aj kaj
      se vi ne savos nin, sinjoro, la horoj de nia vivo estas kalkulitaj
      <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 13a</lok></fnt>;
    </ekz>
    <ekz>
      &Ccirc;inio jam reviziis kaj nuligis serion da le&gcirc;oj kaj
      reguloj mal<tld/>aj al la difinoj de MKO
      <fnt>
      <aut>Zuo Yan</aut>,
      <vrk>&Ccirc;inio plenumas sian promeson al MKO</vrk>,
      <lok><url
      ref="http://www.chinareport.com.cn/18s/wto/wto-12.htm">El Popola
      &Ccirc;inio</url></lok>
      </fnt>;
    </ekz>
    <ekz>
      la violonoj mal<tld/>e pepis, penante agordi&gcirc;i
      al la mallertaj violon&ccirc;eloj
      <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 3a</lok></fnt>.
    </ekz>
    <ref tip="vid" cel="konfli.0a">konflikta</ref>
  </snc>
  <trd lng="cs">nejednotný</trd>
  <trdgrp lng="fr">
    <trd>en <ind>d&eacute;saccord</ind><klr tip="ind"> (loc. adj.)</klr></trd>,
    <trd>en <ind>inad&eacute;quation</ind><klr tip="ind"> (loc.
    adj.)</klr></trd>,
    <trd>contraire<klr tip="ind"> (adj.)</klr></trd>,
    <trd>contradictoire</trd>,
    <trd>d&eacute;cal&eacute;</trd>,
    <trd>d&eacute;phas&eacute;</trd>,
    <trd>divergent</trd>,
    <trd>incoh&eacute;rent</trd>,
    <trd>inharmonieux</trd>,
    <trd>oppos&eacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bez ladu nesúladný</trd>,
    <trd>nezhodný</trd>
  </trdgrp>
</drv>

<drv mrk="akord.mal0igebla">
  <kap>mal<tld/>igebla</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="akord.mal0igebla.KOMUNE">
    <ref tip="dif" cel="akord.ne0igebla">ne<tld/>igebla</ref>
    <ekz>
      tio ja estus mal<tld/>igebla kun via re&gcirc;ina digno
      <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>inconciliable</trd>,
    <trd>antagonique</trd>,
    <trd>antinomique</trd>,
    <trd>contradictoire</trd>,
    <trd>incompatible</trd>,
    <trd>irr&eacute;conciliable</trd>
  </trdgrp>
</drv>

<drv mrk="akord.mis0o">
  <kap>mis<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="akord.mis0o.KOMUNE">
    <dif>
      Nekompleta, malbona <tld/>o:
      <ekz>
        zorgu, ke akraj sonoj ne faru mis<tld/>ojn
        <fnt>
        <aut>N. Boileau, trad. G. Waringhien</aut>,
        <vrk>La arto poezia: Unua kanto</vrk>,
        <lok><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr26/Boileau/kanto_unuau.html">Nica
        Literatura Revuo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        estas ja kelkaj mis<tld/>oj inter la membro&scirc;tatoj pri
        la nele&gcirc;aj rifu&gcirc;intoj
        <fnt>
        <aut>trad. J.-M. Cash</aut>,
        <vrk>Le Monde Diplomatique en Esperanto</vrk>,
        <lok><url
        ref="http://eo.mondediplo.com/article887.html">2004-03</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Scirc;ajnas, ke tiu &ccirc;i vorto rolas &ccirc;efe kiel evitvorto por
      <ref cel="akord.mal0o">mal<tld/>o</ref>.
    </rim>
  </snc>
  <trdgrp lng="fr">
    <trd>dissonance<klr tip="ind"> (fig.)</klr></trd>,
    <trd>fausse <ind>note</ind><klr tip="ind"> (fig.)</klr></trd>,
    <trd>d&eacute;saccord<klr tip="ind"> (l&eacute;ger)</klr></trd>,
    <trd>d&eacute;calage<klr tip="ind"> (entre points de vue)</klr></trd>,
    <trd>discordance</trd>,
    <trd>divergence de <ind>vue</ind>s</trd>,
    <trd>friction<klr tip="ind"> (divergence de vues)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="akord.ne0igebla">
  <kap>ne<tld/>igebla</kap>
  <snc mrk="akord.ne0igebla.KOMUNE">
    <dif>
      Kiun ne eblas <tld/>igi:
      <ekz>
        <klr>[la ekologiaj dama&gcirc;oj necesigas]</klr> politikajn
        decidojn ne<tld/>igeblajn kun la dominanta novliberalisma modelo
        <fnt>
        <aut>trad. J.-M. Cash</aut>,
        <vrk>Le Monde Diplomatique en Esperanto</vrk>,
        <lok><url
        ref="http://eo.mondediplo.com/article988.html">2005-01</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="akord.mal0igebla">mal<tld/>igebla</ref>
  </snc>
  <trd lng="cs">neslučitelný</trd>
  <trdgrp lng="fr">
    <trd>inconciliable</trd>,
    <trd>antagonique</trd>,
    <trd>antinomique</trd>,
    <trd>contradictoire</trd>,
    <trd>incompatible</trd>,
    <trd>irr&eacute;conciliable</trd>
  </trdgrp>
  <trd lng="sk">nezladiteľný</trd>
</drv>

<drv mrk="akord.0ekun">
  <kap><tld/>e kun, <var><kap><tld/>e al</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="akord.0ekun.KOMUNE">
    <dif>
      La&ubreve; komuna sento, komuna intenco, komuna maniero,
      <ref tip="vid" cel="kongru.0ekun.KOMUNE">kongrue kun</ref>,
      <ref tip="vid" cel="konfor.0eal.KOMUNE">konforme al</ref>:
      <ekz>
        ni esperas doni specialan rolon al la teatro,
        <tld/>e kun la tradicio de la gastiganta urbo
        <fnt>
          <aut>Hispana Esperanto-Federacio</aut>,
          <vrk><url
          ref="http://www.esperanto.es/hef/images/kongreso/almagro/anoncokongresoalmagro.pdf">
          71-a Hispana Kongreso de Esperanto</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la dua mondmilito, <tld/>e al la militarisma politiko por 
        ekspansii suden, Japanio pluapa&scirc;e disvolvis en Taiwan diversajn 
        industriojn rilatajn al la milito
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/chinaabc/chapter10/chapter100501.htm">
          Ekspluata Historio de Taiwan</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt> .
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">en accord avec</trd>
</drv>

<drv mrk="akord.mal0ekun">
  <kap>mal<tld/>e kun</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="akord.mal0ekun.KOMUNE">
    <dif>
      Malharmonie,
      <ref tip="vid" cel="kongru.mal0ekun.KOMUNE">malkongrue kun</ref>,
      <ref tip="vid" cel="kontra8.0eal.KOMUNE">kontraste al</ref>:
      <ekz>
        ne rekonis anabaptistoj, mal<tld/>e kun Lutero kaj Swinglio,
        la bapton de infanoj
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url
          ref="http://cfh.ufsc.br/~simpozio/Esperanto/ANABAPTISMO.html">
          Enciklopedio simpozio</url></vrk>,
          <lok>artikolo &leftquot;anabaptismo&rightquot;, [1999]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">en d&eacute;saccord avec</trd>
</drv>

<drv mrk="akord.veni_en_0on">
  <kap>veni en <tld/>on</kap>
  <uzo tip="stl">FRAZ</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akord.veni_en_0on.KOMUNE">
    <ref tip="dif" cel="akord.0igxi"><tld/>i&gcirc;i</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>accorder</ind><klr tip="amb"> (s'harmoniser, se mettre
    d'accord)</klr></trd>,
    <trd>se mettre d'<ind>accord</ind></trd>,
    <trd>tomber d'<ind>accord</ind></trd>,
    <trd>parvenir &agrave; un <ind>accord</ind></trd>,
    <trd>se mettre en <ind>accord</ind></trd>,
    <trd>s'<ind>entendre</ind><klr tip="ind"> (se mettre
    d'accord)</klr></trd>
    <trd>s'<ind>allier</ind><klr tip="ind"> (s'harmoniser)</klr></trd>
    <trd>s'<ind>assembler</ind><klr tip="ind">
    (s'harmoniser)</klr></trd>
    <trd>s'<ind>associer</ind><klr tip="ind"> (s'harmoniser)</klr></trd>
    <trd>s'<ind>assortir</ind><klr tip="ind"> (s'harmoniser)</klr></trd>
    <trd>s'<ind>harmoniser</ind></trd>
    <trd>se <ind>marier</ind><klr tip="ind"> (s'harmoniser)</klr></trd>
    <trd>se mettre en <ind>phase</ind><klr tip="ind"> (avec qc ou
    qn)</klr></trd>
  </trdgrp>
</drv>
</art>
