    <dif>
      Igi <tld/>a:
      <ekz>
        kaze <tld/>igi vorton kun alia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        peni por <tld/>igi la naciajn fierecojn
        <klr>(<ref tip="vid" cel="pac.0igi">pacigi</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>igi horlo&gcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0igi">konformigi</ref>
    </refgrp>
    <trdgrp lng="be">
