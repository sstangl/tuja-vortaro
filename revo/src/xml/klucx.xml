<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: klucx.xml,v 1.15 2016/01/11 13:10:18 revo Exp $">
<kap>
   <rad>klu&ccirc;</rad>/i
</kap>
<drv mrk="klucx.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">TEK</uzo>
   <uzo tip="fak">AUT</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Kunigi per iu mekanismo la motoron de ma&scirc;ino kaj la
organojn,
       kiujn &gcirc;i devas movi.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>kuppeln</trd>,
     <trd>koppeln</trd>,
     <trd>einkuppeln</trd>,
     <trd>verkuppeln</trd>
   </trdgrp>
   <trd lng="en">clutch</trd>
   <trd lng="es">embragar</trd>
   <trdgrp lng="hu">
     <trd>&ouml;sszekapcsol</trd>,
     <trd>tengelykapcsol&oacute;t bekapcsol</trd>,
     <trd>kuplungoz</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>innestare<klr> (mec.)</klr></trd>,
     <trd>ingranare</trd>
   </trdgrp>
   <trd lng="nl">koppelen</trd>
   <trd lng="fr">embrayer</trd>
   <trd lng="pl">sprz&eogonek;ga&cacute;</trd>
   <trdgrp lng="ru">
     <trd>
     &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;
     <ind>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ju;</ind>
     </trd>,
     <trd>
     &c_i;&c_s;&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;
     <ind>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</ind>
     </trd>
     <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;<klr tip="amb">
     (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
   </trdgrp>
</drv>

<drv mrk="klucx.0ado">
   <kap><tld/>ado</kap>
   <snc>
     <dif>
       Ago <tld/>i.
     </dif>
   </snc>
   <trd lng="de">Kuppeln</trd>
   <trd lng="fr">embrayage<klr tip="ind"> (action)</klr></trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;s</trd>,
     <trd>kuplungoz&aacute;s</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>innestare<klr> (mec.)</klr></trd>,
     <trd>ingranare</trd>
   </trdgrp>
   <trd lng="nl">het <ind>koppelen</ind></trd>
   <trdgrp lng="pl">
     <trd>sprz&eogonek;&zdot;enia</trd>,
     <trd>sprz&eogonek;ganie</trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;
   <klr tip="amb"> (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
</drv>

<drv mrk="klucx.0ilo">
   <kap><tld/>ilo</kap>
   <uzo tip="fak">TEK</uzo>
   <uzo tip="fak">AUT</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Mekanismo servanta por <tld/>i:
       <ekz>
         la <tld/>iloj de la a&ubreve;toj estas a&ubreve; konusaj
         a&ubreve; unudiskaj a&ubreve; plurdiskaj; en aliaj ma&scirc;inoj
         ili povas esti mufaj a&ubreve; dentaj.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">Kupplung</trd>
   <trd lng="en">clutch</trd>
   <trd lng="es">embrague</trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;</trd>,
     <trd>kuplung</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>frizione</trd>,
     <trd>innesto<klr> (mec.)</klr></trd>
   </trdgrp>
   <trd lng="nl">koppeling</trd>
   <trd lng="fr">embrayage<klr tip="ind"> (m&eacute;canisme)</klr></trd>
   <trd lng="pl">sprz&eogonek;g&lstroke;o</trd>
   <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>,
     <trd>&c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;
     <klr tip="amb"> (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;.)</klr></trd>
   </trdgrp>
</drv>

<drv mrk="klucx.0opedalo">
   <kap><tld/>opedalo</kap>
   <uzo tip="fak">AUT</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Pedalo, servanta por <tld/>i la motoron de a&ubreve;to kun la
       radoj.
     </dif>
   </snc>
   <trd lng="de">Kupplungspedal</trd>
   <trd lng="en">clutch pedal</trd>
   <trd lng="nl">koppelpedaal</trd>
   <trd lng="fr">p&eacute;dale d'<ind>embrayage</ind></trd>
   <trdgrp lng="hu">
     <trd>tengelykapcsol&oacute;-ped&aacute;l</trd>,
     <trd>kuplungped&aacute;l</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>pedale della <ind>frizione</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>peda&lstroke; <ind>sprz&eogonek;g&lstroke;a</ind></trd>,
     <trd>sprz&eogonek;g&lstroke;o<klr> (pot.)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_l;&c_mol;
   &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="klucx.0risorto">
   <kap><tld/>orisorto</kap>
   <uzo tip="fak">AUT</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Risorto, kiu fikstenas <tld/>on a&ubreve; mal<tld/>on.
     </dif>
   </snc>
   <adm>
     La difino uzas nedifinitajn terminojn ~o kaj mal~o (stato, ago?).
     Ankaux ne klaras, kiel oni povas ilin "fiksteni".
     <aut>MB</aut>
   </adm>
   <trd lng="de">Kupplungsfeder</trd>
   <trd lng="en">clutch return spring</trd>
   <trdgrp lng="fr">
     <trd>diaphragme d'<ind>embrayage</ind></trd>,
     <trd>ressort d'<ind>embrayage</ind></trd>
   </trdgrp>
   <trd lng="hu">tengelykapcsol&oacute;-nyom&oacute;rug&oacute;</trd>
   <trdgrp lng="it">
     <trd>molla della <ind>frizione</ind></trd>
   </trdgrp>
   <trd lng="nl">koppelveer</trd>
</drv>

<drv mrk="klucx.0stango">
   <kap><tld/>stango</kap>
   <uzo tip="fak">AUT</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Stango, per kiu oni estigas <tld/>on a&ubreve; mal<tld/>on.
     </dif>
     <ref tip="vid" cel="rapid.0umo">rapidumo</ref>
   </snc>
   <adm>
     PIV2 nomas tion "rapidum-sxangxilo".
     <aut>MB</aut>
   </adm>
   <trd lng="de">Kuppelstange</trd>
   <trdgrp lng="en">
     <trd>clutch arm</trd>,
     <trd>clutch lever</trd>
   </trdgrp>
   <trd lng="nl">koppelstang</trd>
   <trd lng="fr">levier d'<ind>embrayage</ind></trd>
   <trd lng="hu">tengelykapcsol&oacute;kar</trd>
   <trdgrp lng="it">
     <trd>leva della <ind>frizione</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>d&zacute;wignia <ind>sprz&eogonek;g&lstroke;a</ind></trd>
   </trdgrp>
   <trd lng="ru">
   &c_r;&c_y;&c_ch;&c_a;&c_g;
   &c_s;&c_c;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_ja;
   </trd>
</drv>
</art>
