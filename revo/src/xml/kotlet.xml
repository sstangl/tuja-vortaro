<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kotlet.xml,v 1.16 2015/07/04 08:37:38 revo Exp $">
<kap><ofc>1</ofc>
<rad>kotlet</rad>/o</kap>
<drv mrk="kotlet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Rostita peco el ripviando de &scirc;afo, bovido, porko.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_l;&c_je;&c_t;&c_a;</trd>
  <trd lng="cs">kotleta</trd>
  <trdgrp lng="de">
    <trd>Kotelett [kun ripo]</trd>,
    <trd>Schnitzel [sen ripo]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chop</trd>,
    <trd>cutlet</trd>
  </trdgrp>
  <trd lng="es">chuleta</trd>
  <trd lng="fr">c&ocirc;telette</trd>
  <trdgrp lng="hu">
    <trd>bordaszelet</trd>,
    <trd>karaj</trd>,
    <trd>kotlett</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kotelet</trd>,
    <trd>ribkarbonade</trd>
  </trdgrp>
  <trd lng="pl">kotlet</trd>
  <trdgrp lng="pt">
    <trd>costeleta</trd>,
    <trd>costelinha</trd>
  </trdgrp>
  <trd lng="ro">cotlet</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_t;&c_l;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_b;&c_i;&c_v;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">kotleta <klr>(kuch)</klr></trd>
</drv>
</art>
