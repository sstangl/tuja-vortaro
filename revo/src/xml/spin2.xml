<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: spin2.xml,v 1.14 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>spin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="spin2.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Nombro entjera a&ubreve; entjerplusduona, indikanta la
      transformadan karakteron de ondokampo.
    </dif>
     <trd lng="en">spin</trd>
     <trd lng="es">esp&iacute;n</trd>
    <trd lng="hu">spin</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <ref tip="dif" cel="spin2.0momanto"><tld lit="S"/>momanto</ref>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;</trd>
  <trd lng="ca">spin</trd>
  <trd lng="cs">páteř</trd>
  <trd lng="de">Spin</trd>
  <trd lng="en">spin</trd>
  <trd lng="hu">spin</trd>
  <trd lng="pl">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>chrbtica</trd>,
    <trd>chrbtová kosť</trd>
  </trdgrp>
</drv>

<drv mrk="spin2.0momanto">
  <kap><tld/>momanto</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Momanto de movokvanto interna de elementa korpusklo.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Magneta momanto de elementa korpusklo, proporcia al la <tld/>a
      momanto de movokvanto.
    </dif>
  </snc>
  <ref tip="vid" cel="momant.0o">momanto</ref>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;</trd>
  <trd lng="ca">spin</trd>
  <trd lng="en">spin</trd>
  <trd lng="hu">spin</trd>
  <trd lng="pl">spin</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;</trd>
  <trd lng="sk">spinový moment</trd>
  <trd lng="cs">spinový moment <klr>(hybnosti elementárních částic)</klr></trd>
</drv>
</art>
