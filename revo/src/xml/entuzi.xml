<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: entuzi.xml,v 1.16 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>entuziasm</rad>/o
</kap>

<drv mrk="entuzi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="entuzi.0o.PSI">
    <dif>
      Admira a&ubreve; espera fervorego:
      <ekz>
        &ccirc;iu eltrovinto parolas kun <tld/>o pri sia
        eltrova&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>o forprenas la kritikan kapablon;
      </ekz>
      <ekz>
        blinda <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vibr.0i">vibri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_z;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_n;&c_t;&c_u;&c_s;&c_i;&c_a;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>elán</trd>,
    <trd>entuziazmus</trd>,
    <trd>nadšení</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Begeisterung</trd>,
     <trd>Enthusiasmus</trd>
  </trdgrp>
  <trd lng="es">entusiasmo</trd>
  <trd lng="fr">enthousiasme</trd>
  <trdgrp lng="pl">
    <trd>entuzjazm</trd>,
    <trd>zapa&lstroke;</trd>,
    <trd>furora</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entusiasmo</trd>,
    <trd>&acirc;nimo</trd>,
    <trd>vida</trd>,
    <trd>furor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_i;&c_a;&c_z;&c_m;</trd>,
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_a;&c_z;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadšenie</trd>,
    <trd>oduševnenie</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0a">
  <kap><tld/>a</kap>
  <snc mrk="entuzi.0a.PSI">
    <dif>
      Plena je <tld/>o:
      <ekz>
        ili estas <tld/>aj al (por) esp-o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nadšený</trd>,
    <trd>zanícený</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>begeistert</trd>,
    <trd>enthusiastisch</trd>
  </trdgrp>
  <trd lng="fr">enthousiaste<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="pl">
    <trd>entuzjastyczny</trd>,
    <trd>zapalony</trd>,
    <trd>napalony<klr> (na co&sacute;)</klr></trd>
  </trdgrp>
  <trd
  lng="ru">&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nadšený</trd>,
    <trd>zanietený</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="entuzi.0igi.PSI">
    <dif>
      Plenigi je <tld/>o:
      <ekz>
        tiu spektaklo <tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_h;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>nadchnout</trd>,
    <trd>poblouznit</trd>,
    <trd>uvést v nadšení</trd>,
    <trd>zapálit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>begeistern</trd>
  </trdgrp>
  <trd lng="fr">enthousiasmer</trd>
  <trdgrp lng="pl">
    <trd>rozentuzjazmowywa&cacute;</trd>,
    <trd>rozpala&cacute;<klr> (zapa&lstroke;)</klr></trd>,
    <trd>wywo&lstroke;ywa&cacute; <ind>furor&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_l;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadchnúť</trd>,
    <trd>oduševniť</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>emulo</kap></var></kap>
  <snc mrk="entuzi.0ulo.PSI">
    <dif>
      Facile <tld/>i&gcirc;anta homo:
      <ekz>
        la <tld/>uloj estas tre bezonataj por la homa progresado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_s;&c_t;</trd>
  <trd lng="cs">nadšenec</trd>
  <trdgrp lng="de">
     <trd>Begeisterter</trd>,
     <trd>Schw&auml;rmer</trd>
  </trdgrp>
  <trd lng="fr">enthousiaste<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>entuzjasta</trd>,
    <trd>zapaleniec</trd>,
    <trd>napaleniec</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_n;&c_t;&c_u;&c_z;&c_i;&c_a;&c_s;&c_t;</trd>
  <trd lng="sk">nadšenec</trd>
</drv>
</art>
