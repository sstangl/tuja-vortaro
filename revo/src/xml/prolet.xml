<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prolet.xml,v 1.16 2013/09/24 15:10:10 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>prolet</rad>/o
</kap>
<drv mrk="prolet.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Homo, kies nuraj vivrimedoj estas vendo de sia laborforto, &ccirc;u
      korpa,
      &ccirc;u spirita:
      <ekz>
        <tld/>oj el &ccirc;iuj landoj, unui&gcirc;u;
      </ekz>
      <ekz>
        multaj el la intelektuloj estas same <tld/>oj, kiel la
        manlaboristoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="labor.0isto0ulo.proleto">laboristo</ref>
    <ref tip="sin" cel="prolet1.0o">proletario</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_je;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_l;&c_je;&c_t;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="ca">proletari</trd>
  <trd lng="de">Proletarier</trd>
  <trd lng="en">proletarian</trd>
  <trd lng="es">proletario</trd>
  <trd lng="fr">prol&eacute;taire</trd>
  <trd lng="hu">prolet&aacute;r</trd>  
  <trd lng="nl">proletari&euml;r</trd>
  <trd lng="pt">prolet&aacute;rio</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_je;&c_t;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>

<drv mrk="prolet.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <dif>
      Tuto de la klaso de <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_je;&c_t;&c_a;&c_r;&c_y;&c_ja;&c_t;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_l;&c_je;&c_t;&c_a;&c_r;&c_i;&c_a;&c_t;</trd>
  <trd lng="ca">proletariat</trd>
  <trd lng="de">Proletariat</trd>
  <trd lng="en">proletariat</trd>
  <trd lng="es">proletariado</trd>
  <trd lng="fr">prol&eacute;tariat</trd>
  <trd lng="hu">proletari&aacute;tus</trd>  
  <trd lng="nl">proletariaat</trd>
  <trd lng="pt">proletariado</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_l;&c_je;&c_t;&c_a;&c_r;&c_i;&c_a;&c_t;</trd>
</drv>
</art>
