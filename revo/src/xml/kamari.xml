<vortaro>
<art mrk="$Id: kamari.xml,v 1.14 2015/10/16 12:10:18 revo Exp $">
<kap>
  <rad>kamaril</rad>/o
</kap>
<drv mrk="kamari.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="grup.0acxo">Grupa&ccirc;o</ref> de intrigantoj en
      kortego, en registaro.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_r;&c_y;&c_l;&c_mol;&c_ja;</trd>
  <trd lng="ca">camarilla</trd>
  <trdgrp lng="cs">
    <trd>dru&zcaron;ina kr&aacute;lovsk&aacute;</trd>,
    <trd>kamarila</trd>
  </trdgrp>
  <trd lng="de">Kamarilla</trd>
  <trdgrp lng="en">
    <trd>camarilla</trd>,
    <trd>cabal</trd>
  </trdgrp>
  <trd lng="es">camarilla</trd>
  <trd lng="fr">camarilla</trd>
  <trd lng="hu">kamarilla</trd>
  <trd lng="nl">hofkliek</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_mol;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>kamarila</trd>,
    <trd>kr&aacute;&lcaron;ovsk&aacute; dru&zcaron;ina</trd>
  </trdgrp>
</drv>
</art>
