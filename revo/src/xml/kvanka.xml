<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvanka.xml,v 1.20 2014/06/30 08:10:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kvankam</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvanka.0">
   <kap>
     <ofc>*</ofc><tld/>
   </kap>
   <gra><vspec>dependiga konjunkcio</vspec></gra>
   <snc mrk="kvanka.0.KOMUNE">
     <dif>
       Spite al jena malhelpo, malgra&ubreve; ke:
       <ekz>
         mi vidas nenion, <tld/> &ccirc;io estas luma<fnt>Z</fnt>;
       </ekz>
       <ekz>
         pre&gcirc;adi mi ne povas, <tld/> forte mi volus 
pre&gcirc;i<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="spit.0eal">spite al</ref>,
       <ref cel="malgra.0ke">malgra&ubreve; ke</ref>
     </refgrp>
     <rim num="1">
       La verbo <em>esti</em> kelkfoje estas subkomprenita:
       <ekz>
         ni voja&gcirc;is kune, <tld/> malamikoj.
       </ekz>
     </rim>
     <rim num="2">
       La korelativo de <em>kvankam</em> estas <em>tamen</em>. Kelkaj
       uzas en tiu senco <em>sed</em>, kiun oni devas severe eviti.
     </rim>
   </snc>
   <trdgrp lng="be">
     <trd>&c_h;&c_o;&c_c;&c_mol;</trd>,
     <trd>&c_h;&c_a;&c_c;&c_ja;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_ch;&c_je;</trd>
   <trd lng="cs">a&ccaron;koliv</trd>
   <trdgrp lng="de">
     <trd>obwohl</trd>,
     <trd>obgleich <klr>[arkaika]</klr></trd>,
     <trd>obschon <klr>[arkaika]</klr></trd>,
     <trd>obzwar <klr>[arkaika]</klr></trd>
   </trdgrp>
   <trd lng="es">aunque</trd>
   <trd lng="fi">vaikka</trd>
   <trd lng="fr">bien que</trd>
   <trdgrp lng="hu">
     <trd>noha</trd>,
     <trd>hab&aacute;r</trd>,
     <trd>j&oacute;llehet</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>alhoewel</trd>,
     <trd>hoewel</trd>,
     <trd>ofschoon</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>embora <klr tip="amb">(conj.)</klr></trd>,
     <trd>ainda que</trd>,
     <trd>conquanto</trd>,
     <trd>posto que</trd>,
     <trd>apesar de</trd>,
     <trd>se <ind>bem</ind> que</trd>,
     <trd>n&atilde;o <ind>obstante</ind></trd>,
     <trd>em <ind>que</ind></trd>
   </trdgrp>
   <trd lng="ru">&c_h;&c_o;&c_t;&c_ja;</trd>
   <trd lng="sv">fast&auml;n</trd>
   <trd lng="vo">do</trd>
</drv>
</art>
