<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ileks.xml,v 1.17 2013/08/26 08:10:19 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ileks</rad>/o
</kap>
<drv mrk="ileks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="akvifo1.0oj">akvifoliacoj</ref>
      (<trd lng="la">Ilex</trd>), kies plej konata specio estas
      <ref tip="sub" cel="akvifo.0o">akvifolio</ref>.
    </dif>
  </snc>
  <trd lng="de">Stechpalme</trd>
  <trd lng="en">holly</trd>
  <trd lng="fr">ilex</trd>
  <trd lng="hu">magyal(fa)</trd>
  <trd lng="nl">hulst</trd>
  <trd lng="pl">ostrokrzew</trd>
  <trdgrp lng="pt">
   <trd>azevinho</trd>, 
   <trd>azevim</trd>, 
   <trd>azevinheiro</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_d;&c_u;&c_b;</trd>
  <trd lng="sv">j&auml;rnekar</trd>
</drv>
</art>
