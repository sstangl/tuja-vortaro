<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dekret.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>dekret</rad>/o
</kap>
<drv mrk="dekret.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Oficiala ordono de plej supera &scirc;tata reganta povo (kontraste
      al le&gcirc;o):
      <ekz>
        eldoni <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bule.0o">buleo</ref>,
      <ref cel="edikt.0o">edikto</ref>,
      <ref cel="ukaz.0o">ukazo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_r;&c_e;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>decret</trd>,
    <trd>ordre</trd>,
    <trd>edicte</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dekret</trd>,
    <trd>nařízení</trd>,
    <trd>právní nařízení</trd>,
    <trd>vyhláška</trd>,
    <trd>vynos</trd>,
    <trd>výměr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erlass</trd>,
    <trd>Verordnung</trd>,
    <trd>Verf&uuml;gung</trd>,
    <trd>Dekret [arkaika]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decree</trd>,
    <trd>edict</trd>,
    <trd>order</trd>
  </trdgrp>
  <trd lng="es">decreto</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_k;&a_m;</trd>,
    <trd>&a_A;&a_m;&a_r;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cret</trd>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyrendelet</trd>,
    <trd>dekr&eacute;tum</trd>
  </trdgrp>
  <trd lng="pl">dekret</trd>
  <trd lng="pt">decreto</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_k;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_u;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>listina</trd>,
    <trd>nariadenie</trd>
  </trdgrp>
</drv>
<drv mrk="dekret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ordoni per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>decretar</trd>,
    <trd>preceptuar</trd>,
    <trd>ordenar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>přidělit</trd>,
    <trd>ustanovit</trd>,
    <trd>vydat dekret</trd>,
    <trd>vydat úřední rozhodnutí</trd>,
    <trd>vyměřit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erlassen</trd>,
    <trd>verordnen</trd>,
    <trd>verf&uuml;gen</trd>,
    <trd>dekretieren [arkaika]</trd>
  </trdgrp>
  <trd lng="en">decree</trd>
  <trd lng="es">decretar</trd>
  <trdgrp lng="fa">
    <trd>&a_H;&f_k;&a_m; &a_S;&a_A;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_m;&a_r;&f_y;&a_h; &a_S;&a_A;&a_d;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;cr&eacute;ter</trd>
  <trd lng="hu">elrendel</trd>
  <trd lng="pl">dekretowa&cacute;</trd>
  <trd lng="ru">
  &c_d;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
  </trd>
  <trd lng="sk">vydať dekrét</trd>
</drv>
</art>
