<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teac.xml,v 1.10 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>teac</rad>/oj
</kap>
<drv mrk="teac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Theaceae</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_ch;&c_a;&c_je;&c_v;&c_i;</trd>
  <trd lng="ca">te&agrave;cies</trd>
  <trd lng="cs">čajovníkové <klr>(čeleď)</klr></trd>
  <trd lng="de">Teegew&auml;chse</trd>
  <trdgrp lng="hu">
    <trd>teacserjef&eacute;l&eacute;k</trd>,
    <trd>teaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">theeboomfamilie</trd>
  <trdgrp lng="pt">
   <trd>te&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_j;&c_n;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_ch;&c_a;&c_je;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;<klr
tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">čajovníkové</trd>
</drv>
</art>
