<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rodi.xml,v 1.16 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>rodi</rad>/o
</kap>
<drv mrk="rodi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Rh</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="45">elemento</ref> kun
      atomnumero 45, atompezo 102,905;
      transir<ref tip="super" cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_r;&c_o;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">rodiom</trd>
  <trd lng="cs">rhodium</trd>
  <trd lng="de">Rhodium</trd>
  <trd lng="en">rhodium</trd>
  <trd lng="es">rodio</trd>
  <trd lng="fr">rhodium</trd>
  <trd lng="hu">r&oacute;dium</trd>
  <trd lng="id">rodium</trd>
  <trd lng="it">rodio</trd>
  <trd lng="la">Rhodium</trd>
  <trd lng="nl">rhodium</trd>
  <trd lng="pl">rod</trd>
  <trd lng="pt">r&oacute;dio</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_j;</trd>
  <trd lng="sk">rhodium</trd>
  <trd lng="sv">rodium</trd>
</drv>
</art>
