<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: solid.xml,v 1.33 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>solid</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="solid.0a">
  <kap><tld/>a</kap>
  <snc mrk="solid.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Malfluida, malflueca; kvalifikas substancon, kies molekuloj estas
      fikse kunigitaj unu kun la alia, tio estas &ccirc;iun substancon, kiu
      estas nek flueca nek gasa.
    </dif>
    <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>
    <trd lng="de">fest</trd>
    <trd lng="hu">szil&aacute;rd</trd>
    <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="solid.0a.firma">
    <dif>
      <ref tip="dif" cel="firm.0a.fiksita">Firma<sncref/></ref>,
      malfacile detruebla:
      <ekz>
        <tld/>aj botoj;
      </ekz>
      <ekz>
        <tld/>a horlo&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a fadeno, ligilo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_y;&c_v;&c_a;&c_l;&c_y;</trd>,
      <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_u;&c_zh;&c_y;</trd>
    </trdgrp>
    <trd lng="de">fest</trd>
    <trdgrp lng="hu">
      <trd>er&odblac;s</trd>,
      <trd>tart&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.serioza">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Serioza, impona, ne&scirc;erca, malfrivola:
      <ekz>
        fari al iu <tld/>an batadon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <tld/>an moralinstruon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a argumento</ind>;
        <trd lng="hu">komoly &eacute;rv</trd>
      </ekz>
      <ekz>
        tio estos alporto de <tld/>a utilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>e purigadis la lanternon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e bati iun<fnt>Z</fnt>;
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ernsthaft</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>komoly</trd>,
      <trd>j&oacute;zan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="solid.0a.fidinda">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fidinda:
      <ekz>
        serioza kaj <tld/>a homo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a firmo;
      </ekz>
      <ekz>
        <tld/>a spirito<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a sciencisto;
      </ekz>
      <ekz>
        <tld/>aj scioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        homo de <tld/>a (matura) a&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_j;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_n;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vertrauensw&uuml;rdig</trd>,
      <trd>seri&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szolid</trd>,
      <trd>megb&iacute;zhat&oacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_l;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_jo;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>důvěryhodný</trd>,
    <trd>masivní</trd>,
    <trd>pevný</trd>,
    <trd>solidní</trd>,
    <trd>spolehlivý</trd>,
    <trd>tuhý</trd>,
    <trd>tělesový</trd>
  </trdgrp>
  <trd lng="es">s&oacute;lido</trd>
  <trd lng="fr">solide<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="nl">
    <trd>vast</trd>,
    <trd>stevig</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pevný</trd>,
    <trd>solídny</trd>,
    <trd>spoľahlivý</trd>,
    <trd>tuhý</trd>
  </trdgrp>
  <trd lng="tp">kiwen</trd>
</drv>

<drv mrk="solid.0o">
  <kap><tld/>o</kap>
  <snc mrk="solid.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Korpo en <tld/>a stato:
      <ekz>
        hidrargo estas la nura metalo, kiu
        je ordinara temperaturo ne estas <tld/>o.
      </ekz>
    </dif>
    <trd lng="fr">solide<klr tip="ind"> (corps solide)</klr></trd>
    <trd lng="hu">szil&aacute;rdtest</trd>
  </snc>
  <snc mrk="solid.0o.kinematiko">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(kinematiko)</uzo>
    <dif>
      Tia objekto en la tridimensia spaco, ke la distanco inter
      du ajnaj &gcirc;iaj punktoj ne &scirc;an&gcirc;i&gcirc;as dum
      movo.
    </dif>
    <trd lng="en">rigid body</trd>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>Festk&ouml;rper</trd>
    </trdgrp>
    <trd lng="fr">solide ind&eacute;formable<klr tip="ind">
    (cin&eacute;matique)</klr></trd>
    <trd lng="hu">merev test</trd>
    <trd lng="pl">cia&lstroke;o sta&lstroke;e</trd>
    <trd lng="es">s&oacute;lido r&iacute;gido</trd>
  </snc>
  <snc mrk="solid.0o.geometrio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Tridimensia
      <ref tip="super" cel="figur.geometria0o">geometria figuro</ref>.
    </dif>
    <refgrp tip="sin">
      Malnovaj ekvivalentoj:
      <ref cel="korp.0o.solido">korpo</ref>,
      <ref cel="volume.0ajxo">volumena&jcirc;o</ref>;
    </refgrp>
    <refgrp tip="sub">
      Specifaj solidoj:
      <ref cel="angul.solida0o"><tld/>a angulo</ref>,
      <ref cel="edr.du0o">duedro</ref>,
      <ref cel="edr.tri0o">triedro</ref>,
      <ref cel="edr.plur0o">pluredro</ref>,
      <ref cel="sfer.0o.solido">sfero</ref>,
      <ref cel="cilind.0o.solido">cilindro</ref>,
      <ref cel="konus.0o.solido">konuso</ref>,
      <ref cel="elipso.0o">elipsoido</ref>,
      <ref cel="sektor.0o.de_sfero">sektoro</ref>,
      <ref cel="segmen.0o.sfero">segmento</ref>,
      <ref cel="tor.0o.MAT">toro</ref>;
    </refgrp>
    <refgrp tip="vid">
      Rimarkindaj ecoj de solido:
      <ref cel="bar.0ita.metrikaspaco">barita</ref>,
      <ref cel="bar.ne0ita.MAT">nebarita</ref>,
      <ref cel="konvek.0a.subaro">konveksa</ref>,
      <ref cel="konkav.0a.subaro">konkava</ref>,
      <ref cel="rivolu.0a.figuro">rivolua</ref>;
      atributoj de iuj solidoj :
      <ref cel="are.0o.mezuro">areo</ref>,
      <ref cel="rand.0o.figuro">rando</ref>,
      <ref cel="volume.0o.MAT">volumeno</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_je;&c_l;&c_a;</trd>,
      <trd>&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>K&ouml;rper</trd>,
      <trd>geometrischer <ind>K&ouml;rper</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>geometric <ind>body</ind></trd>,
      <trd>solid</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>solide<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>volume<klr tip="ind"> (figure g&eacute;om&eacute;trique)</klr></trd>,
      <trd>corps g&eacute;om&eacute;trique</trd>
    </trdgrp>
    <trd lng="hu">(m&eacute;rtani) <ind>test</ind></trd>
    <trd lng="pl">bry&lstroke;a</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;&c_je; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
  <trd lng="cs">těleso</trd>
  <trd lng="es">s&oacute;lido</trd>
  <trd lng="ru">&c_t;&c_v;&c_jo;&c_r;&c_d;&c_o;&c_je; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
  <trd lng="sk">teleso</trd>
</drv>

<drv mrk="solid.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="solid.0eco.FIZ">
    <dif>
      Kvalito de io densa, eltenema, malfacile
      rompebla, fidinda:
      <ekz>
        tio pruvas la <tld/>econ de la nova vitro;
      </ekz>
      <ekz>
        li &scirc;ajnis konvena homo kaj &ccirc;io en li montris
        <tld/>econ<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>solidnost</trd>,
    <trd>tuhost</trd>
  </trdgrp>
  <trd lng="fr">solidit&eacute;</trd>
  <trdgrp lng="sk">
    <trd>dôkladnosť</trd>,
    <trd>slušnosť</trd>,
    <trd>solídnosť</trd>,
    <trd>spoľahlivosť</trd>
  </trdgrp>
</drv>

<drv mrk="solid.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="solid.0igi.FIZ">
    <dif>
      Igi <tld/>a, kunteni&gcirc;a, ne facile diserigebla
      a&ubreve; rompebla, firma, fidinda.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>consolider</trd>,
    <trd>solidifier</trd>
  </trdgrp>
</drv>

<drv mrk="solid.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="solid.0igxi.FIZ">
    <dif>
      I&gcirc;i <tld/>a, rezista, malmola,
      nerompi&gcirc;ema, nefleksebla, serioza:
      <ekz>
        ili estas iom malbonkondutaj, sed kun la tempo ili jam
        <tld/>i&gcirc;os<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>consolider</ind></trd>,
    <trd>se <ind>solidifier</ind></trd>
  </trdgrp>
</drv>

<drv mrk="solid.platona0o">
  <kap>platona <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib><lok>regula</lok></fnt>
  <snc>
    <dif>
      <ref cel="regul.0a.pluredro">Regula</ref>
      <ref tip="super" cel="edr.plur0o">pluredro</ref>.
    </dif>
    <ref tip="vid" cel="platon.0o">Platono.</ref>
    <trd lng="be">&c_P;&c_l;&c_ja;&c_t;&c_o;&c_n;&c_a; <ind>&c_c;&c_je;&c_l;&c_a;</ind></trd>
    <trd lng="de">Platonischer <ind>K&ouml;rper</ind></trd>
    <trd lng="en">Platonic <ind>body</ind></trd>
    <trd lng="fr">solide platonicien</trd>
    <trdgrp lng="hu">
      <trd>szab&aacute;lyos <ind>test</ind></trd>,
      <trd>platoni <ind>test</ind></trd>
    </trdgrp>
    <trd lng="pl">bry&lstroke;y plato&nacute;skie</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_o;&c_n;&c_o;&c_v;&c_o; <ind>&c_t;&c_je;&c_l;&c_o;</ind></trd>
  </snc>
  <trd lng="cs">platónské těleso</trd>
  <trd lng="sk">platónske teleso</trd>
</drv>
</art>
