<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gx.xml,v 1.5 2014/04/12 07:10:29 revo Exp $">
<kap>
    <ofc>*</ofc><rad>&gcirc;</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="gx.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a <ref
      tip="dif" cel="afrika.0o">afrikata</ref> <ref tip="dif"
      cel="gingiv.0o">gingivalo</ref> [d&#865;&#658;]. 	
    </dif>
  <trd lng="fr">&gcirc;<klr> (consonne)</klr></trd>
  </snc>
  <snc>
     <dif>
      La na&ubreve;a <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto.
     </dif>
  <trd lng="fr">&gcirc;<klr> (lettre)</klr></trd>
  </snc>
</drv>
</subart>

<subart>
<drv mrk="gx.0.nomo">
  <kap><tld/>o</kap>
  <snc>   
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  </snc>
  <trd lng="fr">dj</trd>
  </drv>
</subart>
</art>

