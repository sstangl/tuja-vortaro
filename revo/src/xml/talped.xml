  <trd lng="hu">vakondokf&eacute;l&eacute;k</trd>
  <trd lng="nl">mollen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pt">
   <trd>talp&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_t;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
