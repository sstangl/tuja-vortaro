<vortaro>
<art mrk="$Id: anasti.xml,v 1.18 2015/09/17 22:10:20 revo Exp $">
<kap>
  <rad>anastigmat</rad>/a
</kap>
<drv mrk="anasti.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Senigita je astigmateco.
    </dif>
  </snc>
  <trd
  lng="be">&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">anastigm&agrave;tic</trd>
  <trd lng="cs">anastigmatick&yacute;</trd>
  <trd lng="de">anastigmatisch</trd>
  <trd lng="en">anastigmatic</trd>
  <trd lng="es">anastigm&aacute;tico</trd>
  <trd lng="fr">anastigmatique</trd>
  <trd lng="nl">anastigmatisch</trd>
  <trd lng="pl">anstygmatyczny</trd>
  <trd
  lng="ru">&c_a;&c_n;&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">anastigmatick&yacute;</trd>
  <trd lng="tr">anastigmat</trd>
</drv>

<drv mrk="anasti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <tld lit="A"/>a objektivo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_b;'&c_je;&c_k;&c_t;&c_y;&c_w;-&c_a;&c_n;&c_a;&c_s;&c_t;&c_y;&c_g;&c_m;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">anastigmat</trd>
  <trd lng="fr">anastigmat</trd>
  <trdgrp lng="pl">
    <trd>anastygmat</trd>,
    <trd>obiektyw <ind>anastygmatyczny</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;-&c_a;&c_n;&c_a;&c_s;&c_t;&c_i;&c_g;&c_m;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anastigmatick&aacute; <ind>&scaron;o&scaron;ovka</ind></trd>,
    <trd>anastigmatick&aacute; &scaron;o&scaron;ovka <klr>(objekt&iacute;v)</klr></trd>
  </trdgrp>
  <trd lng="tr">anastigmat</trd>
</drv>
</art>
