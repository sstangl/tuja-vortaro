<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ventri.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>ventrikl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ventri.0o">
  <kap><tld/>o</kap>
  <snc mrk="ventri.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Nomo donata al kava&jcirc;oj de kelkaj organoj:
      <ekz>
        la koro havas du vestiblojn kaj du <tld/>ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxrik">a&ubreve;riklo</ref>,
      <ref cel="vestib.0o.ANA">vestiblo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_u;&c_n;&c_a;&c_ch;&c_a;&c_k;</trd>
  <trd lng="ca">ventricle</trd>
  <trdgrp lng="cs">
    <trd>komora</trd>,
    <trd>tělní dutina</trd>,
    <trd>ventrikl</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ventrikel</trd>,
    <trd>Kammer</trd>
  </trdgrp>
  <trd lng="en">ventricle</trd>
  <trd lng="es">ventr&iacute;culo</trd>
  <trd lng="fr">ventricule</trd>
  <trd lng="hu">(sz&iacute;v)<ind>kamra</ind></trd>
  <trd lng="it">ventricolo</trd>
  <trdgrp lng="nl">
    <trd>ventrikel</trd>,
    <trd>kamer<klr tip="ind"> (v.hart)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">                                        
    <trd>jama <klr>(anatomia)</klr></trd>,      
    <trd>komora <klr>(serca)</klr></trd>,                               
       
  </trdgrp>
  <trd lng="pt">ventr&iacute;culo</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_ch;&c_je;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>komora</trd>,
    <trd>telesná dutina</trd>
  </trdgrp>
</drv>
</art>
