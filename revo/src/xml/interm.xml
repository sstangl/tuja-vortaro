<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: interm.xml,v 1.20 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>intermit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interm.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="interm.0i.KOMUNE">
    <dif>
      Periode interrompi&gcirc;i, kaj rekomenci&gcirc;i ordinare post egalaj
      intertempoj:
      <ekz>
        la febro <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>intermitovat</trd>,
    <trd>přerušovat se</trd>,
    <trd>přestávat</trd>,
    <trd>vynechávat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittieren</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzen</ind></trd>
  </trdgrp>
  <trd lng="en">be intermittent</trd>
  <trd lng="es">intermitir</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>intermittent</ind></trd>,
    <trd>s'<ind>interrompre</ind> p&eacute;riodiquement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakozik</ind></trd>,
    <trd>v&aacute;ltakozva <ind>kihagy</ind></trd>,
    <trd>szaggatottan m&udblac;k&ouml;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>intermitteren</trd>,
    <trd>met <ind>tussenpozen</ind> werken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przerwom ulega&cacute;</trd>,
    <trd>ustawa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">intermitir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prerušovať</trd>,
    <trd>vynechávať</trd>
  </trdgrp>
</drv>

<drv mrk="interm.0o">
  <kap><tld/>o</kap>
  <snc mrk="interm.0o.KOMUNE">
    <dif>
      Periodo, dum kiu io <tld/>anta interrompi&gcirc;as:
      <ekz>
        li reprenis sian laboron dum la <tld/>oj de lia malsano;
      </ekz>
      <ekz>
        en la unua momento de <tld/>o <klr>(de la socia premo)</klr>
        mi refari&gcirc;as tio, kion la naturo volis
        <fnt><bib>Revadoj</bib>, <lok>Na&ubreve;a promenado</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>přerušení</trd>,
    <trd>přestávka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterbrechungsdauer</trd>,
    <trd>Aussetzzeit</trd>
  </trdgrp>
  <trd lng="en">intermission</trd>
  <trd lng="es">intermitencia</trd>
  <trdgrp lng="fr">
    <trd>interruption<klr tip="ind"> (d'un ph&eacute;nom&egrave;ne 
intermittent)</klr></trd>,
    <trd>intervalle<klr tip="ind"> (entre deux occurrences)</klr></trd>,
    <trd>r&eacute;pit<klr tip="ind"> (entre deux occurrences)</klr></trd>,
    <trd>rel&acirc;che<klr tip="ind"> (fig., interruption)</klr></trd>,
    <trd>tr&ecirc;ve<klr tip="ind"> (fig., interruption)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&odblac;szakos <ind>sz&uuml;net</ind></trd>,
    <trd>id&odblac;szakos <ind>kimarad&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="nl">tussenpoos</trd>
  <trdgrp lng="pl">
    <trd>intermisja</trd>,
    <trd>przerwy okres</trd>
  </trdgrp>
  <trd lng="pt">intermit&ecirc;ncia</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_r;&c_y;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>prerušenie</trd>,
    <trd>prestávka</trd>
  </trdgrp>
</drv>

<drv mrk="interm.0a">
  <kap><tld/>a</kap>
  <snc mrk="interm.0a.KOMUNE">
    <dif>
      <tld lit="I"/>anta:
      <ekz>
        <tld/>a fonto, febro, pulso;
      </ekz>
      <ekz>
        lumturo kun <tld/>aj ekbriloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>intermitentní</trd>,
    <trd>přerušovaný přestávkami</trd>,
    <trd>přerývaný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittierend</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzend</ind></trd>
  </trdgrp>
  <trd lng="en">intermittent</trd>
  <trd lng="es">intermitente</trd>
  <trd lng="fr">intermittent</trd>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakoz&oacute;</ind></trd>,
    <trd>id&odblac;szakosan <ind>kimarad&oacute;</ind></trd>,
    <trd>szaggatott</trd>
  </trdgrp>
  <trd lng="nl">intermitterend</trd>
  <trdgrp lng="pl">
    <trd>intermistyczny</trd>,
    <trd>przerywany</trd>
  </trdgrp>
  <trd lng="pt">intermitente</trd>
  <trd lng="ro">intermitent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_zh;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>občasný</trd>,
    <trd>prerušovaný</trd>
  </trdgrp>
</drv>

<drv mrk="interm.0e">
  <kap><tld/>e</kap>
  <snc mrk="interm.0e.KOMUNE">
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        la luno <tld/>e brilis inter la nuboj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chvílemi</trd>,
    <trd>přerušeně</trd>,
    <trd>přerušovaně</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>intermittierend</trd>,
    <trd>von Zeit zu Zeit <ind>aussetzend</ind></trd>
  </trdgrp>
  <trd lng="en">intermittently</trd>
  <trd lng="es">intermitentemente</trd>
  <trdgrp lng="fr">
    <trd>par <ind>intermittence</ind></trd>,
    <trd>par <ind>acc&egrave;s</ind></trd>,
    <trd>par <ind>intervalle</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>(id&odblac;ben) <ind>v&aacute;ltakozva</ind></trd>,
    <trd>szaggatottan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>met <ind>tussenpozen</ind></trd>
  </trdgrp>
  <trd lng="pl">z przerwami</trd>
  <trd lng="pt">intermitentemente</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_i;&c_s;&c_t;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>prerušovane</trd>,
    <trd>striedavo</trd>
  </trdgrp>
</drv>

<drv mrk="interm.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="interm.sen0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="romp.sen0a.KOMUNE">Senrompa</ref>,
      <ref tip="vid" cel="kontin3.0a.KOMUNE">kontinua</ref>:
      <ekz>
        ero en &ccirc;en' sen<tld/>a
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url
          ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">persistant</trd>
</drv>
</art>
