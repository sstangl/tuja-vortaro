<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oliban.xml,v 1.14 2016/03/12 14:10:16 revo Exp $">
<kap>
  <rad>oliban</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oliban.0o">
  <kap><tld/>o</kap>
  <snc mrk="oliban.0o.PAR">
    <dif>
      <ref tip="super" cel="rezin.0o">Rezino</ref> 
      eltirata el abisina arbo kaj uzata kiel incenso a&ubreve;
      medikamentero:
      <ekz>
        <tld/>o ja ne povas tiom &ccirc;i odora&ccirc;i
        <fnt>
          <aut>trad. M. Blahu&scirc;</aut>,
          <vrk><url ref="http://filmoj.net/kusxejetoj">
          Ku&scirc;ejetoj</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_ib;&c_m;&c_ib;&c_ja;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>arabsk&eacute; kadidlo</trd>,
    <trd>olibanum</trd>,
    <trd>olib&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Olibanum</trd>,
     <trd>Weihrauchharz</trd>
  </trdgrp>
  <trd lng="en">frankincense</trd>
  <trdgrp lng="fr">
    <trd>encens naturel</trd>,
    <trd>oliban</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>jegenyefeny&odblac;-gyanta</trd>,
     <trd>feny&odblac;t&ouml;mj&eacute;n</trd>
  </trdgrp>
  <trd lng="nl">wierook</trd>
  <trd lng="pt">ol&iacute;bano</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_f;&c_i;&c_m;&c_i;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arabsk&eacute; kadidlo</trd>,
    <trd>olib&aacute;n</trd>
  </trdgrp>
</drv>
</art>
