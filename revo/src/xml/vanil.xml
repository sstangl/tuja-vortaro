<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vanil.xml,v 1.25 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vanil</rad>/o
</kap>
<drv mrk="vanil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref>
       el familio <ref tip="malprt"
      cel="orkidac.0oj">orkidacoj</ref>
      (<trd lng="la">Vanilla</trd>). La grajnujon de kelkaj specioj,
      ekz.
      <trd lng="la">Vanilla planifolia</trd>) oni uzas kiel parfumigan 
      <ref tip="lst" cel="spic.0ajxo" lst="voko:spicoj">spica&jcirc;on</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="br">vanilha</trd>
  <trd lng="ca">vainilla</trd>
  <trd lng="cs">vanilka <klr>(koření)</klr></trd>
  <trd lng="de">Vanille</trd>
  <trd lng="el">&beta;&alfa;&ny;&jota_ton;&lambda;&jota;&alfa;</trd>
  <trd lng="en">vanilla</trd>
  <trd lng="es">vainilla</trd>
  <trd lng="fa">&a_w;&a_A;&a_n;&f_y;&a_l;</trd>
  <trd lng="fr">vanille</trd>
  <trdgrp lng="he">
    <trd>&shin;&nun;&fpe;</trd>,
    <trd>&vav;&nun;&yod;&lamed;</trd>
  </trdgrp>
  <trd lng="hu">van&iacute;lia</trd>
  <trd lng="id">vanili</trd>
  <trd lng="nl">vanille</trd>
  <trd lng="pt">baunilha</trd>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_i;&c_l;&c_mol;</trd>
  <trd lng="sk">vanilovník</trd>
  <trd lng="tr">vanilya</trd>
</drv>
</art>
