<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akvedu.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akvedukt</rad>/o
</kap>
<drv mrk="akvedu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Masona&jcirc;a kondukilo, subtera a&ubreve; surponta, kiu kondukas akvon
      inter du malproksimaj lokoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_v;&c_e;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>akvadukt</trd>,
    <trd>antický vodovod</trd>
  </trdgrp>
  <trd lng="de">Aqu&auml;dukt</trd>
  <trd lng="en">aqueduct</trd>
  <trd lng="es">acueducto</trd>
  <trd lng="fr">aqueduc</trd>
  <trd lng="hu">v&iacute;zvezet&eacute;k<klr tip="ind"> (aquaeductus)</klr></trd>
  <trd lng="nl">aquaduct</trd>
  <trd lng="pt">aqueduto</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_v;&c_je;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akvadukt</trd>,
    <trd>antický vodovod</trd>,
    <trd>vodovod</trd>
  </trdgrp>
</drv>
</art>
