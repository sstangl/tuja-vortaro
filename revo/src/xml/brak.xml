<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brak.xml,v 1.47 2016/02/28 19:10:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brak</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brak.0o.homa_membro">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La supera <ref tip="super" cel="membr.0o.ANA">membro</ref>
      &ccirc;e la
      homo:
      <ekz>
        grandan paperujon mi portas sub la <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o en <tld/>o ili iris
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i pa&scirc;is &ccirc;e
        <klr>(sub)</klr><fnt>Z</fnt> la <tld/>o
        de sia edzo.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_u;&c_k;&c_a;<klr>
    (&c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>
    <trd lng="bg">&c_r;&c_malmol;&c_k;&c_a;</trd>
    <trd lng="de">Arm</trd>
    <trd lng="hu">kar</trd>
    <trd lng="nl">arm</trd>
    <trd lng="pl">r&eogonek;ka</trd>
    <trd lng="pt">bra&ccedil;o</trd>
    <trd lng="ru">&c_r;&c_u;&c_k;&c_a;
    <klr>(&c_v;&c_s;&c_ja;)</klr></trd>
    <trd lng="sk">pa&zcaron;a</trd>
    <trd lng="sv">arm</trd>
  </snc>
  <snc mrk="brak.0o.sxultro_gxis_kubuto">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de tiu membro ekde &scirc;ultro &gcirc;is kubuto.
    </dif>
    <trd lng="be">&c_r;&c_u;&c_k;&c_a; <klr>(&c_a;&c_d;
    &c_p;&c_l;&c_ja;&c_ch;&c_a; &c_d;&c_a;
    &c_k;&c_ib;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trd lng="de">Oberarm</trd>
    <trd lng="en">upper arm</trd>
    <trd lng="hu">felkar</trd>
    <trd lng="nl">bovenarm</trd>
    <trd lng="pl">rami&eogonek;</trd>
    <trd lng="ru">&c_r;&c_u;&c_k;&c_a; <klr>(&c_o;&c_t;
    &c_p;&c_l;&c_je;&c_ch;&c_a; &c_d;&c_o;
    &c_k;&c_i;&c_s;&c_t;&c_i;)</klr></trd>
    <trd lng="sk">rameno</trd>
    <trd lng="sv">&ouml;verarm</trd>
  </snc>
  <snc mrk="brak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Parto de objekto, kiu etendi&gcirc;as, flanke de &gcirc;i kiel
      <tld/>o:
      <ekz>
        <tld/>o de cirkelo, de argano, de kandelabro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        de lustro, de helico, de levilo, de rivero;
      </ekz>
      <ekz>
        la mano, ku&scirc;anta sur <tld/>o de la se&gcirc;o, tremetis
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 19a, la
        krizo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="al1.0o">alo</ref>,
      <ref cel="brancx.0o.FIG">bran&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_ch;&c_o;<klr>(&c_r;&c_y;&c_ch;&c_a;&c_g;&c_a;)</klr></trd>,
      <trd>&c_r;&c_u;&c_k;&c_a;&c_w;
      <klr>(&c_r;&c_a;&c_k;&c_ib;)</klr></trd>
    </trdgrp>
    <trd lng="de">Arm</trd>
    <trd lng="hu">kar</trd>
    <trd lng="nl">arm</trd>
    <trd lng="pl">rami&eogonek;</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_ch;&c_o;<klr>
      (&c_r;&c_y;&c_ch;&c_a;&c_g;&c_a;)</klr></trd>,
      <trd>&c_r;&c_u;&c_k;&c_a;&c_v;<klr>
      (&c_r;&c_je;&c_k;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="br">brec'h</trd>
  <trd lng="ca">bra&ccedil;</trd>
  <trdgrp lng="cs">
    <trd>pa&zcaron;e</trd>,
    <trd>rameno</trd>,
    <trd>ruka</trd>,
    <trd>r&aacute;m&ecaron;</trd>
  </trdgrp>
  <trd lng="en">arm</trd>
  <trd lng="es">brazo</trd>
  <trd lng="fr">bras</trd>
  <trd lng="he">&zayin;&resh;&vav;&ayin;</trd>
  <trd lng="it">braccio</trd>
  <trdgrp lng="ku">
    <trd>bask</trd>,
    <trd>mil</trd>,
    <trd>nig</trd>
  </trdgrp>
  <trd lng="oc">bra&ccedil;</trd>
  <trd lng="tp">luka</trd>
  <trd lng="tr">kol</trd>
  <trd lng="vo">lam</trd>
</drv>

<drv mrk="brak.0en0e">
  <kap><tld/>en<tld/>e</kap>
  <snc mrk="brak.0en0e.MIEN">
    <dif>
      <tld lit="B"/>on intermetitan kun <tld/>o de apudulo:
      <ekz>
        oni ameme gvidas lin, <tld/>en<tld/>e, la&ubreve;
        la stratetoj de la provinca urbo
        <fnt>
          <aut>S. Rodin</aut>,
          <vrk>Bildoj pri Norda Lando</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">arm in arm</trd>
  <trd lng="fr">bras-dessus bras-dessous</trd>
  <trd lng="pl">pod <ind>r&eogonek;k&eogonek;</ind></trd>
</drv>

<drv mrk="brak.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif"
    cel="brak.cxirkaux0i">&ccirc;irka&ubreve;<tld/>i</ref>
    <ekz>
      &scirc;i tre vigli&gcirc;is <klr>[kaj]</klr> sin premis al mi
      <tld/>umante min je la kolo
      <fnt><bib>MkM</bib><lok>&ccirc;ap. 13</lok></fnt>;
    </ekz>
    <ekz>
      mi vin kisis, kisis sur kanap', mi vin alpremis en
      <ind><tld/>umo</ind> muta
      <fnt>
        <aut>P. Peneter</aut>
        <vrk>Sekretaj Sonetoj</vrk>
        <lok>XLVII</lok>
      </fnt>.
      <trdgrp lng="fr">
        <trd>accolade</trd>,
        <trd>embrassement</trd>,
        <trd>enlacement</trd>,
        <trd>&eacute;treinte</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>kucakla&scedil;ma</trd>,
        <trd>sar&inodot;lma</trd>
      </trdgrp>
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">briata</trd>
  <trdgrp lng="cs">
    <trd>obejmout</trd>,
    <trd>obj&iacute;mat</trd>
  </trdgrp>
  <trd lng="de">umarmen</trd>
  <trd lng="en">hug</trd>
  <trd lng="es">abrazar</trd>
  <trdgrp lng="fr">
    <trd>embrasser<klr tip="ind"> (enlacer)</klr></trd>,
    <trd>enlacer</trd>,
    <trd>&eacute;treindre<klr tip="ind"> (avec les bras)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&het;&bet;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>meg&ouml;lel</trd>,
    <trd>&aacute;t&ouml;lel</trd>
  </trdgrp>
  <trd lng="it">abbracciare</trd>
  <trd lng="nl">omarmen</trd>
  <trdgrp lng="pl">
    <trd>obejmowa&cacute; <klr>(ramionami)</klr></trd>
    <trd>bra&cacute; w <ind>ramiona</ind></trd>,
    <trd>u&sacute;ciska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_v;
    &c_o;&c_b;&c_malmol;&c_ja;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obja&tcaron;</trd>,
    <trd>zovrie&tcaron; do n&aacute;ru&ccaron;ia</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>krama</trd>,
    <trd>omfamna</trd>
  </trdgrp>
</drv>

<drv mrk="brak.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="brak.antaux0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la supera membro, de kubuto &gcirc;is mano:
      <ekz>       
        la tria knabino dormas kun kapo apogita
        sur la anta&ubreve;<tld/>oj
        <fnt>
          <aut>S. Rodin</aut>,
          <vrk>Bildoj pri Norda Lando</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_p;&c_l;&c_je;&c_ch;&c_ch;&c_a;</trd>
  <trd lng="br">arvrec'h</trd>
  <trd lng="de">Unterarm</trd>
  <trd lng="en">forearm</trd>
  <trd lng="es">antebrazo</trd>
  <trd lng="fr">avant-bras</trd>
  <trd lng="he">&bet;&yod;&tav; &he;&shin;&het;&yod;</trd>
  <trd lng="hu">alkar</trd>
  <trd lng="it">avanbraccio</trd>
  <trd lng="nl">onderarm</trd>
  <trd lng="pl">przedrami&eogonek;</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_l;&c_je;&c_ch;&c_mol;&c_je;</trd>
  <trd lng="sk">predlaktie</trd>
  <trd lng="sv">underarm</trd>
</drv>

<drv mrk="brak.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brak.cxirkaux0i.brakumi">
    <dif>
      &Ccirc;irka&ubreve;igi per amba&ubreve; <tld/>oj, plejofte kun
      samtempa amema premo al la &ccirc;irka&ubreve;igata objekto
      a&ubreve;
      persono:
      <ekz>
        estas tempo por &ccirc;irka&ubreve;<tld/>i, kaj tempo
        por foriri de &ccirc;irka&ubreve;<tld/>ado
        <fnt><bib>MT</bib><lok>&Pre; 3:5</lok></fnt>;
      </ekz>
      <ekz>
        preninte infanon, li starigis &gcirc;in meze de ili, kaj
        &ccirc;irka&ubreve;<tld/>inte &gcirc;in, li diris
        <klr>[...]</klr>
        <fnt><bib>NT</bib><lok>&Mar; 9:36</lok></fnt>;
      </ekz>
      <ekz>
        mi ricevis sufi&ccirc;egan dankon, dol&ccirc;an kison kaj
        <ind>&ccirc;irka&ubreve;<tld/>o</ind>n
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 5</lok></fnt>.
        <trdgrp lng="fr">
          <trd>accolade</trd>,
          <trd>embrassement</trd>,
          <trd>enlacement</trd>,
          <trd>&eacute;treinte</trd>
        </trdgrp>
        <trd lng="nl">omhelzing</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="brak.0umi"><tld/>umi</ref>
    <rim>
      Estas strange, ke amba&ubreve; PIV-oj tute ignoras
      &ccirc;i tiun vorton.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">briata</trd>
  <trd lng="de">umarmen</trd>
  <trd lng="en">hug</trd>
  <trd lng="es">abrazar</trd>
  <trdgrp lng="fr">
    <trd>embrasser<klr tip="ind"> (enlacer)</klr></trd>,
    <trd>enlacer</trd>,
    <trd>&eacute;treindre<klr tip="ind"> (avec les bras)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&het;&bet;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>meg&ouml;lel</trd>,
    <trd>&aacute;t&ouml;lel</trd>
  </trdgrp>
  <trd lng="it">abbracciare</trd>
  <trdgrp lng="nl">
    <trd>omarmen</trd>,
    <trd>omhelzen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obejmowa&cacute; <klr>(ramionami)</klr></trd>
    <trd>bra&cacute; w <ind>ramiona</ind></trd>,
    <trd>u&sacute;ciska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_v;
    &c_o;&c_b;&c_malmol;&c_ja;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">obja&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>krama</trd>,
    <trd>omfamna</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>kucaklamak</trd>,
    <trd>sar&inodot;lmak</trd>
  </trdgrp>
</drv>

<drv mrk="brak.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brak.en0igi.surbrakigi">
    <dif>
      Levi, surpreni sur siajn <tld/>ojn:
      <ekz>
        Marta <klr>[...]</klr> genui&gcirc;inte volis en<tld/>igi la
        filineton
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="br">kemer en e zivrec'h</trd>
    <trd lng="fr">prendre sur ses bras</trd>
    <trdgrp lng="it">
      <trd>inbracciare</trd>,
      <trd>prendere in braccio</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bra&cacute; na <ind>r&eogonek;ce</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="brak.en0igi.cxirkauxbraki">
    <ref tip="dif"
    cel="brak.cxirkaux0i.brakumi">&ccirc;irka&ubreve;<tld/>i</ref>
    <ekz>
      kial, mia filo, vi volas ser&ccirc;i al vi plezuron &ccirc;e
      fremda virino kaj en<tld/>igi ne apartenantan al vi?
      <fnt><bib>MT</bib>, <lok>&Sen; 5:20</lok></fnt>.
    </ekz>
    <trdgrp lng="pl">
      <trd>obejmowa&cacute; <klr>(ramionami)</klr></trd>
      <trd>bra&cacute; w <ind>ramiona</ind></trd>,
      <trd>u&sacute;ciska&cacute;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>krama</trd>,
      <trd>omfamna</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">obejmout</trd>
  <trd lng="en">hug</trd>
  <trd lng="nl">omhelzen</trd>
  <trdgrp lng="sk">
    <trd>obja&tcaron;</trd>,
    <trd>zovrie&tcaron; do n&aacute;ru&ccaron;ia</trd>
  </trdgrp>
  <adm>
     Interesa estas anka&ubreve; la ekzemplo el Sentencoj
     "se vi [la sa&gcirc;on] enbrakigos", kun la &scirc;ajna senco
     "alpreni", "akcepti kiel gvidregulon"... (kiel la
     franca "embrasser (une opinion)"), sed eble anka&ubreve;
     "protekti".
     <aut>MB</aut>
  </adm>
</drv>

<drv mrk="brak.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="brak.sub0o.ANA">
    <ref tip="dif" cel="aksel.0o">akselo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_h;&c_a;</trd>
  <trd lng="br">kazel</trd>
  <trdgrp lng="cs">
    <trd>podpa&zcaron;d&iacute;</trd>,
    <trd>podpa&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Achsel</trd>
  <trdgrp lng="en">
    <trd>armpit</trd>,
    <trd>underarm</trd>
  </trdgrp>
  <trd lng="es">sobaco</trd>
  <trd lng="fr">aisselle</trd>
  <trd lng="hu">h&oacute;nalj</trd>
  <trd lng="it">ascella</trd>
  <trd lng="pl">pacha</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_m;&c_y;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">pazucha</trd>
  <trd lng="sv">armh&aring;la</trd>
  <trd lng="tr">koltuk alt&inodot;</trd>
</drv>

<drv mrk="brak.0ringo">
  <kap><tld/>ringo</kap>
  <snc mrk="brak.0ringo.GAL">
    <dif>
      <ref tip="super" cel="bracel.0o">Braceleto</ref> portata &ccirc;e
      la
      <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_z;&c_a;&c_l;&c_je;&c_t;</trd>
  <trd lng="bg">&c_g;&c_r;&c_i;&c_v;&c_n;&c_a;</trd>
  <trd lng="br">tro-vrec'h</trd>
  <trd lng="cs">n&aacute;ramek</trd>
  <trd lng="de">Armreif</trd>
  <trd lng="en">bracelet</trd>
  <trdgrp lng="es">
    <trd>pulsera</trd>,
    <trd>brazalete</trd>
  </trdgrp>
  <trd lng="fr">bracelet<klr tip="ind"> (de bras)</klr></trd>
  <trd lng="he">&tsadi;&mem;&yod;&dalet;</trd>
  <trd lng="hu">karperec</trd>
  <trdgrp lng="it">
    <trd>bracciale</trd>,
    <trd>braccialetto</trd>
  </trdgrp>
  <trd lng="pl">bransoleta</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_s;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">n&aacute;ramok</trd>
  <trd lng="tr">bilezik</trd>
</drv>

</art>
