<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dali.xml,v 1.21 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>dali</rad>/o
</kap>
<drv mrk="dali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Dahlia</trd>), kun grandaj brilkoloraj floraroj,
      multe kulturataj &gcirc;ardene.
    </dif>
    <ref tip="sin" cel="georgi.0o">georgino</ref>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_r;&c_g;&c_ib;&c_n;&c_ja;</trd>
  <trd lng="ca">d&agrave;lia</trd>
  <trdgrp lng="cs">
    <trd>georgína</trd>,
    <trd>ji&rcaron;inka</trd>,
    <trd>jiřina</trd>,
    <trd>jiřinka</trd>
  </trdgrp>
  <trd lng="de">Dahlie</trd>
  <trd lng="en">dahlia</trd>
  <trd lng="es">dalia</trd>
  <trd lng="fr">dahlia</trd>
  <trd lng="hu">d&aacute;lia</trd>
  <trd lng="nl">dahlia</trd>
  <trdgrp lng="pl">
    <trd>dalia</trd>,
    <trd>georginia</trd>
  </trdgrp>
  <trd lng="pt">d&aacute;lia</trd>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_r;&c_g;&c_i;&c_n;</trd>
  <trd lng="sk">georgína</trd>
</drv>
</art>
