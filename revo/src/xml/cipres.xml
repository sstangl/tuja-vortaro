<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cipres.xml,v 1.30 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>cipres</rad>/o
</kap>
<drv mrk="cipres.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="cipres1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cupressus</trd>), rezina piramidforma pinglofolia
      arbo el <ref tip="lst" cel="konifer.0o"
      lst="voko:koniferoj">koniferoj</ref>:
          <ekz>
            aleo da <tld/>oj kondukis al granda malferma&jcirc;o en alta,
            ekstera muro
	    <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 2a.</lok></fnt>.
          </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_p;&c_a;&c_r;&c_y;&c_s;</trd>
  <trd lng="bg">&c_k;&c_i;&c_p;&c_a;&c_r;&c_i;&c_s;</trd>
  <trdgrp lng="br">
    <trd>siprezenn</trd>,
    <trd>gwezenn-siprez</trd>
  </trdgrp>
  <trd lng="ca">xiprer</trd>
  <trd lng="cs">cypříšek</trd>
  <trd lng="de">Zypresse</trd>
  <trd
  lng="el">&kappa;&ypsilon;&pi;&alfa;&rho;&jota_ton;&sigma;&sigma;&jota;</trd>
  <trd lng="en">cypress</trd>
  <trd lng="es">cipr&eacute;s</trd>
  <trd lng="fr">cypr&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>ciprusfa</trd>,
    <trd>val&oacute;di <ind>ciner&aacute;ria</ind></trd>
  </trdgrp>
  <trd lng="it">cipresso</trd>
  <trd lng="nl">cipres</trd>
  <trd lng="pl">cyprys</trd>
  <trd lng="pt">cipreste</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_a;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">cypruštek</trd>
</drv>

</art>
