<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dispne.xml,v 1.12 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>dispne</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="dispne.0o">
  <kap><tld/>o</kap>
  <snc mrk="dispne.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Misspirado:
      <ekz>
        <tld/>o komenci&gcirc;is tuj post tiuj vortoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 97a, volumo 3a, p.
        242a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_h;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_y;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_s;&c_p;&c_n;&c_je;&c_ja;</trd>
  <trd lng="ca">dispnea</trd>
  <trdgrp lng="cs">
    <trd>dušnost</trd>,
    <trd>dyspnoe</trd>,
    <trd>dýchavičnost</trd>
  </trdgrp>
  <trd lng="de">Dyspnoe</trd>
  <trd lng="en">dyspnea</trd>
  <trd lng="es">disnea</trd>
  <trd lng="fa">&a_n;&a_f;&a_s;&zwnj;&a_t;&a_n;&f_g;&f_y;</trd>
  <trd lng="fr">dyspn&eacute;e</trd>
  <trd lng="id">dispnea</trd>
  <trdgrp lng="nl">
    <trd>dyspneu</trd>,
    <trd>benauwdheid</trd>,
    <trd>kortademigheid</trd>
  </trdgrp>
  <trd lng="pt">dispn&eacute;ia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_y;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_p;&c_n;&c_o;&c_e;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_u;&c_d;&c_n;&c_jo;&c_n;&c_n;&c_o;&c_je; &c_d;&c_y;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">dýchavičnosť</trd>
</drv>
</art>

