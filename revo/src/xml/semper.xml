<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: semper.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>semperviv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="semper.0o">
  <kap><tld/>o</kap>
  <snc mrk="semper.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Dikfolia herbeto kreskanta en montaro, sur
      tegmentoj kaj malnovaj muroj, <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" 
      cel="krasul1.0oj">krasulacoj</ref> (<trd 
      lng="la">Sempervivum</trd>):
      <ekz>
        <tld/>o, kiu tie silente /
        obea al simpla kamparanaro /
        enkreskis en la dika pajlo tegmenta
        <fnt>
          <aut>G. Gezelle, trad. L. De Cooman</aut>,
          <vrk><url
          ref="http://users.pandora.be/griza_leono/sempervivo.htm">
          Sempervivo</url></vrk>,
          <lok>2006-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_t;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">net≈ôesk</trd>
  <trd lng="de">Hauswurz</trd>
  <trd lng="fr">joubarbe</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;vir&oacute;zsa</trd>,
    <trd>k&odblac;r&oacute;zsa</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>semprevivo</trd>,
    <trd>sempervivum</trd>
  </trdgrp>
  <trd lng="nl">huislook</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_i;&c_l;&c_o;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_u;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>netresk</trd>,
    <trd>skalnica</trd>
  </trdgrp>
</drv>
</art>
