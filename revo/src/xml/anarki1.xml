<vortaro>
<art mrk="$Id: anarki1.xml,v 1.15 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anarkist</rad>/o
</kap>
<drv mrk="anarki1.0">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="anarki2.0isto">anarkiisto</ref>
  <ekz>
    la <tld/>oj partoprenantaj la sindikatan laboron
    <fnt><bib>Lanti</bib>, <lok>&leftquot;ho malfidemulo!...&rightquot;</lok></fnt>.
  </ekz>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">anarquista</trd>
  <trdgrp lng="cs">
    <trd>anarchista</trd>,
    <trd>bouřlivák</trd>,
    <trd>nedisciplinovaný člověk</trd>
  </trdgrp>
  <trd lng="de">Anarchist</trd>
  <trd lng="en">anarchist</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="fr">anarchiste</trd>
  <trd lng="nl">anarchist<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">anarchista</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">anarchista</trd>
</drv>
</art>
