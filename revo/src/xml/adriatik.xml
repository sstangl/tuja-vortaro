<kap>
    <rad>Adriatik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="adriatik.0o">
  <kap><tld/>o</kap>
  <snc mrk="adriatik.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
     <ref tip="dif" cel="mar.0o">Maro</ref> inter la balkana kaj itala
     duoninsuloj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>Adriatické moře</trd>,
    <trd>Jaderské moře</trd>,
    <trd>Jadran</trd>
  </trdgrp>
  <trd lng="fr">Adriatique</trd>
  <trd lng="sk">Adriatické more</trd>
</drv>
</art>

