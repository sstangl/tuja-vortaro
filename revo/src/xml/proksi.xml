<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: proksi.xml,v 1.26 2016/02/10 21:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>proksim</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="proksi.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="proksi.0a.KOMUNE">
    <dif>
      Apartigita per malgranda distanco:
    </dif>
    <subsnc mrk="proksi.0a.kie">
      <dif>
        En spaco:
        <ekz>
          <tld/>a urbo, najbaro, lando;
        </ekz>
        <ekz>
          la malfeli&ccirc;a infano forkuris kaj ka&scirc;is sin en la plej <tld/>a
          arbaro
          <fnt><bib>F</bib>, <lok>&FE; 21</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>a je, al, de io;
        </ekz>
        <ekz>
          rigardi ion de <tld/>e;
        </ekz>
        <ekz>
          alporti ion <ind>de mal<tld/>e</ind><fnt>Z</fnt>;
          <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_l;&c_je;&c_k;&c_a;</trd>
          <trd lng="hu">messzir&odblac;l</trd>
        </ekz>
        <ekz>
          sendi iun mal<tld/>en<fnt>Z</fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="najbar.0a">najbara</ref>
      </refgrp>
      <trdgrp lng="en">
        <trd>close</trd>,
        <trd>near</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="proksi.0a.kiam">
      <dif>
        En tempo:
        <ekz>
          la <tld/>an (pasintan a&ubreve; venontan) jaron;
        </ekz>
        <ekz>
          la tago de la edzi&gcirc;o estis <tld/>a;
        </ekz>
        <ekz>
          <ind>la plej <tld/>a</ind> estonteco<fnt>Z</fnt>.
          <trd lng="ru">&c_b;&c_l;&c_i;&c_zh;&c_a;&c_j;&c_sh;&c_i;&c_j;</trd>
        </ekz>
      </dif>
      <trd lng="en">soon</trd>
      <trd lng="fr">prochain<klr> (adj.)</klr></trd>
    </subsnc>
    <subsnc mrk="proksi.0a.FIG">
      <dif>
        Per metaforo:
        <ekz>
          <tld/>a parenco;
        </ekz>
        <ekz>
          jam <tld/>a <klr>(tuj preta)</klr> estas Lia helpo
          al tiuj, kiuj Lin timas
          <fnt><bib>MT</bib> <lok>&Psa; 85:9</lok></fnt>;
        </ekz>
        <ekz>
          &gcirc;is la fina malapero de la muroj inter la popoloj, estas
          ankora&ubreve;
          tre mal<tld/>e<fnt>Z</fnt>.
        </ekz>
      </dif>
        <trdgrp lng="en">
        <trd>close</trd>,
        <trd>near</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>bl&iacute;zk&yacute;</trd>,
    <trd>p&rcaron;ibli&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">nahe</trd>
  <trd lng="es">pr&oacute;ximo</trd>
  <trd lng="fr">proche</trd>
  <trd lng="hu">k&ouml;zeli</trd>
  <trd lng="nl">dichtbij</trd>
  <trdgrp lng="pl">
    <trd>bliski</trd>,
    <trd>niedaleki</trd>,
    <trd>nieodleg&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pr&oacute;ximo</trd>,
    <trd>prop&iacute;nquo</trd>,
    <trd>junto</trd>,
    <trd>vizinho</trd>,
    <trd>circunvizinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_zh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bl&iacute;zky</trd>,
    <trd>pribli&zcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proksi.0igi.KOMUNE">
    <dif>
      Fari ion <tld/>a:
      <ekz>
        <tld/>igu tiun se&gcirc;on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_b;&c_l;&c_ib;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_l;&c_ib;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;ibl&iacute;&zcaron;it</trd>
  <trd lng="de">ann&auml;hern</trd>
  <trd lng="en">to bring near</trd>
  <trd lng="es">aproximar</trd>
  <trd lng="fr">approcher<klr> (tr.)</klr></trd>
  <trd lng="hu">k&ouml;zel&iacute;t</trd>
  <trd lng="nl">naderbij brengen</trd>
  <trdgrp lng="pl">
    <trd>zbli&zdot;a&cacute;</trd>,
    <trd>przybli&zdot;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">pribl&iacute;&zcaron;i&tcaron;,</trd>
</drv>

<drv mrk="proksi.0ede">
  <kap><tld/>e de,
  <var><kap><tld/>e al</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="proksi.0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>,
      je malgranda distanco de:
      <ekz>
        <tld/>e al la fenestro estis libroj
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 13a, p. 119a</lok></fnt>.
      </ekz>
      <ekz>
        la unua estis mortpafita kiel ribelinto en milita malliberejo
        <tld/>e de la memorparko
        <fnt><aut>G. MacAoidh</aut>, <vrk>Amnezio-amnestio</vrk>,
        <bib>Monato</bib>, <lok>2003:2, p. 16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_z;&c_u;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_l;&c_ib;&c_z;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="en">near</trd>
  <trd lng="fr">pr&egrave;s de</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;zel vmihez</trd>,
    <trd>vmi <ind>k&ouml;zel&eacute;ben</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>blisko</trd>,
    <trd>niedaleko</trd>,
    <trd>w pobli&zdot;u</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_ja;&c_d;&c_o;&c_m; &c_s;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_l;&c_je;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_l;&c_o;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="proksi.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        ne <tld/>i&gcirc;u al la pordo de &scirc;ia domo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        via tasko <tld/>i&gcirc;as al la fino;
      </ekz>
      <ekz>
        la vintro al<tld/>i&gcirc;as;
      </ekz>
      <ekz>
        tiuj, kiuj mal<tld/>i&gcirc;as de Dio, pereas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la popoloj inter<tld/>i&gcirc;as kiel unu familio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_b;&c_l;&c_ib;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_l;&c_ib;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>n&auml;hern</ind></trd>
  <trd lng="en">to come close</trd>
  <trd lng="es">aproximar se</trd>
  <trdgrp lng="fr">
    <trd>approcher<klr> (intr.)</klr></trd>,
    <trd>s'<ind>approcher</ind> de</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;zeledik</trd>
  <trd lng="nl">naderbij komen</trd>
  <trdgrp lng="pl">
    <trd>zbli&zdot;a&cacute; si&eogonek;</trd>,
    <trd>przybli&zdot;a&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="proksi.0ulo.KOMUNE">
    <dif>
      &Ccirc;iu homo, rilate al alia:
      <ekz>
        amu vian <tld/>ulon kiel vin mem
        <fnt><bib>NT</bib>, <lok>&Mar;, 12:31</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu vidu en sia <tld/>ulo nur homon kaj fraton<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_zh;&c_n;&c_ib;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_ib; <klr tip="amb">(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="cs">bli&zcaron;n&iacute;</trd>
  <trd lng="de">N&auml;chster</trd>
  <trdgrp lng="en">
    <trd>fellow man</trd>,
    <trd>neighbor</trd>
  </trdgrp>
  <trd lng="fr">prochain<klr> (subst.)</klr></trd>
  <trd lng="hu">felebar&aacute;t</trd>
  <trd lng="nl">naaste</trd>
  <trdgrp lng="pl">
    <trd>bli&zacute;ni</trd>,
    <trd><klr>kto&sacute;</klr> <ind>bliski</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_z;&c_k;&c_i;&c_j; <klr tip="amb">(&c_s;&c_u;&c_shch;.)</klr></trd>,
    <trd>&c_b;&c_l;&c_i;&c_zh;&c_n;&c_i;&c_j; <klr tip="amb">(&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">bl&iacute;&zcaron;ny</trd>
</drv>

<drv mrk="proksi.0uma">
  <kap><tld/>uma</kap>
  <snc mrk="proksi.0uma.MEZ">
    <dif>
      Ne tute &gcirc;usta, sed nur <tld/>a al la &gcirc;usto:
      <ekz>
        <tld/>uma kalkulo de la lo&gcirc;antoj;
      </ekz>
      <ekz>
        <tld/>uma signifo de vorto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_b;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>aproximativn&iacute;</trd>,
    <trd>p&rcaron;ibli&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">ungef&auml;hr</trd>
  <trd lng="en">approximate</trd>
  <trd lng="fr">approximatif</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;r&uuml;lbel&uuml;li</trd>,
    <trd>megk&ouml;zel&iacute;t&odblac;</trd>
  </trdgrp>
  <trd lng="nl">benaderend</trd>
  <trd lng="pl">przybli&zdot;ony</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_r;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pribli&zcaron;n&yacute;</trd>
</drv>

<drv mrk="proksi.0ume">
  <kap><tld/>ume</kap>
  <snc mrk="proksi.0ume.MEZ">
    <dif>
      La&ubreve; ne preciza takso, ne plene trafante la veron a&ubreve; celon:
      <ekz>
        la biblioteko enhavas <tld/>ume (&ccirc;irka&ubreve;e) 100 volumojn;
      </ekz>
      <ekz>
        <tld/>ume post unu horo la buso haltis
        <fnt><aut>G. L. Arosev</aut>, <vrk>Nerekonebla</vrk>,
        <bib>Monato</bib>, <lok>2001:5, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_b;&c_l;&c_ib;&c_z;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_z;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">m&eacute;s o menys</trd>
  <trdgrp lng="cs">
    <trd>asi</trd>,
    <trd>cca</trd>,
    <trd>p&rcaron;ibli&zcaron;n&ecaron;</trd>
  </trdgrp>
  <trd lng="en">approximately</trd>
  <trdgrp lng="fr">
    <trd>&agrave; peu pr&egrave;s</trd>,
    <trd>environ<klr> (&agrave; peu pr&egrave;s)</klr></trd>,
    <trd>approximativement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;r&uuml;lbel&uuml;l</trd>,
    <trd>megk&ouml;zel&iacute;t&odblac;leg</trd>,
    <trd>nagyj&aacute;b&oacute;l</trd>
  </trdgrp>
  <trd lng="oc">a pauc pr&egrave;s</trd>
  <trdgrp lng="pl">
    <trd>w <ind>przybli&zdot;eniu</ind></trd>,
    <trd>oko&lstroke;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_je;&c_r;&c_n;&c_o;<klr tip="ind"> (&c_p;&c_r;&c_i;&c_b;&c_l;&c_i;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>asi</trd>,
    <trd>pribli&zcaron;ne</trd>,
    <trd>zhruba</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="proksi.mal0a.KOMUNE">
    <dif>
      Ne apuda, trans longa spaca, tempa a&ubreve; spirita distanco:
      <ekz>
        tiu &ccirc;i malfeli&ccirc;a infano devis du fojojn en &ccirc;iu tago iri &ccirc;erpi
        akvon en tre mal<tld/>a loko
        <fnt><bib>F</bib>, <lok>&FE; 13</lok></fnt>;
      </ekz>
      <ekz>
        pre&gcirc;ejo <tld/>a, sed Dio mal<tld/>a;
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        mal<tld/>a de mi estas la deziro ribeli<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a de mi estas la opinio ke...;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pli bona amiko intima ol parenco mal<tld/>a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib;</trd>
  <trd lng="cs">vzd&aacute;len&yacute;</trd>
  <trdgrp lng="de">
    <trd>fern</trd>,
    <trd>entfernt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>far</trd>,
    <trd>distant</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>distant</trd>,
    <trd>&eacute;loign&eacute;</trd>,
    <trd>lointain</trd>
  </trdgrp>
  <trd lng="hu">t&aacute;voli</trd>
  <trdgrp lng="pl">
    <trd>daleki</trd>,
    <trd>odleg&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_jo;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>od&lcaron;ahl&yacute;</trd>,
    <trd>vzdialen&yacute;</trd>,
    <trd>&dcaron;alek&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proksi.mal0igi.KOMUNE">
    <dif>
      Fari mal<tld/>a:
      <ekz>
        la artikolo ankora&ubreve; pli ilin mal<tld/>igas de mi<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">odd&aacute;lit</trd>
  <trd lng="en">to distance</trd>
  <trd lng="fr">&eacute;loigner</trd>
  <trd lng="sk">vzdiali&tcaron;</trd>
</drv>

<drv mrk="proksi.mal0ede">
  <kap>mal<tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="proksi.mal0ede.KOMUNE">
    <dif>
      Je granda distanco, longa vojo, tempo a&ubreve; strebo de:
      <ekz>
        la stacio ku&scirc;as unu mejlon mal<tld/>e de mia domo;
      </ekz>
      <ekz>
        resti mal<tld/>e de la celo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tia projekto staras ankora&ubreve; mal<tld/>e de reali&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        stari mal<tld/>e de la vero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_a; &c_a;&c_d;</trd>
  <trdgrp lng="en">
    <trd>away</trd>,
    <trd>far off</trd>
  </trdgrp>
  <trd lng="fr">loin de</trd>
  <trd lng="hu">t&aacute;vol vmit&odblac;l</trd>
  <trd lng="pl">daleko <klr>od</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_d;&c_a;&c_l;&c_i; &c_o;&c_t;</trd>,
    <trd>&c_d;&c_a;&c_l;&c_je;&c_k;&c_o; &c_o;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="proksi.tusx0a">
  <kap>tu&scirc;<tld/>a</kap>
  <snc mrk="proksi.tusx0a.KOMUNE">
    <dif>
      Tre <tld/>a, kvaza&ubreve; oni povus tu&scirc;i:
      <ekz>
