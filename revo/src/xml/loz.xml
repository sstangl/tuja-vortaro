<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: loz.xml,v 1.7 2015/10/27 11:10:20 revo Exp $">
<kap>
    <rad>loz</rad>/a <fnt><bib>PIV3</bib></fnt>
</kap>

<drv mrk="loz.0a">
  <kap><tld/>a</kap>
  <snc mrk="loz.0a.KOMUNE">
    <dif>
      Malstrikta, malstre&ccirc;ita, malkompakta:
    </dif>
      <ekz>
      <tld/>a bluzo, pantalono; <tld/>e ligita &scirc;nuro.
      </ekz>
      <trdgrp lng="en">
        <trd>loose</trd>,
        <trd>slack</trd>
      </trdgrp>
  <trd lng="fr">rel&acirc;ch&eacute;</trd>
  <trd lng="nl">los</trd>
  </snc>
  
  <snc mrk="loz.0a.KOMERCO">
    <dif>
      Ne enpakita:
    </dif>
      <ekz>
      <tld/>a kargo.
      </ekz>
      <trdgrp lng="en">
        <trd>loose</trd>,
        <trd>unrestrained</trd>
      </trdgrp>
  <trd lng="fr">en <ind>vrac</ind></trd>
  <trd lng="nl">bulk</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>uvoln&ecaron;n&yacute;</trd>,
    <trd>voln&yacute;</trd>
  </trdgrp>
  <trd lng="pl">lu&zacute;ny</trd>
  <trdgrp lng="sk">
    <trd>uvo&lcaron;nen&yacute;</trd>,
    <trd>vo&lcaron;n&yacute;</trd>
  </trdgrp>
</drv>
</art>

