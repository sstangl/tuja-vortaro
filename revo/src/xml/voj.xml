<drv mrk="voj.fer0o">
  <kap><ofc>*</ofc>fer<tld/>o</kap>
  <snc mrk="voj.fer0o.rel0o">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="voj.rel0o">rel<tld/>o</ref>
    <ekz>
      tiu &ccirc;i fer<tld/>o estas konstruota en la da&ubreve;ro de du
      jaroj
      <fnt><bib>F</bib><lok>&FE; 25</lok></fnt>;
    </ekz>
    <ekz>
      la gladilo <klr>[...]</klr> imagis al si, ke
      &gcirc;i estas vaporma&scirc;ina kaldrono, kiu devas iri
      sur la fer<tld/>on, por tiri vagonojn
      <fnt><bib>Fab3</bib>, <lok>Kolumo</lok></fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd>Gleis</trd>,
      <trd>Bahnlinie</trd>.
    </trdgrp>
    <trdgrp lng="fr">
      <trd>voie<klr tip="ind"> (de chemin de fer)</klr></trd>,
      <trd>voie ferr&eacute;e</trd>,
      <trd><ind>chemin de fer</ind><klr tip="ind"> 
      (voie ferr&eacute;e)</klr></trd>
    </trdgrp>
    <trd lng="hu">v&aacute;g&aacute;ny</trd>
    <trdgrp lng="it">
      <trd>ferrovia</trd>
    </trdgrp>
    <trd lng="nl">spoor</trd>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_a;&c_ja; 
      &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>,
      <trd>&c_r;&c_je;&c_l;&c_mol;&c_s;&c_y;</trd>
    </trdgrp>
  </snc>
  <snc mrk="voj.fer0o.FER">
    <uzo tip="fak">FER</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Sistemo por transporti pasa&gcirc;erojn kaj varojn
      per veturiloj irantaj sur <ref cel="voj.rel0o">rel<tld/>oj</ref>:
      <ekz>
	<klr>[estus malsa&gcirc;e]</klr> sendi ion el Parizo Peterburgon
	per &ccirc;evaloj, kiam ni havas la eblon fari tion &ccirc;i per
	fer<tld/>o
	<fnt><bib>EE</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_y;&c_g;&c_u;&c_n;&c_k;&c_a;</trd>
    <trd lng="de">Eisenbahn</trd>
    <trdgrp lng="fr">
      <trd><ind>chemin de fer</ind><klr tip="ind"> 
      (moyen de transport)</klr></trd>,
      <trd>rail<klr tip="ind"> (moyen de transport)</klr></trd>,
      <trd>train<klr tip="ind"> (moyen de transport)</klr></trd>
    </trdgrp>
    <trd lng="hu">vas&uacute;t</trd>
    <trdgrp lng="it">
      <trd>ferrovia</trd>
    </trdgrp>
    <trd lng="nl">spoorweg</trd>
    <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_a;&c_ja; 
    &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
  </snc>
  <trd lng="br">hent-houarn</trd>
  <trdgrp lng="cs">
    <trd>&zcaron;eleznice</trd>,
    <trd>&zcaron;elezni&ccaron;n&iacute; dr&aacute;ha</trd>
  </trdgrp>
  <trd lng="hu">vas&uacute;t</trd>
  <trdgrp lng="pl">
    <trd>kolej</trd>,
    <trd>droga &zdot;elazna<klr> (rzadkie)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dr&aacute;ha</trd>,
    <trd>&zcaron;eleznica</trd>
  </trdgrp>
</drv>

<drv mrk="voj.fer0oj">
  <kap>fer<tld/>oj, <var><kap>fer<tld/>kompanio</kap></var></kap>
  <snc mrk="voj.fer0oj.TRA">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      Entrepreno, &scirc;tata a&ubreve; privata, ekspluatanta fer<tld/>on:
      <ekz>
	la &ccirc;efa linio de la Danaj fer<tld/>oj ligas Kopenhagon
	kun Korsor
	<fnt><bib>Fab4</bib>, <lok>Peco da perlovico</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Eisenbahngesellschaft</trd>
  <trdgrp lng="fr">
    <trd><ind>chemins de fer</ind><klr tip="ind">
    (compagnie)</klr></trd>,
    <trd>compagnie de <ind>chemins de fer</ind></trd>
  </trdgrp>
  <trd lng="hu">vas&uacute;tt&aacute;rsas&aacute;g</trd>
  <trdgrp lng="it">
    <trd>ferrovie<klr> (compagnia)</klr></trd>,
    <trd><ind>compagnia</ind> ferroviaria<klr> (fig)</klr></trd>
  </trdgrp>
  <trd lng="pl">kolej</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_y;&c_je; &c_d;&c_o;&c_r;&c_o;&c_g;&c_i;</trd>
</drv>

<drv mrk="voj.fer0a">
  <kap>fer<tld/>a</kap>
  <snc mrk="voj.fer0a.FER">
    <dif>
      Rilata al fer<tld/>o, al fer<tld/>oj:
      <ekz>
	fer<tld/>a societo
	<fnt><bib>Voja&gcirc;Imp</bib><lok>Du blinduloj</lok></fnt>;
      </ekz>
      <ekz>
	<ind>fer<tld/>a stacio</ind>
	<fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 15</lok></fnt>,
	<trd lng="fr">gare<klr tip="ind"> (de chemins de fer)</klr></trd>
	<trd lng="hu">vas&uacute;t&aacute;llom&aacute;s</trd>
      </ekz>
      <ekz>
	<ind>fer<tld/>a trotuaro</ind>
	<klr>(= <ref cel="kaj1.0o.FER">kajo</ref>)</klr>
	<fnt>
	<aut>H. Vallienne</aut>,
	<vrk>Kastelo de Prelongo</vrk>,
        <lok>1907</lok>
	</fnt>,
	<trd lng="fr">quai<klr tip="ind"> (de chemin de fer)</klr></trd>
	<trd lng="hu">peron</trd>
      </ekz>
      <ekz>
	<ind>fer<tld/>a oficisto</ind>
	<fnt>
	<aut>H. Vallienne</aut>,
	<vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
	</fnt>,
	<trd lng="fr">employ&eacute; du <ind>chemin de fer</ind></trd>
	<trd lng="hu">vas&uacute;ti tiszt</trd>
      </ekz>
      <ekz>
	<ind>fer<tld/>a laboristo</ind>,
	<trd lng="fr">ouvrier du <ind>chemin de fer</ind></trd>
	<trd lng="hu">vas&uacute;ti dolgoz&oacute;</trd>
      </ekz>
      <ekz>
	<ind>fer<tld/>a bileto</ind>
	<fnt>
	<aut>H. Vallienne</aut>,
	<vrk>&Ccirc;u li?</vrk>,
        <lok>1908</lok>
	</fnt>,
	<trd lng="hu">vonatjegy</trd>
      </ekz>
      <ekz>
	fer<tld/>a akcidento
	<fnt>
	<aut>H. Vallienne</aut>,
	<vrk>Kastelo de Prelongo</vrk>,
        <lok>1907</lok>
	</fnt>;
      </ekz>
      <ekz>
	mi sidis sur la lokomotivo anta&ubreve; fer<tld/>a vagonaro
	<fnt><bib>Fab4</bib>, <lok>Kuristoj</lok></fnt>;
      </ekz>
      <ekz>
	mi ne havas plu la forton, por elteni fer<tld/>an voja&gcirc;on
	<fnt><bib>Fab4</bib>, <lok>Peco da perlovico</lok></fnt>;
      </ekz>
      <ekz>
	oni ne povas kulpigi la fer<tld/>ajn in&gcirc;enierojn,
	se trovi&gcirc;as personoj, kiuj volas uzi la fer<tld/>on por
	iaj celoj &leftquot;utopiaj&rightquot;
	<fnt><bib>OV</bib>, <lok>&para; Homaranismo (III-8)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ferroviaire</trd>,
    <trd>de <ind>chemin de fer</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ferroviario</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vas&uacute;ti</trd>,
    <trd>vas&uacute;t-</trd>
  </trdgrp>
  <trd lng="nl">spoor-</trd>
  <trd lng="pl">kolejowy</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_o;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="voj.fer0isto">
