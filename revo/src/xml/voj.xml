<drv mrk="voj.hauxl0o">
  <kap>ha&ubreve;l<tld/>o</kap>
  <snc mrk="voj.hauxl0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <tld lit="V"/>o la&ubreve; rivero a&ubreve; kanalo, sur
      kiu iras homoj a&ubreve; brutoj, ha&ubreve;lante bar&gcirc;on:
      <ekz>
        mi <klr>...</klr> trenas &scirc;ipojn &scirc;vite sur ha&ubreve;l<tld/>o
        <fnt>
          <aut>Wang Jian, trad. Vejdo</aut>,
          <vrk>Kanzoneto pri &Scirc;ipha&ubreve;listo</vrk>,
          <lok><url
          ref="http://www.esperanto.es:8080/jspui/bitstream/11013/3087/1/Penseo_2007_n.%20183.pdf">
          Penseo, 2007-01 (183)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chemin de halage</trd>
</drv>

<drv mrk="voj.kablo0o">
