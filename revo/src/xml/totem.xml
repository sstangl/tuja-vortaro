<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: totem.xml,v 1.15 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>totem</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="totem.0o">
  <kap><tld/>o</kap>
  <snc mrk="totem.0o.ETN">
    <dif>
      Esta&jcirc;o, &gcirc;enerale besto 
      a&ubreve; vegeta&jcirc;o, konsiderata de iu
      <ref cel="klan.0o.ETN">klano</ref> kiel
      praulo kaj protektanto de la
      grupo, al kiu &gcirc;i servas kiel insigno kaj kolektiva nomo:
      <ekz>
        foje estis ceremonioj por nur unu el la kvar <tld/>oj,
        foje du a&ubreve; e&ccirc; &ccirc;iuj kvar <tld/>grupoj kunvenis
        <fnt><aut>T. Steele</aut>, <vrk>Flugi kun kakatuoj</vrk>, 
        <lok>2010</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_t;&c_e;&c_m;</trd>
  <trd lng="ca">t&ograve;tem</trd>
  <trd lng="cs">totem</trd>
  <trdgrp lng="de">
    <trd>Totem</trd>,
    <trd>Stammeszeichen</trd>
  </trdgrp>
  <trd lng="en">totem</trd>
  <trd lng="es">t&oacute;tem</trd>
  <trd lng="fr">totem</trd>
  <trd lng="hu">totem</trd>
  <trd lng="nl">totem</trd>
  <trd lng="pl">totem</trd>
  <trd lng="pt">totem</trd>
  <trd lng="ru">&c_t;&c_o;&c_t;&c_je;&c_m;</trd>
  <trd lng="sk">totem</trd>
</drv>

<drv mrk="totem.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="totem.0ismo.ETN">
    <dif>
      Soci-religia sistemo bazita sur parenceco kaj ligo inter iu
      <tld/>o
      kaj iu homa grupo.
    </dif>
    <refgrp tip="vid">
      <ref cel="tabu.0o">tabuo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_t;&c_e;&c_m;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">totemisme</trd>
  <trd lng="cs">totemizmus</trd>
  <trdgrp lng="de">
    <trd>Totemismus</trd>,
    <trd>Stammesglaube</trd>
  </trdgrp>
  <trd lng="en">totemism</trd>
  <trd lng="es">totemismo</trd>
  <trd lng="fr">tot&eacute;misme</trd>
  <trd lng="hu">totemizmus</trd>
  <trd lng="nl">totemisme</trd>
  <trd lng="pt">totemismo</trd>
  <trd lng="ru">&c_t;&c_o;&c_t;&c_je;&c_m;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">totemizmus</trd>
</drv>
</art>
