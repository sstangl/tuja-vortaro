    <trd>&c_p;&c_o;&c_l;&c_u;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="konduk.duon0anto">
  <kap>duon<tld/>anto</kap>
  <snc mrk="konduk.duon0anto.ELE">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref cel="konduk.duon0a">Duon<tld/>a</ref> materialo.
