<drv mrk="konduk.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konduk.en0i.enirigi">
    <dif>
      <tld lit="K"/>i al io kaj enirigi en &gcirc;in:
      <ekz>
        la lakeo en<tld/>is junan sinjoron en la kabineton.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>hineinf&uuml;hren</trd>,
      <trd>hineinbringen</trd>
    </trdgrp>
    <trd lng="en">to lead in</trd>
    <trd lng="nl">binnenleiden</trd>
  </snc>
  <snc mrk="konduk.en0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Enigi ion novan en uzadon; uzi kaj apliki nova&jcirc;on:
      <ekz>
        apartaj artaj rimedoj en<tld/>as grandan ekonomion &ccirc;e la lernado
        de
        la vortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en<tld/>i novan modon, radikon en nia lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i sistemo postulas tro grandan pripensadon por povi esti
        en<tld/>ita en la ordinaran interkomuniki&gcirc;on kun malproksimaj
        popoloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>einf&uuml;hren</trd>,
      <trd>aufbringen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to introduce</trd>,
      <trd>initiate</trd>, <trd>present</trd>
    </trdgrp>
    <trd lng="hu">bevezet</trd>
    <trd lng="nl">inleiden</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;iv&eacute;st</trd>,
    <trd>uvozovat</trd>,
    <trd>uv&eacute;st</trd>,
    <trd>zav&aacute;d&ecaron;t</trd>,
    <trd>zav&eacute;st</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>introduire</trd>
    <trd>faire <ind>entrer</ind></trd>,
    <trd>incorporer</trd>,
    <trd>ins&eacute;rer</trd>,
    <trd>implanter</trd>,
    <trd>lancer<klr tip="ind"> (introduire)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>uvies&tcaron;</trd>,
    <trd>vovies&tcaron;</trd>
  </trdgrp>
</drv>

