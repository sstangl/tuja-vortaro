    <trd lng="ru">&c_o;&c_t;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>odv&eacute;st</trd>,
    <trd>sv&eacute;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abraten</trd>,
    <trd>abbringen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner</trd>,
    <trd>d&eacute;vier<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elvezet</trd>,
    <trd>elterel</trd>,
    <trd>levezet</trd>
  </trdgrp>
  <trd lng="nl">afbrengen</trd>
  <trd lng="sk">odvies&tcaron;</trd>
</drv>

<drv mrk="konduk.duon0a">
