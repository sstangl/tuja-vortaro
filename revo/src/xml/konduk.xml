    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;porter</trd>,
      <trd>&eacute;loigner</trd>,
      <trd>emmener<klr tip="ind"> (au loin)</klr></trd>
      <trd>&eacute;vacuer</trd>
    </trdgrp>
