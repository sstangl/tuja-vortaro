<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: satan.xml,v 1.18 2015/07/04 08:37:43 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>satan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="satan.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       &Ccirc;efo de la ribelaj an&gcirc;eloj.
     </dif>
     <trd lng="en">Satan</trd>
     <trd lng="it">Satana</trd>
     <trd lng="sk">Satan</trd>
   </snc>
   <snc>
     <dif>
       Diablo.
     </dif>
     <trd lng="en">devil</trd>
     <trdgrp lng="it">
     <trd>diavolo</trd>, 
     <trd>satanasso</trd>,
     <trd>demonio</trd>
   </trdgrp>
     <trd lng="sk">diabol</trd>
   </snc>
  <trdgrp lng="be">
     <trd>&c_d;'&c_ja;&c_b;&c_a;&c_l;</trd>,
     <trd>&c_s;&c_a;&c_t;&c_a;&c_n;&c_a;</trd>,
     <trd>&c_sh;&c_a;&c_t;&c_a;&c_n;</trd>,
     <trd>&c_n;&c_ja;&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>satan</trd>,
    <trd>satanáš</trd>,
    <trd>ďábel</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Satan</trd>,
     <trd>Teufel</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>sat&aacute;n</trd>,
     <trd>diablo</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>satan</trd>,
     <trd>diable</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>s&aacute;tan</trd>,
     <trd>&ouml;rd&ouml;g</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>satan</trd>,
     <trd>duivel</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_s;&c_a;&c_t;&c_a;&c_n;&c_a;</trd>,
     <trd>&c_d;&c_mol;&c_ja;&c_v;&c_o;&c_l;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>satan</trd>,
    <trd>satanáš</trd>
  </trdgrp>
</drv>

<drv mrk="satan.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="satan.0ismo.REL">
    <uzo tip="fak">REL</uzo>    
    <dif>
      Kultado, adorado de <tld/>o:
      <ekz>
        &ccirc;i-okaze majstro Christoforo vendis sian animon al la <tld 
        lit="S"/>o, do, li uzas <tld/>ismon
        <fnt><bib>SatirRak</bib>, <lok>Supersti&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">satanizmus</trd>
  <trd lng="fr">satanisme</trd>
  <trd lng="sk">satanizmus</trd>
</drv>

<drv mrk="satan.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="satan.0isto.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Ano de <ref tip="vid" cel="satan.0ismo.REL"><tld/>ismo</ref>:
      <ekz>
        hororfilmoj kun diabloj kaj <tld/>istoj
        <fnt>
          <aut>P. Cadei</aut>,
          <vrk><url
          ref="http://www.radio-vatikana-esperanto.org/elsendo.php?d=2004-06-17">
          Diablo kaj diableca spirito</url></vrk>,
          <lok>2004-06-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">satanista</trd>
  <trd lng="fr">sataniste</trd>
  <trd lng="sk">satanista</trd>
</drv>
</art>
