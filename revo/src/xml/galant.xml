<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: galant.xml,v 1.21 2015/07/04 08:37:34 revo Exp $">
<kap><ofc>2</ofc>
<rad>galant</rad>/o</kap>
<drv mrk="galant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="amaril1.0oj">amarilidacoj</ref>
      (<trd lng="la">Galanthus</trd>),
      floranta en la fino de la vintro,
      kun malgrandaj blankaj sonorilformaj floroj el
      du rondoj de petaloj, tri eksteraj, ovalaj kaj pure
      blankaj, kaj tri internaj kunklinitaj kiel kroneto kun
      verdaj makuloj &ccirc;e la pintoj.
    </dif>
  </snc>
  <bld lok="../bld/galant.jpg">
    <tld/>oj
  </bld>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Galanthus</trd>,
    <trd>lliri de neu</trd>
  </trdgrp>
  <trd lng="de">Schneegl&ouml;ckchen</trd>
  <trd lng="fr">perce-neige</trd>
  <trd lng="hu">h&oacute;vir&aacute;g</trd>
  <trd lng="nl">sneeuwklokje</trd>
  <trdgrp lng="pt">
   <trd>galanto</trd>, 
   <trd>fura-neve</trd>, 
   <trd>cip&oacute;-da-praia</trd>,
   <trd>campainha-branca</trd>,
   <trd>salsa-da-praia</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_n;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">sn&ouml;droppe</trd>
  <trd lng="tr">kardelen</trd>
  <trd lng="sk">snežienka</trd>
  <trd lng="cs">sněženka</trd>
</drv>
</art>
