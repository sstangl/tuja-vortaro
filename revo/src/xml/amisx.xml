<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amisx.xml,v 1.2 2012/01/03 23:10:17 revo Exp $">
<kap>
  <rad>ami&scirc;</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="amisx.0o">
  <kap><tld/>o</kap>
  <snc mrk="amisx.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de <ref tip="vid" cel="amisx.0ismo.KRI"><tld/>ismo</ref>:
      <ekz>
        mi por la unua fojo en mia vivo vidis realan <tld/>on kiu iris 
        sur sia &ccirc;aro
        <fnt>
          <aut>Be&ccirc;jo</aut>,
          <vrk><url
          ref="http://becxjo.blogspot.com/2010/09/tre-freneza-vojago-de-mi.html">
          Tre freneza voja&gcirc;o de mi</url></vrk>,
          <lok>2010-09-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">amish <klr>(subst.)</klr></trd>
  <trd lng="fr">amish</trd>
</drv>

<drv mrk="amisx.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="amisx.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Doktrino kaj praktiko de
      <ref tip="vid" cel="amisx.0o.KRI"><tld/>oj</ref>;
      bran&ccirc;o de <ref tip="vid" 
      cel="menonit.0ismo.KRI">menonitismo</ref>
      germandevena, nunepoke precipe usona,
      rimarkinda pro malakcepto de modernaj te&hcirc;nika&jcirc;oj.
    </dif>
  </snc>
  <trd lng="ca">doctrina amish</trd>
</drv>
</art>

