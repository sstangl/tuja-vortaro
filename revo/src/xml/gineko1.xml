<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gineko1.xml,v 1.20 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>ginekologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gineko1.0o">
  <kap><tld/>o</kap>
  <snc mrk="gineko1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicino-kirurgia fako, kiu studas la <ref tip="vid"
      cel="fiziol1.0o">fiziologion</ref> kaj la <ref tip="vid"
      cel="patolo.0o">patologion</ref> de la virina genera sistemo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_n;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">ginecologia</trd>
  <trd lng="cs">gynekologie</trd>
  <trdgrp lng="de">
    <trd>Frauenheilkunde</trd>,
    <trd>Gyn&auml;kologie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gynaecology</trd>,
    <trd>gynecology</trd>
  </trdgrp>
  <trd lng="es">ginecolog&iacute;a</trd>
  <trd lng="fr">gyn&eacute;cologie</trd>
  <trdgrp lng="hu">
    <trd>n&odblac;gy&oacute;gy&aacute;szat</trd>,
    <trd>ginekol&oacute;gia</trd>
  </trdgrp>
  <trd lng="nl">gynecologie</trd>
  <trd lng="pt">ginecologia</trd>
  <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">ženské lekárstvo</trd>
  <trdgrp lng="tr">
    <trd>nisaiye</trd>,
    <trd>jinekoloji</trd>
  </trdgrp>
</drv>

<drv mrk="gineko1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="gineko1.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="gineko.0o">ginekologo</ref>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_n;&c_e;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">ginec&ograve;leg</trd>
  <trdgrp lng="cs">
    <trd>gynekolog</trd>,
    <trd>lékař ženský</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Frauenarzt</trd>,
    <trd>Gyn&auml;kologe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gynaecologist</trd>,
    <trd>gynecologist</trd>
  </trdgrp>
  <trd lng="es">ginec&oacute;logo</trd>
  <trd lng="fr">gyn&eacute;cologue</trd>
  <trdgrp lng="hu">
    <trd>n&odblac;gy&oacute;gy&aacute;sz</trd>,
    <trd>ginekol&oacute;gus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vrouwenarts</trd>,
    <trd>gynecoloog</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>ginec&oacute;logo</trd>, 
   <trd>ginecologista</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">ženský lekár</trd>
  <trd lng="tr">jinekolog</trd>
</drv>
  </art>
