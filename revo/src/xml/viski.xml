<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: viski.xml,v 1.21 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>viski</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="viski.0o">
  <kap><tld/>o</kap>
  <snc mrk="viski.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Britdevena <ref tip="super" cel="brand.0o">brando</ref> el greno:
      <ekz>
        ver&scirc;u iom da <tld/>o al vi
        <fnt><aut>J. Modest</aut>, <vrk>En la ombro de la
        silento</vrk>, <bib>Monato</bib>, <lok>jaro 1997a, numero 3a, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">whisky</trd>
  <trd lng="cs">whisky</trd>
  <trd lng="de">Whisky</trd>
  <trdgrp lng="en">
    <trd>whiskey</trd>,
    <trd>whisky</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>whisky</trd>,
    <trd>g&uuml;isqui</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&f_y;&a_s;&f_k;&f_y;</trd>
  <trd lng="fr">whisky</trd>
  <trd lng="hu">whisky</trd>
  <trd lng="nl">whisky</trd>
  <trd lng="pt">u&iacute;sque</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_k;&c_i;</trd>
  <trd lng="sk">whisky</trd>
  <trd lng="sv">whisky</trd>
</drv>

<drv mrk="viski.maiz0o">
  <kap>maiz<tld/>o</kap>
  <snc mrk="viski.maiz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="brand.0o">Brando</ref> ordinare usondevena,
      <tld/>o el maizgrajnoj:
      <ekz>
        maiza <tld/>o a&ubreve; maiz<tld/>o
        <fnt>
        <bib>Viki</bib>,
        <lok>burbono, 2005-06-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">bourbon</trd>
  <trd lng="fr">bourbon<klr tip="amb"> (boisson)</klr></trd>
</drv>
</art>
