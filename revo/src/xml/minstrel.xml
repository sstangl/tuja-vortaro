<vortaro>
<art mrk="$Id: minstrel.xml,v 1.5 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>minstrel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="minstrel.0o">
  <kap><tld/>o</kap>
  <snc mrk="minstrel.0o.HIS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POE</uzo>
    <ref tip="dif" cel="menestre.0o.HIS">menestrelo</ref>
    <ekz>
      li komencis travagi la najbara&jcirc;on kiel vaganta 
      <tld/>o
      <fnt>
        <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
        <vrk>Montara Vila&gcirc;o</vrk>,
        <lok>1984</lok>
      </fnt>.
    </ekz>
    <trd lng="ca">joglar</trd>
    <trd lng="pl">minstrel</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>minesengr</trd>,
    <trd>minsterel</trd>
  </trdgrp>
  <trd lng="sk">spevák <klr>(stredoveký)</klr></trd>
</drv>
</art>

