<kap>
  <rad>gagat</rad>/o
</kap>
<drv mrk="gagat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de nigra lignito, uzata por juveloj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_g;&c_a;&c_t;</trd>
  <trd lng="ca">atzabeja</trd>
  <trd lng="cs">gagát</trd>
  <trdgrp lng="de">
   <trd>Gagatkohle</trd>,
   <trd>Jett</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&gamma;&alfa_ton;&tau;&eta;&sigma_fina;</trd>
  <trd lng="en">jet</trd>
  <trdgrp lng="hu">
    <trd>gag&aacute;t</trd>,
    <trd>fekete borosty&aacute;nk&odblac;</trd>,
    <trd>zsett</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>git</trd>,
    <trd>gagaat</trd>,
    <trd>bergwas</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>azeviche</trd>,
    <trd>gagata</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;&c_t;</trd>
  <trd lng="sk">gagát</trd>
</drv>
</art>
