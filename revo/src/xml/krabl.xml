<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krabl.xml,v 1.6 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>krabl</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="krabl.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="krabl.0i.KOMUNE">
    <dif>
      Iri, rampi, vigle movante la korpon kaj la krurojn, kiel iuj
      bestetoj:
      <ekz>
        &scirc;telisto <klr>...</klr> scias <tld/>i sur muro kiel mu&scirc;o
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gatejar</trd>,
    <trd>anar de quatre grapes</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lézt</trd>,
    <trd>sunout se <klr>(housenka)</klr></trd>,
    <trd>šourat se</trd>
  </trdgrp>
  <trd lng="fr">carapater</trd>
  <trdgrp lng="pt">
    <trd>andar de <ind>gatinhas</ind></trd>,
    <trd>andar de <ind>gatas</ind></trd>,
    <trd>andar de <ind>quatro</ind></trd>,
    <trd>andar de quatro <ind>p&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_z;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;, &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;
  &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>liezť</trd>,
    <trd>sunúť sa <klr>(húsenica)</klr></trd>
  </trdgrp>
</drv>
</art>

