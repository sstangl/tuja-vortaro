<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vand.xml,v 1.18 2015/05/08 22:10:15 revo Exp $">
<kap>
  <ofc>9</ofc><rad>vand</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vand.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="vand.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Interna maldika mureto dividanta ejon:
      <ekz>
        <tld/>o de tendo, vitra <tld/>o;
      </ekz>
      <ekz>
        grandampleksaj salonoj estis transformitaj per <tld/>oj en
        komfortajn luksajn &ccirc;ambrojn.
      </ekz>
    </dif>
    <trd lng="ca">cloenda</trd>
    <trdgrp lng="it">
      <trd>parete<klr> (interna)</klr></trd>,
      <trd>tramezza</trd>,
      <trd>tramezzo</trd>,
      <trd>divisorio</trd>
    </trdgrp>
    <trd lng="nl">wand</trd>
    <trdgrp lng="pt">
      <trd>tabique</trd>,
      <trd>parede interna</trd>,
      <trd>divis&oacute;ria</trd>
    </trdgrp>
  </snc>
  <snc mrk="vand.0o.MEB">
    <dif>
      Interna maldika apartiga&jcirc;o dividanta meblon, vazon, objekton:
      <ekz>
        <tld/>o de &scirc;ranko, kesto, forno, tubo.
      </ekz>
    </dif>
    <trd lng="ca">paret</trd>
    <trdgrp lng="it">
      <trd>pannello</trd>,
      <trd>divisorio</trd>
    </trdgrp>
    <trd lng="nl">tussenschot</trd>
    <trd lng="pt">divis&oacute;ria<klr tip="amb"> (de um objeto)</klr></trd>
  </snc>
  <snc mrk="vand.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Interna maldika apartiga&jcirc;o dividanta kavan organon &ccirc;e vegeta&jcirc;o
      a&ubreve;
      besto:
      <ekz>
        <tld/>o de frukto, nazo, koro.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>septe</trd>,
      <trd>env&agrave;</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>setto</trd>,
      <trd>sepimento</trd>,
      <trd>dissepimento</trd>
    </trdgrp>
    <trd lng="nl">wand</trd>
    <trdgrp lng="pt">
      <trd>septo</trd>,
      <trd>tabique<klr tip="amb"> (anatomia)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_je;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Wand</trd>
  <trd lng="en">partition</trd>
  <trd lng="es">tabique</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&f_y;&a_w;&a_A;&a_r;&a_h;</trd>,
    <trd>&a_j;&a_d;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cloison</trd>,
    <trd>paroi</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;laszfal</trd>,
    <trd>s&ouml;v&eacute;ny <klr tip="ind">(septum)</klr></trd>,
    <trd>septum</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
