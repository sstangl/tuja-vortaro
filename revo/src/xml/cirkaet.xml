<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cirkaet.xml,v 1.4 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>cirkaet</rad>/o
  <fnt>
    <bib>PIV3</bib>
  </fnt>
</kap>

<drv mrk="cirkaet.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="agl.serpent0o">serpentaglo</ref>
    <trd lng="ca"><ind>&agrave;guila</ind> marcenca</trd>
    <trd lng="en">short-toed <ind>eagle</ind></trd>
    <trd lng="pl">gado&zdot;er</trd>
    <trd lng="sv">orm<ind>&ouml;rn</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>orel krátkoprstý</trd>,
    <trd>orlík krátkoprstý</trd>
  </trdgrp>
  <trd lng="sk">hadiar krátkoprstý</trd>
</drv>

</art>

