<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxtokton.xml,v 1.3 2012/02/08 14:10:12 revo Exp $">
<kap>
  <rad>a&ubreve;tokton</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="auxtokton.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtokton.0o.ETN">
    <ref tip="dif" cel="indigx.0o.ETN">indi&gcirc;eno</ref>
  </snc>
  <trdgrp lng="it">
    <trd>autoctono<klr> (sost.)</klr></trd>,
    <trd>nativo<klr> (sost.)</klr></trd>,
    <trd>indigeno<klr> (sost.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="auxtokton.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtokton.0a.ETN">
    <ref tip="dif" cel="indigx.0a.ETN">indi&gcirc;ena</ref>
  <ekz>
    la esperanta prononco estas tre malsimila al la landnomo <tld/>a
    <fnt>
      <aut>Remush</aut>,
      <vrk>Asia esperanto</vrk>,
      <lok><url
      ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
      2004-07-10</lok>
    </fnt>.
  </ekz>
  </snc>
  <trdgrp lng="it">
    <trd>autoctono<klr> (agg.)</klr></trd>,
    <trd>nativo<klr> (agg.)</klr></trd>,
    <trd>indigeno<klr> (agg.)</klr></trd>
  </trdgrp>
</drv>
</art>

