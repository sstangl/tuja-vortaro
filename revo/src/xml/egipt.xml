<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: egipt.xml,v 1.28 2015/07/04 08:37:33 revo Exp $">
    <kap><rad>egipt</rad>/o</kap>

    <drv mrk="egipt.0o">
      <kap><tld/>o</kap>
      <snc mrk="egipt.0o.HIS">
        <uzo tip="fak">HIS</uzo>
        <dif>
          Ano de antikva popolo, kiu kreis grandan civilizon en la
          malsupra fluo de Nilo, en Afriko:
          <ekz>
            Potipanea, flavaviza&gcirc;a <tld/>o, en pastra vesto de sia
            lando <klr>...</klr> staris &ccirc;e la pordo
            <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap.
            20a</lok></fnt>;
          </ekz>
          <ekz>
            similan teorion li prezentas pri <tld lit="E"/>io,
            atentigante,
            ke antikvaj skriba&jcirc;oj mencias iujn misterajn
            marpopolojn kaj tiel
            nomatajn <ctl>re&gcirc;ojn-pa&scirc;tistojn</ctl>, de kiuj
            la
            <tld/>oj supozeble heredis sian sperton pri navigado
            <fnt>
              <aut>Garbhan Macaoidh</aut>: <vrk>&Ccirc;u Esperanto de la
              Andoj?</vrk>
              <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            &scirc;i havis sklavinon, <ind><tld/>ino</ind>n, kies nomo
            estis Hagar
            <fnt>
              <bib>MT</bib>, <lok>&Gen; 16:1</lok>
            </fnt>.
            <trd lng="pl">Egipcjanka</trd>
            <trd
            lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>
          </ekz>
        </dif>
        <trd
        lng="be">&c_e;&c_g;&c_ib;&c_p;&c_c;&c_ja;&c_n;&c_ib;&c_n;</trd>
        <trd lng="de">&Auml;gypter</trd>
        <trd lng="en">Egyptian</trd>
        <trd lng="fa">&a_m;&a_S;&a_r;&f_y;</trd>
        <trd lng="fr">&Eacute;gyptien<klr> (subst.)</klr></trd>
        <trd lng="es">egipcio</trd>
        <trd lng="hu">egyiptomi</trd>
        <trd lng="nl">Egyptenaar</trd>
      <trd lng="no">egypter</trd>
        <trd lng="pl">Egipcjanin</trd>
        <trd lng="pt">eg&iacute;pcio</trd>
        <trd
        lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
      </snc>
      <snc mrk="egipt.0o.POL">
        <uzo tip="fak">POL</uzo>
          <ref tip="dif" cel="egipt.0ujano"><tld lit="E"/>ujano</ref>
          <ekz>
            la unuan fojon &scirc;i a&ubreve;dis la legendon, kiun la
            <tld/>oj rakontas al si pri la struto
            <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
            re&gcirc;o</lok></fnt>.
          </ekz>
        <trd
        lng="be">&c_e;&c_g;&c_ib;&c_p;&c_c;&c_ja;&c_n;&c_ib;&c_n;</trd>
        <trdgrp lng="de">
          <trd>&Auml;gypter</trd>,
          <trd>&auml;gyptischer <ind>Staatsb&uuml;rger</ind></trd>
        </trdgrp>
        <trd lng="en">Egyptian</trd>
        <trd lng="es">egipcio</trd>
        <trd lng="fa">&a_m;&a_S;&a_r;&f_y;</trd>
        <trd lng="fr">&Eacute;gyptien<klr> (subst.)</klr></trd>
        <trd lng="hu">egyiptomi</trd>
        <trd lng="nl">Egyptenaar</trd>
      <trd lng="no">egypter</trd>
        <trd lng="pl">Egipcjanin</trd>
        <trd
        lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
      </snc>
      <snc mrk="egipt.0o.Kabe">
        <uzo tip="stl">EVI</uzo>
        <dif>
        <klr><tld lit="E"/>o</klr>
        </dif>
          <ref tip="dif" cel="egipt.E0ujo"><tld lit="E"/>ujo</ref>
          <ekz>
            li estis fama en la tuta <tld lit="E"/>o
            <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 2a</lok></fnt>;
          </ekz>
          <ekz>
          de la komenco &gcirc;is nun mi &ccirc;iam uzadis la nomon "<tld
          lit="E"/>o" ne por la lando, sed por la popolo: la landon mi
          nomadis "<tld lit="E"/>ujo"<fnt><bib>LR</bib>, <lok>Respondo
          47,
          Oficiala Gazeto, III, 1911, p. 291</lok></fnt>.
          </ekz>
        <trd lng="ca">Egipte</trd>
        <trd lng="de">&Auml;gypten</trd>
        <trd lng="es">Egipto</trd>
        <trd lng="fa">&a_m;&a_S;&a_r;</trd>
        <trd lng="fr">&Eacute;gypte</trd>
        <trd lng="hu">Egyiptom</trd>
        <trd lng="nl">Egypte</trd>
        <trd lng="no">Egypt</trd>
      </snc>
      <trdgrp lng="ca">
        <trd>egipci</trd>,
        <trd>egipci&agrave;<klr> (subst.)</klr></trd>
      </trdgrp>
      
    </drv>

    <drv mrk="egipt.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al la <tld/>oj.
        </dif>
      </snc>
      <snc>
        <dif>
          <tld lit="E"/>uja, rilata al la <tld lit="E"/>ujo.
        </dif>
      </snc>
  <trd lng="be">&c_e;&c_g;&c_ib;&c_p;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="ca">egipci<klr> (adj.)</klr></trd>
  <trd lng="cs">egyptský</trd>
  <trd lng="de">&auml;gyptisch</trd>
  <trd lng="en">Egyptian</trd>
  <trd lng="es">egipcio</trd>
  <trd lng="fa">&a_m;&a_S;&a_r;&f_y;</trd>
  <trd lng="fr">&eacute;gyptien<klr> (adj.)</klr></trd>
  <trd lng="hu">egyiptomi</trd>
  <trd lng="nl">Egyptisch</trd>
  <trd lng="no">egyptisk</trd>
  <trd lng="pl">egipski</trd>
  <trd
      lng="ru">&c_je;&c_g;&c_i;&c_p;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">egyptský</trd>
</drv>

    <drv mrk="egipt.E0ujo">
      <kap><tld lit="E"/>ujo <fnt><bib>ZR</bib></fnt>
      </kap>
      <snc mrk="egipt.E0ujo.GEO">
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Lando en la nordorienta Afriko, &ccirc;irka&ubreve; la
          malsupra parto de
          Nilo, kie iam esti&gcirc;is la civilizo de la
          <ref cel="egipt.0o.HIS"><tld/>oj<sncref/></ref> kaj nun
          estas araba &scirc;tato.
          Teritorio de <em>Araba Respubliko Egiptujo</em>.
          ISO-3166-1 kodo: EG.
        </dif>
      </snc>
      <trd lng="ar">&a_m;&a_S;&a_r;</trd>
      <trd lng="be">&c_E;&c_g;&c_ib;&c_p;&c_e;&c_t;</trd>
      <trd lng="bg">&c_Je;&c_g;&c_i;&c_p;&c_je;&c_t;</trd>
      <trdgrp lng="ca">
        <trd>Egipte</trd>,
        <trd>Rep&uacute;blica &Agrave;rab d'<ind>Egipte</ind></trd>
      </trdgrp>
      <trd lng="de">&Auml;gypten</trd>
      <trd lng="en">Egypt</trd>
      <trd lng="es">Egipto</trd>
      <trd lng="fa">&a_m;&a_S;&a_r;</trd>
      <trdgrp lng="fr">
        <trd>&Eacute;gypte</trd>,
        <trd>R&eacute;publique arabe d'&Eacute;gypte</trd>
      </trdgrp>
      <trd lng="hu">Egyiptom</trd>
      <trd lng="id">Mesir</trd>
      <trdgrp lng="pl">
        <trd>Egipt</trd>,
        <trd>Arabska Republika Egiptu</trd>
      </trdgrp>
      <trd lng="pt">Egito</trd>
      <trd lng="ru">&c_Je;&c_g;&c_i;&c_p;&c_je;&c_t;</trd>
      <trd lng="nl">Egypte</trd>
      <trd lng="no">Egypt</trd>
      <trd lng="tr">M&inodot;s&inodot;r</trd>
    </drv>

    <drv mrk="egipt.0ujano">
      <kap><tld/>ujano</kap>
      <snc>
        <dif>
          Lo&gcirc;anto de <tld lit="E"/>ujo:
          <ekz>
            la modernaj <tld/>ujanoj estas, plejparte, araboj kaj
            koptoj;
          </ekz><ekz>
            Omar kaj Ijob esprimis koran dankon kaj &scirc;aton, kaj
            la <tld/>ujanoj aprobis ilin per &gcirc;entila riverenco
	    <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 5a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd
      lng="be">&c_e;&c_g;&c_ib;&c_p;&c_c;&c_ja;&c_n;&c_ib;&c_n;</trd>
      <trd lng="ca">egipci<klr> (subst.)</klr></trd>
      <trd lng="de">&Auml;gypter</trd>
      <trd lng="en">Egyptian</trd>
      <trd lng="es">egipcio</trd>
      <trd lng="fa">&a_m;&a_S;&a_r;&f_y;</trd>
      <trd lng="fr">&Eacute;gyptien<klr> (subst.)</klr></trd>
      <trd lng="hu">egyiptomi<klr tip="ind"> (lakos)</klr></trd>
      <trd lng="nl">Egyptenaar</trd>
      <trd lng="no">egypter</trd>
      <trd lng="pl">Egipcjanin</trd>
      <trd
      lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
    </drv>
  </art>
