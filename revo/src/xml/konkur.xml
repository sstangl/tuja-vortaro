<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkur.xml,v 1.19 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konkur</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konkur.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="konkur.0i.KOMUNE">
    <dif>
      Vetbatali kontra&ubreve; alia por atingi anta&ubreve; li difinitan
      celon; peni por superi alian:
      <ekz>
        <tld/>i iun, kun iu per talento, klopodoj;
      </ekz>
      <ekz>
        <tld/>i kontra&ubreve; iu por akiri ies favoron;
      </ekz>
      <ekz>
        <tld/>i kun iu pri vakanta ofico;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>antaj interesoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la muziko pene <tld/>adis kun la rapida dancado<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">competir</trd>
  <trdgrp lng="cs">
    <trd>soutěžit</trd>,
    <trd>závodit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>konkurrieren</trd>,
     <trd>wetteifern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compete</trd>,
    <trd>contend</trd>,
    <trd>rival</trd>
  </trdgrp>
  <trd lng="es">competir</trd>
  <trdgrp lng="fr">
     <trd>rivaliser</trd>,
     <trd>concurrencer</trd>,
     <trd>faire concurrence &agrave;</trd>
  </trdgrp>
  <trd lng="nl">wedijveren</trd>
  <trdgrp lng="pt">
    <trd>concorrer</trd>,
    <trd>competir</trd>,
    <trd>rivalizar</trd>,
    <trd>porfiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">súťažiť</trd>
  <trd lng="tp">utala</trd>
</drv>

<drv mrk="konkur.0ado">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="konkur.0ado.KOMUNE">
    <dif>
      Vetbatalo inter pluraj personoj:
      <ekz>
        lojala <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>o pri profito;
      </ekz>
      <ekz>
        <tld/>ado al feli&ccirc;o, en militpreparoj;
      </ekz>
      <ekz>
        la ka&ubreve;zo de la intergenta malamo ne estas la <tld/>ado
        ekonomia<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konkur1.0o">konkurenco</ref>,
      <ref cel="konkur2.0o">konkurso</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">competici&oacute;</trd>
  <trdgrp lng="cs">
    <trd>hospodářská soutěž</trd>,
    <trd>rivalita</trd>,
    <trd>soutěžení</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Wettbewerb</trd>,
     <trd>Wettstreit</trd>,
     <trd>Wettlauf</trd>
  </trdgrp>
  <trd lng="en">competition</trd>
  <trd lng="es">competici&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>concours</trd>,
    <trd>concurrence</trd>,
    <trd>comp&eacute;tition</trd>,
    <trd>rivalit&eacute;</trd>
  </trdgrp>
  <trd lng="nl">wedijver</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_r;&c_je;&c_v;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">hospodárska súťaž</trd>
  <trd lng="tp">utala</trd>
</drv>

<drv mrk="konkur.0e">
  <kap><tld/>e</kap>
  <snc mrk="konkur.0e.KOMUNE">
    <dif>
      Kiel <tld/>antoj; <ref tip="vid" cel="vet.0e.FIG">vete</ref>:
      <ekz>
        ili <tld/>e paroladis;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>e rapidas al la pordo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_y;&c_p;&c_je;&c_r;&c_a;&c_d;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>en competici&oacute;</trd>,
    <trd>competitivament</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>um die Wette</trd>
  </trdgrp>
  <trd lng="en">competitively</trd>
  <trd lng="nl">om het <ind>hardst</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_k;&c_i;</trd>
  <trd lng="fr"><ind>concurremment</ind> (en concurrence)</trd>
</drv>
</art>
