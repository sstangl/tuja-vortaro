<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ronk.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ronk</rad>/i
</kap>
<drv mrk="ronk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi bruon spirante dum la dormo.
    </dif>
    <trd lng="be">&c_h;&c_r;&c_a;&c_p;&c_c;&c_ib;</trd>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>rinxolar</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trd lng="hu">horkol</trd>    
    <trd lng="nl">snurken</trd>
    <trd lng="pt">roncar</trd>
    <trd lng="ru">&c_h;&c_r;&c_a;&c_p;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sv">snarka</trd>
  </snc>
  <snc>
    <dif>
      Eligi bruon similantan al <tld/>ado;
      <ref tip="sin" cel="grunt.0i">grunti</ref>:
      <ekz>
        li <tld/>is kiel porko, kiel kolera leonido<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>remorejar</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szuszog</trd>,
      <trd>f&uacute;jtat</trd>,
      <trd>horkant</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knorren</trd>,
      <trd>grommen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chrochtat</trd>,
    <trd>chrápat</trd>,
    <trd>frkat <klr>(kůň)</klr></trd>
  </trdgrp>
  <trd lng="de">schnarchen</trd>
  <trd lng="en">snore</trd>
  <trd lng="fr">ronfler</trd>
  <trdgrp lng="sk">
    <trd>chrochtať</trd>,
    <trd>chrápať</trd>
  </trdgrp>
</drv>
</art>
