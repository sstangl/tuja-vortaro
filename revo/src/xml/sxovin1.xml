<vortaro>
<art mrk="$Id: sxovin1.xml,v 1.18 2015/04/17 22:10:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;ovinist</rad>/o
</kap>
<drv mrk="sxovin1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fanatikulo konceptanta &scirc;ovinisme la patriotismon:
      <ekz>
        ju pli ni interparolis, des pli mi konvinki&gcirc;is, ke li i&gcirc;is
        <tld/>o, legante propagandajn presa&jcirc;ojn de rusaj <tld/>oj,
        kiujn ili senpage propagas<fnt><aut>JeZo</aut>: <vrk>Krimoj en
        Krimeo</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 6a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_v;&c_ib;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="de">Chauvinist</trd>
  <trd lng="el">
&sigma;&omega;&beta;&jota;&ny;&jota;&sigma;&tau;&eta_ton;&sigma_fina;
</trd>
  <trdgrp lng="en">
    <trd>chauvinist</trd>,
    <trd>jingo</trd>,
    <trd>jingoist</trd>
  </trdgrp>
  <trd lng="es">chovinista</trd>
  <trd lng="fa">&a_sh;&a_w;&a_w;&f_y;&a_n;&f_y;&a_s;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>chauvin<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">soviniszta</trd>
  <trd lng="it">sciovinista<klr> (sost.)</klr></trd>
  <trd lng="nl">chauvinist</trd>
  <trd lng="pt">chauvinista</trd>
  <trd lng="ru">&c_sh;&c_o;&c_v;&c_i;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
