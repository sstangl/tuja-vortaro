<kap>
<rad>falarop</rad>/o
</kap>
<drv mrk="falaro.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
       <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
       cel="skolop2.0oj">skolopedoj</ref>
       (<trd lng="la">Phalaropus</trd>), birdo el la arkta regiono.
    </dif>
  </snc>
  <trd lng="ca">escuraflascons</trd>
  <trdgrp lng="cs">
    <trd>bouřlivák</trd>,
    <trd>lyskonoh</trd>
  </trdgrp>
  <trd lng="de">Wassertreter</trd>
  <trd lng="en">phalarope</trd>
  <trd lng="fr">phalarope</trd>
  <trd lng="hu">v&iacute;ztapos&oacute;</trd>
  <trd lng="pl">p&lstroke;atkon&oacute;g</trd>
  <trdgrp lng="pt">
    <trd>falaropo</trd>,
    <trd>falaropod&iacute;deo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_v;&c_u;&c_n;&c_ch;&c_i;&c_k;</trd>
  <trd lng="sk">lyskonoh</trd>
</drv>
</art>

