<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cum.xml,v 1.7 2009/08/19 18:42:18 revo Exp $">
<kap>
  <rad>cum</rad>/o
</kap>
<drv mrk="cum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>,
      &ccirc;e kiu sub la unua floro aperas flankaj bran&ccirc;oj
      fini&gcirc;antaj per floro, sub kiu denove aperas flankaj
      bran&ccirc;oj ktp:
      <ekz>
        <ind>du<tld/>o</ind>
        <klr>(sub &ccirc;iu floro aperas du flankaj
        bran&ccirc;oj)</klr>;
        <trd lng="de">Dichasium</trd>
        <trd lng="hu">kett&odblac;s <ind>bog</ind></trd>
        <trd lng="pl">wierzchotka dwuramienna<klr>
        (kwiatostan)</klr></trd>
      </ekz>
      <ekz>
        <ind>plur<tld/>o</ind>
        <klr>(sub &ccirc;iu floro aperas pluraj flankaj
        bran&ccirc;oj)</klr>;
        <trdgrp lng="de">
          <trd>Pleiochasium</trd>, <trd>Trugdolde</trd>
        </trdgrp>
        <trd lng="hu">t&ouml;bbesbog</trd>
        <trdgrp lng="pl">
          <trd>wierzchotka wieloramienna<klr> (kwiatostan)</klr></trd>,
          <trd>wierzchotka wielopromienista<klr>
          (kwiatostan)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>kunmetita <tld/>o</ind>
        <klr>(&ccirc;e meza akso, sidas cumetoj; tirso)</klr>
        <trd lng="de">Thyrsus</trd>
        <trd lng="hu">&ouml;sszetett <ind>bogas</ind> vir&aacute;gzat</trd>
      </ekz>
      <ekz>
        <ind>alterna unu<tld/>o</ind>
        <klr>(sub &ccirc;iu floro aperas nur unu plia bran&ccirc;o
        sur alternantaj flankoj, tiel ke la infloresko aperas zigzage;
        cincino)</klr>;
        <trd lng="de">Wickel</trd>
        <trd lng="hu">forg&oacute; <ind>vir&aacute;gzat</ind></trd>
        <trd lng="pl">dwurz&eogonek;dka<klr> (kwiatostan
        wierzchotkowy)</klr></trd>
      </ekz>
      <ekz>
        <ind>&scirc;ra&ubreve;ba unu<tld/>o</ind>
        <klr>(sub &ccirc;iu floro aperas nur unu plia bran&ccirc;o
        &ccirc;iam sur sama flanko, tiel ke la infloresko aperas
        kiel &scirc;ra&ubreve;blinio;
        bostriko)</klr>;
        <trd lng="de">Schraubel</trd>
        <trd lng="hu">kunkor</trd>
        <trd lng="pl">skr&eogonek;tek<klr> (kwiatostan
        wierzchotkowy)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zym&ouml;ser Bl&uuml;tenstand</trd>
  <trd lng="fr">cyme</trd>
  <trd lng="hu">bogas vir&aacute;gzat</trd>
  <trd lng="it">cima<klr tip="amb"> (bot.)</klr></trd>
  <trdgrp lng="nl">
    <trd>middelpuntvliedende <ind>bloeiwijze</ind></trd>,
    <trd>bijschermdragende <ind>bloeiwijze</ind></trd>
  </trdgrp>
  <trd lng="pl">wierzchotka<klr> (kwiatostan)</klr></trd>
  <trd lng="ru">&c_c;&c_i;&c_m;&c_o;&c_z;&c_n;&c_o;&c_je;
  &c_s;&c_o;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;</trd>
</drv>
</art>
