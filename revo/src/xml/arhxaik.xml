<art mrk="$Id: arhxaik.xml,v 1.7 2015/10/30 07:10:15 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>ar&hcirc;aik</rad>/a
</kap>
<drv mrk="arhxaik.0a">
  <kap><tld/>a</kap>
  <snc mrk="arhxaik.0a.KOMUNE">
    <ref tip="dif" cel="arkaik.0a.KOMUNE">arkaika</ref>
    <ekz>
      <tld/>a klas-sistemo
      <fnt>
      <aut>W. H. Simcock</aut>,
      <vrk>&Ccirc;u en Britio?</vrk>,
      <bib>Monato</bib>,
      <lok>1997:4, p. 9a</lok>
      </fnt>;
    </ekz>
    <ekz>
      La eventoj estas rakontitaj <klr>...</klr> en iom <tld/>a
      &leftquot;di-okula&rightquot;
      stilo
      <fnt>
      <aut>Familio Macgill</aut>,
      <vrk>June, nejune, pretertempe</vrk>,
      <bib>Monato</bib>,
      <lok>2001:7, p. 26a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_a;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">arcaic</trd>
  <trd lng="en">archaic</trd>
  <trd lng="es">arc&aacute;ico</trd>
  <trd lng="fr">archa&iuml;que</trd>
  <trdgrp lng="hu">
    <trd>archaikus</trd>,
    <trd>&odblac;sr&eacute;gi</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>archaiczny</trd>,
    <trd>przestarza&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_h;&c_a;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_v;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zastaran&yacute;</trd>,
    <trd>archaick&yacute;</trd>
  </trdgrp>
  <trd lng="pt">arcaico</trd>
</drv>
</art>
