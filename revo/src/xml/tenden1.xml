<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tenden1.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>tendenc</rad>/o
</kap>

<drv mrk="tenden1.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Da&ubreve;ra inklino al io difinita:
       <ekz>
         en la cerbo de l' homo la mistika <tld/>o batalas
         kontra&ubreve; la racia.
       </ekz>
     </dif>
     <trdgrp lng="nl">
       <trd>neiging</trd>,
       <trd>tendens</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>tendencja</trd>,
       <trd>d&aogonek;&zdot;no&sacute;&cacute;</trd>,
       <trd>kierunek</trd>,
       <trd>sk&lstroke;onno&sacute;&cacute;</trd>
     </trdgrp>
   </snc>
   <snc>
     <dif>
       Da&ubreve;ra emo disvastigi iun ideon rekomendante &gcirc;in pli
       a&ubreve; malpli
       malka&scirc;e en literatura verko:
       <ekz>
         rakonto kun morala <tld/>o;
       </ekz>
       <ekz>
         gazeto kun reakcia <tld/>o;
       </ekz>
       <ekz>
         <ind><tld/>a dramo</ind>;
         <trd lng="hu">ir&aacute;nydr&aacute;ma</trd>
       </ekz>
       <ekz>
         <ind><tld/>a</ind> <klr>(partieca)</klr> raporto.
         <trd lng="be">
         &c_t;&c_e;&c_n;&c_d;&c_e;&c_n;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
         <trd lng="bg">
         &c_t;&c_je;&c_n;&c_d;&c_je;&c_n;&c_c;&c_i;&c_o;&c_z;&c_je;&c_n;
         </trd>
         <trd lng="ca">tendenci&oacute;s</trd>
         <trd lng="de">tendenzi&ouml;s</trd>
         <trdgrp lng="hu">
           <trd>tendenci&oacute;zus</trd>,
           <trd>ir&aacute;nyzatos</trd>
         </trdgrp>
         <trdgrp lng="pl">
           <trd>tendencja</trd>,
           <trd>sk&lstroke;onno&sacute;&cacute;</trd>
         </trdgrp>
         <trd lng="ru">
     &c_t;&c_je;&c_n;&c_d;&c_je;&c_n;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;
         </trd>
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_e;&c_n;&c_d;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_je;&c_n;&c_d;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">tend&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>ráz</trd>,
    <trd>tendence</trd>,
    <trd>zabarvení</trd>,
    <trd>zaměření</trd>,
    <trd>zaujatost</trd>,
    <trd>záměr</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Neigung</trd>,
     <trd>Tendenz</trd>
   </trdgrp>
  <trd lng="en">tendency</trd>
  <trd lng="es">tendencia</trd>
  <trd lng="fr">tendance</trd>
  <trdgrp lng="hu">
     <trd>tendencia</trd>,
     <trd>ir&aacute;nyzat</trd>,
     <trd>ir&aacute;nyul&aacute;s</trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>tend&ecirc;ncia</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_d;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>snaha</trd>,
    <trd>tendencia</trd>,
    <trd>zámer</trd>,
    <trd>úmysel</trd>
  </trdgrp>
</drv>
</art>
