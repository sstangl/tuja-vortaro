<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kotang.xml,v 1.20 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kotangent</rad>/o
</kap>
<drv mrk="kotang.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="kotang.0o.entriangulo">
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> <frm>&alfa;</frm>)</klr>
      <ref cel="tangen.0o">Tangento</ref> de &gcirc;ia
      <ref cel="komple1.0o.angulo">komplemento</ref>; 
      simb. <frm>cotg &alfa;</frm> 
      a&ubreve; <frm>kotang &alfa;</frm>:
      <ekz>
        la <tld/>o de angulo estas anka&ubreve; la inverso de &gcirc;ia
        tangento.
      </ekz>
    </dif>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm>&alfa;</frm> egalas al <frm>BU/OM</frm> -->
      la <tld/>o de angulo &alfa; egalas al BU/OM 
    </bld>
  </snc>
  <snc mrk="kotang.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu &jcirc;etas
      reelon <frm><k>x</k></frm> al la 
      <tld/>o <sncref ref="kotang.0o.entriangulo"/> de angulo, kies mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas de <frm><k>x</k></frm> 
      per oblo de <frm>&pi;</frm>; 
      simb. <frm>cotg</frm> a&ubreve; <frm>kotang</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun
        <ref cel="period.0o.funkcio">periodo <sncref/></ref> <frm>&pi;</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>cotangens</trd>,
    <trd>kotangens</trd>,
    <trd>kotangenta</trd>
  </trdgrp>
  <trd lng="de">Kotangens</trd>
  <trd lng="en">cotangent</trd>
  <trd lng="fr">cotangente</trd>
  <trd lng="hu">kotangens</trd>
  <trd lng="pl">cotangens</trd>
  <trd lng="pt">co-tangente</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="sk">kotangens</trd>
</drv>

<drv mrk="kotang.are0o">
  <kap>are<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <ref tip="dif" cel="kotang.inversa_hiperbola0o">inversa hiperbola <tld/>o.</ref>
    <rim>
      Vd rimarkon sub <ref cel="are.0o.argumento">areo</ref>.
    </rim>
  </snc>
</drv>

<drv mrk="kotang.ark0o">
  <kap>ark<tld/>o, <var><kap>arko<tld/>o</kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="kotang.0o.funkcio"><tld/>o</ref> al la
      intervalo <frm>]0,&pi;[</frm>; simb. <frm>arccotg</frm>
      a&ubreve; <frm>arkkotang</frm>:
      <ekz>
        ark<tld/>o de <frm>0</frm> egalas al <frm>&pi;/2</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_k;&c_a;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="de">Arkuskotangens</trd>
  <trd lng="en">arc cotangent</trd>
  <trd lng="fr">arc cotangente</trd>
  <trd lng="hu">arkusz kotangens</trd>
  <trd lng="pl">arcus cotangens</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
</drv>

<drv mrk="kotang.hiperbola0o">
  <kap>hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu &jcirc;etas
      reelon <frm><k>x</k></frm> al 
      <frm>(cosh <k>x</k>)/(sinh <k>x</k>)</frm>; 
      simb. <frm>cotgh</frm> a&ubreve; <frm>kotangh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_k;&c_a;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  <trdgrp lng="de">
    <trd>Hyperbelkotangens</trd>,
    <trd>hyperbolischer <ind>Kotangens</ind></trd>
  </trdgrp>
  <trd lng="en">hyperbolic <ind>cotangent</ind></trd>
  <trd lng="fr">cotangente hyperbolique</trd>
  <trd lng="hu">kotangens hiperbolikusz</trd>  
  <trd lng="pl">cotangens hiperboliczny</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; 
  <ind>&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
</drv>

<drv mrk="kotang.inversa_hiperbola0o">
  <kap>inversa hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="kotang.hiperbola0o">hiperbola <tld/>o</ref> al la
      intervalo <frm>]0,+&#8734;[</frm>; 
      simb. <frm>arcotgh</frm> a&ubreve; <frm>arkotangh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_k;&c_a;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  <trd lng="de">Areakotangens</trd>
  <trdgrp lng="en">
    <trd>area-hyperbolic cotangent</trd>,
    <trd>inverse hyperbolic cotangent</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>argument de la <ind>cotangente</ind> hyperbolique</trd>,
    <trd>cotangente hyperbolique inverse</trd>
  </trdgrp>
  <trd lng="hu">arkusz<ind>kotangens</ind> hiperbolikusz</trd>  
  <trd lng="pl">area cotangens hiperboliczny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_a;-&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_k;&c_o;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  </trdgrp>
</drv>
</art>
