<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teatr.xml,v 1.31 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>teatr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="teatr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="teatr.0o.belarto">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="lst" cel="art.bel0o" lst="voko:artoj">Belarto</ref>
      pri verkado kaj aran&gcirc;ado de rakontoj prezentotaj
      de aktoroj a&ubreve; pupoj al spektantoj:
      <ekz>
        de miaj infanaj jaroj mi amas la <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 11a, p.
        453a</lok></fnt>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>art dramatique</trd>
    </trdgrp>
    <trd lng="pl">teatr</trd>
    <trd lng="ro">teatru</trd>
  </snc>
  <snc mrk="teatr.0o.ejo">
    <uzo tip="fak">TEA</uzo>
    <ref tip="dif" cel="teatr.0ejo"><tld/>ejo</ref>
    <ekz>
      la koncernan petskribon alportis en la <tld/>on lia edzino
      <fnt><bib>MkM</bib>, <lok>epilogo, p. 338a</lok></fnt>;
    </ekz>
    <trd lng="pl">teatr</trd>
  </snc>
  <snc mrk="teatr.0o.spektantaro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Spektantaro:
      <ekz>
        la malla&ubreve;do de kompetenta homo devas esti por vi pli
        grava ol la apla&ubreve;doj de tuta <tld/>o da aliaj personoj
        <fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="teatr.0o.aktoreco">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Aktora profesio.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">divadlo</trd>
  <trd lng="de">Theater</trd>
  <trd lng="en">theatre</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z</trd>
  <trd lng="id">teater</trd>
  <trdgrp lng="nl">
    <trd>theater</trd>,
    <trd>toneel</trd>
  </trdgrp>
  <trd lng="pl">teatr</trd>
  <trdgrp lng="pt">
   <trd>teatro</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">divadlo</trd>
  <trd lng="sv">teater</trd>
</drv>

<drv mrk="teatr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="teatr.0ajxo.TEA">
    <tezrad fak="TEA"/>	  
    <uzo tip="fak">TEA</uzo>
    <dif>
      <tld lit="T"/>a verko kun ties roloj, dialogoj kaj agadoj:
      <ekz>
        kroataj <tld/>a&jcirc;oj kaj aktoroj multe kontribuis al la
        esperanta <tld/>o
        <fnt>
          <aut>B. Kolker</aut>,
          <vrk>Teatra antologio</vrk>,
          <bib>Monato</bib>,
          <lok>1998:3, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;'&c_je;&c_s;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce de th&eacute;&acirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;nm&udblac;</trd>,
    <trd>sz&iacute;ndarab</trd>
  </trdgrp>
  <trd lng="nl">toneelstuk</trd>
  <trdgrp lng="pl">
    <trd>sztuka <ind>teatralna</ind></trd>,
    <trd>przedstawienie <ind>teatralne</ind></trd>,
    <trd>spektakl <ind>teatralny</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
</drv>

<drv mrk="teatr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="teatr.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Loko, ordinare <ref tip="super" cel="konstr.0ajxo">
      konstrua&jcirc;o</ref>,
      en kiu oni prezentas <tld/>a&jcirc;ojn:
      <ekz>
        la blindige brilaj lumtuboj de la strat'angula <tld/>ejo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a, p. 204a</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="teatr.0o.ejo">teatro <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;<klr>
  (&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">divadlo <klr>(budova)</klr></trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre<klr> (b&acirc;timent)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&iacute;nh&aacute;z<klr tip="amb">
  (&eacute;p&uuml;let)</klr></trd>
  <trd lng="nl">schouwburg</trd>
  <trd lng="pl">teatr</trd>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r;<klr tip="amb">
  (&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">divadlo <klr>(budova)</klr></trd>
</drv>

<drv mrk="teatr.film0o">
  <kap>film<tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="teatr.film0o.kinejo">
    <uzo tip="fak">KIN</uzo>
    <ref tip="dif" cel="kin.0ejo">kinejo</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="fr">cin&eacute;matographe</trd>
  <trd lng="hu">filmsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">bioscoop</trd>
  <trd lng="pl">kino</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
</drv>

<drv mrk="teatr.kino0o">
  <kap>kino<tld/>o</kap>
  <snc mrk="teatr.kino0o.kinejo">
    <uzo tip="fak">KIN</uzo>
    <ref tip="dif" cel="kin.0ejo">kinejo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>biograf</trd>,
    <trd>kino</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>movie theater</trd>,
    <trd>cinema</trd>,
    <trd>movie house</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;matographe</trd>
  <trd lng="he">&bet;&yod;&tav; &qof;&vav;&lamed;&nun;&vav;&ayin;</trd>
  <trd lng="nl">bioscoop</trd>
  <trd lng="pl">kino</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">kino</trd>
</drv>

<drv mrk="teatr.ombro0o">
  <kap>ombro<tld/>o</kap>
  <snc mrk="teatr.ombro0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <tld lit="T"/>o en kiu rolulojn
      prezentas plataj figuroj movataj anta&ubreve; prilumata ekrano:
      <ekz>
        Longdong-ombroteatro tre furoris en la dinastioj Ming kaj Qing
        <klr>...</klr> de la 14-a jarcento &gcirc;is la 19-a jarcento
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/chinaabc/chapter21/chapter210101.htm">
          Ombroteatro</url></vrk>,
          <lok>&Ccirc;ina abc, [vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">stínové divadlo</trd>
  <trdgrp lng="fr">
    <trd>ombres chinoises</trd>,
    <trd>th&eacute;&acirc;tre d'ombres</trd>
  </trdgrp>
  <trd lng="pl">teatr cieni</trd>
  <trd lng="sk">tieňové divadlo</trd>
</drv>

<drv mrk="teatr.pup0o">
  <kap>pup<tld/>o</kap>
  <snc mrk="teatr.pup0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0o.belarto">Teatro</ref>
      en kiu rolas pupoj:
      <ekz>
        la &ccirc;ina pup<tld/>o havas historion de pli ol 2000 jaroj
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro
        &leftquot;Pupteatro&rightquot;, p. 232a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_l;&c_je;&c_ch;&c_n;&c_y; <ind>&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</ind></trd>
  <trd lng="cs">loutkové divadlo</trd>
  <trd lng="de">Puppentheater</trd>
  <trdgrp lng="fr">
    <trd>th&eacute;&acirc;tre de marionnettes</trd>
  </trdgrp>
  <trd lng="hu">b&aacute;bsz&iacute;nh&aacute;z</trd>
  <trd lng="nl">poppentheater</trd>
  <trdgrp lng="pl">
    <trd>teatr lalek</trd>,
    <trd>teatrzyk kukie&lstroke;kowy</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_t;&c_je;&c_a;&c_t;&c_r;</ind></trd>
  <trd lng="sk">bábkové divadlo</trd>
  <trd lng="sv">dockteater</trd>
</drv>

<drv mrk="teatr.varieta0o">
  <kap>varietea <tld/>o</kap>
  <snc mrk="teatr.varieta0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0o.belarto">Teatro</ref>
      en kiu oni prezentas
      <ref tip="vid" cel="variet.0o.TEA">varieteon</ref>.
    </dif>
    <ref tip="vid" cel="kabare.0o.ejo">kabareto</ref>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_t;&c_r; 
    <ind>&c_v;&c_a;&c_r;'&c_ja;&c_t;&c_e;</ind></trd>
  <trd lng="cs">varietní divadlo</trd>
  <trdgrp lng="de">
    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;theater</trd>
  </trdgrp>
  <trd lng="fr">th&eacute;&acirc;tre de <ind>vari&eacute;t&eacute;</ind>s</trd>
  <trdgrp lng="hu">
    <trd>variet&eacute;sz&iacute;nh&aacute;z</trd>,
    <trd>orfeum</trd>,
    <trd>zen&eacute;s kabar&eacute;</trd>
  </trdgrp>
  <trd lng="nl">variet&eacute;-theater</trd>
  <trdgrp lng="pl">
    <trd>kabaret</trd>,
    <trd>variete</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>teatro de variedades</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_a;&c_t;&c_r; <ind>&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</ind></trd>
  <trd lng="sk">varietné divadlo</trd>
</drv>
</art>
