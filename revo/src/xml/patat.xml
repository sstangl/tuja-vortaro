<kap>
  <rad>patat</rad>/o
</kap>
<drv mrk="patat.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
      <ref tip="dif" cel="batat.0o">batato</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;&c_ja; &c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>batát</trd>,
    <trd>sladký bramor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Batate</trd>,
    <trd>S&uuml;&szlig;kartoffel</trd>
  </trdgrp>
  <trd lng="fr">batate</trd>
  <trdgrp lng="hu">
    <trd>bat&aacute;ta</trd>,
    <trd>&eacute;desburgonya</trd>
  </trdgrp>
  <trd lng="nl">bataat</trd>
  <trd lng="pt">batata-doce</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j; &c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>batát</trd>,
    <trd>sladký zemiak</trd>
  </trdgrp>
</drv>
</art>
