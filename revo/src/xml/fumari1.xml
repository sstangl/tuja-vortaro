  <trd lng="pt">fumari&aacute;ceas</trd>
  <trd lng="ru">&c_d;&c_y;&c_m;&c_ja;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

