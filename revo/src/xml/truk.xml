<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: truk.xml,v 1.8 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>truk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="truk.0o">
  <kap><tld/>o</kap>
  <snc mrk="truk.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="artifi.0o.KOMUNE">Artifiko</ref>, kiu
      iluzias
      al la spektantoj:
      <ekz>
        kiam mi dis&scirc;iris ilin kaj rekunmetis ilin, tio estis nur
        man<tld/>o
        <fnt><bib>DOz</bib>, <lok>&leftquot;Doroteo plukas la
        princinon&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">truc</trd>
  <trdgrp lng="cs">
    <trd>filmový trik</trd>,
    <trd>obratný kousek</trd>,
    <trd>trik</trd>
  </trdgrp>
  <trd lng="de">Trick</trd>
  <trd lng="es">truco</trd>
  <trdgrp lng="fr">
    <trd>astuce</trd>,
    <trd>trucage</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trik</trd>,
    <trd>trick</trd>
  </trdgrp>
  <trd lng="pt">truque</trd>
  <trdgrp lng="sk">
    <trd>fígeľ</trd>,
    <trd>trik</trd>
  </trdgrp>
</drv>
</art>

