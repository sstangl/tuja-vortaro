<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frango.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap><ofc>*</ofc>
<rad>frangol</rad>/o</kap>
<drv mrk="frango.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="ramnac.0oj">ramnacoj</ref>
      (<trd lng="la">Frangula</trd>);
      <ref tip="lst" cel="arb.0eto"
      lst="voko:rozoparencoj">arbeto</ref>, 
      kies beroj kaj &scirc;elo estas uzataj kiel laksigilo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_sh;&c_y;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>krušina obecná</trd>,
    <trd>krušina olšová</trd>
  </trdgrp>
  <trd lng="de">Faulbaum</trd>
  <trd lng="fr">bourdaine</trd>
  <trd lng="hu">kutyabenge</trd>
  <trdgrp lng="nl">
    <trd>vuilboom</trd>,
    <trd>sporkeboom</trd>,
    <trd>zwarte <ind>els</ind></trd>
  </trdgrp>
  <trd lng="pt">amieiro-negro</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_sh;&c_i;&c_n;&c_a; &c_l;&c_o;&c_m;&c_k;&c_a;&c_ja;</trd>
  <trd lng="sk">krušina</trd>
</drv>
</art>
