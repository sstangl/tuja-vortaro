<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: civit.xml,v 1.29 2015/07/04 08:37:32 revo Exp $">
<kap><ofc>3</ofc> <rad>civit</rad>/o</kap>

<drv mrk="civit.0o">
       <kap><tld/>o</kap>
       <snc mrk="civit.0o.urbo">
         <uzo tip="fak">HIS</uzo>
         <dif>
           A&ubreve;tonoma urbo, konsiderata kiel politika tuto.
         </dif>
         <trdgrp lng="de">
           <trd>Stadtstaat</trd>,
           <trd>Polis</trd>
         </trdgrp>
         <trd lng="en">polity</trd>
         <trdgrp lng="he">
           <trd>&mem;&shin;&tet;&resh;</trd>,
           <trd>&mem;&dalet;&yod;&nun;&he;</trd>,
           <trd>&ayin;&yod;&resh;&yod;&yod;&he;</trd>
         </trdgrp>
         <trd lng="fr">cit&eacute;</trd>
         <trdgrp lng="it">
         <trd>citt&agrave;<klr> (pol.)</klr></trd>, 
         <trd>comune<klr> (pol., stor.)</klr></trd>, 
         <trd>polis<klr> (pol., stor.)</klr></trd>
         </trdgrp>
         <trd lng="pl">pa&nacute;stwo-miasto</trd>
         <trdgrp lng="ru">
           <trd>&c_p;&c_o;&c_l;&c_i;&c_s;</trd>,
           <trd>&c_g;&c_o;&c_r;&c_o;&c_d;-&c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
         </trdgrp>
       </snc>
       <snc>
         <uzo tip="fak">HIS</uzo>
         <dif>
           Tuta&jcirc;o de la urbanoj posedantaj la politikajn
           rajtojn.
         </dif>
         <trd lng="de">B&uuml;rgerschaft</trd>
         <trd lng="en">citizenry</trd>
         <trd lng="fr">cit&eacute;</trd>
         <trd lng="it">cittadinanza<klr> (insieme di
         cittadini)</klr></trd>
         <trd lng="pl">mieszcza&nacute;stwo</trd>
       </snc>
       <snc>
         <dif>
           <tld lit="C"/>anaro, la tuto de la regnanoj posedantaj la
           politikajn rajtojn.
         </dif>
         <trd lng="en">citizenry</trd>
         <trd lng="fr">nation<klr tip="ind"> (la totalit&eacute; des
         citoyens)</klr></trd>
         <trd lng="it">cittadinanza<klr> (insieme di
         cittadini)</klr></trd>
         <trd lng="pl">spo&lstroke;ecze&nacute;stwo</trd>
       </snc>
       <adm>
         Jen konfuza amaso da sencoj, kies sola firma bazo estas la
         reala vorto &leftquot;civitano&rightquot;; la resto estas
         kopiita el iu
         fremdlingva enciklopedio sen propraj radikoj en Esperanto mem.
         Surbaze de tiu &hcirc;imero nun esti&gcirc;is egale
         &hcirc;imera
         &leftquot;Esperanto-Civito&rightquot;, eble registrinda.
         <aut>Sergio</aut>
       </adm>
       <trdgrp lng="hu">
         <trd>v&aacute;ros <klr tip="ind">(&ouml;nkorm&aacute;nyzattal)</klr></trd>,
         <trd>civitas</trd>
       </trdgrp>
       <trdgrp lng="pt">
         <trd>cidade</trd>,
         <trd>cidade livre</trd>
       </trdgrp>
       <trd lng="sk">obec</trd>
  <trd lng="sk">obec, občianstvo</trd>
  <trd lng="sk">obec, občianstvo</trd>
  <trdgrp lng="cs">
    <trd>městský stát</trd>,
    <trd>obec</trd>,
    <trd>občanstvo</trd>
  </trdgrp>
</drv>

<drv mrk="civit.0ano">
       <kap><tld/>ano</kap>
       <snc>
         <dif>
           <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>
           &gcirc;uanta la
           politikajn rajtojn, &ccirc;u en sendependa urbo, &ccirc;u en
           regno:
           <ekz>
             <tld/>anaj sentoj kaj devoj<fnt>Z</fnt>;
           </ekz>
           <ekz>
             <ind><tld/>aneco</ind>;
             <trd lng="en">citizenship</trd>
             <trd lng="fr">citoyennet&eacute;</trd>
             <trd lng="he">&alef;&zayin;&resh;&het;&vav;&tav;</trd>
             <trd lng="pl">obywatelstwo</trd>
           </ekz>
           <ekz>
             <ind>inter<tld/>ana milito</ind>.
             <trd lng="en">civil war</trd>
             <trd lng="es">guerra <ind>civil</ind></trd>
             <trd lng="fr">guerre civile</trd>
             <trd lng="it">guerra civile</trd>
             <trd lng="he">&mem;&lamed;&het;&mem;&tav;
&alef;&zayin;&resh;&het;&yod;&fmem;</trd>
             <trd lng="nl">burgeroorlog</trd>
             <trd lng="pl">wojna domowa</trd>
             <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja;
             &c_v;&c_o;&c_j;&c_n;&c_a;</trd>
           </ekz>
         </dif>
         <trdgrp lng="de">
           <trd>B&uuml;rger</trd>,
           <trd>Staatsb&uuml;rger</trd>
         </trdgrp>
         <trd lng="en">citizen</trd>
         <trd lng="es">ciudadano</trd>
         <trd lng="fr">citoyen</trd>
         <trd lng="he">&alef;&zayin;&resh;&het;</trd>
         <trdgrp lng="hu">
           <trd>polg&aacute;r</trd>,
           <trd>civis</trd>
         </trdgrp>
         <trd lng="it">cittadino<klr> (pol.)</klr></trd>
         <trd lng="nl">burger</trd>
         <trd lng="pl">obywatel</trd>
         <trd lng="pt">cidad&atilde;o</trd>
         <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;</trd>
       </snc>
  <trdgrp lng="cs">
    <trd>obyvatel říše</trd>,
    <trd>občan</trd>,
    <trd>státní příslušník</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ob&ccaron;an</trd>,
    <trd>obyvateľ</trd>,
    <trd>štátny príslušník</trd>
  </trdgrp>
</drv>

<drv mrk="civit.0ismo">
       <kap><tld/>ismo</kap>
       <snc>
         <dif>
           <tld lit="C"/>ana virto.
         </dif>
       </snc>
  <trd lng="cs">občanské uvědomění</trd>
  <trdgrp lng="en">
         <trd>civic virtue</trd>,
         <trd>good citizenship</trd>
       </trdgrp>
  <trd lng="fr">civisme</trd>
  <trd lng="he">&alef;&zayin;&resh;&het;&vav;&tav;
&tet;&vav;&bet;&he;</trd>
  <trd lng="hu">polg&aacute;ri &eacute;rz&uuml;let</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">občianske uvedomenie</trd>
</drv>

<drv mrk="civit.0anigi">
       <kap><tld/>anigi</kap>
       <uzo tip="fak">JUR</uzo>
       <uzo tip="fak">POL</uzo>
       <snc>
         <dif>
           Doni al fremdulo a&ubreve; neplenrajta &scirc;tatano la
           rajtojn de
           <tld/>ano.
         </dif>
       </snc>
  <trd lng="cs">udělit občanství</trd>
  <trdgrp lng="de">
         <trd>einb&uuml;rgern</trd>,
         <trd>B&uuml;rgerrechte verleihen</trd>
       </trdgrp>
  <trd lng="en">naturalize</trd>
  <trd lng="es">naturalizar</trd>
  <trd lng="fr">naturaliser<klr tip="ind"> (un
       &eacute;tranger)</klr></trd>
  <trd lng="he">&lamed;<ind>&alef;&zayin;&resh;&het;</ind></trd>
  <trd lng="hu">polg&aacute;rjogot ad</trd>
  <trd lng="it">naturalizzare</trd>
  <trd lng="nl">de <ind>burgerrechten</ind> geven</trd>
  <trdgrp lng="pl">
         <trd>nadawa&cacute; <ind>obywatelstwo</ind></trd>,
         <trd>naturalizowa&cacute;</trd>
       </trdgrp>
  <trdgrp lng="ru">
         <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
         <ind>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>,
         <trd>&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
       </trdgrp>
  <trdgrp lng="sk">
    <trd>udeli&tcaron; <ind>ob&ccaron;ianstvo</ind></trd>,
    <trd>udeliť občianstvo</trd>
  </trdgrp>
</drv>

<drv mrk="civit.mond0ano">
       <kap>mond<tld/>ano</kap>
       <snc>
         <dif>
           Homo, kiu rigardas la tutan mondon kiel sian patrujon.
         </dif>
       </snc>
       <refgrp tip="sin">
         <ref cel="kosmop.0o.mondcivitano">kosmopolito</ref>
       </refgrp>
       <trdgrp lng="de">
           <trd>Weltb&uuml;rger</trd>,
           <trd>Kosmopolit</trd>
       </trdgrp>
       <trdgrp lng="en">
           <trd>world citizen</trd>,
           <trd>cosmopolitan</trd>
       </trdgrp>
       <trd lng="es">ciudadano del mundo</trd>
       <trdgrp lng="fr">
         <trd>citoyen du monde</trd>,
         <trd>cosmopolite<klr> (subst.)</klr></trd>
       </trdgrp>
       <trdgrp lng="he">
         <trd>&alef;&zayin;&resh;&het;
         &he;&ayin;&vav;&lamed;&fmem;</trd>,
         <trd>
&qof;&vav;&samekh;&mem;&vav;&pe;&vav;&lamed;&yod;&tet;&yod;</trd>
       </trdgrp>
       <trdgrp lng="hu">
         <trd>vil&aacute;gpolg&aacute;r</trd>,
         <trd>kozmopolita</trd>
       </trdgrp>
       <trd lng="it">cosmopolita</trd>
       <trd lng="nl">wereldburger</trd>
       <trdgrp lng="pl">
         <trd>obywatel &sacute;wiata</trd>,
         <trd>kosmopolita</trd>
       </trdgrp>
       <trd lng="pt">cosmopolita</trd>
       <trdgrp lng="ru">
         <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;
         &c_m;&c_i;&c_r;&c_a;</trd>,
         <trd>&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;</trd>
       </trdgrp>
       <trd lng="sk">svetoob&ccaron;an</trd>
  <trd lng="cs">světoobčan</trd>
</drv>

<drv mrk="civit.mond0aneco">
       <kap>mond<tld/>aneco</kap>
       <snc>
         <dif>
           Mond<tld/>ana sinteno.
         </dif>
       </snc>
       <refgrp tip="sin">
         <ref cel="hom.0aranismo">homaranismo</ref>,
         <ref cel="kosmop.0eco">kosmopoliteco</ref>
       </refgrp>
       <trd lng="de">Weltb&uuml;rgertum</trd>
       <trdgrp lng="fr">
         <trd>citoyennet&eacute; mondiale</trd>,
         <trd>cosmopolitisme</trd>
       </trdgrp>
       <trdgrp lng="en">
         <trd>cosmopolitanism</trd>,
         <trd>world citizenship</trd>
       </trdgrp>
       <trdgrp lng="hu">
         <trd>vil&aacute;gpolg&aacute;rs&aacute;g</trd>,
         <trd>kozmopolitizmus</trd>
       </trdgrp>
       <trd lng="it">cosmopolitismo</trd>
       <trd lng="nl">wereldburgerschap</trd>
       <trd lng="pl">kosmopolityzm</trd>
       <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_p;&c_o;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
       <trd lng="sk">svetoob&ccaron;ianstvo</trd>
  <trd lng="cs">světoobčanství</trd>
</drv>
</art>
