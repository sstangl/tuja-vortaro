<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: slav.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
   <rad>slav</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="slav.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Ano de <tld/>lingva nacio:
       <ekz>
         estas vero, ke <tld/>oj ofte posedas en Esperanto pli
         bonan stilon, ol germanoj a&ubreve; romanoj; sed tio &ccirc;i 
venas ne
         de tio, ke la stilo en Esperanto estas <tld/>a, sed nur de
         tio, ke la <tld/>aj lingvoj havas vortordon pli simplan kaj
         sekve anka&ubreve; pli proksiman al la vortordo en Esperanto
         <fnt><bib>LR</bib>, <lok>Resp. n-ro 1 (1906).</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="ca">eslau</trd>
  <trd lng="cs">Slovan</trd>
  <trd lng="de">Slawe</trd>
  <trd lng="en">Slav</trd>
  <trd lng="es">eslavo</trd>
  <trd lng="fr">Slave</trd>
  <trd lng="hu">szl&aacute;v</trd>
  <trd lng="nl">Slaaf</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">Slovan</trd>
</drv>

<drv mrk="slav.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>oj, ilia historio kaj lingvoj.
     </dif>
   </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">eslau</trd>
  <trd lng="cs">slovanský</trd>
  <trd lng="de">slawisch</trd>
  <trd lng="en">Slav</trd>
  <trd lng="es">eslavo</trd>
  <trd lng="fr">slave</trd>
  <trd lng="hu">szl&aacute;v</trd>
  <trd lng="nl">Slavisch</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">slovanský</trd>
</drv>

<drv mrk="slav.0isto">
   <kap><tld/>isto</kap>
   <snc>
     <dif>
       Fakulo pri filologiaj, historiaj a&ubreve; etnografiaj studoj de la
       <tld/>a mondo.
     </dif>
   </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_v;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_v;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">eslavista</trd>
  <trd lng="cs">slavista</trd>
  <trd lng="de">Slawist</trd>
  <trd lng="es">eslavista</trd>
  <trd lng="fr">slavisant</trd>
  <trd lng="hu">szlavista</trd>
  <trd lng="nl">slavist</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">slavista</trd>
</drv>

<drv mrk="slav.tut0ismo">
   <kap>tut<tld/>ismo<fnt>PIV</fnt></kap>
   <snc>
     <dif>
       Kultura kaj politika movado celanta unuigi la <tld/>ojn
       (18-19 jc).
     </dif>
   </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_s;&c_l;&c_a;&c_v;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_s;&c_l;&c_a;&c_v;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">paneslavisme</trd>
  <trdgrp lng="cs">
    <trd>panslavismus</trd>,
    <trd>všeslovanství</trd>
  </trdgrp>
  <trd lng="de">Panslawismus</trd>
  <trd lng="en">Panslavism</trd>
  <trd lng="es">paneslavismo</trd>
  <trd lng="fr">panslavisme</trd>
  <trd lng="hu">p&aacute;nszl&aacute;vizmus</trd>
  <trd lng="nl">panslavisme</trd>
  <trd 
lng="ru">&c_p;&c_a;&c_n;&c_s;&c_l;&c_a;&c_v;(&c_ja;&c_n;)&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>panslavizmus</trd>,
    <trd>všeslovanstvo</trd>
  </trdgrp>
</drv>

<drv mrk="slav.0a.lingvo">
   <kap><tld/>a lingvo</kap>
   <uzo tip="fak">LIN</uzo>
   <snc>
     <dif>
       Lingvo apartenanta al la <tld/>a bran&ccirc;o de la
       <ref tip="malprt" cel="hind.0euxropa">hinde&ubreve;ropa</ref>
       lingvofamilio: la <ref cel="rus.0a">rusa</ref>,
       la pola, la bulgara,
       <ref tip="prt" cel="slavon.0o">Slavono</ref> k.a.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
   <trd lng="bg">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i; &c_je;&c_z;&c_i;&c_k;</trd>
   <trd lng="ca">llengua eslava</trd>
   <trd lng="de">slawische Sprache</trd>
   <trd lng="en">Slavic language</trd>
   <trd lng="es">lengua eslava</trd>
   <trd lng="hu">szl&aacute;v nyelv</trd>   
   <trd lng="nl">Slavische taal</trd>
   <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
</drv>
</art>

