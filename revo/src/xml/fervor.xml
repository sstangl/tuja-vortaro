<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fervor.xml,v 1.25 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fervor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fervor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fervor.0o.PSI">
    <dif>
      Diligenta agemo, ardo por iu a&ubreve; io:
      <ekz>
        religia, politika <tld/>o;
      </ekz>
      <ekz>
        <tld/>o pri Via domo min konsumis
        <fnt><bib>MT</bib> <lok>&Psa; 69:9</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e labori.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="fajr.0o.fervoro">fajro <sncref/></ref>,
      <ref cel="febr.0o.fervoro">febro <sncref/></ref>,
      <ref cel="flam.0o.fervoro">flamo <sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0ego.fervoro">varmego <sncref/></ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="cs">horlivost</trd>
  <trdgrp lng="de">
    <trd>Eifer</trd>,
    <trd>Begeisterung</trd>,
    <trd>Inbrunst</trd>,
    <trd>Leidenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ardour</trd>,
    <trd>fervour</trd>,
    <trd>fervency</trd>,
    <trd>passion</trd>,
    <trd>zeal</trd>
  </trdgrp>
  <trd lng="es">fervor</trd>
  <trdgrp lng="fr">
    <trd>ferveur</trd>,
    <trd>ardeur</trd>,
    <trd>passion</trd>,
    <trd>z&egrave;le</trd>
  </trdgrp>
  <trd lng="hu">buzgalom</trd>
  <trd lng="nl">ijver</trd>
  <trdgrp lng="pl">
    <trd>zapa&lstroke;</trd>,
    <trd>&zdot;arliwo&sacute;&cacute;</trd>,
    <trd>gorliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fervor</trd>,
    <trd>zelo</trd>,
    <trd>ardor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">horlivosť</trd>
</drv>

<drv mrk="fervor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fervor.0i.PSI">
    <dif>
      Esti <tld/>a:
      <ekz>
        tro <tld/>i estas dan&gcirc;ere.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i.FIG">ardi <sncref/></ref>,
      <ref cel="bol.0i.fervori">boli <sncref/></ref>,
      <ref cel="brul.0i.FIG">bruli <sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dávat přednost <klr>(někomu)</klr></trd>,
    <trd>nadržovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eifern</trd>,
    <trd>eifrig sein</trd>,
    <trd>mit <ind>Begeisterung</ind> bei der Sache sein</trd>,
    <trd>mit <ind>Inbrunst</ind> tun</trd>,
    <trd><ind>leidenschaftlich</ind> dabei sein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be <ind>fervent</ind></trd>,
    <trd>be <ind>passionate</ind></trd>,
    <trd>be <ind>zealous</ind></trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>fervent</ind></trd>
  <trd lng="hu">buzg&oacute;lkodik</trd>
  <trd lng="nl">ijveren</trd>
  <trd lng="ru">&c_u;&c_s;&c_je;&c_r;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">dávať prednosť</trd>
</drv>

<drv mrk="fervor.0a">
  <kap><tld/>a</kap>
  <snc mrk="fervor.0a.PSI">
    <dif>
      Varmege, pasie diligenta:
      <ekz>
        <tld/>a esperantisto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>horlivý</trd>,
    <trd>pilný</trd>,
    <trd>s pílí konaný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eifrig</trd>,
    <trd>begeistert</trd>,
    <trd>inbr&uuml;nstig</trd>,
    <trd>leidenschaftlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ardent</trd>,
    <trd>eager</trd>,
    <trd>fervent</trd>,
    <trd>fervid</trd>,
    <trd>impassioned</trd>,
    <trd>zealous</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ardent</trd>,
    <trd>fervent</trd>,
    <trd>passionn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">buzg&oacute;</trd>
  <trd lng="nl">ijverig</trd>
  <trdgrp lng="pl">
    <trd>zapalczywy</trd>,
    <trd>&zdot;arliwy</trd>,
    <trd>zagorza&lstroke;y</trd>,
    <trd>gorliwy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_je;&c_r;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">horlivý</trd>
</drv>

<drv mrk="fervor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fervor.0igi.PSI">
    <dif>
      Fari iun <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_h;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">rozohnit</trd>
  <trdgrp lng="de">
    <trd>anfeuern</trd>,
    <trd>anstacheln</trd>,
    <trd>begeistern</trd>,
    <trd><klr>in </klr>Begeisterung <ind>versetzen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>incite the <ind>fervour</ind> (of)</trd>,
    <trd>galvanize<klr tip="ind"> (excite passion)</klr></trd>,
    <trd>inflame<klr tip="ind"> (incite the fervour of)</klr></trd>,
    <trd>make fervid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>susciter la <ind>ferveur</ind> (de)</trd>,
    <trd>exalter<klr tip="ind"> (passionner)</klr></trd>,
    <trd>galvaniser<klr tip="ind"> (passionner)</klr></trd>,
    <trd>passionner<klr tip="ind"> (susciter la ferveur)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>buzd&iacute;t</trd>,
    <trd>&ouml;szt&ouml;n&ouml;z</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozentuzjamowa&cacute;</trd>,
    <trd>rozpala&cacute;<klr tip="ind"> (np. rozpala&cacute; uczucie)</klr></trd>,
    <trd>zagrzewa&cacute;<klr tip="ind"> (np. zagrzewa&cacute; do walki)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">rozhorliť</trd>
</drv>

<drv mrk="fervor.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="fervor.0ulo.PSI">
    <dif>
      Pasia kaj diligenta aganto por iu afero:
      <ekz>
        ili &ccirc;iuj estas <tld/>uloj por la le&gcirc;o
        <fnt><bib>NT</bib>, <lok>&Ago; 21:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>fanda</trd>,
    <trd>horlivec</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>devotee</trd>,
    <trd>enthusiast</trd>,
    <trd>zealot</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adepte fervent</trd>,
    <trd>passionn&eacute;<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zapaleniec</trd>,
    <trd>entuzjasta</trd>,
    <trd>pasjonat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_ja;&c_ch;&c_i;&c_j; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j; &c_p;&c_o;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">fanúšik</trd>
</drv>

<drv mrk="fervor.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="fervor.mal0a.PSI">
    <dif>
      Malhavanta <tld/>on, senpasia,
      <ref tip="vid" cel="indife.0a.iu">indiferenta</ref>:
      <ekz>
        ili rigardadis indiferente, rigardadis per mal<tld/>aj 
        kaprinokuloj
        <fnt>
          <aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>,
          <vrk>Sep fratoj</vrk>,
          <lok>1947</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">lhostejný</trd>
  <trdgrp lng="en">
    <trd>apathetic</trd>,
    <trd>indifferent</trd>
  </trdgrp>
  <trd lng="fr">apathique</trd>
  <trd lng="sk">ľahostajný</trd>
</drv>
</art>
