<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: inicia.xml,v 1.21 2016/03/16 15:10:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>iniciat</rad>/i
</kap>
<drv mrk="inicia.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti la unua, kiu proponas a&ubreve; faras ion:
      <ekz>
        <tld/>i novan metodon, eksperimentojn, kuracilon;
      </ekz>
      <ekz>
        li maltime <tld/>is en, pri tiu afero;
      </ekz>
      <ekz>
        &gcirc;uste mi <tld/>is la esperantan korespondon
        kun la ru&gcirc;armeanoj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u &scirc;i efektive <tld/>is la tutan konspiradon,
        tion li ne povis konkludi el la leteroj, sed tio ne estis
        elbarita
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komenc.0i">komenci</ref>,
      <ref cel="entrep.0i">entrepreni</ref>,
      <ref cel="debut.0i">debuti</ref>,
      <ref cel="lev.0i">levi</ref>,
      <ref cel="instig.0i">instigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t podnet</trd>,
    <trd>d&aacute;t popud</trd>,
    <trd>iniciovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>initiieren</trd>,
    <trd>einf&uuml;hren</trd>,
    <trd>den <ind>Ansto&szlig;</ind> geben</trd>
  </trdgrp>
  <trd lng="en">initiate</trd>
  <trd lng="es">iniciar</trd>
  <trdgrp lng="fr">
    <trd>initier<klr tip="ind"> (amorcer)</klr></trd>,
    <trd>amorcer<klr tip="ind"> (lancer)</klr></trd>,
    <trd>engager<klr tip="ind"> (lancer)</klr></trd>,
    <trd>entamer<klr tip="ind"> (lancer)</klr></trd>,
    <trd>entreprendre<klr tip="ind"> (lancer)</klr></trd>,
    <trd>lancer<klr tip="ind"> (un projet, etc.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez</trd>
  <trdgrp lng="pt">
   <trd>iniciar<klr tip="amb"> (tomar iniciativa)</klr></trd>, 
   <trd>tomar <ind>iniciativa</ind></trd>,
   <trd>lan&ccedil;ar<klr tip="amb"> (um projeto etc)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_ch;&c_i;&c_n;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_o;&c_r;&c_o;&c_m;</ind><klr>
    (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; popud</trd>,
    <trd>d&aacute;va&tcaron; podnet</trd>,
    <trd>iniciova&tcaron;</trd>,
    <trd>zosnova&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#3619;&#3636;&#3648;&#3619;&#3636;&#3656;&#3617;</trd>
</drv>

<drv mrk="inicia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        lia <tld/>o sukcesis;
      </ekz>
      <ekz>
        preni sur sin la <tld/>on de la afero.
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia1.0o.iniciato">iniciativo <sncref/>.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>iniciativa</trd>,
    <trd>podnet</trd>,
    <trd>popud</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Initiative</trd>,
    <trd>Ansto&szlig;</trd>,
    <trd>Anregung</trd>,
  </trdgrp>
  <trd lng="en">initiative</trd>
  <trd lng="es">iniciativa</trd>
  <trdgrp lng="fr">
    <trd>initiative<klr tip="ind"> (fait de lancer un
    projet)</klr></trd>,
    <trd>entreprise<klr tip="ind"> (initiative)</klr></trd>,
    <trd>lancement<klr tip="ind"> (initiative)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&eacute;s</trd>
  <trd lng="nl">initiatief</trd>
  <trd lng="pt">iniciativa<klr tip="amb">
  (a&ccedil;&atilde;o)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>iniciat&iacute;va</trd>,
    <trd>podnet</trd>,
    <trd>popud</trd>
  </trdgrp>
</drv>

<drv mrk="inicia.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Kapablo de tiu, kiu emas <tld/>i:
      <ekz>
        homo sen <tld/>emo;
      </ekz>
      <ekz>
        <ind><tld/>ema</ind> karaktero.
        <trdgrp lng="es">
          <trd>emprendedor</trd>,
          <trd>con iniciativa</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>entreprenant<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>dynamique<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>audacieux<klr tip="ind"> (plein d'initiative)</klr></trd>
        </trdgrp>
        <trd lng="hu">kezdem&eacute;nyez&odblac;k&eacute;sz</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia1.0o.iniciatemo">iniciativo
    <sncref/>.</ref>
  </snc>
  <trd
  lng="be">&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Unternehmungslust</trd>,
    <trd>Unternehmungsgeist</trd>,
    <trd>Entschlusskraft</trd>
  </trdgrp>
  <trd lng="en">enterprising</trd>
  <trd lng="es">iniciativa</trd>
  <trdgrp lng="fr">
    <trd>initiative<klr tip="ind"> (qualit&eacute;)</klr></trd>,
    <trd>esprit d'<ind>initiative</ind></trd>,
    <trd>esprit d'<ind>entreprise</ind></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&odblac;k&eacute;szs&eacute;g</trd>
  <trd lng="nl">ondernemingsgeest</trd>
  <trdgrp lng="pt">
   <trd>iniciativa<klr tip="amb"> (capacidade)</klr></trd>, 
   <trd>esp&iacute;rito de <ind>iniciativa</ind></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_i;&c_m;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="inicia.0into">
  <kap><tld/>into</kap>
  <snc>
    <dif>
      La unua, kiu faris a&ubreve; aran&gcirc;is aferon:
      <ekz>
        mi tamen asertas, ke Zamenhof fakte <ctl>kreis</ctl> la
        fundamenton
        de nia lingvo <klr>[...]</klr>, sed prave oni anka&ubreve; povas
        argumenti, ke li estis nur &gcirc;ia <tld/>into
        <fnt><bib>Lanti</bib><lok>&Ccirc;u kreo a&ubreve; nur
        iniciato?</lok></fnt>;
      </ekz>
      <ekz>
        unu Lemuel, <klr>[...]</klr> fanatika malamiko de niaj dioj, kiu
        estis
        intimulo de Omar, kaj kun<tld/>into de ribelo kontra&ubreve;
        ili,
        jam pagis per la vivo sian blasfemadon
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia2.0o">iniciatoro.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>inici&aacute;tor</trd>,
    <trd>p&uring;vodce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Initiator</trd>,
    <trd>Ansto&szlig;geber</trd>,
    <trd>Urheber</trd>
  </trdgrp>
  <trd lng="en">initiator</trd>
  <trd lng="es">iniciador</trd>
  <trdgrp lng="fr">
    <trd>initiateur<klr tip="ind"> (promoteur)</klr></trd>,
    <trd>pionnier<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>auteur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>cr&eacute;ateur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>promoteur<klr tip="ind"> (initiateur)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&odblac;</trd>
  <trd lng="pt">iniciador</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_o;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">p&ocirc;vodca</trd>
  <trd lng="th">&#3612;&#3641;&#3657;&#3619;&#3636;&#3648;&#3619;&#3636;&#3656;&#3617;</trd>
</drv>
</art>
