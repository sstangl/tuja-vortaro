<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: talp.xml,v 1.27 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>talp</rad>/o
</kap>
<drv mrk="talp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subtere vivanta <ref tip="lst" cel="mangx.insekto0ulo"
      lst="voko:insektoman&gcirc;uloj">insektoman&gcirc;ulo</ref>
      kun velureca felo; <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="talped.0oj"><tld/>edoj</ref>
      (<trd lng="la">Talpa</trd>):
      <ekz>
        <ref cel="blind.0a">blinda</ref> kiel <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_t;</trd>
  <trd lng="ca">talp</trd>
  <trd lng="cs">krtek</trd>
  <trd lng="de">Maulwurf</trd>
  <trd lng="en">mole</trd>
  <trd lng="fr">taupe</trd>
  <trd lng="hu">vakond</trd>
  <trd lng="id"><ind>tikus</ind> tanah</trd>
  <trd lng="nl">mol</trd>
  <trd lng="pl">kret</trd>
  <trdgrp lng="pt">
   <trd>toupeira</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_t;</trd>
  <trd lng="sk">krt</trd>
  <trd lng="sv">mullvad</trd>
</drv>
</art>
