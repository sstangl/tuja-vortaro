<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brav.xml,v 1.29 2015/10/06 11:10:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>brav</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brav.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="brav.0a.kuragxa">
     <dif>
       Lerta kaj preta por kontra&ubreve;stari dan&gcirc;eron;
       <refgrp tip="sin">
	 <ref cel="kuragx.0a">kura&gcirc;a</ref>,
	 <ref cel="tim.mal0a">maltima</ref>
       </refgrp>, sentima:
       <ekz>
	 nia lando venkos, &ccirc;ar nia militistaro estas granda kaj <tld/>a
	 <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravo</trd>,
       <trd>valeroso</trd>,
       <trd>aguerrido</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>vit&eacute;z</trd>,
       <trd>b&aacute;tor</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>&sacute;mia&lstroke;y</trd>,
       <trd>chwacki</trd>,
       <trd>dziarski</trd>,
       <trd>bitny</trd>,
       <trd>waleczny</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>bravo</trd>,
       <trd>valente</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;&c_j;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
   <snc mrk="brav.0a.honesta">
     <dif>
       Preta por plenumi sian devon,
       <refgrp tip="sin">
	 <ref cel="honest.0a.honorinda">honesta</ref>,
	 <ref cel="lojal.0a">lojala</ref>
       </refgrp>:
       <ekz>
	 <tld/>a virino estas krono por sia edzo,
	 sed senhonora estas kiel puso en liaj ostoj
	 <fnt><bib>MT</bib>, <lok>&Sen; 12:4.</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_v;&c_a;&c_r;&c_t;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>
     </trdgrp>
     <trd lng="es">devoto</trd>
     <trdgrp lng="hu">
       <trd>der&eacute;k</trd>,
       <trd>j&oacute;raval&oacute;</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>dobry</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr
       tip="amb"> (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
   <snc mrk="brav.0a.bonkora">
     <dif>
       Preta por fari komplezon al aliaj; bonkora.
     </dif>
     <rim>
       Mi tre dubas ke <tld/>a posedas tiun sencon en Esperanto.
       La PIV-oj &gcirc;in ne transprenis.
       <aut>Sergio Pokrovskij</aut>
     </rim>
     <trd lng="de">g&uuml;tig</trd>
     <trd lng="hu">rendes</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>chrabr&yacute;</trd>,
    <trd>state&ccaron;n&yacute;</trd>,
    <trd>udatn&yacute;</trd>,
    <trd>&rcaron;&aacute;dn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>worthy</trd>,
     <trd>valiant</trd>,
     <trd>gallant</trd>,
     <trd>brave</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>brave</trd>,
     <trd>courageux</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>chrabr&yacute;</trd>,
    <trd>ochotn&yacute;</trd>,
    <trd>odv&aacute;&zcaron;ny</trd>,
    <trd>stato&ccaron;n&yacute;</trd>,
    <trd>udatn&yacute;</trd>,
    <trd>&ccaron;estn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brav.0e">
   <kap><tld/>e</kap>
   <snc mrk="brav.0e.maniere">
     <dif>
       <tld lit="B"/>amaniere.
     </dif>
     <trdgrp lng="be">
       <trd>&c_b;&c_r;&c_a;&c_v;&c_a;</trd>,
       <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_a;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>,
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>,
       <trd>g&uuml;tig</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravamente</trd>,
       <trd>valerosamente</trd>,
       <trd>aguerridamente</trd>
     </trdgrp>
     <trd lng="fr">bravement</trd>
     <trdgrp lng="hu">
       <trd>vit&eacute;z&uuml;l</trd>,
       <trd>derekasan</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielnie</trd>,
       <trd>m&eogonek;&zdot;nie</trd>,
       <trd>odwa&zdot;nie</trd>,
       <trd>&sacute;mia&lstroke;o</trd>,
       <trd>chwacko</trd>,
       <trd>po <ind>chwacku</ind></trd>,
       <trd>dziarsko</trd>,
       <trd>bitnie</trd>,
       <trd>walecznie</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_s;&c_m;&c_je;&c_l;&c_o;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>
     </trdgrp>
     <trd lng="sk">odv&aacute;&zcaron;ne</trd>
   </snc>
   <snc mrk="brav.0e.bone">
     <dif>
       Bone! <klr>(por aprobi kaj apla&ubreve;di)</klr>.
     </dif>
     <trd lng="be">&c_b;&c_r;&c_a;&c_v;&c_a;!</trd>
     <trd lng="de">bravo</trd>
     <trd lng="en">bravo!</trd>
     <trd lng="es">bravo <klr>(expresi&oacute;n de aplauso)</klr></trd>
     <trd lng="fr">bravo!</trd>
     <trd lng="hu">brav&oacute;!</trd>
     <trd lng="pl">brawo!</trd>
     <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_o;!</trd>
   </snc>
  <trd lng="cs">state&ccaron;n&ecaron;</trd>
  <trd lng="sk">stato&ccaron;ne</trd>
</drv>

<drv mrk="brav.0acxa">
   <kap><tld/>a&ccirc;a</kap>
   <snc mrk="brav.0acxa.KOMUNE">
     <dif>
       Elmontranta troan, simulatan <tld/>on:
       <ekz>
	 ni ne posedas ilian <tld/>a&ccirc;an memfidon
	 <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 10a, p.
	 57a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="fanfar1.0i">fanfaroni</ref>,
       <ref cel="spit.0ema">spitema</ref>,
       <ref cel="sxajn.0igi">&scirc;ajnigi</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_z;&c_u;&c_h;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
     <trd>&c_r;&c_y;&c_z;&c_y;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>,
     <trd>&c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="en">impudent</trd>
   <trdgrp lng="fr">
     <trd>bravache</trd>,
     <trd>fanfaron</trd>,
     <trd>pr&eacute;somptueux</trd>
   </trdgrp>
   <trd lng="pl">brawurowy</trd>
   <trdgrp lng="ru">
     <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_d;&c_je;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="brav.0acxo">
   <kap><tld/>a&ccirc;o</kap>
   <snc mrk="brav.0acxo.KOMUNE">
     <dif>
       Flegata aspekto de <tld/>o, de spita memfido.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_r;&c_y;&c_z;&c_y;&c_k;&c_a;&c_n;&c_c;&c_t;&c_v;&c_a;</trd>,
     <trd>&c_z;&c_u;&c_h;&c_a;&c_v;&c_a;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="en">impudence</trd>
   <trdgrp lng="fr">
     <trd>cr&acirc;nerie</trd>,
     <trd>pr&eacute;somption</trd>
   </trdgrp>
   <trd lng="pl">brawura</trd>
   <trdgrp lng="ru">
     <trd>&c_f;&c_a;&c_n;&c_f;&c_a;&c_r;&c_o;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
     <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
     <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_d;&c_je;&c_ja;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="brav.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="brav.0eco.KOMUNE">
    <dif>
      Senheziteco anta&ubreve; dan&gcirc;eroj a&ubreve; malfacilaj taskoj:
      <ekz>
        per via <tld/>eco vi gajnis altan rangon inter ni
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">state&ccaron;nost</trd>
  <trd lng="fr">bravoure</trd>
  <trd lng="sk">stato&ccaron;nos&tcaron;</trd>
</drv>

<drv mrk="brav.0ulo">
   <kap><tld/>ulo</kap>
   <snc mrk="brav.0ulo.KOMUNE">
     <dif>
       Viro <tld/>a:
       <ekz>
	 <uzo tip="stl">FRAZ</uzo>
	 <ind>kontra&ubreve; mu&scirc;oj <tld/>ulo, kontra&ubreve;
	  homoj timulo</ind>
	 <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_d;&c_je;&c_c; &c_s;&c_r;&c_je;&c_d;&c_i; &c_o;&c_v;&c_je;&c_c;</trd>
	 <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_e;&c_c;</trd>,
     <trd>&c_m;&c_a;&c_l;&c_a;&c_d;&c_z;&c_je;&c_c;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>chlap&iacute;k</trd>,
    <trd>frajer</trd>,
    <trd>hrdina</trd>,
    <trd>kabr&ncaron;&aacute;k</trd>,
    <trd>sek&aacute;&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Held</trd>,
     <trd>tapferer <ind>Kerl</ind></trd>,
     <trd>wackerer <ind>Mann</ind></trd>,
     <trd>braver <ind>Mann</ind></trd>,
     <trd>treue <ind>Seele</ind></trd>,
     <trd>fideler <ind>Kamerad</ind></trd>,
     <trd>t&uuml;chtiger <ind>Bursche</ind></trd>,
     <trd>g&uuml;tiger <ind>Mensch</ind></trd>
   </trdgrp>
  <trd lng="es">valiente <klr>(persona de sexo masculino)</klr></trd>
  <trd lng="fr">brave</trd>
  <trdgrp lng="pl">
     <trd>chwat</trd>,
     <trd>zuch</trd>,
     <trd>junak<klr> (przest.)</klr></trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_je;&c_c;</trd>,
     <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_je;&c_c;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>sek&aacute;&ccaron;</trd>,
    <trd>&scaron;vih&aacute;k</trd>
  </trdgrp>
</drv>

<drv mrk="brav.0ulino">
   <kap><tld/>ulino</kap>
   <snc mrk="brav.0ulino.KOMUNE">
     <dif>
       Ino <tld/>a:
       <ekz>
	 estas sciate en la tuta urbo, ke vi estas <tld/>ulino
	 <fnt><bib>MT</bib>, <lok>&Rut; 3:11</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Heldin</trd>,
     <trd>tapferes <ind>Kind</ind></trd>,
     <trd>tapfere <ind>Streiterin</ind></trd>,
     <trd>wackeres <ind>Weib</ind></trd>,
     <trd>braves <ind>Weib</ind></trd>,
     <trd>treue <ind>Seele</ind></trd>,
     <trd>t&uuml;chtiges <ind>M&auml;dchen</ind></trd>,
     <trd>g&uuml;tige <ind>Frau</ind></trd>
   </trdgrp>
   <trd lng="es">valiente <klr>(persona de sexo femenino)</klr></trd>
   <trd lng="fr">brave</trd>
   <trd lng="pl">zuch <klr>dziewczyna</klr></trd>
   <trdgrp lng="ru">
     <trd>&c_s;&c_l;&c_a;&c_v;&c_n;&c_a;&c_ja;
     &c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>,
     <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_je;&c_c;<klr tip="amb"> (&c_o;
     &c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_je;)</klr></trd>
   </trdgrp>
</drv>
</art>
