<kap>
  <ofc>8</ofc>
  <rad>eskap</rad>/i
</kap>
<drv mrk="eskap.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
    Sukcese eviti per forkuro: <ekz><tld/>i ion, de io, el io.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_mol;&c_b;&c_je;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;
    <klr>(&c_b;&c_je;&c_g;&c_s;&c_t;&c_v;&c_a;&c_m;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_b;&c_ja;&c_g;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>uniknout</trd>,
    <trd>utéci</trd>,
    <trd>vyváznout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entkommen</trd>,
    <trd>entrinnen</trd>,
    <trd>entwischen</trd>,
    <trd>entfliehen</trd></trdgrp>
  <trd lng="en">escape</trd>
  <trd lng="es">escapar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;chapper (&agrave;)</trd>,
    <trd>&eacute;viter<klr tip="ind"> (&eacute;chapper
    &agrave;)</klr></trd></trdgrp>
  <trdgrp lng="pl">
    <trd>uciec</trd>,
    <trd>zbiec</trd>,
    <trd>zwia&cacute;<klr> (pot.)</klr></trd>,
    <trd>umkn&aogonek;&cacute;</trd>,
    <trd>wymkn&aogonek;&cacute; si&eogonek;</trd>,
    <trd>wy&sacute;lizgn&aogonek;&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>escapar <klr tip="amb">(de)</klr></trd>,
    <trd>fugir <klr tip="amb">(de)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_b;&c_je;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_s;&c_t;&c_i;&c_s;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uniknúť</trd>,
    <trd>utiecť</trd>
  </trdgrp>
</drv>
</art>
