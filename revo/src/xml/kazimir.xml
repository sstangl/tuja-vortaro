<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kazimir.xml,v 1.9 2015/06/22 14:10:17 revo Exp $">
<kap>
  <rad>Kazimir</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kazimir.0o">
  <kap><tld/>o</kap>
  <snc mrk="kazimir.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> slavdevena:
      <ekz>
        la lar&gcirc;aj kaj luksaj konstrua&jcirc;oj de la <tld/>a palaco
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <ctl>Sankta <tld/>o (1458-1484)</ctl>
        <fnt>
          <aut>P. Gavenas, trad. C. Lemos, trad. L. Knoedt</aut>,
          <vrk>Sankta Kazimiro (1458-1484)</vrk>,
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_K;&c_a;&c_z;&c_i;&c_m;&c_i;&c_r;</trd>
  <trd lng="ca">Casimir</trd>
  <trd lng="de">Kasimir</trd>
  <trd lng="fr">Casimir</trd>
  <trd lng="it">Casimiro</trd>
  <trd lng="nl">Kasimir</trd>
  <trd lng="pl">Kazimierz</trd>
  <trd lng="ru">&c_K;&c_a;&c_z;&c_i;&c_m;&c_i;&c_r;</trd>
</drv>
</art>

