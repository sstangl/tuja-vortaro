<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jut.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>jut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="jut.0o">
  <kap><tld/>o</kap>
  <snc mrk="jut.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Hinda teksebla planto;
      genro el familio liliacoj.
    </dif>
    <trd lng="de">Jute</trd>
    <trdgrp lng="it">
      <trd>corcoro</trd>, 
      <trd>iuta</trd>, 
      <trd>juta</trd>
    </trdgrp>
    <trd lng="la">Corchorus</trd>
    <trd lng="pl">juta</trd>
  </snc>
  <snc mrk="jut.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksmaterialo el prilaboro de
      <ref tip="vid" cel="jut.0o.BOT"><tld/>o</ref>:
      <ekz>
        hindoj teksas siajn vestojn
        el <tld/>o.
      </ekz>
    </dif>
    <trd lng="pl">juta</trd>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_u;&c_t;</trd>
  <trd lng="bg">&c_ju;&c_t;&c_a;</trd>
  <trd lng="cs">juta</trd>
  <trdgrp lng="de">
    <trd>Jute</trd>,
    <trd>Jutefaser</trd>,
    <trd>Bastfaser</trd>,
    <trd>Bastgewebe</trd>
  </trdgrp>
  <trd lng="el">&gamma;&jota;&omikron;&ypsilon_ton;&tau;&alfa;</trd>
  <trd lng="en">jute</trd>
  <trd lng="fr">jute</trd>
  <trd lng="hu">juta</trd>
  <trdgrp lng="it">
    <trd>iuta</trd>, 
    <trd>juta</trd>
    </trdgrp>
  <trd lng="nl">jute</trd>
  <trd lng="pt">juta</trd>
  <trd lng="ru">&c_d;&c_zh;&c_u;&c_t;</trd>
  <trd lng="sk">juta</trd>
  <trd lng="sv">jute</trd>
</drv>
</art>
