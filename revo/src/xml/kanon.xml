<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanon.xml,v 1.30 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>kanon</rad>/o
</kap>
<drv mrk="kanon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanon.0o.legxo">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Grava eklezia regulo, koncernanta la kredon, kulton kaj
      disciplinon.
    </dif>
    <trd lng="it"><ind>regola</ind> canonica</trd>
  </snc>
  <snc mrk="kanon.0o.listo">
    <uzo tip="fak">REL</uzo>
    <dif>
      Norma listo:
    </dif>
    <subsnc mrk="kanon.0o.BIB">
      <uzo tip="fak">BIB</uzo>
      <dif>
        Norma listo da libroj kiujn koncerna konfesio rigardas
        kiel sanktajn:
        <ekz>
          <ind>malnovtestamenta <tld/>o</ind>,
          <trd lng="hu">&oacute;testamentumi <ind>k&aacute;non</ind>
          </trd>
          <trd lng="nl">canon van het Oude Testament</trd>
          <trd lng="ru">&c_v;&c_je;&c_t;&c_h;&c_o;&c_z;&c_a;&c_v;&c_je;&c_t;&c_n;&c_y;&c_j; &c_k;&c_a;&c_n;&c_o;&c_n;</trd>
        </ekz>
        <ekz>
          <ind>novtestamenta <tld/>o</ind>,
          <trd lng="hu">&uacute;jtestamentumi <ind>k&aacute;non</ind>
          </trd>
          <trd lng="nl">canon van het Nieuwe Testament</trd>
          <trd lng="ru">&c_n;&c_o;&c_v;&c_o;&c_z;&c_a;&c_v;&c_je;&c_t;&c_n;&c_y;&c_j; &c_k;&c_a;&c_n;&c_o;&c_n;</trd>
        </ekz>
        <ekz>
          <ind>masora <tld/>o</ind>.
          <trd lng="hu">masz&oacute;rikus <ind>k&aacute;non</ind></trd>
          <trd lng="ru">&c_m;&c_a;&c_s;&c_o;&c_r;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_k;&c_a;&c_n;&c_o;&c_n;</trd>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kanon.0o.sanktularo">
      <uzo tip="fak">KRI</uzo>
      <dif>
        Eklezia katalogo de la sanktuloj.
      </dif>
      <trd lng="ru">&c_l;&c_i;&c_k; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;</trd>
    </subsnc>
  </snc>
  <ref tip="hom" cel="kanon1.0o">kanono</ref>
  <trd lng="be">&c_k;&c_a;&c_n;&c_o;&c_n;</trd>
  <trd lng="ca">c&agrave;non</trd>
  <trd lng="en">canon</trd>
  <trd lng="es">canon</trd>
  <trd lng="fr">canon<klr tip="ind"> (religion)</klr></trd>
  <trd lng="hu">k&aacute;non</trd>
  <trd lng="it">canone</trd>
  <trd lng="nl">canon</trd>
  <trd lng="pl">kanon</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>delo</trd>,
    <trd>kanón</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dělo</trd>,
    <trd>kanón</trd>
  </trdgrp>
</drv>

<drv mrk="kanon.0a">
  <kap><tld/>a</kap>
  <snc mrk="kanon.0a.JUR">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="kanon.0o.legxo"/>,
      konforma al &gcirc;i:
      <ekz>
        la <ind>kanona juro</ind>,
        <trd lng="ca">dret can&ograve;nic</trd>
        <trd lng="en">canon law</trd>
        <trd lng="hu">k&aacute;nonjog</trd>
        <trd lng="nl">canoniek recht</trd>
        <trd lng="ru">
        &c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
        &c_p;&c_r;&c_a;&c_v;&c_o;
        </trd>
      </ekz>
      <ekz>
        la <ind>kanonaj horoj</ind>.
        <trd lng="ca">hores can&ograve;niques</trd>
        <trd lng="hu">k&aacute;noni &oacute;r&aacute;k</trd>
        <trd lng="en">canonical hours</trd>
        <trdgrp lng="nl">
          <trd>canonieke uren</trd>,
          <trd>daggetijden</trd>
        </trdgrp>
        <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_v;&c_n;&c_y;&c_je; &c_ch;&c_a;&c_s;&c_y; &c_m;&c_o;&c_l;&c_i;&c_t;&c_v;</trd>
      </ekz>
    </dif>
    <trd lng="nl">canoniek</trd>
  </snc>
  <snc mrk="kanon.0a.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="kanon.0o.BIB"/>:
      <ekz>
        la kvar kanonaj evangelioj.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&aacute;noni</trd>,
      <trd>kanonikus</trd>
    </trdgrp>
    <trd lng="nl">canoniek</trd>
  </snc>
  <snc mrk="kanon.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      Plej natura.
    </dif>
    <refgrp tip="vid">
      <ref cel="baz.kanona0o"><tld/>a bazo</ref>,
      <ref cel="projek.0o.kanona"><tld/>a projekcio</ref>.
    </refgrp>
    <rim>
      La vorto ne havas precizan sencon. &Gcirc;i nur servas por
      kvalifiki iujn objektojn, kiujn oni povas ial konsideri plej
      <ctl>naturaj</ctl>,
      <ctl>evidentaj</ctl>, <ctl>regulaj</ctl>, ktp.
    </rim>
    <trd lng="en">canonical</trd>
    <trd lng="de">kanonisch</trd>
    <trd lng="hu">kanonikus</trd>
    <trd lng="nl">natuurlijk<klr tip="ind"> (wisk.)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">can&ograve;nic</trd>
  <trd lng="es">can&oacute;nico</trd>
  <trd lng="fr">canonique</trd>
  <trd lng="it">canonico</trd>
  <trd lng="pl">kanoniczny</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kanon.en0igi">
  <kap>en<tld/>igi</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Solene enskribi iun en la <tld/>on de la sanktuloj
      a&ubreve; beatuloj.
    </dif>
  </snc>
  <trd lng="be">
  &c_k;&c_a;&c_n;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">canonitzar</trd>
  <trd lng="cs">kánonizovat</trd>
  <trd lng="en">canonize</trd>
  <trd lng="es">canonizar</trd>
  <trd lng="fr">canoniser</trd>
  <trdgrp lng="hu">
    <trd>felvesz a szentek sor&aacute;ba</trd>,
    <trd>kanoniz&aacute;l</trd>
  </trdgrp>
  <trd lng="it">canonizzare</trd>
  <trdgrp lng="nl">
    <trd>canoniseren</trd>,
    <trd>heilig verklaren</trd>,
    <trd>zalig verklaren</trd>
  </trdgrp>
  <trd lng="pl">kanonizowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>
    &c_p;&c_r;&c_i;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_mol; &c_k;
    &c_l;&c_i;&c_k;&c_u; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;
    </trd>,
    <trd>&c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kanonizovať</trd>
</drv>

<drv mrk="kanon.dua0a">
  <kap>dua<tld/>a</kap>
  <uzo tip="fak">BIB</uzo>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Apartenanta al iu el la Bibliaj libroj (a&ubreve; ties partoj)
      kies a&ubreve;tento estas kontestata kaj kiujn la eklezio
      rigardas malpli a&ubreve;toritataj.
    </dif>
    <rim num="1">
      La termino estas romkatolika. &Gcirc;i &scirc;ajnas implici
      ekziston de iu &leftquot;dua <tld/>o&rightquot;, pri kio fakte ne
      temas.
    </rim>
    <rim num="2">
      Simila nocio ekzistas en ortodoksismo; tamen tie temas pri
      <ctl>libroj bibliaj</ctl>, el kiuj unuj estas <tld/>aj,
      kaj iuj estas &leftquot;bibliaj ekster<tld/>aj&rightquot; (la
      eklezio anka&ubreve; ilin rigardas instruaj kaj rekomendindaj,
      presigas ilin en siaj biblioj, sed ne plene agnoskas ilin
      <ctl>Die inspiritaj</ctl>). La ortodoksa Biblio entenas je 4
      libroj pli da ili ol la romkatolika Vulgato.
    </rim>
    <rim num="3">
      Protestantoj, polemike, rifuzas distingi tiajn librojn
      disde la apokrifoj.
    </rim>
    <rim num="3">
      Precipe temas pri la malnovtestamentaj libroj kiuj ne
      eniris la masoran <tld/>on; tamen anka&ubreve; iujn librojn
      novtestamentajn iuj (i.a. la romkatolikoj) rigardas
      dua<tld/>aj: Hebreoj, Jakobo, Judas, 2Petro, 1Johano,
      2Johano, Apokalipso.
    </rim>
  </snc>
  <trd lng="ca">deuterocan&ograve;nic</trd>
  <trd lng="en">deuterocanonical</trd>
  <trd lng="fr">deut&eacute;rocanonique</trd>
  <trd lng="hu">deuterokanonikus</trd>
  <trd lng="id">deuterokanonika</trd>
  <trd lng="it">deuterocanonico</trd>
  <trd lng="nl">deuterocanoniek</trd>
  <trdgrp lng="pl">
    <trd>apokryficzny</trd>,
    <trd>psudoepigraficzny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_t;&c_o;&c_r;&c_o;&c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_a;&c_n;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
