  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Interreta universala ser&ccirc;ilo.
      </dif>
  </snc>
  <trd lng="en">Google</trd>
  <trd lng="fr">Google</trd>
  <trd lng="pl">Google</trd>
</drv>


</art>

