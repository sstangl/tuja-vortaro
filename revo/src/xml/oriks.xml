<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oriks.xml,v 1.9 2013/09/01 13:11:03 revo Exp $">
<kap>
  <rad>oriks</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="oriks.0o">
  <kap><tld/>o</kap>
  <snc mrk="oriks.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="super" 
      cel="antilo.0o.subfamiliano">antilopo</ref> kun longaj kurbaj 
      <ref tip="prt" cel="korn.0o.besta">kornoj</ref>, nigra maska
      desegno sur la viza&gcirc;o, el Afriko a&ubreve; Arabio,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="boved.0oj.ZOO">bovedoj</ref> 
      (<trd lng="la">Oryx</trd>):
      <ekz>
        &ccirc;iam nomadas ekzemple balenoj, &ccirc;impanzoj, rangiferoj, <tld/>oj
        <fnt>
          <aut>J. &Scirc;uvajev</aut>,
          <vrk><url
          ref="http://www.fw.hu/eventoj/steb/zoologio/konduto_de_bestoj/f874-p30.htm">
          Pri la konduto de bestoj</url></vrk>,
          <lok>Fokuso, 1987:1, p. 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <tld/>oj montris siajn longajn kornojn kaj malaperis 
        post &ccirc;eno de arbustoj
        <fnt><aut>I. Efremov, trad. J. Finkel</aut>, <vrk><url 
        ref="http://www.esperanto.mv.ru/Efremov/arko.html">La Granda 
        Arko</url></vrk>, <lok>2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Oryxantilope</trd>
  <trd lng="es">oryx</trd> 
  <trd lng="fr">oryx</trd>
  <trd lng="it">orice</trd>
  <trd lng="ca">&ograve;rix</trd>
  <trd lng="nl">sabelantiloop</trd>
  <trdgrp lng="pl">
    <trd>oryx</trd>,
    <trd>oryks</trd>
  </trdgrp>
  <trd lng="oc">orix</trd>
  <trdgrp lng="pt">
    <trd>&oacute;rix</trd>,
    <trd>guelengue</trd>
  </trdgrp>
</drv>
</art>

