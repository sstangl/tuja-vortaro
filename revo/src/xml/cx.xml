<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cx.xml,v 1.7 2015/10/11 15:10:15 revo Exp $">
<kap>
    <ofc>*</ofc><rad>&ccirc;</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="cx.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a <ref
      tip="dif" cel="afrika.0o">afrikata</ref> <ref tip="dif"
      cel="gingiv.0o">gingivalo</ref> [t&#865;&#643;].
    </dif>
  <trd lng="fr">&ccirc;<klr> (consonne)</klr></trd>
  <trd lng="nl">&ccirc;<klr> (medeklinker)</klr></trd>
  <trd lng="no">&ccirc;<klr> (konsonant)</klr></trd>
  <trd lng="pt">&ccirc;<klr> (consoante)</klr></trd>
  </snc>
  <snc>
     <dif>
      La kvara <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto.
     </dif>
     <trd lng="fr">&ccirc;<klr> (lettre)</klr></trd>
     <trd lng="nl">&ccirc;<klr> (letter)</klr></trd>
     <trd lng="no">&ccirc;<klr> (bokstav)</klr></trd>
     <trd lng="pt">&ccirc;<klr> (letra)</klr></trd>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="cx.0.nomo">
  <kap><tld/>o</kap>
  <snc> 
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
    <trd lng="fr">tch</trd>
    <trd lng="nl">tsj</trd>
    <trd lng="no">tsj</trd>
   </snc>
  </drv>
</subart>
</art>

