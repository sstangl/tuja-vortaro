  <trd lng="hu">f&uuml;z&eacute;nyf&eacute;l&eacute;k</trd>
  <trd lng="nl">kattenstaartfamilie</trd>
  <trd lng="pt">litr&aacute;ceas</trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
