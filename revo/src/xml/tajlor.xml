<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tajlor.xml,v 1.19 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tajlor</rad>/o
</kap>
<drv mrk="tajlor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Homo, kies metio estas fari vestojn:
      <ekz>
        &ccirc;iu <tld/>o havas sian tran&ccirc;manieron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="ca">sastre</trd>
  <trdgrp lng="cs">
    <trd>krejčí</trd>,
    <trd>tailleur</trd>,
    <trd>tailor</trd>
  </trdgrp>
  <trd lng="de">Schneider</trd>
  <trd lng="en">tailor</trd>
  <trd lng="es">sastre</trd>
  <trd lng="fa">&a_kh;&f_y;&a_A;&a_T;</trd>
  <trdgrp lng="fr">
    <trd>styliste</trd>,
    <trd>tailleur</trd>
  </trdgrp>
  <trd lng="hu">szab&oacute;</trd>
  <trd lng="nl">kleermaker</trd>
  <trd lng="pl">krawiec</trd>
  <trdgrp lng="pt">
   <trd>costureiro</trd>,
   <trd>alfaiate</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">krajčír</trd>
  <trd lng="sv">skr&auml;ddare</trd>
</drv>
</art>
