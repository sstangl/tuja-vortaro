<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katiz.xml,v 1.16 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>katiz</rad>/i
</kap>
<drv mrk="katiz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katiz.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Doni apreturan gluon, brilecon al &scirc;tofo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">aprestar</trd>
  <trdgrp lng="cs">
    <trd>apretovat</trd>,
    <trd>zušlechťovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>glanzpressen</trd>,
    <trd>pressen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>size</trd>,
    <trd>glaze</trd>
  </trdgrp>
  <trd lng="es">aprestar</trd>
  <trd lng="fr">catir</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;nyez</trd>,
    <trd>csinoz</trd>,
    <trd>kat&iacute;roz</trd>,
    <trd>kat&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>glanzen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apretovať</trd>,
    <trd>zušľachťovať</trd>
  </trdgrp>
</drv>

<drv mrk="katiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="katiz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago:
      <ekz>
        <tld/>on oni prifaras per hidra&ubreve;lika ma&scirc;ino.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">aprest</trd>
  <trd lng="cs">apretura</trd>
  <trd lng="de">Pressglanz</trd>
  <trd lng="es">apresto</trd>
  <trdgrp lng="fr">
    <trd>cati</trd>,
    <trd>catissage</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;nyez&eacute;s</trd>,
    <trd>csinoz&aacute;s</trd>,
    <trd>kat&iacute;roz&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">glans</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_s;&c_k;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">apretúra</trd>
</drv>

<drv mrk="katiz.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="katiz.mal0i">mal<tld/>i</ref>
  </snc>
  <trd lng="nl">ontglanzen<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="sk">zbaviť apretúry</trd>
</drv>

<drv mrk="katiz.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katiz.mal0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Forpreni, forigi la apreturan gluon, brilecon de &scirc;tofo.
    </dif>
  </snc>
  <trd lng="ca">desllustrar <klr>(un teixit)</klr></trd>
  <trd lng="cs">odstranit apreturu</trd>
  <trd lng="es">quitar el <ind>apresto</ind> <klr>(a un
  tejido)</klr></trd>
  <trd lng="fr">d&eacute;catir<klr tip="ind"> (&ocirc;ter le cati)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&eacute;nytelen&iacute;t</trd>,
    <trd>dekat&iacute;roz</trd>,
    <trd>dekat&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">ontglanzen</trd>
  <trd lng="sk">odstrániť apretúru</trd>
</drv>
</art>
