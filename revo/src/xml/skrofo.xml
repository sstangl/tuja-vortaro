<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skrofo.xml,v 1.15 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skrofol</rad>/o
</kap>
<drv mrk="skrofo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skrofo.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Formo de <ref tip="lst" cel="tuberk.0o"
      lst="voko:malsanoj">tuberkulozo</ref> karakterizata per &scirc;velo de
      limfaj glandoj
kaj
      inflamo de okuloj kaj artikoj:
      <ekz>
        estis du guvernistoj kun siaj edukatoj, venintaj sin kuraci de
        <tld/>o
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_r;&c_o;&c_f;&c_u;&c_l;</trd>
  <trd lng="cs">krtice</trd>
  <trd lng="de">Skrofel</trd>
  <trd lng="fr">scrofule</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;rv&eacute;ly</trd>,
    <trd>scorfula</trd>
  </trdgrp>
  <trd lng="nl">scrofulose</trd>
  <trdgrp lng="pl">
    <trd>skrofu&lstroke;a</trd>,
    <trd>zo&lstroke;za</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_f;&c_u;&c_l;&c_jo;&c_z;</trd>,
    <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">skroful√≥za</trd>
</drv>
</art>
