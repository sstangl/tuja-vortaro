  <trdgrp lng="en">
    <trd>domestic interior scene</trd>,
    <trd>interior</trd>, <trd>genre art of interior</trd>
  </trdgrp>
  <trd lng="fr">sc&egrave;ne d'int&eacute;rieur</trd>
  <trd lng="sk">interi&eacute;r</trd>
</drv>
