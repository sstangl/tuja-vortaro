<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxere.xml,v 1.8 2009/08/23 16:31:24 revo Exp $">
<kap>
  <rad>&scirc;ere</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sxere.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxere.0o.VIN">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="vin.0o.KUI">Vino</ref> kun aldona
      alkoholo el suda Hispanio:
      <ekz>
        &scirc;i uzis la &scirc;ancon <klr>...</klr> por indulgi sin per 
        plia glaso da <tld/>o
        <fnt>
        <aut>-</aut>,
        <vrk>Fra&ubreve;lino Emily Barlaston (Ordeno de la Brita Imperio)
        </vrk>,
        <lok><url
        ref="http://www.homestead.com/barlaston/recenzoj.html">
        recenzoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_sh;&c_je;&c_r;&c_i;</trd>
  <trd lng="de">Sherry</trd>
  <trd lng="en">sherry</trd>
  <trd lng="fa">&a_sh;&a_kasra;&a_r;&f_y;</trd>
  <trd lng="fr">x&eacute;r&egrave;s</trd>
  <trd lng="it">sherry</trd>
  <trdgrp lng="nl">
    <trd>sherry</trd>,
    <trd>xeres</trd>
  </trdgrp>
  <trd lng="pt">xerez</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_r;&c_r;&c_i;</trd>,
    <trd>&c_h;&c_je;&c_r;&c_je;&c_s;</trd>
  </trdgrp>
</drv>
</art>

