<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: istm.xml,v 1.19 2015/07/04 08:37:36 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>istm</rad>/o
    </kap>
    <drv mrk="istm.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <ref tip="dif" cel="kol.ter0o">terkolo</ref>
      <ekz><tld/>o de Panama, de Suez.</ekz>
      <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_sh;&c_y;&c_je;&c_k;</trd>
      <trd lng="ca">istme</trd>
      <trdgrp lng="de">
        <trd>Isthmus</trd>,
        <trd>Landenge</trd>
      </trdgrp>
      <trd lng="en">isthmus</trd>
      <trd lng="es">istmo</trd>
      <trd lng="fr">isthme</trd>
      <trdgrp lng="hu">
        <trd>f&ouml;ldszoros</trd>,
        <trd>isztmosz</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>isthmus</trd>,
        <trd>landengte</trd>
      </trdgrp>
      <trd lng="pt">istmo<klr tip="amb"> (geografia)</klr></trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_sh;&c_je;&c_je;&c_k;</trd>
      <trd lng="sk">zemská úžina</trd>
  <trdgrp lng="cs">
    <trd>istmus</trd>,
    <trd>pevninská šíje</trd>
  </trdgrp>
</drv>
  </art>
