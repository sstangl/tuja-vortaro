<vortaro>
<art mrk="$Id: gamb.xml,v 1.23 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>gamb</rad>/o
</kap>
<drv mrk="gamb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Tuto de la suba membro de homo.
    </dif>
    <refgrp tip="vid">
      <ref cel="krur.0o.gambo">kruro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_g;&c_a; <klr tip="amb">(&c_u;&c_s;&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_k;</trd>
  <trd lng="ca">cama</trd>
  <trdgrp lng="cs">
    <trd>dolní končetina</trd>,
    <trd>noha</trd>
  </trdgrp>
  <trd lng="de">Bein</trd>
  <trd lng="en">leg</trd>
  <trd lng="es">pierna<klr> (todo el miembro inferior)</klr></trd>
  <trd lng="fr">jambe</trd>
  <trd lng="hu">l&aacute;b<klr tip="ind"> (als&oacute; v&eacute;gtag)</klr></trd>
  <trd lng="it">gamba<klr> (arto inferiore)</klr></trd>
  <trd lng="nl">been</trd>
  <trd lng="no">ben</trd>
  <trd lng="pl">noga</trd>
  <trd lng="pt">perna</trd>
  <trd lng="ru">&c_n;&c_o;&c_g;&c_a; <klr>(&c_v;&c_s;&c_ja;)</klr></trd>
  <trdgrp lng="sk">
    <trd>dolná končatina</trd>,
    <trd>noha</trd>
  </trdgrp>
  <trd lng="sv">ben</trd>
  <trd lng="tr">bacak</trd>
  <trd lng="vo">l&ouml;g</trd>
</drv>
</art>
