<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: butler.xml,v 1.10 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>butler</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="butler.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="butler.0o.KUI">
    <ref tip="dif" cel="tabl.0estro.KUI">tablestro</ref>
    <ekz>
      alia viro, funkcianta kiel <tld/>o, metis pladon da fruktoj
      anta&ubreve; la gubernatoro
      <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 47a, p.
      651a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="cs">vrchní stolník</trd>
  <trdgrp lng="de">
    <trd>Butler</trd>,
    <trd>Diener</trd>,
    <trd>Haushofmeister</trd>
  </trdgrp>
  <trd lng="en">butler</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>,
    <trd>&a_kh;&a_w;&a_A;&a_n;&a_s;&a_A;&a_l;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ma&icirc;tre d'h&ocirc;tel</trd>,
    <trd>majordome<klr> (ma&icirc;tre d'h&ocirc;tel)</klr></trd>
  </trdgrp>
  <trd lng="it">maggiordomo</trd>
  <trd lng="nl">butler</trd>
  <trd lng="sk">vrchný stolník</trd>
</drv>
</art>

