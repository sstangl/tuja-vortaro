<vortaro>
<art mrk="$Id: lagri.xml,v 1.14 2014/04/14 14:10:14 revo Exp $">
<kap>
  <rad>lagri</rad>/o
</kap>
<drv mrk="lagri.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lagrie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lagria</trd>),
      malgranda koleoptero kun flavbrunaj lanecaj flugilkovriloj.
    </dif>
  </snc>
  <trd lng="de">Wollk&auml;fer</trd>
  <trd lng="fr">lagria</trd>
  <trd lng="hu">gyapjasbog&aacute;r</trd>
  <trd lng="nl">ruigkever</trd>
  <trdgrp lng="pt">
    <trd>idiamim </trd>,
    <trd>bicho-capixaba</trd>,
    <trd>capixabinha</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
