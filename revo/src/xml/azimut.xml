<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: azimut.xml,v 1.15 2015/07/29 16:10:15 revo Exp $">
<kap>
  <rad>azimut</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="azimut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Angulo inter la meridiano astronomia a&ubreve; magneta kaj la vertikala
      ebeno a&ubreve; la vertikalo, kiu trapasas tra difinita astro
      a&ubreve; tra difinita terpunkto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_ib;&c_m;&c_u;&c_t;</trd>
  <trd lng="bg">&c_a;&c_z;&c_i;&c_m;&c_u;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>azimut</trd>,
    <trd>sm&ecaron;rov&yacute; &uacute;hel</trd>
  </trdgrp>
  <trd lng="de">Azimut</trd>
  <trd lng="en">azimuth</trd>
  <trdgrp lng="es">
    <trd>acimut</trd>,
    <trd>azimut</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_m;&a_t;<klr tip="ind"> (&a_A;&a_kh;&a_t;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y;)</klr></trd>
  <trd lng="fr">azimut</trd>
  <trd lng="hu">azimut</trd>
  <trdgrp lng="it">
    <trd>azimut</trd>,
    <trd>azimuth</trd>
  </trdgrp>
  <trd lng="nl">azimut</trd>
  <trd lng="ru">&c_a;&c_z;&c_i;&c_m;&c_u;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>azimut</trd>,
    <trd>smerov&yacute; uhol</trd>
  </trdgrp>
</drv>
</art>
