<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: banahx.xml,v 1.11 2012/08/25 11:10:10 revo Exp $">
<kap>
  <rad>Bana&hcirc;</rad>/o
</kap>
<drv mrk="banahx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Pollingve: Stefan <trd lng="pl"><klr tip="amb">Stefan</klr>
      Banach</trd>, 1892-1945.
      Pola matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_n;&c_a;&c_h;</trd>
  <trd lng="ca">Banach</trd>
  <trd lng="de">Banach</trd>
  <trd lng="en">Banach</trd>
  <trd lng="fa">&a_b;&a_A;&a_n;&a_A;&a_kh;</trd>
  <trd lng="fr">Banach</trd>
  <trd lng="hu">Banach</trd>
  <trd lng="it">Banach</trd>
  <trd lng="ru">&c_B;&c_a;&c_n;&c_a;&c_h;</trd>
</drv>

<drv mrk="banahx.b0a">
  <kap><tld lit="b"/>a</kap>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="spac.banahxa0o"><tld lit="b"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_a;&c_h;&c_a;&c_w;</trd>
  <trd lng="ca">de Banach</trd>
  <trd lng="de">Banachsch</trd>
  <trd lng="en">Banach['s]</trd>
  <trd lng="fr">de <ind>Banach</ind></trd>
  <trd lng="hu">Banach-</trd>
  <trd lng="it">di <ind>Banach</ind></trd>
  <trd lng="pl">banacha<klr> {D.}</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_a;&c_h;&c_o;&c_v;</trd>
</drv>
</art>
