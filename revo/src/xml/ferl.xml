<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ferl.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>ferl</rad>/i
</kap>
<drv mrk="ferl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Volvi velon kaj alligi &gcirc;in sur jardon.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ohnout</trd>,
    <trd>podkasat</trd>,
    <trd>stáhnout plachtu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>festmachen</trd>,
    <trd>festzurren</trd>,
    <trd>einziehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>furl</trd>,
    <trd>roll up</trd>,
    <trd>take in</trd>
  </trdgrp>
  <trd lng="es">recoger velas<klr tip="amb"> (at&aacute;ndolas en la
  verga)</klr></trd>
  <trd lng="fr">serrer<klr tip="ind"> (une voile)</klr></trd>
  <trdgrp lng="hu">
    <trd>bevon<klr tip="ind"> (vitorl&aacute;t)</klr></trd>,
    <trd>felteker<klr tip="ind"> (vitorl&aacute;t)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ferrar velas</trd>,
    <trd>colher velas</trd>,
    <trd>amarrar velas</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_r;&c_u;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podkasať</trd>,
    <trd>stiahnuť plachtu</trd>
  </trdgrp>
</drv>

<drv mrk="ferl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i kaj rezulto de tiu ago.
    </dif>
  </snc>
  <trd lng="cs">podkasání</trd>
  <trdgrp lng="de">
    <trd>Festmachen</trd>,
    <trd>Festzurren</trd>,
    <trd>Einziehen</trd>
  </trdgrp>
  <trd lng="hu">vitorlabevon&aacute;s</trd>
  <trd lng="sk">podkasanie</trd>
</drv>
</art>
