<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: nordrejnvest.xml,v 1.11 2013/12/02 09:10:22 revo Exp $">
   <kap>
    <rad>Nordrejn-Vestfali</rad>/o
   </kap>
   <drv mrk="nordrejnvest.N0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
       <dif>
      La plej lo&gcirc;antori&ccirc;a el la federaciaj &scirc;tatoj de
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, situanta en
      la
plej okcidenta parto kaj trafluata de la rivero <ref tip="vid"
cel="rejn.R0o">Rejno</ref>.
      Regurbo estas Duseldorfo, plej granda urbo estas <ref tip="vid"
cel="kolonj.K0o">Kolonjo</ref>.
       </dif>
       <trd lng="ca">Rin del Nord-Westf&agrave;lia</trd>
       <trd lng="de">Nordrhein-Westfalen</trd>
       <trd lng="en">North Rhine-Westphalia</trd>
       <trd lng="fr">Rh&eacute;nanie du Nord-Westphalie</trd>
       <trd lng="hu">&Eacute;szak-Rajna-Vesztf&aacute;lia</trd>
       <trd lng="nl">Noordrijn-Westfalen</trd>
       <trd lng="pl">Nadrenia P&oacute;&lstroke;nocna-Westfalia</trd>
       <trd lng="pt">Ren&acirc;nia do Norte-Vestef&aacute;lia</trd>
    </snc>
</drv>

  <drv mrk="nordrejnvest.0a">
    <kap><tld lit="n"/>a</kap>
    <snc>
      <dif>
         Rilatanta al Nordrejn-Vestfalio.
      </dif>
    </snc>
    <trd lng="ca">Rin del Nord-Westfali&agrave;</trd>
    <trdgrp lng="de">
       <trd>nordrhein-westf&auml;lisch</trd>
    </trdgrp>
    <trd lng="hu">&eacute;szak-rajna-vesztf&aacute;liai</trd>
    <trd lng="nl">Noordrijn-Westfaals</trd>    
  </drv>

  </art>

