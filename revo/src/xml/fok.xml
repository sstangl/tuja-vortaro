<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fok.xml,v 1.27 2015/07/04 08:37:34 revo Exp $">
<kap><ofc>*</ofc>
<rad>fok</rad>/o</kap>
<drv mrk="fok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="fok.0o.ZOO">
    <dif>
       Mara amfibia felhava 
       <ref tip="lst" cel="best.rabo0o"
       lst="voko:rabobestoj">rabobesto</ref>;
       <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
       cel="foked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Phoca</trd>):
      <ekz>
        mi apena&ubreve; eraras komparante &scirc;iajn vo&ccirc;ovibrojn
        kun tiuj de
        gaja
        <tld/>ido
        <fnt>
        <aut>Poul Thorsen</aut>,
        <vrk>Stokita vino. - Edistudio, 1995</vrk>,
        <lok><url ref="http://www.poulthorsen.dk/verywell.htm">Very
        well,
        Hortensia</url></lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">tuleň</trd>
  <trdgrp lng="de">
    <trd>Seehund</trd>,
    <trd>Robbe</trd>
  </trdgrp>
  <trd lng="en">seal <klr>(animal)</klr></trd>
  <trd lng="es">foca</trd>
  <trd lng="fa">&a_f;&a_damma;&f_k;</trd>
  <trd lng="fr">phoque</trd>
  <trd lng="hu">f&oacute;ka</trd>
  <trd lng="id"><ind>anjing</ind> laut</trd>
  <trdgrp lng="nl">
    <trd>zeehond</trd>,
    <trd>rob</trd>
  </trdgrp>
  <trd lng="pt">foca</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">tuleň</trd>
  <trd lng="tr">fok</trd>
</drv>
</art>
