<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lici.xml,v 1.14 2013/11/02 18:10:15 revo Exp $">
<kap>
  <rad>lici</rad>/o
</kap>
<drv mrk="lici.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Lycium</trd>), 
      <ref tip="lst" cel="arbed.0o"
      lst="voko:solanoparencoj">arbedo</ref>
      kun ordinare dornaj bran&ccirc;oj, glatrandaj folioj ordinare sidantaj
      en 
      faskoj kaj kun ru&gcirc;aj venenaj beroj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_r;&c_a;&c_z;&c_a;</trd>
  <trd lng="de">Bocksdorn</trd>
  <trd lng="en">boxthorn</trd>
  <trd lng="hu">&ouml;rd&ouml;gc&eacute;rna</trd>
  <trd lng="nl">boksdoorn</trd>
  <trd lng="pt">l&iacute;cio <klr tip="amb">(bot.)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_je;&c_z;&c_a;</trd>
</drv>
</art>
