<vortaro>
<art mrk="$Id: hindi.xml,v 1.4 2014/06/13 08:10:12 revo Exp $">
<kap>
  <rad>hindi</rad>/o
</kap>

<drv mrk="hindi.0o">
  <kap>
    <tld lit="H"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="hindi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
      <ref tip="dif" cel="hind.H0ujo">Hindujo</ref>
    </snc>
    <ref tip="sin" cel="barat.B0o">Barato</ref>
    <trd lng="bg">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
    <trd lng="de">Indischer Subkontinent</trd>
    <trdgrp lng="fr">
      <trd>Inde</trd>,
      <trd>R&eacute;publique de l'Inde</trd>
    </trdgrp>
    <trd lng="it">India</trd>
    <trdgrp lng="pl">
      <trd>Indie</trd>
    </trdgrp>
</drv>

</art>

