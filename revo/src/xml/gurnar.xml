<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gurnar.xml,v 1.11 2014/06/21 17:10:11 revo Exp $">
<kap>
  <rad>gurnard</rad>/o
</kap>
<drv mrk="gurnar.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="trigl.0o">triglo</ref>
      (<trd lng="la">Trigla gurnardus</trd>), kun grizaj
      makuloj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cap d'ase</trd>,
    <trd>lluerna verda</trd>
  </trdgrp>
  <trd lng="de">Grauer Knurrhahn</trd>
  <trd lng="en">grey gurnard</trd>
  <trd lng="fr">grondin</trd>
  <trd lng="hu">sz&uuml;rke <ind>morg&oacute;hal</ind></trd>
  <trd lng="nl">grauwe <ind>poon</ind></trd>
  <trd lng="pl">kurek szary</trd>
  <trd lng="pt">roncador <klr tip="amb">(zool.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_g;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_p;&c_je;&c_t;&c_u;&c_h;</trd>
  </trdgrp>
</drv>
</art>

