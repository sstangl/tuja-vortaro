<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diagno.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diagnoz</rad>/o
</kap>
<drv mrk="diagno.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="diagno.0o.MED">
    <dif>
      Ekkono de malsano per &gcirc;iaj simptomoj:
      <ekz>
        laboratoriaj testoj por tuja <tld/>o<fnt><aut>P. S.
        Viana</aut>, <vrk>Sukceso kontra&ubreve; aidoso</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 8a, p.
        15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_g;&c_n;&c_a;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>diagnóza</trd>,
    <trd>lékařský nález</trd>
  </trdgrp>
  <trd lng="de">Diagnose</trd>
  <trd lng="en">diagnosis</trd>
  <trd lng="es">diagn&oacute;stico</trd>
  <trd lng="fr">diagnostic</trd>
  <trdgrp lng="hu">
    <trd>diagn&oacute;zis</trd>,
    <trd>k&oacute;risme</trd>,
    <trd>k&oacute;rmeghat&aacute;roz&aacute;s</trd>
  </trdgrp>
  <trd lng="pl">diagnoza</trd>
  <trdgrp lng="pt">
    <trd>diagn&oacute;stico</trd>,
    <trd>diagnose</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>diagn&oacute;za</trd>,
    <trd>diagnóza</trd>
  </trdgrp>
  <trd lng="sv">diagnos</trd>
</drv>

<drv mrk="diagno.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="diagno.0i.MED">
    <dif>
      Difini malsanon la&ubreve; &gcirc;iaj simptomoj:
      <ekz>
        estas malfacile <tld/>i mildan koratakon<fnt><aut>D.
        Curtis</aut>, <vrk>Koratako</vrk>, <bib>Monato</bib>, <lok>jaro
        2002a, numero 1a, p. 28-34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_ja;&c_g;&c_n;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>diagnostikovat</trd>,
    <trd>diagnozovat</trd>,
    <trd>rozpoznat</trd>,
    <trd>stanovit diagnózu</trd>,
    <trd>vyšetřit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>diagnostizieren</trd>,
    <trd>feststellen</trd>
  </trdgrp>
  <trd lng="en">diagnose</trd>
  <trd lng="es">diagnosticar</trd>
  <trd lng="fr">diagnostiquer</trd>
  <trdgrp lng="hu">
    <trd>diagnosztiz&aacute;l</trd>,
    <trd>k&oacute;rt meg&aacute;llap&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>diagnozowa&cacute;</trd>,
    <trd>stawia&cacute; <ind>diagnoz&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
    &c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>,
    <trd>&c_d;&c_i;&c_a;&c_g;&c_n;&c_o;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diagnostikova&tcaron;</trd>,
    <trd>ur&ccaron;i&tcaron; <ind>diagn&oacute;zu</ind></trd>,
    <trd>určiť diagnózu</trd>
  </trdgrp>
  <trd lng="sv">diagnosticera</trd>
</drv>
</art>
