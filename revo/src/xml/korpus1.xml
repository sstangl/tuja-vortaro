<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korpus1.xml,v 1.12 2009/07/17 16:30:49 revo Exp $">
<kap>
  <rad>korpuskl</rad>/o
</kap>
<drv mrk="korpus1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc mrk="korpus1.0o.SCI">
    <dif>
      Tre malgranda peco de materio; tre malgranda korpo, esta&jcirc;o:
      <ekz>
        la infuzoroj estas <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moleku.0o.KEM">molekulo</ref>
    </refgrp>
    <ref tip="sin" cel="partik2.0o">partiklo</ref>
  </snc>
  <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_r;&c_p;&c_u;&c_s;&c_k;&c_u;&c_l;&c_a;</trd>,
     <trd>&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_a;</trd>,
     <trd>&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">corpuscle</trd>
  <trdgrp lng="de">
     <trd>Korpuskel</trd>,
     <trd>Teilchen</trd>,
     <trd>Elementarteilchen</trd>,
     <trd>K&ouml;rnchen</trd>,
     <trd>St&auml;ubchen</trd>
  </trdgrp>
  <trd lng="en">corpuscle</trd>
  <trd lng="es">corp&uacute;sculo</trd>
  <trd lng="fa">&a_dh;&a_r;&a_h;</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;szecske</trd>,
    <trd>korpuszkula</trd>
  </trdgrp>  
  <trdgrp lng="pl">
    <trd>cz&aogonek;steczka</trd>,
    <trd>korpusku&lstroke;a</trd>
  </trdgrp>
  <trd lng="pt">corp&uacute;sculo</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_p;&c_u;&c_s;&c_k;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
</drv>
</art>
