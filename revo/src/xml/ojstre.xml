<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ojstre.xml,v 1.7 2013/08/31 15:10:31 revo Exp $">
<kap>
  <rad>ojstred</rad>/oj
</kap>
<drv mrk="ojstre.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Oestridae</trd>), grandaj haraj insektoj, kies
      larvoj
      parazitas en hufuloj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_d;&c_n;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Dasselfliegen</trd>,
    <trd>Biesfliegen</trd>
  </trdgrp>
  <trd lng="hu">bag&oacute;cslegyek<klr tip="amb">(csal&aacute;d)</klr></trd>
  <trd lng="nl">horzels</trd>
  <trdgrp lng="pt">
    <trd>estr&iacute;deos</trd>,
    <trd>oestr&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_v;&c_o;&c_d;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
