<vortaro>
<art mrk="$Id: zebu.xml,v 1.21 2015/07/04 08:37:46 revo Exp $">
<kap>
<rad>zebu</rad>/o
</kap>
<drv mrk="zebu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="bov.0o">bovo</ref>
      (<trd lng="la">Bos mutus</trd>), kun &gcirc;ibo,
      bredata en Hindujo kaj najbaraj landoj kiel hejmbesto.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_e;&c_b;&c_u;</trd>
  <trd lng="bg">&c_z;&c_je;&c_b;&c_u;</trd>
  <trd lng="br">zebu</trd>
  <trd lng="ca">zeb&uacute;</trd>
  <trd lng="cs">zebu</trd>
  <trdgrp lng="de">
    <trd>Zebu</trd>,
    <trd>Buckelrind</trd>
  </trdgrp>
  <trd lng="el">&zeta;&epsilon;&my;&pi;&omikron;&ypsilon_ton;</trd>
  <trdgrp lng="en">
    <trd>Brahman</trd>,
    <trd>zebu</trd>
  </trdgrp>
  <trd lng="es">ceb&uacute;</trd>
  <trd lng="fr">z&eacute;bu</trd>
  <trdgrp lng="hu">
    <trd>zebu</trd>,
    <trd>p&uacute;pos <ind>tulok</ind></trd>
  </trdgrp>
  <trd lng="it">zeb&ugrave;</trd>
  <trd lng="nl">zeboe</trd>
  <trd lng="pl">zebu</trd>
  <trd lng="pt">zebu</trd>
  <trd lng="ru">&c_z;&c_je;&c_b;&c_u;</trd>
  <trd lng="sk">zebu</trd>
</drv>
</art>
