<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vjel.xml,v 1.6 2013/09/08 18:11:44 revo Exp $">
<kap>
  <rad>vjel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="vjel.0o">
  <kap><tld/>o</kap>
  <snc mrk="vjel.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropa <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>
      en kiu kranke movata radeto frotas la kordojn, kaj klavaro 
      modifas la kordolongojn:
      <ekz>
        ekzistis kordinstrumentoj; liuto, gitaro, liro, <tld/>o
        <fnt>
          <aut>J. L. Tortel</aut>,
          <vrk>La trobadoroj</vrk>,
          <lok><url ref="http://thor.prohosting.com/jesuo/k44.htm">
          Kajeroj el la Sudo, 1999-10, numero 44a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Klara kaj mi prenis <tld/>ojn por ludi dum ili kantas
        <fnt>
          <aut>Mari-elen</aut>,
          <vrk><url ref="http://faldfolio.blogspot.com/">
          Faldfolio</url></vrk>,
          <lok>2007-01-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>samfoina</trd>,
    <trd><ind>viola</ind> de roda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Drehleier</trd>,
    <trd>Radleier</trd>
  </trdgrp>
  <trd lng="fr">vielle &agrave; roue</trd>
  <trdgrp lng="hu">
    <trd>teker&odblac;lant</trd>,
    <trd>nyenyere</trd>
  </trdgrp>
  <trd lng="it">ghironda</trd>
  <trd lng="nl">draailier</trd>
</drv>
</art>

