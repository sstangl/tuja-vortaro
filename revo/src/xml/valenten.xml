<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: valenten.xml,v 1.12 2015/02/26 19:10:37 revo Exp $">
    <kap>
      <rad>Valenten</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="valenten.0o">
      <kap><tld/>o</kap>
      <snc mrk="valenten.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
        </dif>
      </snc>
      <trd lng="ca">Valent&iacute;</trd>
      <trd lng="de">Valentin</trd>
      <trd lng="fr">Valentin</trd>
      <trdgrp lng="hu">
         <trd>B&aacute;lint</trd>,
         <trd>Valentin</trd>
      </trdgrp>
      <trd lng="id">Valentine</trd>
      <trd lng="it">Valentino</trd>
      <trd lng="nl">Valentijn</trd>
      <trd lng="pl">Walenty</trd>
      <trd lng="pt">Valentim</trd>
      <trd lng="ru">&c_V;&c_a;&c_l;&c_je;&c_n;&c_t;&c_i;&c_n;</trd>
    </drv>

    <drv mrk="valenten.tagode0o">
      <kap>tago de <tld/>o</kap>
      <snc mrk="valenten.tagode0o.KAL">
        <uzo tip="fak">KAL</uzo>
        <dif>
          <ref tip="lst" cel="fest.0o" lst="voko:festoj">Festo</ref> 
          de geamantoj kaj amdeklaroj, en la 14a tago de februaro:
          <ekz>
            en la tago de <tld/>o
            <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;solidareco 
                de stomakoj&rightquot;, p.
                47a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">festa de sant <ind>Valent&iacute;</ind></trd>
      <trd lng="de">Valentinstag</trd>
      <trd lng="fr">la <ind>Saint Valentin</ind></trd>
      <trdgrp lng="hu">
         <trd>B&aacute;lint-nap</trd>,
         <trd>Valentin-nap</trd>
      </trdgrp>
      <trd lng="id"><ind>Hari</ind> Valentine</trd>
      <trd lng="it">(giorno di) San <ind>Valentino</ind></trd>
      <trd lng="nl">Valentijnsdag</trd>
      <trd lng="pl">walentynki</trd>
      <trd lng="pt">dia dos Namorados (no Brasil, em 12 de junho)</trd>
      <trd lng="ru">&c_V;&c_a;&c_l;&c_je;&c_n;&c_t;&c_i;&c_n;&c_o;&c_v;
&c_d;&c_je;&c_n;&c_mol;</trd>
    </drv>
  </art>

