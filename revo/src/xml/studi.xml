<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: studi.xml,v 1.3 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>studi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="studi.0o">
  <kap><tld/>o</kap>
  <snc mrk="studi.0o.KOMUNE">
    <dif>
      Registrejo de bildo- a&ubreve; sonodokumentoj kun tiucelaj &ccirc;ambroj kaj
      ma&scirc;inoj.
    </dif>
  </snc>
  <snc mrk="studi.0o.EKON">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Registrokompanio posedanta <tld/>ojn:
      <ekz>
        <ctl>La mondo de Nemo</ctl> <klr>...</klr>, lasta 
        superprodukta&jcirc;o de la <tld/>oj Disney kaj Pixar
        <fnt>
          <aut>F. Mazoyer, trad. V. Lutermano</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article851.html">
          Konsekro de la mutaciulo</url></vrk>,
          <lok>le Monde diplomatique, 2004-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">studio <klr>(filmové| televizní)</klr></trd>
  <trd lng="fr">studio d'enregistrement</trd>
  <trd lng="sk">štúdio</trd>
</drv>

<drv mrk="studi.film0o">
  <kap>film<tld/>o, <var><kap>filmo<tld/>o</kap></var></kap>
  <snc mrk="studi.film0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      farado de filmoj:
      <ekz>
        <ctl>Lappar, la Antikristo</ctl> estas lia tria filmo. Produktis &gcirc;in 
        film<tld/>o Index en Lodzo
        <fnt>
          <aut>T. Chmielik</aut>,
          <vrk>Esperanta Antikristo...</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="studi.foto0o">
  <kap>foto<tld/>o</kap>
  <snc mrk="studi.foto0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      fotado:
      <ekz>
        unu estis dungito de vestovendejo kaj la alia havis okupon en 
        foto<tld/>o
        <fnt>
          <aut>Bu Tieli</aut>,
          <vrk>Ilia vivo &scirc;an&gcirc;i&gcirc;is</vrk>,
          <lok><url ref="http://english.china.org.cn/EL/EL/ElPopolaCxinio/">
          El popola &Ccirc;inio, 2003-03</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>studio de prise de vue</trd>,
    <trd>studio photographique</trd>
  </trdgrp>
</drv>

<drv mrk="studi.kino0o">
  <kap>kino<tld/>o, <var><kap>kin<tld/>o</kap></var></kap>
  <snc mrk="studi.kin0o.KIN">
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      farado de kinofilmoj:
      <ekz>
        li laboris en la Tallinna kino<tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/biblioteko/lokomobilo.htm">
          Danco &ccirc;irka&ubreve; lokomobilo</url></vrk>,
          <lok>Eesti raamat, 1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">studio de cin&eacute;ma</trd>
</drv>

<drv mrk="studi.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="studi.muzik0o.MUZ">
    <ref tip="dif" cel="studi.son0o.MUZ">sonstudio</ref>
    <ekz>
      Esperanto-Muzik-<tld lit="S"/>o Vinilkosmo
      <fnt>
        <aut>D. E. Wickstr&ouml;m</aut>,
        <vrk><url ref="http://artista.ikso.net/eurokka/studio.php">
        Retpa&gcirc;o de Eurokka</url></vrk>,
        <lok>[vidita en 2007-03]</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">studio d'enregistrement sonore</trd>
</drv>

<drv mrk="studi.radi0o">
  <kap>radi<tld/>o, <var><kap>radio<tld/>o</kap></var></kap>
  <snc mrk="studi.radi0o.RAD">
    <uzo tip="fak">RAD</uzo>
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      farado de radielsenda&jcirc;oj:
      <ekz>
        La regiona radio<tld/>o en Pardubice invitis <klr>...</klr> la 
        prezidantinon de AEH por preska&ubreve; 50minuta viva interparolo
        <fnt>
          <aut>-</aut>,
          <vrk>Radio-elsendo...</vrk>,
          <lok><url ref="http://www.volny.cz/tomac.cs/aeh-ikeh/informilo/INFORMILO_2005_6.htm">
          Informilo 2005-6</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">studio radiophonique</trd>
</drv>

<drv mrk="studi.son0o">
  <kap>son<tld/>o</kap>
  <snc mrk="studi.son0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      farado de sonregistra&jcirc;oj, precipe muzikaj:
      <ekz>
        oni povos havi &gcirc;isdatan son<tld/>on en sia hejmo
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">
          taglibro</url></vrk>,
          <lok>2006-03-28</lok>
        </fnt>.    
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">studio d'enregistrement sonore</trd>
</drv>

<drv mrk="studi.televid0o">
  <kap>televid<tld/>o</kap>
  <snc mrk="studi.televid0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="studi.0o.KOMUNE"><tld lit="S"/>o</ref> por 
      farado de televidfilmoj:
      <ekz>
        la Centra Televid<tld/>o
        <klr>(nomo de televidfirmao)</klr>
        <fnt>
          <aut>Shi Chengtai</aut>,
          <vrk>La palaj tagoj sen kiso</vrk>,
          <lok><url ref="http://www38.websamba.com/penseo/pen56.htm">
          Penseo, 1995-02, numero 56a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">studio de t&eacute;l&eacute;vision</trd>
</drv>
</art>

