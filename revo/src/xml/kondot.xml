<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kondot.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kondotier</rad>/o
</kap>
<drv mrk="kondot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En mezepoko kaj renesanco, itala aventurema bandestro de dungataj
      soldatoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_a;&c_c;&c_mol;&c_je;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>condotiere</trd>,
    <trd>kondotiér</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Condottiere</trd>,
    <trd>italienischer Freischarf&uuml;hrer</trd>
  </trdgrp>
  <trd lng="fr">condottiere</trd>
  <trdgrp lng="it">
    <trd>condottiero</trd>,
    <trd><ind>capitano</ind> di ventura</trd>
  </trdgrp>
  <trd lng="nl">condottiere</trd>
  <trdgrp lng="pt">
    <trd>condottiero</trd>,
    <trd>condottiere</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_d;&c_o;&c_t;&c_mol;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>condotiere</trd>,
    <trd>kondotiér</trd>
  </trdgrp>
</drv>
</art>
