<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: izomor1.xml,v 1.15 2007/04/20 16:31:42 revo Exp $">
<kap>
<rad>izomorfi</rad>/o
</kap>
<drv mrk="izomor1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bijekc.0a">Bijekcia</ref>
      <ref tip="super" cel="homomo.0o">homomorfio</ref>:
      <ekz>
        anka&ubreve; la <ref cel="invers.0o.variaj">inverso</ref> de
        <tld/>o estas <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_m;&c_a;&c_r;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">isomorfisme</trd>
  <trd lng="de">Isomorphie</trd>
  <trd lng="en">isomorphism</trd>
  <trd lng="es">isomorfismo</trd>
  <trd lng="fr">isomorphisme</trd>
  <trd lng="hu">izomorfizmus</trd>  
  <trd lng="nl">isomorfisme</trd>
  <trd lng="pl">izomorfizm</trd>
  <trd lng="pt">isomorfismo</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="izomor1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="izomor1.0a.strukt">
    <dif>
      <klr>(p.p. du <ref cel="strukt.algebra0o">algebraj strukturoj</ref>)</klr>
      Tiaj, ke ekzistas <ref cel="izomor1.0o"><tld/>o</ref> de unu al la alia.
    </dif>
    <rim>
      Por &ccirc;i tiu senco ekzistas kutima sinonimo <ctl>izomorfa</ctl>
      kiel en <fnt><bib>PIV2</bib></fnt>.
    </rim>
  </snc>
  <snc mrk="izomor1.0a.bildigo">
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta ecojn de <ref cel="izomor1.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;</trd>
  <trd lng="ca">isomorf</trd>
  <trd lng="de">isomorph</trd>
  <trd lng="en">isomorphic</trd>
  <trd lng="es">isomorfo</trd>
  <trd lng="fr">isomorphe</trd>
  <trd lng="hu">izomorf</trd>  
  <trd lng="nl">isomorfisch</trd>
  <trd lng="pl">izomorficzny</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
