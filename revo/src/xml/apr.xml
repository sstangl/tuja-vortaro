<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: apr.xml,v 1.28 2015/09/14 12:10:15 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>apr</rad>/o
    </kap>

    <drv mrk="apr.0o">
      <kap>
        <ofc>*</ofc>
        <tld/>o
      </kap>
      <uzo tip="fak">ZOO</uzo>
      <snc mrk="apr.0o.ZOO">
        <dif>
          Specio de <ref tip="super" cel="pork.0o">porko</ref>
          (<trd lng="la">Sus scrofa</trd>), sova&gcirc;a porko.
        </dif>
      </snc>
      <bld lok="../bld/apr.jpg">
        <tld/>oj kun <tld/>idoj
      </bld>
      <trdgrp lng="be">
        <trd>&c_v;&c_ja;&c_p;&c_r;&c_u;&c_k;</trd>, 
        <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib; &c_k;&c_a;&c_b;&c_a;&c_n;</trd>
      </trdgrp>
      <trd lng="bg">&c_d;&c_i;&c_v;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
      <trd lng="br">hoc'h-gouez</trd>
      <trd lng="ca">porc senglar</trd>
      <trd lng="de">Wildschwein</trd>
      <trd lng="en">Eurasian wild <ind>boar</ind></trd>
      <trd lng="es">jabal&iacute;</trd>
      <trdgrp lng="fr">
        <trd>sanglier</trd>,
        <trd>laie</trd>,
        <trd>marcassin</trd>
      </trdgrp>
      <trd lng="he">&het;&zayin;&yod;&resh; &bet;&resh;</trd>
      <trd lng="hu">vaddiszn&oacute;</trd>
      <trdgrp lng="pl">
        <trd>dzik</trd>,
        <trd>dzika <ind>&sacute;winia</ind></trd>
      </trdgrp>
      <trd lng="pt">javali</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_p;&c_r;&c_mol;</trd>,
        <trd>&c_d;&c_i;&c_k;&c_i;&c_j;
        <ind>&c_k;&c_a;&c_b;&c_a;&c_n;</ind></trd>
      </trdgrp>
      <trd lng="sk">diviak</trd>
    </drv>

    <drv mrk="apr.vir0o">
      <kap>
        vir<tld/>o
      </kap>
      <uzo tip="fak">ZOO</uzo>
      <snc mrk="apr.vir0o.ZOO">
        <dif>
          Virseksa <tld/>o.
        </dif>
      </snc>
      <trdgrp lng="pl">
        <trd>odyniec</trd>,
        <trd>knur</trd>
      </trdgrp>
    </drv>

    <drv mrk="apr.0ino">
      <kap>
        <tld/>ino
      </kap>
      <uzo tip="fak">ZOO</uzo>
      <snc mrk="apr.0ino.ZOO">
        <dif>
          Inseksa <tld/>o.
        </dif>
        <trdgrp lng="pl">
          <trd>locha</trd>,
          <trd>maciora</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="apr.0ido">
      <kap>
        <tld/>ido
      </kap>
      <uzo tip="fak">ZOO</uzo>
      <snc mrk="apr.0ido.ZOO">
        <dif>
          Ido de <tld/>o.
        </dif>
        <trdgrp lng="pl">
          <trd>warchlak</trd>
          <trd>prosi&eogonek; <ind>dzika</ind></trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="apr.0ajxo">
      <kap>
        <tld/>a&jcirc;o
      </kap>
      <snc mrk="apr.0ajxo.KUI">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Viando de <tld/>o.
        </dif>
        <trd lng="ca">carn de porc senglar</trd>
        <trd lng="pl">dziczyzna</trd>
      </snc>
    </drv>

    <drv mrk="apr.veruk0o">
      <kap>veruk<tld/>o <fnt><bib>PIV2</bib>,
      <lok>apro</lok></fnt></kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>
          Nerema&ccirc;anta
          <ref tip="super" cel="huf.par0uloj">parhufulo</ref>
          el la genro
          <trd lng="la">Phacochoerus</trd>, vivanta en Afriko suden de
          Saharo, parenca al <tld/>o, kun verukformaj
          &scirc;vela&jcirc;oj
          sur la muzelo.
        </dif>
      </snc>
      <refgrp tip="sin">
        <ref cel="fakocx.0o">fako&ccirc;ero</ref>,
        <ref cel="fakohx.0o">fako&hcirc;ero</ref>
      </refgrp>
      <ref tip="vid" cel="babiru.0o">babiruso</ref>
      <trd lng="bg">&c_b;&c_r;&c_a;&c_d;&c_a;&c_v;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
      <trd lng="ca">facoquer</trd>
      <trd lng="de">Warzenschwein</trd>
      <trd lng="en">warthog</trd>
      <trd lng="fr">phacoch&egrave;re</trd>
      <trd lng="hu">varacskos <ind>diszn&oacute;</ind></trd>
      <trd lng="nl">wrattenzwijn</trd>
      <trdgrp lng="pl">
        <trd>guziec</trd>,
        <trd>&sacute;winia afryka&nacute;ska</trd>
      </trdgrp>
      <trd
      lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
    </drv>

  </art>
