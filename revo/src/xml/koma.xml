<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koma.xml,v 1.12 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>koma</rad>/o
</kap>
<drv mrk="koma.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Intervalo de du notoj enharmoniaj (&ccirc;irka&ubreve; unu na&ubreve;ono de tono).
    </dif>
  </snc>
  <ref tip="vid" cel="harmon1.0o">harmonio</ref>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_a;<klr tip="ind"> (muz.)</klr></trd>
  <trd lng="ca">coma</trd>
  <trd lng="es">coma</trd>
  <trd lng="fr">comma</trd>
  <trd lng="nl">komma<klr tip="ind"> (muz.)</klr></trd>
  <trdgrp lng="pt">
    <trd>coma <klr tip="amb">(m&uacute;s.)</klr></trd>,
    <trd>microtom</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_m;&c_a;</trd>
  <trd lng="sk">koma</trd>
  <trd lng="cs">koma</trd>
</drv>
</art>

