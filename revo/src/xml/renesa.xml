<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: renesa.xml,v 1.20 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>renesanc</rad>/o
</kap>
<drv mrk="renesa.0o">
  <kap><tld/>o</kap>
  <snc mrk="renesa.0o.HIS">
  <uzo tip="fak">HIS</uzo>
    <dif>
      E&ubreve;ropa arta kaj scienca 
      <ref tip="lst" cel="evolu.0o" lst="voko:eventoj">evoluo</ref> 
      en la 15a kaj 16a jarcentoj,
      karakterizata interalie per 
      revivi&gcirc;o de la beletro kaj artoj influita 
      de la helenlatina antikveco.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>
    &c_a;&c_d;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;
    </trd>,
    <trd>&c_r;&c_e;&c_n;&c_e;&c_s;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">el Renaixement</trd>
  <trd lng="cs">renesance</trd>
  <trd lng="de">Renaissance</trd>
  <trd lng="en">renaissance</trd>
  <trd lng="fr">Renaissance</trd>
  <trd lng="hu">renesz&aacute;nsz</trd>
  <trd lng="nl">renaissance</trd>
  <trd lng="pl">renesans</trd>
  <trd lng="pt">Renascen&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_n;&c_je;&c_s;&c_s;&c_a;&c_n;&c_s;</trd>,
    <trd>
    &c_V;&c_o;&c_z;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prerod</trd>,
    <trd>renesancia</trd>
  </trdgrp>
</drv>
</art>
