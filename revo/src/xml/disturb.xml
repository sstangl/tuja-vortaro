<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: disturb.xml,v 1.1 2013/06/01 13:10:08 revo Exp $">
<kap>
  <rad>disturb</rad>/i
</kap>
<drv mrk="disturb.0i">
  <kap><tld/>i</kap>
  <snc mrk="disturb.0i.KOMUNE">
    <ref tip="dif" cel="pertur.0i.KOMUNE">perturbi</ref>
      <ekz>
        la apudesto de mia &ccirc;evalino povas <tld/>i vian &ccirc;evalon
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 16a, p. 
        476a</lok></fnt>;
      </ekz>
      <ekz>
        iu sor&ccirc;a dia&jcirc;o komprenis miajn planojn kaj plene ilin 
        <tld/>is
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url
          ref="http://www.esperanto.gr/lit/els/lukio.pdf">Lukio a&ubreve;
          azeno</url></vrk>,
          <lok>fonto, 1988</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ina&ubreve;guran ceremonion <tld/>is &gcirc;ustamomente &scirc;ar&gcirc;aviadila bruo
        <fnt>
          <aut>B. Ragnarsson</aut>,
          <vrk>la&ubreve; neplanitaj padoj</vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>g&ecirc;ner</trd>,
    <trd>perturber</trd>
  </trdgrp>
</drv>
</art>

