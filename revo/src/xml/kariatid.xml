<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kariatid.xml,v 1.3 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kariatid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kariatid.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariatid.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="kolon.0o.ARKI">Kolono</ref> en formo de
      grandega homstatuo:
      <ekz>
        <klr>A&ubreve;guste Rodin</klr> iris al Bruxelles, kie li faris la 
        <tld/>ojn de l' <ctl>Palais de Bourse</ctl>
        <fnt>
         <aut>P. Vars&aacute;nyi</aut>,
         <vrk>Auguste Rodin</vrk>,
         <lok><url
         ref="http://literaturamondo.org/PDF/1924/LM%201924-01bc.pdf">
         Literatura Mondo, 1924-01 (3:1)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>břevnonoš</trd>,
    <trd>karyatida</trd>,
    <trd>kóra</trd>
  </trdgrp>
  <trd lng="fr">cariatide</trd>
  <trdgrp lng="sk">
    <trd>karyatída</trd>,
    <trd>podperná socha</trd>
  </trdgrp>
</drv>
</art>

