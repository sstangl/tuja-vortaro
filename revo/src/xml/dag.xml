<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dag.xml,v 1.11 2015/06/25 09:10:15 revo Exp $">
<kap>
  <rad>dag</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="dag.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="glav.0o.MIL">Glavo</ref> kun larĝa,
      mallonga
      klingo, por persona sindefendo:
      <ekz>
        ŝi portis ĉe la zono, ne glavon, sed gemo-garnitan
        <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, ĉapitro 49a, p.
        669a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>кінжал</trd>,
    <trd>корцік</trd>
  </trdgrp>
  <trd lng="bg">кама</trd>
  <trd lng="ca">daga</trd>
  <trd lng="cs">dýka</trd>
  <trd lng="de">Dolch</trd>
  <trd lng="en">dagger</trd>
  <trd lng="es">daga</trd>
  <trd lng="fr">dague<klr> (arme)</klr></trd>
  <trd lng="nl">dolk</trd>
  <trdgrp lng="pl">
    <trd>daga</trd>,
    <trd>deka</trd>
  </trdgrp>
  <trd lng="pt">adaga</trd>
  <trd lng="zh">匕首 [<ind>bǐ shǒu</ind>]</trd>
</drv>
</art>

