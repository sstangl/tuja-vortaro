<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kres.xml,v 1.17 2014/12/08 13:10:19 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kres</rad>/o
</kap>
<drv mrk="kres.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Iu el pluraj specioj el familio <ref tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>,
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herbo</ref>
      kun akreta gusto, uzata kiel spica&jcirc;o kaj salato.
    </dif>
  </snc>
  <trd lng="ca">morritort</trd>
  <trd lng="en">cress</trd>
  <trdgrp lng="es">
    <trd>mastuerzo</trd>,
    <trd>berro</trd>
  </trdgrp>
  <trd lng="de">Kresse</trd>
  <trd lng="fr">cresson</trd>
  <trd lng="hu">zs&aacute;zsa</trd>
  <trd lng="nl">tuinkers</trd>
  <trd lng="pt">agri&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;-&c_s;&c_a;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kres.akvo0o">
  <kap>
    akvo<tld/>o
  </kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:botanikaj_specioj">Specio</ref>
      el familio <ref tip="malprt" 
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Nasturtium officinale</trd>),
      man&gcirc;ebla <ref tip="lst" cel="herb.0o.BOT" 
      lst="voko:brasikoparencoj">herbo</ref>, kiu kreskas en
      subakvaj lokoj.
    </dif>
  </snc>
  <trd lng="ca">creixen</trd>
  <trd lng="en">watercress</trd>
  <trd lng="es">berro</trd>
  <trd lng="de">Brunnenkresse</trd>
  <trd lng="fr">cresson de fontaine</trd>
  <trd lng="hu">v&iacute;zitorma</trd>
  <trd lng="nl">waterkers</trd>
  <trd lng="pt">mastru&ccedil;o-dos-rios</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_u;&c_n;&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_u;&c_h;&c_a; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
