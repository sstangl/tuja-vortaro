<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: macx.xml,v 1.31 2015/02/15 11:10:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ma&ccirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>


<drv mrk="macx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.0i.ANA">
    <dif>
      Pisti en bu&scirc;o per la dentoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia brako estis for<tld/>ita de la dentumitaj radoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prem.dis0i">dispremi</ref>,
      <ref cel="muel.0i">mueli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_v;&c_ch;&c_a;</trd>
  <trd lng="br">chaokat</trd>
  <trd lng="ca">mastegar</trd>
  <trd lng="cs">&zcaron;v&yacute;kat</trd>
  <trd lng="de">kauen</trd>
  <trdgrp lng="en">
    <trd>chew</trd>,
    <trd>masticate</trd>
  </trdgrp>
  <trd lng="es">masticar</trd>
  <trd lng="fr">m&acirc;cher</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;g</trd>,
    <trd>megr&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">kauwen</trd>
  <trd lng="pl">&zdot;u&cacute;</trd>
   <trdgrp lng="pt">
    <trd>mastigar</trd>,
    <trd>mascar</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">tugga</trd>
</drv>

<drv mrk="macx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="macx.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>ado:
      <ekz>
        <tld/>, <tld/>, <tld/>, sonis la makzeloj
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li venis 
        trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">miam</trd>
</drv>


<drv mrk="macx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.re0i.ANA">
    <dif>
      Denove <tld/>i.
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;</trd>
    <trd lng="cs">p&rcaron;e&zcaron;vykovat</trd>
    <trd lng="de">nochmals kauen</trd>
    <trd lng="hu">&uacute;jrar&aacute;g</trd>
  </snc>
  <snc mrk="macx.re0i.ZOO">
    <dif>
      Revenigi nutra&jcirc;on el la stomako en la bu&scirc;on por denove
       <tld/>i &gcirc;in, kiel faras iuj bestoj, ekzemple bovo, kamelo.
    </dif>
    <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;</trd>
    <trd lng="br">daskiriat</trd>
    <trdgrp lng="ca">
      <trd>remugar</trd>,
      <trd>ruminar</trd>
    </trdgrp>
    <trd lng="de">wiederk&auml;uen</trd>
    <trdgrp lng="en">
      <trd>chew the cud</trd>,
      <trd>ruminate</trd>,
    </trdgrp>
    <trd lng="es">rumiar</trd> 
    <trd lng="hu">k&eacute;r&odblac;dzik</trd>
  </snc>
  <snc mrk="macx.re0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ripete pripensi:
      <ekz>
        konstante re<tld/>i projekton, reakciajn ideojn.
      </ekz>
    </dif>
    <trdgrp lng="bg">
        <trd>&c_p;&c_r;&c_je;&c_d;&c_malmol;&c_v;&c_k;&c_v;&c_a;&c_m;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_v;&c_t;&c_a;&c_r;&c_ja;&c_m;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_ja;&c_m;</trd>
    </trdgrp>
    <trd lng="de">hin und her &uuml;berlegen</trd>
    <trdgrp lng="en">
      <trd>ruminate</trd>,
      <trd>reflect on</trd>,
      <trd>ponder</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_zh;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>rem&acirc;cher</trd>,
    <trd>ruminer</trd>
  </trdgrp>
  <trd lng="hu">megr&aacute;g <klr tip="amb">(megfontol)</klr></trd>
  <trd lng="nl">herkauwen</trd>
  <trd lng="pl">prze&zdot;uwa&cacute;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_zh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="macx.re0uloj">
  <kap>re<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="macx.re0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_grupoj">Subordo</ref> de <ref tip="malprt"
      cel="huf.par0uloj">parhufuloj</ref> 
      (&scirc;afoj, &gcirc;irafoj, cervedoj, bovedoj, kameloj, k.a.) 
      kiuj re<tld/>as kaj havas plurpartan stomakon, plejofte kun kvar 
      kameroj (<trd lng="la">Ruminantia</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_n;&c_i; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_i;</trd>
  <trd lng="ca">remugants</trd>
  <trd lng="cs">p&rcaron;e&zcaron;v&yacute;kavci</trd>
  <trd lng="de">Wiederk&auml;uer</trd>
  <trd lng="en">ruminants</trd>
  <trd lng="es">rumiantes</trd> 
  <trd lng="fr">ruminants</trd>
  <trd lng="nl">herkauwers</trd>
</drv>

<drv mrk="macx.krak0i">
  <kap>krak<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="macx.krak0i.ANA">
    <dif>
      Rompi per la dentoj kun krakbruo.
    </dif>
  </snc>
  <trd lng="ca">rompre amb les dents</trd>
  <trd lng="de">zerbei&szlig;en</trd>
  <trd lng="fr">croquer</trd>
  <trd lng="hu">ropogtat <klr tip="amb">(&eacute;telt)</klr></trd>
  <trd lng="nl">stukkauwen</trd>
  <trd lng="pl">chrupa&cacute;<klr> gry&zacute;&cacute;</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_g;&c_r;&c_y;&c_z;&c_a;&c_t;&c_mol; &c_s; &c_h;&c_r;&c_u;&c_s;&c_t;&c_o;&c_m;</trd>
</drv>
</art>
