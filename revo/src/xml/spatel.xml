<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: spatel.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>spatel</rad>/o
</kap>
<drv mrk="spatel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de plata kulereto, uzata por miksi a&ubreve; sterni duonlikvajn
      substancojn, a&ubreve; por meti farbojn sur paletron.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_sh;&c_p;&c_a;&c_t;&c_u;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>lopatička</trd>,
    <trd>stěrka</trd>,
    <trd>špachtle</trd>
  </trdgrp>
  <trd lng="de">Spatel</trd>
  <trd lng="en">spatula</trd>
  <trd lng="fr">spatule</trd>
  <trdgrp lng="hu">
    <trd>lapoc</trd>,
    <trd>spatula</trd>,
    <trd>sim&iacute;t&oacute;lap&aacute;t</trd></trdgrp>
  <trd lng="nl">spatel</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">špachtľa</trd>
</drv>
</art>
