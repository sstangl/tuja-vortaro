<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bril.xml,v 1.23 2015/10/07 15:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bril</rad>/i
</kap>
<drv mrk="bril.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.0i.lumo">
    <dif>
      Eligi a&ubreve; resendi helan lumon:
      <ekz>
        <tld/>as suno, oro, spegulo, polurita metalo, blanka ne&gcirc;o;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is pro plezuro;
      </ekz>
      <ekz>
        <ind>ne &ccirc;io <tld/>anta estas diamanto</ind>
        <trd lng="fr">tout ce qui brille n'est pas or</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io utilas, kio brilas<fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="or.0o">oro</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fajr.0i">fajri</ref>,
      <ref cel="flam.0i">flami</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="fulm.0i">fulmi</ref>,
      <ref cel="glim">glimi</ref>,
      <ref cel="lum.0i">lumi</ref>,
      <ref cel="hel">heli</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcze&cacute;</trd>,
      <trd>l&sacute;ni&cacute;</trd>,
      <trd>ja&sacute;nie&cacute;</trd>
    </trdgrp>
    <trd lng="pt">brilhar</trd>
  </snc>
  <snc mrk="bril.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Altiri la atenton per sia prospereco, eleganteco, lukso:
      <ekz>
        <tld/>anta sukceso, festo, societo, stato, sano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flor.0i">flori</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="juvel.0o">juvelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_z;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_ib;&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bl&yacute;skat se</trd>,
    <trd>lesknout se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>t&rcaron;pytit se</trd>,
    <trd>z&aacute;&rcaron;it</trd>,
    <trd>z&aacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gl&auml;nzen</trd>,
    <trd>scheinen</trd>
  </trdgrp>
  <trd lng="en">to shine</trd>
  <trd lng="es">brillar</trd>
  <trdgrp lng="fr">
    <trd>briller</trd>,
    <trd>luire</trd>
  </trdgrp>
  <trd lng="hu">ragyog</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iari&tcaron;</trd>,
    <trd>ligota&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0o">
  <kap><tld/>o</kap>
  <snc mrk="bril.0o.FIZ">
    <dif>
      Hellumo sendata de io:
      <ekz>
        <tld/>o de fajro, de kvarco;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de la luno;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni ne lasu nin trompi per la <tld/>o de la vortoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>eto de kontenteco &scirc;teliris trans lia viza&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ja;&c_s;&c_k;</trd>,
    <trd>&c_z;&c_mol;&c_z;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_ib;&c_h;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lesk</trd>,
    <trd>z&aacute;&rcaron;</trd>,
    <trd>z&aacute;&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glanz</trd>,
    <trd>Schein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>light</trd>,
    <trd>brightness</trd>, <trd>brilliance</trd>
  </trdgrp>
  <trd lng="es">brillo</trd>
  <trdgrp lng="fr">
    <trd>&eacute;clat <klr tip="ind">(lueur)</klr></trd>,
    <trd>lueur</trd>
  </trdgrp>
  <trd lng="hu">ragyog&aacute;s</trd>
  <trdgrp lng="pl">
    <trd>blask</trd>,
    <trd>l&sacute;nienie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iara</trd>,
    <trd>lesk</trd>,
    <trd>ligot</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="B"/>anta:
      <ekz>
        <tld/>a festo, kura&gcirc;a&jcirc;o, venko, stilo;
      </ekz>
      <ekz>
        <tld/>e glorigi iun<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_u;&c_ch;&c_y;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_o;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gl&auml;nzend</trd>
    <trd lng="es">brillante</trd>
    <trd lng="fr">brillant <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">ragyog&oacute;</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcz&aogonek;cy</trd>,
      <trd>l&sacute;ni&aogonek;cy</trd>,
      <trd>ja&sacute;niej&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;</trd>,
      <trd>
    &c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
      </trd>,
      <trd>&c_s;&c_i;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>jiskrn&yacute;</trd>,
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>,
    <trd>z&aacute;&rcaron;iv&yacute;</trd>
  </trdgrp>
   <trdgrp lng="en">
      <trd>bright</trd>,
      <trd>shiny</trd>, <trd>shining</trd>, <trd>brilliant</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iariv&yacute;</trd>,
    <trd>leskl&yacute;</trd>,
    <trd>ligotav&yacute;</trd>,
    <trd>skvel&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bril.re0i">
  <kap>re<tld/>i</kap>
  <snc mrk="bril.re0i.FIZ">
    <dif>
      Lume speguli&gcirc;i:
      <ekz>
        la steloj re<tld/>is en la lago.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol; <ind>
  &c_v;&c_o;&c_d;&c_b;&c_l;&c_je;&c_s;&c_k;</ind></trd>
  <trd lng="cs">odr&aacute;&zcaron;et se</trd>
  <trd lng="en">to reflect</trd>
  <trdgrp lng="fr">
    <trd>se <ind>refl&eacute;ter</ind></trd>,
    <trd>se <ind>r&eacute;fl&eacute;chir</ind> <klr tip="ind">
    (se refl&eacute;ter)</klr></trd>,
    <trd>miroiter</trd>
  </trdgrp>
  <trd lng="hu">visszat&uuml;kr&ouml;z&odblac;dik</trd>
  <trdgrp lng="pl">
    <trd>odb&lstroke;yskiwa&cacute;</trd>,
    <trd>puszcza&cacute; zaj&aogonek;czki<klr> (lusterkiem)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; <ind>
  &c_o;&c_t;&c_b;&c_l;&c_je;&c_s;&c_k;</ind></trd>
  <trd lng="sk">odr&aacute;&zcaron;a&tcaron; sa</trd>
</drv>

<drv mrk="bril.trem0i">
  <kap>trem<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.trem0i.FIZ">
    <dif>
      Intermite &jcirc;eti lumbriletojn:
      <ekz>
        la sunlumo trem<tld/>is en la <klr>...</klr> kupoloj de la 
        pre&gcirc;ejoj <fnt><bib>Voja&gcirc;Imp</bib>, 
        <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>;
      </ekz>
      <ekz>
        la steloj trem<tld/>is en la silenta nokto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blikat</trd>,
    <trd>mrkat</trd>,
    <trd>m&zcaron;ikat</trd>,
    <trd>t&rcaron;pytit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flimmern</trd>,
    <trd>glitzern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to sparkle</trd>,
    <trd>glitter</trd>
  </trdgrp>
  <trd lng="es">parpadear<klr> (una luz)</klr></trd>
  <trd lng="fr">scintiller</trd>
  <trdgrp lng="hu">
    <trd>villog</trd>,
    <trd>csillog</trd>,
    <trd>vill&oacute;dzik</trd>
  </trdgrp>
  <trd lng="nl">flikkeren</trd>
  <trdgrp lng="pl">
    <trd>migota&cacute;</trd>,
    <trd>b&lstroke;yska&cacute;</trd>,
    <trd>pe&lstroke;ga&cacute;<klr> (o ogniu)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_c;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kmita&tcaron;</trd>,
    <trd>trblieta&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
