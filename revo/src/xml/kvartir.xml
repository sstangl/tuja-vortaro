<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvartir.xml,v 1.5 2015/12/30 20:10:18 revo Exp $">
<kap>
  <rad>kvartir</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kvartir.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvartir.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="kanton1.0o.MIL">kantonmento</ref>
    <ekz>
      Kie ili mar&scirc;is, haltis en <tld/>o, /
      Ofte la knabinoj ploris &ccirc;e l' foriro
      <fnt>
        <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
        <vrk>Johano la brava</vrk>,
        <lok>1984</lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="ca">acantonament</trd>
  <trd lng="cs">ubytov&aacute;n&iacute; vojska</trd>
  <trd lng="de">Truppenunterkunft</trd>
  <trdgrp lng="en">
    <trd>billet</trd>,
    <trd>cantonment</trd>
  </trdgrp>
  <trd lng="fr">cantonnement<klr tip="ind"> (de troupe)</klr></trd>
  <trdgrp lng="hu">
    <trd>kv&aacute;rt&eacute;ly <klr>(katonai)</klr></trd>,
    <trd>sz&aacute;ll&aacute;sk&ouml;rlet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accantonamento<klr> (mil.)</klr></trd>,
    <trd>alloggiamento<klr> (mil.)</klr></trd>,
    <trd>quartiere<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">kantonnement</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr>(&c_v;&c_o;&c_i;&c_n;&c_s;&c_k;&c_o;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i; &c_v; &c_n;&c_a;&c_s;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_m;
  &c_p;&c_u;&c_n;&c_k;&c_t;&c_je;)</klr></trd>
  <trd lng="sk">prechodn&yacute; vojensk&yacute; byt</trd>
</drv>

<drv mrk="kvartir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kvartir.0i.MIL">
    <ref tip="dif" cel="kanton1.0i.MIL">kantonmenti</ref>
      <ekz>
        unu el <klr>[la]</klr> batalionoj ek<tld/>is en Kol&iacute;n
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
  </snc>
</drv>
</art>

