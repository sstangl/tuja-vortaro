<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hamburg.xml,v 1.14 2013/09/23 19:10:34 revo Exp $">
<kap>
  <rad>Hamburg</rad>/o
</kap>

<drv mrk="hamburg.H0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="hamburg.H0o.urbo">
    <dif>
      Havena <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      en norda <ref
      tip="malprt" cel="german1.G0ujo">Germanujo</ref> (9&amp;ring58'E,
      53&amp;ring33'N), situanta &ccirc;e la rivero <ref tip="vid"
      cel="elb.E0o">Elbo</ref>, regurbo de la federacia
      lando Hamburgo.
    </dif>
    <trd lng="de">Hamburg</trd>
    <trd lng="es">Hamburgo</trd>
    <trd lng="lat">Hamburgum</trd>
  </snc>
  <snc mrk="hamburg.H0o.sxtato">
    <dif>
      Unu el la federaciaj &scirc;tatoj de <ref tip="malprt"
      cel="german1.G0ujo">Germanujo</ref>.
      Regurbo kaj plej granda urbo estas Hamburgo.
    </dif>
    <trd lng="de"><klr>(Stadtstaat) </klr>Hamburg</trd>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trdgrp lng="fr">
    <trd>Hambourg</trd>
  </trdgrp>
  <trd lng="de">Hamburg</trd>
  <trd lng="hu">Hamburg</trd>
  <trd lng="it">Amburgo</trd>
  <trd lng="nl">Hamburg</trd>
  <trd lng="pl">Hamburg</trd>
  <trd lng="pt">Hamburgo</trd>
  <trd lng="ru">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
</drv>

<drv mrk="hamburg.h0ano">
  <kap><tld lit="h"/>ano</kap>
  <snc>
    <dif>
      Civitano de Hamburgo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_a;&c_c;</trd>
  <trd lng="de">Hamburger</trd>
  <trdgrp lng="fr">
    <trd>hambourgeois<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">hamburgi</trd>
  <trd lng="it">amburghese<klr> (sost.)</klr></trd>
  <trd lng="nl">Hamburger</trd>
  <trd lng="pl">mieszkaniec <ind>Hamburga</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_je;&c_c;</trd>
</drv>
</art>
