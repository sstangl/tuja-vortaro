<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: somnol.xml,v 1.4 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>somnol</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="somnol.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="somnol.0i.KOMUNE">
    <ref tip="dif" cel="dorm.duon0i.KOMUNE">duondormi</ref>
    <ekz>
      &gcirc;i aspektas iom senvive, sed &gcirc;i nur <tld/>as
      <fnt>
        <aut>J. Camacho</aut>,
        <vrk>La Majstro kaj Martinelli</vrk>,
        <lok>1993</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>klímat</trd>,
    <trd>podřimovat</trd>
  </trdgrp>
  <trd lng="sk">podriemkavať</trd>
</drv>

<drv mrk="somnol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="somnol.0o.KOMUNE">
    <ref tip="dif" cel="dorm.duon0o.KOMUNE">duondormo</ref>
    <ref tip="vid" cel="letarg.0o.FIG">letargio<sncref/></ref>
    <ref tip="vid" cel="torpor.0o.KOMUNE">torporo</ref>
    <ekz>
      ankora&ubreve; en <tld/>o, &scirc;i trenis sin al la fenestro
      <fnt>
        <aut>M. Fern&aacute;ndez</aut>,
        <vrk>Du Gardenioj</vrk>,
        <lok>Ekstremoj, 1997</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>dřímota</trd>,
    <trd>polospánek</trd>
  </trdgrp>
  <trd lng="sk">polospánok</trd>
</drv>
</art>

