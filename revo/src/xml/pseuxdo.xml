<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pseuxdo.xml,v 1.4 2015/09/18 14:10:17 revo Exp $">
<kap>
    <rad>pse&ubreve;do</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pseuxdo.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="pseuxdo.0.KOMUNE">
    <dif>
      <ref tip="vid" cel="fals.0a">Falsa</ref>, nur &scirc;ajna, nea&ubreve;tenta:
      <ekz>
        <tld/>instruitaj sofistoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        senenhava <tld/>scienca babilado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>patriotismo</ind><fnt>Z</fnt>;
        <trdgrp lng="be">
          <trd>&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_p;&c_a;&c_t;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_z;&c_m;</trd>,
          <trd>&c_ib;&c_l;&c_zh;&c_e;&c_p;&c_a;&c_t;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_z;&c_m;</trd>
        </trdgrp>
        <trd lng="en">pseudo-patriotism</trd>
        <trd lng="hu">&aacute;lhazafis&aacute;g</trd>        
        <trdgrp lng="ru">
          <trd>&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_p;&c_a;&c_t;&c_r;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>,
          <trd>&c_l;&c_zh;&c_je;&c_p;&c_a;&c_t;&c_r;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>sufikso</ind><fnt>Z</fnt>;
        <trd lng="en">pseudo-suffix</trd>
      </ekz>
      <ekz>
        <ind><tld/>nomo</ind>
        <klr>(pse&ubreve;donimo)</klr>;
        <trd lng="be">&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_n;&c_ib;&c_m;</trd>
        <trd lng="en">pseudonym</trd>
        <trd lng="hu">&aacute;ln&eacute;v</trd>
        <trd lng="pl">pseudonim</trd>
        <trd lng="ru">&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_n;&c_i;&c_m;</trd>
      </ekz>
      <ekz>
        <ind><tld/>scienca</ind><fnt>Z</fnt>.
        <trdgrp lng="be">
          <trd>&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_y;</trd>,
          <trd>&c_ib;&c_l;&c_zh;&c_e;&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_y;</trd>
        </trdgrp>
        <trd lng="en">pseudo-scientific</trd>
        <trd lng="hu">&aacute;ltudom&aacute;nyos</trd>
        <trd lng="pl">pseudonaukowy</trd>
        <trdgrp lng="ru">
          <trd>&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_n;&c_a;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_l;&c_zh;&c_je;&c_n;&c_a;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pseuxd.0a.KOMUNE">pse&ubreve;da</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_ib;&c_l;&c_zh;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_ib;&c_l;&c_zh;&c_e;-</trd>,
    <trd>&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;-</trd>
  </trdgrp>
  <trd lng="ca">pseudo-</trd>
  <trd lng="en">pseudo-</trd>
  <trd lng="fr">pseudo-</trd>
  <trdgrp lng="hu">
    <trd>&aacute;l-</trd>,
    <trd>&aacute;ls&aacute;gos</trd>,
    <trd>hamis</trd>
  </trdgrp>
  <trd lng="nl">pseudo-</trd>
  <trdgrp lng="pl">
    <trd>pseudo-</trd>,
    <trd>niby-</trd>,
    <trd>p&oacute;&lstroke;-</trd>,
    <trd>prawie-</trd>
  </trdgrp>
  <trd lng="pt">pseud(o)-</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;-</trd>
  </trdgrp>
</drv>
</art>

