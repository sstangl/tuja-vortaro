<kap>
  <rad>dekalog</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="dekalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="dekalo.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      La dek religiaj instruoj, kiujn la Biblio prezentas kiel
      diktitajn de Javeo al Moseo, kaj kiuj i&gcirc;is baza le&gcirc;o
      de la
      hebrea popolo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_k;&c_a;&c_l;&c_jo;&c_g;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_s;&c_ja;&c_c;&c_mol;
    &c_z;&c_a;&c_p;&c_a;&c_v;&c_je;&c_d;&c_z;&c_ja;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">dec&agrave;leg</trd>
  <trdgrp lng="cs">
    <trd>dekalog</trd>,
    <trd>desatero přikázání</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dekalog</trd>,
    <trd>Zehn Gebote</trd>
  </trdgrp>
  <trd lng="en">decalogue</trd>
  <trd lng="es">dec&aacute;logo</trd>
  <trd lng="fa">&a_d;&a_h;&zwnj;&a_f;&a_r;&a_m;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;calogue</trd>,
    <trd>dix commandements</trd>
  </trdgrp>
  <trd lng="hu">t&iacute;zparancsolat</trd>
  <trdgrp lng="id">
    <trd>dekalog</trd>,
    <trd>Sepuluh Perintah Tuhan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Dekalog</trd>,
    <trd>Dziesi&eogonek;&cacute; Przykaza&nacute;<klr tip="amb">
    Bo&zdot;ych</klr></trd>
  </trdgrp>
  <trd lng="pt">dec&aacute;logo</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_mol;
  &c_z;&c_a;&c_p;&c_o;&c_v;&c_je;&c_d;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>desa&tcaron; <ind>prik&aacute;zan&iacute;</ind></trd>,
    <trd>desať prikázaní <klr>(božích)</klr></trd>
  </trdgrp>
</drv>
</art>
