<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: emerit1.xml,v 1.11 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>emeritur</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="emerit1.0o">
<kap><tld/>o</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Emerita pensio.  
</dif>
</snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>důchod</trd>,
    <trd>odpočinek</trd>,
    <trd>penze <klr>(stav)</klr></trd>,
    <trd>výslužba</trd>
  </trdgrp>
  <trdgrp lng="de">
           <trd>Ruhegehalt</trd>,
           <trd>Pension</trd>
</trdgrp>
  <trdgrp lng="es">
  <trd>pensi&oacute;n de jubilaci&oacute;n</trd>,
  <trd>jubilaci&oacute;n<klr tip="ind">
(pensi&oacute;n)</klr></trd></trdgrp>
  <trdgrp lng="fr">
  <trd>pension de retraite</trd>,
  <trd>retraite<klr tip="ind"> (pension)</klr></trd></trdgrp>
  <trd lng="pl">emerytura</trd>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_i;&c_ja; <klr>(&c_p;&c_o;
&c_v;&c_y;&c_s;&c_l;&c_u;&c_g;&c_je; &c_l;&c_je;&c_t;)</klr></trd>
  <trdgrp lng="sk">
    <trd>dôchodok</trd>,
    <trd>penzia</trd>,
    <trd>výslužné</trd>
  </trdgrp>
</drv>
</art>
