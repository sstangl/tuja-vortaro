<kap>
  <rad>Fabian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fabian.0o">
  <kap><tld/>o</kap>
  <snc mrk="fabian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>,
      varia&jcirc;o de <ref tip="vid" cel="fabi.0o">Fabio</ref>:
      <ekz>
        <tld/>o komencis elakvigadi malgrandajn fi&scirc;etojn
        el la akvario de la patro
        <fnt>
          <aut>V. V&aacute;&ncaron;a</aut>,
          <vrk>Kiel ni Mortigadis Leporon...</vrk>,
          <lok>1961</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Fabi&agrave;</trd>
    <trd lng="fr">Fabien</trd>
    <trd lng="pl">Fabian</trd>
  </snc>
</drv>
</art>

