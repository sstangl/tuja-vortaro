        skribi <klr>...</klr> al vi, plej eminenta <tld/>o
        <fnt><bib>NT</bib>, <lok>&Luk; 1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Te&ograve;fil</trd>
  <trd lng="fr">Th&eacute;ophile</trd>
  <trd lng="it">Teofilo</trd>
  <trd lng="pl">Teofil</trd>
