<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: plezur.xml,v 1.19 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plezur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="plezur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plezur.0o.KOMUNE">
    <dif>
      Agrabla sento de bonstato, komforteco, &gcirc;ojo, kiun naskas
      kontentigo de deziro a&ubreve; inklino:
      <ekz>
        via vizito faris al mi <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        doni al iu <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        havi, trovi, senti, ricevi grandan <tld/>on en io<fnt>Z</fnt>,
      </ekz>
      <ekz>
        per io, &ccirc;e io<fnt>Z</fnt>,
      </ekz>
      <ekz>
        el io;
      </ekz>
      <ekz>
        ek&gcirc;ui <tld/>on vidante...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volupt.0o">volupto</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>plaer</trd>,
    <trd>delit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pot&ecaron;&scaron;en&iacute;</trd>,
    <trd>potěšení</trd>,
    <trd>radost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vergn&uuml;gen</trd>,
    <trd>Freude</trd>,
    <trd>Wohlgefallen</trd>,
    <trd>Behagen</trd>
  </trdgrp>
  <trd lng="en">pleasure</trd>
  <trd lng="fr">plaisir</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;m</trd>,
    <trd>gy&ouml;ny&ouml;r</trd>
    <trd>&eacute;lvezet</trd>,
    <trd>megel&eacute;gedetts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>plezier</trd>,
    <trd>genoegen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prazer</trd>,
    <trd>deleite</trd>,
    <trd>del&iacute;cia</trd>,
    <trd>regalo</trd>,        
    <trd>gosto</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>potešenie</trd>,
    <trd>radosť</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0a">
  <kap><tld/>a</kap>
  <snc mrk="plezur.0a.KOMUNE">
    <dif>
      Plena je <tld/>o, akompanata de <tld/>o, agrabla.
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>plaent</trd>,
      <trd>agradable</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vergn&uuml;glich</trd>,
      <trd>vergn&uuml;gt</trd>,
      <trd>angenehm</trd>
    </trdgrp>
    <trd lng="fr">agr&eacute;able</trd>
    <trd lng="nl">plezierig</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="plezur.0a.amuza">
    <dif>
      Celanta nur <tld/>on:
      <ekz>
        <tld/>a voja&gcirc;o;
      </ekz>
      <ekz>
        <tld/>aj artoj;
      </ekz>
      <ekz>
        <ref tip="vid" cel="gxarde.plezur0o.HOR"><tld/>&gcirc;ardeno</ref>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>de plaer</trd>,
      <trd>per plaer</trd>
    </trdgrp>
    <trd lng="fr">d'<ind>agr&eacute;ment</ind></trd>
    <trd lng="nl">plezier-</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>radostný</trd>,
    <trd>zábavný</trd>
  </trdgrp>
  <trd lng="en">pleasurable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;r&ouml;mteli</trd>,
    <trd>gy&ouml;ny&ouml;rteli</trd>
    <trd>kellemes</trd>,
    <trd>&eacute;lvezetes</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>radostný</trd>,
    <trd>zábavný</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="plezur.0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <tld/>iga akcepto.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delectable</trd>
  <trdgrp lng="cs">
    <trd>radostný</trd>,
    <trd>zábavný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vergn&uuml;gen bereitend</trd>,
    <trd>erquickend</trd>
  </trdgrp>
  <trd lng="en">enjoyable</trd>
  <trd lng="fr">d&eacute;licieux</trd>
  <trdgrp lng="hu">
    <trd>&eacute;lvezetes</trd>,
    <trd>gy&ouml;ny&ouml;rk&ouml;dtet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">prettig</trd>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>radostný</trd>,
    <trd>zábavný</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="plezur.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on al...
    </dif>
  </snc>
  <trd lng="ca">delectar</trd>
  <trd lng="cs">potěšit</trd>
  <trd lng="de">Vergn&uuml;gen bereiten</trd>
  <trd lng="en">gratify</trd>
  <trd lng="fr">faire <ind>plaisir</ind> &agrave;</trd>
  <trd lng="hu">gy&ouml;ny&ouml;rk&ouml;dtet</trd>
  <trd lng="nl">plezier doen</trd>
  <trd lng="oc">agradar</trd>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
  <ind>&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</ind></trd>
  <trd lng="sk">potešiť <klr>(niekoho)</klr></trd>
</drv>

<drv mrk="plezur.0i">
  <kap><tld/>i, <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <snc>
    <dif>
      Senti <tld/>on:
      <ekz>
        la infanoj <tld/>as &ccirc;e la mirrakontoj;
      </ekz>
      <ekz>
        &ccirc;e la malfeli&ccirc;o de via malamiko ne <tld/>i&gcirc;u<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delectar-se en</trd>
  <trd lng="en">take pleasure in</trd>
  <trd lng="fr">avoir <ind>plaisir</ind></trd>
</drv>

<drv mrk="plezur.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Malagrabla&jcirc;o, &ccirc;agreno:
      <ekz>
        pro la multaj mal<tld/>oj li grizi&gcirc;is<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dorn.0o">dorno</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">disgust</trd>
  <trdgrp lng="cs">
    <trd>mrzutost</trd>,
    <trd>nepříjemnost</trd>
  </trdgrp>
  <trd lng="de">Missvergn&uuml;gen</trd>
  <trd lng="en">displeasure</trd>
  <trd lng="fr">d&eacute;plaisir</trd>
  <trdgrp lng="hu">
    <trd>f&aacute;jdalom</trd>,
    <trd>keser&udblac;s&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">misnoegen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_u;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mrzutosť</trd>,
    <trd>nepríjemnosť</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="plezur.mal0a.KOMUNE">
    <dif>
      Sena je <tld/>o, malagrabla:
      <ekz>
        la bastonfrapoj komencis tamburi mal<tld/>an
        ritmon sur la dorso
        <fnt>
          <aut>M. Wajsblum</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5703/kankro.html">
          Kankro ne estas fi&scirc;o...</url></vrk>,
          <lok>Norda Prismo, 1957:3, p. 172a-174a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;sagr&eacute;able</trd>
</drv>

<drv mrk="plezur.mal0iga">
  <kap>mal<tld/>iga</kap>
  <snc mrk="plezur.mal0iga.KOMUNE">
    <dif>
      Ka&ubreve;zanta mal<tld/>on:
      <ekz>
        pardonu min, se mi diros ion mal<tld/>igan
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;plaisant</trd>,
    <trd>f&acirc;cheux</trd>
  </trdgrp>
</drv>

<drv mrk="plezur.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plezur.mal0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi mal<tld/>on al...:
      <ekz>
        ne mal<tld/>igu min post la penado, kiun mi suferis
        <fnt>
          <aut>-</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://tatoeba.org/">
          Tatoeba, [vidita en 2014]</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>znechutit</trd>,
    <trd>znepříjemnit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contrarier</trd>,
    <trd>f&acirc;cher</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>znechutiť</trd>,
    <trd>znepríjemniť</trd>
  </trdgrp>
</drv>
</art>
