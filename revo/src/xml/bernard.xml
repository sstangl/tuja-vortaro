<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bernard.xml,v 1.9 2015/02/26 20:11:36 revo Exp $">
<kap>
  <rad>Bernard</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bernard.0o">
  <kap><tld/>o</kap>
  <snc mrk="bernard.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;erman-devena:
      <ekz>
        <tld/>o el Turingujo en la jaro 960 anta&ubreve;diris proksiman
        finon de
        la mondo
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>            
  <trd lng="bg">&c_B;&c_je;&c_r;&c_n;&c_h;&c_a;&c_r;&c_d;</trd>             
  <trd lng="br">Bernez</trd>
  <trd lng="de">Bernhard</trd>
  <trd lng="es">Bernardo</trd>
  <trd lng="fr">Bernard</trd>
  <trd lng="it">Bernardo</trd>
  <trd lng="pl">Bernard</trd>
  <trd lng="ru">&c_B;&c_je;&c_r;&c_n;&c_a;&c_r;&c_d;</trd>
</drv>
</art>

