<kap>
  <ofc>*</ofc>
  <rad>cirkuler</rad>/o
</kap>
<drv mrk="cirkul1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Komuna <ref cel="aviz.0o">aviza letero</ref> dissendata al multaj
personoj:
      <ekz>
        mi sendis kaj ricevis centojn da retpo&scirc;taj leteroj,
bultenoj kaj
        <tld/>oj de post 1992<fnt><aut>Claude Rouget</aut>:
<vrk>Internet
        ne (tiom) malbonas</vrk>, <bib>Monato</bib><lok>jaro 1997a,
numero
        8a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bulten.0o">bulteno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trd lng="br">kelc'hlizher</trd>
  <trd lng="ca">circular</trd>
  <trdgrp lng="cs">
    <trd>cirkulář</trd>,
    <trd>oběžník</trd>
  </trdgrp>
  <trd lng="en">circular<klr> (letter, notice)</klr></trd>
  <trd lng="es">circular</trd>
  <trd lng="fr">circulaire<klr> (avis)</klr></trd>
  <trd lng="hu">k&ouml;rlev&eacute;l</trd>
  <trd lng="it">circolare<klr> (informativa)</klr></trd>
  <trd lng="nl">circulaire</trd>
  <trd lng="pl">ok&oacute;lnik</trd>
  <trd lng="pt">circular</trd>
  <trd lng="ru">&c_c;&c_i;&c_r;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trd lng="sk">obe&zcaron;n&iacute;k</trd>
</drv>
</art>
