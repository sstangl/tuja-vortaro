<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: herold.xml,v 1.18 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>herold</rad>/o
</kap>
<drv mrk="herold.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Regna militisto a&ubreve; oficisto, kies rolo estas anonci grava&jcirc;ojn
      (proklamo de kronado, de milito ktp):
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas &ccirc;iam la <tld/>o de sia propra gloro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stafet.0o">stafeto</ref>,
      <ref cel="kurier.0o">kuriero</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_r;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>herold</trd>,
    <trd>hlasatel</trd>,
    <trd>posel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausrufer</trd>,
    <trd>Verk&uuml;ndiger</trd>,
    <trd>Herold</trd>
  </trdgrp>
  <trd lng="en">herald</trd>
  <trd lng="es">heraldo</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;raut</trd>,
    <trd>chantre</trd>,
    <trd>messager</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&ouml;k</trd>,
    <trd>kiki&aacute;lt&oacute;</trd>,
    <trd>herold</trd>
  </trdgrp>
  <trd lng="it">araldo</trd>
  <trd lng="nl">heraut</trd>
  <trd lng="pt">arauto</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_r;&c_o;&c_l;&c_mol;&c_d;</trd>,
    <trd>&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hlásateľ</trd>,
    <trd>posol</trd>
  </trdgrp>
</drv>

<drv mrk="herold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>o diskonigi:
      <ekz>
        <tld/>i la famon de eltrovinto;
      </ekz>
      <ekz>
        la suno <tld/>is la festan tagon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">disfamigi</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="glor.0i">glori</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">hlásat</trd>
  <trdgrp lng="de">
    <trd>ausrufen</trd>,
    <trd>verk&uuml;ndigen</trd>
  </trdgrp>
  <trd lng="en">herald</trd>
  <trdgrp lng="fr">
    <trd>annoncer</trd>,
    <trd>se faire le <ind>h&eacute;raut</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiki&aacute;lt</trd>,
    <trd>kihirdet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>annunciare</trd>,
    <trd>proclamare</trd>
  </trdgrp>
  <trd lng="nl">uitbazuinen</trd>
  <trd lng="pt">anunciar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hlásať</trd>,
    <trd>zvestovať</trd>
  </trdgrp>
</drv>
</art>
