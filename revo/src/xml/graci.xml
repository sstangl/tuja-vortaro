<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: graci.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>graci</rad>/a
</kap>
<drv mrk="graci.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref>, pla&ccirc;a per facilaj
      kaj harmoniaj movoj a&ubreve; bele proporciaj formoj:
      <ekz>
        <tld/>a virino, vazo, stilo;
      </ekz>
      <ekz>
        <tld/>aj lipoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delika.0a">delikata</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graciézní</trd>,
    <trd>graciózní</trd>,
    <trd>křehký</trd>,
    <trd>ladný</trd>,
    <trd>půvabný</trd>,
    <trd>ztepilý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grazi&ouml;s</trd>,
    <trd>anmutig</trd>,
    <trd>zierlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>graceful</trd>
  </trdgrp>
  <trd lng="es">gracioso, -a<klr> (bello, hermoso)</klr></trd>
  <trdgrp lng="fr">
    <trd>gracieux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kecses</trd>,
    <trd>b&aacute;jos</trd>,
    <trd>k&ouml;nnyed mozg&aacute;s&uacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gracieus</trd>,
    <trd>bevallig</trd>,
    <trd>bekoorlijk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pe&lstroke;ny <ind>gracji</ind></trd>,
    <trd>zgrabny</trd>,
    <trd>powabny</trd>,
    <trd>przystojny</trd>,
    <trd>nadobny</trd>,
    <trd>lekki</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gracioso</trd>,
   <trd>esbelto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_i;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pôvabný</trd>
  <trdgrp lng="tr">
    <trd>incelikli</trd>,
    <trd>zarafetli</trd>,
     <trd>l&uuml;tuflu</trd>
  </trdgrp>
</drv>

<drv mrk="graci.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        <tld/>e prezenti tason al vizitanto;
      </ekz>
      <ekz>
        li saltas <tld/>e, kiel urso ebria
        <klr>(ironie)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graciézně</trd>,
    <trd>graciózně</trd>,
    <trd>ladně</trd>,
    <trd>půvabně</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grazi&ouml;s</trd>,
    <trd>anmutig</trd>,
    <trd>zierlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gracefully</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>graciosamente</trd>,
    <trd>con gracia</trd>
  </trdgrp>
  <trd lng="fr">gracieusement<klr tip="ind"> (avec gr&acirc;ce)</klr>
  </trd>
  <trdgrp lng="hu">
    <trd>kecsesen</trd>,
    <trd>b&aacute;josan</trd>,
    <trd>k&ouml;nnyed&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sierlijk</trd>,
    <trd>met <ind>gratie</ind></trd>,
    <trd>op <ind>hoffelijke</ind> wijze</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z <ind>gracj&aogonek;</ind></trd>,
    <trd>zgrabnie</trd>,
    <trd>powabnie</trd>,
    <trd>przystojnie</trd>,
    <trd>nadobnie</trd>,
    <trd>lekko</trd>
  </trdgrp>
  <trd lng="pt">graciosamente</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_i;&c_o;&c_z;&c_n;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>graciózne</trd>,
    <trd>lahodne</trd>,
    <trd>pôvabne</trd>
  </trdgrp>
  <trd lng="tr">zerafetle</trd>
</drv>

<drv mrk="graci.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito de iu a&ubreve; io <tld/>a:
      <ekz>
        <tld/>eco de infana viza&gcirc;o, de antikva statufragmento.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graciéznost</trd>,
    <trd>gracióznost</trd>,
    <trd>ladnost</trd>,
    <trd>půvab</trd>,
    <trd>ztepilost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Grazie</trd>,
    <trd>Anmut</trd>,
    <trd>Liebreiz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gracefulness</trd>
  </trdgrp>
  <trd lng="es">gracia<klr> (elegancia)</klr></trd>
  <trd lng="fr">gr&acirc;ce<klr tip="ind"> (charme)</klr></trd>
  <trdgrp lng="hu">
    <trd>kecsess&eacute;g</trd>,
    <trd>b&aacute;joss&aacute;g</trd>,
    <trd>k&ouml;nnyeds&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gratie</trd>,
    <trd>bevalligheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gracja</trd>,
    <trd>zgrabno&sacute;&cacute;</trd>,
    <trd>powab</trd>,
    <trd>przystojno&sacute;&cacute;</trd>,
    <trd>nadobno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">graciosidade</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_c;&c_i;&c_o;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol;
    </trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>graciozita</trd>,
    <trd>pôvabnosť</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>incelik</trd>,
    <trd>zarafet</trd>,
     <trd>l&uuml;tuf</trd>
  </trdgrp>
</drv>

<drv mrk="graci.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta senharmoniajn movojn a&ubreve; formojn.
    </dif>
    <ref tip="vid" cel="plump.0a">plumpa</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_h;&c_l;&c_ja;&c_m;&c_ja;&c_zh;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nemotorný</trd>,
    <trd>šeredný</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>plump</trd>,
    <trd>derb</trd>,
    <trd>schwerf&auml;llig</trd>,
    <trd>ungeschlacht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>awkward</trd>,
    <trd>clumsy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>torpe</trd>,
    <trd>feo</trd>
  </trdgrp>
  <trd lng="fr">disgracieux</trd>
  <trdgrp lng="hu">
    <trd>mack&oacute;s</trd>,
    <trd>otromba</trd>,
    <trd>neh&eacute;zkes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onbekoorlijk</trd>,
    <trd>onbevallig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>oci&eogonek;&zdot;a&lstroke;y</trd>,
    <trd>niezgrabny</trd>
  </trdgrp>
  <trd lng="pt">desgracioso</trd>
  <trd lng="ru">&c_n;&c_je;&c_u;&c_k;&c_l;&c_ju;&c_zh;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nemotorný</trd>,
    <trd>nešikovný</trd>,
    <trd>ťarbavý</trd>
  </trdgrp>
</drv>

<drv mrk="graci.G0o">
  <kap><tld lit="G"/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la tri antikvaj dia&jcirc;oj, reprezentantaj la plej
      gracian belecon.
    </dif>
    <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
    <trd lng="en">Grace</trd>
    <trd lng="fr">Gr&acirc;ce<klr> (les 3 ~s)</klr></trd>
    <trd lng="hu">Gr&aacute;cia</trd>
    <trd lng="nl">gratie</trd>
    <trd lng="pl">Gracja</trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>graciéznost</trd>,
    <trd>gracióznost</trd>,
    <trd>ladnost</trd>,
    <trd>půvab</trd>,
    <trd>ztepilost</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>graciozita</trd>,
    <trd>pôvabnosť</trd>
  </trdgrp>
</drv>
</art>
