<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sair.xml,v 1.7 2009/09/20 16:31:06 revo Exp $">
<kap>
  <rad>sair</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sair.0o">
  <kap><tld/>o</kap>
  <snc mrk="sair.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Marfi&scirc;o proksimume 30 centimetrojn longa, kun bekformaj makzeloj
      kaj malhela karno (<trd lng="la">Cololabis saira</trd>):
      <ekz>
        iu soldato <klr>...</klr> alportis ladskatolon da <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;iun tagon por mortigi
        tiun
        memoron&rightquot;, p. 239a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pazifischer Makrelenhecht</trd>,
    <trd>Kurzschnabelmakrelenhecht</trd>
  </trdgrp>
  <trd lng="en">pacific saury</trd>
  <trd lng="fr">balaou du japon</trd>
  <trd lng="hu">szaira</trd>
  <trd lng="it">luccio sauro <klr>del pacifico</klr></trd>
  <trd lng="nl">Japanse <ind>makreelgeep</ind></trd>
  <trd lng="pt">agulh&atilde;o-do-jap&atilde;o</trd>
  <trd lng="ru">&c_s;&c_a;&c_j;&c_r;&c_a;</trd>
</drv>
</art>

