<art mrk="$Id: anased.xml,v 1.7 2013/08/31 08:11:06 revo Exp $">
<kap>
  <rad>anased</rad>/oj
</kap>
<drv mrk="anased.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="anser.0oformaj">anseroformaj</ref>
      birdoj (<trd lng="la">Anatidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">an&agrave;tids</trd>
  <trd lng="de">Enten</trd>
  <trd lng="fr">anatid&eacute;s</trd>
  <trd lng="hu">r&eacute;cef&eacute;l&eacute;k</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_i;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_u;&c_t;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>

