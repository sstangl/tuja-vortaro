<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kis.xml,v 1.26 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kis</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kis.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kis.0i.FIZL">
    <dif>
      Tu&scirc;i per la lipoj por esprimi amon a&ubreve; respekton:
      <ekz>
        <tld/>i la manon al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i iun sur la vangoj;
      </ekz>
      <ekz>
        <tld/>i krucifikson;
      </ekz>
      <ekz>
        batu malbonulon, li vin flatos, <tld/>u, li vin batos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">besar</trd>
    <trdgrp lng="es">
      <trd>dar un beso</trd>,
      <trd>besar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>embrasser<klr tip="ind"> (avec les l&egrave;vres)</klr></trd>,
      <trd>donner un <ind>baiser</ind> (&agrave;)</trd>,
      <trd>faire la <ind>bise</ind> (&agrave;)</trd>,
      <trd>baiser<klr tip="ind"> (embrasser)</klr></trd>,
    </trdgrp>
    <trd lng="he">&lamed;<ind>&nun;&shin;&qof;</ind></trd>
    <trd lng="tr">&ouml;pmek</trd>
  </snc>
  <snc mrk="kis.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Milde, delikate tu&scirc;eti:
      <ekz>
        la ondoj al<tld/>as la kolonojn;
      </ekz>
      <ekz>
        la bran&ccirc;oj <tld/>as la klaran spegulon de la akvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lastaj radioj de la suno silente <tld/>is la supron de la
        montoj.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>acaronar</trd>,
      <trd>amanyagar</trd>
    </trdgrp>
    <trd lng="es">acariciar<klr> (fig.)</klr></trd>
    <trdgrp lng="fr">
      <trd>effleurer</trd>,
      <trd>caresser<klr tip="ind"> (effleurer)</klr></trd>,
      <trd>l&eacute;cher<klr tip="ind"> (effleurer)</klr></trd>
    </trdgrp>
    <trd lng="he">&lamed;&nun;&shin;&vav;&qof; &lamed;- \<ind>&nun;&shin;&qof;</ind>\</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_c;&c_je;&c_l;&c_u;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>hubičkovat</trd>,
    <trd>líbat</trd>
  </trdgrp>
  <trd lng="de">k&uuml;ssen</trd>
  <trd lng="en">kiss</trd>
  <trd lng="fa">&a_b;&a_w;&a_s;&f_y;&a_d;&a_n;</trd>
  <trd lng="hu">cs&oacute;kol</trd>
  <trd lng="id">[<ind>cium</ind>] mencium</trd>
  <trd lng="nl">kussen<klr tip="ind"> (ww.)</klr></trd>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">bozka&tcaron;</trd>
</drv>

<drv mrk="kis.0o">
  <kap><tld/>o</kap>
  <snc mrk="kis.0o.FIZL">
    <dif>
      Tu&scirc;ado per la lipoj:
      <ekz>
        ordonita <tld/>o havas guston maldol&ccirc;an
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        inter&scirc;an&gcirc;i <tld/>ojn;
      </ekz>
      <ekz>
        premi <tld/>on sur la vangon de iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kovri iun per <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>perfingra <tld/>o</ind><fnt>Z</fnt>;
        <trd lng="nl">kushandje</trd>
      </ekz>
      <ekz>
        <ind><tld/>o de Judaso</ind><fnt>Z</fnt> <klr>(amika&jcirc;o de
        perfidonto)</klr>;
        <trd lng="nl">judaskus</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="kis.0o.FIG">
    <dif>
      Delikata tu&scirc;eto:
      <ekz>
        la arbaron jam tu&scirc;is la flava <tld/>o
        de a&ubreve;tuno<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_a;&c_l;&c_u;&c_n;&c_a;&c_k;</trd>
  <trd lng="bg">&c_c;&c_je;&c_l;&c_u;&c_v;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>bes</trd>,
    <trd>pet&oacute;</trd>,
    <trd>besada</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubička</trd>,
    <trd>polibek</trd>,
    <trd>políbení</trd>,
    <trd>pusa</trd>
  </trdgrp>
  <trd lng="de">Kuss</trd>
  <trd lng="es">beso</trd>
  <trd lng="fa">&a_b;&a_w;&a_s;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>baiser<klr tip="ind"> (subst.)</klr></trd>,
    <trd>b&eacute;cot</trd>,
    <trd>bise<klr tip="ind"> (baiser)</klr></trd>,
    <trd>caresse<klr tip="ind"> (effleurement)</klr></trd>,
    <trd>effleurement</trd>
  </trdgrp>
  <trd lng="he">&nun;&shin;&yod;&qof;&he;</trd>
  <trd lng="hu">cs&oacute;k</trd>
  <trd lng="id"><ind>cium</ind>an</trd>
  <trd lng="nl">kus</trd>
  <trd lng="oc">poton</trd>
  <trd lng="ru">&c_p;&c_o;&c_c;&c_je;&c_l;&c_u;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bozk</trd>,
    <trd>pusa</trd>
  </trdgrp>
</drv>

<drv mrk="kis.inter0i">
  <kap>inter<tld/>i,
  <var><kap>inter<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kis.inter0i.FIZL">
    <dif>
      Reciproke <tld/>i:
      <ekz>
        la pubereco estas tiu tempo, en kiu knaboj kaj
        knabinoj ne scias, &ccirc;u ili interbatu a&ubreve; inter<tld/>u
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/eng/sentences/show/2272373">
          Frazo 2272373a</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ne&hcirc;ludov inter<tld/>i&gcirc;is kun la kampulo
        <fnt>
          <aut>L. Tolstoj, trad. V. Sapo&jcirc;nikov</aut>,
          <vrk><url ref="http://sezonoj.ru/2015/04/tolstoj/">
          Resurekto</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s'<ind>embrasser</ind></trd>
</drv>
</art>
