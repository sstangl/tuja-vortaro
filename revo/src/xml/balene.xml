<art mrk="$Id: balene.xml,v 1.7 2013/08/31 08:10:29 revo Exp $">
<kap>
  <rad>balened</rad>/oj
</kap>
<drv mrk="balene.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Balaenidae</trd>), kun tre granda kapo (triono de
      la
      korpolongo) kaj longaj bartoj.
    </dif>
  </snc>
  <trd lng="ca">bal&egrave;nids</trd>
  <trd lng="de">Glattwale</trd>
  <trd lng="en">whales</trd>
  <trd lng="es">bal&eacute;nidos</trd>
  <trd lng="fr">bal&eacute;nid&eacute;s</trd>
  <trd lng="hu">sima <ind>b&aacute;lnaf&eacute;l&eacute;k</ind></trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_je;
<ind>&c_k;&c_i;&c_t;&c_y;</ind> <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

