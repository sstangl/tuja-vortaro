<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parcel.xml,v 1.14 2015/09/30 05:10:19 revo Exp $">
<kap>
  <rad>parcel</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parcel.0o">
  <kap><tld/>o</kap>
  <snc mrk="parcel.0o.KOMUNE">
    <dif>
      Parto de tero apartigita de tuta bieno:
      <ekz>
        en sia apuddoma <tld/>o el 9 aroj li aran&gcirc;is japanstilan
        &gcirc;ardenon
        <fnt>
          <aut>L. Straznickas</aut>,
          <vrk>Entuziasmulo pri Japanio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 5a, p. 26a</lok>
         </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ter.0peco">terpeco</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>parcela</trd>,
    <trd>pozemek</trd>
  </trdgrp>
  <trd lng="en">parcel<klr> (of land)</klr></trd>
  <trdgrp lng="fr">
    <trd>parcelle<klr> (terrain)</klr></trd>,
    <trd>terrain</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>telek</trd>,
    <trd>parcella</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>perceel</trd>,
    <trd>kavel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>parcela</trd>,
    <trd>dzia&lstroke;ka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parcela <klr>(de terreno)</klr></trd>,
    <trd>lote</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</ind></trd>,
    <trd>&c_n;&c_a;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_c;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pozemok</trd>
</drv>

<drv mrk="parcel.dis0igi">
  <kap>dis<tld/>igi, <var><kap>dis<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="parcel.dis0igi.KOMUNE">
    <dif>
      Dividi bienon en <tld/>ojn.
    </dif>
  </snc>
  <trd lng="en">divide<klr> (land)</klr> into lots</trd>
  <trdgrp lng="fr">
    <trd>lotir<klr> (un terrain)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>parcell&aacute;z</trd>,
    <trd>kim&eacute;r <klr tip="amb">(f&ouml;ldet)</klr></trd>
  </trdgrp>
  <trd lng="nl">verkavelen</trd>
  <trd lng="pl">rozparcelowa&cacute;</trd>
</drv>
</art>
