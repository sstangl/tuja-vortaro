<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: domini.xml,v 1.18 2016/04/12 19:10:16 revo Exp $">
<kap>
  <rad>domini</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="domini.0o">
  <kap><tld/>o</kap>
  <snc mrk="domini.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Eks<ref tip="vid" cel="koloni.0o.lando">kolonio</ref> havanta
      propran 
      registaron, agnoskanta almena&ubreve; formale la anta&ubreve;e
      regantan regnon: 
      <ekz>
        niaj <tld/>oj kaj kolonioj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 3a, &ccirc;apitro 4a</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi diras: &leftquot;La <tld/>oj, Kanado, A&ubreve;stralio,
        Nov-Zelando, 
        Sudafriko!&rightquot;, ili dra&scirc;os min 
        <fnt>
        <bib>Mortul&Scirc;ip</bib>
        <lok>&ccirc;apitro 9a</lok>
        </fnt>.
      </ekz>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_ib;&c_n;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="en">dominion</trd>
  <trdgrp lng="cs">
    <trd>dominion</trd>,
    <trd>dominium</trd>
  </trdgrp>
  <trd lng="fr">dominion</trd>
  <trd lng="pl">dominium</trd>
  <trd lng="pt">dom&iacute;nio</trd>
  <trd lng="sk">dom&iacute;nium</trd>
</drv>
</art>
