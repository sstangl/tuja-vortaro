<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rund.xml,v 1.10 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>rund</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="rund.0i">
  <kap><tld/>i</kap>
  <snc mrk="rund.0i.MIL">
    <dif>
      Rondiri tra loko por &gcirc;in <ref tip="super"
      cel="kontro.0i">kontroli</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kontrollgang machen</trd>,
    <trd>Runde gehen<klr tip="amb"> (kontrollieren)</klr></trd>
  </trdgrp>
  <trd lng="fr">faire une <ind>ronde</ind><klr> (de
  surveillance)</klr></trd>
  <trdgrp lng="hu">
    <trd>j&aacute;r&odblac;r&ouml;z</trd>,
    <trd>cirk&aacute;l</trd>,
    <trd>ellen&odblac;rz&odblac; <ind>k&ouml;rutat</ind> tesz</trd>
  </trdgrp>
  <trd lng="it">fare la <ind>ronda</ind></trd>
  <trdgrp lng="pt">
    <trd>rondar</trd>,
    <trd>fazer a <ind>ronda</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_o;&c_b;&c_h;&c_o;&c_d;</ind></trd>,
    <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="rund.0o">
  <kap><tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="rund.0o.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
      Kontrola vizito, rondiro en iu loko, por sekureco, ordo, apliko de
      reguloj:
      <ekz>
        la junulo foriris, kaj la gubernatoro da&ubreve;rigis sian <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 49a, p.
        668a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kolo sport. zápasu</trd>,
    <trd>obchůzka</trd>,
    <trd>vylučovací střetnutí</trd>,
    <trd>vylučovací zápasy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kontrollgang</trd>,
    <trd>Runde<klr tip="amb"> (Kontrollgang)</klr></trd>
  </trdgrp>
  <trd lng="fr">ronde<klr> (de surveillance)</klr></trd>
  <trdgrp lng="hu">
    <trd>j&aacute;r&odblac;r&ouml;z&eacute;s</trd>,
    <trd>ellen&odblac;rz&odblac; k&ouml;r&uacute;t</trd>
  </trdgrp>
  <trd lng="it">ronda</trd>
  <trd lng="pt">ronda</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_b;&c_h;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">kolo športového zápasu</trd>
</drv>
</art>

