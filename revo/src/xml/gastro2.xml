<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gastro2.xml,v 1.18 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>gastropod</rad>/oj
</kap>
<drv mrk="gastro2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">Klaso</ref> de <ref tip="malprt"
      cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Gastropoda</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_a;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_h;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">gastr&ograve;podes</trd>
  <trdgrp lng="cs">
    <trd>břichonožci</trd>,
    <trd>plži</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnecken</trd>,
    <trd>Bauchf&uuml;&szlig;er</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&sigma;&tau;&epsilon;&rho;&omikron_ton;&pi;&omikron;&delta;&alfa;</trd>
  <trd lng="es">gaster&oacute;podos</trd>
  <trd lng="fr">gast&eacute;ropodes</trd>
  <trdgrp lng="hu">
    <trd>csig&aacute;k<klr tip="ind"> (oszt&aacute;ly)</klr></trd>,
    <trd>hasl&aacute;b&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slakken</trd>,
    <trd>buikpotigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;limaki</trd>,
    <trd>brzuchonogi</trd>
  </trdgrp>
  <trd lng="pt">gastr&oacute;podes</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_h;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">mäkkýše <klr>(slimákovité)</klr></trd>
</drv>
</art>
