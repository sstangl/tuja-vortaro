<kap>
  <ofc>2</ofc>
  <rad>klinik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klinik.0o">
  <kap><tld/>o</kap>
  <snc mrk="klinik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sekcio de <ref tip="malprt" cel="hospit.0o">hospitalo</ref>,
      kie majstro instruas siajn dis&ccirc;iplojn,
      farante anta&ubreve; ili diagnozojn &ccirc;e la litoj de malsanuloj.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">cl&iacute;nica</trd>
  <trd lng="cs">klinika</trd>
  <trdgrp lng="de">
   <trd>Klinik</trd>,
   <trd>Lehrkrankenhaus</trd></trdgrp>
  <trd lng="en">clinic</trd>
  <trd lng="es">cl&iacute;nica</trd>
  <trd lng="fr">clinique</trd>
  <trd lng="it">clinica<klr> (osped. univ.)</klr></trd>
  <trd lng="nl">kliniek</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">klinika</trd>
</drv>
</art>
