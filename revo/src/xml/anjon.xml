<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anjon.xml,v 1.3 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anjon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anjon.0o">
  <kap><tld/>o</kap>
  <snc mrk="anjon.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="jon.0o.KEM">Jono</ref>
      ricevinta negativan elektran &scirc;argon:
      <ekz>
        &ccirc;irka&ubreve; katjono grupi&gcirc;as <tld/>oj kun kontra&ubreve;a &scirc;argo
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://eo.wikibooks.org/wiki/Kondukilo_en_kemion">
          Kondukilo en kemion</url></vrk>,
          <lok>Vikilibroj, 2013-10-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="katjon.0o.KEM">katjono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>anion</trd>,
    <trd>aniont</trd>,
    <trd>záporný ion</trd>
  </trdgrp>
  <trd lng="en">anion</trd>
  <trd lng="fr">anion</trd>
  <trd lng="sk">anión</trd>
</drv>
</art>

