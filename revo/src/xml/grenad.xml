<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grenad.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>grenad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="grenad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grenad.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ferglobo, plenigita je pulvo kaj garnita per me&ccirc;o a&ubreve;
      eksplodigilo,
      kiun oni &jcirc;etas per mano a&ubreve; speciala &jcirc;etilo, por ke &gcirc;i eksplodu
      meze de la malamikoj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="obus.0o">obuso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">granada</trd>
  <trd lng="cs">ruční granát</trd>
  <trd lng="de">Granate</trd>
  <trd lng="en">grenade</trd>
  <trd lng="es">granada</trd>
  <trd lng="fr">grenade<klr tip="ind"> (arme)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;t</trd>
  <trd lng="id">granat</trd>
  <trd lng="it">granata<klr> (mil.)</klr></trd>
  <trd lng="nl">granaat</trd>
  <trd lng="pl">granat <klr>(bro&nacute;)</klr></trd>
  <trd lng="pt">granada</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">ručný granát</trd>
  <trd lng="sv">granat</trd>
</drv>

<drv mrk="grenad.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="grenad.0isto.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Soldato speciale destinata por &jcirc;eti <tld/>ojn.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_a;&c_m;&c_jo;&c_t;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_e;&c_n;&c_a;&c_d;&c_e;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">granader</trd>
  <trdgrp lng="cs">
    <trd>granadýr</trd>,
    <trd>granátník</trd>,
    <trd>grenadýr</trd>
  </trdgrp>
  <trd lng="de">Grenadier</trd>
  <trd lng="en">grenadier</trd>
  <trd lng="es">granadero</trd>
  <trd lng="fr">grenadier<klr tip="ind"> (soldat)</klr></trd>
  <trd lng="hu">gr&aacute;n&aacute;tos</trd>
  <trd lng="it">granatiere</trd>
  <trd lng="nl">grenadier</trd>
  <trd lng="pl">grenadier</trd>
  <trd lng="pt">granadeiro</trd>
  <trdgrp lng="ru">
 <trd>&c_g;&c_r;&c_a;&c_n;&c_a;&c_t;&c_o;&c_m;&c_jo;&c_t;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_r;&c_je;&c_n;&c_a;&c_d;&c_jo;&c_r; <klr>(arkaika)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>granátnik</trd>,
    <trd>grenadýr</trd>
  </trdgrp>
</drv>
</art>
