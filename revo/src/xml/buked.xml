<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: buked.xml,v 1.18 2015/10/07 11:10:17 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>buked</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="buked.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="buked.0o.fasko">
      <dif>
        Kunliga&jcirc;o da detran&ccirc;itaj floroj:
        <ekz>
          <tld/>o da, el siringofloroj.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="fask.0o.KOMUNE">fasko</ref>
      </refgrp>
      <trd lng="hu">vir&aacute;gcsokor</trd>
      <trd lng="pl">bukiet</trd>
    </snc>
    <snc mrk="buked.0o.FIG">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Aro da bela&jcirc;oj:
        <ekz>
          <tld/>o da belaj virinoj, da agrablaj versoj.
        </ekz>
      </dif>
      <trd lng="hu">csokor</trd>
      <trdgrp lng="pl">
        <trd>bukiet</trd>,
        <trd>kwiat</trd>
      </trdgrp>
    </snc>
  <trd lng="be">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
  <trdgrp lng="br">   
      <trd>boked</trd>,
      <trd>bokedad</trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>kytice</trd>,
    <trd>puget</trd>
  </trdgrp>
  <trd lng="en">bouquet</trd>
  <trd lng="es">ramo</trd>
  <trd lng="fr">bouquet</trd>
  <trd lng="pt">buqu&ecirc;</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_je;&c_t;</trd>
  <trd lng="sk">kytica</trd>
</drv>
</art>
