<vortaro>
<art mrk="$Id: tercet.xml,v 1.18 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tercet</rad>/o
</kap>
<drv mrk="tercet.0o">
  <kap><tld/>o</kap>
  <snc mrk="tercet.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o por 3 vo&ccirc;oj a&ubreve; instrumentoj.
    </dif>
      <ref tip="sin" cel="tri1.0o">trio</ref>
      <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_e;&c_t;</trd>
      <trdgrp lng="ca">
        <trd>trio</trd>,
        <trd>tercet</trd>
      </trdgrp>
      <trd lng="de">Terzett</trd>
      <trd lng="en">trio</trd>
      <trd lng="fr">trio<klr> (pi&egrave;ce de musique)</klr></trd>
      <trd lng="hu">tercett</trd>
      <trd lng="nl">terzet</trd>
      <trd lng="pl">tercet</trd>
      <trdgrp lng="pt">
        <trd>terceto</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_je;&c_r;&c_c;&c_je;&c_t;</trd>
      <trd lng="sv">terzett</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>tercet</trd>,
    <trd>terceto</trd>
  </trdgrp>
  <trd lng="sk">terceto</trd>
</drv>
</art>
