<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karavel.xml,v 1.10 2012/07/30 10:10:20 revo Exp $">
<kap>
  <rad>karavel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="karavel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SHI</uzo>
  <snc>
    <dif>
      Negranda e&ubreve;ropa <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>,
      uzata por transmara komercado en la 15a kaj 16a jarcentoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_v;&c_e;&c_l;&c_a;</trd>
  <trd lng="ca">caravel&middot;la</trd>
  <trd lng="de">Karavelle</trd>
  <trdgrp lng="en">
    <trd>caravelle</trd>,
    <trd>caravel</trd>
  </trdgrp>
  <trd lng="es">carabela</trd>
 <trdgrp lng="fr">
    <trd>caravelle</trd>
  </trdgrp>
  <trd lng="hu">karavella</trd>
  <trd lng="nl">karveel</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_a;&c_v;&c_je;&c_l;&c_l;&c_a;</trd>
</drv>
</art>

