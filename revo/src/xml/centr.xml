  <trd lng="br">kreizenn simetriezh</trd>
  <trd lng="ca">centre de simetria</trd>
  <trd lng="de">Symmetriezentrum</trd>
  <trd lng="en">symmetry <ind>center</ind></trd>
  <trd lng="fr">centre de sym&eacute;trie</trd>
  <trd lng="hu">szimmetria-k&ouml;z&eacute;ppont</trd>
  <trd lng="it">centro di simmetria</trd>
  <trd lng="pl">&sacute;rodek symetrii</trd>
  <trd lng="pt">centro de simetria</trd>
  <trd lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;
  &c_s;&c_i;&c_m;&c_m;&c_je;&c_t;&c_r;&c_i;&c_i;</trd>
  <trd lng="sk">stred s&uacute;mernosti</trd>
</drv>

<drv mrk="centr.sam0a">
  <kap>sam<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
    <klr>(p.p. <ref cel="figur.geometria0o">geometriaj
    figuroj</ref> kun <tld/>o)</klr>
    Havantaj saman <ref cel="centr.0o.MAT"><tld/>on</ref>:
    <ekz>
      sam<tld/>aj cirkloj,
    </ekz>
    <ekz>
      kvadratoj,
    </ekz>
    <ekz>
      sferoj...
    </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="br">kengreiz</trd>
  <trd lng="ca">conc&egrave;ntric</trd>
  <trdgrp lng="cs">
    <trd>se stejným středem</trd>,
    <trd>soustředný</trd>,
    <trd>stejného středu</trd>
  </trdgrp>
  <trd lng="de">konzentrisch</trd>
  <trd lng="en">concentric</trd>
  <trd lng="fr">concentrique</trd>
  <trd lng="it">concentrico</trd>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;&sacute;rodkowy</trd>,
    <trd>koncentryczny</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>koncentrický</trd>,
    <trd>sústredný</trd>
  </trdgrp>
</drv>

<drv mrk="centr.sun0a">
