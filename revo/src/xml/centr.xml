    <uzo tip="fak">AST</uzo>
    <dif>
      Havanta la sunon kiel sian centron:
      <ekz>
        Aristarko el Samoso starigas la bazojn de sun<tld/>a teorio
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Astronomio">
          Astronomio</url>, 2011-04-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">helioc&egrave;ntric</trd>
  <trd lng="cs">heliocentrický</trd>
  <trd lng="fr">h&eacute;liocentrique</trd>
  <trd lng="nl">heliocentristisch</trd>
  <trd lng="sk">heliocentrický</trd>
</drv>

<drv mrk="centr.sun0ismo">
  <kap>sun<tld/>ismo, <var><kap>suno<tld/>ismo</kap></var></kap>
  <snc mrk="centr.sun0ismo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Astronomia priskribo de la sunsistemo,
      kiu difinas la sunon kiel &gcirc;ian centron:
      <ekz>
        nur en 1822 la katolikaj ekleziaj a&ubreve;toritatuloj permesis la 
        legadon de la libro de Koperniko pri sun<tld/>ismo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Eppur_si_muove">
          Eppur si muove</url>, 2010-03-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">heliocentrisme</trd>
  <trd lng="fr">h&eacute;liocentrisme</trd>
  <trd lng="nl">heliocentrisme</trd>
</drv>

<drv mrk="centr.ter0a">
  <kap>ter<tld/>a, <var><kap>tero<tld/>a</kap></var></kap>
  <snc mrk="centr.ter0a.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Havanta la teron kiel sian centron:
      <ekz>
        la antikvaj astronomoj modelis en tero<tld/>a sistemo
        la plane<klr>d</klr>movojn vidatajn de la tero
        <fnt>
          <aut>W. Ruff</aut>,
          <vrk><url ref="http://www.pwruff.de/aesp.htm">
          Cikloidoj kaj epicikloj</url></vrk>,
          <lok>[2008?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">geoc&egrave;ntric</trd>
  <trd lng="fr">g&eacute;ocentrique</trd>
  <trd lng="nl">geocentristisch</trd>
</drv>

