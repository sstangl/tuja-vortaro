<vortaro>
<art mrk="$Id: auxgusten.xml,v 1.5 2015/02/26 20:11:05 revo Exp $">
<kap>
  <rad>A&ubreve;gusten</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="auxgusten.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxgusten.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        &scirc;i povis nur stari ekstere kaj kanti: &leftquot;Ha, vi
        kara <tld/>o<klr>...</klr>&rightquot;
        <fnt><bib>Fab2</bib>, <lok>pa&scirc;tisto de porkoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Augustine</trd>
  <trd lng="fr">Augustin</trd>
  <trd lng="it">Augustino</trd>
  <trd lng="pl">Augustyn</trd>
</drv>
</art>

