<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: epilob.xml,v 1.17 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>epilobi</rad>/o<fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="epilob.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="enoter1.0oj">enoteracoj</ref>
      (<trd lng="la">Epilobium</trd>).
    </dif>
  </snc>
  <bld lok="../bld/epilob.jpg">
    <ind>hirta <tld/>o</ind>
    <trd lng="de">zottiges Weidenr&ouml;schen</trd>
    <trd lng="hu">borzas f&uuml;zike</trd>
    <trd lng="la">Epilobium hirsutum</trd>
  </bld>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_p;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Weidenr&ouml;schen</trd>
  <trdgrp lng="en">
    <trd>willow-herb</trd>,
    <trd>willowherb</trd>,
    <trd>fireweed</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pilobe</trd>
  <trdgrp lng="hu">
    <trd>f&uuml;zike</trd>,
    <trd>der&eacute;ce</trd>
  </trdgrp>
  <trd lng="pl">wierzbownica</trd>
  <trd lng="pt">epil&oacute;bio</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_r;&c_je;&c_j;</trd>
  <trd lng="sk">vrbovka</trd>
  <trd lng="cs">vrbovka</trd>
</drv>
</art>
