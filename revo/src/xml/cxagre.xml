<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxagre.xml,v 1.24 2012/02/17 21:10:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;agren</rad>/i
</kap>

<drv mrk="cxagre.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxagre.0i.per_kontrauxstaro">
    <dif>
      Ka&ubreve;zi malplezuron al iu, kontra&ubreve;starante al liaj 
      intencoj a&ubreve; deziroj:
      <ekz>
        tiu perdo de tempo <tld/>is min;
      </ekz>
      <ekz>
        <tld/>ita, ke oni lin tiel trompis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se ni ne trovos pli bonan esprimon, tio ne devas nin 
        <tld/>i<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="incit.0i">inciti</ref>,
      <ref cel="cxikan.0i">&ccirc;ikani</ref>,
      <ref cel="ted.0i">tedi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>,
      
      <trd>&c_n;&c_a;&c_z;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>, 
      <trd>&c_d;&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd> 
    </trdgrp>
    <trd lng="ca">empipar</trd>
    <trdgrp lng="fr">
      <trd>chagriner<klr tip="ind"> (contrarier)</klr></trd>,
      <trd>contrarier</trd>,
      <trd>f&acirc;cher<klr tip="ind"> (contrarier)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bosszant</trd>,
      <trd>b&aacute;nt<klr> (bosszant)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>angustiare</trd>,
      <trd>contrariare</trd>,
      <trd>rammaricare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>frasowa&cacute;</trd>,
      <trd>gniewa&cacute;</trd>,
      <trd>martwi&cacute;<klr> (kogo&sacute;)</klr></trd>,
      <trd>niepokoi&cacute;</trd>,
      <trd>smuci&cacute;</trd>,
      <trd>zasmuca&cacute;</trd>,
      <trd>trapi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>, 
      <trd>&c_d;&c_o;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="cxagre.0i.aflikti">
    <ref tip="dif" cel="aflikt.0i">aflikti</ref>
    <ekz>
      tiu morto tre <tld/>is min; 
    </ekz>
    <ekz>
      <tld/>ita viza&gcirc;o
      <fnt><bib>MT</bib><lok>&Sen; 25:23</lok></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="turmen.0i.anime">turmenti</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">afligir</trd>
    <trdgrp lng="fr">
      <trd>chagriner<klr tip="ind"> (affliger)</klr></trd>,
      <trd>accabler<klr tip="ind"> (affliger)</klr></trd>,
      <trd>affliger</trd>,
      <trd>affecter<klr tip="ind"> (peiner)</klr></trd>,
      <trd>atteindre<klr tip="ind"> (peiner)</klr></trd>,
      <trd>attrister</trd>,
      <trd>d&eacute;soler</trd>,
      <trd>navrer</trd>,
      <trd>peiner<klr tip="ind"> (chagriner)</klr></trd>,
      <trd>toucher<klr tip="ind"> (peiner)</klr></trd>
    </trdgrp>
    <trd lng="hu">elkeser&iacute;t</trd>
    <trdgrp lng="it">
      <trd>addolorare</trd>,
      <trd>rattristare</trd>,
      <trd>rammaricare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bole&cacute;<klr> (nad czym&sacute;)</klr></trd>,
      <trd>frasowa&cacute; si&eogonek;</trd>,
      <trd>by&cacute; <ind>zafrasowanym</ind></trd>,
      <trd>gry&zacute;&cacute; si&eogonek;<klr> (jakim&sacute;
      problemem)</klr></trd>
      <trd>martwi&cacute; si&eogonek;</trd>,
      <trd>niepokoi&cacute; si&eogonek;</trd>,
      <trd>by&cacute; <ind>nieszcz&eogonek;&sacute;liwym</ind></trd>,
      <trd>by&cacute; <ind>zatroskanym</ind></trd>,
      <trd>smuci&cacute; si&eogonek;</trd>,
      <trd>trapi&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_g;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>amo&iuml;nar</trd>,
    <trd>molestar</trd>
  </trdgrp>
  <trd lng="de">&auml;rgern</trd>
  <trd lng="en">vex</trd>
  <trdgrp lng="es">
    <trd>disgustar</trd>,
    <trd>molestar</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ergeren</trd>,
    <trd>ontstemmen</trd>,
    <trd>irriteren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>afligir</trd>,
    <trd>incomodar</trd>,
    <trd>magoar</trd>
  </trdgrp>
</drv>

<drv mrk="cxagre.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxagre.0o.KOMUNE">
    <dif>
      Sento kaj stato de iu <tld/>ita:
      <ekz>
        &ccirc;e <tld/>o de la koro
        <fnt><bib>MT</bib><lok>&Sen; 15:13</lok></fnt>;
      </ekz>
      <ekz>
        por unu festeno, por alia <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        grandan &gcirc;ojon akompanas grandaj <tld/>oj
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 21</lok></fnt>;
      </ekz>
      <ekz>
        beda&ubreve;ro kaj <tld/>o &scirc;uldon ne kovras
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxagre.0igxo"><tld/>i&gcirc;o</ref>
    <ref tip="vid" cel="humor">malbonhumoro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>disgust</trd>,
    <trd>enuig</trd>,
    <trd>pena</trd>,   
    <trd>abatiment</trd> 
  </trdgrp>
  <trd lng="de">&Auml;rger</trd>
  <trd lng="es">disgusto</trd> 
  <trdgrp lng="fr">
    <trd>chagrin<klr tip="ind"> (sentiment)</klr></trd>,
    <trd>accablement</trd>,
    <trd>affliction</trd>
    <trd>contrari&eacute;t&eacute;</trd>,
    <trd>peine<klr tip="ind"> (affliction)</klr></trd>,
    <trd>tristesse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bossz&uacute;s&aacute;g</trd>,
    <trd>elkeseredetts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dolore</trd>,
    <trd>afflizione</trd>,
    <trd>dispiacere</trd>
    <trd>cruccio</trd>,
    <trd>patema</trd>,
    <trd>rammarico</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ergernis</trd>,
    <trd>ontstemming</trd>
  </trdgrp>
    <trdgrp lng="pl">
      <trd>bol&aogonek;czka</trd>,
      <trd>frasunek</trd>,
      <trd>gniew</trd>,
      <trd>nieszcz&eogonek;&sacute;cie</trd>,
      <trd>niepok&oacute;j</trd>,
      <trd>smutek</trd>,
      <trd>strapienie</trd>,
      <trd>troska</trd>,
      <trd>zgryzota</trd>,
      <trd>zmartwenie</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>, 
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxagre.0a">
  <kap><tld/>a</kap>
  <snc mrk="cxagre.0a.KOMUNE">
    <dif>
      <tld lit="&Ccirc;"/>anta:
      <ekz>
        la modo estas por mi tre <tld/>a afero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>amo&iuml;n&oacute;s</trd>,
    <trd>molest</trd>
  </trdgrp>
  <trd lng="de">&auml;rgerlich</trd>
  <trdgrp lng="es">
    <trd>molesto <klr>(ser)</klr></trd>,
    <trd>irritante</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affligeant</trd>,
    <trd>contrariant</trd>,
    <trd>d&eacute;solant</trd>
  </trdgrp>
  <trd lng="hu">bosszant&oacute;</trd>
  <trdgrp lng="nl">
    <trd>ergerlijk</trd>,
    <trd>irriterend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obola&lstroke;y</trd>,
    <trd>zafrasowany</trd>,
    <trd>zagniewany</trd>,
    <trd>nieszcz&eogonek;&sacute;liwy</trd>,
    <trd>niespokojny</trd>,
    <trd>zaniepokojony</trd>,
    <trd>smutny</trd>,
    <trd>zasmucony</trd>,
    <trd>strapiony</trd>,
    <trd>zatroskany</trd>,
    <trd>zmartwiony</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="cxagre.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="cxagre.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        li kutimis al la orienta malrapideco kaj li tute ne
        <tld/>i&gcirc;is kaj ne perdis la tempon
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 10</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bedaux.0i">beda&ubreve;ri</ref>,
      <ref cel="pent.0i">penti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>amo&iuml;nar-se</trd>,
    <trd>entristir-se</trd>,
    <trd>disgustar-se</trd>
  </trdgrp>
  <trd lng="de">&auml;rgern (sich)</trd>
  <trdgrp lng="fr">
    <trd>se <ind>chagriner</ind><klr tip="ind">
    (s'affliger)</klr></trd>,
    <trd>s'<ind>affliger</ind></trd>
    <trd>s'<ind>attrister</ind></trd>,
    <trd>se <ind>contrarier</ind></trd>,
    <trd>se <ind>d&eacute;soler</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bosszankodik</trd>,
    <trd>elkeseredik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>addolorarsi</trd>,
    <trd>rattristarsi</trd>,
    <trd>rammaricarsi</trd>,
    <trd>angustiarsi</trd>,
    <trd>contrariarsi</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich <ind>ergeren</ind></trd>,
    <trd>ontstemd geraken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bole&cacute;<klr> (nad czym&sacute;)</klr></trd>,
    <trd>frasowa&cacute; si&eogonek;</trd>,
    <trd>by&cacute; <ind>zafrasowanym</ind></trd>,
    <trd>gry&zacute;&cacute; si&eogonek;<klr> (jakim&sacute;
      problemem)</klr></trd>
    <trd>martwi&cacute; si&eogonek;</trd>,
    <trd>niepokoi&cacute; si&eogonek;</trd>,
    <trd>by&cacute; <ind>nieszcz&eogonek;&sacute;liwym</ind></trd>,
    <trd>by&cacute; <ind>zatroskanym</ind></trd>,
    <trd>smuci&cacute; si&eogonek;</trd>,
    <trd>trapi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="cxagre.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="cxagre.0igxo.cxagreno">
    <ref tip="dif" cel="cxagre.0o"><tld/>o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>disgust</trd>,
    <trd>pena</trd>
  </trdgrp>
  <trd lng="de">&Auml;rger</trd>
  <trdgrp lng="fr">
    <trd>chagrin<klr tip="ind"> (sentiment)</klr></trd>,
    <trd>accablement</trd>,
    <trd>affliction</trd>
    <trd>contrari&eacute;t&eacute;</trd>,
    <trd>peine<klr tip="ind"> (affliction)</klr></trd>,
    <trd>tristesse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bossz&uacute;s&aacute;g</trd>,
    <trd>elkeseredetts&eacute;g</trd>
  </trdgrp>
 <trdgrp lng="it">
      <trd>disappunto</trd>,
      <trd>rattristamento</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ergernis</trd>,
    <trd>ontstemming</trd>
  </trdgrp>
    <trdgrp lng="pl">
      <trd>bol&aogonek;czka</trd>,
      <trd>frasunek</trd>,
      <trd>niepok&oacute;j</trd>,
      <trd>nieszcz&eogonek;&sacute;cie</trd>,
      <trd>smutek</trd>,
      <trd>strapienie</trd>,
      <trd>troska</trd>,
      <trd>zatroskanie</trd>,
      <trd>zgryzota</trd>,
      <trd>zmartwienie</trd>
    </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_o;&c_r;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>
