<vortaro>
<art mrk="$Id: klauxdi.xml,v 1.5 2015/02/26 19:11:58 revo Exp $">
<kap>
  <rad>Kla&ubreve;di</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="klauxdi.0o">
  <kap><tld/>o</kap>
  <snc mrk="klauxdi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o jam ordonis, ke &ccirc;iuj Judoj foriru el Romo
        <fnt><bib>NT</bib>, <lok>&Ago; 18:2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Claudius</trd>
  <trdgrp lng="fr">
    <trd>Claude</trd>,
    <trd>Claudius</trd>
  </trdgrp>
  <trd lng="it">Claudio</trd>
</drv>
</art>

