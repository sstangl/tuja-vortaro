<vortaro>
<art mrk="$Id: ferdinand.xml,v 1.9 2015/02/26 20:11:19 revo Exp $">
<kap>
  <rad>Ferdinand</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ferdinand.0o">
  <kap><tld/>o</kap>
  <snc mrk="ferdinand.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        koketa Emma kisis lin, <tld/>o metis la manojn apud li sur la
        kuseno
        <fnt><bib>FK</bib>, <lok>Neniam plu!</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_F;&c_je;&c_r;&c_d;&c_i;&c_n;&c_a;&c_n;&c_d;</trd>
  <trd lng="de">Ferdinand</trd>
  <trd lng="fa">&a_f;&a_r;&a_d;&f_y;&a_n;&a_A;&a_n;&a_d;</trd>
  <trd lng="fr">Ferdinand</trd>
  <trd lng="it">Ferdinando</trd>
  <trd lng="pt">Ferdinando</trd>
  <trd lng="ru">&c_F;&c_je;&c_r;&c_d;&c_i;&c_n;&c_a;&c_n;&c_d;</trd>
</drv>
</art>

