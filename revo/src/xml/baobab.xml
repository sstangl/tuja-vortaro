<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: baobab.xml,v 1.20 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>baobab</rad>/o
</kap>
<drv mrk="baobab.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO"
       lst="voko:botanikaj_genroj">Genro</ref> el familio
       <ref tip="malprt" cel="bombak1.0oj">bombakacoj</ref>
       (<trd lng="la">Adansonia</trd>), tropika arbo kun dikega
       trunko kaj acidetaj kukumformaj fruktoj.
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_a;&c_a;&c_b;&c_a;&c_b;</trd>
  <trdgrp lng="cs">
    <trd>baobab</trd>,
    <trd>baobab prstnatý</trd>,
    <trd>chlebovník</trd>
  </trdgrp>
  <trdgrp lng="de"><trd>Baobab</trd>, <trd>Affenbrotbaum</trd></trdgrp>
  <trd lng="en">baobab</trd>
  <trd lng="es">baobab</trd>
  <trd lng="fr">baobab</trd>
  <trdgrp lng="hu"><trd>majomkeny&eacute;rfa</trd>,
   <trd>baobab</trd></trdgrp>
  <trdgrp lng="nl"><trd>baobab</trd>, <trd>apenbroodboom</trd></trdgrp>
  <trd lng="pl">baobab</trd>
  <trd lng="ru">&c_b;&c_a;&c_o;&c_b;&c_a;&c_b;</trd>
  <trd lng="sk">chlebovník</trd>
  <trd lng="sv">baobabsl&auml;ktet</trd>
</drv>
</art>
