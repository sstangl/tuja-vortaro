<vortaro>
<art mrk="$Id: veratr.xml,v 1.12 2013/08/27 10:10:39 revo Exp $">
<kap>
  <rad>veratr</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="veratr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Veratrum</trd>), kies radiko
      estas emetika, laksiga, sed en tro granda dozo mortiga.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_m;&c_ja;&c_r;&c_y;&c_c;&c_a;</trd>
  <trd lng="de">Germer</trd>
  <trd lng="hu">z&aacute;szpa</trd>
  <trd lng="nl">veratrum</trd>
  <trdgrp lng="pt">
    <trd>verator</trd>,
    <trd>flor-da-verdade</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_m;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
