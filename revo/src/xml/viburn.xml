<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: viburn.xml,v 1.17 2015/08/22 14:10:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>viburn</rad>/o
</kap>
<drv mrk="viburn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kaprif1.0oj">kaprifoliacoj</ref>
      (<trd lng="la">Viburnum</trd>), blankflora kaj ru&gcirc;frukta.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">viburn</trd>
  <trd lng="cs">kalina</trd>
  <trd lng="de">Schneeball</trd>
  <trd lng="en">viburnum</trd>
  <trd lng="fr">viorne</trd>
  <trd lng="hu">bangita</trd>
  <trd lng="nl">sneeuwbal<klr tip="ind"> (struik)</klr></trd>
  <trd lng="pt">viburno</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">kalina <klr>(rastlina)</klr></trd>
</drv>
</art>
