<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: helian2.xml,v 1.13 2013/10/31 14:10:15 revo Exp $">
<kap>
  <rad>heliantem</rad>/o
</kap>
<drv mrk="helian2.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="cist.0oj">cistacoj</ref>,
      (<trd lng="la">Helianthemum</trd>), 
      <ref tip="lst" cel="arbust.0o"
      lst="voko:malvoparencoj">duonarbusto</ref>
      multspecie kreskanta en Mediteraneo; kun 
      simplaj kontra&ubreve;sidaj folioj, belaj, kvinnombraj 
      kaj multstamenaj, ofte orflavaj floroj direkti&gcirc;antaj al la suno;  
      kaj kapsulaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">Sonnenr&ouml;schen</trd>
  <trd lng="fr">h&eacute;lianth&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>napvir&aacute;g</trd>,
    <trd>tetemold&oacute;</trd>,
    <trd>tetemtold&oacute;</trd>
  </trdgrp>
  <trd lng="nl">zonneroosje</trd>
  <trd lng="pt">heli&acirc;ntemo</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;&c_c;&c_v;&c_je;&c_t;</trd>
</drv>
</art>
