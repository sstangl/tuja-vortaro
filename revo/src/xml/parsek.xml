<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parsek.xml,v 1.11 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>parsek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="parsek.0o">
  <kap><tld/>o</kap>
  <mlg>pc</mlg>
  <snc mrk="parsek.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">mezurunuo</ref> de
      <ref tip="vid" cel="long.0o.FIZ">longo</ref>, uzata por mezuri 
      grandegajn interastrajn distancojn, egalanta 3,25
      <ref tip="vid" cel="jar.lum0o.AST">lumjarojn</ref>:
      <ekz>
        &leftquot;Teluro&rightquot; iris kun subluma rapido en distanco je malpli ol
        kvar 
        <tld/>oj disde la giganta malhela stelo
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url
          ref="http://www.esperanto.mv.ru/Efremov/koro_01.html">
          La Koro de la Serpento</url></vrk>,
          <lok>2006-2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">parsek</trd>
  <trd lng="de">Parsek</trd>
  <trd lng="en">parsec</trd>
  <trd lng="es">parsec</trd>
  <trd lng="fr">parsec</trd>
  <trd lng="hu">parsec</trd><trd lng="hu">parsec</trd>
  <trd lng="id">parsec</trd>
  <trd lng="it">parsec</trd>
  <trd lng="nl">parsec</trd>
  <trd lng="pl">parsek</trd>
  <trd lng="sk">parsek</trd>
  <trd lng="sv">parsec</trd>
</drv>
</art>
