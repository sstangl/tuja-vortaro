  <trd lng="de">Aviso</trd>
  <trd lng="en">cutter</trd>
  <trd lng="es">aviso</trd>
  <trd lng="fr">aviso</trd>
  <trd lng="hu">&odblac;rnasz&aacute;d</trd>
  <trd lng="it">aviso</trd>
