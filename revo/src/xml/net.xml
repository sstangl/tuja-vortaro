<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: net.xml,v 1.16 2015/07/04 08:37:40 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>net</rad>/a
    </kap>

    <drv mrk="net.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="net.0a.klara">
        <dif>
          Nekonfuza kaj preciza:
          <ekz>
            <tld/>a skriba&jcirc;o, desegno, kli&scirc;a&jcirc;o;
          </ekz><ekz>
            <tld/>aj konturoj.
          </ekz>
        </dif>
        <trd lng="be">&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>
        <trd lng="de">rein</trd>
        <trdgrp lng="hu">
          <trd>tiszta</trd>,
          <trd>letiszt&aacute;zott</trd>,
          <trd>tiszt&aacute;zati</trd>,
          <trd>nett</trd>
        </trdgrp>
        <trd lng="nl">net</trd>
        <trdgrp lng="pt">
          <trd>limpo</trd>,
          <trd>claro</trd>,
          <trd>distinto</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>,
          <trd>&c_b;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
        </trdgrp>
      </snc>
      <snc mrk="net.0a.sentara">
        <dif>
          <ref tip="sub" cel="tar.sen0a">Sentara</ref>,
          senigita je &ccirc;iu ajn kroma&jcirc;o, kalkulata la&ubreve; sia nura
          propra kvanto:
          <ekz>
            <ind><tld/>a pezo</ind>
            (sentara maso, pezo de la varo mem ne enkalkulante la
            &ccirc;irka&ubreve;volva&jcirc;on);
            <trd lng="hu">nett&oacute; s&uacute;ly</trd>        
            <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j; &c_v;&c_je;&c_s;</trd>
          </ekz><ekz>
            <tld/>a gajno, <tld/>a profito
            <klr>(definitiva profito, kiu restas post depreno de elspezoj
              faritaj a&ubreve; de perdoj okazintaj)</klr>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>,
          <trd>&c_n;&c_e;&c_t;&c_a;</trd>
        </trdgrp>
        <trd lng="de">netto</trd>
        <trd lng="en">net</trd>
        <trd lng="es">neto</trd>
        <trd lng="fr">net</trd>
        <trd lng="hu">nett&oacute;</trd>    
        <trd lng="nl">netto</trd>
        <trd lng="pt">l&iacute;quido</trd>
        <trdgrp lng="ru">
          <trd>&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
          <trd>&c_n;&c_je;&c_t;&c_t;&c_o;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>hladký</trd>,
    <trd>netto</trd>,
    <trd>opracovaný</trd>,
    <trd>začištěný</trd>,
    <trd>čistý <klr>(výnos,váha)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladký</trd>,
    <trd>netto</trd>,
    <trd>čistý <klr>(výnos, váha)</klr></trd>
  </trdgrp>
</drv>

    <drv mrk="net.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Definitiva klara skriba&jcirc;o, sen korektoj kaj sen makuloj, ordinare
          kopiita la&ubreve; alia prepara pli malpli malklara skriba&jcirc;o.
        </dif>
      </snc>
  <trd lng="be">&c_ch;&c_y;&c_s;&c_t;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>váha netto</trd>,
    <trd>čistá váha</trd>
  </trdgrp>
  <trd lng="de">Reinschrift</trd>
  <trd lng="fr">propre (subst.)</trd>
  <trd lng="hu">tiszt&aacute;zat</trd>
  <trd lng="nl">net</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">čistá váha</trd>
</drv>

    <drv mrk="net.mal0o">
      <kap>mal<tld/>o</kap>
      <snc>
        <dif>
          Unua prova skriba&jcirc;o kun korektoj, plibonigoj a&ubreve; makuloj, la&ubreve; kiu oni
          poste transskribas la <tld/>on.
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_v;&c_ib;&c_k;</trd>,
        <trd>&c_n;&c_a;&c_k;&c_ib;&c_d;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>koncept</trd>,
    <trd>makulář</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Konzept</trd>,
        <trd>Entwurf</trd>,
        <trd>Kladde</trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>rough draft</trd>,
        <trd>first draft</trd>
      </trdgrp>
  <trd lng="es">borrador</trd>
  <trd lng="fr">brouillon (subst.)</trd>
  <trd lng="hu">piszkozat</trd>
  <trd lng="nl">klad</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_o;&c_v;&c_i;&c_k;</trd>
  <trd lng="sk">koncept <klr>(písomnej práce)</klr></trd>
</drv>
  </art>
