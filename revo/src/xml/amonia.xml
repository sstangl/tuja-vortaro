<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amonia.xml,v 1.20 2015/09/10 11:10:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>amoniak</rad>/o
</kap>
<drv mrk="amonia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      NH<sub>3</sub>. Gaso konsistanta el azoto kaj hidrogeno, ofte
      uzata en akva solva&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_ib;&c_ja;&c_k;</trd>
  <trd lng="br">amoniak</trd>
  <trdgrp lng="cs">
    <trd>amoniak</trd>,
    <trd>&ccaron;pavek</trd>
  </trdgrp>
  <trd lng="de">Ammoniak</trd>
  <trd lng="en">anhydrous ammonia</trd>
  <trd lng="es">amoniaco</trd>
  <trd lng="fa">&a_A_madda;&a_m;&a_w;&a_n;&f_y;&a_A;&f_k;</trd>
  <trd lng="fr">ammoniac</trd>
  <trd lng="nl">ammoniak</trd>
  <trd lng="pl">amoniak</trd>
  <trd lng="pt">amon&iacute;aco</trd>
  <trd lng="ru">&c_a;&c_m;&c_m;&c_i;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>amoniak</trd>,
    <trd>&ccaron;pavok</trd>
  </trdgrp>
  <trd lng="sv">ammoniak</trd>
</drv>
</art>
