<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxikan.xml,v 1.17 2015/10/11 14:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ikan</rad>/i
</kap>
<drv mrk="cxikan.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxikan.0i.gxeni">
    <dif>
      Fari al iu malpravajn kontra&ubreve;a&jcirc;ojn, malhelpojn, 
      malagrabla&jcirc;ojn:
      <ekz>&ccirc;u foje vi vidis, ke iu <tld/>as kontra&ubreve; ni?
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 52a, p.
      177a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="turmen.0i">turmenti</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>schikanieren</trd>,
       <trd>mobben</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>embrollar</trd>,
      <trd>enredar</trd>,
      <trd>perjudicar</trd>,
      <trd>jorobar</trd>,
      <trd>fastidiar</trd>,
      <trd>putear<klr> (fastidiar)</klr></trd>,
      <trd>trapacear</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>zaklat</trd>,
      <trd>bosszant</trd>,
      <trd>piszk&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>infastidire</trd>,
      <trd>tormentare</trd>,
      <trd>vessare</trd>
    </trdgrp>
    <trd lng="pl">szykanowa&cacute;</trd>
  </snc>
  <snc mrk="cxikan.0i.harfendi">
    <dif>
      Tro subtile kritika&ccirc;i, pedante disputadi.
    </dif>
    <refgrp tip="sin">
      disfendi <ref cel="har.0o">harojn</ref>,
      <ref cel="fend.har0i">harfendi</ref>.
    </refgrp>
    <adm>
      Enkonduki "harfendi" sub "haro". <aut>MB</aut>
    </adm>
    <trd lng="hu">okvetetlenkedik</trd>
    <trdgrp lng="it">
      <trd>cavillare</trd>,
      <trd>pignoleggiare</trd>,
      <trd>sottilizzare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>&scaron;ikanovat</trd>,
    <trd>t&yacute;rat</trd>
  </trdgrp>
  <trd lng="en">quibble</trd>
  <trdgrp lng="fr">
    <trd>chicaner</trd>,
    <trd>ergoter</trd>,
    <trd>chipoter</trd>
  </trdgrp>
 
  <trdgrp lng="pt">
    <trd>atazanar</trd>,
    <trd>chicanar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>
&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_t;&c_v;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="sk">&scaron;ikanova&tcaron;</trd>
</drv>
</art>
