<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvalif.xml,v 1.13 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kvalif</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvalif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kvalif.0i.KOMUNE">
    <ref tip="dif" cel="kvalifik.0i.KOMUNE">kvalifiki <sncref/></ref>
    <trd lng="be">&c_k;&c_v;&c_a;&c_l;&c_ib;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>oszt&aacute;lyoz</trd>,
      <trd>min&odblac;s&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">kwalificeren</trd>
    <trd lng="ru">&c_k;&c_v;&c_a;&c_l;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="kvalif.0i.GRA">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="kvalifik.0i.GRA">kvalifiki <sncref/></ref>
    <trd lng="be">&c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trd lng="hu">meghat&aacute;roz <klr tip="ind">(nyelvtanilag)</klr></trd>
    <trd lng="nl">bepalen<klr tip="ind"> (ling.)</klr></trd>
    <trd lng="ru">&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">kvalifikovat</trd>
  <trdgrp lng="de">
    <trd>kennzeichnen</trd>,
    <trd>qualifizieren</trd>
  </trdgrp>
  <trd lng="fr">qualifier</trd>
  <trdgrp lng="pt">
    <trd>qualificar</trd>,
    <trd>classificar</trd>,
    <trd>capitular</trd>
  </trdgrp>
  <trd lng="sk">kvalifikova≈•</trd>
</drv>

<drv mrk="kvalif.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kvalif.mal0i.KOMUNE">
    <ref tip="dif" cel="kvalifik.mal0i.KOMUNE">malkvalifiki</ref>
    <ekz>
      li tute ne estas stulta, sed lia financa stato povus
      mal<tld/>i lin
      <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
      <lok>tk-stafeto, 1982</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">disqualifier</trd>
</drv>
</art>
