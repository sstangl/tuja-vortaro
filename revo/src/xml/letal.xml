<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: letal.xml,v 1.3 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>letal</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="letal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="letal.0a.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="mort.0iga.KOMUNE">mortiga</ref>
    <ekz>
      &scirc;in pereigis <klr>...</klr> ne <tld/>a veneno,
      sed la kruelo kaj la rigoro
      <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 70a, p.
      787a</lok></fnt>.
    </ekz>
  </snc>
  <!-- formon evitindan prefere ne traduku -->
  <trdgrp lng="sk">
    <trd>smrteľný</trd>,
    <trd>smrtiaci</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>letální</trd>,
    <trd>smrtelný</trd>,
    <trd>smrtící</trd>
  </trdgrp>
</drv>
</art>

