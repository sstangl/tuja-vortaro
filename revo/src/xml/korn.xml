<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korn.xml,v 1.30 2015/12/22 21:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>korn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="korn.0o.besta">
    <dif>
      Malmola elstara&jcirc;o, kiu kreskas sur frunto a&ubreve; alia kapo-parto de
      bestoj, precipe de viraj rema&ccirc;uloj:
      <ekz>
        <tld/>o de vir&scirc;afo, rinocero;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>preni la bovon per la <tld/>oj</ind>
        <trd lng="fr">prendre le taureau par les cornes</trd>
        <klr>(iri rekte kaj decide al la plej malfacila afero)</klr>
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>obati, <tld/>ofrapi;
      </ekz>
      <ekz>
        <tld/>havaj bestoj.
      </ekz>
    </dif>
    <subsnc mrk="korn.0o.ajxo">
      <dif>
        Bova <tld/>o a&ubreve; similforma&jcirc;o uzata kiel ujo:
        <ekz>
          Cadok prenis la <tld/>on kun oleo <klr>...</klr> kaj
          sanktoleis
          Salomonon
          <fnt><bib>MT</bib>, <lok>&Re1; 1:39</lok></fnt>;
        </ekz>
        <ekz>
          trinki el <tld/>o.
        </ekz>
      </dif>
      <trd lng="de">Horn</trd>
      <trd lng="en">horn</trd>
      <trdgrp lng="hu">
        <trd>iv&oacute;k&uuml;rt</trd>,
        <trd>kupa</trd>,
        <trd>szarupoh&aacute;r</trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_o;&c_g;</trd>
    </subsnc>
    <subsnc mrk="korn.0o.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Besta korno truita &ccirc;e la pinto a&ubreve; &ccirc;e flanko,
        uzata kiel <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>:
        <ekz>
          de la supro eksonis <tld/>o, al kiu tuj respondis la princa
          ta&ccirc;mento
          <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_r;&c_o;&c_g;</trd>,
        <trd>&c_r;&c_a;&c_zh;&c_o;&c_k; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
      </trdgrp>
      <trd lng="de">Horn</trd>
      <trdgrp lng="fr">
        <trd>cor<klr> (musique)</klr></trd>,
        <trd>trompe<klr> (musique)</klr></trd>
      </trdgrp>
      <trd lng="hu">k&uuml;rt</trd>
      <trdgrp lng="ru">
        <trd>&c_r;&c_o;&c_g;</trd>,
        <trd>&c_r;&c_o;&c_zh;&c_o;&c_k;<klr>
        (&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
        &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
      </trdgrp>
    </subsnc>
    <trd lng="de">Horn</trd>
    <trd lng="en">horn</trd>
    <trdgrp lng="hu">
      <trd>szarv</trd>,
      <trd>t&uuml;l&ouml;k</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>chifre</trd>,
      <trd>corno <klr tip="amb">(chifre)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_o;&c_g;</trd>
  </snc>
  <snc mrk="korn.0o.substanco">
    <dif>
      <tld lit="K"/>a substanco.
    </dif>
    <trd lng="de">Horn</trd>
    <trd lng="en">horny</trd>
    <trd lng="hu">szaru</trd>
  </snc>
  <snc mrk="korn.0o.FIG">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Simbolo de potenco kaj fiero:
      <ekz>
        la nacioj <klr>...</klr> levis sian <tld/>on kontra&ubreve; la
        lo&gcirc;antojn
        de Judujo
        <fnt><bib>MT</bib>, <lok>&Zeh; 1:21</lok></fnt>;
      </ekz>
      <ekz>
        li rompis &ccirc;iujn <tld/>ojn de Izrael
        <fnt><bib>MT</bib>, <lok>&Plo; 2:3</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">szarv <klr tip="ind">(&aacute;tv.)</klr></trd>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>roh <klr>(zv&iacute;&rcaron;ec&iacute;)</klr></trd>,
    <trd>rohovina</trd>
  </trdgrp>
  <trd lng="es">cuerno</trd>
  <trdgrp lng="fr">
    <trd>corne</trd>
  </trdgrp>
  <trd lng="nl">hoorn</trd>
  <trd lng="sk">h&uacute;ka&ccaron;ka auta</trd>
</drv>

<drv mrk="korn.0eca">
  <kap><tld/>eca</kap>
  <snc mrk="korn.0eca.KOMUNE">
    <dif>
      Farita el substanco simila al <tld/>o:
      <ekz>
        <tld/>eca kombilo, tabakujo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>rohovkov&yacute;</trd>,
    <trd>zrohovat&ecaron;l&yacute;</trd>
  </trdgrp>
  <trd lng="en">horny</trd>
  <trdgrp lng="fr">
    <trd>corn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">szaru-</trd>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="sk">zrohovaten&yacute;</trd>
</drv>

<drv mrk="korn.0aro">
  <kap><tld/>aro, <var><kap><tld/>obran&ccirc;aro</kap></var></kap>
  <snc mrk="korn.0aro.ZOO">
    <dif>
      Bran&ccirc;formaj <tld/>oj de <ref tip="vid"
      cel="cerved.0oj">cervedoj</ref>:
      <ekz>
        bela cervo kun majesta <tld/>aro
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la paradizo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_g;&c_ib; <klr tip="amb">(&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_ja; - &c_ja;&c_k; &c_u; &c_a;&c_l;&c_je;&c_n;&c_ja;,
  &c_l;&c_a;&c_s;&c_ja;)</klr></trd>
  <trd lng="nl">hoornachtig</trd>
  <trd lng="en">antlers</trd>
  <trdgrp lng="fr">
    <trd>andouiller</trd>,
    <trd>bois<klr> (de cervid&eacute;)</klr></trd>,
    <trd>cors<klr> (de cervid&eacute;)</klr></trd>,
    <trd>massacre<klr> (de cervid&eacute;)</klr></trd>,
    <trd>ramure<klr> (de cervid&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">agancs</trd>
  <trd lng="ru">&c_r;&c_o;&c_g;&c_a;<klr>
  (&c_v;&c_je;&c_t;&c_v;&c_i;&c_s;&c_t;&c_y;&c_je;)</klr></trd>
</drv>

<drv mrk="korn.abundo0o">
  <kap>abundo<tld/>o</kap>
  <snc mrk="korn.abundo0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Tradicia motivo de okcidenta arto &mdash;
      granda <tld/>o elbu&scirc;iganta florojn, fruktojn
      kaj aliajn simbolojn de abundo kaj prospero:
      <ekz>
        abundo<tld/>o mia al vi ver&scirc;us /
        precize &ccirc;ion far vi sopiratan
        <fnt>
          <aut>E. de Kock</aut>,
          <vrk>Testamente</vrk>,
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">F&uuml;llhorn</trd>
  <trd lng="en">cornucopia</trd>
  <trd lng="fr">corne d'abondance</trd>
  <trd lng="it">cornucopia</trd>
</drv>

<drv mrk="korn.auxto0o">
  <kap>a&ubreve;to<tld/>o</kap>
  <snc mrk="korn.auxto0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="hup.0o.SON">Hupo</ref> de a&ubreve;to,
      origine farita el <ref tip="vid" cel="korn.0o.MUZ">korno</ref>
      kaj plue tiel nomata:
      <ekz>
        eta &leftquot;PEE&rightquot; de a&ubreve;to<tld/>o altiris lian atenton
        <fnt>
          <aut>S. Rodin</aut>,
          <vrk>Bildoj pri Norda Lando</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>avertisseur</trd>,
    <trd>klaxon</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tromba<klr> (di automobile)</klr></trd>,
    <trd>clacson</trd>
  </trdgrp>
  <trd lng="pt">buzina<klr> de autom&oacute;vel</klr></trd>
</drv>

<drv mrk="korn.cxas0o">
  <kap>&ccirc;as<tld/>o</kap>
  <snc mrk="korn.cxas0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;ropa metala <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      kun longa, cirkle volvita tubo kaj vasta klo&scirc;o:
      <ekz>
        jen inter la montoj eka&ubreve;di&gcirc;is la sonoj de &ccirc;as<tld/>o
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_g; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;, &c_p;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="de">Horn</trd>
  <trd lng="en">hunting horn</trd>
  <trdgrp lng="fr">
    <trd>cor<klr> (d'orchestre)</klr></trd>,
    <trd>cor de chasse</trd>
  </trdgrp>
  <trd lng="hu">vad&aacute;szk&uuml;rt</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_h;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_i;&c_j;
    &c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_o;&c_g;<klr>
    (&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
    &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="korn.paper0eto">
  <kap>paper<tld/>eto</kap>
  <snc mrk="korn.paper0eto.KOMUNE">
    <dif>
      Papersaketo pinte rulita:
      <ekz>
        fari paper<tld/>etojn <klr>...</klr> oni ne &ccirc;iutage bezonas, sed
        tiam oni entreprenas ion alian
        <fnt><bib>Fab3</bib>, <lok>nokta &ccirc;apo de fra&ubreve;lo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_zh;&c_o;&c_k; <klr tip="amb">(&c_z; &c_p;&c_a;&c_p;&c_je;&c_r;&c_y;)</klr></trd>,
    <trd>&c_s;&c_k;&c_r;&c_u;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kornout</trd>,
    <trd>kornoutek</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cornet de papier</trd>
  </trdgrp>
  <trd lng="hu">pap&iacute;rt&ouml;lcs&eacute;r</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_zh;&c_o;&c_k;<klr>
    (&c_b;&c_u;&c_m;&c_a;&c_zh;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_k;&c_u;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">korn&uacute;tik</trd>
</drv>

<drv mrk="korn.sxu0o">
  <kap>&scirc;u<tld/>o</kap>
  <snc mrk="korn.sxu0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      La&ubreve;longa duono de <tld/>o a&ubreve; simila duonfunela ilo
      por faciligi engliton de kalkano en &scirc;uon.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>shoe horn</trd>,
    <trd>shoe spoon</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>corne &agrave; chaussure</trd>
  </trdgrp>
  <trd lng="hu">cip&odblac;kan&aacute;l</trd>
  <trdgrp lng="nl">
    <trd>schoenlepel</trd>,
    <trd>schoentrekker</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k;<klr> (&c_d;&c_l;&c_ja;
  &c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>
</drv>

<drv mrk="korn.unu0ulo">
  <kap>unu<tld/>ulo</kap>
  <snc mrk="korn.unu0ulo.ZOO">
    <dif>
      Besto kun unu <tld/>o:
      <ekz>
        la unu<tld/>ulo tuj aperis kaj kuris rekte kontra&ubreve; la tajloro,
        kvaza&ubreve; por trapiki lin
        <fnt><bib>ElektFab</bib>, <lok>La kura&gcirc;a tajloreto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_r;&c_o;&c_g;</trd>
  <trd lng="cs">jednoro&zcaron;ec</trd>
  <trd lng="de">Einhorn</trd>
  <trd lng="en">unicorn</trd>
  <trdgrp lng="fr">
    <trd>licorne</trd>,
    <trd>unicorne</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyszarv&uacute;</trd>,
    <trd>unicornis</trd>
  </trdgrp>
  <trd lng="nl">eenhoorn</trd>
  <trdgrp lng="pt">
   <trd>unic&oacute;rnio</trd>
  </trdgrp>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_r;&c_o;&c_g;</trd>
  <trd lng="sk">jednoro&zcaron;ec</trd>
</drv>

<drv mrk="korn.du0ulo">
  <kap>du<tld/>ulo</kap>
  <snc mrk="korn.du0ulo.ZOO">
    <dif>
      Besto kun du <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_r;&c_o;&c_g;</trd>
  <trd lng="hu">k&eacute;tszarv&uacute;</trd>
  <trd lng="nl">tweehoorn</trd>
</drv>
</art>
