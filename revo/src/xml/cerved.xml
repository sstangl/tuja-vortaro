  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_n;&c_ib; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Hirsche</trd>
  <trd
  lng="el">&epsilon;&lambda;&alfa;&phi;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="es">c&eacute;rvidos</trd>
  <trd lng="fr">cervid&eacute;s</trd>
  <trd lng="hu">szarvasf&eacute;l&eacute;k</trd>
  <trd lng="pl">jeleniowate</trd>
  <trd lng="pt">cerv&iacute;deos</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_i; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;
  &c_p;&c_a;&c_r;&c_n;&c_o;&c_k;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
</drv>
