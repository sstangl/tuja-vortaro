<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: merin.xml,v 1.13 2012/07/21 10:10:25 revo Exp $">
<kap>
  <rad>merin</rad>/o
</kap>
<drv mrk="merin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Speco de &scirc;afo el hispana raso, kies longhara lano estas tre
      delikata.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_y;&c_n;&c_o;&c_s;</trd>
  <trd lng="ca">ovella merina</trd>
  <trd lng="de">Merinoschaf</trd>
  <trd lng="en">merino</trd>
  <trd lng="hu">merin&oacute;</trd>
  <trd lng="nl">merinosschaap</trd>
  <trd lng="pt">merino</trd>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_i;&c_n;&c_o;&c_s; <klr>(&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; &c_t;&c_o;&c_n;&c_k;&c_o;&c_r;&c_u;&c_n;&c_n;&c_y;&c_h; &c_o;&c_v;&c_je;&c_c;)</klr></trd>
</drv>

<drv mrk="merin.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a lano.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">mer&iacute;</trd>
  <trd lng="en">merino</trd>
  <trd lng="hu">merin&oacute;i</trd>
  <trd lng="pt">merino</trd>
</drv>

<drv mrk="merin.0olano">
  <kap>
    <tld/>olano
  </kap>
  <snc>
    <dif>
      Lano de <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">llana merina</trd>
  <trd lng="de">Merinowolle</trd>
  <trd lng="en">merino wool</trd>
  <trd lng="hu">merin&oacute;i <ind>gyapj&uacute;</ind></trd>
  <trd lng="nl">merinoswol</trd>
  <trd lng="pt">merino</trd>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_i;&c_n;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja; &c_sh;&c_je;&c_r;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="merin.0osxtofo">
  <kap>
    <tld/>o&scirc;tofo
  </kap>
  <snc>
    <dif>
      &Scirc;tofo farita el <tld/>olano:
      <ekz>
        <tld/>olana &scirc;alo.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">tela merina</trd>
  <trd lng="de">Merino</trd>
  <trd lng="hu">merin&oacute;<klr tip="amb"> (sz&ouml;vet)</klr></trd>
  <trd lng="nl">merinosstof</trd>
  <trdgrp lng="pt">
   <trd>merino</trd>,
   <trd>merin&oacute;</trd>
  </trdgrp>
</drv>
</art>
