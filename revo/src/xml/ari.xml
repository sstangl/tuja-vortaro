<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ari.xml,v 1.18 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ari</rad>/o
</kap>
<drv mrk="ari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Strofa, lirika <ref tip="super" cel="kant.0ajxo">kanta&jcirc;o</ref>
      por solisto, parto de ampleksa <ref tip="malprt"
      cel="kompon.0ajxo">kompona&jcirc;o</ref>,
      ekzemple de opero, oratorio, pasiono.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">&agrave;ria</trd>
  <trdgrp lng="cs">
    <trd>sólová píseň</trd>,
    <trd>árie</trd>
  </trdgrp>
  <trd lng="de">Arie</trd>
  <trd lng="en">aria</trd>
  <trd lng="es">aria</trd>
  <trd lng="fa">&a_A_madda;&a_r;&f_y;&a_A;<klr tip="ind"> (&a_m;&a_w;&a_s;&f_y;&a_q;&f_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>air<klr> (de musique)</klr></trd>,
    <trd>aria</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ria</trd>
  <trd lng="it">aria<klr> (mus.)</klr></trd>
  <trd lng="ru">&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">&aacute;ria</trd>
</drv>
</art>
