<vortaro>
<art mrk="$Id: vici.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>vici</rad>/o
</kap>
<drv mrk="vici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Vicia</trd>). Kelkaj specioj estas kulturataj,
      ekzemple
      la <ref tip="sub" cel="fab.0o">fabo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">g&egrave;nere Vicia</trd>
  <trd lng="cs">vikev</trd>
  <trd lng="de">Wicke</trd>
  <trd lng="en">vetch</trd>
  <trd lng="fr">vesce</trd>
  <trd lng="hu">b&uuml;kk&ouml;ny</trd>
  <trd lng="nl">wikke</trd>
  <trd lng="ru">&c_v;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>vika</trd>,
    <trd>ľadník kŕmny</trd>
  </trdgrp>
</drv>
</art>
