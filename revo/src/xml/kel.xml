<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kel.xml,v 1.21 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kel</rad>/o
</kap>
<drv mrk="kel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Subtera &ccirc;ambro, en kiu oni konservas provizojn,
      vinon, nutra&jcirc;on karbon, ktp:
      <ekz>
        <ind>li havas nek &ccirc;elon nek <tld/>on</ind>
        <klr>(li estas senhejmulo)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">il est &agrave; la <ind>rue</ind></trd>
      </ekz>
      <ekz>
        malfermita <tld/>o tentas al &scirc;telo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_v;&c_a;&c_l;</trd>,
      <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="br">kav</trd>
    <trd lng="de">Keller</trd>
    <trd lng="en">cellar</trd>
    <trdgrp lng="es">
      <trd>bodega</trd>,
      <trd>despensa</trd>,
      <trd>almac&eacute;n</trd>
    </trdgrp>
    <trd lng="fa">&a_z;&f_y;&a_r;&a_z;&a_m;&f_y;&a_n;</trd>
    <trdgrp lng="pt">
      <trd>adega</trd>,
      <trd>despensa</trd>,
      <trd>frasqueira</trd>,
      <trd>garrafeira</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;</trd>,
      <trd>&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      La tuta provizo da vino entenata en la <tld/>o:
      <ekz>
        li estis fiera de sia <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="br">kaviad</trd>
    <trd lng="de">Weinkeller</trd>
    <trd lng="fa">&a_A;&a_n;&a_b;&a_A;&a_r; &a_sh;&a_r;&a_A;&a_b;</trd>
    <trd lng="ru">&c_v;&c_i;&c_n;&c_n;&c_y;&c_j; <ind>&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>sklep</trd>,
    <trd>suterén</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cave<klr tip="ind"> (subst. f&eacute;m.)</klr></trd>,
    <trd>sous-sol</trd>
  </trdgrp>
  <trd lng="hu">pince</trd>
  <trd lng="nl">kelder</trd>
  <trd lng="sk">pivnica</trd>
</drv>

<drv mrk="kel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Servisto, kiu zorgas speciale pri la vinprovizoj.
    </dif>
  </snc>
  <trd lng="br">paotr ar c'hav</trd>
  <trd lng="cs">sklepník</trd>
  <trd lng="de">Kellermeister</trd>
  <trd lng="en">cellarman</trd>
  <trd lng="es">bodeguero</trd>
  <trdgrp lng="fr">
    <trd>caviste</trd>,
    <trd>sommelier</trd>,
    <trd>pr&eacute;pos&eacute; aux vins</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pincemester</trd>,
    <trd>pincemunk&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">bottelier</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j; &c_v;&c_i;&c_n;&c_n;&c_y;&c_m; &c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j; &c_v;&c_i;&c_n;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">vedúci pivnice</trd>
</drv>
</art>
