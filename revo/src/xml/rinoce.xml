<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rinoce.xml,v 1.18 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rinocer</rad>/o
</kap>

<drv mrk="rinoce.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Granda sova&gcirc;a dikha&ubreve;ta mambesto, kun unu a&ubreve; du pintaj kornoj
      elstarantaj sur la nazo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_a;&c_r;&c_o;&c_g;</trd>
  <trd lng="bg">&c_n;&c_o;&c_s;&c_o;&c_r;&c_o;&c_g;</trd>
  <trd lng="br">frikorneg</trd>
  <trd lng="ca">rinoceront</trd>
  <trd lng="cs">nosorožec</trd>
  <trdgrp lng="de">
    <trd>Rhinozeros</trd>,
    <trd>Nashorn</trd>
  </trdgrp>
  <trd lng="en">rhinoceros</trd>
  <trd lng="es">rinoceronte</trd>
  <trd lng="fr">rhinoc&eacute;ros</trd>
  <trdgrp lng="hu">
    <trd>orrszarv&uacute;</trd>,
    <trd>rinoc&eacute;rosz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neushoorn</trd>,
    <trd>rinoceros</trd>
  </trdgrp>
  <trd lng="pl">nosoro&zdot;ec</trd>
  <trd lng="pt">rinoceronte</trd>
  <trd lng="ru">&c_n;&c_o;&c_s;&c_o;&c_r;&c_o;&c_g;</trd>
  <trd lng="sk">nosorožec</trd>
  <trd lng="sv">nosh&ouml;rning</trd>
</drv>
</art>
