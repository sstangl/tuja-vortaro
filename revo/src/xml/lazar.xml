        li kriis per la&ubreve;ta vo&ccirc;o: <tld/>o, elvenu
        <fnt><bib>NT</bib>, <lok>&Joh; 11:43</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Ll&agrave;tzer</trd>
  <trd lng="de">Lazarus</trd>
  <trd lng="fr">Lazare</trd>
  <trd lng="hu">L&aacute;z&aacute;r</trd>
  <trd lng="it">Lazzaro</trd>
  <trd lng="nl">Lazarus</trd>
  <trd lng="pt">L&aacute;zaro</trd>
  <trd lng="ru">&c_L;&c_a;&c_z;&c_a;&c_r;&c_mol;</trd>
</drv>
</art>

