<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grop.xml,v 1.11 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>grop</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="grop.0o">
  <kap><tld/>o</kap>
  <snc mrk="grop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malanta&ubreve;a parto de la <ref tip="malprt"
      cel="dors.0o.ZOO">dorso</ref> de kvarpiedulo:
      <ekz>
        dusako, <klr>...</klr> kiu ku&scirc;is sur la <tld/>o de la
        &ccirc;evalo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 3a, p.
        40a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lumb.0o">lumbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh; 
  <klr tip="amb">(&c_u; &c_k;&c_a;&c_n;&c_ja;)</klr></trd>
  <trd lng="ca">gropa</trd>
  <trdgrp lng="cs">
    <trd>koňský zadek</trd>,
    <trd>ptačí zadek</trd>,
    <trd>zvířecí zadek</trd>
  </trdgrp>
  <trd lng="de">Kruppe</trd>
  <trd lng="fr">croupe</trd>
  <trd lng="hu">far</trd>
  <trd lng="pl">zad</trd>
  <trd lng="pt">garupa</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p;</trd>
  <trd lng="sk">kôň <klr>(športové náradie)</klr></trd>
</drv>
</art>
