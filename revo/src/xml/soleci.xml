  <ofc>2</ofc>
  <rad>solecism</rad>/o
</kap>
<drv mrk="soleci.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Sintaksa malkorekta&jcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="barbar1.0o">barbarismo</ref>.
    </refgrp>
