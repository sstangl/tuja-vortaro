  <trd lng="ca">solecisme</trd>
  <trd lng="cs">solecizmus</trd>
  <trd lng="de">Sol&ouml;zismus</trd>
  <trd lng="fr">sol&eacute;cisme</trd>
  <trdgrp lng="hu">
    <trd>durva nyelvtani <ind>hiba</ind></trd>,
    <trd>szolecizmus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>solecisme</trd>,
    <trd>stijlfout</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_l;&c_je;&c_c;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_s;&c_i;&c_n;&c_t;&c_a;&c_k;&c_s;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">solecizmus</trd>
</drv>
</art>
