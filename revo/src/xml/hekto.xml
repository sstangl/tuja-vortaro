<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hekto.xml,v 1.2 2013/09/09 10:10:18 revo Exp $">
<kap>
  <rad>hekto</rad>- <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hekto.0">
  <kap><tld/>-</kap>
  <mlg>h</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="hekto.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Cent&rightquot; (100)
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        rajdinte kelkajn <tld/>metrojn en la arbaron, li staris sub 
        areto da altaj arboj, kiuj lin &scirc;irmetis kontra&ubreve; la ventego
        <fnt>
          <aut>T. Smollett, trad. D. H. Lambert</aut>,
          <vrk>Teruriga Aventuro de Ferdinando, Grafo Fathom</vrk>,
          <lok><url
          ref="http://www.gutenberg.org/files/34042/34042-0.txt">
          The Esperantist, 1904, vol. 1a, num. 8a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_k;&c_t;&c_a;-</trd>
  <trd lng="bg">&c_h;&c_je;&c_k;&c_t;&c_o;-</trd>
  <trd lng="br">hekto-</trd>
  <trd lng="ca">hecto-</trd>
  <trd lng="de">Hekto-</trd>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron_ton;-</trd>
  <trd lng="en">hecto-</trd>
  <trd lng="es">hecto-</trd>
  <trd lng="fa">&a_h;&f_k;&a_t;&a_w;</trd>
  <trd lng="fr">hecto-</trd>
  <trd lng="hu">hekto-</trd>
  <trd lng="it">etto-</trd>
  <trd lng="nl">hecto-</trd>
  <trd lng="no">hekto-</trd>
  <trd lng="pl">hekto-</trd>
  <trd lng="pt">hecto-</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;-</trd>
  <trd lng="sv">hekto-</trd>
</drv>
</art>

