<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: buhxor.xml,v 1.7 2013/12/02 10:10:25 revo Exp $">
    <kap>
      <rad>Bu&hcirc;or</rad>/o
    </kap>
    <drv mrk="buhxor.B0o">
      <kap>
        <tld/>o
      </kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
        <dif>
          Provinca &ccirc;efurbo en Uzbekujo; historie, grava centro
          budaisma, poste kaj precipe, granda kaj fama urbo de la
          islama mondo; iama &ccirc;efurbo de la regno Samanida, poste de
          <tld/>aj &hcirc;anujo kaj emirujo.
        </dif>
      </snc>
      <trd lng="be">&c_B;&c_u;&c_h;&c_a;&c_r;&c_a;</trd>
      <trd lng="bg">&c_B;&c_u;&c_h;&c_a;&c_r;&c_a;</trd>
      <trd lng="de">Buchara</trd>
      <trdgrp lng="en">
        <trd>Bukhara</trd>,
        <trd>Buxoro</trd>
      </trdgrp>
      <trd lng="fr">Boukhara</trd>
      <trd lng="hu">Buhara</trd>
      <trd lng="ru">&c_B;&c_u;&c_h;&c_a;&c_r;&c_a;</trd>
      <trdgrp lng="sv">
        <trd>Buchara</trd>,
        <trd>Bukhara</trd>,
        <trd>Buxoro</trd>
      </trdgrp>
    </drv>

  </art>

