<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trakta.xml,v 1.25 2016/02/28 19:10:56 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>traktat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trakta.0o">
  <kap><tld/>o</kap>
  <snc mrk="trakta.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Interregna <ref tip="super" cel="kontra3.0o.JUR">kontrakto</ref>:
      <ekz>
        <tld/>o pri paco
        <klr>(= <ref cel="kontra3.pac0o">packontrakto</ref>)</klr>;
      </ekz>
      <ekz>
        komerca <tld/>o inter Germanujo kaj Rusujo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_t;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_o;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vertrag</trd>,
      <trd>Staatsvertrag</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>treaty</trd>,
      <trd>pact</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_p;&f_y;&a_m;&a_A;&a_n;</trd>,
      <trd>&a_q;&a_r;&a_A;&a_r;&a_d;&a_A;&a_d;</trd>,
      <trd>&a_m;&a_ayn;&a_A;&a_h;&a_d;&a_h;</trd>,
      <trd>&a_ayn;&a_h;&a_d;&a_n;&a_A;&a_m;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;llamszerz&odblac;d&eacute;s</trd>,
      <trd>&aacute;llamk&ouml;zi <ind>egyezm&eacute;ny</ind></trd>
    </trdgrp>
    <trd lng="nl">verdrag</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_t;<klr> (&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;, &c_u;&c_s;&c_t;.)</klr></trd>,
      <trd>&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;<klr> (&c_m;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="trakta.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Trakta&jcirc;o, verko en kiu oni orde kaj metode
      traktas iun temon:
      <ekz>
        <tld/>o pri geometrio.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_t;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Traktat</trd>,
      <trd>Abhandlung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>treatise</trd>,
      <trd>discourse</trd>,
      <trd>monograph</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_r;&a_s;&a_A;&a_l;&a_h;</trd>,
      <trd>&a_m;&a_q;&a_A;&a_l;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">trait&eacute;</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rtekez&eacute;s</trd>,
      <trd>tanulm&aacute;ny</trd>
    </trdgrp>
    <trd lng="nl">verhandeling</trd>
    <trd lng="pt">tratado</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_a;&c_k;&c_t;&c_a;&c_t;</trd>,
      <trd>&c_n;&c_a;&c_u;&c_ch;&c_n;&c_y;&c_j; &c_t;&c_r;&c_u;&c_d;</trd>,
      <trd>&c_m;&c_o;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
   <ref tip="hom" cel="trakt.0i">trakt/at/o</ref>
  <trd lng="ca">tractat</trd>
  <trdgrp lng="cs">
    <trd>mezin&aacute;rodn&iacute; smlouva</trd>,
    <trd>trakt&aacute;t</trd>,
    <trd>u&ccaron;en&eacute; pojedn&aacute;n&iacute;</trd>,
    <trd>v&ecaron;deck&aacute; rozprava</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>medzin&aacute;rodn&aacute; zmluva</trd>,
    <trd>trakt&aacute;t</trd>,
    <trd>vedeck&aacute; rozprava</trd>
  </trdgrp>
</drv>

<drv mrk="trakta.0o.NATO">
  <kap>Nord-Atlantika <tld lit="T"/>-Organiza&jcirc;o</kap>
  <mlg>NATO</mlg>
  <uzo tip="fak">POL</uzo>
  <fnt><bib>ManPol</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="alianc.0o"
      lst="voko:organiza&jcirc;oj">Alianco</ref> de suverenaj <ref tip="vid"
      cel="sxtat.0o">&scirc;tato</ref>j,
      fondita por komuna defendo de okcidenta <ref tip="vid"
      cel="euxrop1.E0o">E&ubreve;ropo</ref>, Kanado kaj <ref tip="vid"
      cel="uson.U0o">Usono</ref>:
      <ekz>
        la lastan tagon de la milito, &scirc;i biciklis al la centro de la
        urbo
        por atendi la trupojn de NATO, &ccirc;ar tiel informis &scirc;in iu
        maljunulo.
        <fnt><aut>Bardhyl SELIMI</aut>,
        <vrk><url ref="&Monato;/007205.html">Voja&gcirc;impresoj, somero
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 01/2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        efektive, la&ubreve; ekonomia politiko kaj internaciaj celoj (evoluigi
        sukcesan sociliberalan kapitalismon kaj esti akceptita en
        E&ubreve;ropa
        Unio kaj en Nord-Atlantika <tld lit="T"/>-Organiza&jcirc;o), &ccirc;iuj
        estas
        egalaj.
        <fnt><aut>Z. Ti&scirc;ljar</aut>,
        <vrk><url ref="&Monato;/007251.html">Ordo tordita - dekstren
        maldekstren</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 01/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja; &c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_a;&c_t;&c_l;&c_ja;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_j;
    <ind>&c_d;&c_a;&c_m;&c_o;&c_v;&c_y;</ind></trd>,
    <trd>&c_N;&c_A;&c_T;&c_O;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;&c_t;&c_a; &c_n;&c_a;
    &c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_o;&c_a;&c_t;&c_l;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_ja;
    <ind>&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;</ind></trd>,
    <trd>&c_N;&c_A;&c_T;&c_O;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; del Tractat de l'Atl&agrave;ntic Nord</trd>,
    <trd>Alian&ccedil;a Atl&agrave;ntica</trd>,
    <trd>OTAN</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nordatlantikpakt</trd>,
    <trd>NATO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>North Atlantic <ind>Treaty</ind> Organisation</trd>,
    <trd>NATO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n <ind>Tratado</ind> de Atl&aacute;ntico Norte</trd>,
    <trd>OTAN</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_z;&a_m;&a_A;&a_n; <ind>&f_p;&f_y;&a_m;&a_A;&a_n;</ind> &a_A_madda;&a_t;&a_l;&a_A;&a_n;&a_t;&f_y;&f_k;
    &a_sh;&a_m;&a_A;&a_l;&f_y;</trd>,
    <trd>&a_n;&a_A;&a_t;&a_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation pour le <ind>Trait&eacute;</ind> de l'Atlantique
    Nord</trd>,
    <trd>OTAN</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&Eacute;szak-atlanti <ind>Szerz&odblac;d&eacute;s</ind> Szervezete</trd>,
    <trd>NATO</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Noord-Atlantische <ind>Verdragsorganisatie</ind></trd>,
    <trd>NAVO</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja; &c_S;&c_je;&c_v;&c_je;&c_r;&c_o;&c_a;&c_t;&c_l;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_g;&c_o;
    <ind>&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_a;</ind></trd>,
    <trd>&c_N;&c_A;&c_T;&c_O;</trd>
  </trdgrp>
</drv>
</art>
