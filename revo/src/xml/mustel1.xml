<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mustel1.xml,v 1.11 2013/10/21 18:10:10 revo Exp $">
<kap>
  <rad>musteled</rad>/oj
</kap>
<drv mrk="mustel1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Mustelidae</trd>), al kiu apartenas
      <refgrp tip="prt">
        <ref cel="lutr.0o">lutro</ref>,
        <ref cel="martes.0o">marteso</ref>,
        <ref cel="mustel.0o">mustelo</ref>,
        k.a.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">must&egrave;lids</trd>
  <trd lng="de">Marder</trd>
  <trd lng="hu">meny&eacute;tf&eacute;l&eacute;k</trd>
  <trd lng="nl">marterachtigen</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;asicowate</trd>,
    <trd>kunowate</trd>
  </trdgrp>
  <trd lng="pt">mustel&iacute;deos <klr tip="amb">(zool.)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_mol;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
