<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karik.xml,v 1.13 2012/09/01 20:10:18 revo Exp $">
<kap>
  <rad>karik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karik.0o">
  <kap><tld/>o</kap>
  <snc mrk="karik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Carica</trd>),
      kies specio melonarbo (<trd lng="la">Carica papaya</trd>)
      estas kultivata pro la melonsimilaj fruktoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_a;&c_ja; <klr tip="amb">(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="ca">papaier</trd>
  <trdgrp lng="de">
    <trd>Melonenbaum</trd>,
    <trd>Papaya</trd>
  </trdgrp>
  <trd lng="en">papaya</trd>
  <trd lng="fa">&a_kh;&a_r;&a_b;&a_z;&a_h;&a_hamza_sure; &a_d;&a_r;&a_kh;&a_t;&f_y;</trd>
  <trd lng="fr">papayer</trd>
  <trdgrp lng="hu">
    <trd>dinnyefa</trd>,
    <trd>papaja</trd>
  </trdgrp>
  <trd lng="nl">papayaboom</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_y;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_a;&c_j;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
