<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glat.xml,v 1.22 2015/07/04 08:37:35 revo Exp $">
<kap><ofc>*</ofc>
<rad>glat</rad>/a</kap>
<drv mrk="glat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Malraspa, sen elstara&jcirc;oj sur la supra&jcirc;o; sur kiu la fingro
      facile glitas:
      <ekz>
        <tld/>a vitro, &scirc;tofo, ha&ubreve;to;
      </ekz>
      <ekz>
        la akvo fari&gcirc;is <tld/>a kiel spegulo
        <fnt><bib>Fab2</bib>, <lok>Najbaraj familioj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel manplato;
      </ekz>
      <ekz>finita kaj <tld/>igita
        <klr>(tute preta)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>e kombitaj haroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>e razi iun;
      </ekz>
      <ekz>
        Esav estas ja homo harkovrita kaj mi <klr>(Jakob)</klr>
        estas homo <tld/>a
        <fnt><bib>MT</bib>, <lok>&Gen; 27:11</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>suau</trd>,
      <trd>llis</trd>
    </trdgrp>
 <trd lng="es">liso, -a</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senobstrukca, senembarasa:
      <ekz>
        <tld/>a vojo;
      </ekz>
      <ekz>
        vojo <tld/>a kiel bilardo;
      </ekz>
      <ekz>
        inter la <tld/>a&jcirc;oj de la valo estas via parto
        <fnt><bib>MT</bib>, <lok>&Jes; 57:6</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lliure</trd>,
      <trd>rodat</trd>
    </trdgrp>
 <trd lng="es">sin obst&aacute;culos</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facila, progresanta sen malhelpoj a&ubreve; baroj:
      <ekz>
        <tld/>a laboro, stilo, parolo;
      </ekz>
      <ekz>vivo <ind><tld/>e</ind> ne fluas, &ccirc;iam batas kaj skuas;
        <trd lng="fr">bien<klr tip="ind"> (sans
        anicroche)</klr></trd><fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        mi esperas, ke &ccirc;io <tld/>e pasos<fnt>B</fnt>;
      </ekz>
      <ekz>vivi sate kaj <tld/>e<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>e travivi la tagojn;
      </ekz>
      <ekz>la afero ne iras <tld/>e
        (<ref tip="vid"
        cel="tuber.0o">tubero</ref>)<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>por <tld/>e mensogi oni spriton
      bezonas<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la adapti&gcirc;o al la novaj kondi&ccirc;oj ne
        plenumi&gcirc;is tute <tld/>e;
      </ekz>
      <ekz>
        li <tld/>e venkis en la konkurso.
      </ekz>
      <trd lng="es">f&aacute;cil</trd>
    </dif>
    <refgrp tip="vid">
      <ref cel="rel.0o">relo</ref>,
      <ref cel="buter.0o">butero</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>flu&iuml;d</trd>,
      <trd>natural</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karesa, dol&ccirc;a, lerte flata:
      <ekz>
        virino, kies paroloj estas <tld/>aj
        <fnt><bib>MT</bib>, <lok>&Sen; 2:16</lok></fnt>;
      </ekz>
      <ekz>
        per sia <tld/>a bu&scirc;o &scirc;i lin entiris
        <fnt><bib>MT</bib>, <lok>&Sen; 7:21</lok></fnt>;
      </ekz>
      <ekz>
        la bu&scirc;o de mal&ccirc;astulino elver&scirc;as mielon kaj
        &scirc;ia gor&gcirc;o estas pli <tld/>a ol oleo
        <fnt><bib>MT</bib>, <lok>&Sen; 5:3</lok></fnt>;
      </ekz>
      <ekz>
        por gardi vin kontra&ubreve; <tld/>a lango de fremdulino
        <fnt><bib>MT</bib>, <lok>&Sen; 6:24</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e komplimenti iun.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>dol&ccedil;</trd>,
      <trd>tendre</trd>
    </trdgrp>
 <trdgrp lng="es">
      <trd>gentil</trd>,
   <trd>suave<klr> (figurativo)</klr></trd>,
   <trd>dulce<klr> (figurativo)</klr></trd>
 </trdgrp>
    <trd lng="en">slick</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">hladký</trd>
  <trd lng="de">glatt</trd>
  <trd lng="en">smooth</trd>
  <trdgrp lng="fr">
    <trd>&eacute;gal<klr tip="ind"> (sans asp&eacute;rit&eacute;)</klr></trd>,
    <trd>lisse</trd>,
    <trd>plat<klr tip="ind"> (lisse)</klr></trd>,
    <trd>poli<klr tip="ind"> (lisse)</klr></trd>,
    <trd>sans <ind>asp&eacute;rit&eacute;</ind></trd>,
    <trd>fluide<klr tip="ind"> (sans &agrave;-coup)</klr></trd>,
    <trd>coulant<klr tip="ind"> (sans &agrave;-coup)</klr></trd>,
    <trd>sans <ind>&agrave;-coup</ind></trd>,
    <trd>glabre</trd>,
    <trd>flatteur</trd>,
    <trd>imberbe</trd>,
    <trd>sans <ind>anicroche</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&lamed;&qof;</trd>,
    <trd>&het;&lamed;&qof;&lamed;&qof;</trd>,
    <trd>&mem;&lamed;&vav;&tet;&shin;</trd>
  </trdgrp>
  <trd lng="nl">glad</trd>
  <trdgrp lng="pt">
   <trd>liso<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>plano<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>polido<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladký</trd>,
    <trd>plochý</trd>,
    <trd>rovný</trd>
  </trdgrp>
</drv>

<drv mrk="glat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi sian barbon;
      </ekz>
      <ekz>
        karese <tld/>igi <klr>(ordigi)</klr> al knabo la harojn;
      </ekz>
      <ekz>
        <klr>[la vento]</klr> plugas, ree <tld/>igas, tordas, alpelas
        novajn akvajn montojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi <klr>(trabati)</klr> la vojon al iu;
      </ekz>
      <ekz>
        <tld/>igi <klr>(reguligi)</klr> sian stilon;
      </ekz>
      <ekz>
        <tld/>igi la malfacila&jcirc;ojn;
      </ekz>
      <ekz>
        lia malafabla viza&gcirc;o iom <tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="glad.0i">gladi.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>allisar</trd>,
    <trd>aplanar</trd>,
    <trd>igualar</trd>,
    <trd>deslliurar</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hladit</trd>,
    <trd>uhladit</trd>,
    <trd>uhlazovat</trd>,
    <trd>urovnat</trd>,
    <trd>vyhladit</trd>,
    <trd>zahladit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gl&auml;tten</trd>,
    <trd><ind>glatt</ind> streichen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>smooth</trd>,
    <trd>smoothen</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>alisar</trd>, 
   <trd>suavizar<klr> (figurativo)</klr></trd>, 
   <trd>pulir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lisser</trd>,
    <trd>aplanir<klr tip="ind"> (rendre lisse, supprim&eacute; les
    difficult&eacute;s)</klr></trd>,
    <trd>peaufiner</trd>,
    <trd>polir<klr tip="ind"> (peaufiner)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&het;&lamed;&yod;&qof;</ind></trd>,
    <trd>&lamed;<ind>&lamed;&tet;&shin;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>glad maken</trd>,
    <trd>glad <ind>strijken</ind></trd>,
    <trd>polijsten</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>alisar</trd>, 
   <trd>polir</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyhladiť</trd>
</drv>

    <drv mrk="glat.0umi">
      <kap><tld/>umi <fnt><bib>PIV1</bib></fnt></kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Konduki (plej ofte, ripete kaj samdirekte)  la
          manplaton a&ubreve; la fingrojn sur io karesata:
          <ekz>
            infano <tld/>umas la hundon;
          </ekz><ekz>
            li manon || Unu al sia nigra silka zono metis, ||
            Per dua sian grizan barbon <tld/>umetis
            <fnt>
              <aut>Adam Mickiewicz, tr. Grabowski</aut>:
              <vrk>Sinjoro Tadeo</vrk>. <lok>Libro 4a</lok>.
            </fnt>;
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ca">
        <trd>acariciar</trd>,
        <trd>acaronar</trd>
      </trdgrp>
      <trd lng="en">stroke</trd>
      <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
    </drv>

<drv mrk="glat.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Raspa, maldol&ccirc;a por la tu&scirc;o; obstrukca; malfacila:
      <ekz>
        mal<tld/>a lano, vojo, verko, vivo;
      </ekz>
      <ekz>
        mal<tld/>aj versoj;
      </ekz>
      <ekz>
        <ind>mal<tld/>a&jcirc;o</ind>.
        <trdgrp lng="ca">
          <trd>entrebanc, trava</trd>,
          <trd>aspresa</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>rugosit&eacute;<klr tip="ind"> (in&eacute;galit&eacute; de
          surface)</klr></trd>,
          <trd>accroc<klr tip="ind"> (incident)</klr></trd>,
          <trd>anicroche</trd>,
          <trd>incident</trd>,
          <trd>obstacle</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aspre</trd>,
    <trd>abrupte</trd>,
    <trd>dif&iacute;cil</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drsný</trd>,
    <trd>hrbolatý</trd>,
    <trd>nehladký</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rau</trd>,
    <trd>grob</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rough</trd>,
    <trd>coarse</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>&aacute;spero, -a</trd>, 
   <trd>dif&iacute;cil<klr> (figurativo)</klr></trd>, 
   <trd>carrasposo, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rugueux</trd>,
    <trd>r&ecirc;che</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&het;&vav;&samekh;&pe;&samekh;</trd>,
    <trd>&lamed;&alef; <ind>&het;&lamed;&qof;</ind></trd>,
    <trd>&gimel;&samekh;</trd>
  </trdgrp>
  <trd lng="nl">ruw<klr tip="ind"> (niet glad)</klr></trd>
  <trdgrp lng="pt">
   <trd>&aacute;spero</trd>, 
   <trd>rugoso</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_sh;&c_je;&c_r;&c_o;&c_h;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drapľavý</trd>,
    <trd>hrboľatý</trd>,
    <trd>nerovný</trd>
  </trdgrp>
</drv>
</art>
