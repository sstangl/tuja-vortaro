<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tusx.xml,v 1.24 2011/04/29 19:10:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tu&scirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tusx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tusx.0i.fingre">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Senpere alproksimigi la fingrojn a&ubreve; ian parton de la korpo
      al io a&ubreve;
      iu:
      <ekz>infano, ne <tld/>u la spegulon<fnt><bib>F</bib>,
      <lok>&FE; 20</lok></fnt>!</ekz>;
      <ekz>
        liaj lipoj apena&ubreve; <tld/>is la glason
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        &gcirc;i e&ccirc; ne <tld/>as lian orelon
        <klr>(= li estas indiferenta)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&eacute;rint</trd>
    <trdgrp lng="pl">
      <trd>dotyka&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tusx.0i.esti_en_kontakto">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Atingi ion, kontakti&gcirc;i kun io:
      <ekz>
        longa kurteno <tld/>anta la plankon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="traf.0i">trafi</ref>.
    </refgrp>
  <trd lng="hu">&eacute;r <klr tip="amb">(vmeddig)</klr></trd>
  <trdgrp lng="pl">
    <trd>dotyka&cacute;</trd>
  </trdgrp>
  </snc>
  <snc mrk="tusx.0i.mise_modifi">
    <dif>
      Fari malbonajn &scirc;an&gcirc;ojn en io:
      <ekz>
        etendu Vian manon kaj ek<tld/>u &ccirc;ion, kion li
        havas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne <tld/>u miajn sanktoleitojn, kaj al miaj profetoj ne faru
        malbonon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        energie batali la ek<tld/>on de tiu principo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la fundamento de nia lingvo devas resti por &ccirc;iam
        ne<tld/>ebla<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">hozz&aacute;ny&uacute;l</trd>
    <trdgrp lng="pl">
      <trd>rusza&cacute;</trd>,
      <trd>narusza&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tusx.0i.pritrakti_temon">
    <dif>
      Mallonge kaj akcesore pritrakti temon:
      <ekz>
        dum la kunveno oni <tld/>is tiun demandon;
      </ekz>
      <ekz>
        mi volas hodia&ubreve; <tld/>i la aferon nur per malmultaj
        vortoj.
      </ekz>
    </dif>
    <trd lng="hu">&eacute;rint <klr tip="amb">(&aacute;tv.)</klr></trd>
    <trd lng="pl">porusza&cacute;<klr> temat, zagadnienie</klr></trd>
  </snc>
  <snc mrk="tusx.0i.impresi">
    <dif>
      Forte impresi ies koron, spiriton, animon:
      <ekz>
        &gcirc;i <tld/>as lin kiel akvo anseron
        <klr>(neniel)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tiu respondo malagrable <tld/>is lin<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ita de scivolemo, admiro, timo;
      </ekz>
      <ekz>
        &scirc;i estis <tld/>ita &gcirc;is larmoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>anta sceno<fnt>B</fnt>,
      </ekz>
      <ekz>
        rakonto<trdgrp lng="pl">
          <trd></trd>
        </trdgrp>;
      </ekz>
      <ekz>
        senkulpa kaj <tld/>anta vo&ccirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>anta (= elvokanta) pentra&jcirc;o de bruna,
        &scirc;a&ubreve;ma biera
        kru&ccirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        paroli kun <tld/>iteco de l' koro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">meghat</trd>
    <trdgrp lng="pl">
      <trd>porusza&cacute;</trd>,
      <trd>wzrusza&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tusx.0i.koncerni">
    <dif>
      <ref cel="koncer.0i">Koncerni</ref>,
      rilati al:
      <ekz>
        min ne <tld/>as la afero mi staras ekstere
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per kio tio <tld/>as min?<fnt>Z</fnt>
      </ekz>
      <ekz>
        le&gcirc;o pasinta&jcirc;on ne <tld/>as
        <klr>(valoras nur por poste sed ne por anta&ubreve;e)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        decidoj <tld/>antaj la dubajn demandojn de la lingvo.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>tyczy&cacute;</trd>,
      <trd>dotyczy&cacute;</trd>
    </trdgrp>
    <trd lng="hu">vonatkozik</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">dotknout se</trd>
  <trd lng="de">ber&uuml;hren</trd>
  <trd lng="en">touch</trd>
  <trd lng="es">tocar</trd>
  <trd lng="fr">toucher</trd>
  <trd lng="nl">aanraken</trd>
  <trdgrp lng="pt">
   <trd>tocar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_o;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_t;&c_r;&c_o;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_k;&c_o;&c_s;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>r&ouml;ra<klr> (vid)</klr></trd>,
    <trd>ber&ouml;ra</trd>
  </trdgrp>
</drv>

<drv mrk="tusx.0egi">
  <kap><tld/>egi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Maldelikate kaj abrupte interfrapi&gcirc;i kun io:
      <ekz>
        ek<tld/>egi meblon en mallumo;
      </ekz>
      <ekz>
        tiu reformo <tld/>egi&gcirc;us kontra&ubreve; nevenkebla
        kontra&ubreve;staro
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolizi.0i">kolizii</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_u;&c_r;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ansto&szlig;en</trd>
  <trd lng="es">chocar<klr tip="amb"> (con algo)</klr></trd>
  <trd lng="fr">heurter</trd>
  <trd lng="hu">bele&uuml;tk&ouml;zik</trd>
  <trd lng="nl">stoten</trd>
  <trdgrp lng="pl">
    <trd>zawadza&cacute;<klr> o co&sacute;</klr></trd>,
    <trd>wpada&cacute;<klr> na co&sacute;, kogo&sacute;</klr></trd>,
    <trd>potr&aogonek;ca&cacute;<klr> co&sacute;,
    kogo&sacute;</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="tusx.0eti">
  <kap><tld/>eti<fnt>Z</fnt>,
  <var><kap>flug<tld/>i</kap></var>,
  <var><kap>duon<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Delikate apena&ubreve; <tld/>i
    </dif>
    <refgrp tip="vid">
      <ref cel="frot">froteti</ref>,
      <ref cel="kares.0i">karesi</ref>,
      <ref cel="kis.0i">kisi</ref>,
      <ref cel="lek.0i">leki</ref>,
      <ref cel="flug">preterflugi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">streicheln</trd>
  <trd lng="fr">effleurer</trd>
  <trd lng="hu">&eacute;pp hogy <ind>&eacute;rint</ind></trd>
  <trd lng="pl">musn&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_je;&c_g;&c_k;&c_a;
    <ind>&c_k;&c_o;&c_s;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>,
    <trd>&c_z;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tusx.inter0i">
  <kap>inter<tld/>i</kap>
  <snc mrk="tusx.inter0i.KOMUNE">
    <dif>
      <tld lit="T"/>i unu la alian, ne lasi interspacon,
      <ref tip="vid" cel="kontak.0i.havi_0on">kontakti</ref>:
      <ekz>
        Maribor <klr>...</klr> ku&scirc;as &ccirc;e la rivero Drava, kie 
        inter<tld/>as kvin diversaj provincoj
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.burger.si/Maribor/Maribor_esperanto.html">
          Maribor</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>toucher</ind></trd>
</drv>

<drv mrk="tusx.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Korekti desegnon, gravuron, pentra&jcirc;on a&ubreve; fotografan
      kli&scirc;a&jcirc;on.
    </dif>
    <trd
    lng="be">&c_r;&c_e;&c_t;&c_u;&c_sh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">retuschieren</trd>
    <trd lng="es">retocar</trd>
    <trd lng="hu">retus&aacute;l</trd>
    <trdgrp lng="pl">
      <trd>retuszowa&cacute;</trd>,
      <trd>dokona&cacute; <ind>retuszu</ind></trd>,
      <trd>zrobi&cacute; <ind>retusz</ind></trd>
    </trdgrp>
    <trd
    lng="ru">&c_r;&c_je;&c_t;&c_u;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Korekti, plibonigi ion ajn.
    </dif>
    <trd lng="de">verbessern</trd>
    <trdgrp lng="hu">
      <trd>kijav&iacute;t</trd>,
      <trd>csiszol <klr tip="amb">(&aacute;tv.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>retuszowa&cacute;</trd>,
      <trd>dokona&cacute; <ind>retuszu</ind></trd>,
      <trd>zrobi&cacute; <ind>retusz</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr
    tip="amb">(&c_n;&c_a;&c_p;&c_r;. &c_o;&c_d;&c_je;&c_zh;&c_d;&c_u;
    &c_p;&c_o; &c_f;&c_i;&c_g;&c_u;&c_r;&c_je;)</klr></trd>
  </snc>
  <trd lng="fr">retoucher</trd>
  <trd lng="nl">retoucheren</trd>
  <trd lng="sv">retuschera</trd>
</drv>
</art>
