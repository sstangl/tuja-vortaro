<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fotoliz.xml,v 1.6 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fotoliz</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="fotoliz.0i">
  <kap><tld/>i</kap>
  <snc mrk="fotoliz.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Malkomponi per luma efiko.
    </dif>
  </snc>
  <trd lng="ca">produir-se la fot&ograve;lisi</trd>
  <trd lng="cs">provádět fotolýzu</trd>
  <trd lng="fr">photolyser</trd>
  <trd lng="sk">rozkladať svetlom</trd>
</drv>

<drv mrk="fotoliz.0o">
  <kap><tld/>o</kap>
  <snc mrk="fotoliz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Okazado, efiko, rezulto de <tld/>ado:
      <ekz>
        en malprofundaj akvoj, PCP anka&ubreve; estas rapide forigita per 
        <tld/>o
        <fnt>
          <vrk><url
          ref="http://epo.wikitrans.net/Pentaklorofenol?eng=Pentachlorophenol">
          pentaklorofenol</url></vrk>,
          <lok>WikiTrans, 2012</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">fot&ograve;lisi</trd>
  <trd lng="cs">fotolýza</trd>
  <trd lng="fr">photolyse</trd>
  <trd lng="sk">fotolýza</trd>
</drv>
</art>

