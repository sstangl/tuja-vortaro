<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grenad1.xml,v 1.8 2015/01/10 13:10:23 revo Exp $">
<kap>
  <rad>Grenad</rad>/o
</kap>

<drv mrk="grenad1.0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="grenad1.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>GD</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en la samnoma insulo (kaj kelkaj apudaj).
    </dif>
    <trdgrp lng="pl">
      <trd>Grenada</trd>
    </trdgrp>
  </snc>
  <snc mrk="grenad1.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref> 
      en la Kariba <ref tip="malprt" cel="mar.0o">Maro</ref>,
      teritorio de <ref tip="vid" cel="grenad1.0o.sxtato">&scirc;tato
      Grenado</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Grenada</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_G;&c_r;&c_je;&c_n;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">Grenada</trd>
  <trd lng="de">Grenada</trd>
  <trd lng="en">Grenada</trd>
  <trd lng="fr">Grenade</trd>
  <trd lng="id">Grenada</trd> 
  <trd lng="pt">Granada</trd>
</drv>

</art>

