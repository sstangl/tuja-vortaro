<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pimp.xml,v 1.6 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pimp</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pimp.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref> pro spontanea, vigla
      fre&scirc;o:
      <ekz>
        &gcirc;iaj &ccirc;iam tiel fre&scirc;e lavitaj kaj <tld/>aj
        enlo&gcirc;antoj
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        16a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elegan.0a">eleganta</ref>,
      <ref cel="sxik.0a">&scirc;ika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>elegantní</trd>,
    <trd>přitažlivý</trd>,
    <trd>svůdný</trd>,
    <trd>vystrojený</trd>,
    <trd>švarný</trd>
  </trdgrp>
  <trd lng="fr">pimpant</trd>
  <trdgrp lng="nl">
    <trd>fleurig</trd>,
    <trd>elegant</trd>,
    <trd>koket</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elegancki</trd>,
    <trd>strojny</trd>,
    <trd>wymuskany</trd>,
    <trd>wytworny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sedutora</trd>,
    <trd>atraente</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pôvabný</trd>,
    <trd>vábivý</trd>,
    <trd>švárny</trd>
  </trdgrp>
</drv>
</art>

