<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: umbili.xml,v 1.27 2015/07/04 08:37:45 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>umbilik</rad>/o  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="umbili.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="umbili.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Ronda kaveto meze de ventro de mamuloj,
       produktita de cikatro de la
       <tld/>a &scirc;nuro.
     </dif>
     <trd lng="bg">&c_p;&c_malmol;&c_p;</trd>
     <trd lng="br">begel</trd>
     <trd lng="ca">melic</trd>
     <trd lng="de">Nabel</trd>
     <trd lng="fi">napa<klr tip="amb"> (anat.)</klr></trd>
     <trd lng="en">navel</trd>
     <trd lng="es">ombligo</trd>
     <trdgrp lng="fr">
       <trd>nombril</trd>,
       <trd>ombilic</trd>
     </trdgrp>
     <trd lng="he">&tet;&bet;&vav;&resh;<klr tip="amb">
("&pe;&vav;&pe;&yod;&qof;")</klr></trd>
     <trd lng="id">pusar</trd>
     <trd lng="it">ombelico</trd>
     <trd lng="nl">navel</trd>
     <trd lng="pl">p&eogonek;pek</trd>
     <trd lng="pt">umbigo</trd>
     <trd lng="ru">&c_p;&c_u;&c_p;&c_o;&c_k;</trd>
     <trd lng="sv">navel</trd>
   </snc>
   <snc mrk="umbili.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <ref tip="dif" cel="hilum.0o">hilumo</ref>
     <trd lng="fi"><klr tip="amb">(siemenen) </klr>arpi</trd>
     <trd lng="ca">h&iacute;lum</trd>
     <trdgrp lng="nl">
       <trd>hilum</trd>,
       <trd>zaadnavel</trd>
     </trdgrp>
     <trd lng="pl">osadka</trd>
     <trd lng="ru">&c_r;&c_u;&c_b;&c_ch;&c_i;&c_k;</trd>
   </snc>
  <trdgrp lng="ca">
     <trd>llombr&iacute;gol</trd>,
     <trd>melic</trd>
   </trdgrp>
  <trd lng="cs">jizva <klr>(místo na semeni rostlin| jímž bylo semeno připevněno k poutku)</klr></trd>
  <trd lng="hu">k&ouml;ld&ouml;k</trd>
  <trd lng="sk">jazva <klr>(miesto kde bolo semeno pripevnené)</klr></trd>
</drv>

<drv mrk="umbili.0istino">
  <kap><tld/>istino</kap>
  <snc mrk="umbili.0istino.MED">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="akusx.0istino.MED">aku&scirc;istino</ref>
    <ekz>
      ni lavu nin puraj, kvaza&ubreve; ni estus venintaj
      el la viglaj manoj de la <tld/>istino
      <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
      fratoj</vrk>, <lok>1947</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">accoucheuse</trd>
</drv>

<drv mrk="umbili.0asxnuro">
   <kap><tld/>a &scirc;nuro</kap>
   <snc mrk="umbili.0asxnuro.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Membrana tubo, kuniganta feton kun placento.
     </dif>
   </snc>
   <trd lng="bg">&c_p;&c_malmol;&c_p;&c_n;&c_a; &c_v;&c_r;&c_malmol;&c_v;</trd>
   <trd lng="br">funig ar begel</trd>
   <trd lng="ca">cord&oacute; umbilical</trd>
   <trd lng="de">Nabelschnur</trd>
   <trd lng="en">umbilical chord</trd>
   <trd lng="es">cord&oacute;n umbilical</trd>
   <trd lng="fi">napanuora</trd>
   <trd lng="fr">cordon ombilical</trd>
   <trd lng="he">&het;&bet;&lamed; &he;&tet;&bet;&vav;&resh;</trd>
   <trd lng="hu">k&ouml;ld&ouml;kzsin&oacute;r</trd>
   <trd lng="id"><ind>tali</ind> pusar</trd>
   <trd lng="it">cordone<klr> ombelicale</klr></trd>
   <trd lng="nl">navelstreng</trd>
   <trd lng="pl">p&eogonek;powina</trd>
   <trd lng="pt">cord&atilde;o umbilical</trd>
   <trd lng="ru">&c_p;&c_u;&c_p;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
   <trd lng="sv">navelstr&auml;ng</trd>
</drv>
</art>
