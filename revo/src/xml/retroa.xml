    <trd>visszahat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">met <ind>terugwerkende kracht</ind></trd>
  <trd lng="pt">retroativo</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_t;&c_r;&c_o;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;&c_je;</ind> &c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">majúci spätnú platnosť</trd>
  <trd lng="sv">retroaktiv</trd>
</drv>
