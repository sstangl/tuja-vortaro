<kap>
  <ofc>1</ofc>
  <rad>sekundant</rad>/o
</kap>
<drv mrk="sekund1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekund1.0o.MOR">
    <dif>
      Homo komisiita de duelanto por interkonsenti pri la kondi&ccirc;oj de
      <ref tip="vid" cel="duel.0o">duelo</ref> kaj por kontroli la
      batalon:
      <ekz>
        rusa oficiro, ricevinte de unu alilandulo elvokon al duelo,
        sendis
        al li kiel <tld/>on unu sian kolegon
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>sekundant</trd>,
    <trd>svÄ›dek</trd>
  </trdgrp>
  <trd lng="de">Sekundant</trd>
  <trd lng="en">second</trd>
  <trd lng="fr">t&eacute;moin<klr tip="amb"> (dans un duel)</klr></trd>
  <trdgrp lng="hu">
    <trd>p&aacute;rbajseg&eacute;d</trd>,
    <trd>szekund&aacute;ns</trd>
  </trdgrp>
  <trd lng="it">secondo<klr> (di duello)</klr></trd>
  <trd lng="nl">secondant</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>sekundant</trd>,
    <trd>svedok</trd>
  </trdgrp>
</drv>
</art>
