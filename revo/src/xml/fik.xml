<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fik.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fik</rad>/i
</kap>
<drv mrk="fik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">VULG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="kun.seks0igxi">sekskuni&gcirc;i</ref>
    <ekz>
      <klr>[mia obeema Gnom']</klr> povos fine <tld/>i sen kondom'!
      <fnt><aut>P. Peneter</aut><vrk>Sekretaj
      Sonetoj</vrk><lok>25</lok></fnt>;
    </ekz>
    <ekz>
      <klr>[la gastiganto]</klr> fikas vin, kaj vi diras nenion!
      <fnt>
      <aut>L. Beaucaire</aut>
      <vrk>Kruko kaj Baniko en Bervalo</vrk>
      <lok>La dormanto veki&gcirc;as</lok></fnt>.
    </ekz>
    <refgrp tip="sin">
      <ref cel="amor.0i">amori</ref>,
      <ref cel="koit.0i">koiti</ref>,
      <ref cel="kopula.0i">kopulacii</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_je;&c_b;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>follar</trd>,
    <trd>cardar</trd>,
    <trd>fotre</trd>
  </trdgrp>
  <trd lng="cs">souložit <klr>(vulg.)</klr></trd>
  <trdgrp lng="de">
    <trd>ficken</trd>,
    <trd>bumsen</trd>,
    <trd>pimpern</trd>,
    <trd>v&ouml;geln</trd>
  </trdgrp>
  <trd lng="en">fuck</trd>
  <trd lng="es">follar<klr> (vulg.)</klr></trd>
  <trdgrp lng="fr">
    <trd>baiser<klr tip="ind"> (faire l'amour)</klr></trd>,
    <trd>coucher (avec)</trd>
  </trdgrp>
  <trd lng="hu">baszik</trd>
  <trd lng="nl">neuken</trd>
  <trdgrp lng="pl">
    <trd>pieprzy&cacute;</trd>,
    <trd>dupczy&cacute;</trd>,
    <trd>wali&cacute;</trd>,
    <trd>jeba&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>foder</trd>,
    <trd>copular</trd>,
    <trd>transar</trd>,
    <trd>trepar</trd>,
    <trd>fazer <ind>amor</ind></trd>,
    <trd>meter</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jeba&tcaron;</trd>,
    <trd>súložiť <klr>(vulg.)</klr></trd>
  </trdgrp>
  <trd lng="tp">unpa</trd>
</drv>

<drv mrk="fik.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        kun &ccirc;armo pla&ccirc;a ektrilos al mi
        e&ccirc; la vorto: <tld/>'
        <fnt><aut>P. Peneter</aut><vrk>Sekretaj
        Sonetoj</vrk><lok>33</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="amor.0o.seksa">amoro</ref>,
      <ref cel="koit.0o">koito</ref>,
      <ref cel="kopula.0o">kopulacio</ref>,
      <ref cel="kun.seks0igxo">sekskuni&gcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>"follada"</trd>
  </trdgrp>
  <trd lng="cs">soulož <klr>(vulg.)</klr></trd>
  <trd lng="fr">baise<klr tip="ind"> (faire l'amour)</klr></trd>
  <trdgrp lng="pl">
    <trd>pieprzenie</trd>,
    <trd>dupczenie</trd>,
    <trd>walenie</trd>,
    <trd>jebanie</trd>
  </trdgrp>
  <trd lng="sk">súlož <klr>(vulg.)</klr></trd>
</drv>
</art>
