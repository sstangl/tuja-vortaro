<vortaro>
<art mrk="$Id: vitac.xml,v 1.8 2013/08/25 16:11:29 revo Exp $">
<kap>
  <rad>vitac</rad>/oj
</kap>
<drv mrk="vitac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Vitaceae</trd>), al kiu apartenas la vito.
    </dif>
  </snc>
  <trd lng="ca">vit&agrave;cies</trd>
  <trd lng="de">Rebengew&auml;chse</trd>
  <trd lng="hu">sz&odblac;l&odblac;f&eacute;l&eacute;k</trd>
  <trd lng="nl">wijnstokfamilie</trd>
  <trd lng="pt">vit&aacute;ceas</trd>
  <trd
lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
