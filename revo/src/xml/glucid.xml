<kap>
  <rad>glucid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glucid.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="glucid.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <ref tip="dif" cel="hidrat.karbon0o.KEM">karbonhidrato</ref>
    <ekz>
      eviti la trodiki&gcirc;on per redukto de
      la nutra&jcirc;aj lipidoj favore al <tld/>oj
      (fekula&jcirc;oj, cerealoj)
      <fnt>
        <aut>trad. C. Bertin</aut>,
        <vrk><url ref="http://bertin.ifrance.com/artikol/nutrado.htm">
        Nutraj konsiloj</url></vrk>,
        <lok>2003-04</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="ca">
    <trd>gl&uacute;cid</trd>,
    <trd>gl&iacute;cid</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>glucid</trd>,
    <trd>uhlohydrát</trd>
  </trdgrp>
  <trd lng="en">carbohydrate</trd>
  <trd lng="fr">glucide</trd>
  <trdgrp lng="pt">
    <trd>glic&iacute;dio</trd>,
    <trd>gluc&iacute;dio</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>glucid</trd>,
    <trd>uhľohydrát</trd>
  </trdgrp>
</drv>
</art>

