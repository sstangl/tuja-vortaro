<vortaro>
<art mrk="$Id: pinak.xml,v 1.10 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pinakotek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pinak.0o">
  <kap><tld/>o</kap>
  <snc mrk="pinak.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="muze.0o.ejo">Muzeo</ref> de pentra&jcirc;oj.
    </dif>
  </snc>
  <trd lng="cs">pinakotéka</trd>
  <trdgrp lng="de">
    <trd>Bildersammlung</trd>,
    <trd>Gem&auml;ldesammlung</trd>,
    <trd>Pinakothek</trd>
  </trdgrp>
  <trd lng="fr">pinacoth&egrave;que</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;pt&aacute;r</trd>,
    <trd>gal&eacute;ria</trd>
  </trdgrp>
  <trd lng="nl">pinacotheek</trd>
  <trd lng="pt">pinacoteca</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_i;&c_n;&c_n;&c_a;&c_ja;
    &c_g;&c_a;&c_l;&c_je;&c_r;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_i;&c_n;&c_a;&c_k;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">obrazáreň</trd>
</drv>
</art>
