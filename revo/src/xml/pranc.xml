<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pranc.xml,v 1.14 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pranc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pranc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pranc.0i.RAJ">
    <uzo tip="klr">(pri &ccirc;evalo)</uzo>
    <dif>
      Ribele starigi sin sur postaj piedoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_n;&c_a; &c_d;&c_y;&c_b;&c_k;&c_ib;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_d;&c_y;&c_b;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de"><klr>sich </klr>b&auml;umen</trd>
    <trd lng="en">prance</trd>
    <trd lng="fr">se <ind>cabrer</ind></trd>
    <trd lng="hu">&aacute;gaskodik</trd>
    <trd lng="nl">steigeren</trd>
    <trdgrp lng="pt">
      <trd>empinar-se</trd>,
      <trd>encabritar-se</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_d;&c_y;&c_b;&c_y;</trd>
  </snc>
  <snc mrk="pranc.0i.AMOR">
    <uzo tip="stl">VULG</uzo>
    <uzo tip="klr">(precipe pri viro, pri peniso)</uzo>
    <dif>
      Sperti erekti&gcirc;on,
      <ref tip="dif" cel="erekt.0igxi.FIZL">erekti&gcirc;i</ref>:
      <ekz>
        dum <tld/>is mia kompatinda Stango
        <fnt>
        <aut>P. Peneter</aut>,
        <vrk><url
        ref="http://eo.wikisource.org/wiki/Sekretaj_Sonetoj/I">
        Sekretaj Sonetoj</url></vrk>,
        <lok>1932</lok>
        </fnt>.
      </ekz>
      </dif>
    <trd lng="fr">bander<klr> (vulg.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dupat</trd>,
    <trd>hrabat</trd>,
    <trd>kopat</trd>,
    <trd>naparovat se</trd>,
    <trd>vzpínat se <klr>(kůň)</klr></trd>
  </trdgrp>
  <trd lng="sk">vzpínať sa</trd>
</drv>
</art>
