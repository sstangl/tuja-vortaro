<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: morn.xml,v 1.10 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>morn</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="morn.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malgajiga, ka&ubreve;zanta melankolion:
      <ekz>
        en la son&gcirc;o &scirc;i vidis nekonatan pejza&gcirc;on &ndash;
        senesperan, <tld/>an, sub malserena &ccirc;ielo de frua printempo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 190a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="melank.0a.melankoliiga">melankolia
    <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>chmurný</trd>,
    <trd>pochmurný</trd>,
    <trd>smutný</trd>,
    <trd>tísnivý</trd>,
    <trd>zasmušilý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>tr&uuml;bselig</trd>,
    <trd>d&uuml;ster</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gloomy</trd>,
    <trd>bleak</trd>
  </trdgrp>
  <trd lng="fr">morne</trd>
  <trdgrp lng="it">
    <trd>melanconico</trd>,
    <trd>triste</trd>,   
   <trd>tristo</trd>,   
   <trd>cupo</trd>
  </trdgrp>
  <trd lng="nl">somber</trd>
  <trdgrp lng="pt">
   <trd>opressivamente <ind>triste</ind></trd>,
    <trd>melanc&oacute;lico</trd>,
    <trd>taciturno</trd>,   
   <trd>entristecedor</trd>,   
   <trd>nebuloso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chmúrny</trd>,
    <trd>drsný</trd>,
    <trd>zasmušilý</trd>
  </trdgrp>
</drv>
</art>
