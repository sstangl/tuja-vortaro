<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diplom.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diplom</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="diplom.0o">
  <kap><tld/>o</kap>
  <snc mrk="diplom.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Oficiala dokumento,
      <ref tip="super" cel="atest.0ilo.JUR">atestilo</ref>
      pri rajto, privilegio a&ubreve; ranga titolo:
      <ekz>
        pergamena, papirusa <tld/>o;
      </ekz>
      <ekz>
        <tld/>o pri nobeligo;
        <trd lng="hu">nemesi <ind>oklev&eacute;l</ind></trd>
      </ekz>
      <ekz>
        <tld/>o de profesoro;
      </ekz>
      <ekz>
        post sukcesa ekzameno li ricevis la <tld/>on de abituriento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_jo;&c_m;</trd>
  <trd lng="bg">&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>diplom</trd>,
    <trd>listina</trd>,
    <trd>osvědčení</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Diplom</trd>,
    <trd>Ernennungsurkunde</trd>,
    <trd>Verleihungsurkunde</trd>
  </trdgrp>
  <trd lng="en">diploma</trd>
  <trd lng="es">diploma</trd>
  <trd lng="fr">dipl&ocirc;me</trd>
  <trdgrp lng="hu">
    <trd>oklev&eacute;l</trd>,
    <trd>okirat</trd>,
    <trd>diploma</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dyplom</trd>,
    <trd>cenzurka</trd>,
    <trd>&sacute;wiadectwo<klr> szkolne</klr></trd>
  </trdgrp>
  <trd lng="pt">diploma</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_a;&c_t;&c_t;&c_je;&c_s;&c_t;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diplom</trd>,
    <trd>osvedčenie</trd>
  </trdgrp>
</drv>

<drv mrk="diplom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="diplom.0i.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Alju&gcirc;i <tld/>on al:
      <ekz>
        <tld/>ita instruisto.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">udělit diplom</trd>
  <trdgrp lng="de">
    <trd>diplomieren</trd>,
    <trd>ernennen</trd>,
    <trd>verleihen</trd>
  </trdgrp>
  <trd lng="en">grant a diploma</trd>
  <trd lng="fr">dipl&ocirc;mer</trd>
  <trdgrp lng="hu">
    <trd>oklev&eacute;llel t&uuml;ntet ki</trd>,
    <trd>oklev&eacute;llel l&aacute;t el</trd>,
    <trd>diplom&aacute;t ad</trd>
  </trdgrp>
  <trd lng="pl">dyplomowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_s;
    &c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_o;&c_m;</trd>,
    <trd>&c_d;&c_i;&c_p;&c_l;&c_o;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">udeliť diplom</trd>
</drv>
</art>
