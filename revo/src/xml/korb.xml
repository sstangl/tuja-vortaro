<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korb.xml,v 1.19 2015/04/26 16:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>korb</rad>/o
</kap>
<drv mrk="korb.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Portebla ujo el kunplektitaj salikvergetoj, bastorubandoj, 
      pajlaj a&ubreve; junkaj trunketoj, metalfadenoj ks, por enteni 
      diversajn objektojn:
      <ekz>
        <tld/>o por pano, da fruktoj;
      </ekz>
      <ekz>
        <ind>abel<tld/>o</ind>;
        <trd lng="id"><ind>sarang</ind> lebah</trd>
        <trd lng="nl">bijenkorf</trd>
      </ekz>
      <ekz>
        <ind>paper<tld/>o</ind>;
        <trd lng="nl">papiermand</trd>
      </ekz>
      <ekz>pano estas alportita, <tld/>o estu for&jcirc;etita
        <klr>(ni ne bezonas plu tiun, kiu jam faris al ni
        servon)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_sh;</trd>,
    <trd>&c_k;&c_o;&c_sh;&c_y;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_e;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_o;&c_sh;</trd>
  <trd lng="ca">cistell</trd>
  <trd lng="de">Korb</trd>
  <trd lng="en">basket</trd>
  <trd lng="fr">corbeille</trd>
  <trd lng="es">cesta</trd>
  <trd lng="hu">kos&aacute;r</trd>
  <trd lng="id">keranjang</trd>
  <trd lng="nl">mand</trd>
  <trdgrp lng="pl">
    <trd>kosz</trd>,
    <trd>koszyk</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cesto <klr tip="amb">((&ecirc;))</klr></trd>,
    <trd>cesta <klr tip="amb">((&ecirc;))</klr></trd>
  </trdgrp>
  <trd lng="ro">co&scedil;ule&tcedil;</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_z;&c_i;&c_n;&c_a;</trd>
  </snc>

  <snc mrk="korb.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
    <ref tip="vid" cel="gol.0ejo">Golejo</ref> 
    (en la formo de pendanta korbo) uzata en 
    <ref tip="vid" cel="pilk.korbo0ado">korbopilkado</ref>. &Gcirc;i estas 
    muntata sur la stango kun tabulo kaj konsistas el korboringo kaj 
    korboreto.
    </dif>
    <trd lng="fr">panier<klr> (de basket)</klr></trd>
    <trd lng="nl">korf</trd>
    <trd lng="pl">kosz</trd>
    <trdgrp lng="pt">
      <trd>cesto <klr tip="amb">((&ecirc;))</klr></trd>,
      <trd>cesta <klr tip="amb">((&ecirc;))</klr></trd>
    </trdgrp>
  </snc>
</drv>
</art>
