<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monoma.xml,v 1.14 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>monomani</rad>/o
</kap>
<drv mrk="monoma.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">PSI</uzo>
    <dif>
      Speco de frenezeto en kiu la spirito estas tute absorbita de
      unusola
      sen&scirc;an&gcirc;a ideo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fixní idea</trd>,
    <trd>monomanie</trd>
  </trdgrp>
  <trd lng="de">Monomanie</trd>
  <trd lng="en">monomania</trd>
  <trd lng="es">monoman&iacute;a</trd>
  <trd lng="fr">monomanie</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;geszme</trd>,
    <trd>monom&aacute;nia</trd></trdgrp>
  <trd lng="nl">idee-fixe</trd>
  <trdgrp lng="pt">
   <trd>monomania</trd>,
   <trd>id&eacute;ia fixa</trd>,   
   <trd>obceca&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_m;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_a;&c_ja; &c_i;&c_d;&c_je;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fixná idea</trd>,
    <trd>monománia</trd>
  </trdgrp>
</drv>
</art>
