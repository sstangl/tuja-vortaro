          <trd>port&aacute;til</trd>,
          <trd>laptop</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>ordinateur portable</trd>,
          <trd>portable<klr tip="ind"> (ordinateur)</klr></trd>,
          <trd>notebook</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>t&aacute;skag&eacute;p</trd>,
          <trd>notebook</trd>,
          <trd>laptop</trd>
        </trdgrp>
        <trdgrp lng="id">
          <trd><ind>komputer</ind> jinjing</trd>,
          <trd>laptop</trd>
        </trdgrp>
        <trd lng="nl">laptop</trd>
        <trdgrp lng="pl">
          <trd>laptop</trd>,
          <trd>notebook</trd>
        </trdgrp>
        <trd lng="pt">computador port&aacute;til</trd>
        <trd lng="ru">&c_n;&c_o;&c_u;&c_t;&c_b;&c_u;&c_k;</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>laptop</trd>,
    <trd>p&rcaron;enosn&yacute; po&ccaron;&iacute;ta&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="sk">
