<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lodikl.xml,v 1.13 2012/07/20 08:10:15 revo Exp $">
<kap>
  <rad>lodikl</rad>/o
</kap>
<drv mrk="lodikl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      unu el ordinare du preska&ubreve; senkoloraj, skvamformaj
      <ref tip="super" cel="flor.0folio">florfolietoj</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="glum.0o">glumo</ref>,
      <ref cel="pale.0o">paleo</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">lod&iacute;cula</trd>
  <trd lng="de">Schwellk&ouml;rper</trd>
  <trd lng="en">lodicule</trd>
  <trd lng="hu">pikkelylev&eacute;l<klr tip="amb">
(kal&aacute;szon)</klr></trd>
  <trd lng="nl">bloemdekschub</trd>
  <trdgrp lng="pt">
    <trd>glom&eacute;lula</trd>,
    <trd>lod&iacute;cula</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_d;&c_i;&c_k;&c_u;&c_l;&c_a;</trd>
</drv>
</art>
