<kap>
  <ofc>1</ofc>
  <rad>loteri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="loteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="loteri.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Hazardludo la&ubreve; kiu la partoprenantoj ricevas numeritajn biletojn,
      kiuj rajtigas ilin al &scirc;ancoj pri gajno montrota de la sorto.
    </dif>
    <trdgrp lng="de">
      <trd>Gewinnspiel</trd>,
      <trd>Verlosung</trd>
    </trdgrp>
  </snc>
  <snc mrk="loteri.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afero de hazardo:
      <ekz>
        edzi&gcirc;o estas nura <tld/>o;
      </ekz>
      <ekz>
        la diverskolora <tld/>o de la vivo, en kiu multaj metas en la
        ludon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gl&uuml;cksspiel</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_t;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="cs">loterie</trd>
  <trdgrp lng="de">
    <trd>Lotterie</trd>
  </trdgrp>
  <trd lng="en">lottery</trd>
  <trd lng="es">loter&iacute;a</trd>
  <trd lng="fr">loterie</trd>
  <trd lng="hu">lott&oacute;</trd>
  <trd lng="nl">loterij</trd>
  <trd lng="pt">loteria</trd>
  <trd lng="ru">&c_l;&c_o;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">lot√©ria</trd>
</drv>
</art>
