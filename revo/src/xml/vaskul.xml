<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vaskul.xml,v 1.7 2015/07/04 08:37:45 revo Exp $">
<kap>
<rad>vaskul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vaskul.0o">
  <kap><tld/>o</kap>
  <snc mrk="vaskul.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="angi.0o.ANA">angio</ref>
  </snc>
  <trd lng="ca">vas</trd>
  <trd lng="cs">c√©va</trd>
  <trd lng="sk">cieva</trd>
</drv>

<drv mrk="vaskul.0o.perfekta">
  <kap>perfekta <tld/>o<fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="vaskul.perfekta0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="trahxe.0o.BOT">tra&hcirc;eo <sncref/></ref>
  </snc>
  <trd lng="ca">tr&agrave;quea<klr> (bot.)</klr></trd>
</drv>

<drv mrk="vaskul.0ajplantoj">
  <kap><tld/>aj plantoj</kap>
  <snc mrk="vaskul.0ajplantoj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
       <ref tip="lst" cel="divizi.0o.BOT"
       lst="voko:botanikaj_grupoj">Divizio</ref> de <ref tip="malprt"
       cel="plant.0oj">plantoj</ref> (<trd lng="la">Tracheophyta</trd>)
       havanta <tld/>ojn.
    </dif>
    <trd lng="de">Gef&auml;&szlig;pflanzen</trd>
  </snc>
</drv>
</art>
