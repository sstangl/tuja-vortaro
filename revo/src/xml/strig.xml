<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: strig.xml,v 1.28 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>strig</rad>/o
</kap>
<drv mrk="strig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="strig.0o.komune">
    <dif>
      Iu el <ref tip="malprt" cel="strige.0oj"><tld/>edoj</ref>.
    </dif>
    <trd lng="be">&c_s;&c_a;&c_v;&c_a;</trd>
    <trd lng="br">kaouenn</trd>
    <trd lng="ca">estr&iacute;gid</trd>
    <trd lng="cs">sova</trd>
    <trd lng="de">Eule</trd>
    <trd lng="en">owl</trd>
    <trd lng="es">buho</trd>
    <trd lng="fr">hibou</trd>
    <trd lng="hu">bagoly</trd>
    <trd lng="nl">uil</trd>
    <trd lng="pl">sowa</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="strig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Strix</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Strix</trd>
    <trd lng="de">Kauz</trd>
    <trd lng="nl">uil</trd>
    <trd lng="ru">&c_n;&c_je;&c_ja;&c_s;&c_y;&c_t;&c_mol; <klr
tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
  <trd lng="sk">sova</trd>
</drv>

<drv mrk="strig.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt"
      cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Strigiformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">estrigiformes</trd>
  <trd lng="cs">sovy</trd>
  <trd lng="de">Eulen</trd>
  <trd lng="fr">Strigiformes</trd>
  <trd lng="hu">baglyok<klr tip="amb"> (rend)</klr></trd>
  <trd lng="nl">uilen</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_y; <klr
tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">sovy</trd>
</drv>

<drv mrk="strig.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio el la familio <ref tip="super"
      cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Asio otus</trd>), kun orelaspektaj plumtufoj.
    </dif>
  </snc>
  <bld lok="../bld/strig.jpg">
    orel<tld/>o
  </bld>
  <trdgrp lng="br">
    <trd>penn-kazh</trd>,
    <trd>toud<klr tip="ind"> (evn></klr></trd>
  </trdgrp>
  <trd lng="ca">mussol banyut</trd>
  <trd lng="de">Waldohreule</trd>
  <trd lng="en">long-eared owl</trd>
  <trdgrp lng="fr">
    <trd>hibou moyen duc</trd>,
    <trd>hibou &agrave; aigrettes longues</trd>
  </trdgrp>
  <trd lng="hu">erdei <ind>f&uuml;lesbagoly</ind></trd>
  <trd lng="nl">ransuil</trd>
  <trd lng="pl">uszatka</trd>
  <trd lng="ru">&c_u;&c_sh;&c_a;&c_s;&c_t;&c_a;&c_ja;
<ind>&c_s;&c_o;&c_v;&c_a;</ind></trd>
  <trd lng="sk">myšiarka ušatá</trd>
  <trd lng="cs">kalous ušatý</trd>
</drv>
</art>
