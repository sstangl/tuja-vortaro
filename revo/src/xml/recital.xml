          <vrk>Informletro n-ro 4</vrk>,
          <lok><url
ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.
esperanto</url>, 2003-12-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">recitál</trd>
  <trd lng="es">recital</trd>
  <trd lng="fr">r&eacute;cital</trd>
