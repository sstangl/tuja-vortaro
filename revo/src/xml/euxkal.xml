<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: euxkal.xml,v 1.20 2013/08/27 15:10:30 revo Exp $">
<kap>
<ofc>9</ofc><rad>e&ubreve;kalipt</rad>/o
</kap>
<drv mrk="euxkal.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="euxkal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda rapidkreska arbo indi&gcirc;ena en
      A&ubreve;stralio, kun tre 
      alta trunko kaj kun folioj enhavantaj oleon fortodoran kaj
      seninfektigan, 
      kreskigata por medikamenta kaj papera industrioj, <ref tip="lst"
      cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="mirtac.0oj">mirtacoj</ref> (<trd
      lng="la">Eucalyptus</trd>):
      <ekz>
        alta ebenejo kun arbaro de <tld/>oj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;la superba
        kafvojo&rightquot;,
        p. 144a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_k;&c_a;&c_l;&c_ib;&c_p;&c_t;</trd>
  <trd lng="ca">eucaliptus</trd>
  <trd lng="de">Eukalyptus</trd>
  <trdgrp lng="en">
    <trd>eucalyptus</trd>,
    <trd>jarrah</trd>
  </trdgrp>
  <trd lng="es">eucalipto</trd>
  <trd lng="fa">&a_A;&a_w;&f_k;&a_A;&a_l;&f_y;&f_p;&a_t;&a_w;&a_s;</trd>
  <trd lng="fr">eucalyptus</trd>
  <trd lng="hu">eukaliptusz</trd>
  <trdgrp lng="nl">
    <trd>gomboom</trd>,
    <trd>eucaliptus</trd>
  </trdgrp>
  <trd lng="pl">eukaliptus</trd>
  <trd lng="pt">eucalipto</trd>
  <trd lng="ru">&c_e;&c_v;&c_k;&c_a;&c_l;&c_i;&c_p;&c_t;</trd>
</drv>
</art>
