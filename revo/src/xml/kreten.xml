  <trdgrp lng="nl">
    <trd>zwakzinnige</trd>,
    <trd>idioot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kretyn</trd>,
    <trd>dure&nacute;</trd>,
    <trd>idiota</trd>,
    <trd>mato&lstroke;</trd>
  </trdgrp>
  <trd lng="pt">cretino</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_t;&c_i;&c_n;</trd>
</drv>
