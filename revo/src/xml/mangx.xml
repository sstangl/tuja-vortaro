        <trd>collation</trd>,
        <trd>en-cas<klr tip="ind"> (casse-cro&ucirc;te)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>t&iacute;z&oacute;rai</trd>,
        <trd>uzsonna</trd>
      </trdgrp>
      <trd lng="nl">tussenmaaltijd</trd>
      <trd lng="pl">przek&aogonek;ska</trd>
      <trd lng="ru">&c_z;&c_a;&c_k;&c_u;&c_s;&c_k;&c_a;<klr tip="ind">
          (&c_ch;&c_t;&c_o;&c_b;&c_y;
&c_p;&c_je;&c_r;&c_je;&c_k;&c_u;&c_s;&c_i;&c_t;&c_mol;)</klr></trd>
      <trd lng="sv">mellanm&aring;l</trd>
    </drv>

    <drv mrk="mangx.post0o">
      <kap>post<tld/>o</kap>
      <uzo tip="stl">RAR</uzo>
      <snc>
        <dif>
          Tempoperiodo, etendi&gcirc;anta de unu (tagmeza)
          man&gcirc;o al la sekvonta:
          <ekz>
            post paso de du a&ubreve; tri horoj mi revenis
            &scirc;ar&gcirc;i&gcirc;inte per abunda rikolto,
            amuzprovizo por la post<tld/>o en la lo&gcirc;domo
            se pluvis
            <fnt><bib>Revadoj</bib>, <lok>Kvina promenado</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>apr&egrave;s-midi</trd>,
        <trd>apr&egrave;s-din&eacute;e<klr tip="ind"> (vx,
        apr&egrave;s-midi)</klr></trd>
      </trdgrp>
      <trd lng="nl">namiddag<klr tip="ind"> (arch.)</klr></trd>
    </drv>

    <drv mrk="mangx.abel0ulo">
      <kap>abel<tld/>ulo</kap>
      <snc mrk="mangx.abel0ulo.ZOO">
        <uzo tip="stl">KOMUNE</uzo>
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Brilkolora birdo kun longa kurba beko, man&gcirc;anta abelojn,
          
          vespojn kaj similajn, genro el familio meropedoj
          
          (<trd 
            lng="la">Merops</trd>):
          <ekz>
            videblaj specioj: <klr>...</klr> faska upupo, e&ubreve;ropa 
            abel<tld/>ulo, mar&ccirc;a cirkuo
            <fnt>
              <aut>I. Bertin</aut>,
              <vrk><url
                  ref="http://bertin.ifrance.com/bretonio/dep-29/treguenn.htm"
                  >Tr&eacute;guennec...</url></vrk>,
              <lok>[vidita en 2007-04]</lok>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="merop.0o.ZOO">meropo</ref>
        <trd lng="de">Bienenfresser</trd>
        <trd lng="en">bee-eater</trd>
        <trd lng="fr">gu&ecirc;pier<klr> (oiseau)</klr></trd>
        <trd lng="hu">gyurgyalag</trd>
        <trd lng="nl">bijeneter</trd>
        <trd lng="la">Merops apiaster</trd>
        <trd lng="pl">&zdot;o&lstroke;na<klr> (nie:
        pszczo&lstroke;ojad!)</klr></trd>
        <trd lng="pt">abelaruco<klr> (comum)</klr></trd>
        <trd lng="ru">&c_o;&c_s;&c_o;&c_je;&c_d;</trd>
        <trd lng="sv">bi&auml;tare</trd>
      </snc>
    </drv>

    <drv mrk="mangx.bluvangaabel0ulo">
      <kap>bluvanga abel<tld/>ulo</kap>
