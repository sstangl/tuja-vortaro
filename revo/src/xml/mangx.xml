    <drv mrk="mangx.formik0ulo">
      <kap>formik<tld/>ulo</kap>
      <snc mrk="mangx.formik0ulo.ZOO">
        <uzo tip="stl">KOMUNE</uzo>
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Besto <tld/>anta formikojn, termitojn, precipe longalangaj 
          mamuloj el familioj mirmekofagoj, manisedoj:
          <ekz>
            granda formiko<tld/>ulo voros proksimume 30 000 formikojn
            kaj 
            termitojn dum tago
            <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;rekordoj de
            mamuloj&rightquot;, 
                2007-02-26</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
