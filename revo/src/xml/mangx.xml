      </trdgrp>
    </drv>

    <drv mrk="mangx.tag0i">
      <kap><ofc>*</ofc>tag<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="mangx.tag0i.KUI">
        <uzo tip="fak">KUI</uzo>
        <dif>
          <tld lit="M"/>i &ccirc;irka&ubreve; la tagmezo:
          <ekz>
            se vi volas, venu tag<tld/>i
            <fnt>
              <aut>N.V. Gogol, trad. &Z;</aut>, <vrk>La revizoro</vrk>.
            </fnt>;
          </ekz><ekz>
            li tag<tld/>is kun ili &ccirc;iutage
            <fnt>
              <aut>Den DROWN</aut>, <vrk>Elektoj</vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">merenna&ntilde;<klr tip="ind"> (v.g.)</klr></trd>
      <trd lng="de">zu Mittag <ind>essen</ind></trd>
      <trd lng="id">makan siang</trd>
      <trdgrp lng="nl">
        <trd>lunchen</trd>,
        <trd>het <ind>middagmaal</ind> nemen</trd>
      </trdgrp>
      <trd lng="pl">obiad je&sacute;&cacute;</trd>
      <trd lng="ro">a servi pr&acirc;nzul</trd>
      <trd lng="ru">&c_o;&c_b;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="mangx.tag0o">
      <kap>tag<tld/>o</kap>
      <snc mrk="mangx.tag0o.ado">
        <uzo tip="fak">KUI</uzo>
        <dif>
          <ref tip="super" cel="mangx.0o.regula"><tld
              lit="M"/>o<sncref/></ref>
          normale okazanta &ccirc;irka&ubreve; tagmezo:
          <ekz>
            mi nur iom malfrui&gcirc;os al la tag<tld/>o,
            sed tio ne estas grava
            <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>,
              <vrk>Marta.</vrk></fnt>;
          </ekz><ekz>
            kiam venis la horo de la tag<tld/>o de l' soldatoj,
            li <tld/>is ilian hordean panon kun seka viando
            <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24.</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="mangx.tag0o.ajxo">
        <dif>
          La tuto da <tld/>a&jcirc;oj kaj trinka&jcirc;oj preparitaj
          a&ubreve;
          komsumitaj
          dum <ref cel="mangx.tag0o.ado">tag<tld/>o<sncref/></ref>:
          <ekz>
            post kelke da tagoj &scirc;i jam ne havus por kio
            a&ccirc;eti
            nek
            tag<tld/>on por vi kaj por si, nek lignon por fari fajron
            <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>,
              <vrk>Marta.</vrk></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_a;&c_b;&c_je;&c_d;</trd>
      <trd lng="bg">&c_o;&c_b;&c_ja;&c_d;</trd>
      <trd lng="br">merenn</trd>
      <trdgrp lng="fr">
        <trd>d&eacute;jeuner<klr tip="ind"> (repas de midi)</klr></trd>,
        <trd>diner<klr tip="ind"> (repas de midi)</klr></trd>
      </trdgrp>
      <trd lng="de">Mittagessen</trd>
      <trd lng="hu">eb&eacute;d</trd>
      <trdgrp lng="nl">
        <trd>lunch</trd>,
        <trd>middagmaal</trd>
      </trdgrp>
      <trd lng="pl">obiad</trd>
      <trd lng="ro">pr&acirc;nz</trd>
      <trd lng="ru">&c_o;&c_b;&c_je;&c_d;</trd>
      <trd lng="tr">&ouml;&gbreve;len yeme&gbreve;i</trd>
    </drv>

    <drv mrk="mangx.sankta-0o">
      <kap>Sankta <tld lit="M"/>o<fnt>PIV</fnt></kap>
      <snc mrk="mangx.sankta-0o.BELA">
        <uzo tip="fak">BELA</uzo>
        <uzo tip="fak">KRI</uzo>
        <dif>
          La evangelia epizodo pri la lasta vesperman&gcirc;o paska
          de <ref cel="jesu.0o.homo">Jesuo</ref> kun la apostoloj,
          kiun la kristanoj rigardas prototipo de
          <ref cel="euxkar.0o">e&ubreve;karistio</ref>, ofta temo de
          pentra&jcirc;oj.
        </dif>
      </snc>
      <trdgrp lng="br">
        <trd>Koan-Fask</trd>,
        <trd>Koan diwezha&ntilde;</trd>
      </trdgrp>
      <trd lng="de">Abendmahl</trd>
      <trd lng="en">Last <ind>Supper</ind></trd>
      <trd lng="es">La &uacute;ltima <ind>cena</ind></trd>
      <trd lng="fr">la sainte <ind>C&egrave;ne</ind></trd>
      <trd lng="hu">utols&oacute; <ind>vacsora</ind></trd>
      <trd lng="nl">laatste <ind>avondmaal</ind></trd>
      <trd lng="pl">Ostatnia Wieczerza</trd>
      <trdgrp lng="pt">
        <trd>Santa <ind>Ceia</ind></trd>,
        <trd>&Uacute;ltima <ind>Ceia</ind></trd>,
        <trd>Ceia do Senhor</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_a;&c_j;&c_n;&c_a;&c_ja;
      &c_v;&c_je;&c_ch;&c_je;&c_r;&c_ja;</trd>
    </drv>

    <drv mrk="mangx.vesper0i">
      <kap><ofc>*</ofc>vesper<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="mangx.vesper0i.KUI">
        <dif>
          <tld lit="M"/>i en la vespero:
          <ekz>
            la familio de la masonisto, kolekti&gcirc;inte en la
            malvasta
            &ccirc;ambro, sidi&gcirc;is por vesper<tld/>i
            <fnt><aut>E. Orzeszko, trad. Kabe</aut>,
              <vrk>La bona sinjorino</vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">koania&ntilde;</trd>
      <trd lng="de">zu Abend <ind>essen</ind></trd>
      <trd lng="id">makan malam</trd>
      <trd lng="nl">het <ind>avondmaal</ind> gebruiken</trd>
      <trd lng="pl">kolacj&eogonek; je&sacute;&cacute;</trd>
      <trd lng="ro">a servi cina</trd>
      <trd lng="ru">&c_u;&c_zh;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="mangx.vesper0o">
      <kap>vesper<tld/>o</kap>
      <snc mrk="mangx.vesper0o.ado">
        <uzo tip="fak">KUI</uzo>
        <dif>
          <ref tip="super" cel="mangx.0o.regula"><tld
              lit="M"/>o<sncref/></ref>
          normale okazanta fine de la tago:
          <ekz>
            post la vesper<tld/>o niaj fratoj eliris kun la gastoj
            <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
          </ekz><ekz>
            &ccirc;e la vesper<tld/>o oni malmulte parolis
            <fnt><bib>PatrojFiloj</bib>
              <lok>&ccirc;apitro 4a, p. 18a</lok></fnt>
          </ekz>
        </dif>
      </snc>
      <snc mrk="mangx.vesper0o.ajxo">
        <uzo tip="fak">KUI</uzo>
        <dif>
          La tuto da <tld/>a&jcirc;oj kaj trinka&jcirc;oj preparitaj
          a&ubreve;
          komsumitaj
          dum <ref
          cel="mangx.vesper0o.ado">vesper<tld/>o<sncref/></ref>:
          <ekz>
            la kuiristino alportis al mi la vesper<tld/>on
            <fnt>
              <aut>Vasilij Ero&scirc;enko</aut>,
              <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>,
              <lok><url
                  ref="http://www.esperanto.mv.ru/Cxukcxoj/cxukcx3.html"
                  >La trimova &scirc;akproblemo</url></lok>
            </fnt>;
          </ekz><ekz>
            bongusta vesper<tld/>o.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_v;&c_ja;&c_ch;&c_e;&c_r;&c_a;</trd>
      <trd lng="bg">&c_v;&c_je;&c_ch;&c_je;&c_r;&c_ja;</trd>
      <trd lng="br">koan</trd>
      <trdgrp lng="de">
        <trd>Abendessen</trd>,
        <trd>Abendbrot</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>cena</trd>,
        <trd>merienda</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>diner<klr tip="ind"> (repas du soir)</klr></trd>,
        <trd>souper<klr tip="ind"> (subst.)</klr></trd>
      </trdgrp>
      <trd lng="hu">vacsora</trd>
      <trd lng="nl">avondmaal</trd>
      <trd lng="pl">kolacja</trd>
      <trd lng="ro">cin&abreve;</trd>
      <trd lng="ru">&c_u;&c_zh;&c_i;&c_n;</trd>
      <trd lng="tr">ak&scedil;am yeme&gbreve;i</trd>
    </drv>
