<kap><rad>furier</rad>/o</kap>
<drv mrk="furier.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Suboficiro, kiu zorgas pri la lo&gcirc;igo kaj nutrigo de la
      soldatoj dum la restado en fremda loko (= lo&gcirc;ejpreparisto).
    </dif>
  </snc>
  <trd lng="de">Quartiermacher</trd>
  <trd lng="fr">fourrier</trd>
  <trd lng="hu">sz&aacute;ll&aacute;smester</trd>
  <trdgrp lng="it">
    <trd>furiere</trd>,
    <trd>sottufficiale logistico</trd>
  </trdgrp>
  <trd lng="nl">foerier</trd>
  <trd lng="pl">kwatermistrz</trd>
  <trd lng="pt">furriel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_p;&c_t;&c_je;&c_n;&c_a;&c_r;&c_m;&c_u;&c_s;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_mol;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
