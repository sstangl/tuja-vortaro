<kap>
  <rad>abeled</rad>/oj
</kap>
<drv mrk="abeled.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Apidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ch;&c_a;&c_l;&c_ib;&c_n;&c_y;&c_ja; <klr
tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">&Agrave;pids</trd>
  <trdgrp lng="de">
     <trd>Apoidea</trd>,
     <trd>Hautfl&uuml;gler</trd>
  </trdgrp>
  <trd lng="en">Apidae</trd>
  <trd lng="fr">apid&eacute;s</trd>
  <trd lng="hu">m&eacute;hf&eacute;l&eacute;k</trd>
  <trd lng="it">apidi</trd>
  <trd lng="oc">Apids</trd>
  <trd lng="pl">pszczo&lstroke;owate</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je;
<ind>&c_p;&c_ch;&c_jo;&c_l;&c_y;</ind> <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
