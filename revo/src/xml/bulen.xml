<art mrk="$Id: bulen.xml,v 1.14 2015/07/04 08:37:31 revo Exp $">
<kap><rad>bulen</rad>/o</kap>
<drv mrk="bulen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;nuro fiksita &ccirc;e la mezoj a&ubreve; flankoj de velo
      kaj kiu servas por
      devigi &gcirc;in al oblikvi&gcirc;o kaj akcepto de la vento.
    </dif>
    <trd lng="be">&c_b;&c_u;&c_l;&c_ib;&c_n;&c_mol;</trd>
    <trd lng="ca">bolina</trd>
    <trd lng="de">Bulin</trd>
    <trd lng="en">bowline</trd>
    <trd lng="es">bolina</trd>
    <trd lng="fr">bouline</trd>
    <trd lng="hu">vitorlafesz&iacute;t&odblac; k&ouml;t&eacute;l</trd>
    <trd lng="nl">boelijn</trd>
    <trd lng="pl">szot</trd>
    <trd lng="ru">&c_b;&c_u;&c_l;&c_i;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="cs">bulin <klr>(plachetní provaz)</klr></trd>
  <trd lng="sk">plachtové lano</trd>
</drv>
</art>
