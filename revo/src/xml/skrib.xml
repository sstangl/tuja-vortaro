  <trd lng="sk">dodatok <klr>(v liste)</klr></trd>
  <trdgrp lng="sv">
    <trd>efterskrift</trd>,
    <trd>post scriptum</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrib.sub0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <tld lit="S"/>i sian nomon sub io por montri sian a&ubreve;torecon
      a&ubreve; sian aprobon:
      <ekz>
        en 1948 multaj landoj <klr>...</klr> sub<tld/>is la Deklaracion
        de
        Homaj Rajtoj
        <fnt>
        <aut>M. McKay</aut>,
        <vrk>Pri libroj ... kaj pri braceletoj</vrk>,
