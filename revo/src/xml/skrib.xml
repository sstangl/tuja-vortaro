<drv mrk="skrib.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrib.al0i.skribi">
    <dif>
      Sendi, havigi <tld/>a&jcirc;on al:
      <ekz>
        li al<tld/>is la urbestraron
        <fnt>
          <aut>Milokula Kato</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/57082/208788">
          Tro Elspeziga Blogado</url></vrk>,
          <lok>2009-12-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">escribir a</trd>
    <trd lng="fr">&eacute;crire &agrave;<klr> (qqun)</klr></trd>
  </snc>
  <snc mrk="skrib.al0i.atribui">
    <dif>
      Rigardi kiel respondeca pri, koncerna pri;
      <ref tip="vid" cel="jugx.al0i">alju&gcirc;i</ref>;
      <ref tip="vid" cel="atribu.0i">atribui</ref>:
      <ekz>
        la popolamaso <klr>...</klr> al<tld/>as al &ccirc;io tio
        precipan, misteran por &gcirc;i kaj gravan signifon
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url
         ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">imputar</trd>
    <trd lng="fr">imputer</trd>
  </snc>
  <trd lng="cs">připsat</trd>
  <trd lng="sk">pripísať</trd>
</drv>

<drv mrk="skrib.bel0ado">
