  <trdgrp lng="es">
    <trd>firmar<klr tip="ind"> (por ej. un contrato)</klr></trd>,
    <trd>subscribir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>signer<klr tip="ind"> (par ex. un contrat)</klr></trd>,
    <trd>souscrire</trd>
  </trdgrp>
  <trd lng="hu">al&aacute;&iacute;r</trd>
  <trdgrp lng="id">
    <trd>[<ind>tanda</ind> tangan] menandatangani</trd>,
    <trd>teken</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>firmare</trd>, <trd>sottoscrivere</trd>
  </trdgrp>
  <trd lng="nl">ondertekenen</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">podpísať</trd>
  <trdgrp lng="sv">
    <trd>underteckna</trd>,
    <trd>skriva under</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="skrib.sub0o.JUR">
    <dif>
      <tld lit="S"/>a almeto de sia nomo, ordinare malsupre de dokumento
      a&ubreve; a&jcirc;o, por marki posedon, a&ubreve;tentikon, konsenton:
    </dif>
    <refgrp tip="vid">
      <ref cel="sigel.0o">sigelo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">podpis</trd>
  <trdgrp lng="de">
    <trd>Unterschrift</trd>,
    <trd>Signatur</trd>
  </trdgrp>
  <trd lng="en">signature</trd>
  <trd lng="es">firma</trd>
  <trdgrp lng="fr">
    <trd>signature</trd>
  </trdgrp>
  <trd lng="hu">al&aacute;&iacute;r&aacute;s</trd>
  <trd lng="id"><ind>tanda</ind> tangan</trd>
  <trd lng="it">firma</trd>
  <trd lng="nl">handtekening</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_mol;</trd>
  <trd lng="sk">podpis</trd>
  <trd lng="sv">underskrift</trd>
</drv>

<drv mrk="skrib.sur0o">
  <kap>sur<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="S"/>a&jcirc;o sur objekto a&ubreve; <ref tip="vid"
      cel="monume.0o">monumento</ref>:
      <ekz>
        fragmento de &scirc;tono kun hebrea sur<tld/>o
        <fnt>
        <aut>A. Taylor Szrajbman</aut>,
        <vrk>Kadi&scirc; por Jedwabne (2)</vrk>,
        <bib>Monato</bib>,
        <lok>2002:6, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nápis</trd>
  <trdgrp lng="de">
    <trd>Aufschrift</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inscripci&oacute;n</trd>,
    <trd>ep&iacute;grafe</trd>    
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pigraphe<klr tip="ind"> (subst.)</klr></trd>,
    <trd>inscription</trd>
  </trdgrp>
  <trd lng="hu">felirat</trd>
  <trdgrp lng="id">
    <trd>epigraf</trd>,
    <trd>inskripsi</trd>,
    <trd>prasasti</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>epigrafe</trd>,
    <trd>iscrizione</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opschrift</trd>,
    <trd>grafschrift</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nadpis</trd>,
    <trd>nápis</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.trans0i">
  <kap><ofc>*</ofc>trans<tld/>i</kap>
  <snc mrk="skrib.trans0i.kopii">
    <dif>
      <ref tip="vid" cel="kopi.0i.tekston">Kopii</ref> tekston en alian
      dokumenton:
      <ekz>
        verkisto verkas librojn, kaj <tld/>isto simple trans<tld/>as
        paperojn
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>&uuml;bertragen</trd>,
      <trd>umschreiben</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>copy</trd>,
      <trd>transcribe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;t&iacute;r <klr tip="ind">(vhov&aacute;)</klr></trd>,
      <trd>&aacute;tm&aacute;sol</trd>,
      <trd>&aacute;tvezet <klr tip="ind">(dokumentumon)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>[<ind>kopi</ind>] mengopi</trd>,
      <trd>[<ind>salin</ind>] menyalin</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>trascrivere</trd>,
      <trd>copiare</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_u;&c_d;&c_a;-&c_l;.)</klr></trd>
  </snc>
  <snc mrk="skrib.trans0i.alisisteme">
    <dif>
      Re<tld/>i tekston per alia <tld/>sistemo a&ubreve; ortografio:
    </dif>
    <ekz>
      trans<tld/>i &ccirc;inan tekston per latinaj literoj;
    </ekz>
    <ekz>
      trans<tld/>i anglan vorton per la Internacia Alfabeto Fonetika.
    </ekz>
    <trd lng="de">transkribieren</trd>
    <trdgrp lng="it">
      <trd>trascrivere</trd>,
      <trd>traslitterare</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>opsat</trd>,
    <trd>přepsat</trd>
  </trdgrp>
  <trd lng="es">transcribir</trd>
  <trdgrp lng="fr">
    <trd>transcrire</trd>
  </trdgrp>
  <trd lng="hu">&aacute;t&iacute;r</trd>
  <trd lng="nl">overschrijven</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_i;&c_t;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr>
(alilingven)</klr></trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_k;&c_r;&c_i;&c_b;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr>
(fonetike)</klr></trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kopírovať</trd>,
    <trd>prepísať</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.plugo0i">
  <kap>plugo<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="skrib.plugo0i.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Skribi en antikva
      <ref tip="super" cel="skrib.0maniero.sistemo">skribmaniero</ref>,
      &ccirc;e kiu la parnumeraj linioj havas
      kontra&ubreve;an direkton ol la neparnumeraj, simile al la sulkoj
      de kamparano pluganta sian kampon:
      <ekz>
        <ind>plugo<tld/>ado</ind>n praktikis prakritanoj, &hcirc;etidoj,
        etruskoj k.a.;
        <trd lng="en">boustrophedon</trd>
        <trd lng="es">bustrofedon</trd>
        <trd lng="fr">boustroph&eacute;don</trd>
        <trd lng="grc">&beta;&omikron;&ypsilon;&sigma;&tau;&rho;&omikron;&phi;&eta;&delta;&omikron_ton;&ny;</trd>
        <trdgrp lng="hu">
          <trd>busztrofedon</trd>,
          <trd>megt&eacute;r&odblac; <ind>&iacute;r&aacute;s</ind></trd>
        </trdgrp>
        <trd lng="nl">ploegschrift</trd>
        <trd lng="ru">&c_b;&c_u;&c_s;&c_t;&c_r;&c_o;&c_f;&c_je;&c_d;&c_o;&c_n;</trd>
      </ekz>
      <ekz>
        en plugo<tld/>a&jcirc;o alterni&gcirc;as la linioj dedekstraj kaj la
        demaldekstraj, a&ubreve; (malpli ofte) desupraj kaj desubaj.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>escribir en <ind>bustrofedon</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&eacute;crire en <ind>boustroph&eacute;don</ind></trd>
    </trdgrp>
    <trd lng="hu">v&aacute;ltakoz&oacute; ir&aacute;nyban <ind>&iacute;r</ind></trd>
    <trd lng="it">scrittura <ind>bustrofedica</ind></trd>
    <trd lng="nl">in <ind>ploegschrift</ind> schrijven</trd>
    <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol; <ind>&c_b;&c_u;&c_s;&c_t;&c_r;&c_o;&c_f;&c_je;&c_d;&c_o;&c_n;&c_o;&c_m;</ind></trd>
  </snc>
</drv>

<drv mrk="skrib.SanktaS0o">
  <kap>Sankta <tld lit="S"/>o</kap>
  <snc mrk="skrib.SanktaS0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Verko, kiun religia grupo rigardas die inspirita:
      <ekz>
        la&ubreve;te li proklamis la vortojn de la Sankta <tld lit="S"/>o
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Scripture</trd>,
    <trd>Holy Scripture</trd>,
    <trd>Holy Writ</trd>
  </trdgrp>
  <trd lng="es">Santas <ind>Escrituras</ind></trd>
  <trd lng="fr"><ind>&Eacute;criture</ind> Sainte</trd>
  <trd lng="hu">Szent&iacute;r&aacute;s</trd>
  <trd lng="id"><ind>Kitab</ind> Suci</trd>
  <trd lng="it">Sacre <ind>Scritture</ind></trd>
  <trd lng="nl">Heilige <ind>Schrift</ind></trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="skrib.0eraro">
  <kap><tld/>eraro</kap>
  <snc mrk="skrib.0eraro.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Misa uzo de literoj rilate al ortografio:
      <ekz>
        en la sciencaj nomoj troveblas kelkaj <tld/>eraroj
        <fnt>
        <aut>W. M. A. De Smet</aut>,
        <vrk>Ekzotaj arboj en Esperantujo</vrk>,
        <bib>Monato</bib>,
        <lok>1997:10, p. 19a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kobold.pres0o">preskoboldo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Schreibfehler</trd>
  </trdgrp>
  <trd lng="en">typo</trd>
  <trdgrp lng="es">
    <trd>error de escritura</trd>,
    <trd>falta de ortograf&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faute d'&eacute;criture</trd>,
    <trd>faute d'orthographe</trd>
  </trdgrp>
  <trd lng="hu">el&iacute;r&aacute;s</trd>
  <trdgrp lng="id">
    <trd>[<ind>salah</ind> cetak] kesalahan cetak</trd>,
    <trd>typo</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>errore di scrittura</trd>,
    <trd>errore di ortografia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schrijffout</trd>,
    <trd>spellingsfout</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>stavfel</trd>,
    <trd>skrivfel</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.0karaktero">
  <kap><tld/>karaktero</kap>
  <snc mrk="skrib.0karaktero.mana">
    <dif>
      Ies persona stilo man<tld/>i (precipe pri la aspekto de
      la literoj):
      <ekz>
        gracie &gcirc;i estis <tld/>ita, tio estis <tld/>karaktero de
        sinjorino
        <fnt><bib>Fab2</bib>,
             <lok>Malnova strata lanterno</lok></fnt>;
       </ekz><ekz>
         La <tld/>karaktero de mia frato!
            <fnt>
              <aut>&Scirc;ilero, tr. &Z;</aut>: <vrk>La rabistoj</vrk>.
              <lok><url
ref="https://books.google.ru/books?id=Re_FBgAAQBAJ&amp;pg=PA23&amp;lpg=PA23&amp;dq=a+skribkaraktero+de+mia+frato&amp;source=bl&amp;ots=FMEQ_GQVyF&amp;sig=lhuaxdxditFVOOZku79ZTe1MK1Y&amp;hl=en&amp;sa=X&amp;ei=UaDuVM-jOam9ygPUhoKwDw&amp;ved=0CCIQ6AEwAA#v=onepage&amp;q&amp;f=false">1:2</url></lok>
            </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>charakter písma</trd>,
    <trd>rukopis</trd>,
    <trd>způsob psaní</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schreibweise</trd>
  </trdgrp>
  <trd lng="en">handwriting</trd>
  <trd lng="id"><ind>tulis</ind>an tangan</trd>
  <trdgrp lng="it">
    <trd>scrittura<klr> (modo personale)</klr></trd>,
    <trd>calligrafia<klr> (scrittura)</klr></trd>
  </trdgrp>
  <trd lng="nl">schrijfwijze</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_je;&c_r;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>charakter písma</trd>,
    <trd>rukopis</trd>
  </trdgrp>
  <trd lng="sv">handstil</trd>
</drv>

<drv mrk="skrib.0maniero">
  <kap><tld/>maniero</kap>
  <snc mrk="skrib.0maniero.mana">
    <ref tip="dif" cel="skrib.0karaktero"><tld/>karaktero</ref>
    <ekz>
      la koverto estis sur<tld/>ita per junece nestabila, kvaza&ubreve;
      infaneca <tld/>maniero
      <fnt>
      <aut>N. Lavrentjeva, trad. A. Sidorov</aut>,
      <vrk>Po&scirc;trestante</vrk>,
      <lok>Nica Literatura Revuo, 3(1).</lok></fnt>;
    </ekz><ekz>
      datoj mankas, sed la malsamaj inkkoloro kaj <tld/>maniero
      pruvas, ke &gcirc;i estis skribita en malsamaj tempoj
      <fnt>
      <aut>Lusin, trad. Elpin</aut>,
      <vrk>La Taglibro de Frenezulo</vrk>,
      <lok>&Ccirc;ina Antologio (1919-1949), 1986</lok>
      </fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd>Schreibweise</trd>
    </trdgrp>
    <trd lng="en">handwriting</trd>
    <trd lng="id"><ind>tulis</ind>an tangan</trd>
    <trdgrp lng="it">
      <trd>scrittura<klr> (modo personale)</klr></trd>,
      <trd>calligrafia<klr> (scrittura)</klr></trd>
    </trdgrp>
    <trd lng="nl">schrijfwijze</trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_je;&c_r;&c_k;</trd>
    <trd lng="sv">handstil</trd>
  </snc>
  <snc mrk="skrib.0maniero.sistemo">
    <dif>
      Maniero aran&gcirc;i <tld/>on (ortografiaj konvencioj, <tld/>a sistemo
      ktp):
      <ekz>
        disa, kuna, minuskla <tld/>maniero;
      </ekz><ekz>
        sed, se vi deziras anstata&ubreve;igi la supersignon en &ccirc; kaj &scirc;, vi
        nepre devas anstata&ubreve;igi &gcirc;in anka&ubreve; en &jcirc;, &hcirc; kaj &gcirc;, &ccirc;ar
        <klr>...</klr> uzado de ia <tld/>maniero miksita estus jam
        arbitra&jcirc;o kaj kondukus al &hcirc;aoso
        <fnt><aut>&Z;</aut>, <vrk>Lingva respondo 45</vrk>,
          <lok>La Revuo, 1908-08</lok>.</fnt>.
      </ekz>
    </dif>
    <trd lng="id"><ind>sistem</ind> penulisan</trd>
    <trdgrp lng="it">
      <trd>scrittura<klr> (sistema)</klr></trd>
    </trdgrp>
    <trd lng="nl">zetwijze</trd>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>charakter písma</trd>,
    <trd>rukopis</trd>,
    <trd>způsob psaní</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escritura</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;criture</trd>
  </trdgrp>
  <trd lng="hu">&iacute;r&aacute;sm&oacute;d</trd>
  <trdgrp lng="sk">
    <trd>charakter písma</trd>,
    <trd>rukopis</trd>
  </trdgrp>
</drv>

<drv mrk="skrib.0tablo">
  <kap><ofc>*</ofc><tld/>tablo</kap>
  <snc>
    <dif>
      Tablo uzata por skribi, ordinare kun tirkestoj, ilaroj kaj
      similaj:
      <ekz>
        en mia <tld/>otablo sin trovas kvar tirkestoj
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>tablo a&ubreve; <tld/>otablo (= tablo, sur kiu oni skribas)
        <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">psací stůl</trd>
  <trdgrp lng="de">
    <trd>Schreibtisch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escritorio</trd>,
    <trd>bur&oacute;<klr tip="ind"> (mueble)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bureau<klr tip="ind"> (meuble)</klr></trd>
  </trdgrp>
  <trd lng="hu">&iacute;r&oacute;asztal</trd>
  <trd lng="id"><ind>meja</ind> tulis</trd>
  <trd lng="it">scrittoio<klr> (mobile)</klr></trd>
  <trd lng="nl">schrijftafel</trd>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j; &c_s;&c_t;&c_o;&c_l;</trd>
  <trd lng="sk">písací stôl</trd>
  <trd lng="sv">skrivbord</trd>
</drv>

<drv mrk="skrib.0tabulo">
  <kap><tld/>tabulo</kap>
  <snc>
    <dif>
      Granda tabulo sur la muro por skribi per kreto,
      feltkrajono kaj similaj:
      <ekz>
        mi desegnas arbon en la <tld/>tabulo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>encerado</trd>,
    <trd>pizarra</trd>
  </trdgrp>
  <trd lng="fr">tableau noir</trd>
</drv>

