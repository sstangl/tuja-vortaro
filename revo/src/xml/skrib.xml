        <lok>1998:5, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="paraf.0o">parafo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">podepsat</trd>
