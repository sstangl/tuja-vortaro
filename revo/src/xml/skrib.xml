  <trdgrp lng="id">
    <trd>notabene</trd>,
    <trd>N.B.</trd>
  </trdgrp>
  <trd lng="it">post scriptum</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_s;&c_k;&c_r;&c_i;&c_p;&c_t;&c_u;&c_m;</trd>,
