<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: edelve.xml,v 1.20 2015/07/04 08:37:33 revo Exp $">
<kap>
<rad>edelvejs</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="edelve.0o">
  <kap><tld/>o</kap>
  <snc mrk="edelve.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el la familio <ref tip="super"
      cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Leontopodium</trd>), kun blanka laneca floro,
      kreskanta sur kalka grundo en montaro.
    </dif>
    <ref tip="vid" cel="flor.negxo0o.BOT">ne&gcirc;ofloro</ref>
  </snc>
  <trd lng="be">&c_e;&c_d;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_j;&c_s;</trd>
  <trd lng="bg">&c_d;&c_je;&c_l;&c_v;&c_a;&c_j;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>plesnivec alpský</trd>,
    <trd>protěž alpská</trd>
  </trdgrp>
  <trd lng="de">Edelwei&szlig;</trd>
  <trd lng="en">edelweiss</trd>
  <trd lng="fr">edelweiss</trd>
  <trd lng="hu">havasi gyop&aacute;r</trd>
  <trd lng="nl">edelweiss</trd>
  <trd lng="pl">szarotka</trd>
  <trd lng="pt">edelvais</trd>
  <trd lng="ru">&c_e;&c_d;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_j;&c_s;</trd>
  <trd lng="sk">plesnivec alpský</trd>
</drv>
</art>
