<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkre.xml,v 1.19 2015/10/15 05:10:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konkret</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="konkre.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Esprimanta reala&jcirc;on; perceptebla per sentumoj:
      <ekz>
        <tld/>a vorto, ri&ccirc;o, rezultato;
      </ekz>
      <ekz>
        formon <tld/>an al nia batalado ni donis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>a religio heredigas senfine en maniero a&ubreve;tomata, sed
        abstrakta senreligieco ne povas esti heredata<fnt>Z</fnt>;
      </ekz>
      <ekz>
        montri <tld/>e sian dankon;
      </ekz>
      <ekz>
        nia standardo la unuan fojon <tld/>e aperis en
        Bulonjo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="abstra.0a">abstrakta</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_r;&c_e;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>concret</trd>,
    <trd>real</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>konkret</trd>,
     <trd>wirklich</trd>,
     <trd>fasslich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>concrete</trd>,
    <trd>in <ind>material form</ind></trd>,
    <trd>perceptible <klr>by the senses</klr></trd>,
    <trd>positive</trd>,
    <trd>real</trd>
  </trdgrp>
  <trd lng="es">concreto</trd>
  <trd lng="fr">concret</trd>
  <trd lng="nl">concreet</trd>
  <trdgrp lng="pl">
    <trd>konkretny</trd>,
    <trd>realny</trd>
  </trdgrp>
  <trd lng="pt">concreto <klr tip="amb">(adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_r;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>;
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="konkre.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Igi pli facile imagebla, donante <tld/>an formon:
      <ekz>
        <tld/>igi ideon per vortbildoj;
      </ekz>
      <ekz>
        tiu revo <tld/>i&gcirc;is en bela libro;
      </ekz>
      <ekz>
        niaj kunvenoj kaj kongresoj estas la plej klara <tld/>igo
        de Esp-ismo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_r;&c_e;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>exemplificar</trd>,
    <trd>il&middot;lustrar</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>konkretisieren</trd>,
     <trd>veranschaulichen</trd>,
     <trd>fasslich machen</trd>
  </trdgrp>
  <trd lng="en">put into <ind>concrete</ind> form</trd>
  <trd lng="es">concretar</trd>
  <trd lng="fr">concr&eacute;tiser</trd>
  <trd lng="nl">concretiseren</trd>
  <trd lng="pl">konkretyzowa&cacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_k;&c_r;&c_je;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="konkre.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <ref tip="ant" cel="konkre.0a"><tld/>a</ref>
    <ref tip="sin" cel="abstra.0a">abstrakta</ref>
    <dif>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_k;&c_a;&c_n;&c_k;&c_r;&c_e;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_b;&c_s;&c_t;&c_r;&c_a;&c_k;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">abstracte</trd>
  <trdgrp lng="en">
    <trd>intangible</trd>,
    <trd>incorporeal</trd>
  </trdgrp>
  <trd lng="fr">abstrait</trd>
  <trdgrp lng="nl">
    <trd>abstract</trd>,
    <trd>niet <ind>concreet</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niekonkretny</trd>,
    <trd>abstrakcyjny</trd>
  </trdgrp>
</drv>

</art>
