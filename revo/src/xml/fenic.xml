<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fenic.xml,v 1.10 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fenic</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fenic.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de popolo vivinta ekde la 3a jarmilo a. K. en <ref tip="vid"
      cel="fenic.0ujo"><tld lit="F"/>ujo</ref>, aktiva en &scirc;ipa komercado
      kaj koloniado &ccirc;irka&ubreve; Mediteraneo,
      ekde la 9a jarcento a. K. dependa je diversaj imperioj:
      <ekz>
        la <tld/>oj parolis &scirc;emidan lingvon
        <fnt>
        <aut>Sergio Pokrovskij</aut>,
        <vrk>Slavonaj skriboj</vrk>,
        <bib>LOdE</bib>,
        <lok>numero 52a, 1999:2a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_je;&c_c;</trd>
  <trd lng="cs">Féničan</trd>
  <trd lng="de">Ph&ouml;nizier</trd>
  <trd lng="en">Phoenician</trd>
  <trd lng="fr">Ph&eacute;nicien<klr> (subst.)</klr></trd>
  <trd lng="hu">f&ouml;n&iacute;ciai</trd>
  <trd lng="pl">Fenicjanin</trd>
  <trd lng="pt">fen&iacute;cio</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;&c_i;&c_je;&c_c;</trd>
  <trd lng="sk">Feničan</trd>
</drv>

<drv mrk="fenic.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">ph&ouml;nizisch</trd>
  <trd lng="en">Phoenician</trd>
  <trd lng="fr">ph&eacute;nicien<klr> (adj.)</klr></trd>
  <trd lng="pt">fen&iacute;cio</trd>
  <trd lng="hu">f&ouml;n&iacute;ciai</trd>
  <trd lng="pl">fenicki</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_k;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="fenic.0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      La devena lando de <tld/>oj, inter Lebanona montaro kaj
      la orienta bordo de Mediteranea maro, proksimume nuna Libano.
    </dif>
    <ref tip="sin" cel="fenici.0o">Fenicio</ref>
  </snc>
  <trd lng="be">&c_F;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_ja;</trd>
  <trd lng="de">Ph&ouml;nizien</trd>
  <trd lng="en">Phoenicia</trd>
  <trd lng="fr">Ph&eacute;nicie</trd>
  <trd lng="hu">F&ouml;n&iacute;cia</trd>
  <trd lng="pl">Fenicja</trd>
  <trd lng="pt">Fen&iacute;cia</trd>
  <trd lng="ru">&c_F;&c_i;&c_n;&c_i;&c_k;&c_i;&c_ja;</trd>
</drv>
</art>

