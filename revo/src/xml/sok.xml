<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sok.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sok</rad>/o
</kap>
<drv mrk="sok.0o">
  <kap><tld/>o<fnt>D</fnt></kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Parto de plugilo, konsistanta el triangula oblikva klingo,
      kiu sublevas la teron kaj renversas &gcirc;in flanken.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_m;&c_je;&c_h;</trd>,
    <trd>&c_l;&c_ja;&c_m;&c_je;&c_sh;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_o;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>radlice</trd>,
    <trd>rádlo</trd>
  </trdgrp>
  <trd lng="de">Pflugschar</trd>
  <trdgrp lng="en">
    <trd>ploughshare<klr> (brite)</klr></trd>,
    <trd>plowshare<klr> (usone)</klr></trd>
  </trdgrp>
  <trd lng="fr">soc</trd>
  <trd lng="hu">ekevas</trd>
  <trd lng="nl">ploegschaar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_je;&c_m;&c_je;&c_h;</trd>
  </trdgrp>
  <trd lng="sk">radlica <klr>(poľnohosp.)</klr></trd>
</drv>
</art>
