<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: promulg.xml,v 1.3 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>promulg</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="promulg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="promulg.0i.POL">
    <uzo tip="fak">POL</uzo>
    <uzo tip="klr">(pri le&gcirc;o, regulo...)</uzo>
    <dif>
      Ek-efikigi, ordoni aplikon de...:
      <ekz>
        ni festas la <tld/>adon de la Konstitucio de la 
        Esperanta Civito
        <fnt>
          <aut>G. Silfer</aut>,
          <vrk><url ref="http://www.esperantio.net/index.php?id=883">
          Nia lingvo 122-jara</url></vrk>,
          <lok>HeKo, 2009-06-02 (391)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>promulgovat</trd>,
    <trd>vyhlásit</trd>,
    <trd>zveřejnit <klr>(zákon, rozsudek)</klr></trd>
  </trdgrp>
  <trd lng="fr">promulguer</trd>
  <trdgrp lng="sk">
    <trd>vyhlásiť</trd>,
    <trd>zverejniť <klr>(zákon, rozsudok)</klr></trd>
  </trdgrp>
</drv>
</art>

