<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grajn.xml,v 1.25 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>grajn</rad>/o
</kap>
<drv mrk="grajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grajn.0o.globeto">
    <dif>
      Malgranda malmola globa a&jcirc;o:
      <ekz>
        <tld/>o de hajlo, pulvo;
      </ekz>
      <ekz>
        sur la fundo de la rivero trovi&gcirc;as <tld/>oj da oro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eto da vitro <klr>[penetris]</klr> en lian okulon
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>etoj de sablo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj de kolzono.
      </ekz>
    </dif>
  <subsnc mrk="grajn.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Ccirc;iu frukto de <ref tip="malprt" cel="gren.0o.planto">greno</ref>
      entenata en la spiko kaj uzata por fari farunon:
      <ekz>
        <tld/>o de tritiko, aveno, rizo, pizo, pipro;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o el fre&scirc;aj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Lev; 2:14</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">grain</trd>
    <trd lng="hu">mag</trd>
    <trd lng="nl">graankorrel</trd>
    <trd lng="pl">ziarno</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </subsnc>
  <subsnc mrk="grajn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="grajn.sem0o">Sem<tld/>o</ref>
    <trd lng="hu">vet&odblac;mag</trd>
    <trdgrp lng="nl">
      <trd>zaaigoed</trd>,
      <trd>zaaigraan</trd>
    </trdgrp>
    <trd lng="pl">ziarno</trd>
    <trd lng="pt">semente</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </subsnc>
  <subsnc mrk="grajn.0o.kerno">
    <dif>
      Malgranda <ref tip="dif" cel="kern.0o.BOT">kerno</ref> de
      kernofruktoj, ekzemple de pomo, piro.
    </dif>
    <trdgrp lng="en">
      <trd>seed</trd>,
      <trd>pip</trd>
    </trdgrp>
    <trd lng="hu">mag<klr tip="ind"> (csonth&eacute;jas)</klr></trd>
    <trd lng="nl">pit</trd>
    <trdgrp lng="pl">
    <trd>ziarno</trd>,
    <trd>ziarenko</trd>,
    <trd>pestka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>,
      <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </subsnc>
    <refgrp tip="super">
      <ref cel="er1.0o">ero</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>grain</trd>,
      <trd>granule</trd>,
      <trd>speck</trd>,
      <trd>particle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&ouml;g<klr tip="ind"> (szemcse)</klr></trd>,
      <trd>szemcse</trd>
    </trdgrp>
    <trd lng="nl">korrel</trd>
    <trdgrp lng="pl">
      <trd>ziarno</trd>,
      <trd>drobina</trd>,
      <trd>granulka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.iometo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iometo da:
      <ekz>
        estas tamen en la afero <tld/>o da vero;
      </ekz>
      <ekz>
        li ne havas en la kapo e&ccirc; unu <tld/>on da sa&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gut.0o">guto</ref>,
      <ref cel="ombr.0o">ombro</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>grain</trd>,
      <trd>granule</trd>,
      <trd>speck</trd>,
      <trd>particle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csipetnyi</trd>,
      <trd>cs&ouml;ppnyi</trd>
    </trdgrp>
    <trd lng="nl">greintje</trd>
    <trdgrp lng="pl">
      <trd>ziarno</trd>,
      <trd>odrobina</trd>
    </trdgrp>
    <trd lng="pt">pedacinho<klr tip="amb"> (fig.)</klr></trd>
    <trd lng="ru">&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>granula</trd>,
    <trd>granule</trd>,
    <trd>granálie</trd>,
    <trd>sbalek</trd>,
    <trd>zrno</trd>
  </trdgrp>
  <trd lng="de">Korn</trd>
  <trd lng="es">grano</trd>
  <trdgrp lng="fr">
    <trd>grain</trd>,
    <trd>graine</trd>,
    <trd>p&eacute;pin</trd>,
    <trd>goutte<klr tip="ind"> (petite quantit&eacute;)</klr></trd>,
    <trd>soup&ccedil;on<klr tip="ind"> (petite quantit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gr&atilde;o</trd>,
    <trd>semente</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>granula</trd>,
    <trd>zrno</trd>
  </trdgrp>
  <trd lng="tr">tane<klr>(bu&gbreve;day ve benzeri)</klr></trd>
</drv>

<drv mrk="grajn.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="grajn.0igi.KOMUNE">
    <dif>
      Fari <tld/>ojn el ia maso.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>drobit</trd>,
    <trd>granulovat</trd>
  </trdgrp>
  <trd lng="en">granulate</trd>
  <trdgrp lng="fr">
    <trd>concasser</trd>,
    <trd>grainer<klr tip="ind"> (r&eacute;duire en grains)</klr></trd>,
    <trd>grener<klr tip="ind"> (r&eacute;duire en grains)</klr></trd>
  </trdgrp>
  <trd lng="hu">granul&aacute;l</trd>
  <trd lng="nl">verpulveren</trd>
  <trdgrp lng="pl">
    <trd>rozdrabnia&cacute;</trd>,
    <trd>granulowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>granar<klr tip="amb"> (reduzir a gr&atilde;os)</klr></trd>,
    <trd>granular<klr tip="amb"> (reduzir a gr&atilde;os)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_r;&c_u;&c_p;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>drobiť</trd>,
    <trd>granulovať</trd>
  </trdgrp>
</drv>

<drv mrk="grajn.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="grajn.0igxi.BOT">
    <dif>
      Kreski aperigante sem<tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">monter en <ind>graine</ind></trd>
  <trd lng="hu">magot terem</trd>
  <trd lng="nl">granen<klr tip="ind"> (ww.)</klr></trd>
  <trdgrp lng="pt">
    <trd>granar<klr tip="amb"> (criar gr&atilde;o)</klr></trd>,
    <trd>granear<klr tip="amb"> (criar gr&atilde;o)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_r;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="grajn.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="grajn.0umo.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="perika1.0o">perikarpo</ref>
  </snc>
  <trdgrp lng="en">
    <trd>pericarp</trd>,
    <trd>seed case</trd>,
    <trd>seed vessel</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;ricarpe</trd>
  <trd lng="hu">term&eacute;sfal</trd>
  <trd lng="nl">pericarpium</trd>
  <trd lng="pl">owocnia</trd>
  <trd lng="pt">pericarpo</trd>
  <trd lng="ru">&c_o;&c_k;&c_o;&c_l;&c_o;&c_p;&c_l;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="grajn.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc mrk="grajn.dis0igi.KOMUNE">
    <dif>
      De&scirc;iri la <tld/>ojn:
      <ekz>
        dis<tld/>igi spikon, vinberaron...
      </ekz>
    </dif>
  </snc>
  <trd lng="es">desgranar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;grainer<klr tip="ind"> (d&eacute;tacher les grains)</klr></trd>,
    <trd>&eacute;grener<klr tip="ind"> (d&eacute;tacher les grains)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kimagoz</trd>,
    <trd>lemorzsol</trd>
  </trdgrp>
  <trd lng="nl">dorsen</trd>
  <trd lng="pt">debulhar</trd>
</drv>

<drv mrk="grajn.bel0o">
  <kap>bel<tld/>o</kap>
  <snc mrk="grajn.bel0o.KOS">
    <dif>
      Bruna <ref tip="super" cel="makul.hauxt0o.MED">ha&ubreve;tmakulo</ref>,
      malgranda <ref tip="vid" cel="nevus.0o.MED">nevuso</ref>
      kontrasta sur hela ha&ubreve;to:
      <ekz>
        revokante &scirc;ian viza&gcirc;on kaj bel<tld/>on &ccirc;e la liva bu&scirc;angulo
        <fnt>
        <aut>Guo Moruo</aut>,
        <vrk><url ref="http://esperanto.cri.cn/1/2003/12/26/1@2087.htm">
        Karamela Knabino</url></vrk>,
        <lok>2003-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>piha krásy</trd>,
    <trd>znaménko krásy</trd>
  </trdgrp>
  <trd lng="en">beauty mark</trd>
  <trd lng="fr">grain de beaut&eacute;</trd>
  <trd lng="sk">pôsobivé materské znamienko na tvári ženy</trd>
  <trd lng="tr">ben<klr>(tende)</klr></trd>
</drv>

<drv mrk="grajn.sem0o">
  <kap>sem<tld/>o</kap>
  <snc mrk="grajn.sem0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="prt" cel="embri.0o">Embrio</ref> de floroplanto
      kun nutra&jcirc;o kaj &scirc;irma &scirc;elo,
      esti&gcirc;inta el fekundigita <ref cel="ovul.0o">ovulo</ref>,
      entenata en la <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>,
      kaj produktanta la <ref cel="gxerm.0o">&gcirc;ermon</ref>.
    </dif>
  </snc>
  <trd lng="cs">zrno k setí</trd>
  <trd lng="de">Samenkorn</trd>
  <trd lng="fr">graine<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="hu">vet&odblac;mag</trd>
  <trdgrp lng="nl">
    <trd>zaaigoed</trd>,
    <trd>zaaigraan</trd>
  </trdgrp>
  <trd lng="pl">ziarno</trd>
  <trd lng="pt">semente</trd>
  <trd lng="ru">&c_s;&c_je;&c_m;&c_ja;</trd>
  <trd lng="sk">semeno</trd>
  <trd lng="tr">tohum</trd>
</drv>
</art>
