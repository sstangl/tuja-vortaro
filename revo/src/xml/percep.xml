<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: percep.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>percept</rad>/o
</kap>
<drv mrk="percep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Kono, kiun ni akiras pri la ekstera mondo pere de niaj kvin
      sentumorganoj a&ubreve; pri ni mem pere de la konscio.
    </dif>
    <rim>
      Ne konfuzu kun <em>koncepto</em>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_e;&c_r;&c_c;&c_e;&c_p;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>přijetí</trd>,
    <trd>vjem</trd>,
    <trd>vnímání</trd>
  </trdgrp>
  <trd lng="de">Wahrnehmung</trd>
  <trd lng="en">perception</trd>
  <trd lng="es">percepci&oacute;n</trd>
  <trd lng="fr">perception</trd>
  <trdgrp lng="hu">
    <trd>&eacute;szlelet</trd>,
    <trd>percepci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waarneming</trd>,
    <trd>gewaarwording</trd>
  </trdgrp>
  <trd lng="pt">percep&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>prijatie</trd>,
    <trd>vnem</trd>,
    <trd>vnímanie</trd>
  </trdgrp>
</drv>

<drv mrk="percep.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havi <tld/>on pri io:
      <ekz>
        apena&ubreve; <tld/>ebla klini&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pochopit</trd>,
    <trd>vnímat</trd>
  </trdgrp>
  <trd lng="de">wahrnehmen</trd>
  <trd lng="en">perceive</trd>
  <trd lng="es">percibir</trd>
  <trd lng="fr">percevoir</trd>
  <trd lng="hu">&eacute;szlel</trd>
  <trdgrp lng="nl">
    <trd>waarnemen</trd>,
    <trd>gewaar worden</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pochopiť</trd>,
    <trd>vnímať</trd>
  </trdgrp>
</drv>
</art>
