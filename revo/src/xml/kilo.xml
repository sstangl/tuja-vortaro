<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kilo.xml,v 1.3 2013/09/09 10:10:17 revo Exp $">
<kap>
  <rad>kilo</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kilo.0">
  <kap><tld/>-</kap>
  <mlg>k</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="kilo.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Mil&rightquot; (1000)
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        <ref tip="vid" cel="kilome.0o"><tld/>metro</ref>;
      </ekz>
      <ekz>
        <ref tip="vid" cel="kilovo.0o"><tld/>volto</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;-</trd>
  <trd lng="bg">&c_k;&c_i;&c_l;&c_o;-</trd>
  <trdgrp lng="ca">
    <trd>kilo-</trd>
    <trd>quilo-</trd>,
  </trdgrp>
  <trd lng="de">Kilo-</trd>
  <trd lng="en">kilo-</trd>
  <trd lng="es">kil&oacute;-</trd>
  <trd lng="fa">&f_k;&f_y;&a_l;&a_w;</trd>
  <trd lng="fr">kilo-</trd>
  <trd lng="hu">kilo-</trd>
  <trd lng="it">chilo-</trd>
  <trd lng="nl">kilo-</trd>
  <trd lng="no">kilo-</trd>
  <trd lng="pl">kilo-</trd>
  <trd lng="pt">quilo-</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;-</trd>
  <trd lng="sv">kilo-</trd>	
</drv>
</art>

