    <trd>indefinite <ind>integral</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>primitive<klr tip="ind"> (subst., math.)</klr></trd>,
    <trd>int&eacute;grale ind&eacute;finie</trd>
  </trdgrp>
  <trd lng="hu">hat&aacute;rozatlan <ind>integr&aacute;l</ind></trd>
  <trdgrp lng="pl">
