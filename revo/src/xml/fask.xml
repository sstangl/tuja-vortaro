<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fask.xml,v 1.21 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fask</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fask.0o.KOMUNE">
    <dif>
      Aro da kunigitaj samspecaj longformaj objektoj:
      <ekz>
        &scirc;i renkontis la pordiston, kiu portis <tld/>on da ligno al
        unu el la lo&gcirc;ejoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la nuksoj pendis en formo de pezaj <tld/>oj
        <fnt><bib>Fab3</bib>, <lok>historio de la jaro</lok></fnt>;
      </ekz>
      <ekz>
        dense, kiel rafanetoj en <tld/>o
        <fnt><bib>Fab1</bib>, <lok>vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        granda <tld/>o da &scirc;losiloj
        <fnt><bib>Fab1</bib>,
        <lok>floroj de la malgranda Ida</lok></fnt>;
      </ekz>
      <ekz>
        luma <tld/>o <klr>(de lumaj radioj)</klr>;
      </ekz>
      <ekz>
        ornama plum<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>B&uuml;ndel</trd>,
      <trd>B&uuml;schel</trd>,
      <trd>Faszikel</trd>,
      <trd>Garbe</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;teg</trd>,
      <trd>nyal&aacute;b</trd>,
      <trd>csom&oacute;<klr tip="ind"> (nyal&aacute;b)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bos<klr tip="ind"> (bussel)</klr></trd>,
      <trd>bussel</trd>,
      <trd>bundel</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>p&eogonek;k</trd>,
      <trd>p&eogonek;czek</trd>,
      <trd>wi&aogonek;zka</trd>,
      <trd>plik</trd>
    </trdgrp>
    <subsnc mrk="fask.0o.HIS">
      <dif>
        Liga&jcirc;o da vergetoj &ccirc;irka&ubreve; hakilo, kiu estis
        emblemo de la publika povo en antikva Romio.
      </dif>
      <trd lng="de">Liktorenb&uuml;ndel</trd>
      <trdgrp lng="hu">
        <trd>fasces</trd>,
        <trd>vessz&odblac;nyal&aacute;b</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="tuf.0o">tufo</ref>
    </refgrp>
  </snc>
  <snc mrk="fask.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kuni&gcirc;o kaj unui&gcirc;o:
      <ekz>
        kunigi en <tld/>on &ccirc;iujn disigitajn fortojn.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>B&uuml;ndel</trd>,
      <trd>Bund</trd>
    </trdgrp>
    <trd lng="nl">verbond</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_h;&c_a;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_ja;&c_r;&c_e;&c_m;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_r;&c_e;&c_m;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blok</trd>,
    <trd>otep</trd>,
    <trd>otýpka</trd>,
    <trd>skupina</trd>,
    <trd>soubor</trd>,
    <trd>svazek</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bundle</trd>,
    <trd>sheaf</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>haz</trd>,
    <trd>manojo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faisceau</trd>,
    <trd>fagot</trd>,
    <trd>botte<klr tip="ind"> (de paille, etc.)</klr></trd>,
    <trd>trousseau<klr tip="ind"> (de clefs)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>feixe</trd>,
    <trd>molho</trd>,
    <trd>lio</trd>,
    <trd>penca</trd>,
    <trd>atado</trd>,
    <trd>atilho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_h;&c_a;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blok</trd>,
    <trd>skupina</trd>,
    <trd>súbor</trd>,
    <trd>zväzok</trd>
  </trdgrp>
</drv>

<drv mrk="fask.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="fask.har0o.FRIZ">
    <dif>
      Aro de haroj kune elpendantaj:
      <ekz>
        bela fra&ubreve;lineto kun oraj har<tld/>etoj
        &ccirc;irka&ubreve; la blanka frunto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="bukl.0o.hara">buklo</ref>
  <trd lng="en">ponytail</trd>
  <trd lng="es">coleta</trd>
  <trd lng="fr">m&ecirc;che <klr> (de cheveux)</klr></trd>
  <trdgrp lng="pl">
    <trd>pasemko<klr tip="amb"> w&lstroke;os&oacute;w</klr></trd>,
    <trd>str&aogonek;k<klr tip="amb"> w&lstroke;os&oacute;w</klr></trd>,
    <trd>dred</trd>
  </trdgrp>
  <trd lng="sk">prameň vlasov</trd>
  <trdgrp lng="cs">
    <trd>kštice</trd>,
    <trd>pramen vlasů</trd>
  </trdgrp>
</drv>

<drv mrk="fask.faden0o">
  <kap>faden<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="fask.faden0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa fadeno plurfoje refaldita.
    </dif>
  </snc>
  <trd lng="cs">svazek vláken</trd>
  <trdgrp lng="en">
    <trd>hank</trd>,
    <trd>skein</trd>
  </trdgrp>
  <trd lng="fr">&eacute;cheveau</trd>
  <trd lng="hu">motring</trd>
  <trd lng="sk">zväzok vláken</trd>
</drv>
</art>
