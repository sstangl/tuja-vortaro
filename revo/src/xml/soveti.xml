<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: soveti.xml,v 1.4 2015/07/04 08:37:43 revo Exp $">
<kap>
    <rad>Soveti</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="soveti.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <ref tip="dif" cel="sovet.UniodeS0ajSocialismajRespublikoj">Unio de
  Sovetaj Socialismaj Respublikoj</ref>
  <ekz>
    tiel restis &gcirc;is la atako de Germanio kontra&ubreve; <tld
    />o, la 22an
    de junio 1941<fnt><aut>Zofia Banet-Fornalowa</aut>, <vrk>Kadi&scirc;
    por
    Jedwabne</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero 1a, p.
    18a</lok></fnt>;
  </ekz>
  <ekz>
    <tld/>o estas laborista &scirc;tato
    <fnt><bib>Metrop</bib></fnt>.
  </ekz>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;
  &c_S;&c_a;&c_ju;&c_z;</trd>
  <trd lng="cs">Sovětský svaz</trd>
  <trd lng="fr">URSS</trd>
  <trdgrp lng="pl">
    <trd>Zwi&aogonek;zek Socjalistycznych Republik Radzieckich</trd>,
    <trd>Sowiety<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;
    &c_S;&c_o;&c_ju;&c_z;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;
    &c_S;&c_o;&c_v;&c_je;&c_t;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">Sovietsky zväz</trd>
</drv>

<drv mrk="soveti.s0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        Ili havas &ccirc;i tie pli, ol cent leterojn el USSR,
        po&scirc;tkartojn,
        amason da fotoj, <tld lit="s"/>ajn librojn kaj gazetojn
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;</trd>
  <trd lng="fr">sovi&eacute;tique<klr> (adj.)</klr></trd>
  <trdgrp lng="pl">
    <trd>radziecki</trd>,
    <trd>sowiecki<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="de">sowjetisch</trd>
</drv>


</art>

