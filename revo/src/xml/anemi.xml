<art mrk="$Id: anemi.xml,v 1.16 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anemi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="anemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="anemi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Manko de sango, malbona stato de la sango:
      <ekz>
        dum gravedeco &ccirc;e &scirc;i okazis <tld/>o, kiun forigi helpis nek
        tablojdoj, nek injektado de feraj prepara&jcirc;oj
        <fnt>
        <aut>N. Stepanov</aut>,
        <vrk>Debato/Vegetarismo - &ccirc;u granda mispa&scirc;o a&ubreve;
        sa&gcirc;a konduto?</vrk>,
        <bib>Monato</bib>,
        <lok>1995:7, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_e;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="cs">chudokrevnost</trd>
  <trdgrp lng="de">
    <trd>Blutarmut</trd>,
    <trd>An&auml;mie<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">anemia</trd>
  <trd lng="es">anemia</trd>
  <trd lng="fr">an&eacute;mie</trd>
  <trd lng="nl">anemie</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_a;&c_n;&c_je;&c_m;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>an&eacute;mia</trd>,
    <trd>málokrvnosť</trd>,
    <trd>nedostatok červených krviniek</trd>
  </trdgrp>
</drv>
</art>
