<art mrk="$Id: karame.xml,v 1.18 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>karamel</rad>/o
</kap>
<drv mrk="karame.0o">
  <kap><tld/>o</kap>
  <snc mrk="karame.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Rostita bruni&gcirc;inta sukero:
      <ekz>
        li eltiris el la po&scirc;o <tld/>ojn
        <klr>(<tld/>bombonojn)</klr>
        <fnt><aut>Lina Gabrielli</aut>, <vrk>Karnavalo. - Romo:
        International Center of Arts, 1973</vrk>, <lok>p.
        11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_m;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;</trd>
  <trd lng="ca">caramel</trd>
  <trdgrp lng="cs">
    <trd>cukrové barvivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Karamell</trd>,
    <trd>Karamellzucker</trd>
  </trdgrp>
  <trd lng="en">caramel</trd>
  <trd lng="es">caramelo</trd>
  <trd lng="fr">caramel</trd>
  <trd lng="hu">karamella</trd>
  <trd lng="nl">caramel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_m;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cukrové farbivo</trd>,
    <trd>karamel</trd>
  </trdgrp>
  <trd lng="tr">karamela</trd>
</drv>
</art>
