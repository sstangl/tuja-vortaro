<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: morkel.xml,v 1.15 2013/10/27 18:10:15 revo Exp $">
<kap>
  <rad>morkel</rad>/o
</kap>
<drv mrk="morkel.0o">
  <kap><tld/>o</kap>
  <snc mrk="morkel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Bongusta man&gcirc;ebla <ref tip="lst" cel="fung.0o.fruktokorpo"
      lst="voko:fungoj">fungo</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref>
       de <ref tip="malprt" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Morchella</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_r;&c_zh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_u;&c_m;&c_p;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">m&uacute;rgola</trd>
  <trd lng="de">Morchel</trd>
  <trd lng="en">morel</trd>
  <trd lng="fr">morille</trd>
  <trd lng="hu">kucsmagomba</trd>
  <trdgrp lng="nl">
    <trd>morille</trd>,
    <trd>morielje</trd>
  </trdgrp>
  <trd lng="pt">fung&atilde;o <klr>(fungo comest&iacute;vel)</klr></trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
