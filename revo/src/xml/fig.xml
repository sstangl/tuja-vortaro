<kap>
<ofc>*</ofc>
<rad>fig</rad>/o
</kap>
<drv mrk="fig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sukerdol&ccirc;a mola <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de <tld/>arbo.
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_m;&c_o;&c_k;&c_i;&c_n;&c_ja;</trd>
  <trd lng="ca">figa</trd>
  <trd lng="cs">f√≠k</trd>
  <trd lng="de">Feige</trd>
  <trd lng="en">fig</trd>
  <trd lng="es">higo</trd>
  <trd lng="fa">&a_A;&a_n;&a_j;&f_y;&a_r;</trd>
  <trd lng="fr">figue</trd>
  <trd lng="hu">f&uuml;ge</trd>
  <trd lng="nl">vijg</trd>
  <trd lng="pl">figa</trd>
  <trd lng="pt">figo</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_zh;&c_i;&c_r;</trd>,
    <trd>&c_s;&c_m;&c_o;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">figa</trd>
  <trd lng="tr">incir</trd>
</drv>
<drv mrk="fig.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="morusa.0oj">morusacoj</ref>
      (<trd lng="la">Ficus</trd>).
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>figuera</trd>,
    <trd>ficus</trd>
  </trdgrp>
  <trd lng="de">Feige</trd>
  <trd lng="en">fig</trd>
  <trd lng="es">higuera</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_A;&a_n;&a_j;&f_y;&a_r;</ind></trd>
  <trd lng="fr">figuier</trd>
  <trd lng="hu">f&uuml;gefa</trd>
  <trd lng="nl">vijgenboom</trd>
  <trd lng="pl">figowiec<klr> pospolity</klr></trd>
  <trd lng="pt">figueira</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_k;&c_o;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_n;&c_zh;&c_i;&c_r;</trd>
  </trdgrp>
</drv>
</art>
