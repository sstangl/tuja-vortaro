<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: babiru.xml,v 1.17 2013/09/03 17:10:32 revo Exp $">
<kap><rad>babirus</rad>/o</kap>
<drv mrk="babiru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="porked.0oj">porkedoj</ref>
      (<trd lng="la">Babyrousa</trd>), el Celebeso kaj najbaraj insuloj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_ib;&c_r;&c_u;&c_s;&c_a;</trd>
  <trd lng="ca">babirussa</trd>
  <trd lng="de">Hirscheber</trd>
  <trdgrp lng="en">
    <trd>babiroussa</trd>,
    <trd>babirussa</trd>,
    <trd>babirusa</trd>
  </trdgrp>
  <trd lng="fr">babiroussa</trd>
  <trd lng="hu">babirussza</trd>
  <trd lng="it">babirussa</trd>
  <trdgrp lng="nl">
    <trd>hertzwijn</trd>,
    <trd>babiroesa</trd>
  </trdgrp>
  <trd lng="pl">babirusa</trd>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_i;&c_r;&c_u;&c_s;&c_s;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>hjortsvin</trd>,
    <trd>babyrousa</trd>,
    <trd>babirusa</trd>
  </trdgrp>
</drv>
</art>
