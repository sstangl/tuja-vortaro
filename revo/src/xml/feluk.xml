<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: feluk.xml,v 1.9 2015/10/27 11:10:28 revo Exp $">
<kap>
  <rad>feluk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="feluk.0o">
  <kap><tld/>o</kap>
  <snc mrk="feluk.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Malgranda bordira <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      kun triangulaj veloj, kiajn oni uzis en la Mediteranea Maro:
      <ekz>
        sur la <tld/>oj trovi&gcirc;is marokaj fi&scirc;istoj
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        33a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">feluka</trd>
  <trd lng="de">Felucke</trd>
  <trd lng="en">felucca</trd>
  <trd lng="es">falucho</trd>
  <trd lng="fr">felouque</trd>
  <trd lng="pl">feluka</trd>
  <trdgrp lng="pt">
    <trd>falucho</trd>,
    <trd>felucho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_l;&c_u;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_je;&c_l;&c_ju;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">feluka</trd>
</drv>
</art>

