<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pingl.xml,v 1.27 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pingl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pingl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pingl.0o.tajlora">
    <dif>
      Metala stangeto, pinta &ccirc;e unu ekstremo, kiun oni uzas
      por pike fiksi teksa&jcirc;on a&ubreve; veston:
      <ekz>
        estis tiel lume, ke oni povus trovi <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>sidi sur <tld/>oj</ind>
        <klr>(maltrankvilege atendi)</klr>
        <trdgrp lng="fr">
          <trd>&ecirc;tre assis sur des &eacute;pingles</trd>,
          <trd>ne pas tenir en place</trd>
        </trdgrp>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; <tld/>o <klr>(bagatelo)</klr> povas koron trapiki
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="fiks.0ilo.TEK">fiksilo</ref>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>,
      <ref cel="fibul.0o">fibulo</ref>,
      <ref cel="najl.0o">najlo</ref>
    </refgrp>
    <trd lng="de">Stecknadel</trd>
    <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;</trd>
    <trd lng="hu">t&udblac;</trd>
    <trd lng="nl">speld</trd>
    <trd lng="pt">alfinete</trd>
    <trd lng="es">alfiler</trd>
    <trd lng="ru">&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="pingl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="P"/>oforma <ref tip="super"
      cel="foli.0o.BOT">folio</ref>
      de <ref tip="malprt" cel="konifer.0o">konifero</ref>:
      <ekz>
        <tld/>oj de pino<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_s;&a_w;&a_z;&a_n;<klr tip="amb"> (&a_b;&a_r;&f_g;
    &f_k;&a_A;&a_j;)</klr></trd>
    <trd lng="hu">t&udblac;lev&eacute;l</trd>
    <trd lng="nl">naald</trd>
    <trd lng="ru">&c_h;&c_v;&c_o;&c_ja;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>brož</trd>,
    <trd>jehličí</trd>,
    <trd>spona</trd>,
    <trd>špendlík</trd>
  </trdgrp>
  <trd lng="de">Nadel</trd>
  <trd lng="en">needle</trd>
  <trd lng="fr">&eacute;pingle</trd>
  <trdgrp lng="sk">
    <trd>bodec</trd>,
    <trd>ihlica</trd>,
    <trd>špendlík</trd>
  </trdgrp>
</drv>

<drv mrk="pingl.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pingl.al0i.KOMUNE">
    <dif>
      Fiksi per <tld/>o:
      <ekz>
        Panjo, panjo, atendu, mi al<tld/>os malanta&ubreve;e la tuketon
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 6a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">přišpendlit</trd>
  <trdgrp lng="de">
    <trd>anheften</trd>,
    <trd>anpinnen</trd>,
    <trd>festpinnen</trd>
  </trdgrp>
  <trd lng="en">pin</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_n;&a_j;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_s;&a_n;&a_j;&a_A;&a_q; &a_z;&a_d;&a_n;<klr tip="amb">
    (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;pingler</trd>
  <trdgrp lng="hu">
    <trd>megt&udblac;z</trd>,
    <trd>odat&udblac;z<klr> (t&udblac;vel)</klr></trd>,
    <trd>let&udblac;z</trd>,
  </trdgrp>
  <trd lng="nl">vastspelden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_sh;&c_p;&c_i;&c_l;&c_i;&c_t;&c_mol;
    <klr>(&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_o;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">prišpendliť</trd>
</drv>

<drv mrk="pingl.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pingl.de0i.KOMUNE">
    <dif>
      Demeti ion fiksitan per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;s&eacute;pingler</trd>
  <trd lng="nl">losspelden</trd>
</drv>

<drv mrk="pingl.dent0o">
  <kap>dent<tld/>o</kap>
  <snc mrk="pingl.dent0o.HEJM">
    <ref tip="dif" cel="dent.0opikilo">dentopikilo</ref>
    <ekz>
      La&ubreve;-&Ccirc;eng penis eligi per dent<tld/>o sezaman grajnon 
      el inter siaj dentoj
      <fnt>
        <aut>Lao She, trad. Wang Chongfang</aut>,
        <vrk><url
        ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
        Kamelo &Scirc;jangzi</url></vrk>,
        <lok>1988</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_u;&c_b;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_p;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_ch;&c_k;&c_a; &c_z;&c_a;
  &c_z;&c_malmol;&c_b;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>escuradents</trd>,
    <trd>furgadents</trd>
  </trdgrp>
  <trd lng="de">Zahnstocher</trd>
  <trd lng="en">toothpick</trd>
  <trd lng="es">mondadientes</trd>
  <trd lng="fr">cure-dent</trd>
  <trd lng="he">&qof;&yod;&samekh;&fmem;
    &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>fogv&aacute;j&oacute;</trd>,
    <trd>fogpiszk&aacute;l&oacute;</trd>
  </trdgrp>
  <trd lng="it">stuzzicadenti</trd>
  <trd lng="pl">wyka&lstroke;aczka</trd>
  <trd
  lng="ru">&c_z;&c_u;&c_b;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="pingl.friz0o">
  <kap>friz<tld/>o</kap>
  <snc mrk="pingl.friz0o.KOS">
    <dif>
      Cilindreto &ccirc;irka&ubreve; kiu oni rulas hartufon
      por &gcirc;in bukligi:
      <ekz>
        blondulino de iom alta kresko kun <klr>...</klr>
        friz<tld/>oj en la hararo
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">natáčka na vlasy</trd>
  <trd lng="de">Lockenwickler</trd>
  <trd lng="fr">bigoudi</trd>
  <trd lng="sk">natáčka na vlasy</trd>
</drv>

<drv mrk="pingl.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="pingl.har0o.KOS">
    <dif>
      Metala senpinta stangeto, ofte fleksita la&ubreve; du paralelaj
      bran&ccirc;oj, uzata por fiksi la harojn:
      <ekz>
        viajn harligojn vi levus iom pli alte kaj ornamus ilin
        <klr>...</klr> per oraj harpingloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jehlice do vlasů</trd>,
    <trd>sponka na vlasy</trd>,
    <trd>vlásenka</trd>
  </trdgrp>
  <trd lng="de">Haarnadel</trd>
  <trd lng="es">alfiler de pelo</trd>
  <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;&zwnj;&a_s;&a_r;</trd>
  <trd lng="fr">&eacute;pingle &agrave; cheveux</trd>
  <trd lng="hu">hajt&udblac;</trd>
  <trd lng="nl">haarspeld</trd>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_l;&c_mol;&c_k;&c_a;
  <klr>(&c_d;&c_l;&c_ja; &c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  <trdgrp lng="sk">
    <trd>ihlica do vlasov</trd>,
    <trd>sponka do vlasov</trd>
  </trdgrp>
</drv>

<drv mrk="pingl.kravat0o">
  <kap>kravat<tld/>o</kap>
  <snc mrk="pingl.kravat0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="P"/>o per kiu oni alpikas kravaton al &ccirc;emizo:
      <ekz>
        la detektivo rapide mensnotis 
        la orrandajn okulvitrojn, oran horlo&gcirc;eton, oran 
        kravat<tld/>on
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li venis 
        trakosme?</vrk>, <lok>1980</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">špendlík do kravaty</trd>
  <trd lng="de">Krawattennadel</trd>
  <trd lng="es">alfiler de corbata</trd>
  <trd lng="fr">&eacute;pingle &agrave; cravate</trd>
  <trd lng="sk">špendlík do kravaty</trd>
</drv>

<drv mrk="pingl.prem0o">
  <kap>prem<tld/>o</kap>
  <snc mrk="pingl.prem0o.KOMUNE">
    <dif>
      Mallonga <tld/>o kun lar&gcirc;a plata kapo, kiun oni enpikas
      per fingropremo por alkro&ccirc;i desegnopaperon, mapon ktp.
    </dif>
  </snc>
  <trd lng="cs">připínáček</trd>
  <trd lng="de">Reiszwecke</trd>
  <trd lng="es">chincheta</trd>
  <trd lng="fa">&f_p;&a_w;&a_n;&a_z;</trd>
  <trd lng="fr">punaise<klr tip="amb"> (de bureau)</klr></trd>
  <trd lng="hu">rajzt&udblac;</trd>
  <trdgrp lng="nl">
    <trd>duimspijker</trd>,
    <trd>punaise</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_n;&c_o;&c_p;&c_k;&c_a;
  <klr>(&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">pripináčik</trd>
</drv>

<drv mrk="pingl.sendangxera0o">
  <kap>sendan&gcirc;era <tld/>o</kap>
  <snc mrk="pingl.sendangxera0o.HEJM">
    <dif>
      <tld lit="P"/>o fleksita risorte kiel U kun o-forma cirklo en la
      mezo, kun ingo &ccirc;e unu ekstermo, en kiun ingon eblas fiksi la
      alian (pikan) ekstremon. Oni uzas tion por fiksi etenda&jcirc;ojn
      de vesto a&ubreve; banda&gcirc;o a&ubreve; dorse de insignoj kiel
      <ref tip="super" cel="fiks.0ilo.TEK">fiksilon</ref>.
    </dif>
    <ref tip="vid" cel="agraf.0o">agrafo</ref>
  </snc>
  <trd lng="de">Sicherheitsnadel</trd>
  <trd lng="es">imperdible</trd>
  <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;&zwnj;&a_q;&a_f;&a_l;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pingle &agrave; nourrice</trd>,
    <trd>&eacute;pingle de suret&eacute;</trd>
  </trdgrp>
  <trd lng="hu">biztost&udblac;</trd>
  <trd lng="nl">veiligheidsspeld</trd>
  <trd lng="ru">&c_a;&c_n;&c_g;&c_l;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</ind></trd>
</drv>
</art>
