<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kipr.xml,v 1.10 2014/06/10 10:10:26 revo Exp $">
<kap>
  <rad>Kipr</rad>/o
</kap>

<drv mrk="kipr.K0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kipr.K0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>CY</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      en la samnoma insulo kun &ccirc;efurbo Nikosio,
      teritorio de Respubliko Kipro.
    </dif>
    <trdgrp lng="pl">
      <trd>Cypr</trd>,
      <trd>Republika <ind>Cypryjska</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="kipr.K0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea
      Maro</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Cypr</trd>,
    </trdgrp>
  </snc>
  <trd lng="bg">&c_K;&c_i;&c_p;&c_malmol;&c_r;</trd>
  <trd lng="de">Zypern</trd>
  <trd lng="en">Cyprus</trd>
  <trd lng="fa">&a_q;&a_b;&a_r;&a_s;</trd>
  <trd lng="fr">Chypre</trd>
  <trd lng="id">Siprus</trd>
  <trd lng="nl">Cyprus</trd>
  <trd lng="pt">Chipre</trd>
  <trd lng="tr">K&inodot;br&inodot;s</trd>
</drv>
</art>
