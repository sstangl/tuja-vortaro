<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: olog.xml,v 1.3 2009/07/09 16:31:24 revo Exp $">
<kap>
  -<rad>olog</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="olog.0">
  <kap>-<tld/></kap>
  <gra><vspec>sufikso</vspec></gra>
  <snc mrk="olog.0.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="vid" cel="scienc.0ulo">Scienculo</ref>,
      <ref tip="vid" cel="fak.0ulo.KOMUNE">fakulo</ref>
      pri la koncerna temo:
      <ekz>
        la politik<tld/>o Jonathan Pool <klr>...</klr> identigis kvin 
        tipojn de lingva malegaleco
        <fnt>
          <aut>M. Fettes</aut>,
          <vrk><url
          ref="http://nitobe.info/ld/dokumentujo/dokumentoj/nc_dokumento-12-lingvopolitiko2_eo.pdf">
          Principoj kaj praktiko en la Esperanto-komunumo</url></vrk>,
          <lok>1996</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri la teoria parto helpis fama ruslanda kultur<tld/>o,
        profesoro Markov
        <fnt>
          <aut>J. Dmitrijevskaja-Nilsson</aut>,
          <vrk>Oni povas paroli pri vera triumfo</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ologi.0">-ologi</ref>
  </refgrp>
  <trd lng="de">-ologe</trd>
  <trd lng="fr">-ologue</trd>
  <trd lng="nl">-oloog</trd>
</drv>
</art>

