<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: varicel.xml,v 1.7 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>varicel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="varicel.0o">
  <kap><tld/>o</kap>
  <snc mrk="varicel.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Virusdevena, konta&gcirc;a
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>,
      kiu trafas precipe infanojn, ka&ubreve;zante jukajn vezikojn sur la 
      ha&ubreve;to:
      <ekz>
        &scirc;ia korpo estas kovrita de aknoj kaj <tld/>aj makuloj
        <fnt>
          <aut>F. Wodou</aut>,
          <vrk><url ref="http://vitorluiz.6te.net/?p=234">
          Varicelo en Togolando</url></vrk>,
          <lok>2010-01-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">varicel&middot;la</trd>
  <trdgrp lng="cs">
    <trd>plané neštovice</trd>,
    <trd>varicela</trd>
  </trdgrp>
  <trd lng="de">Windpocken</trd>
  <trdgrp lng="en">
    <trd>chickenpox</trd>,
    <trd>chicken pox</trd>
  </trdgrp>
  <trd lng="fr">varicelle</trd>
  <trd lng="id"><ind>cacar</ind> air</trd>
  <trdgrp lng="sk">
    <trd>ovčie kiahne</trd>,
    <trd>varicela</trd>
  </trdgrp>
</drv>
</art>

