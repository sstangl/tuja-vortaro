<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sik.xml,v 1.7 2013/09/22 19:11:11 revo Exp $">
<kap>
  <rad>sik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sik.0o">
  <kap><tld/>o</kap>
  <snc mrk="sik.0o.REL">
    <dif>
      Ano de <tld/>ismo - religia komunumo el Hindujo.
    </dif>
  </snc>
  <trd lng="fr">sikh</trd>
  <trdgrp lng="pt">
    <trd>sique</trd>,
    <trd>sikh</trd>
  </trdgrp>
  <trd lng="pl">sikh</trd>
  <trd lng="ru">&c_s;&c_i;&c_k;&c_h;</trd>
</drv>

<drv mrk="sik.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="sik.0ismo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Unudia <ref tip="lst" cel="religi.0o.REL"
      lst="voko:religioj">religio</ref> fondita en
      la 16a jarcento en Pan&gcirc;abo, kun doktrino origine difinita de
      &gcirc;iaj sinsekvaj dek majstroj:
      <ekz>
        la guruo ordonis, ke <ctl>Guru Granth Sahib</ctl>,
        la sankta libro de <tld/>ismo, estu la spirita a&ubreve;toritato
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;sikismo&rightquot;,
        2005-10-18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sikhismus</trd>
  <trd lng="en">sikhism</trd>
  <trd lng="es">sikhismo</trd>
  <trd lng="fr">sikhisme</trd>
  <trd lng="it">sikhismo</trd>
  <trd lng="nl">sikhisme</trd>
  <trd lng="pl">sikhizm</trd>
  <trdgrp lng="pt">
    <trd>siquismo</trd>,
    <trd>sikhismo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_i;&c_k;&c_h;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
