<kap>
  <rad>kalistef</rad>/o
</kap>
<drv mrk="kalist.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Callistephus</trd>), kun multaj &gcirc;ardene 
      kulturataj varia&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a; &c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="ca">octubrera</trd>
  <trdgrp lng="cs">
    <trd>astra</trd>,
    <trd>hvězdnice zahr.</trd>
  </trdgrp>
  <trd lng="de">China-Aster</trd>
  <trd lng="en">china aster</trd>
  <trd lng="hu">&odblac;szir&oacute;zsa</trd>
  <trd lng="nl">Chinese <ind>aster</ind></trd>
  <trd lng="pt">cal&iacute;stefo</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_a;
&c_s;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja;</trd>
  <trd lng="sk">astra čínska</trd>
</drv>
</art>
