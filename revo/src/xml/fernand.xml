<vortaro>
<art mrk="$Id: fernand.xml,v 1.7 2015/02/26 20:11:18 revo Exp $">
<kap>
  <rad>Fernand</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fernand.0o">
  <kap><tld/>o</kap>
  <snc mrk="fernand.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        mi uzis la forkon de Stanislav, la tason de <tld/>o, la
        tran&ccirc;ilon
        de Ruben
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Ferran</trd>
  <trd lng="de">Ferdinand</trd>
  <trd lng="fr">Fernand</trd>
  <trdgrp lng="hu">
    <trd>Ferdin&aacute;nd</trd>,
    <trd>N&aacute;ndor</trd>
  </trdgrp>
  <trd lng="it">Fernando</trd>
  <trd lng="pt">Fernando</trd>
</drv>
</art>

