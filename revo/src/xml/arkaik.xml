<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arkaik.xml,v 1.23 2016/04/11 07:10:20 revo Exp $">
<kap><ofc>3</ofc>
<rad>arkaik</rad>/a</kap>
<drv mrk="arkaik.0a">
  <kap><tld/>a</kap>
  <snc mrk="arkaik.0a.KOMUNE">
    <uzo tip="klr">(precipe pri vortoj kaj
      esprimoj)</uzo>
    <dif>
      Malnovi&gcirc;inta, ne estanta plu en uzado;
      <ref tip="sin" cel="arhxaik.0a.KOMUNE">ar&hcirc;aika</ref>,
      <ref tip="sin" cel="temp.eks0a.KOMUNE">ekstempa</ref>:
      <ekz>
        tie talibanoj (koran-dis&ccirc;iploj) establis obskuran
        re&gcirc;imon, <tld/>an teokration
        <fnt>
          <aut>S. Maul</aut>,
          <vrk>Murdi je nomo de dio</vrk>,
          <bib>Monato</bib>,
          <lok>2001:10, p. 5a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mod">eksmoda</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_a;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">arcaic</trd>
  <trdgrp lng="cs">
    <trd>archaick&yacute;</trd>,
    <trd>starobyl&yacute;</trd>,
    <trd>zastaral&yacute;</trd>
  </trdgrp>
  <trd lng="de">archaisch</trd>
  <trd lng="en">archaic</trd>
  <trd lng="es">arc&aacute;ico</trd>
  <trd lng="fr">archa&iuml;que</trd>
  <trdgrp lng="hu">
    <trd>archaikus</trd>,
    <trd>&odblac;sr&eacute;gi</trd>
  </trdgrp>
  <trd lng="it">arcaico</trd>
  <trd lng="nl">archa&iuml;sch</trd>
  <trd lng="pl">archaiczny</trd>
  <trd lng="pt">arcaico</trd>
  <trd lng="ro">arhaic</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_h;&c_a;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_r;&c_h;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_v;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>archaick&yacute;</trd>,
    <trd>starobyl&yacute;</trd>,
    <trd>zastaran&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="arkaik.0a&jcirc;o">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
    A&jcirc;o <ref tip="vid" cel="arkaik.0a">arkaika</ref>.
    </dif>
  </snc>
  <trd lng="fr">antiquit&eacute;</trd>
</drv>
</art>
