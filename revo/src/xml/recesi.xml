<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: recesi.xml,v 1.6 2015/07/04 08:37:42 revo Exp $">
<kap>
  <rad>recesi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="recesi.0o">
  <kap><tld/>o</kap>
  <snc mrk="recesi.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Gcirc;enerala malgrandi&gcirc;o de produktado:
      <ekz>
        La
        koktelejoj estas malplenaj, la
        omarrestoracioj estas
        malplenaj, ni havas
        <tld/>on, oni diras
        <fnt>
          <aut>C. Bukowski, trad. Cezar</aut>,
          <vrk><url
          ref="http://www.ipernity.com/blog/cezar/143219">Recesio</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="depresi.0o.EKON">depresio <sncref/></ref>
  </refgrp>
  <trd lng="bg">&c_r;&c_je;&c_c;&c_je;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">recessi&oacute;</trd>
  <trd lng="de">Rezession</trd>
  <trd lng="fr">r&eacute;cession</trd>
  <trd lng="nl">recessie</trd>
  <trdgrp lng="sk">
    <trd>recesia</trd>,
    <trd>výstredný žart</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>recese</trd>,
    <trd>výstřední žerty</trd>
  </trdgrp>
</drv>
</art>

