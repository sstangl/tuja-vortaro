  <ofc>2</ofc>
  <rad>lama</rad>/o
</kap>
<drv mrk="lama.0o">
  <kap><tld/>o</kap>
  <snc mrk="lama.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      <ref tip="super" cel="majstr.0o.rabeno">Majstro</ref> en tibeta
      kaj mongola budaismo, kaj preska&ubreve; &ccirc;iam anka&ubreve;
      <ref tip="super" cel="monahx.0o">mona&hcirc;o</ref>:
      <ekz>
        Alexandra sukcesis trapasi la tibetan limon vestinte sin kiel
        almozulino kune kun fidela <tld/>o Yongden
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Verkistino - orientalistino - esploristino</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 1a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_m;&c_a; <klr>(&c_b;&c_u;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_m;&c_a;&c_n;&c_a;&c_h;)</klr></trd>
