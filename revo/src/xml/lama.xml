  <trd lng="ru">&c_l;&c_a;&c_m;&c_a; <klr>(&c_b;&c_u;&c_d;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
