<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: papava.xml,v 1.11 2015/07/04 08:37:41 revo Exp $">
<kap><rad>papavac</rad>/oj</kap>
<drv mrk="papava.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Papaveraceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_m;&c_a;&c_k;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">papaver&agrave;cies</trd>
  <trd lng="cs">mákovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Mohngew&auml;chse</trd>
  <trd lng="hu">m&aacute;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">papaverfamilie</trd>
  <trd lng="pt">papaver&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">makovité</trd>
</drv>
</art>

