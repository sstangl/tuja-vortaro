<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komatu.xml,v 1.9 2013/09/01 09:10:19 revo Exp $">
<kap>
  <rad>komatul</rad>/oj
</kap>
<drv mrk="komatu.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="komatu.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> 
      el la filumo <ref tip="super"
      cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>
      (<trd lng="la">Comatulida</trd>).
    </dif>
  </snc>
  <trd lng="de">Federsterne</trd>
  <trd lng="en">feather star</trd>
  <trd lng="fr">comatules</trd>
  <trdgrp lng="pt">
    <trd>equinodermos</trd>,
    <trd>com&aacute;tulas</trd>
  </trdgrp>
</drv>
</art>

