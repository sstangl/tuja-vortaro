<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: japanesk.xml,v 1.1 2013/07/29 01:10:13 revo Exp $">
<kap>
  <rad>japanesk</rad>/o
</kap>
<drv mrk="japanesk.0o">
  <kap><tld/>o</kap>
  <snc mrk="japanesk.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Io kun japanaj ecoj, interalie
      <ref tip="super" cel="poezi.0ajxo.POE">poezia&jcirc;o</ref>
      farita la&ubreve; tradicia japana formo:
      <ekz>
        ni rimarkis la multjaran furoron de <tld/>o,
        kiu, la&ubreve; Waringhien, funkciis kvaza&ubreve; kiel prezentanto
        de la orienta poezio
        <fnt>
          <aut>Guozhu</aut>,
          <vrk>Diverska&ubreve;ze &gcirc;ojinda</vrk>,
          <lok><url ref="www.elerno.cn/penseo/PEN189.pdf">
          Penseo, 2007-07 (189)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>japonaiserie</trd>,
    <trd>po&egrave;me &agrave; la mani&egrave;re japonaise</trd>
  </trdgrp>
</drv>
</art>

