<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: antise1.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>antiseps</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="antise1.0o">
  <kap><tld/>o</kap>
  <snc mrk="antise1.0o.MED">
    <uzo tip="fak">KIR</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Procedo, per kiu oni detruas en &hcirc;irurgio
      la infektantajn mikrobojn per &hcirc;emiaj substancoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="aseps.0o">asepso</ref>.
    </refgrp>
  </snc>
  <rim>
    &leftquot;Antisepso&rightquot; estas medicina
    vorto kaj rilatas al uzo de substancoj, kiujn
    viva korpo eltenas.  
    &leftquot;<ref tip="vid" cel="desinf.0i.SAN">Desinfekto</ref>&rightquot;
    esence samas, sed koncernas purigon de &ccirc;iaj objektoj,
    do estas plenumebla per ajna efika rimedo.
    <aut>&jcirc;v</aut>
  </rim>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_p;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>esterilitzaci&oacute;</trd>,
    <trd>as&egrave;psia</trd>
  </trdgrp>
  <trd lng="en">antisepsis</trd>
  <trd lng="es">antisepsia</trd>
  <trd lng="fr">antisepsie</trd>
  <trd lng="hu">antiszepszis</trd>  
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">antisepsa</trd>
  <trd lng="sk">antiseptikum</trd>
  <trdgrp lng="cs">
    <trd>antiseptický prostředek</trd>,
    <trd>antiseptikum</trd>
  </trdgrp>
</drv>

<drv mrk="antise1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="antise1.0i.MED">
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="desinf.0i">desinfekti</ref>
    </refgrp>
  </snc>
  <trd lng="ca">desinfectar</trd>
  <trd lng="cs">použít antiseptický prostředek</trd>
  <trd lng="de">desinfizieren</trd>
  <trd lng="en">disinfect</trd>
  <trd lng="fr">d&eacute;sinfecter</trd>
  <trdgrp lng="hu">
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>dezinfici&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sterilizačný postup</trd>,
    <trd>vykonať antisepsu</trd>
  </trdgrp>
</drv>

<drv mrk="antise1.0a">
  <kap><tld/>a</kap>
  <snc mrk="antise1.0a.MED">
    <dif>
      Per <tld/>o detruanta la infektajn mikrobojn;
      kontra&ubreve;mikroba;
      senmikrobiga:
      <ekz>
        mi estis trabanita en <tld/>a &scirc;prucado
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>as&egrave;ptic</trd>,
    <trd>desinfectant</trd>
  </trdgrp>
  <trd lng="cs">antiseptický</trd>
  <trd lng="de">antiseptisch</trd>
  <trd lng="en">antiseptic</trd>
  <trd lng="es">antis&eacute;ptico</trd>
  <trd lng="fr">antiseptique<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>fert&odblac;tlen&iacute;t&odblac;</trd>,
    <trd>cs&iacute;r&aacute;tlan&iacute;t&oacute;</trd>,
    <trd>antiszeptikus</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">antiseptick&yacute;</trd>
</drv>

<drv mrk="antise1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="antise1.0ajxo.MED">
    <dif>
      Substanco uzata por detrui &ccirc;iajn mikrobojn a&ubreve;
      fungojn, kontraste al
      <ref tip="vid" cel="antibi.0o.MED">antibiotiko</ref>,
      kiu efikas kontra&ubreve; apartaj specoj:
      <ekz>
        lavenda oleo <klr>...</klr> efikas kiel
        trankviligilo kaj kiel <tld/>a&jcirc;o
        <fnt>
          <aut>A. Watanuki</aut>,
          <vrk><url
          ref="http://www.kt.rim.or.jp/~aleks-jp/aromaterapio/aromaoleo.htm">
          Eteraj oleoj</url></vrk>,
          <lok>[2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">antiseptique<klr> (subst.)</klr></trd>
</drv>
</art>
