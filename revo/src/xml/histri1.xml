<vortaro>
<art mrk="$Id: histri1.xml,v 1.15 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>histrion</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="histri1.0o">
  <kap><tld/>o</kap>
  <snc mrk="histri1.0o.amuzisto">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="amuz.0isto.KOMUNE">Amuzisto</ref>
      prezentanta
      vulgarajn kaj krudajn komika&jcirc;ojn.
      <ekz>
        ne gravas kiu estas la homo &ndash; &ccirc;u prozulo au poeto, re&gcirc;o a&ubreve;
        <tld/>o, miliardulo a&ubreve; almozpetanto
        <fnt>
        <aut>faa</aut>,
        <vrk>REK - EoLA invitas vin!</vrk>,
        <lok>soc.culture.esperanto, 2000-12-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Histrione</trd>
    <trdgrp lng="hu">
      <trd>poj&aacute;ca</trd>,
      <trd>kom&eacute;di&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">histrion</trd>
    <trd lng="pt">histri&atilde;o</trd>
  </snc>
  <snc mrk="histri1.0o.aktoracxo">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Aktora&ccirc;o.
    </dif>
    <trdgrp lng="de">
      <trd>Kom&ouml;diant</trd>,
      <trd>Schmierenschauspieler</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>ham<klr tip="amb"> (bad actor)</klr></trd>
      <trd>third-rate actor</trd>
    </trdgrp>
    <trd lng="hu">csep&udblac;r&aacute;g&oacute;</trd>
    <trdgrp lng="nl">
      <trd>komediant</trd>,
      <trd>slecht <ind>toneelspeler</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>harlekýn</trd>,
    <trd>histrión</trd>,
    <trd>klaun</trd>,
    <trd>komediant</trd>,
    <trd>šašek</trd>
  </trdgrp>
  <trd lng="fr">histrion</trd>
  <trd lng="it">istrione</trd>
  <trd lng="ru">&c_f;&c_i;&c_g;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>komediant</trd>,
    <trd>šašo</trd>
  </trdgrp>
</drv>
</art>
