<vortaro>
<art mrk="$Id: buksac.xml,v 1.7 2014/01/13 07:10:30 revo Exp $">
<kap>
  <rad>buksac</rad>/oj
</kap>
<drv mrk="buksac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Buxaceae</trd>), &ccirc;iamverdaj arbetoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_sh;&c_y;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Buchsbaumgew&auml;chse</trd>
  <trd lng="fr">buxac&eacute;es</trd>
  <trd lng="hu">puszp&aacute;ngf&eacute;l&eacute;k</trd>
  <trd lng="pl">bukszpanowate</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_sh;&c_i;&c_t;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

