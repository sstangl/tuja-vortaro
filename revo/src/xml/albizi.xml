<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: albizi.xml,v 1.6 2014/09/12 07:10:16 revo Exp $">
<kap>
  <rad>albizi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="albizi.0o">
  <kap><tld/>o</kap>
  <snc mrk="albizi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> de arboj el familio <ref
      tip="malprt"
      cel="mimoza.0oj">mimozacoj</ref>
      (<trd lng="la">Albizia</trd>) el diversaj tropikaj landoj
      kun kunmetitaj folioj kaj kvastoformaj floretoj:
      <ekz>
        kuzino Ora diris, ke <klr><tld/>o</klr> populare estas
        Noktokunigo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, volumo 2a, p.
        605a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_a;&c_l;&c_b;&c_i;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">alb&iacute;zia</trd>
  <trd lng="da">albizia</trd>
  <trdgrp lng="de">
    <trd>Schlafbaum</trd>,
    <trd>Seidenbaum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>albizia</trd>,
    <trd>silk plant</trd>,
    <trd>silk tree</trd>,
    <trd>siris</trd>
  </trdgrp>
  <trd lng="fr">albizia</trd>
  <trd lng="it">albizia</trd>
  <trd lng="pl">albicja</trd>
</drv>
</art>

