<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: abak.xml,v 1.42 2015/09/17 16:10:25 revo Exp $">
<kap>
  <rad>abak</rad>/o
</kap>
<drv mrk="abak.0o">
  <kap><tld/>o</kap>
  <snc mrk="abak.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Supera plata parto de kolona
      <ref tip="vid" cel="kapite.0o">kapitelo</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">&agrave;bac</trd>
    <trdgrp lng="de">
      <trd>S&auml;ulenplatte</trd>,
      <trd>Kapitelldeckplatte</trd>
    </trdgrp>
    <trd lng="el">&alfa_ton;&beta;&alfa;&xi;</trd>
    <trd lng="en">abacus</trd>
    <trd lng="fa">&a_s;&a_r;&a_s;&a_t;&a_w;&a_n;&f_y;</trd>
    <trd lng="fi">abakus</trd>
    <trd lng="fr">abaque<klr tip="ind"> (architecture)</klr></trd>
    <trdgrp lng="hu">
      <trd>abakusz</trd>,
      <trd>fejlemez<klr> (oszlopf&odblac;n)</klr></trd>
    </trdgrp>
    <trd lng="hr">abak</trd>
    <trd lng="it">abaco</trd>
    <trd lng="nl">abacus</trd>
    <trdgrp lng="pl">
      <trd>abakus</trd>,
      <trd>abak</trd>,
      <trd>p&lstroke;yta g&lstroke;owicowa<klr> (kolumny)</klr></trd>
    </trdgrp>
    <trd lng="pt">&aacute;baco</trd>
    <trd lng="ru">&c_a;&c_b;&c_a;&c_k;&c_a;</trd>
    <trd lng="sk">abakus</trd>
  </snc>
  <snc mrk="abak.0o.kalkulilo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Speco de <ref tip="super" cel="kalkul.0ilo">kalkulilo</ref>
      por plenumi aritmetikajn operaciojn,
      konsistanta &ccirc;efe el buletoj &scirc;oveblaj sur
      stangetoj:
      <ekz>
	vi mispu&scirc;as globetojn de via <tld/>o
	<fnt>
          <bib>RugxDom</bib> 
          <lok>&ccirc;apitro 65a, p. 408a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="soroban.0o">sorobano</ref>
    <trd
    lng="be">&c_l;&c_ib;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_ib;</trd>
    <trd lng="bg">&c_a;&c_b;&c_a;&c_k;&c_u;&c_s;</trd>
    <trd lng="ca">&agrave;bac</trd>
    <trdgrp lng="de">
      <trd>Rechenbrett</trd>,
      <trd>Abakus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>abacus</trd>,
      <trd>calculating frame</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_t;&f_k;&a_h;</trd>,
      <trd>&f_ch;&a_r;&a_t;&f_k;&a_h;</trd>
    </trdgrp>
    <trd lng="fi">helmitaulu</trd>
    <trdgrp lng="fr">
      <trd>abaque<klr tip="ind"> (boulier)</klr></trd>,
      <trd>boulier</trd>
    </trdgrp>
    <trd lng="he">&het;&shin;&bet;&vav;&nun;&yod;&he;</trd>
    <trdgrp lng="hu">
      <trd>abakusz</trd>,
      <trd>sz&aacute;mol&oacute;g&eacute;p</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>abaco</trd>,
      <trd>pallottoliere</trd>
    </trdgrp>
    <trdgrp lng="hr">
      <trd>abak</trd>
    </trdgrp> 
    <trdgrp lng="nl">
      <trd>abacus</trd>,
      <trd>telraam</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>abakus</trd>,
      <trd>liczyd&lstroke;o</trd>,
      <trd>tabliczka Pitagorasa</trd>
    </trdgrp> 
    <trd lng="pt">&aacute;baco</trd>
    <trd lng="ru">&c_s;&c_ch;&c_jo;&c_t;&c_y;</trd>
    <trd lng="sk">po&ccaron;&iacute;tadlo<klr>
    (guli&ccaron;kov&eacute;)</klr></trd>
  </snc>
  <snc mrk="abak.0o.grafika.tabela">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Grafika&jcirc;o a&ubreve; tabelo, uzata por anstata&ubreve;i
      kalkulon en la solvo de kelkaj matematikaj
      a&ubreve; te&hcirc;nikaj problemoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_m;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;
      <ind>&c_t;&c_a;&c_b;&c_l;&c_ib;&c_c;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>nomograma</trd>,
      <trd>&agrave;bac</trd>,
      <trd>gr&agrave;fic</trd>
    </trdgrp>
    <trd lng="de">Rechentafel</trd>
    <trdgrp lng="en">
      <trd>nomogram</trd>,
      <trd>chart</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_d;&a_s;&a_t;&a_w;&a_r;&a_n;&f_g;&a_A;&a_r;</trd>,
      <trd>&a_n;&a_w;&a_m;&a_w;&f_g;&a_r;&a_A;&a_m;</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>nomogrammi</trd>,
      <trd>laskutaulukko</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>nomogramme</trd>,
      <trd>abaque<klr tip="ind"> (nomogramme)</klr></trd>,
      <trd>table<klr tip="ind"> (nomogramme)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nomogram</trd>,
      <trd>sz&aacute;mol&oacute;t&aacute;bla</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>abaco</trd>,
      <trd>nomogramma</trd>,
      <trd>nomografo</trd>
    </trdgrp>
    <trd lng="nl">abacus</trd>
    <trd lng="pl">nomogram</trd>
    <trd lng="pt">&aacute;baco</trd>	  
    <trdgrp
    lng="ru"><trd>&c_n;&c_o;&c_m;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_je;
      &c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_y;</trd>
    </trdgrp>
    <trd lng="sk">matematick&aacute; tabu&lcaron;ka</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>abak</trd>,
    <trd>nomogram</trd>
  </trdgrp>
  <trd lng="es">&aacute;baco</trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;kov&eacute; po&ccaron;&iacute;tadlo</trd>,
    <trd>gu&lcaron;kov&eacute; po&ccaron;&iacute;tadlo, abakus</trd>
  </trdgrp>
  <trd lng="tr">abak&uuml;s</trd>
</drv>
</art>
