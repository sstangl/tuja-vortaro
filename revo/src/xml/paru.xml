<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paru.xml,v 1.25 2015/06/29 16:40:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>paru</rad>/o
</kap>
<drv mrk="paru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="paru.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="parued.0oj"><tld/>edoj</ref>
      (<trd lng="la">Parus</trd>), gracia birdo kun mallonga beko
      man&gcirc;anta insektojn kaj grengrajnojn.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">pennduig</trd>
  <trd lng="cs">s&yacute;kora</trd>
  <trd lng="de">Meise</trd>
  <trdgrp lng="en">
    <trd>titmouse</trd>,
    <trd>tit</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;sange</trd>
  <trd lng="hu">cinege</trd>
  <trd lng="nl">mees</trd>
  <trdgrp lng="pl">
    <trd>sikora</trd>,
    <trd>sikorka<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">abelheiro</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_i;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_z;&c_o;&c_r;&c_je;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_s;&c_k;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">s√Ωkorka</trd>
</drv>

<drv mrk="paru.blua0o">
  <kap>blua <tld/>o</kap>
  <snc mrk="paru.blua0.specio">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio el la genro <ref tip="super"
      cel="paru.0o"><tld/>oj</ref> (<trd lng="la">Parus caeruleus</trd>)
      kun blua krono, blankaj frunto kaj vangoj kun nigra streko tra la
      okulo 
      kaj blanka streko super &gcirc;i, nigra gor&gcirc;o, flavaj subaj
      partoj kun
      iom da blueca nuanco ventre, bluaj flugiloj kaj vosto, olivverda
      dorso.
    </dif>
  </snc>
  <bld lok="../bld/bluaparuo.jpg" lrg="400">
    blua paruo
  </bld>
  <trd lng="de">Blaumeise</trd>
  <trd lng="en">blue tit</trd>
  <trd lng="fr">m&eacute;sange bleue</trd>
  <trd lng="nl">pimpelmees</trd>
  <trdgrp lng="pl">
    <trd>sikora modra</trd>,
    <trd>modraszka</trd>
  </trdgrp>
</drv>

<drv mrk="paru.granda0o">
  <kap>granda <tld/>o</kap>
  <snc mrk="paru.granda0.specio">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio el la genro <ref tip="super"
      cel="paru.0o"><tld/>oj</ref> (<trd lng="la">Parus major</trd>)
      kun nigra kapo kaj kolo, blankaj vangoj kaj orelkovriloj, flava
      ventro kun nigra streko en la mezo kaj olivverda dorso.
    </dif>
  </snc>
  <trd lng="de">Kohlmeise</trd>
  <trd lng="en">great tit</trd>
  <trd lng="es"><ind>carbonero</ind> com&uacute;n</trd>
  <trd lng="fr">m&eacute;sange charbonni&egrave;re</trd>
  <trd lng="nl">koolmees</trd>
  <trdgrp lng="pl">
    <trd>sikora bogatka</trd>,
    <trd>bogatka</trd>
  </trdgrp>
</drv>

</art>
