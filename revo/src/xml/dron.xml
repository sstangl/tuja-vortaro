<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: dron.xml,v 1.26 2015/10/30 11:10:23 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>dron</rad>/i
    </kap>

    <drv mrk="dron.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="dron.0i.morti">
        <dif>
          Perei per sufoki&gcirc;o en akvo:
          <ekz>
            <tld/>anto e&ccirc; herbeton kaptas avide
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>v&iacute;zbe <ind>f&uacute;l</ind></trd>,
          <trd>belefullad</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>uton&aogonek;&cacute;</trd>,
          <trd>utopi&cacute; si&eogonek;</trd>
        </trdgrp>
        <trd lng="pt">afogar-se</trd>
        <trd lng="sv">drunkna</trd>
      </snc>
      <snc mrk="dron.0i.MAR">
        <uzo tip="fak">MAR</uzo>
        <dif>
          &Scirc;ipperei per subakvi&gcirc;o, enabismi&gcirc;i:
          <ekz>
            la &scirc;ipo <tld/>is kun la tuta &scirc;ipanaro.
          </ekz>
        </dif>
        <!-- refgrp tip="vid">
        <ref cel="sink">sinki</ref>.
      </refgrp -->
        <adm>
          "Enabismi&gcirc;i" meritus aperi sub "abismo". &Gcirc;i
          aspektas
          bona beletra sinonimo de tiu snc kaj anka&ubreve; de la
          figuraj.
          Enkonduki artikolon "sinki".
          <aut>MB</aut>
        </adm>
        <trd lng="hu">els&uuml;llyed</trd>
        <trd lng="pl">zaton&aogonek;&cacute;</trd>
        <trdgrp lng="pt">
          <trd>afundar</trd>,
          <trd>so&ccedil;obrar</trd>,
          <trd>ir a <ind>pique</ind></trd>,
          <trd>ir ao <ind>fundo</ind></trd>
        </trdgrp>
        <trd lng="sv">g&aring; under</trd>
      </snc>
      <snc mrk="dron.0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Enprofundi&gcirc;i kaj malaperi a&ubreve; perdi&gcirc;i:
          <ekz>
            <tld/>i en amaso da kusenoj;
          </ekz><ekz>
            <tld/>antaj en koto reloj;
          </ekz><ekz>
            domo <tld/>anta en silento kaj duonlumo<fnt>B</fnt>;
          </ekz><ekz>
            la vo&ccirc;o <tld/>is en bruo;
          </ekz><ekz>
            <tld/>i en krepusko<fnt>B</fnt>,
          </ekz><ekz>
            nebulo, mallumo, malproksimo;
          </ekz><ekz>
            en azia lukso <tld/>is la energio de l' faraonoj
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap.
            Anta&ubreve;parolo</lok></fnt>;
          </ekz><ekz>
            tute <tld/>inta epoko.
          </ekz>
        </dif>
        <trd lng="hu">elmer&uuml;l</trd>
        <trdgrp lng="pl">
          <trd>ton&aogonek;&cacute;</trd>,
          <trd>uton&aogonek;&cacute;</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>afundar</trd>,
          <trd>mergulhar</trd>,
        </trdgrp>
        <trd lng="sv">drunkna</trd>
      </snc>
      <snc mrk="dron.0i.tro">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Embarasi&gcirc;i kaj impliki&gcirc;i, esti super&scirc;utata
          kaj
          embarasata per io:
          <ekz>
            <tld/>i en &scirc;uldoj<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>i en detaloj<fnt><bib>PrV</bib></fnt>;</ekz><ekz>
            <tld/>i en laboroj &gcirc;is super la oreloj<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trd lng="hu">mer&uuml;l</trd>
        <trdgrp lng="pl">
          <trd>ton&aogonek;&cacute;</trd>,
          <trd>uton&aogonek;&cacute;</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>afundar</trd>,
          <trd>afogar-se</trd>,
          <trd>mergulhar</trd>,
        </trdgrp>
        <trd lng="sv">drunkna</trd>
      </snc>
      <snc mrk="dron.0i.okupigxi">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Enprofundi&gcirc;i kaj esti ekskluzive okupata per:
          <ekz>
            <tld/>i en medito<fnt>B</fnt>;
          </ekz><ekz>
            <tld/>i en revoj, en siaj pensoj, en malespero<fnt>B</fnt>;
          </ekz><ekz>
            <tld/>i en larmoj, en enuo, en admiro<fnt>B</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>elm&eacute;lyed</trd>,
          <trd>elmer&uuml;l</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>ton&aogonek;&cacute;</trd>,
          <trd>uton&aogonek;&cacute;</trd>,
          <trd>zaton&aogonek;&cacute;</trd>,
          <trd>zatopi&cacute; si&eogonek;</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>afundar</trd>,
          <trd>afogar-se</trd>,
          <trd>mergulhar</trd>,
        </trdgrp>
        <trdgrp lng="sv">
          <trd>f&ouml;rsjunka</trd>,
          <trd>drunkna</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_t;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_a;&c_t;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>tonout</trd>,
    <trd>topit se</trd>,
    <trd>utopit se</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>ertrinken</trd>,
        <trd>untergehen</trd>
      </trdgrp>
  <trd lng="en">drown</trd>
  <trdgrp lng="fr">
        <trd>couler<klr tip="ind"> (sombrer)</klr></trd>,
        <trd>faire <ind>naufrage</ind></trd>,
        <trd>se <ind>noyer</ind></trd>,
        <trd>sombrer<klr tip="ind"> (couler)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_t;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_t;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_t;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">topi&tcaron; sa</trd>
</drv>

    <drv mrk="dron.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Igi <tld/>anta 1 kaj 2:
          <ekz>
            por hundon <tld/>igi, oni nomas &gcirc;in rabia
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            ne maro <tld/>igas &scirc;ipon sed la ventoj
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="akv">subakvigi</ref>,
          <ref cel="merg.0i">mergi</ref>,
          <ref cel="fund">alfundigi</ref>,
          <ref cel="abism">enabismigi</ref>.
        </refgrp>
        <trdgrp lng="en"><trd>to drown</trd>, <trd>sink</trd></trdgrp>
        <trd lng="hu">v&iacute;zbe <ind>fojt</ind></trd>
        <trdgrp lng="pl">
          <trd>topi&cacute;</trd>,
          <trd>zatapia&cacute;</trd>
        </trdgrp>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Enprofundigi:
          <ekz>
            <tld/>igi la manojn en la sangon de iu;
          </ekz><ekz>
            la infano <tld/>igis en la longaj haroj de l' hundo siajn
            malgrandajn manetojn<fnt>B</fnt>;
          </ekz><ekz>
            <tld/>igi glavon en ies bruston<fnt>B</fnt>;
          </ekz><ekz>
            la brilo de la luno <tld/>igis la montetojn kvaza&ubreve; en
            vasta ar&gcirc;enta bano;
          </ekz><ekz>
            tiu kvartalo <tld/>i&gcirc;as en
            luksegaj &gcirc;ardenoj.
          </ekz>
        </dif>
        <trdgrp lng="en">
          <trd>to submerge</trd>,
          <trd>immerse</trd>, <trd>to drown in</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>s&uuml;llyeszt</trd>,
          <trd>belem&eacute;lyeszt</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>topi&cacute;</trd>,
          <trd>zatapia&cacute;</trd>
        </trdgrp>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Malaperigi, &ccirc;esigi:
          <ekz>
            la tondra ruli&gcirc;ado de a&ubreve;tomobiloj <tld/>igas
            <klr>(superbruas, sena&ubreve;digas)</klr> &ccirc;iujn
            aliajn bruojn;
          </ekz><ekz>
            la ribelo estis <tld/>igita en sango.
          </ekz>
        </dif>
        <trd lng="en">to drown out</trd>
        <trdgrp lng="hu">
          <trd>elfojt</trd>,
          <trd>megfojt</trd>
        </trdgrp>
        <trd lng="pl">topi&cacute;</trd>
      </snc>
  <trdgrp lng="be">
        <trd>&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_u;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>topit</trd>,
    <trd>utopit</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>ertr&auml;nken</trd>,
        <trd>tauchen</trd>,
        <trd>tunken</trd>,
        <trd>untertunken</trd>,
        <trd>eintunken</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>couler<klr tip="ind"> (faire sombrer)</klr></trd>,
        <trd>faire <ind>sombrer</ind></trd>,
        <trd>noyer</trd>,
        <trd>plonger<klr tip="ind"> (qc dans qc)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">utopi&tcaron;</trd>
  <trd lng="sv">dr&auml;nka</trd>
</drv>

    <drv mrk="dron.sin0igi">
      <kap>sin <tld/>igi</kap>
      <snc>
        <dif>
          <ref tip="super" cel="mort.sin0igi">Sin mortigi</ref>
          per subakvi&gcirc;o:
          <ekz>
            &#8208; &Ccirc;ion bonan, <em>messire</em>, &#8208; &scirc;i
            diris vo&ccirc;e
            kaj pensis: &leftquot;For, iel ajn for de &ccirc;i tie,
            ekstere mi
            iros al la rivero kaj min <tld/>igos&rightquot;
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_t;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_u;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="de">sich <ind>ertr&auml;nken</ind></trd>
      <trd lng="en">to drown oneself</trd>
      <trd lng="fr">se <ind>noyer</ind><klr tip="ind"> (se tuer par
          noyade)</klr></trd>
      <trd lng="hu">v&iacute;zbe &ouml;li mag&aacute;t</trd>
      <trdgrp lng="pl">
        <trd>topi&cacute; si&eogonek;</trd>,
        <trd>utopi&cacute; si&eogonek;</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_u;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="dron.0into">
      <kap><tld/>into</kap>
      <snc>
        <dif>
          Homo, kiu <tld/>is.
        </dif>
        <trd lng="de">Ertrunkener</trd>
        <trd lng="pl">topielec</trd>
      </snc>
  <trd lng="cs">utopenec</trd>
  <trd lng="en">drowned one</trd>
  <trd lng="sk">utopenec</trd>
</drv>
  </art>
