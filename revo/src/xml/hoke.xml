<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hoke.xml,v 1.17 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hoke</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hoke.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Pilka ludo, &ccirc;e kiu la pilko estas pu&scirc;ata kaj batata per
      specialaj kurbaj bastonoj t.n. <tld/>iloj:
      <ekz>
        la regularo de <tld/>oludo similas tiun de la piedpilka ludo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_k;&c_je;&c_j;</trd>
  <trd lng="bg">&c_h;&c_o;&c_k;&c_je;&c_j;</trd>
  <trd lng="cs">hokej</trd>
  <trd lng="de">Hockey</trd>
  <trd lng="el">&chi;&omikron_ton;&kappa;&jota;</trd>
  <trd lng="en">hockey</trd>
  <trd lng="fa">&a_h;&a_A;&f_k;&f_y;</trd>
  <trd lng="fr">hockey</trd>
  <trdgrp lng="hu">
    <trd>hoki</trd>,
    <trd>j&eacute;korong</trd>,
    <trd>gyeplabda</trd>
  </trdgrp>
  <trd lng="it">hockey</trd>
  <trd lng="nl">hockey</trd>
  <trd lng="pt">h&oacute;quei</trd>
  <trd lng="ru">&c_h;&c_o;&c_k;&c_k;&c_je;&c_j;</trd>
  <trd lng="sk">hokej</trd>
</drv>

<drv mrk="hoke.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Speco de kurba bastono uzata &ccirc;e la <tld/>a ludo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_h;&c_a;&c_k;&c_je;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_s;&c_t;&c_i;&c_k; &c_z;&c_a; &c_h;&c_o;&c_k;&c_je;&c_j;</trd>
  <trd lng="cs">hokejka</trd>
  <trd lng="de">Hockeyschl&auml;ger</trd>
  <trd lng="en">hockey stick</trd>
  <trd lng="fa">&f_ch;&a_w;&a_b; <ind>&a_h;&a_A;&f_k;&f_y;</ind></trd>
  <trd lng="fr">crosse<klr tip="ind"> (de hockey)</klr></trd>
  <trd lng="hu">hoki&uuml;t&odblac;</trd>
  <trd lng="it">bastone da <ind>hockey</ind></trd>
  <trd lng="nl">hockeystick</trd>
  <trd lng="pt">estique<klr tip="amb"> (de h&oacute;quei)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_ju;&c_sh;&c_k;&c_a; <klr>(&c_h;&c_o;&c_k;&c_k;&c_je;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">hokejka</trd>
</drv>
</art>
