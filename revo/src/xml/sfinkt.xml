<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sfinkt.xml,v 1.17 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sfinkter</rad>/o
</kap>
<drv mrk="sfinkt.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Ringforma muskolo, kiu stre&ccirc;i&gcirc;ante fermas iujn tubformajn
      organojn.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_f;&c_ib;&c_n;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">esf&iacute;nter</trd>
  <trdgrp lng="cs">
    <trd>sfinkter</trd>,
    <trd>svěrací sval</trd>,
    <trd>svěrač</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sphinkter</trd>,
    <trd>Ringmuskel</trd>,
    <trd>Schlie&szlig;muskel</trd>
  </trdgrp>
  <trd lng="en">sphincter</trd>
  <trd lng="es">esf&iacute;nter</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_f;&a_n;&f_k;&a_t;&a_r;</trd>,
    <trd>&a_ayn;&a_D;&a_l;&a_h;&a_hamza_sure; &a_H;&a_l;&a_q;&a_w;&f_y;</trd>,
    <trd>&a_ayn;&a_D;&a_l;&a_h;&a_hamza_sure; &a_h;&a_m;&zwnj;&f_k;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">sphincter</trd>
  <trd lng="hu">z&aacute;r&oacute;izom</trd>
  <trd lng="it">sfintere</trd>
  <trdgrp lng="nl">
    <trd>kringspier</trd>,
    <trd>sluitspier</trd>
  </trdgrp>
  <trd lng="pl">zwieracz <klr>(mi&eogonek;sie&nacute;)</klr></trd>
  <trd lng="ru">&c_s;&c_f;&c_i;&c_n;&c_k;&c_t;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>sfinkter</trd>,
    <trd>zvierač <klr>(sval)</klr></trd>
  </trdgrp>
</drv>
</art>
