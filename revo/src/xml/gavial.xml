<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gavial.xml,v 1.20 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gavial</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gavial.0o">
  <kap><tld/>o</kap>
  <snc mrk="gavial.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" cel="reptil.0o"
      lst="voko:krokodiluloj">reptilio</ref>
      kun mallar&gcirc;a muzelo, vivanta en norda 
      <ref tip="vid" cel="hind.H0ujo">Hindujo</ref> kaj 
      <ref tip="vid" cel="nepal.N0o">Nepalo</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la familio <ref
      tip="malprt"
      cel="gavial1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gavialis</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_v;&c_ib;&c_ja;&c_l;</trd>
  <trd lng="ca">gavial</trd>
  <trd lng="cs">gaviál <klr>(krokodýl)</klr></trd>
  <trdgrp lng="de">
    <trd>Gavial</trd>,
    <trd>Gangeskrokodil</trd></trdgrp>
  <trd lng="fr">gavial</trd>
  <trdgrp lng="hu">
    <trd>gavi&aacute;l</trd>,
    <trd>cs&odblac;r&ouml;s <ind>krokodil</ind></trd></trdgrp>
  <trd lng="pt">gavial</trd>
  <trd lng="ru">&c_g;&c_a;&c_v;&c_i;&c_a;&c_l;</trd>
  <trd lng="sk">gaviál <klr>(krokodíl)</klr></trd>
</drv>

</art>

