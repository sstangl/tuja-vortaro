<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: navet.xml,v 1.6 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>navet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="navet.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="naved.0o">navedo</ref>
  </snc>
  <trd lng="cs">člunek</trd>
  <trd lng="pt">lan&ccedil;adeira</trd>
  <trdgrp lng="sk">
    <trd>člnok <klr>(tkáčsky, alebo v šijacom stroji)</klr></trd>,
    <trd>člnok <klr>(tkáčsky| alebo v šijacom stroji)</klr></trd>
  </trdgrp>
  <trd lng="sv">skyttel</trd>
</drv>

<drv mrk="navet.0i">
  <kap><tld/>i</kap>
  <snc>
    <ref tip="dif" cel="naved.0i">navedi</ref>
    <ekz>
      amba&ubreve; <tld/>adis almena&ubreve; kvinfoje <klr>...</klr> por fini la
      transportadon
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 359a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="sv">skyttla</trd>
</drv>
</art>

