<kap>
<rad>felandri</rad>/o
</kap>
<drv mrk="feland.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="enant.0o">enanto</ref>
      (<trd lng="la">Oenanthe aquatica</trd>).
    </dif>
  </snc>
  <trd lng="cs">fenykl vodní</trd>
  <trdgrp lng="de">
    <trd>Wasserfenchel</trd>,
    <trd>Wasserpferdesaat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cig&uuml;e aquatique</trd>,
    <trd>phellandre</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>v&iacute;zi <ind>borgy&ouml;k&eacute;r</ind></trd>,
    <trd>v&iacute;zi <ind>m&eacute;telyk&oacute;r&oacute;</ind></trd>
  </trdgrp>
  <trd lng="nl">watertorkruid</trd>
  <trdgrp lng="pt">
    <trd>enante</trd>,
    <trd>fel&#226;ndrio</trd>,
    <trd>quebra-pedra <klr tip="amb">(bot.: Oenanthe aquatica)</klr></trd>
  </trdgrp>
  <trd lng="sk">fenikel vodný</trd>
</drv>
</art>
