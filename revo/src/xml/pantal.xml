<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pantal.xml,v 1.27 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>*</ofc><rad>pantalon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="pantal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pantal.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS">Vesto</ref> kovranta la korpon
      de zono &gcirc;is gamboj, dupartigita tiele, ke &gcirc;i &ccirc;irka&ubreve;as aparte
      &ccirc;iun gambon:
      <ekz>
        en la po&scirc;o de mia <tld/>o mi portas monujon
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_v;&c_ib;&c_c;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_t;&c_a;&c_l;&c_o;&c_n;</trd>
  <trd lng="br">brago&ugrave;</trd>
  <trdgrp lng="cs">
    <trd>gatě</trd>,
    <trd>gaťata</trd>,
    <trd>kalhoty</trd>,
    <trd>katě</trd>,
    <trd>kaťata</trd>
  </trdgrp>
  <trd lng="de">Hose</trd>
  <trdgrp lng="en">
    <trd>pants</trd>,
    <trd>trousers</trd>
  </trdgrp>
  <trd lng="es">pantal&oacute;n</trd>
  <trd lng="fa">&a_sh;&a_l;&a_w;&a_A;&a_r;</trd>
  <trd lng="fr">pantalon</trd>
  <trdgrp lng="hu">
    <trd>nadr&aacute;g</trd>,
    <trd>pantall&oacute;</trd>
  </trdgrp>
  <trd lng="nl">lange <ind>broek</ind></trd>
  <trdgrp lng="pl">
    <trd>spodnie</trd>,
    <trd>portki <klr>(region.)</klr></trd>,
    <trd>gacie <klr>(pot.)</klr></trd>,
    <trd>pantalony <klr>(przest.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cal&ccedil;a</trd>,
    <trd>cal&ccedil;as</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>gate</trd>,
    <trd>nohavice</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>byxa</trd>,
    <trd>byxor</trd>
  </trdgrp>
</drv>

<drv mrk="pantal.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="pantal.0eto.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS">Vesto</ref> kovranta la korpon
      de zono &gcirc;is femuroj a&ubreve; genuoj, &ccirc;irka&ubreve;ante aparte &ccirc;iun
      femuron:
      <ekz>
        jen iras dekdujara knabo en mallonga <tld/>eto
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_o;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_je;&c_r;&c_m;&c_u;&c_d;&c_a;</trd>
  <trd lng="br">brago&ugrave; berr</trd>
  <trdgrp lng="cs">
    <trd>krátké kalhoty</trd>,
    <trd>šortky</trd>
  </trdgrp>
  <trd lng="en">pantaloon</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_l;&a_w;&a_A;&a_r;&f_k;</trd>
    <trd>&a_n;&f_y;&a_m;&zwnj;&a_sh;&a_l;&a_w;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">short</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;vidnadr&aacute;g</trd>,
    <trd>sort</trd>,
    <trd>bermuda</trd>
  </trdgrp>
  <trd lng="nl">korte <ind>broek</ind></trd>
  <trdgrp lng="pl">
    <trd>kr&oacute;tkie <ind>spodenki</ind></trd>,
    <trd>szorty</trd>,
    <trd>gatki <klr>(pot.)</klr></trd>,
    <trd>bermudy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bermuda</trd>,
    <trd>cal&ccedil;&atilde;o</trd>,
    <trd>short</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_o;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krátke nohavice</trd>,
    <trd>nohavičky</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>kortbyxor</trd>,
    <trd>shorts</trd>
  </trdgrp>
</drv>

<drv mrk="pantal.plagx0">
  <kap>pla&gcirc;<tld/>o<fnt><bib>EBV</bib></fnt></kap>
  <snc mrk="pantal.plagx0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="pantal.0o.TEKS"><tld lit="P"/>o</ref>
      kun striktaj krurumoj atingantaj &gcirc;is meze de suroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="kulot.0o">kuloto</ref>
    </refgrp>
  </snc>
  <trd lng="fr">pantacourt</trd>
</drv>

<drv mrk="pantal.puf0o">
  <kap>puf<tld/>o</kap>
  <snc mrk="pantal.puf0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="pantal.0o.TEKS"><tld lit="P"/>o</ref>
      pufa, lar&gcirc;a precipe &ccirc;irka&ubreve; la posta&jcirc;o:
      <ekz>
        dum kelkaj jaroj puf<tld/>oj estis la&ubreve;modaj, sed ili ne tre 
        pla&ccirc;is al ni
        <fnt>
          <aut>E. De Cooman</aut>,
          <vrk>Ordinara tago</vrk>,
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">široké kalhoty</trd>
  <trd lng="fr">pantalon bouffant</trd>
  <trd lng="sk">široké nohavice</trd>
</drv>

<drv mrk="pantal.sak0o">
  <kap>sak<tld/>o</kap>
  <snc mrk="pantal.sak0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="pantal.0o.TEKS"><tld lit="P"/>o</ref>
      penda sub la talio kiel sako:
      <ekz>
        li surhavis dis&scirc;iritan kaftanon kaj tataran sak<tld/>on
        <fnt>
          <aut>A. Pu&scirc;kin, trad. M. &Scirc;idlovskaja</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20puskin,%20aleksandr%20-%20la%20filino%20de%20la%20kapitano.pdf">
          La Kapitanfilino</url></vrk>,
          <lok>1927-2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu estis tiu virino, tiu turkino en mallonga filigranornamita 
        &jcirc;aketo kaj atlasa sak<tld/>o?
        <fnt>
          <aut>J. Jovkov, trad. A. Simeonov</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr71/gardistaro.html">
          Plej fidela gardistaro</url></vrk>,
          <lok>Nica Literatura Revuo, 1961-09 &gcirc;is 10, numero 37a, p.
          16-24a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">baggy</trd>
</drv>
</art>
