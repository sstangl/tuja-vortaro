<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: admira1.xml,v 1.19 2015/07/04 08:37:29 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>admiralitat</rad>/o
</kap>
<drv mrk="admira1.0o">
  <kap><tld/>o</kap>
  <snc mrk="admira1.0o.aro">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Aro de la <ref cel="admira.0o">admiraloj</ref> de iu regno:
      <ekz>
        la Lordoj de la <tld lit="A"/>o
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="admira1.0o.administracio">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      La administracio, kiu komandas mararmeon, kaj
      ties sidejo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_ib;&c_r;&c_a;&c_l;&c_ib;&c_t;&c_e;&c_t;</trd>
  <trd lng="ca">almirallat</trd>
  <trd lng="cs">admiralita</trd>
  <trd lng="de">Admiralit&auml;t</trd>
  <trd lng="en">admiralty</trd>
  <trd lng="fr">amiraut&eacute;</trd>
  <trd lng="hu">admiralit&aacute;s</trd>
  <trd lng="it">ammiragliato</trd>
  <trd lng="nl">admiraliteit</trd>
  <trd lng="pl">admiralicja</trd>
  <trd lng="pt">almirantado</trd>
  <trd lng="ru">&c_a;&c_d;&c_m;&c_i;&c_r;&c_a;&c_l;&c_t;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>admiralita <klr>(&uacute;rad alebo zamestnanci)</klr></trd>,
    <trd>admiralita <klr>(zamestnanci)</klr></trd>
  </trdgrp>
</drv>
</art>
