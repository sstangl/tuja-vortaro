<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eklog.xml,v 1.17 2015/07/04 08:37:33 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>eklog</rad>/o
</kap>
<drv mrk="eklog.0o">
  <kap><tld/>o</kap>
  <snc mrk="eklog.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="poem.0o.POE">Poemeto</ref>, en kiu oni
      paroligas pa&scirc;tistojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="idili.0o">idilio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_l;&c_o;&c_g;&c_a;</trd>
  <trd lng="ca">&egrave;gloga</trd>
  <trdgrp lng="cs">
    <trd>ekloga</trd>,
    <trd>pastýřská píseň</trd>,
    <trd>selanka</trd>
  </trdgrp>
  <trd lng="de">Ekloge</trd>
  <trd lng="en">eclogue</trd>
  <trd lng="es">&eacute;gloga</trd>
  <trd lng="fr">&eacute;glogue</trd>
  <trdgrp lng="hu">
    <trd>ekloga</trd>,
    <trd>p&aacute;sztork&ouml;ltem&eacute;ny</trd>
  </trdgrp>
  <trd lng="pl">ekloga</trd>
  <trdgrp lng="pt">
    <trd>&eacute;cloga</trd>,
    <trd>pastorela</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_l;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ekloga</trd>,
    <trd>pastierska pieseň</trd>
  </trdgrp>
</drv>
</art>
