<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: verben.xml,v 1.22 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>verben</rad>/o
</kap>
<drv mrk="verben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="verben1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Verbena</trd>), ornama bonodora vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_r;&c_b;&c_e;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_r;&c_b;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">berbena</trd>
  <trd lng="cs">sporýš</trd>
  <trd lng="de">Eisenkraut</trd>
  <trd lng="el">&jota;&epsilon;&rho;&omikron;&beta;&omikron;&tau;&alfa_ton;&ny;&eta;</trd>
  <trdgrp lng="en">
    <trd>verbena</trd>,
    <trd>vervain</trd>
  </trdgrp>
  <trd lng="fr">verveine</trd>
  <trdgrp lng="hu">
    <trd>vasf&udblac;</trd>,
    <trd>verb&eacute;na</trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ijzerkruid</trd>,
    <trd>verbena</trd>,
    <trd>ijzerhard</trd>
  </trdgrp>
  <trd lng="pt">verbena</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">železník</trd>
</drv>
</art>
