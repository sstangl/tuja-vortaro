<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: limf.xml,v 1.22 2013/04/01 01:10:12 revo Exp $">
<kap>
  <ofc>9</ofc><rad>limf</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="limf.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="limf.0o.ZOO">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flaveta a&ubreve; senkolora likva&jcirc;o konsistanta el akvo,
      <ref tip="prt" cel="plasm.0o">plasmo</ref>, proteinoj,
      blankaj sango&ccirc;eloj k.a., kiu cirkulas en
      <ref cel="angi.0o">angioj</ref> kaj nutras histojn.
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;</trd>
    <trd lng="ca">limfa</trd>
    <trd lng="de">Lymphe</trd>
    <trd lng="el">&lambda;&ypsilon_ton;&my;&phi;&eta;</trd>
    <trd lng="en">lymph</trd>
    <trd lng="es">linfa</trd>
    <trd lng="fr">lymphe</trd>
    <trd lng="hu">nyirok</trd>
    <trd lng="la">lympha</trd>
    <trd lng="nl">lymfevocht</trd>
    <trd lng="pl">limfa</trd>
    <trd lng="pt">linfa</trd>
    <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;</trd>
  </snc>
  <snc mrk="limf.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Akveca suko, cirkulanta en vegeta&jcirc;oj:
      <ekz>
        ci vidos la <tld/>on suprenrampi la&ubreve; la trunko
        de la plantoj
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">saba</trd>
    <trd lng="en">sap</trd>
    <trd lng="fr">s&egrave;ve</trd>
    <trd lng="pt">seiva</trd>
  </snc>
  <trd lng="it">linfa</trd>
</drv>

<drv mrk="limf.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o a&ubreve; enhavanta <tld/>on.
    </dif>
    <trd lng="ca">limf&agrave;tic</trd>
    <trd lng="de">lymphisch</trd>
    <trd lng="en">lymphatic</trd>
    <trdgrp lng="hu">
      <trd>nyirok-</trd>,
      <trd>limfatikus</trd>,
      <trd>limfo-</trd>
    </trdgrp>
    <trd lng="pl">limfatyczny</trd>
    <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="el">&lambda;&ypsilon;&my;&phi;&alfa;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">lymphatique</trd>
  <trd lng="it">linfatico</trd>
  <trd lng="pt">linf&aacute;tico</trd>
</drv>

<drv mrk="limf.0aangio">
  <kap><tld/>a angio</kap>
  <snc mrk="limf.0aangio.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organa tubo, <ref tip="super" cel="angi.0o">angio</ref> en kiu
      cirkulas <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">vas limf&agrave;tic</trd>
  <trd lng="de">Lymphgef&auml;&szlig;</trd>
  <trd lng="es">vaso linf&aacute;tico</trd>
  <trd lng="fr">vaisseau lymphatique</trd>
  <trd lng="hu">nyirok&eacute;r</trd>
  <trd lng="it"><ind>vaso</ind> linfatico</trd>
  <trd lng="nl">lymfevaten</trd>
  <trd lng="pl">naczynie limfatyczne</trd>
  <trd lng="pt">vaso linf&aacute;tico</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_s;&c_o;&c_s;&c_u;&c_d;</trd>
</drv>

<drv mrk="limf.0a.vazo">
  <kap><tld/>a vazo</kap>
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="limf.0aangio"><tld/>a angio</ref>
</drv>

<drv mrk="limf.0onodo">
  <kap><tld/>onodo</kap>
  <snc mrk="limf.0onodo.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;e la mamuloj kaj birdoj, &ccirc;iu el la molaj tuberetoj
      grupe situantaj en certaj lokoj la&ubreve;longe de la
      <tld/>aj tuboj:
      <ekz>
        diki&gcirc;o de unu flanko de la kolo estas ka&ubreve;zita de
        &scirc;veli&gcirc;o de <tld/>onodoj a&ubreve; dentodoloro
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://es.chinabroadcast.cn/1/2005/03/18/1@22350.htm">
          Fruaj simptomoj de malsanoj</url></vrk>, 
          <lok>2005-03-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;&c_v;&c_u;&c_z;&c_je;&c_l;</trd>
  <trd lng="ca">gangli limf&agrave;tic</trd>
  <trd lng="de">Lymphknoten</trd>
  <trd lng="es">ganglio linf&aacute;tico</trd>
  <trd lng="fr">ganglion lymphatique</trd>
  <trd lng="hu">nyirokcsom&oacute;</trd>
  <trd lng="it">linfonodo</trd>
  <trd lng="la">nodus lymphaticus</trd>
  <trd lng="nl">lymfeknoop</trd>
  <trd lng="pl">gruczo&lstroke; limfatyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_m;&c_f;&c_o;&c_u;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_l;&c_i;&c_m;&c_f;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_u;&c_z;&c_je;&c_l;</trd>
  </trdgrp>
</drv>
</art>
