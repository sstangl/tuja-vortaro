<vortaro>
<art mrk="$Id: fusten.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fusten</rad>/o
</kap>
<drv mrk="fusten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtof.0o.TEKS">&Scirc;tofo</ref> el kotono kaj
      lino
      a&ubreve; kanabo iam uzata por subvestoj:
      <ekz>
        unu viro proksimi&gcirc;is al Sancho, &jcirc;etis sur lin robon el nigra
        <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 69a, p.
        783a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_n;</trd>
  <trd lng="cs">barchet</trd>
  <trd lng="de">Barchent</trd>
  <trd lng="en">fustian</trd>
  <trd lng="fa">&a_f;&a_A;&a_s;&a_t;&a_w;&a_n;&f_y;</trd>
  <trd lng="fr">futaine</trd>
  <trd lng="hu">barhent</trd>
  <trd lng="nl">fustein</trd>
  <trd lng="pt">fust&atilde;o</trd>
  <trd lng="ru">&c_b;&c_u;&c_m;&c_a;&c_z;&c_je;&c_ja;</trd>
  <trd lng="sk">barchet</trd>
</drv>
</art>
