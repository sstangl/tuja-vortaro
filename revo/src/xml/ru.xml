<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ru.xml,v 1.11 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>ru</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Maceri tekseblan planton en likva&jcirc;o por apartigi &gcirc;iajn fibrojn de
      aliaj substancoj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>močit <klr>(len, konopí)</klr></trd>,
    <trd>rosit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&ouml;sten</trd>,
    <trd>rotten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ret</trd>,
    <trd>steep</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ztat <klr tip="amb">(pl. kendert)</klr></trd>
  <trd lng="nl">roten</trd>
  <trdgrp lng="pl">
    <trd>rosi&cacute;</trd>,
    <trd>moczy&cacute; <klr>(len, konopie)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_jo;&c_n;, &c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ju;)</klr></trd>
  <trdgrp lng="sk">
    <trd>močiť</trd>,
    <trd>rosiť <klr>(ľan, konope)</klr></trd>
  </trdgrp>
</drv>
</art>
