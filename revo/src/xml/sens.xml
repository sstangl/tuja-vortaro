<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sens.xml,v 1.14 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>sens</rad>/o
</kap>
<drv mrk="sens.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <ref tip="dif" cel="sent.0umo">Sentumo</ref>
    <ekz>
      voja&gcirc;oj instigas anka&ubreve; ordinarajn homojn al pli intensa
      perceptado
      de la okaza&jcirc;oj, des pli poetojn, homojn kun viglaj, fajnaj
      <tld/>oj<fnt><aut>Franz-Georg R&ouml;ssler</aut>: <vrk>Profite voja&gcirc;u
      kun
      Edwin</vrk>, <bib>Monato</bib><lok>jaro 1994a, numero 6a, p.
      21a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>schopnost vnímat</trd>,
    <trd>smysl <klr>(vnímací)</klr></trd>
  </trdgrp>
  <trd lng="de">Sinn</trd>
  <trd lng="en">sense</trd>
  <trdgrp lng="fr">
    <trd>sens<klr> (perceptif)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;rz&eacute;kszerv</trd>
  <trd lng="it">senso<klr> (percezione)</klr></trd>
  <trd lng="nl">zintuig</trd>
  <trd lng="ru">&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o; <klr tip="amb">(&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;
  &c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_ja;:
  &c_s;&c_l;&c_u;&c_h;, &c_z;&c_r;&c_je;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>zmyslový orgán</trd>,
    <trd>ústroj</trd>
  </trdgrp>
</drv>

<drv mrk="sens.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La kvin sentumoj.
    </dif>
  </snc>
  <trd lng="cs">vnímací smysly</trd>
  <trdgrp lng="fr">
    <trd>les cinq <ind>sens</ind></trd>
  </trdgrp>
  <trd lng="hu">az &ouml;t <ind>&eacute;rz&eacute;kszerv</ind></trd>
  <trd lng="it">i cinque <ind>sensi</ind></trd>
  <trd lng="nl">zintuigen</trd>
  <trd lng="sk">vnímacie zmysly</trd>
</drv>

<drv mrk="sens.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <ref tip="dif" cel="sent.0umajxo">Sentuma&jcirc;o</ref>
    <trd lng="nl">zintuiglijke <ind>waarneming</ind></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>sensation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;kel&eacute;s</trd>,
    <trd>&eacute;rz&eacute;klet</trd>
  </trdgrp>
  <trd lng="it">sensazione</trd>
</drv>

<drv mrk="sens.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <ref tip="dif" cel="sent.0ilo.TEK">sentilo <sncref/></ref>
    <ekz>
      malgrandaj <tld/>iloj kontrolos la staton de paciento dum kirurgia
      operacio<fnt><aut>Gianfranco Cazzaro</aut>: <vrk>Mikroskope vastaj
      horizontoj</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a, p.
      12a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>buňka příjmající vjemy</trd>,
    <trd>senzila</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Geberger&auml;t</trd>;
      <trd>F&uuml;hler</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>sensor</trd>, <trd>sensing device</trd>, <trd>pickup</trd>,
      <trd>transducer</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>capteur</trd>,
    <trd>senseur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;kel&odblac;</trd>,
    <trd>szenzor</trd>
  </trdgrp>
  <trd lng="it">sensore</trd>
  <trd lng="pl">czujnik</trd>
  <trdgrp lng="ru">
      <trd>&c_d;&c_a;&c_t;&c_ch;&c_i;&c_k;</trd>;
      <trd>&c_shch;&c_u;&c_p;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>bunka prijímajúca vnemy</trd>,
    <trd>senzila</trd>
  </trdgrp>
</drv>

<drv mrk="sens.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu &ccirc;ia kono venas de la <tld/>a&jcirc;oj kaj nur de
      ili.
    </dif>
    <refgrp tip="sin">
      <ref cel="sensua.0o">sensualismo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="empiri.0ismo">empiriismo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">senzualizmus</trd>
  <trd lng="de">Sensualismus</trd>
  <trdgrp lng="fr">
    <trd>sensualisme<klr> (philosophique)</klr></trd>
  </trdgrp>
  <trd lng="hu">szenzualizmus</trd>
  <trdgrp lng="it">
    <trd>sensismo<klr> (fil.)</klr></trd>, 
    <trd>sensualismo<klr> (fil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">sensualisme</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_s;&c_u;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">senzualizmus</trd>
</drv>
</art>
