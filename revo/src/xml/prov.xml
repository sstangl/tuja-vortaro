      <ref cel="edz.prov0igxo"><tld/>edzi&gcirc;o</ref>,
      <ref cel="ekzemp1.prov0o"><tld/>ekzemplero</ref>,
      <ref cel="jar.prov0o"><tld/>jaro</ref>,
      <ref cel="leg.prov0i"><tld/>legi</ref>,
      <ref cel="lud.prov0i"><tld/>ludi</ref>,
      <ref cel="numer.prov0o"><tld/>numero</ref>,
      <ref cel="temp.prov0o"><tld/>tempo</ref>,
      <ref cel="tub.prov0o"><tld/>tubo</ref>
    </refgrp>
  </ekz>
</rim>
