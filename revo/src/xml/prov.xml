      <trd>sp&eacute;cimen<klr tip="ind"> (de ce qu'on peut faire)
      </klr></trd>
    </trdgrp>
    <trd lng="hu">pr&oacute;badarab</trd>
    <trd lng="nl">proefstuk</trd>
  <trdgrp lng="pl">
    <trd>pr&oacute;bka</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_a;</trd>
  </snc>
  <snc mrk="prov.0ajxo.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="dif" cel="prov.pres0ajxo">pres<tld/>a&jcirc;o</ref>
