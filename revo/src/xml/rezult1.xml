<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rezult1.xml,v 1.17 2015/08/21 13:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rezultat</rad>/o
</kap>
<drv mrk="rezult1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fina sekvo de ia agado:
      <ekz>
        doni, alporti bonan <tld/>on;
      </ekz>
      <ekz>
        akiri, ricevi, trafi <tld/>on;
      </ekz>
      <ekz>
        brila, ri&ccirc;a <tld/>o;
      </ekz>
      <ekz>
        sed ni ne povis atingi ian <tld/>on kaj ni disiris
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 42</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezult.0o">rezulto</ref>
    <refgrp tip="vid">
      <ref cel="frukt.0o">frukto</ref>,
      <ref cel="produk">produkto</ref>,
      <ref cel="rezult.0o">rezulto</ref>,
      <ref cel="efik.0o">efiko</ref>,
      <ref cel="ating">atinga&jcirc;o</ref>,
      <ref cel="bilanc.0o">bilanco</ref>,
      <ref cel="rikolt">rikolto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>rezult&aacute;t</trd>,
    <trd>v&yacute;sledek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ergebnis</trd>,
    <trd>Resultat</trd>
  </trdgrp>
  <trd lng="en">result</trd>
  <trd lng="fa">&a_n;&a_t;&f_y;&a_j;&a_h;</trd>
  <trd lng="fr">r&eacute;sultat</trd>
  <trdgrp lng="hu">
    <trd>eredm&eacute;ny</trd>,
    <trd>k&ouml;vetkezm&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>resultaat</trd>,
    <trd>uitkomst</trd>
  </trdgrp>
  <trd lng="pt">resultado</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>rezult&aacute;t</trd>,
    <trd>v&yacute;sledok</trd>
  </trdgrp>
</drv>

<drv mrk="rezult1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sekvi <tld/>e el io.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">result</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_n;&a_t;&a_j; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_n;&a_A;&a_sh;&f_y; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;sulter</trd>
  <trdgrp lng="hu">
    <trd>ad&oacute;dik</trd>,
    <trd>kij&ouml;n <klr tip="ind">(eredm&eacute;nyk&eacute;nt)</klr></trd>,
    <trd>k&ouml;vetkezik</trd>,
    <trd>sz&aacute;rmazik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_o;&c_m;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="rezult1.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Sekvigi kiel <tld/>on
    </dif>
    <refgrp tip="vid">
      <ref cel="kauxz.0i">ka&ubreve;zi</ref>,
      <ref cel="vek.0i">veki</ref>,
      <ref cel="instig.0i">instigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredm&eacute;nyez</trd>,
    <trd>okoz</trd>,
    <trd>hoz <klr tip="ind">(eredm&eacute;ny&uuml;l)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; <klr>(&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_u;,
    &c_s;&c_l;&c_je;&c_d;&c_s;&c_t;&c_v;&c_i;&c_ju;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rezult1.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Senutila, vana:
      <ekz>
        sen<tld/>a peno
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vant.0a">vanta</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="cel.sen0a">sencela</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_v;&c_y;&c_n;&c_ib;&c_k;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&zwnj;&a_n;&a_t;&f_y;&a_j;&a_h;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_th;&a_m;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredm&eacute;nytelen</trd>,
    <trd>medd&odblac;</trd>,
    <trd>hi&aacute;baval&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
