<vortaro>
<art mrk="$Id: endote.xml,v 1.11 2011/12/06 15:10:16 revo Exp $">
<kap>
  <rad>endoterm</rad>/a
</kap>
<drv mrk="endote.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
    Karakterizanta &hcirc;emian reakcion, &ccirc;e kiu la varmo estas
    konsumata. Kp. <ref tip="vid" cel="ekzoterm.0a">ekzoterma</ref>. 
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_n;&c_d;&c_a;&c_t;&c_e;&c_r;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">endot&egrave;rmic</trd>
  <trdgrp lng="de">
     <trd>endotherm</trd>,
     <trd>endothermisch</trd>,
     <trd>unter W&auml;rmezufuhr</trd>,
     <trd>W&auml;rme bindend<klr> [evitu]</klr></trd>,
     <trd>W&auml;rme verbrauchend<klr> [evitu]</klr></trd>
  </trdgrp>
  <trd lng="fr">endotherme</trd>
  <trd lng="pl">endotermiczny</trd>
  <trd lng="pt">endot&eacute;rmico</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_d;&c_o;&c_t;&c_je;&c_r;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_o;&c_shch;&c_a;&c_ju;&c_shch;&c_i;&c_j;
    &c_t;&c_je;&c_p;&c_l;&c_o;&c_t;&c_u;</trd>
  </trdgrp>
</drv>
</art>
