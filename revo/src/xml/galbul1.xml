  <trd lng="nl">glansvogels<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">galbul&iacute;deos</trd>
  <trd lng="ru">&c_zh;&c_a;&c_k;&c_a;&c_m;&c_a;&c_r;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
