<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pedali.xml,v 1.11 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pedaliac</rad>/oj
</kap>
<drv mrk="pedali.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Pedaliaceae</trd>), ekz.
      <ref tip="sub" cel="sezam.0o">sezamo</ref>.
    </dif>
  </snc>
  <trd lng="ca">pedali&agrave;cies</trd>
  <trd lng="cs">sezamovité <klr>(čeleď)</klr></trd>
  <trdgrp lng="de">
    <trd>Sesamgew&auml;chse</trd>,
    <trd>Pedaliengew&auml;chse</trd>
  </trdgrp>
  <trd lng="hu">szez&aacute;mf&eacute;l&eacute;k</trd>
  <trd lng="nl">sesamzaadfamilie</trd>
  <trd lng="pt">pedali&aacute;ceas</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_z;&c_a;&c_m;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_k;&c_u;&c_n;&c_zh;&c_u;&c_t;&c_n;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">sézamovité</trd>
</drv>
</art>
