<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolone.xml,v 1.18 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kolonel</rad>/o
</kap>
<drv mrk="kolone.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo, 
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas inter
      <ref cel="kolone.sub0o">sub<tld/>o</ref> kaj 
      <ref cel="genera1.0o">generalo</ref>:
      <ekz>
        regimentestro estas <tld/>a ofico.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kolonel</trd>,
    <trd>plukovník</trd>
  </trdgrp>
  <trd lng="de">Oberst</trd>
  <trd lng="en">colonel</trd>
  <trd lng="fr">colonel</trd>
  <trd lng="hu">ezredes</trd>
  <trd lng="it">colonnello</trd>
  <trd lng="nl">kolonel</trd>
  <trd lng="pt">coronel</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">plukovník</trd>
</drv>

<drv mrk="kolone.sub0o">
  <kap>sub<tld/>o</kap>
    <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo, 
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas inter
      <ref cel="major.0o">majoro</ref> kaj
      <ref cel="kolone.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">
&c_p;&c_a;&c_d;&c_p;&c_a;&c_l;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">podplukovník</trd>
  <trd lng="en">lieutenant-colonel</trd>
  <trd lng="fr">lieutenant-colonel</trd>
  <trd lng="hu">alezredes</trd>
  <trd lng="it">tenente colonnello</trd>
  <trd lng="nl">luitenant-kolonel</trd>
  <trd lng="ru">
&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">podplukovník</trd>
</drv>
</art>
