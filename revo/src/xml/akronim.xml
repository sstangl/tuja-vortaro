<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akronim.xml,v 1.1 2013/07/24 01:10:14 revo Exp $">
<kap>
  <rad>akronim</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="akronim.0o">
  <kap><tld/>o</kap>
  <snc mrk="akronim.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="super" cel="long.mal0igo.GRA">Mallongigo</ref>
      farita el la komencoj de sinsekvaj vortoj:
      <ekz>
        Pekoteko estas akronima mallongigo por
        <ctl>Perkomputora Termino-Kolekto</ctl>
        <fnt>
          <aut>R. Eichholz</aut>,
          <vrk><url
          ref="http://www.familienforschung-pabst.de/EspBiographien/PP-pdf/11paroletoj.pdf">
          Dekunu Paroladetoj pri Pekoteko</url></vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">acronym</trd>
  <trd lng="fr">acronyme</trd>
</drv>
</art>

