<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: imun.xml,v 1.19 2015/10/14 05:10:16 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>imun</rad>/a
</kap>
<drv mrk="imun.0a">
  <kap><tld/>a</kap>
  <snc mrk="imun.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tute &scirc;irmita kontra&ubreve; infekta malsano.
    </dif>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_w;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y; <klr tip="amb">(&c_d;&c_a;
      &c_ib;&c_n;&c_f;&c_e;&c_k;&c_c;&c_y;&c_ib;, &c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_y;)</klr></trd>
    </trdgrp>
    <trd lng="de">immun</trd>
    <trd lng="en">immune</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;dett<klr tip="ind"> (betegs&eacute;ggel szemben)</klr></trd>,
      <trd>ellen&aacute;ll&oacute;k&eacute;pes<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>immunis</trd>
    </trdgrp>
    <trd lng="nl">immuun</trd>
    <trd lng="pl">odporny</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_i;&c_m;&c_ch;&c_i;&c_v;&c_y;&c_j; (&c_k; &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_i;)</trd>,
      <trd>&c_i;&c_m;&c_u;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">immun</trd>
  </snc>
  <snc mrk="imun.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ne respondeca anta&ubreve; la le&gcirc;o, &scirc;irmita per le&gcirc;a privilegio.
    </dif>
    <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">immun</trd>
    <trd lng="en">exempt</trd>
    <trdgrp lng="hu">
      <trd>mentes<klr tip="ind">(mentess&eacute;get &eacute;lvez&odblac;)</klr></trd>,
      <trd>mentess&eacute;get &eacute;lvez&odblac;</trd>
    </trdgrp>
    <trd lng="nl">onschendbaar</trd>
    <trdgrp lng="pl">
      <trd>nietykalny</trd>,
      <trd>korzystaj&aogonek;cy z <ind>immunitetu</ind></trd>,
      <trd>korzystaj&aogonek;cy z <ind>listu &zdot;elaznego</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_p;&c_r;&c_i;&c_k;&c_o;&c_s;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>chr&aacute;n&ecaron;n&yacute;</trd>,
    <trd>imunn&iacute;</trd>
  </trdgrp>
  <trd lng="es">inmune</trd>
  <trdgrp lng="fr">
    <trd>immun</trd>,
    <trd>immuniser</trd>,
    <trd>jouissant de l'<ind>immunit&eacute;</ind></trd>
  </trdgrp>
  <trd lng="pt">imune</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>chr&aacute;nen&yacute;</trd>,
    <trd>im&uacute;nny</trd>
  </trdgrp>
</drv>

<drv mrk="imun.0eco">
  <kap><tld/>eco</kap>
  <dif>
    Stato de iu <tld/>a:
  </dif>
  <snc mrk="imun.0eco.MED">
    <ekz>
      <uzo tip="fak">MED</uzo>
      <tld/>eco kontra&ubreve; tifoida febro povus esti akirita per serumo.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_e;&c_t;</trd>,
      <trd>&c_n;&c_je;&c_w;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Immunit&auml;t</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;detts&eacute;g<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>ellen&aacute;ll&oacute;k&eacute;pess&eacute;g<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>immunit&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">immuniteit</trd>
    <trd lng="pl">odporno&sacute;&cacute;</trd>
  </snc>
  <snc mrk="imun.0eco.JUR">
    <ekz>
      <uzo tip="fak">JUR</uzo>
      <tld/>eco de la parlamentanoj.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_e;&c_t;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Immunit&auml;t</trd>
    <trd lng="hu">mentess&eacute;g</trd>
    <trd lng="nl">onschendbaarheid</trd>
    <trdgrp lng="pl">
      <trd>nietykalno&sacute;&cacute;</trd>,
      <trd>immunitet</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_p;&c_r;&c_i;&c_k;&c_o;&c_s;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>imunita</trd>,
    <trd>imunnost</trd>,
    <trd>nedotknutelnost</trd>
  </trdgrp>
  <trd lng="en">immunity</trd>
  <trd lng="es">inmunidad</trd>
  <trd lng="fr">immunit&eacute;</trd>
  <trd lng="pt">imunidade</trd>
  <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>imunita</trd>,
    <trd>nedotknute&lcaron;nos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="imun.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="imun.0igi.MED">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <uzo tip="fak">MED</uzo>
        vakcino <tld/>igas kontra&ubreve; variolo.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">immunisieren</trd>
    <trd lng="en">immunize</trd>
    <trd lng="fr">immuniser</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;dett&eacute; tesz</trd>,
      <trd>immuniss&aacute; tesz</trd>,
      <trd>immuniz&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">immuniseren</trd>
    <trd lng="pl">uodparnia&cacute;</trd>
    <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="es">inmunizar</trd>
  <trd lng="pt">imunizar</trd>
</drv>
<adm>
  Enkonduki drv-ojn de la tipo rust~a kaj la
  sencovastigon de ~a, kiun tio implicas.
  <aut>MB</aut>
</adm>
</art>
