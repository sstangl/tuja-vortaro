<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: angar.xml,v 1.9 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>Angar</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>

<drv mrk="angar.A0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref> sudokcidente en la Orienta
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>,
      1779 km longa, la dekstra kaj la plej akvoplena alfluanto de
      <ref cel="jenisej.J0o">Jenisejo</ref>, fluanta el
      <ref cel="bajkal.B0o">Bajkalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_g;&c_a;&c_r;&c_a;</trd>
  <trd lng="cs">Angara</trd>
  <trd lng="de">Angara</trd>
  <trd lng="en">Angara</trd>
  <trd lng="fr">Angara</trd>
  <trd lng="ru">&c_A;&c_n;&c_g;&c_a;&c_r;&c_a;</trd>
  <trd lng="sk">Angara</trd>
  <trd lng="sv">Angara</trd>
</drv>
</art>
