<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lutr.xml,v 1.24 2015/06/29 16:33:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lutr</rad>/o
</kap>
<drv mrk="lutr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lutr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda <ref tip="lst" cel="best.rabo0o" 
      lst="voko:rabobestoj">rabobesto</ref> kun
      na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta &ccirc;e akvo kaj man&gcirc;anta
      fi&scirc;ojn;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Lutra</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="br">
    <trd>dourgi</trd>,
    <trd>ki-dour</trd>
  </trdgrp>
  <trd lng="ca">ll&uacute;dria</trd>
  <trd lng="cs">vydra</trd>
  <trd lng="de">Otter</trd>
  <trd lng="en">otter</trd>
  <trd lng="fr">loutre</trd>
  <trd lng="hu">vidra</trd>
  <trd lng="id">berang-berang</trd>
  <trd lng="it">lontra</trd>
  <trd lng="nl">otter</trd>
  <trd lng="pl">wydra</trd>
  <trd lng="pt">lontra</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_r;&c_a;</trd>
  <trd lng="sk">vydra</trd>
</drv>
</art>
