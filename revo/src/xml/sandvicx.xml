<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sandvicx.xml,v 1.8 2014/01/17 21:10:20 revo Exp $">
<kap>
  <rad>sandvi&ccirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sandvicx.0o">
  <kap><tld/>o</kap>
  <snc mrk="sandvicx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Rapidman&gcirc;a&jcirc;o el pano entran&ccirc;ita a&ubreve;
      disduigita, far&ccirc;ita per
      legomoj,
      viandoj a&ubreve; sa&ubreve;coj:
      <ekz>
        mi nur frandis ian kokinan <tld/>on
        <fnt>
        <aut>knaboj ne ploras</aut>,
        <vrk><url
        ref="http://www.livejournal.com/users/knaboj_neploras/">[blogo]</url></vrk>,
        <lok>Soleca tago, 2004-05-14</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_a;&c_n;&c_d;&c_v;&c_i;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Sandwich</trd>,
    <trd>Schnitte</trd>
  </trdgrp>
  <trd lng="en">sandwich</trd>
  <trd lng="fr">sandwich</trd>
  <trd lng="hu">szendvics</trd>
  <trdgrp lng="it">
   <trd><ind>panino</ind> imbottito</trd>, 
   <trd>sandwich</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sandwich</trd>,
    <trd>belegd broodje</trd>,
    <trd>belegde boterham</trd>,
    <trd>boterham</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>sandu&iacute;che</trd>
  </trdgrp>
</drv>
</art>

