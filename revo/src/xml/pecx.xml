<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pecx.xml,v 1.15 2013/09/04 01:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pe&ccirc;</rad>/o
</kap>
<drv mrk="pecx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pecx.0o.KOMUNE">
    <dif>
      Nigra glueca substanco ekstraktata el rezino:
      <ekz>
        en la aero estis disver&scirc;ita aromo <tld/>a de la betula foliaro 
        kaj mielodora de kampaj floroj
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 36a</lok></fnt>.
      </ekz>
    </dif>
  </snc>                    
  <trd lng="be">&c_s;&c_m;&c_a;&c_l;&c_a;</trd>
  <trd lng="br">peg</trd>
  <trd lng="de">Pech</trd>
  <trd lng="en">pitch</trd>
  <trd lng="es">pez <klr tip="amb">(tipo de resina)</klr></trd> 
  <trd lng="fr">poix</trd>
  <trdgrp lng="hu">
    <trd>szurok</trd>,
    <trd>k&aacute;tr&aacute;ny</trd>
  </trdgrp>
  <trd lng="nl">pek</trd>
  <trdgrp lng="pt">
    <trd>pez</trd>,
    <trd>breu</trd>,
    <trd>piche</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_a;</trd>
</drv>

<drv mrk="pecx.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="pecx.ter0o.asfalto">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="asfalt.0o">asfalto</ref>
  </snc>                         
  <trd lng="be">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="br">asfalt</trd>
  <trd lng="de">Asphalt</trd>
  <trd lng="en">asphalt</trd> 
  <trd lng="es">asfalto</trd>
  <trd lng="fr">asphalte</trd>
  <trd lng="hu">aszfalt</trd>  
  <trdgrp lng="nl">
    <trd>asfalt</trd>,
    <trd>aardpek</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_f;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">asfalt</trd>
  <trd lng="sv">asfalt</trd>
</drv>
</art>
