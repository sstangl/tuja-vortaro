<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tein.xml,v 1.14 2015/11/19 15:11:07 revo Exp $">
<kap>
   <rad>tein</rad>/o
</kap>
<drv mrk="tein.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="alkalo.0o">Alkaloido</ref> ekstraktita el
       <ref tip="vid" cel="te.0o">teo</ref>, &hcirc;emie identa al
       <ref tip="sin" cel="kafein.0o">kafeino</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_e;&c_ib;&c_n;</trd>
  <trd lng="ca">te&iuml;na</trd>
  <trdgrp lng="cs">
    <trd>kofein</trd>,
    <trd>tein</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Teein</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>theine</trd>,
     <trd>caffeine</trd>
   </trdgrp>
  <trd lng="es">te&iacute;na</trd>
  <trd lng="fa">&a_t;&a_y_hamza;&f_y;&a_n;</trd>
  <trd lng="fr">th&eacute;ine</trd>
  <trd lng="hu">tein</trd>
  <trd lng="nl">the&iuml;ne</trd>
  <trd lng="pl">teina</trd>
  <trdgrp lng="pt">
    <trd>te&iacute;na</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>kofe&iacute;n</trd>,
    <trd>te&iacute;n</trd>
  </trdgrp>
</drv>
</art>
