<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nomes.xml,v 1.9 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>nomes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="nomes.0o">
  <kap><tld/>o</kap>
  <snc mrk="nomes.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="sin" cel="provin.0o.POL">Provinco</ref> en antikva
      Egiptujo:
      <ekz>
        la princo <klr>...</klr> foriris en la &ccirc;efurbon de la <tld/>o
        Sohem
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">nom&oacute;s</trd>
  <trd lng="cs">nomarchie</trd>
  <trd lng="de">Gau<klr tip="amb"> (&Auml;gypten)</klr></trd>
  <trd lng="fr">nome<klr tip="amb"> (&Eacute;gypte ancienne)</klr></trd>
  <trd lng="hu">nomosz</trd>
  <trd lng="nl">gouw<klr tip="ind"> (oude Egypte)</klr></trd>
  <trdgrp lng="pt">
    <trd>nomo</trd>,
    <trd>nomarquia</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_o;&c_m;</trd>
  <trd lng="sk">nomarchia</trd>
</drv>
</art>

