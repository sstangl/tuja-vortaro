<vortaro>
<art mrk="$Id: harmon3.xml,v 1.17 2014/10/26 02:10:25 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>harmonium</rad>/o
</kap>
<drv mrk="harmon3.0o">
  <kap><tld/>o</kap>
  <snc mrk="harmon3.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Klavara orgensimila <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref> sen tuboj sed
      kun
      liberaj
      an&ccirc;oj, kiujn piede premataj balgoj vibrigas.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="br">armolud</trd>
  <trd lng="de">Harmonium</trd>
  <trd lng="fr">harmonium</trd>
  <trd lng="hu">harm&oacute;nium</trd>
  <trd lng="id">harmonium</trd>
  <trd lng="it">armonium</trd>
  <trd lng="nl">harmonium</trd>
  <trdgrp lng="pt">
    <trd>harm&ocirc;nio</trd>,
    <trd>harm&ocirc;nico <klr tip="amb">(subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_f;&c_ib;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
</drv>
</art>
