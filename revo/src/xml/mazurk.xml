<vortaro>
<art mrk="$Id: mazurk.xml,v 1.20 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mazurk</rad>/o
</kap>
<drv mrk="mazurk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="mazurk.0o.danco">
    <dif>
      Para, pol-devena <ref tip="super" cel="danc.0o">danco</ref>,
      tri-takta kun dekses-mezuraj frazoj, populara en E&ubreve;ropo kaj
      Nord-ameriko precipe en la 18a kaj 19a jarcentoj:
      <ekz>
        Pa&ubreve;lo Petrovi&ccirc; <klr>...</klr> dancis kun &scirc;i <tld/>on, dum kiu
        &scirc;i
        ne diris e&ccirc; unu prudentan vorton
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 7a, p.
        31a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mazurk.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_u;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">mazurka</trd>
  <trd lng="ca">masurca</trd>
  <trdgrp lng="cs">
    <trd>mazurek</trd>,
    <trd>mazurka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mazurka</trd>,
    <trd>Masurka</trd>
  </trdgrp>
  <trd lng="en">mazurka</trd>
  <trd lng="es">mazurca</trd>
  <trd lng="fa">&a_m;&a_A;&a_z;&a_w;&a_r;&f_k;&a_A;</trd>
  <trd lng="fr">mazurka</trd>
  <trd lng="hu">mazurka</trd>
  <trd lng="nl">mazurka</trd>
  <trdgrp lng="pl">
    <trd>mazurek</trd>,
    <trd>mazur</trd>
  </trdgrp>
  <trd lng="pt">mazurca</trd>
  <trd lng="ru">&c_m;&c_a;&c_z;&c_u;&c_r;&c_k;&c_a;</trd>
  <trd lng="sk">maz√∫rka</trd>
  <trdgrp lng="sv">
    <trd>mazurka</trd>,
    <trd>masurka</trd>
  </trdgrp>
</drv>
</art>
