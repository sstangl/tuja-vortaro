<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mirt.xml,v 1.20 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mirt</rad>/o
</kap>
<drv mrk="mirt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="mirtac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Myrtus</trd>), arbeto kun &ccirc;iamverdaj ledecaj
      folioj kaj bonodoraj blankaj floretoj, dedi&ccirc;ita al la
      militherooj &ccirc;e la grekoj kaj al Venuso &ccirc;e la romanoj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_r;&c_t;</trd>
  <trd lng="ca">murta</trd>
  <trd lng="cs">myrta</trd>
  <trd lng="de">Myrte</trd>
  <trd lng="en">myrtle</trd>
  <trd lng="fr">myrte</trd>
  <trd lng="hu">mirtusz</trd>
  <trdgrp lng="nl">
    <trd>gagel</trd>,
    <trd>mirte</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>murta</trd>,
   <trd>mirta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_r;&c_t;</trd>,
    <trd>&c_m;&c_i;&c_r;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">myrta</trd>
</drv>
</art>
