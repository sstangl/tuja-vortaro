<kap>
  <rad>termodinamik</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="termodinamik.0o">
  <kap><tld/>o</kap>
  <snc mrk="termodinamik.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="fizik.0o.FIZ">fiziko</ref>, kiu 
      studas la energiajn kaj materiajn &scirc;an&gcirc;ojn en sistemoj:
      <ekz>
        klopodo por realigi eternemovan motoron naskis <tld/>on
        <fnt>
          <aut>Segilo</aut>,
          <vrk><url
          ref="http://segilo.cocolog-nifty.com/taglibro/">Taglibro</url></vrk>,
          <lok>2009-02-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_o;&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">termodin&agrave;mica</trd>
  <trdgrp lng="de">
    <trd>Thermodynamik</trd>,
    <trd>W&auml;rmelehre</trd>
  </trdgrp>
  <trd lng="fr">thermodynamique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>h&odblac;tan</trd>,
    <trd>termodinamika</trd>
  </trdgrp>
</drv>
</art>

