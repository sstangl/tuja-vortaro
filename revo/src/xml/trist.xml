<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: trist.xml,v 1.10 2015/07/04 08:37:45 revo Exp $">
<kap>
<rad>trist</rad>/a
</kap>
<drv mrk="trist.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malgaja.
    </dif>
    <ref tip="sin" cel="morn.0a">morna</ref>
  </snc>
  <trd lng="bg">&c_t;&c_malmol;&c_zh;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>trist</trd>,
    <trd>marrit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>smutný</trd>,
    <trd>smutný</trd>,
    <trd>trudný</trd>,
    <trd>zarmoucený</trd>,
    <trd>zasmušilý</trd>,
    <trd>žalostný</trd>
  </trdgrp>
  <trd lng="de">traurig</trd>
  <trd lng="en">sad</trd>
  <trd lng="fr">triste</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;natos</trd>,
    <trd>b&uacute;s</trd>
  </trdgrp>
  <trd lng="it">triste</trd>
  <trd lng="nl">triest</trd>
  <trd lng="no">trist</trd>
  <trd lng="pt">triste</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">smutný</trd>
</drv>
</art>

