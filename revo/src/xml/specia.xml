<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: specia.xml,v 1.21 2015/07/04 08:37:43 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>special</rad>/a
</kap>
<drv mrk="specia.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Destinita por iu aparta celo, ne&gcirc;enerala, nekomuna:
      <ekz>
	pri tiuj &ccirc;i ka&ubreve;zoj, kiel tro <tld/>aj,
	ni tie &ccirc;i detale ne parolos<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="gxener.0a">&gcirc;enerala</ref>
  </snc>
  <rim mrk="specia.0a.SIN">
    <refgrp tip="sin">
      <ref cel="apart.0a.speciala">aparta</ref>,
      <ref cel="fak">faka</ref>,
      <ref cel="precip.0a">precipa</ref>.
    </refgrp>
    <em>Speciala</em> montras destinon por difinita celo:
    <ekz>
      oni uzas por tio <tld/>an ma&scirc;inon;
    </ekz>
    <ekz>
      mi venis <tld/>e por la festo.
    </ekz>
    <em>Aparta</em> montras malkunecon, diferencon:
    <ekz>
      ni pritraktos la du aferojn aparte;
    </ekz>
    <ekz>
      li tenis sin aparte.
    </ekz>
    Ofte &gcirc;i montras elstarecon, eminentecon, distingindecon:
    <ekz>
      la lasta ero en la programo  estis aparte (precipe) &scirc;atinda.
    </ekz>
    <em>Precipa</em> rilatas al grado de kompara graveco:
    <ekz>
      mi atentigas vin precipe pri tio;
    </ekz>
    <ekz>
      mi &scirc;atis precipe lian lastan verkon;
    </ekz>
    <ekz>
      &ccirc;iuj devos iri, sed precipe la plej junaj.
    </ekz>
  </rim>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">especial</trd>
  <trd lng="de">speziell</trd>
  <trd lng="en">special</trd>
  <trd lng="es">especial</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&f_y;&f_zh;&a_h;</trd>,
    <trd>&a_m;&a_kh;&a_S;&a_w;&a_S;</trd>,
    <trd>&a_kh;&a_A;&a_S;</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;cial</trd>
  <trdgrp lng="hu">
    <trd>speci&aacute;lis</trd>,
    <trd>saj&aacute;tos</trd>,
    <trd>k&uuml;l&ouml;n</trd>
  </trdgrp>
  <trd lng="nl">speciaal</trd>
  <trdgrp lng="pl">
    <trd>specjalny</trd>,
    <trd>szczeg&oacute;lny</trd>,
    <trd>specjalistyczny</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>výberový</trd>,
    <trd>zvláštny</trd>,
    <trd>špeciálny</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>speciální</trd>,
    <trd>výběrový</trd>,
    <trd>zvláštní</trd>
  </trdgrp>
</drv>

<drv mrk="specia.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
	lingvo internacia apartenas <tld/>e al neniu nacio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">especialment</trd>
  <trdgrp lng="cs">
    <trd>obzvláště</trd>,
    <trd>především</trd>,
    <trd>zejména</trd>,
    <trd>zvláště</trd>
  </trdgrp>
  <trd lng="de">speziell</trd>
  <trd lng="es">especialmente</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_w;&f_y;&f_zh;&a_h;</trd>,
    <trd>&a_b;&a_kh;&a_S;&a_w;&a_S;</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;cialement</trd>
  <trdgrp lng="hu">
    <trd>speci&aacute;lisan</trd>,
    <trd>saj&aacute;tosan</trd>,
    <trd>k&uuml;l&ouml;n&ouml;sen</trd>
    <trd>k&uuml;l&ouml;n</trd>
  </trdgrp>
  <trd lng="nl">speciaal</trd>
  <trdgrp lng="pl">
    <trd>specjalnie</trd>,
    <trd>w spos&oacute;b <ind>specjalny</ind></trd>,
    <trd>szczeg&oacute;lnie</trd>,
    <trd>w szczeg&oacute;lno&sacute;ci</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>hlavne</trd>,
    <trd>obzvlášť</trd>
  </trdgrp>
</drv>

<drv mrk="specia.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Profesia <ref tip="super" cel="fak.0ulo">fakulo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="ca">especialista</trd>
  <trdgrp lng="cs">
    <trd>odborník</trd>,
    <trd>specialista</trd>
  </trdgrp>
  <trd lng="de">Spezialist</trd>
  <trd lng="en">specialist</trd>
  <trd lng="es">especialista</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_t;&a_kh;&a_S;&a_S;</trd>,
    <trd>&f_k;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_w;&f_y;&f_zh;&a_h;&zwnj;&f_k;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;cialiste</trd>
  <trdgrp lng="hu">
    <trd>szakember</trd>,
    <trd>specialista</trd>
  </trdgrp>
  <trd lng="nl">specialist</trd>
  <trd lng="pl">specjalista</trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">odborník</trd>
</drv>
</art>
