<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekret.xml,v 1.28 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sekret</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekret.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sekret.0o.KOMUNE">
    <dif>
      Io, kio restas a&ubreve; devas resti ka&scirc;ita kaj ne de &ccirc;iu
      ajn konata:
      <ekz>
        nenia peketo restas longe <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>teni ion en <tld/>o</ind><fnt>Z</fnt>
        <klr>(teni tion <tld/>a,
        <ref cel="sekret.0igi"><tld/>igi</ref> &gcirc;in)</klr>:
        <trdgrp lng="fr">
          <trd>tenir qc secret</trd>,
          <trd>celer qc</trd>
        </trdgrp>
        <trd lng="hu">titokban tart vmit</trd>
      </ekz>
      <ekz>
        teni fabrikprocedon en plej granda <tld/>o;
      </ekz>
      <ekz>
        <ind>gardi (la) <tld/>on pri io</ind> <klr>(teni tion
        <tld/>a)</klr>
        <fnt><bib>Revadoj</bib>, <lok>Kvara promenado</lok></fnt>;
        <trd lng="fr">garder le secret</trd>
        <trd lng="hu">meg&odblac;rzi a titkot</trd>
      </ekz>
      <ekz>
        <ind>(el)perfidi <tld/>on</ind><fnt>Z</fnt>,
        <trdgrp lng="fr">
          <trd>&eacute;venter un secret</trd>,
          <trd>trahir un secret</trd>
        </trdgrp>
        <trd lng="hu">titkot el&aacute;rul</trd>
      </ekz>
      <ekz>
        <ind>babili <tld/>on</ind><fnt>B</fnt>;
        <trdgrp lng="fr">
          <trd>&eacute;venter un secret</trd>,
          <trd>trahir un secret</trd>
        </trdgrp>
        <trd lng="hu">titkot kikotyog</trd>
      </ekz>
      <ekz>
        <ind>eltiri de iu la <tld/>on</ind><fnt>Z</fnt>.
        <trd lng="fr">arracher un secret &agrave; qn</trd>
        <trd lng="hu">titkot kih&uacute;z</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_k;&c_r;&c_e;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tajemství</trd>,
    <trd>tajnost</trd>
  </trdgrp>
  <trd lng="de">Geheimnis</trd>
  <trd lng="en">secret</trd>
  <trd lng="fr">secret<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">titok</trd>
  <trd lng="id">rahasia</trd>
  <trd lng="it">segreto<klr tip="ind"> (sost.)</klr></trd>
  <trd lng="nl">geheim</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tajnosť</trd>,
    <trd>tajomstvo</trd>
  </trdgrp>
  <trd lng="sv">hemlighet</trd>
</drv>

<drv mrk="sekret.0a">
  <kap><tld/>a</kap>
  <snc mrk="sekret.0a.KOMUNE">
    <dif>
      Restanta a&ubreve; devanta resti ka&scirc;ita:
      <ekz>
        <tld/>a donaco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vundo <tld/>a doloras plej multe
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a amo;
      </ekz>
      <ekz>
        liaj gepatroj nutris la <tld/>an esperon fari el li pastron
        <fnt><bib>UrdHadda</bib>, <lok>p. 9a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a pordo <klr>(kies ekzisto estas nesuspektebla
        por nekonanto)</klr>
        <fnt><bib>UrdHadda</bib>, <lok>p. 4a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i rememoris la principojn de tiu <tld/>a lingvo,
        kaj &scirc;i sukcesis kompreni &ccirc;ion
        <fnt><bib>Gerda</bib>, <lok>&ccirc;ap. 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="enigm.0a">enigma</ref>,
      <ref cel="mister.0a">mistera</ref>
    </refgrp>
  </snc>
  <adm>
    En "Lasu min paroli plu" Piron havas ankaux "~a servo". Samsence
    Pola Radio uzas "invigilservo".
    <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_k;&c_r;&c_e;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">geheim</trd>
  <trd lng="en">secret</trd>
  <trdgrp lng="fr">
    <trd>cach&eacute;</trd>,
    <trd>dissimul&eacute;<klr tip="ind"> (cach&eacute;)</klr></trd>,
    <trd>d&eacute;rob&eacute;<klr tip="ind"> (cach&eacute;)</klr></trd>,
    <trd>occulte</trd>,
    <trd>secret<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">titkos</trd>
  <trd lng="it">segreto<klr tip="ind"> (agg.)</klr></trd>
  <trd lng="nl">geheim</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">tajn&yacute;</trd>
  <trd lng="sv">hemlig</trd>
  <trd lng="sk">tajný, skrytý</trd>
  <trd lng="sk">tajný, skrytý</trd>
  <trdgrp lng="cs">
    <trd>skrytý</trd>,
    <trd>tajný</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0e">
  <kap><tld/>e</kap>
  <snc mrk="sekret.0e.KOMUNE">
    <dif>
      En <tld/>a maniero,
      <ref tip="vid" cel="kasx">ka&scirc;e</ref>:
      <ekz>
        vi <tld/>e vorton diros, &gcirc;i tra l' tuta mondo iros
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plank.0o">planko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_k;&c_r;&c_e;&c_t;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_je;&c_m;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potají</trd>,
    <trd>tajně</trd>
  </trdgrp>
  <trd lng="de">geheim</trd>
  <trd lng="en">secretly</trd>
  <trdgrp lng="fr">
    <trd>en <ind>cachette</ind></trd>,
    <trd>&agrave; la <ind>d&eacute;rob&eacute;e</ind></trd>,
    <trd>secr&egrave;tement</trd>,
    <trd>en <ind>secret</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>titokban</trd>,
    <trd>titkosan</trd>
  </trdgrp>
  <trd lng="it">segretamente</trd>
  <trdgrp lng="nl">
    <trd>heimelijk</trd>,
    <trd>in het <ind>geheim</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_o; <ind>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_u;</ind></trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_t;&c_a;&c_j;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>potajomky</trd>,
    <trd>tajne</trd>
  </trdgrp>
  <trd lng="sv">i <ind>hemlighet</ind></trd>
</drv>

<drv mrk="sekret.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="sekret.0i.prisekreti">
    <dif>
      Gardi <tld/>on pri io:
      <ekz>
        pri kio amikoj <tld/>as, ili vian ju&gcirc;on ne petas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>avoir un secret</trd>,
      <trd>garder le secret</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>titkol&oacute;dzik</trd>,
      <trd>eltitkol</trd>
    </trdgrp>
    <trd lng="id">[<ind>rahasia</ind>] merahasiakan</trd>
    <trdgrp lng="it">
      <trd>avere un <ind>segreto</ind></trd>,
      <trd>conservare un <ind>segreto</ind></trd>
    </trdgrp>
    <trd lng="sv">h&aring;lla en <ind>hemlighet</ind></trd>
  </snc>
  <snc mrk="sekret.0i.konfidi">
    <ref tip="dif" cel="sekret.en0igi">en<tld/>igi</ref>
    <ekz>
      vi <tld/>os al edzino, &scirc;i <tld/>os al fratino,
      kaj tiel la <tld/>o promenados sen fino
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="fr">confier un secret</trd>
    <trd lng="it">confidare un <ind>segreto</ind></trd>
    <trd lng="sv">anf&ouml;rtro en <ind>hemlighet</ind></trd>
  </snc>
  <adm>
    Pro manko de ekzemploj la senco ne tre certas. La unua redakto (diri
    sekreton
    kun peto ne rediri) estas tro malvasta. &Ccirc;i tiu senco tamen aperas,
    kiam estas
    "al"-komplemento, sed &scirc;ajnas al mi, ke sekreti pri io ne implicas,
ke
    oni la
    sekreton dividas kun aliaj homoj (vd dif de ~ema en PIV2).
    Kontroli la tradukojn be, nl, ru, faritajn anta&ubreve; modifo de la
difino.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_s;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_d; <ind>&c_s;&c_a;&c_k;&c_r;&c_e;&c_t;&c_a;&c_m;</ind></trd>
  <trd lng="nl">toevertrouwen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_p;&c_o; <ind>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_u;</ind></trd>,
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">jednať tajne</trd>
  <trdgrp lng="cs">
    <trd>jednat tajně</trd>,
    <trd>tajnůstkařit</trd>,
    <trd>zamlčovat</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekret.0igi.KOMUNE">
    <dif>
      Igi, teni <tld/>a:
      <ekz>
        por nura <tld/>igo de mesa&gcirc;oj tiaj unudirektaj &ccirc;ifroj
estas ege
        utilaj
        <fnt>
        <aut>R. F&ouml;&szlig;meier</aut>,
        <vrk><url
        ref="http://www.forst.uni-muenchen.de/EXT/AIS/acta/cxifrado/cxif.html">&Ccirc;ifrado
        kaj &gcirc;ia rolo en la nova komunikado</url></vrk>,
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    &Scirc;ajnas al mi, ke
    la senco estas malpli "teni ~a" (PV/PIV diras "teni ~e"), ol
    "igi ~a/~o". Estas la sama nuanco kiel inter "ka&scirc;i" kaj "teni
    ka&scirc;ita". <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_a;&c_k;&c_r;&c_e;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_u; <ind>&c_s;&c_a;&c_k;&c_r;&c_e;&c_c;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="de">geheim halten</trd>
  <trdgrp lng="fr">
    <trd>tenir secret</trd>,
    <trd>celer</trd>
  </trdgrp>
  <trd lng="hu">titkos&iacute;t</trd>
  <trdgrp lng="it">
    <trd>segretare</trd>, 
    <trd>tenere <ind>segreto</ind></trd>, 
    <trd>imporre il <ind>segreto</ind></trd>,
    <trd>celare<klr> (secretare)</klr></trd>
  </trdgrp>
  <trd lng="nl">geheim houden</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v; <ind>&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_je;</ind></trd>,
    <trd>&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v; <ind>&c_t;&c_a;&c_j;&c_n;&c_je;</ind></trd>,
    <trd>&c_t;&c_a;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_a;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">utaji&tcaron;</trd>
  <trd lng="sv">hemligh&aring;lla</trd>
  <trdgrp lng="sk">
    <trd>tajiť</trd>,
    <trd>utajovať</trd>,
    <trd>ututlať</trd>,
    <trd>zamlčať</trd>,
    <trd>zatajiť</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tajit</trd>,
    <trd>tutlat</trd>,
    <trd>utajit</trd>,
    <trd>utajovat</trd>,
    <trd>ututlat</trd>,
    <trd>zamlčet</trd>,
    <trd>zatajit</trd>,
    <trd>zatajovat</trd>
  </trdgrp>
</drv>

<drv mrk="sekret.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc mrk="sekret.0ema.KOMUNE">
    <dif>
      Ege diskreta, ema <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>důvěrný</trd>,
    <trd>mlčenlivý</trd>,
    <trd>tajnůstkářský</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cachottier</trd>,
    <trd>dissimul&eacute;<klr tip="ind"> (cachottier)</klr></trd>,
    <trd>myst&eacute;rieux<klr tip="ind"> (cachottier)</klr></trd>,
    <trd>secret<klr tip="ind"> (adj., enclin au secret)</klr></trd>
  </trdgrp>
  <trd lng="hu">titkol&oacute;dz&oacute;</trd>
  <trd lng="nl">discreet</trd>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dôverný</trd>,
    <trd>mlčanlivý</trd>,
    <trd>tajnostkársky</trd>
  </trdgrp>
  <trd lng="sv">hemlighetsfull</trd>
</drv>

<drv mrk="sekret.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekret.en0igi.KOMUNE">
    <dif>
      Sciigi pri <tld/>o:
      <ekz>
        tiam mi certe ne pensis, ke <klr>...</klr> mi estos
        en<tld/>igita en la pretigadon <klr>[de la supo]</klr>
        <fnt><bib>Fab3</bib>, <lok>Supo el kolbasaj
        bastonetoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfid.0i.pro_honesteco">konfidi<sncref/></ref>,
      <ref cel="sekret.0i.konfidi"><tld/>i<sncref/></ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="inic.0i">inici</ref>,
    </refgrp>
  </snc>
  <adm>
    PIV2 citas la saman ekzemplon, sed senakuzativigis "pretigadon".
    La PIV-a difino enhavas la esprimon "meti en la ~on", kiu, se
    sufi&ccirc;e internacia, meritus aperi en ekz sub ~o.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>mettre au <ind>parfum</ind></trd>,
    <trd>mettre dans le <ind>secret</ind></trd>
  </trdgrp>
  <trd lng="hu">titokba beavat</trd>
  <trdgrp lng="it">
    <trd>confidare<klr> un segreto</klr></trd>,
    <trd>mettere a parte di un <ind>segreto</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol; &c_v; &c_t;&c_a;&c_j;&c_n;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol; &c_v; &c_s;&c_je;&c_k;&c_r;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">zasvätiť do tajomstva</trd>
  <trd lng="cs">zasvětit do tajemství</trd>
</drv>

<drv mrk="sekret.for0igi">
  <kap>for<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekret.for0igi.KOMUNE">
    <dif>
      Ka&scirc;i, forigante <tld/>igi:
      <ekz>
        Zir-Nana <klr>...</klr> konsentigis la re&gcirc;inon
        for<tld/>igi lin en la arbara &ccirc;asdomo de la re&gcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;rober aux regards</trd>
</drv>

<drv mrk="sekret.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekret.mal0igi.KOMUNE">
    <dif>
      Malka&scirc;i, konigi ion <tld/>an.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_s;&c_a;&c_k;&c_r;&c_e;&c_c;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odhalit</trd>,
    <trd>prozradit</trd>,
    <trd>vyjevit</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;voiler<klr tip="ind"> (un secret)</klr></trd>
  <trdgrp lng="hu">
    <trd>felfed <klr tip="ind">(titkot)</klr></trd>,
    <trd>nyilv&aacute;noss&aacute;gra hoz</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>buka</ind>] membuka (rahasia)</trd>,
    <trd>[<ind>bongkar</ind>] membongkar (rahasia)</trd>,
    <trd>[<ind>kuak</ind>] menguak (rahasia)</trd>,
    <trd>[<ind>ungkap</ind>] mengungkapkan (rahasia)</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>svelare un <ind>segreto</ind></trd>,
    <trd>scoprire un <ind>segreto</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verklappen</trd>,
    <trd>ruchtbaar maken</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prezradiť</trd>,
    <trd>vyzradiť</trd>
  </trdgrp>
