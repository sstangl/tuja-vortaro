<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aed.xml,v 1.8 2015/07/04 08:37:29 revo Exp $">
<kap>
  <rad>aed</rad>/o
</kap>
<drv mrk="aed.0o">
  <kap><tld/>o</kap>
  <snc mrk="aed.0o.HIS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POE</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      Helena pratempa <ref tip="super" cel="poet.0o">poeto</ref>, kiu
      kantis akompane de liro, la glorajn 
      agojn de dioj kaj herooj.
    </dif>
    <refgrp tip="vid">
      <ref cel="bard.0o.HIS">bardo</ref>,
      <ref cel="menestre.0o.HIS">menestrelo</ref>,
      <ref cel="rapsod.0o.HIS">rapsodo</ref>,
      <ref cel="skald.0o.HIS">skaldo</ref>,
      <ref cel="trobad.0o">trobadoro</ref>
    </refgrp>
    <trd lng="es">aedo</trd>
    <trd lng="it">aedo</trd>
    <trd lng="ca">aede</trd>
    <trd lng="pl">aojda</trd>
  </snc>
  <snc mrk="aed.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      Genro (Aedes) de <ref tip="super" cel="moskit.0o">moskitoj</ref>,
      kies plej konata speco A. aegytpi 
      transportas la flavan <ref tip="vid"
      cel="febr.0o.MED">febron</ref>.
    </dif>
    <trd lng="es">mosquito aedes aegypti<klr tip="amb"> (fiebre
    amarilla)</klr></trd>
    <trdgrp lng="it">
      <trd>aedes<klr> (zanzara)</klr></trd>,
      <trd><ind>zanzara</ind> tigre</trd>,
      <trd><ind>zanzara</ind> tigre egiziana</trd>
    </trdgrp>
    <trd lng="ca">aedes</trd>
   </snc>
  <trd lng="cs">starořecký básník</trd>
  <trd lng="sk">starogrécky básnik</trd>
</drv>
</art>

