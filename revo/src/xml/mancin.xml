<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mancin.xml,v 1.12 2013/11/02 13:10:14 revo Exp $">
<kap>
  <rad>mancinel</rad>/o
</kap>
<drv mrk="mancin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:botanikaj_specioj">Specio</ref>
       el familio
      <ref tip="malprt" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Hippomane mancinella</trd>), 
      kun folioj kaj fruktoj similaj al 
      pom<ref tip="lst" cel="arb.0o"
      lst="voko:malpigioparencoj">arbo</ref>, 
      sed kies lakteca suko estas tre venena.
    </dif>
  </snc>
  <trd lng="de">Manschinellenbaum</trd>
  <trd lng="en">manchineel</trd>
  <trd lng="es">Manzanilla de la muerte</trd>
  <trd lng="hu">manz&aacute;nilla-almafa</trd>
  <trd lng="nl">manzanillaboom</trd>
  <trd lng="pt">mancenilheira</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_n;&c_c;&c_i;&c_n;&c_je;&c_l;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_sh;&c_i;&c_n;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="mancin.0frukto">
  <kap>
    <tld/>frukto
  </kap>
  <snc>
    <dif>
      Tre venena frukto de la <tld/>o, iomete simila al malgranda pomo.
    </dif>
    <trd lng="hu">manz&aacute;nilla-alma</trd>
  </snc>
</drv>
</art>
