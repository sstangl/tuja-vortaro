<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lesb.xml,v 1.17 2015/11/20 15:10:19 revo Exp $">
<kap>
  <rad>Lesb</rad>/o
</kap>
<drv mrk="lesb.0o.GEOG">
  <kap>
    <tld lit="L"/>o
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref> 
      inter Grekujo kaj Turkujo kun &ccirc;efurbo Mitileno.
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_je;&c_s;&c_b;&c_a;&c_s;</trd>
  <trd lng="ca">Lesbos</trd>
  <trd lng="de">Lesbos</trd>
  <trdgrp lng="en">
    <trd>Lesbos</trd>,
    <trd>Lesvos</trd>, <trd>Mytilini</trd>
  </trdgrp>
  <trd lng="fr">Lesbos</trd> 
  <trd lng="hu">Leszbosz</trd>
  <trd lng="id">Lesbos</trd>
  <trd lng="nl">Lesbos</trd>
  <trdgrp lng="pt">
    <trd>Lesbos</trd>,
    <trd>Mitilene</trd>
  </trdgrp>
  <trd lng="ru">&c_L;&c_je;&c_s;&c_b;&c_o;&c_s;</trd>
</drv>

<drv mrk="lesb.l0o">
  <kap>
    <tld lit="l"/>o
  </kap>
  <snc mrk="lesb.0o.insulano">
    <dif>
      <ref tip="dif" cel="lesb.l0anino.SOC"><tld/>an(in)o</ref>.
    </dif>
    <trdgrp lng="en">
      <trd>resident of Lesbos</trd>,
      <trd>Lesbosian</trd>
    </trdgrp>
  </snc>
  <snc mrk="lesb.0o.samseksema">
    <dif>Virino amanta virinon:
     <ekz>
        kiajn &scirc;ancojn alportas PACS al ni, <tld lit="l"/>oj kaj gejoj?
        <fnt>
        <lok>Forumo, membrogazeto de LSG (Ligo de Samseksamaj
        Geesperantistoj), 1/2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="seks.sam0emulino">samseksemulino</ref>
    <ref tip="vid" cel="gej.0o">gejo</ref>
    <trd lng="en">lesbian</trd>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_s;&c_mol;&c_b;&c_ib;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">lesbiana</trd>
  <trd lng="de">Lesbe</trd>
  <trdgrp lng="fr">
    <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
    <trd>lesbienne</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leszbikus</trd>,
    <trd>leszbi</trd>
  </trdgrp>  
  <trd lng="id">lesbian</trd>
  <trdgrp lng="nl">
    <trd>lesbische</trd>,
    <trd>lesbienne</trd>
  </trdgrp>
  <trd lng="pt">l&eacute;sbica</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_b;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="lesb.l0anino">
  <kap>
    <tld lit="l"/>anino
  </kap>
  <snc mrk="lesb.l0anino.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lo&gcirc;antino de <tld/>o.
    </dif>
    <trd lng="ca">lesbi <klr> (f. l&egrave;sbia)</klr></trd>
    <trd lng="de">Lesbierin</trd>
    <trd lng="en">resident of Lesbos <klr>(female)</klr></trd>
    <trd lng="fr">lesbienne<klr tip="ind"> (habitante de
    Lesbos)</klr></trd>
    <trd lng="hu">leszboszi<klr tip="ind"> (n&odblac;)</klr></trd>
    <trd lng="nl">Lesbische</trd>    
    <trd lng="pt">l&eacute;sbia</trd>
  </snc>
  <snc mrk="lesb.l0anino.SOC">
    <ref tip="dif" cel="lesb.0o.samseksema"><tld lit="l"/>o <sncref
      ref="lesb.0o.samseksema"/></ref>
    <trd lng="be">&c_l;&c_je;&c_s;&c_mol;&c_b;&c_ib;&c_ja;&c_n;&c_k;&c_a;</trd>
    <trd lng="ca">lesbiana</trd>
    <trd lng="de">Lesbe</trd>
    <trd lng="en">lesbian</trd>
    <trdgrp lng="fr">
      <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
      <trd>lesbienne</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>leszbikus</trd>,
      <trd>leszbi</trd>
    </trdgrp>    
    <trd lng="id">lesbian</trd>
    <trdgrp lng="nl">
      <trd>lesbische</trd>,
      <trd>lesbienne</trd>
    </trdgrp>
    <trd lng="pt">l&eacute;sbica</trd>
    <trd lng="ru">&c_l;&c_je;&c_s;&c_b;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>
  </snc>
</drv>

<drv mrk="lesb.l0a">
  <kap>
    <tld lit="l"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld lit="l"/>oj:
      <ekz>
        batali kontra&ubreve; anta&ubreve;ju&gcirc;oj kune kun gefratoj gejaj
        kaj <tld lit="l"/>aj estus granda honoro por mi
        <fnt>
        <lok>Forumo, membrogazeto de LSG, 2/2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vivrilato inter geja a&ubreve; <tld lit="l"/>a paro nun
        &ccirc;iurilate estos le&gcirc;e egala al tiu de geedza paro
        <fnt><aut>Gerrit Berveling</aut>:
        <vrk><url ref="&Monato;/007192.html">Edzi&gcirc;oj anka&ubreve; por samseksaj
        paroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gej.0a">geja</ref>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_s;&c_mol;&c_b;&c_ib;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">lesbi&agrave;<klr> (adj.)</klr></trd>
  <trd lng="de">lesbisch</trd>
  <trd lng="en">lesbian</trd>
  <trd lng="fr">lesbien</trd>
  <trd lng="hu">leszbikus</trd>  
  <trd lng="nl">lesbisch</trd>
  <trd lng="pt">l&eacute;sbico</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_b;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
