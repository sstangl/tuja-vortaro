<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alveol.xml,v 1.18 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>alveol</rad>/o<fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alveol.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kava&jcirc;eto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_v;&c_e;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>alveol</trd>,
    <trd>alveolus</trd>,
    <trd>lůžko</trd>,
    <trd>sklípek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aush&ouml;hlung</trd>,
    <trd>H&ouml;hle</trd>,
    <trd>Hohlraum</trd>,
    <trd>Alveolen<klr> [korp/med]</klr></trd>
  </trdgrp>
  <trd lng="en">cavity</trd>
  <trd lng="fr">alv&eacute;ole</trd>
  <trdgrp lng="hu">
    <trd>&uuml;reg</trd>,
    <trd>kaverna</trd>,
    <trd>alveolus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>holte</trd>,
    <trd>kas</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_v;&c_je;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>alveola</trd>,
    <trd>pľúcna dutinka</trd>,
    <trd>zubná jamka</trd>
  </trdgrp>
  <trd lng="sv">alveol</trd>
</drv>

<drv mrk="alveol.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="alveol.0uloj.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      E&ubreve;kariotaj unu&ccirc;eluloj kun karakteriza RNA
      kaj en sia plasmo disaj <tld/>oj
      (<trd lng="la">Alveolata</trd>).
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>alv&eacute;ol&eacute;s</trd>,
    <trd>alv&eacute;olobiontes</trd>
  </trdgrp>
</drv>
</art>
