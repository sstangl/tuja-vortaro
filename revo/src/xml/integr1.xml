<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: integr1.xml,v 1.17 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>integr</rad>/i
</kap>
<drv mrk="integr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 22</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0i">integrali.</ref>
    <rim>
      En la &ccirc;i-supra fontindiko Bricard parolas nur pri
      <ctl><tld/>i diferencialan ekvacion</ctl>, sed certe la
      senco <ctl><tld/>i funkcion</ctl> ne estis al li fremda.
    </rim>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>integrovat</trd>,
    <trd>počítat integrál</trd>,
    <trd>slučovat</trd>
  </trdgrp>
  <trd lng="de">integrieren</trd>
  <trd lng="es">integrar <klr tip="amb">(mat.)</klr></trd>
  <trd lng="hu">integr&aacute;l</trd>
  <trd lng="pt">integrar <klr tip="amb">(mat.)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>integrovať</trd>,
    <trd>zlučovať</trd>
  </trdgrp>
</drv>

<drv mrk="integr1.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0ado">integralado.</ref>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>integrace</trd>,
    <trd>integrování</trd>
  </trdgrp>
  <trd lng="de">Integration</trd>
  <trd lng="es">integraci&oacute;n</trd>
  <trd lng="hu">integr&aacute;l&aacute;s</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">integrovanie</trd>
</drv>

<drv mrk="integr1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0o">integralo.</ref>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;</trd>
  <trd lng="de">Integral</trd>
  <trd lng="es">integral</trd>
  <trd lng="hu">integr&aacute;l</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;</trd>
</drv>

<drv mrk="integr1.0ato">
  <kap><tld/>ato</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0ato">integralato.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_y;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_y;&c_ja;</ind></trd>,
    <trd>&c_p;&c_a;&c_d;&c_y;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_v;&c_y;&c_r;&c_a;&c_z;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>integrand</trd>,
    <trd>integrovaný výraz</trd>
  </trdgrp>
  <trd lng="de">Integrand</trd>
  <trd lng="es">integrando <klr>(funci&oacute;n a integrar)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_y;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">integrovaný výraz</trd>
</drv>

<drv mrk="integr1.0ebla">
  <kap><tld/>ebla</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0ebla">integralebla.</ref>
  </snc>
  <trd lng="de">integrierbar</trd>
  <trd lng="es">integrable</trd>
  <trd lng="hu">integr&aacute;lhat&oacute;</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_i;&c_r;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
</drv>

<drv mrk="integr1.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <ref tip="dif" cel="integr2.0signo">integralsigno.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_l; <klr tip="amb">(&c_z;&c_n;&c_a;&c_k;)</klr></trd>,
    <trd>&c_z;&c_n;&c_a;&c_k; &c_ib;&c_n;&c_t;&c_e;&c_g;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>integrátor</trd>,
    <trd>znak integrálu</trd>
  </trdgrp>
  <trd lng="de">Integralzeichen</trd>
  <trd lng="es">signo de integral</trd>
  <trd lng="hu">integr&aacute;ljel</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_k; <ind>&c_i;&c_n;&c_t;&c_je;&c_g;&c_r;&c_a;&c_l;&c_a;</ind></trd>
  <trd lng="sk">znak integrálu</trd>
</drv>
</art>
