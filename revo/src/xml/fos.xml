<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fos.xml,v 1.27 2015/08/28 08:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fos</rad>/i
</kap>
<drv mrk="fos.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.0i.tero">
    <dif>
      Fari kava&jcirc;on eltirante teron:
      <ekz>
        <tld/>i teron<fnt>B</fnt>, rokon, sablon;
      </ekz>
      <ekz>
        <tld/>i kanalon<fnt>Z</fnt>,
        kava&jcirc;on<fnt>Z</fnt>, puton<fnt>B</fnt>, tombon;
      </ekz>
      <ekz>
        ili <tld/>is anta&ubreve; mi <tld/>on
        <fnt><bib>MT</bib>, <lok>&Psa; 57:6</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bor.0i">bori</ref>,
      <ref cel="kav.0igi">kavigi</ref>,
      <ref cel="plug.0i">plugi</ref>
    </refgrp>
  <trdgrp lng="es">
    <trd>cavar</trd>,
    <trd>excavar</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cavar</trd>,
    <trd>escavar</trd>,
    <trd>fossar</trd>,
    <trd>minar</trd>,
    <trd>revolver</trd>
  </trdgrp>
  </snc>
  <snc mrk="fos.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Profunde esplori demandon:
      <ekz>
        filozofie <tld/>i en teorioj<fnt>Z</fnt>.
      </ekz>
    </dif>
  <trd lng="es">profundizar<klr> (en el conocimiento)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hloubit <klr>(j&aacute;mu)</klr></trd>,
    <trd>kopat</trd>,
    <trd>p&rcaron;ehrab&aacute;vat</trd>,
    <trd>&scaron;&tcaron;ourat se</trd>
  </trdgrp>
  <trd lng="de">graben</trd>
  <trd lng="en">dig</trd>
  <trd lng="es">excavar</trd>
  <trdgrp lng="fr">
    <trd>creuser</trd>,
    <trd>approfondir<klr tip="ind"> (examiner &agrave; fond)</klr></trd>,
    <trd>foncer<klr> (creuser)</klr></trd>,
    <trd>fouiller<klr tip="ind"> (creuser pour trouver qc)</klr></trd>,
    <trd>retourner<klr tip="ind"> (la terre)</klr></trd>
  </trdgrp>
  <trd lng="hu">&aacute;s</trd>
  <trd lng="nl">graven</trd>
  <trdgrp lng="pt">
    <trd>esquadrinhar</trd>,
    <trd>escarafunchar</trd>,
    <trd>fu&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kopa&tcaron;</trd>,
    <trd>ry&tcaron;</trd>,
    <trd>r&yacute;&lcaron;ova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fos.0ado.KOMUNE">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="cs">kop&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Graben</trd>,
    <trd>Grabung</trd>
  </trdgrp>
  <trd lng="en">digging</trd>
  <trd lng="es">excavaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>creusage</trd>,
    <trd>creusement</trd>
  </trdgrp>
  <trd lng="hu">&aacute;s&aacute;s</trd>
  <trd lng="nl">graafwerk</trd>
  <trd lng="ru">&c_r;&c_y;&c_t;&c_mol;&c_jo;</trd>
  <trd lng="sk">kopanie</trd>
</drv>

<drv mrk="fos.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fos.0ajxo.KOMUNE">
    <dif>
      <tld lit="F"/>ita, ordinare longa kava&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="galeri.0o">galerio</ref>,
      <ref cel="min.0ejo">minejo</ref>,
      <ref cel="put.0o">puto</ref>,
      <ref cel="sulk.0o">sulko</ref>,
      <ref cel="sxakt.0o">&scirc;akto</ref>,
      <ref cel="tomb.0o">tombo</ref>,
      <ref cel="trancx1.0o">tran&ccirc;eo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Graben</trd>
  <trdgrp lng="en">
    <trd>ditch</trd>,
    <trd>hole</trd>,
    <trd>trench</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>foso</trd>,
    <trd>hoyo</trd>,
    <trd>zanja</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fosse</trd>,
    <trd>tranch&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rok</trd>
  <trdgrp lng="nl">
    <trd>kuil</trd>,
    <trd>greppel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_sh;&c_je;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="fos.0ilo.KOMUNE">
    <dif>
      &Ccirc;ia ilo por <tld/>i:
      <ekz>
        per <tld/>ilo ni <tld/>as
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">n&aacute;stroj na obd&ecaron;l&aacute;v&aacute;n&iacute; p&uring;dy</trd>
  <trdgrp lng="de">
    <trd>Grabwerkzeug</trd>,
    <trd>Schaufel</trd>,
    <trd>Spaten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>digging implement</trd>,
    <trd>shovel</trd>,
    <trd>spade</trd>
  </trdgrp>
  <trd lng="es">pala</trd>
  <trd lng="fr">pelle</trd>
  <trd lng="hu">&aacute;s&oacute;</trd>
  <trdgrp lng="nl">
    <trd>schop</trd>,
    <trd>spade</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">n&aacute;stroj na obr&aacute;banie p&ocirc;dy</trd>
</drv>

<drv mrk="fos.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.cxirkaux0i..KOMUNE">
    <dif>
      <tld lit="F"/>i &ccirc;iuflanke:
      <ekz>
        li &ccirc;irka&ubreve;<tld/>is <klr>[sian &gcirc;ardenon]</klr>,
        sen&scirc;tonigis &gcirc;in, kaj plantis en &gcirc;i plej
        bonspecajn vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 5:2</lok></fnt>;
      </ekz>
      <ekz>
        mi hastis anta&ubreve;en por vidi &gcirc;in pla&ubreve;di
        en la <ind>&ccirc;irka&ubreve;<tld/>a&jcirc;o</ind>n tuj
        anta&ubreve; la fermita levponto
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 25</lok></fnt>.
        <trdgrp lng="fr">
          <trd>foss&eacute;<klr tip="ind"> (circulaire)</klr></trd>,
          <trd>douves<klr tip="ind"> (foss&eacute;)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>k&ouml;r&aacute;rok</trd>,
          <trd>v&aacute;r&aacute;rok</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>excavar<klr tip="ind"> (por todas partes)</klr></trd>,
    <trd>voltear<klr> (la tierra)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>creuser<klr tip="ind"> (de toutes parts)</klr></trd>,
    <trd>d&eacute;foncer<klr> (creuser de toutes parts)</klr></trd>,
    <trd>retourner<klr tip="ind"> (la terre)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&ouml;rbe&aacute;s</trd>
  <adm>
    La ekzemplo kun "cxirkaux~a&jcirc;o" suspektigas, ke "&ccirc;irka&ubreve;~i"
    povus signifi anka&ubreve; "~i &ccirc;e la &ccirc;irka&ubreve;o de". Indus ser&ccirc;i
    ekzemplojn.
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="fos.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.dis0i.KOMUNE">
    <dif>
      Malferme <tld/>i, difektante la surfacon:
      <ekz>
        <ind>dis<tld/>i</ind> la teron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        duone dis<tld/>ita pavimo el brikoj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>rozkopat</trd>,
    <trd>rozr&yacute;t</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>abrir una excavaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;foncer<klr tip="ind"> (creuser profond&eacute;ment)</klr></trd>,
    <trd>&eacute;ventrer<klr> (en creusant)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;t&aacute;s</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozkopa&tcaron;</trd>,
    <trd>rozry&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.en0i.KOMUNE">
    <dif>
      <tld lit="F"/>i por enterigi.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hrab&aacute;n&iacute;m zakr&yacute;t</trd>,
    <trd>zahrabat</trd>,
    <trd>zakopat</trd>
  </trdgrp>
  <trd lng="de">eingraben</trd>
  <trd lng="en">bury</trd>
  <trd lng="es">enterrar</trd>
  <trdgrp lng="fr">
    <trd>enfouir</trd>,
    <trd>ensevelir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>be&aacute;s</trd>,
    <trd>bele&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">begraven</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zakopa&tcaron;</trd>
</drv>

<drv mrk="fos.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.el0i.elterigi">
    <dif>
      Elterigi, eltiri per <tld/>ado:
      <ekz>
        el<tld/>isto de trezoroj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nakopat</trd>,
    <trd>vydob&yacute;t</trd>,
    <trd>vyhrabat</trd>,
    <trd>vykopat</trd>
  </trdgrp>
  <trd lng="de">ausgraben</trd>
  <trdgrp lng="en">
    <trd>dig out</trd>,
    <trd>dig up</trd>,
    <trd>excavate</trd>
  </trdgrp>
  <trd lng="es">desenterrar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;couvrir<klr tip="ind"> (qc d'enfoui)</klr></trd>,
    <trd>d&eacute;gager<klr tip="ind"> (qc d'enfoui)</klr></trd>,
    <trd>exhumer</trd>,
    <trd>extraire<klr tip="ind"> (qc d'enfoui)</klr></trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>opgraven</trd>,
    <trd>uitgraven</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vykopa&tcaron;</trd>
</drv>

<drv mrk="fos.el0ado">
  <kap>el<tld/>ado</kap>
  <snc mrk="fos.el0ado.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Elmeto de subtera&jcirc;oj trovitaj per <tld/>o:
      <ekz>
        la grandskalaj el<tld/>adoj postulas la kunlaboron de pluraj
        arkeologoj kaj aliaj fakuloj
        <fnt><bib>IntArkeol</bib>, <lok>p. 10a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">exhumace</trd>
  <trdgrp lng="de">
    <trd>Ausgrabung</trd>
  </trdgrp>
  <trd lng="en">excavation</trd>
  <trd lng="es">extracci&oacute;n</trd>
  <trd lng="fr">exhumation</trd>
  <trdgrp lng="hu">
    <trd>ki&aacute;s&aacute;s</trd>,
    <trd>&aacute;sat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_k;&c_a;&c_p;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">exhum&aacute;cia</trd>
</drv>

<drv mrk="fos.pri0ado">
  <kap>pri<tld/>ado</kap>
  <snc mrk="fos.pri0ado.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Esplorado de la grundo por trovi subtera&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="cs">vykop&aacute;vky <klr>(pr&aacute;ce)</klr></trd>
  <trd lng="es">excavaci&oacute;n<klr> (para explorar un terreno)</klr></trd>
  <trd lng="fr">fouille<klr> (d'un terrain)</klr></trd>
  <trd lng="hu">megkutat<klr> (&aacute;sat&aacute;ssal)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_k;&c_i;</trd>
  <trd lng="sk">vykop&aacute;vky <klr>(pr&aacute;ce)</klr></trd>
</drv>

<drv mrk="fos.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.sub0i.sube">
    <dif>
      <tld lit="F"/>i sube de io.
    </dif>
  </snc>
  <snc mrk="fos.sub0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&scirc;e prepari la ruinigon, la malsukceson de; perfide malutili al,
      nevideble iom post iom difekti:
      <ekz>
        tia metio sub<tld/>as la sanon;
      </ekz>
      <ekz>
        la trolaboro, la &ccirc;agreno sub<tld/>as lin;
      </ekz>
      <ekz>
        sub<tld/>i per kalumnioj la reputacion de iu;
      </ekz>
      <ekz>
        kelkaj personoj komencis sub<tld/>adi la a&ubreve;toritaton de niaj
        kongresoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiaj komplotoj sub<tld/>is la potencon de la regno.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>podkopat</trd>,
    <trd>podkop&aacute;vat</trd>,
    <trd>vym&iacute;lat</trd>
  </trdgrp>
  <trd lng="de">untergraben</trd>
  <trd lng="en">undermine</trd>
  <trdgrp lng="es">
    <trd>minar<klr tip="ind"> (socavar)</klr></trd>,
    <trd>socavar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affouiller</trd>,
    <trd>miner<klr tip="ind"> (saper)</klr></trd>,
    <trd>saper</trd>
  </trdgrp>
  <trd lng="hu">al&aacute;&aacute;s</trd>
  <trd lng="nl">ondermijnen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_a;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_p;&c_o;&c_d;&c_k;&c_o;&c_p;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podkopa&tcaron;</trd>,
    <trd>podry&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.tra0i.KOMUNE">
    <dif>
      Bore <tld/>i kaj esplori, truigante la materialon:
      <ekz>
        ne provizu al vi trezorojn sur la tero, <klr>...</klr> kie
        &scirc;telistoj tra<tld/>as kaj &scirc;telas
        <fnt><bib>NT</bib>, <lok>&Mat; 6:19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">prokopat</trd>
  <trd lng="en">dig through</trd>
  <trd lng="es">perforar</trd>
  <trd lng="fr">percer en creusant</trd>
  <trd lng="hu">(&aacute;s&aacute;ssal) <ind>&aacute;tkutat</ind></trd>
  <trd lng="sk">prekopa&tcaron;</trd>
</drv>

<drv mrk="fos.pik0ilo">
  <kap>pik<tld/>ilo</kap>
  <snc mrk="fos.pik0ilo.AGR">
    <ref tip="dif" cel="piocx.0o">pio&ccirc;o</ref>
  </snc>
  <trd lng="cs">krump&aacute;&ccaron;</trd>
  <trdgrp lng="de">
    <trd>zweizinkige <ind>Hacke</ind></trd>,
    <trd>Kreuzhacke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pick</trd>,
    <trd>pickaxe</trd>
  </trdgrp>
  <trd lng="es">pico</trd>
  <trd lng="fr">pioche</trd>
  <trd lng="hu">kapa</trd>
  <trd lng="nl">houweel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kromp&aacute;&ccaron;</trd>,
    <trd>&ccaron;akan</trd>
  </trdgrp>
</drv>

<drv mrk="fos.plat0ilo">
  <kap>plat<tld/>ilo</kap>
  <snc mrk="fos.plat0ilo.AGR">
    <uzo tip="fak">AGR</uzo>
