<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: herc.xml,v 1.10 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>herc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herc.0o">
  <kap><tld/>o</kap>
  <snc mrk="herc.0o.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <mlg kod="SI">Hz</mlg>
    <dif>
      <ref tip="lst" cel="unu.mezur0o.SCI"
      lst="voko:mezurunuoj">Mezurunuo</ref> de 
      <ref tip="vid" cel="frekve.0o.TEK">frekvenco</ref>
      en la Internacia Sistemo de unuoj, egala al unu 
      oscilo en unu sekundo:
      <ekz>
        homo perceptas la sonondojn inter 20 kaj 16 000 <tld/>oj
        <fnt>
          <bib>Viki</bib>,
          <lok>artikolo &leftquot;senso (birdoj)&rightquot;, 
          2007-01-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_h;&c_je;&c_r;&c_c;</trd>
  <trd lng="cs">hertz</trd>
  <trd lng="de">Hertz<klr tip="amb"> (Einheit)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_r;&a_t;&a_s;</trd>,
    <trd>&a_h;&a_r;&a_t;&a_z;</trd>
  </trdgrp>
  <trd lng="fr">hertz</trd>
  <trd lng="hu">Hertz</trd>
  <trd lng="id">hertz</trd>
  <trd lng="it">hertz</trd>
  <trd lng="nl">hertz</trd>
  <trd lng="sk">karta ƒçervenej farby</trd>
</drv>
</art>

