<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: orf.xml,v 1.28 2015/07/04 08:37:40 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>orf</rad>/o
</kap>
<drv mrk="orf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="orf.0o.GEN">
  <uzo tip="fak">GEN</uzo>
    <dif>
      Knabo a&ubreve; knabino, kies patro a&ubreve; patrino mortis:
      <ekz>
        de edzi&gcirc;o tro malfrua <tld/>oj naski&gcirc;as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj laboriloj ku&scirc;as <tld/>e <klr>(forlasite)</klr> sur la
        stablo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_r;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_s;&c_i;&c_r;&c_a;&c_k;</trd>
  <trd lng="br">emzivad</trd>
  <trd lng="cs">sirotek</trd>
  <trdgrp lng="de">
    <trd>Waisenkind</trd>,
    <trd>Waise</trd>
  </trdgrp>
  <trd lng="el">&omikron;&rho;&phi;&alfa;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">orphan</trd>
  <trd lng="es">hu&eacute;rfano</trd>
  <trd lng="fr">orphelin<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="he">&yod;&tav;&vav;&fmem;</trd>
  <trd lng="hu">&aacute;rva</trd>
  <trd lng="id">yatim piatu</trd>
  <trd lng="nl">wees</trd>
  <trd lng="pl">sierota</trd>
  <trd lng="pt">&oacute;rf&atilde;o</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">sirota</trd>
  <trd lng="sv">f&ouml;r&auml;ldral&ouml;s</trd>
  <trdgrp lng="tr">
    <trd>yetim</trd>,
    <trd>&ouml;ks&uuml;z</trd>
  </trdgrp>
</drv>

<drv mrk="orf.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="orf.0ejo.SAN">
    <dif>
      Rifu&gcirc;a establejo, kie oni zorgas ge<tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_y; &c_d;&c_o;&c_m;</trd>
  <trd lng="bg">&c_s;&c_i;&c_r;&c_o;&c_p;&c_i;&c_t;&c_a;&c_l;&c_i;&c_shch;&c_je;</trd>
  <trd lng="br">ti-emzivaded</trd>
  <trd lng="cs">sirotčinec</trd>
  <trd lng="de">Waisenhaus</trd>
  <trd lng="el">&omikron;&rho;&phi;&alfa;&ny;&omikron;&tau;&rho;&omikron;&phi;&epsilon;&jota_ton;&omikron;</trd>
  <trd lng="en">orphanage</trd>
  <trd lng="es">orfanato</trd>
  <trd lng="fr">orphelinat</trd>
  <trd lng="he">&bet;&yod;&tav; &yod;&tav;&vav;&mem;&yod;&fmem;</trd>
  <trd lng="hu">&aacute;rvah&aacute;z</trd>
  <trd lng="id"><ind>panti</ind> asuhan</trd>
  <trd lng="nl">weeshuis</trd>
  <trdgrp lng="pl">
    <trd>sierociniec <klr>(przest.)</klr></trd>,
    <trd>dom <ind>dziecka</ind></trd>,
    <trd>bidul <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">orfanato</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_d;&c_o;&c_m;</trd>,
    <trd>&c_s;&c_i;&c_r;&c_o;&c_t;&c_s;&c_k;&c_i;&c_j; &c_p;&c_r;&c_i;&c_ju;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">sirotinec</trd>
  <trd lng="sv">barnhem</trd>
  <trdgrp lng="tr">
    <trd>yetimhane</trd>,
    <trd>&ouml;ks&uuml;zler yurdu</trd>
  </trdgrp>
</drv>

<drv mrk="orf.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senigi je patro a&ubreve; patrino, a&ubreve; je simile tre necesa protekto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tagoj kaj noktoj, kiam <tld/>igita animo vane vokas la
        perditon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">emzivada&ntilde;</trd>
  <trd lng="cs">učinit sirotou</trd>
  <trd lng="de">zum <ind>Waisen machen</ind></trd>
  <trd lng="en">orphan</trd>
  <trd lng="es">quedar hu&eacute;rfano</trd>
  <trd lng="fr">rendre <ind>orphelin</ind></trd>
  <trd lng="nl"><ind>wees</ind> maken</trd>
  <trd lng="pl">osieroci&cacute;</trd>
  <trd lng="pt">orfanar</trd>
  <trd lng="sk">urobiť sirotou</trd>
  <trdgrp lng="tr">
    <trd>yetim b&inodot;rakmak</trd>,
    <trd>&ouml;ks&uuml;z b&inodot;rakmak</trd>
  </trdgrp>
</drv>
</art>
