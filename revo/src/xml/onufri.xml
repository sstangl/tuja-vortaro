<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: onufri.xml,v 1.6 2015/02/26 19:11:22 revo Exp $">
<kap>
  <rad>Onufri</rad>/o
</kap>
<drv mrk="onufri.0o">
  <kap><tld/>o</kap>
  <snc mrk="onufri.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        anka&ubreve; la tago de la sankta <tld/>o estas lia nomfesto
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hunfried</trd>
  <trd lng="en">Humphrey</trd>
  <trd lng="fr">Onuphre</trd>
  <trd lng="hu">Onufriusz</trd>
  <trd lng="it">Onofrio</trd>
  <trd lng="lat">Onuphrius</trd>
  <trd lng="pt">Onofre</trd>
  <trdgrp lng="ru">
    <trd>&c_O;&c_n;&c_u;&c_f;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_H;&c_e;&c_m;&c_f;&c_r;&c_i;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>

