<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinod.xml,v 1.13 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sinod</rad>/o
</kap>
<drv mrk="sinod.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      Kunveno de pastroj por konsili&gcirc;i kaj decidi pri ekleziaj aferoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>shromáždění</trd>,
    <trd>sněm</trd>,
    <trd>synod</trd>,
    <trd>synoda</trd>
  </trdgrp>
  <trd lng="de">Synode</trd>
  <trd lng="en">synod</trd>
  <trd lng="fr">synode</trd>
  <trdgrp lng="hu">
    <trd>zsinat</trd>,
    <trd>szin&oacute;dus</trd></trdgrp>
  <trd lng="nl">synode</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>cirkevný snem</trd>,
    <trd>synoda</trd>
  </trdgrp>
</drv>
</art>
