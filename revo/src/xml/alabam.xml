<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alabam.xml,v 1.3 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>Alabam</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="alabam.0o">
  <kap><tld/>o</kap>
  <snc mrk="alabam.0o.GEOG_rivero">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en Usono.
    </dif>
  </snc>
  <snc mrk="alabam.0o.GEOG_sxtato">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la suda parto de
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        Johano lo&gcirc;as en <em>Mobile</em>, <tld/>o
        <fnt>
          <aut>J. Bentley</aut>,
          <vrk><url
          ref="http://bulteno.esperanto-usa.org/2013/6/50-vojagxe-tra-usono.html">
          Lumineska akvo en Alabamo</url></vrk>,
          <lok>Usona Esperantisto, 2013:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Alabama</trd>
  <trd lng="en">Alabama</trd>
  <trd lng="fr">Alabama</trd>
  <trd lng="sk">Alabama</trd>
</drv>
</art>

