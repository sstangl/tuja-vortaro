<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flandr.xml,v 1.12 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>flandr</rad>/o
</kap>
<drv mrk="flandr.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_a;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>
  <trd lng="ca">flamenc<klr> (subst.)</klr></trd>
  <trd lng="cs">Vlám</trd>
  <trd lng="de">Flame</trd>
  <trd lng="en">Flemish</trd>
  <trd lng="fr">Flamand</trd>
  <trd lng="hu">flamand</trd>
  <trd lng="nl">Vlaming</trd>
  <trd lng="no">flamlender</trd>
  <trd lng="pt">flamengo</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  <trd lng="sk">Flám</trd>
</drv>

<drv mrk="flandr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; ilia nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_m;&c_a;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>
  <trd lng="ca">flamenc<klr> (adj.)</klr></trd>
  <trd lng="de">fl&auml;misch</trd>
  <trd lng="en">Flemish</trd>
  <trd lng="fr">flamand</trd>
  <trd lng="hu">flamand</trd>
  <trd lng="nl">Vlaams</trd>
  <trd lng="no">flamsk</trd>
  <trd lng="pt">flamengo</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="flandr.F0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="flandr.F0ujo.reg">
    <dif>
      La norda nederlandlingva regiono de <ref tip="malprt"
      cel="belg.B0ujo">Belgujo</ref>,
      konsistanta el la provincoj
      <ref tip="prt" cel="flandr.F0ujo.prov">Okcidenta <tld
      lit="F"/>ujo</ref>,
      <ref tip="prt" cel="flandr.F0ujo.prov">Orienta <tld
      lit="F"/>ujo</ref>,
      <ref tip="prt" cel="antverp.A0o.prov">Antverpeno</ref>,
      <ref tip="prt" cel="limburg.L0o.prov">Limburgo</ref> kaj
      <ref tip="prt" cel="brabant.B0o"><tld lit="F"/>a Brabanto</ref>:
      <ekz>
        la a&ubreve;toro fami&gcirc;is en <tld lit="F"/>ujo &ccirc;efe pro granda
        nombro da stre&ccirc;aj aventurlibroj por la
        junularo, sed anka&ubreve; pro siaj erotikaj rakontoj
        <fnt><aut>Rejna DE JONG</aut>:
        <vrk><url ref="&Monato;/rec/bpet.html">Ripoziga libro por hasta
        mondo</url></vrk>,
        <bib>Monato</bib>, <lok>n-ro 04/1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flandr.F0ujo.prov">
    <dif>
      Nomo de du <ref tip="super" cel="provin.0o">provincoj</ref> en
      <ref tip="malprt" cel="flandr.F0ujo.reg">Flandrujo</ref>
      (norda Belgujo):
      Okcident-<tld lit="F"/>ujo kun &ccirc;efurbo <ref tip="prt"
      cel="brugx.B0o">Bru&gcirc;o</ref>
      kaj Orient-<tld lit="F"/>ujo kun &ccirc;efurbo <ref tip="prt"
      cel="gent1.G0o">Gento</ref>, de
      regiono en <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>
      (Zelanda <tld lit="F"/>ujo)
      kaj de regiono de <ref tip="malprt"
      cel="franc.F0ujo">Francujo</ref>.
    </dif>
  </snc>
  <snc mrk="flandr.F0ujo.geo">
    <dif>
      Parto de <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      la&ubreve;longe
      de la Norda Maro, inter la artezaj montetoj en
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>
      kaj la skelda estuaro en
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>.
    </dif>
  </snc>
  <ref tip="sin" cel="flandri.0o">Flandrio</ref>
  <trd lng="be">&c_F;&c_l;&c_ja;&c_n;&c_d;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">Flandes</trd>
  <trd lng="de">Flandern</trd>
  <trd lng="cs">Flandry</trd>
  <trd lng="en">Flanders</trd>
  <trd lng="es">Flandes</trd>
  <trd lng="fi">Flanderi</trd>
  <trd lng="fr">Flandre</trd>
  <trd lng="hu">Flandria</trd>
  <trd lng="is">Fl&aelig;mingjaland</trd>
  <trd lng="it">Fiandra</trd>
  <trd lng="lat">Flandria</trd>
  <trd lng="lv">Flandrija</trd>
  <trd lng="nl">Vlaanderen</trd>
  <trd lng="no">Flandern</trd>
  <trd lng="pl">Flandria</trd>
  <trd lng="pt">Flandres</trd>
  <trd lng="ru">&c_F;&c_l;&c_a;&c_n;&c_d;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">Fl&aacute;msko</trd>
  <trd lng="sv">Flandern</trd>
</drv>
</art>
