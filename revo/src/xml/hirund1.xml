<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hirund1.xml,v 1.11 2013/08/31 08:11:23 revo Exp $">
<kap>
  <rad>hirunded</rad>/oj
</kap>
<drv mrk="hirund1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="paser.0oformaj">paseroformaj birdoj</ref>,
      kun pintaj flugiloj kaj plata beko
      (<trd lng="la">Hirundinidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_t;&c_a;&c_w;&c_k;&c_ib; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Schwalben<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hirundinid&eacute;s</trd>
  <trd lng="hu">fecskef&eacute;l&eacute;k</trd>
  <trd lng="it">irondinidi</trd>
  <trd lng="nl">zwaluwen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">jask&oacute;&lstroke;kowate</trd>
  <trd lng="pt">hirundin&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_i; <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
