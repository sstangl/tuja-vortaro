<vortaro>
<art mrk="$Id: kormor1.xml,v 1.6 2013/08/31 14:10:20 revo Exp $">
<kap>
  <rad>kormoraned</rad>/oj
</kap>
<drv mrk="kormor1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="pelika.0oformaj">pelikanoformaj
      birdoj</ref> (<trd lng="la">Phalacrocoracidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Kormorane <klr tip="ind">(Familie)</klr></trd>
  <trd lng="hu">k&aacute;r&oacute;katonaf&eacute;l&eacute;k</trd>
  <trd lng="pl">kormoranowate</trd>
  <trd lng="pt">falacrocorac&iacute;deos</trd>
  <trd lng="ru">&c_b;&c_a;&c_k;&c_l;&c_a;&c_n;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

