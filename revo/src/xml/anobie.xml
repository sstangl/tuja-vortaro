<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: anobie.xml,v 1.5 2013/08/31 15:10:41 revo Exp $">
<kap>
  <rad>anobied</rad>/oj
</kap>
<drv mrk="anobie.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Anobiidae</trd>),
      kies larvoj boras en ligno kaj sekaj materialoj.
    </dif>
  </snc>
  <trd lng="ca">an&ograve;bids</trd>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>,
    <trd>Klopfk&auml;fer</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_u;&c_k;&c_o;&c_v;)</klr></trd>
</drv>
</art>

