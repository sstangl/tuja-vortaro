<kap>
  <ofc>*</ofc>
  <rad>akcipitr</rad>/o
</kap>
<drv mrk="akcipi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akcipi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Accipiter</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_s;&c_t;&c_r;&c_a;&c_b;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_r;&c_v;&c_ja;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_o;&c_l;&c_ja;&c_m; &c_ja;&c_s;&c_t;&c_r;&c_je;&c_b;</trd>
  <trdgrp lng="br">
    <trd>spalouer</trd>,
    <trd>gwazfalc'hun</trd>
  </trdgrp>
  <trd lng="ca">g&egrave;nere Accipiter</trd>
  <trdgrp lng="cs">
    <trd>jestr&aacute;b</trd>,
    <trd>jestřáb</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Habicht</trd>, <trd>Sperber</trd>
  </trdgrp>
  <trd lng="en">hawk</trd>
  <trd lng="fa">&a_b;&a_A;&a_z;<klr tip="ind"> (&f_p;&a_r;&a_n;&a_d;&a_h;)</klr></trd>
  <trdgrp lng="fr">
    <trd>autour<klr tip="amb"> (oiseau)</klr></trd>,
    <trd>&eacute;pervier</trd>
  </trdgrp>
  <trd lng="hu">h&eacute;ja</trd>
  <trd lng="nl">havik</trd>
  <trd lng="pl">jastrz&aogonek;b</trd>
  <trdgrp lng="pt">
    <trd>a&ccedil;or</trd>,
    <trd>ac&iacute;pitre</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_s;&c_t;&c_r;&c_je;&c_b;</trd>,
    <trd>&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd
>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_je;&c_l;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd
>
  </trdgrp>
  <trd lng="sk">jastrab</trd>
  <trd lng="sv">h&ouml;k</trd>
</drv>
</art>
