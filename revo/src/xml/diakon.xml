<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diakon.xml,v 1.16 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diakon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diakon.0o">
  <kap><tld/>o</kap>
  <snc mrk="diakon.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="super" cel="klerik.0o.REL">Kleriko</ref>,
      <ref tip="super" cel="help.0anto.KOMUNE">helpanto</ref>
      de sacerdoto:
      <ekz>
        li estas <tld/>o malri&ccirc;a, ricevanta apena&ubreve; tricent
        rublojn da salajro dum la jaro
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_k;&c_a;&c_n;</trd>
  <trd lng="ca">diaca</trd>
  <trdgrp lng="cs">
    <trd>diakon</trd>,
    <trd>jáhen</trd>,
    <trd>ďák</trd>,
    <trd>ďáček</trd>
  </trdgrp>
  <trd lng="de">Diakon</trd>
  <trd lng="en">deacon</trd>
  <trd lng="es">di&aacute;cono</trd>
  <trd lng="fr">diacre</trd>
  <trdgrp lng="hu">
    <trd>diak&oacute;nus</trd>,
    <trd>szerpap</trd>
  </trdgrp>
  <trd lng="pl">diakon</trd>
  <trd lng="pt">di&aacute;cono</trd>
  <trd lng="ru">&c_d;&c_mol;&c_ja;&c_k;&c_o;&c_n;</trd>
  <trd lng="sk">diakon</trd>
</drv>
</art>
