<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: abajxur.xml,v 1.10 2015/10/08 13:10:16 revo Exp $">
<kap>
  <rad>aba&jcirc;ur</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="abajxur.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="abajxur.0o.MEB">
    <ref tip="dif" cel="sxirm.lum0ilo.MEB">lum&scirc;irmilo
    <sncref/></ref>
      <ekz>
        en milda lumo de ru&gcirc;a <tld/>o staras &scirc;irmite de
        vesto&scirc;ranko 
        viro
        <fnt>
          <aut>P. Kuusberg, trad. A. Lekko</aut>,
          <vrk><url
          ref="http://miresperanto.narod.ru/biblioteko/somermeze.htm">
          Somermeze</url></vrk>,
          <lok>1981</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_zh;&c_u;&c_r;</trd>
  <trd lng="ca">pantalla</trd>
  <trd lng="en">lampshade</trd>
  <trdgrp lng="it">
    <trd>paralume</trd>,
    <trd>abat-jour</trd>
  </trdgrp>
  <trd lng="nl">lampenkap<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pl">aba&zdot;ur</trd>
  <trd lng="pt">abajur</trd>
  <trd lng="fr">abat-jour</trd>
</drv>
</art>

