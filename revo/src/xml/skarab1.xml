<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skarab1.xml,v 1.8 2014/02/10 15:10:15 revo Exp $">
<kap>
  <rad>skarabed</rad>/oj
</kap>
<drv mrk="skarab1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Scarabaeidae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_v;&c_u;&c_s;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_o;&c_r;&c_o;&c_g;&c_i; <ind>&c_b;&c_r;&c_malmol;&c_m;&c_b;&c_a;&c_r;&c_i;</ind></trd>
  <trd lng="ca">escarabeids</trd>
  <trd lng="hu">lemezescs&aacute;p&uacute;ak<klr tip="amb"> (csal&aacute;d)</klr></trd>
  <trdgrp lng="nl">
    <trd>bladsprietkevers<klr tip="ind"> (fam.)</klr></trd>,
    <trd>bladsprietigen</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_u;&c_s;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
