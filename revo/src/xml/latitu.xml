<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: latitu.xml,v 1.19 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>latitud</rad>/o
</kap>
<drv mrk="latitu.0o">
  <kap><tld/>o</kap>
  <snc mrk="latitu.0o.GEOG_AST">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Distanco de iu loko al la ekvatoro, mezurata per la meridiano
      trapasanta tiun lokon.
    </dif>
    <trd lng="de">Breite</trd>
    <trd lng="hu">f&ouml;ldrajzi <ind>sz&eacute;less&eacute;g</ind></trd>    
  </snc>
  <snc mrk="latitu.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de punkto <frm>M</frm> en tridimensia reela e&ubreve;klida
      <ref cel="spac.afina0o">afina spaco</ref> provizita per orta
      ununorma koordinatsistemo
      <frm>(O, <g>i</g>, <g>j</g>, <g>k</g>)</frm>)</klr>
      Mezuro de la <ref cel="angul.0o.afina">angulo</ref> <frm>(OP,
      OM)</frm>,
      kie <frm>P</frm> signas la ortan projekcia&jcirc;on de <frm>M</frm> sur
      
      la ebeno difinita de <frm>(O, <g>i</g>, <g>j</g>)</frm>:
      <ekz>
        <tld/>o estas la tria koordinato en la sfera
        <ref cel="koordi.0sistemo">koordinatsistemo</ref>.
      </ekz>
    </dif>
    <trd lng="de">Breite<klr tip="ind"> (Kugelkoordinate)</klr></trd>
    <trdgrp lng="fr">
      <trd>latitude<klr tip="ind"> (coordon&eacute;e sph&eacute;rique)</klr></trd>,
      <trd>hauteur<klr tip="ind"> (coordon&eacute;e sph&eacute;rique)</klr></trd>
    </trdgrp>
    <trd lng="hu">sz&eacute;less&eacute;g<klr tip="ind">
    (g&ouml;mbkoordin&aacute;ta)</klr></trd>    
    <trd lng="pl">szeroko&sacute;&cacute;<klr tip="ind">
    (wsp&oacute;&lstroke;rz&eogonek;dna)</klr></trd>
    <trd lng="ru">&c_sh;&c_i;&c_r;&c_o;&c_t;&c_a;
    <klr tip="ind"> (&c_k;&c_o;&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
    <adm>
      Kontroli, &ccirc;u la tradukoj de nivelo drv ta&ubreve;gas anka&ubreve;
      &ccirc;i-sence.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>délka</trd>,
    <trd>latituda</trd>,
    <trd>odchylka</trd>
  </trdgrp>
  <trd lng="el">&gamma;&epsilon;&omega;&gamma;&rho;&alfa;&phi;&jota;&kappa;&omikron_ton; &pi;&lambda;&alfa_ton;&tau;&omikron;&sigma_fina;</trd>
  <trd lng="en">latitude</trd>
  <trd lng="es">latitud</trd>
  <trd lng="fr">latitude</trd>
  <trd lng="nl">breedte</trd>
  <trd lng="pt">latitude</trd>
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>dĺžka</trd>,
    <trd>latitúda</trd>,
    <trd>odchýlka</trd>
  </trdgrp>
  <trd lng="tr">enlem</trd>
</drv>

<drv mrk="latitu.astronomiaauxgeografia0o">
  <kap>astronomia a&ubreve; geografia <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Angulo inter la ekvatora ebeno kaj la ortanto je la surfaco &ccirc;e iu
      punkto.
    </dif>
  </snc>
  <adm>
    Tiu termino identas kun ~o 1 (kvankam iom pli moderne esprimita). Mi
    &scirc;an&gcirc;is "normalo" al "ortanto".
    <aut>MB</aut>
  </adm>
  <trd lng="de">geografische Breite</trd>
  <trd lng="hu">f&ouml;ldrajzi <ind>sz&eacute;less&eacute;g</ind></trd>
</drv>

<drv mrk="latitu.geodezia0o">
  <kap>geodezia <tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Astronomia <tld/>o, precize difinita per korekto pro loka ekarto
      de
      la perpendiklilo.
    </dif>
  </snc>
  <adm>
    Difino malklara. Kio estas perpendiklilo?
    <aut>MB</aut>
  </adm>
  <trd lng="de">geod&auml;tische Breite</trd>
  <trd lng="hu">geod&eacute;ziai <ind>sz&eacute;less&eacute;g</ind></trd>  
</drv>

<drv mrk="latitu.tercentra0o">
  <kap>tercentra <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <tld lit="L"/>o inter la ekvatora ebeno kaj la tera radio, kiu
      tran&ccirc;as la
      lokon.
    </dif>
  </snc>
  <trd lng="hu">geocentrikus <ind>sz&eacute;less&eacute;g</ind></trd>  
  <adm>
    Kia diferenco kun ~o 1 ??
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="latitu.cirklode0o">
  <kap>cirklo de <tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Cirklo sur la terglobo, paralela al la ekvatoro.
    </dif>
    <refgrp tip="vid">
      <ref cel="ekvato.0o">ekvatoro</ref>,
      <ref cel="longit.0o">longitudo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_l;&c_je;&c_l;&c_mol;
  <klr>(&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="cs">rovnoběžka zeměpisná</trd>
  <trd lng="de">Breitenkreis</trd>
  <trd lng="fr">parall&egrave;le<klr tip="ind"> (subst., g&eacute;og.)</klr></trd>
  <trd lng="hu">sz&eacute;less&eacute;gi k&ouml;r</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">rovnobežka zemepisná</trd>
</drv>
</art>
