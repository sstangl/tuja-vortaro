<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: femal.xml,v 1.6 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>femal</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="femal.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="femal.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="in.0o.BIO">ino</ref>
    <ekz>
      post dek tagoj demetas la <tld/>o duan serion da ovoloj
      <fnt>
      <bib>Salamandroj</bib>,
      <lok>libro 1a, &ccirc;apitro 12a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="cs">samice</trd>
  <trd lng="de">Weibchen</trd>
  <trd lng="pt">f&ecirc;mea</trd>
  <trd lng="sk">žena</trd>
  <trd lng="tp">meli</trd>
</drv>

<drv mrk="femal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="femal.0a.BIO">
    <ref tip="dif" cel="in.0a.BIO">ina <sncref/></ref>
  </snc>
  <trdgrp lng="cs">
    <trd>samičí</trd>,
    <trd>ženský</trd>
  </trdgrp>
  <trd lng="de">weiblich</trd>
  <trd lng="pt">f&ecirc;meo</trd>
  <trdgrp lng="sk">
    <trd>samičí</trd>,
    <trd>ženský</trd>
  </trdgrp>
  <trd lng="tp">meli</trd>
</drv>
</art>

