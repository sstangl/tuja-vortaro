<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rambutan.xml,v 1.3 2013/08/26 16:10:12 revo Exp $">
<kap>
  <rad>rambutan</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="rambutan.0o">
  <kap><tld/>o</kap>
  <snc mrk="rambutan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo, malgranda <ref tip="super"
      cel="drup.0o">drupo</ref> kun hara &scirc;elo kaj man&gcirc;eblaj karno kaj 
      kerno:
      <ekz>
        <klr>...</klr> ka<klr>&scirc;</klr>tanoj de mezvarmaj zonoj, peze
        &scirc;argitaj per 
        sennombraj fruktoj, similaj al <tld/>oj kun pika&jcirc;oj
        <fnt>
          <aut>Phan Hong Vuong</aut>,
          <vrk><url
          ref="http://www.ipernity.com/blog/phanhongvuong/140736">
          Trung Khanh, pitoreska pejza&gcirc;o</url></vrk>,
          <lok>2009-04-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_r;&c_a;&c_m;&c_b;&c_u;&c_t;&c_a;&c_n;</trd>
  <trd lng="fr">ramboutan<klr> (fruit)</klr></trd>
  <trd lng="id">rambutan</trd>
  <trd lng="nl">ramboetan</trd>
</drv>

<drv mrk="rambutan.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="rambutan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo el Sudorientazio kultivata pro fruktoj kaj kuracaj 
      substancoj
      (<trd lng="la">Nephelium lappaceum</trd>):
      <ekz>
        <tld/>arbo fari&gcirc;as 25 m<klr>etrojn</klr> alta
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Rambutano">Rambutano</url>,
          2010-01-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rambutan</trd>
  <trd lng="en">rambutan</trd>
  <trd lng="fr">ramboutan<klr> (arbre)</klr></trd>
</drv>
</art>

