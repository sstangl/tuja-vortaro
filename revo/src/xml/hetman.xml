<vortaro>
<art mrk="$Id: hetman.xml,v 1.15 2015/07/04 08:37:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hetman</rad>/o
</kap>
<drv mrk="hetman.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Generalo de <ref tip="vid" cel="kozak.0o">kozakoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_t;&c_m;&c_a;&c_n;</trd>,
    <trd>&c_a;&c_t;&c_a;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">hejtman</trd>
  <trdgrp lng="de">
    <trd>Hetman</trd>,
    <trd>Ataman</trd>
  </trdgrp>
  <trd lng="en">hetman</trd>
  <trdgrp lng="fr">
    <trd>ataman</trd>,
    <trd>hetman<klr tip="ind"> (chef cosaque)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hetman</trd>,
    <trd>atam&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>atamano</trd>, 
    <trd>hetman</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hetman</trd>,
    <trd>kozakkenhoofdman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>hetman</trd>,
    <trd>ataman</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>h&eacute;tm&atilde;</trd>, 
   <trd>h&eacute;tman</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_t;&c_m;&c_a;&c_n;</trd>,
    <trd>&c_a;&c_t;&c_a;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ataman</trd>,
    <trd>hajtman</trd>
  </trdgrp>
</drv>
</art>
