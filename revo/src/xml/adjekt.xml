    <art mrk="$Id: adjekt.xml,v 1.27 2015/07/04 08:37:29 revo Exp $">
        <kap>
            <ofc>*</ofc>
            <rad>adjektiv</rad>/o
        </kap>
        <drv mrk="adjekt.0o">
            <kap><ofc>*</ofc><tld/>o</kap>
            <snc>
            <uzo tip="fak">GRA</uzo>
                <dif>
                Vorto esprimanta la kvaliton, kaj &gcirc;enerale
                rilatanta kun <ref tip="dif"
                cel="substa1.0o">substantivo</ref>:
                    <ekz>
                    en esperanto la <tld/>o havas la fina&jcirc;on a;
                    </ekz>
                    <ekz>
                    la <tld/>o povas esti a&ubreve; <ref tip="dif"
                    cel="epitet.0o.GRA">epiteta</ref> a&ubreve; <ref tip="dif"
                    cel="predik2.0o">predikata</ref>.
                    </ekz>
                </dif>
            </snc>
  <trd lng="be">
            &c_p;&c_r;&c_y;&c_m;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">anv-gwan</trd>
  <trd lng="ca">adjectiu</trd>
  <trdgrp lng="cs">
    <trd>adjektivum</trd>,
    <trd>přídavné jméno</trd>
  </trdgrp>
  <trdgrp lng="de">
                <trd>Adjektiv</trd>,
                <trd>Eigenschaftswort</trd>
            </trdgrp>
  <trd lng="en">adjective</trd>
  <trd lng="es">adjetivo</trd>
  <trd lng="fa">&a_S;&a_f;&a_t;</trd>
  <trd lng="fr">adjectif</trd>
  <trd lng="hu">mell&eacute;kn&eacute;v</trd>
  <trd lng="it">aggettivo</trd>
  <trdgrp lng="nl">
                <trd>adjectief</trd>,
                <trd>bijvoeglijk <ind>naamwoord</ind></trd>
            </trdgrp>
  <trd lng="pl">przymiotnik</trd>
  <trd lng="pt">adjetivo</trd>
  <trd 
lng="ru">&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
  <trd lng="sk">pr&iacute;davn&eacute; meno</trd>
  <trd lng="sv">adjektiv</trd>
  <trd lng="zh">形容词 [<ind>xíng róng cí</ind>]</trd>
</drv>
    </art>
