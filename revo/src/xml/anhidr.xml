<art mrk="$Id: anhidr.xml,v 1.13 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>anhidr</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="anhidr.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Ne enhavanta akvon.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_v;&c_o;&c_d;&c_je;&c_n;</trd>
  <trd lng="cs">bezvodý</trd>
  <trd lng="de">wasserfrei</trd>
  <trdgrp lng="en">
    <trd>anhydrous</trd>,
    <trd>waterless</trd>
  </trdgrp>
  <trd lng="fr">anhydre</trd>
  <trd lng="nl">watervrij</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j; <klr tip="amb">(&c_h;&c_i;&c_m;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>bezvod&yacute; <klr>(chem.)</klr></trd>,
    <trd>bezvodý</trd>
  </trdgrp>
</drv>
</art>
