<vortaro>
<art mrk="$Id: mard.xml,v 1.30 2013/09/23 07:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mard</rad>/o
</kap>

<drv mrk="mard.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Tria <ref tip="lst" cel="tag.0o.diurn"
      lst="voko:semajntagoj" val="3">tago</ref> 
      de la
      <ref tip="malprt" cel="semajn.0o.KAL">semajno</ref>, inter lundo
      kaj
      merkredo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_o;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_v;&c_t;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="br">
    <trd>Meurzh<klr tip="ind"> (deiz)</klr></trd>,
    <trd>dimeurzh</trd>
  </trdgrp>
  <trd lng="ca">dimarts</trd>
  <trd lng="cs">&uacute;ter&yacute;</trd>
  <trd lng="de">Dienstag</trd>
  <trd lng="en">Tuesday</trd>
  <trd lng="es">martes</trd>
  <trd lng="fa">&a_s;&a_h;&zwnj;&a_sh;&a_n;&a_b;&a_h;</trd>
  <trd lng="fr">mardi</trd>
  <trd lng="he">&yod;&vav;&fmem; &shin;&lamed;&yod;&shin;&yod;</trd>
  <trd lng="hu">kedd</trd>
  <trd lng="it">marted&igrave;</trd>
  <trd lng="id">Selasa</trd>
  <trd lng="ja">&#28779;&#26332;&#26085; [<ind>&#12363;&#12424;&#12358;&#12403;</ind>]</trd>
  <trd lng="nl">dinsdag</trd>
  <trd lng="pl">wtorek</trd>
  <trd lng="pt">ter&ccedil;a-feira</trd>
  <trd lng="ro">mar&tcedil;i</trd>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">utorok</trd>
  <trd lng="sv">tisdag</trd>
  <trd lng="tr">sal&inodot;</trd>
  <trd lng="vo">tusdel</trd>
</drv>
</art>
