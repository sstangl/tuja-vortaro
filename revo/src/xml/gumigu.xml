<art mrk="$Id: gumigu.xml,v 1.10 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gumigut</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="gumigu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Speco de sekigita flava rezina gumo, uzata kiel kolorilo kaj
      anka&ubreve; kiel laksigilo.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_ib;&c_g;&c_u;&c_t;</trd>
  <trd lng="cs">gumiguta</trd>
  <trdgrp lng="de">
     <trd>Gummigutt</trd>,
     <trd>Gelbharz</trd></trdgrp>
  <trdgrp lng="hu">
     <trd>m&eacute;zgas&aacute;rga</trd>,
     <trd>gumigutti</trd>
  </trdgrp>
  <trd lng="pt">goma-guta</trd>
  <trd lng="ru">&c_g;&c_u;&c_m;&c_m;&c_i;&c_g;&c_u;&c_t;</trd>
  <trd lng="sk">gumiguta</trd>
</drv>
</art>
