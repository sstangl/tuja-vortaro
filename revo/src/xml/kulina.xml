<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kulina.xml,v 1.15 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kulinar</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kulina.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kulina.0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <ref tip="dif" cel="kuir.0arta.KUI">kuirarta</ref>
    <ekz>
      <tld/>a scienco kaj arto<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ib;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">culinari</trd>
  <trdgrp lng="cs">
    <trd>kuchařský</trd>,
    <trd>kulinární</trd>,
    <trd>kulinářský</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>kulinarisch</trd>,
     <trd>feinschmeckerisch</trd>
  </trdgrp>
  <trd lng="en">cullinary</trd>
  <trd lng="es">culinario</trd>
  <trd lng="fr">culinaire</trd>
  <trdgrp lng="hu">
     <trd>kulin&aacute;ris</trd>,
     <trd>konyhai</trd>
  </trdgrp>
  <trd lng="nl">culinair</trd>
  <trd lng="pt">culin&aacute;rio</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_i;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">kuchársky</trd>
</drv>
</art>
