<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dragon.xml,v 1.18 2015/07/04 08:37:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dragon</rad>/o
</kap>
<drv mrk="dragon.0o">
  <kap><tld/>o</kap>
  <snc mrk="dragon.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="soldat.0o">Soldato</ref> de malpeza
      kavalerio, kiu povas batali rajde a&ubreve; piede:
      <ekz>
        li estas le&ubreve;tenanto &ccirc;e la <tld/>a regimento
        &ccirc;evala de Lia
        mo&scirc;to
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e A&hcirc;med&#8217;
        Pa&scirc;a&#8217;, la
        granda pentristo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="ca">drag&oacute; <klr>(mil.)</klr></trd>
  <trdgrp lng="cs">
    <trd>dragoun</trd>,
    <trd>příslušník jezdectva</trd>
  </trdgrp>
  <trd lng="de">Dragoner</trd>
  <trd lng="en">dragoon</trd>
  <trd lng="fr">dragon<klr tip="ind"> (soldat)</klr></trd>
  <trd lng="it">dragone<klr> (mil.)</klr></trd>
  <trd lng="pl">ragon</trd>
  <trd lng="pt">drag&atilde;o</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="sk">dragún</trd>
  <trd lng="sv">dragon</trd>
</drv>
</art>
