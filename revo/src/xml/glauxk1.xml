<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glauxk1.xml,v 1.6 2013/09/14 09:10:34 revo Exp $">
<kap>
  <rad>gla&ubreve;k</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glauxk1.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bluverda, kiel iuj akvejoj:
      <ekz>
        servoknaboj altiris &ccirc;aron, kapu&ccirc;itan per <tld/>a silka dreliko
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 3a, p. 39a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>glauc</trd>,
    <trd>verd blanquin&oacute;s</trd>
  </trdgrp>
  <trd lng="de">blaugr&uuml;n</trd>
  <trd lng="fa">&a_A_madda;&a_b;&f_y; &a_z;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>glauque<klr> (adj., bleu-vert)</klr></trd>,
    <trd>bleu-vert<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>verde-mar<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>glauco<klr tip="ind"> (adj.)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_je;-&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;&c_a; &c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_v;&c_o;&c_l;&c_n;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="glauxk1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="ca">color glauc</trd>
  <trd lng="fa">&a_A_madda;&a_b;&f_y; &a_z;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>glauque<klr> (subst., bleu-vert)</klr></trd>,
    <trd>bleu-vert<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>verde-mar<klr tip="ind"> (subst.)</klr></trd>, 
   <trd>verde azulado<klr tip="ind"> (subst.)</klr></trd>, 
   <trd>glauco<klr tip="ind"> (subst.)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t; &c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_v;&c_o;&c_l;&c_n;&c_y;</trd>
</drv>
</art>

