<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alte.xml,v 1.21 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>alte</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="alte.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Althaea</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_ja;&c_l;&c_a; <ind>&c_r;&c_u;&c_zh;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Althaea</trd>,
    <trd>malv&iacute;, altea</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ibišek</trd>,
    <trd>proskurník</trd>,
    <trd>sléz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Althee</trd>,
    <trd>Eibisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Althea</trd>,
    <trd>hibiscus</trd>
  </trdgrp>
  <trd lng="fr">althaea</trd>
  <trd lng="hu">ziliz</trd>
  <trd lng="nl">heemst</trd>
  <trdgrp lng="pt">
    <trd>alt&eacute;ia</trd>,
    <trd>malva&iacute;sco</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_t;&c_je;&c_j;</trd>
  <trd lng="sk">ibištek</trd>
</drv>

<drv mrk="alte.roz0o">
  <kap>roz<tld/>o</kap>
  <snc mrk="alte.roz0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref> 
      de <tld/>o kun altaj tigoj kaj belaj floroj blankaj a&ubreve; 
      purpuraj, du- a&ubreve; plur-jara (<trd lng="la">Althaea rosea</trd>):
      <ekz>
        per dika mano /
        responde montras vojon &mdash; /
        jen, roz<tld/>o
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url
          ref="http://www.vastalto.com/blog/?m=200705&amp;paged=2">
          Akvosfero</url></vrk>,
          <lok>2007-05-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">malva reial</trd>
  <trd lng="cs">proskurník topolovka</trd>
  <trd lng="fr">rose tr&eacute;mi&egrave;re</trd>
  <trd lng="sk">topoľovka ružová</trd>
</drv>
</art>
