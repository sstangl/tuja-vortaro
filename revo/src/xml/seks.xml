      demeti en ali<tld/>an animalon la semon de ido
      (kontraste kun <ref cel="in.0o">ino</ref>):
      <ekz>
        &leftquot;Krom tio,&rightquot; &scirc;i diris plue,
        &leftquot;&gcirc;i estas vir<tld/>ulo,
        kaj la malbeleco sekve ne tiom multe malutilas
        <klr>...</klr>&rightquot;
        <fnt><bib>Fab1</bib>, <lok>Malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu vir<tld/>ulo &ccirc;e vi estu cirkumcidata
        <fnt><bib>MT</bib>, <lok>&Gen; 17:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_je;&c_c;</trd>
  <trdgrp lng="cs">
    <trd>mu&zcaron;</trd>,
    <trd>osoba rodu mu&zcaron;sk&eacute;ho</trd>
  </trdgrp>
  <trd lng="en">male</trd>
  <trd lng="es">macho</trd>
  <trd lng="fr">m&acirc;le<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">h&iacute;m</trd>
  <trd lng="it">maschio</trd>
  <trd lng="nl">mannetje</trd>
  <trd lng="pl">samiec</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_je;&c_c;</trd>,
    <trd>&c_o;&c_s;&c_o;&c_b;&c_a; <ind>&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mu&zcaron;</trd>,
    <trd>samec</trd>
  </trdgrp>
</drv>

