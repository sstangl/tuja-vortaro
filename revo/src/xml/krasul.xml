<kap>
  <rad>krasul</rad>/o
</kap>
<drv mrk="krasul.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="krasul1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Crassula</trd>), kun bonodorantaj floroj kaj dikaj
      folioj.
    </dif>
  </snc>
  <trd lng="de">Dickblatt</trd>
  <trd lng="hu">pozsga</trd>
  <trd lng="nl">crassula<klr tip="ind"> (vetplant)</klr></trd>
  <trd lng="pt">cr&aacute;ssula</trd>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
