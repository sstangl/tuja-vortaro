<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pean.xml,v 1.14 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pean</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pean.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">POE</uzo>
    <dif>
      Himno gloranta Apolonon, kantata de antikvaj grekoj kiel
      venkokanto.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_a;&c_n;</trd>
  <trd lng="cs">vícehlasá píseň na oslavu boha Apolona nebo vítězství</trd>
  <trd lng="de">P&auml;an</trd>
  <trd lng="en">paean</trd>
  <trd lng="fr">p&eacute;an</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;la&eacute;nek <klr tip="amb">(Apoll&oacute;hoz)</klr></trd>,
    <trd>gy&odblac;zelmi &eacute;nek</trd>,
    <trd>pain</trd>
  </trdgrp>
  <trd lng="nl">paean</trd>
  <trd lng="pt">pe&atilde;</trd>
  <trd lng="ru">&c_p;&c_je;&c_a;&c_n;</trd>
  <trd lng="sk">pieseň na oslavu boha Apolóna</trd>
</drv>
</art>
