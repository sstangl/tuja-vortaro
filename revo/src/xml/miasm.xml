<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miasm.xml,v 1.17 2015/07/04 08:37:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>miasm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="miasm.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="miasm.0o.KOMUNE">
    <dif>
      Malsanigaj haladzoj eli&gcirc;antaj el nevideblaj
      putri&gcirc;antaj eroj de
      organikaj substancoj:
      <ekz>
        abomena kadavro haladzanta mort<tld/>ojn
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_ja;&c_z;&c_m;&c_y;</trd>
  <trd lng="ca">miasma</trd>
  <trd lng="cs">miasma</trd>
  <trdgrp lng="de">
    <trd>Miasma</trd>,
    <trd>verseuchte <ind>Ausd&uuml;nstung</ind></trd>,
    <trd>Pesthauch</trd>
  </trdgrp>
  <trd lng="en">miasma</trd>
  <trd lng="fr">miasmes</trd>
  <trd lng="hu">miazma</trd>
  <trdgrp lng="nl">
    <trd>miasma</trd>,
    <trd>moerasdamp</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miazmat</trd>,
    <trd>wyziew</trd>
  </trdgrp>
  <trd lng="pt">miasma</trd>
  <trd lng="ru">&c_m;&c_i;&c_a;&c_z;&c_m;&c_y;</trd>
  <trd lng="sk">miazma</trd>
</drv>
</art>
