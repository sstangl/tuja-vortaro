<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cximpa.xml,v 1.27 2014/11/09 04:10:23 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>&ccirc;impanz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="cximpa.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="cximpa.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="orangu1.0oj">orangutangedoj</ref>
      (<trd lng="la">Pan</trd>), afrika homsimila 
      <ref tip="lst" cel="simi.0o" lst="voko:simioj">simioj</ref> kun du
      specioj.
    </dif>
  </snc>
  <snc mrk="cximpa.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La pli granda <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> el tiu genro
      (<trd lng="la">Pan troglodytes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_m;&c_p;&c_a;&c_n;&c_z;&c_e;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>
  <trd lng="de">Schimpanse</trd>
  <trd lng="el">&chi;&jota;&my;&pi;&alfa;&ny;&tau;&zeta;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">chimpanzee</trd>
  <trd lng="es">chimpanc&eacute;</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_m;&f_p;&a_A;&a_n;&a_z;&a_h;</trd>,
    <trd>&a_sh;&a_A;&a_m;&f_p;&a_A;&a_n;&a_z;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">chimpanz&eacute;</trd>
  <trd lng="hu">csimp&aacute;nz</trd>
  <trd lng="id">simpanse</trd>
  <trd lng="it">scimpanz&eacute;</trd>
  <trd lng="nl">chimpansee</trd>
  <trd lng="pt">chimpanz&eacute;</trd>
  <trd lng="pl">szympans</trd>
  <trd lng="ru">&c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>
  <trd lng="sk">&scaron;impanz</trd>
  <trd lng="sv">schimpans</trd>
</drv>

<drv mrk="cximpa.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="cximpa.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La pli malgranda <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> el tiu genro
      (<trd lng="la">Pan paniscus</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zwergschimpanse</trd>,
    <trd>Bonobo</trd>
  </trdgrp>
  <trd lng="en">dwarf <ind>chimpanzee</ind></trd>
  <trdgrp lng="fr">
    <trd>chimpanz&eacute; nain</trd>
    <trd>bonobo</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;rpecsimp&aacute;nz</trd>,
    <trd>bonob&oacute;</trd>
  </trdgrp>
  <trd lng="id">bonobo</trd>
  <trd lng="it">bonobo</trd>
  <trdgrp lng="nl">
    <trd>dwergchimpansee</trd>,
    <trd>bonobo</trd>
  </trdgrp>
  <trd lng="pl">szympans kar&lstroke;owaty</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;
    &c_sh;&c_i;&c_m;&c_p;&c_a;&c_n;&c_z;&c_je;</trd>,
    <trd>&c_b;&c_o;&c_n;&c_o;&c_b;&c_o;</trd>
  </trdgrp>
</drv>
</art>
