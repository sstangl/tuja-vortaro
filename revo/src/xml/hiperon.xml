  <rad>hiperon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hiperon.0o">
  <kap><tld/>o</kap>
  <snc mrk="hiperon.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Peza nestabila <ref tip="super" cel="barjon.0o">barjono</ref>.
    </dif>
    <ref tip="prt" cel="kvark.0o">kvarko</ref>
    <ref tip="vid" cel="partik2.0o">partiklo</ref>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_o;&c_n;</trd>
  <trd lng="ca">hiper&oacute;</trd>
  <trd lng="de">Hyperon</trd>
  <trdgrp lng="en"><trd>hyperon</trd>, <trd>hiperon</trd></trdgrp>
  <trd lng="fr">hyp&eacute;ron</trd>
  <trd lng="it">iperone</trd>
  <trd lng="nl">hyperon</trd>
  <trd lng="pt">h&iacute;peron</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_o;&c_n;</trd>
</drv>
</art>
