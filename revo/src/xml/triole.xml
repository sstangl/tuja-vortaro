<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: triole.xml,v 1.12 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>triolet</rad>/o
</kap>
<drv mrk="triole.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Fiksforma okversa poemeto kun ripeto de la unua verso post la tria
      kaj de la du unuaj post la sesa.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ja;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>triola</trd>,
    <trd>trylek</trd>
  </trdgrp>
  <trd lng="de">Triolett</trd>
  <trd lng="fr">triolet</trd>
  <trd lng="hu">triolett</trd>
  <trd lng="nl">triolet</trd>
  <trd lng="pt">triol&eacute;</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">triolet</trd>
</drv>
</art>
