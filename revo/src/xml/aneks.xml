<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: aneks.xml,v 1.19 2015/07/04 08:37:30 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>aneks</rad>/i
</kap>
<drv mrk="aneks.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aneks.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Aldoni al regno fremdan teritorion:
      <ekz>
        la venkinta Germanujo <tld/>is tiun departementon
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_n;&c_e;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>angliedern</trd>,
      <trd>annektieren</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_S;&a_r;&a_f; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_sh;&a_gh;&a_A;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_t;&a_s;&a_kh;&f_y;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">annexer</trd>
    <trd lng="nl">annexeren</trd>
    <trd lng="it">annettere</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_n;&c_n;&c_je;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>inf&ouml;rliva</trd>,
      <trd>annektera</trd>
    </trdgrp>
  </snc>
  <snc mrk="aneks.0i.KOMUNE">
    <dif>
      Aligi ion malpli gravan kaj ne&ccirc;efan al io &ccirc;efa kaj pli grava.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_D;&a_m;&f_y;&a_m;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_n;&a_D;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_l;&a_H;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>anektovat</trd>,
    <trd>násilně připojit</trd>,
    <trd>zabrat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>beilegen</trd>,
      <trd>anh&auml;ngen</trd>
  </trdgrp>
  <trd lng="en">annex</trd>
  <trd lng="es">anexar</trd>
  <trdgrp lng="fr">
    <trd>joindre</trd>,
    <trd>annexer</trd>
  </trdgrp>
  <trd lng="it">aggiungere</trd>
  <trd lng="nl">toevoegen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>anektova&tcaron;</trd>,
    <trd>násilne zabrať <klr>(územie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="aneks.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="aneks.0ajxo.POL">
    <dif>
      Aneksita lando.
    </dif>
    <trd lng="be">&c_a;&c_n;&c_e;&c_k;&c_s;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_t;&c_e;&c_r;&c_y;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Erwerbung</trd>,
      <trd>annektiertes Gebiet</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_n;&a_T;&a_q;&a_h;&a_hamza_sure; <ind>&a_A;&a_sh;&a_gh;&a_A;&a_l;&f_y;</ind></trd>
    <trd lng="fr">d&eacute;pendance</trd>
    <trd lng="it">annessione</trd>
  </snc>
  <snc mrk="aneks.0ajxo.KOMUNE">
    <dif>
      Ne&ccirc;efa&jcirc;o, dependa&jcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="akceso.0a">akcesora</ref>.
    </refgrp>
    <trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&f_y;&a_w;&a_s;&a_t;</trd>,
      <trd>&f_p;&f_y;&zwnj;&a_A;&a_f;&a_z;&a_w;&a_d;</trd>,
      <trd>&f_p;&f_y;&zwnj;&a_n;&a_w;&f_y;&a_s;</trd>,
      <trd>&a_D;&a_m;&f_y;&a_m;&a_h;</trd>,
      <trd>&a_m;&a_l;&a_H;&a_q;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pi&egrave;ce jointe</trd>,
      <trd>annexe</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
      <trd>Anlage</trd>,
      <trd>Anhang</trd>
  </trdgrp>
  <trd lng="en">annex</trd>
  <trd lng="es">anexo</trd>
  <trd lng="nl">bijvoegsel</trd>
</drv>
</art>
