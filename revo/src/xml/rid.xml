<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rid.xml,v 1.36 2015/09/23 22:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rid</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="rid.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="rid.0i.vizagxo">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Montri subitan gajecon per karakteriza stre&ccirc;o de
      la bu&scirc;o kaj esprimo de la viza&gcirc;o, ofte akompanataj
      de &gcirc;oja vo&ccirc;bruo:
      <ekz>
        mi <tld/>as je lia naiveco
        <fnt><bib>F</bib>, <lok>&FE; 29</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>as pro lia naiveco
        <fnt><bib>F</bib>, <lok>&FE; 29</lok></fnt>;
      </ekz>
      <ekz>
        <ind>plej bone <tld/>as, kiu laste <tld/>as</ind>
        <trdgrp lng="ca">
          <trd>riu b&eacute; qui riu darrer</trd>,
          <trd>qui riu primer plora darrer</trd>
        </trdgrp>
        <trd lng="es">el que r&iacute;e el &uacute;ltimo r&iacute;e
        mejor</trd>
        <trd lng="fr">rira bien qui rira le dernier</trd>
        <trd lng="hu">az nevet, aki utolj&aacute;ra nevet</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rid.0i.FIG">
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Montri agrablan, bonstatan, feli&ccirc;an aspekton:
      <ekz>
        <tld/>anta pejza&gcirc;o;
      </ekz><ekz>
        <ind>bone tiu sidas, al kiu la sorto <tld/>as</ind>
        <trd lng="hu">k&ouml;nny&udblac; annak, akire a szerencse
        mosolyog</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rid.0i.priridi">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="rid.pri0i.KOMUNE">pri<tld/>i</ref>
    <ekz>
      mi <tld/>as lian naivecon
      <fnt><bib>F</bib>, <lok>&FE; 29</lok></fnt>;
    </ekz>
    <ekz>
      la knaboj <tld/>is &scirc;in, ke &scirc;i sidas supre kiel kokineto
      <fnt>
        <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
        <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/VIII.">
        Avineto</url></vrk>,
        <lok>[sen dato]</lok>
      </fnt>.
    </ekz>
    <trd lng="fr">moquer<klr> (tr.)</klr></trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_s;&c_m;&c_je;&c_ja; &c_s;&c_je;</trd>
  <trd lng="ca">riure</trd>
  <trd lng="cs">sm&aacute;t se</trd>
  <trd lng="de">lachen</trd>
  <trd lng="en">laugh</trd>
  <trd lng="es">re&iacute;r</trd>
  <trd lng="fr">rire<klr tip="amb"> (verbe)</klr></trd>
  <trd lng="hu">nevet</trd>
  <trd lng="id">[<ind>tawa</ind>]tertawa</trd>
  <trd lng="nl">lachen</trd>
  <trd lng="pt">rir</trd>
  <trd lng="ru">&c_s;&c_m;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">smia&tcaron; sa</trd>
  <trd lng="sv">skratta</trd>
</drv>

<drv mrk="rid.0o">
  <kap><tld/>o</kap>
  <snc mrk="rid.0o.KOMUNE">
    <dif>
      Vo&ccirc;a, senparola esprimo de subita gajo:
      <ekz>
        eksonis en la &ccirc;ambro sonora infana <tld/>o
        <fnt><bib>FK</bib>, <lok>la forgesita pipo</lok></fnt>.
      </ekz>
      <ekz>
        <ind>eksplodi per <tld/>o</ind>
        <trd lng="es">troncharse de risa</trd>
        <trd lng="hu">nevet&eacute;sbe t&ouml;r ki</trd>
        <fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_m;&c_ja;&c_h;</trd>
  <trdgrp lng="ca">
    <trd>riure</trd>,
    <trd>rialla</trd>
  </trdgrp>
  <trd lng="cs">sm&iacute;ch</trd>
  <trd lng="de">Lachen</trd>
  <trd lng="es">risa</trd>
  <trd lng="fr">rire<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">nevet&eacute;s</trd>
  <trd lng="id">tawa</trd>
  <trd lng="nl">lach<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="ru">&c_s;&c_m;&c_je;&c_h;</trd>
  <trd lng="sk">smiech</trd>
</drv>

<drv mrk="rid.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <snc mrk="rid.0acxi.KOMUNE">
    <dif>
      Malice, triviale, malkonvene <tld/>i:
      <ekz>
        iu <klr>...</klr> <tld/>a&ccirc;is responde
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">riure sarc&agrave;sticament</trd>
  <trdgrp lng="es">
    <trd>re&iacute;r sarc&aacute;sticamente</trd>,
    <trd>re&iacute;r maliciosamente</trd>,
    <trd>carcajearse</trd>
  </trdgrp>
  <trd lng="fr">ricaner</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_g;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_u;&c_b;&c_o;&c_s;&c_k;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_m;&c_y;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="hu">r&ouml;h&ouml;g</trd> 
  <trd lng="nl">grijnzen</trd> 
</drv>

<drv mrk="rid.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc mrk="rid.0acxo.KOMUNE">
    <dif>
      Mal&gcirc;entila, malagrabla <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">riure sarc&agrave;stic</trd>
  <trd lng="es">risa sarc&aacute;stica</trd>
  <trd lng="fr">ricanement</trd>
  <trd lng="hu">r&ouml;h&ouml;g&eacute;s</trd>  
  <trd lng="nl">grijnslach</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_zh;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_h;&c_m;&c_y;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="rid.0egi">
  <kap><tld/>egi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rid.0egi.KOMUNE">
    <dif>
      Brue <tld/>i:
      <ekz>
        la knabo denove <tld/>as, <tld/>egas senbride
        <fnt><bib>Kiso</bib>, <lok>la brulanta urbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>petar-se de riure</trd>,
    <trd>cargolar-se de riure</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chechtat se</trd>,
    <trd>velmi se sm&aacute;t</trd>
  </trdgrp>
  <trd lng="es">re&iacute;r a carcajadas</trd>
  <trdgrp lng="fr">
    <trd>rire &agrave; gorge d&eacute;ploy&eacute;e</trd>,
    <trd>rire aux &eacute;clats</trd>
  </trdgrp>
  <trd lng="hu">kacag</trd>
  <trd lng="nl">schaterlachen</trd>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rehli&tcaron; sa</trd>,
    <trd>rehota&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">gapskratta</trd>
</drv>

<drv mrk="rid.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="rid.0ego.KOMUNE">
    <dif>
      Tre la&ubreve;ta <tld/>a vo&ccirc;ado:
      <ekz>
        en la salono eksonis la&ubreve;ta <tld/>ego
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">riallada</trd>
  <trd lng="cs">chechtot</trd>
  <trd lng="es">carcajada</trd>
  <trdgrp lng="fr">
    <trd>hilarit&eacute;</trd>,
    <trd>rigolade</trd>
  </trdgrp>
  <trd lng="hu">kacag&aacute;s</trd>
  <trd lng="nl">schaterlach</trd>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_o;&c_t;</trd>
  <trd lng="sk">rehot</trd>
</drv>

<drv mrk="rid.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="rid.0ema.KOMUNE">
    <dif>
      Facile <tld/>anta, ema al amuzo kaj gajeco:
      <ekz>
        koketa, <tld/>ema virin'
        <fnt>
          <aut>A. Le Pera, trad. -</aut>,
          <vrk><url ref="http://kantaro.ikso.net/pro_nur_unu_kapo">
          Pro nur unu kapo</url></vrk>,
          <lok>kantaro.ikso.net, 2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rieur<klr> (adj.)</klr></trd>
</drv>

<drv mrk="rid.0eti">
  <kap><tld/>eti</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rid.0eti.vizagxo">
    <dif>
      Senbrue montri sian afablecon kaj favoron per delikata stre&ccirc;o
      de la bu&scirc;o kaj bonvola esprimo de la okuloj:
      <ekz>
        &scirc;i <klr>...</klr> <tld/>etis kontente.
        <fnt><bib>FK</bib>, <lok>nur unu vorton!</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rid.0eti.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Agrable, favore aspekti:
      <ekz>
        la bela naturo <tld/>etas al la vizitanto;
      </ekz>
      <ekz>
        <tld/>etis al ni la a&ubreve;roro, la mateno;
      </ekz>
      <ekz>
        ar&gcirc;ente <tld/>etas la luno;
      </ekz>
      <ekz>
        la sorto <tld/>etas al li.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_mol;&c_m;&c_ib;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">somriure</trd>
  <trd lng="cs">usm&iacute;vat se</trd>
  <trd lng="de">l&auml;cheln</trd>
  <trd lng="en">smile</trd>
  <trd lng="es">sonre&iacute;r</trd>
  <trd lng="fr">sourire<klr tip="amb"> (verbe)</klr></trd>
  <trd lng="hu">mosolyog</trd>
  <trd lng="id">[<ind>senyum</ind>]tersenyum</trd>
  <trd lng="nl">glimlachen</trd>
  <trd lng="pt">sorrir, dar um ar de riso</trd>
  <trd lng="ru">&c_u;&c_l;&c_y;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">usmieva&tcaron; sa</trd>
  <trd lng="sv">le</trd>
</drv>

<drv mrk="rid.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc mrk="rid.0eto.KOMUNE">
    <dif>
      Afabla bu&scirc;a kaj viza&gcirc;a esprimo de bonvolo:
      <ekz>
        sur lia viza&gcirc;o mi vidis &gcirc;ojan <tld/>eton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">somr&iacute;s</trd>
  <trd lng="cs">&uacute;sm&ecaron;v</trd>
  <trd lng="de">L&auml;cheln</trd>
  <trd lng="es">sonrisa</trd>
  <trd lng="fr">sourire<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">mosoly</trd>
  <trd lng="id">senyum</trd>
  <trd lng="nl">glimlach</trd>
  <trd lng="ru">&c_u;&c_l;&c_y;&c_b;&c_k;&c_a;</trd>
  <trd lng="sk">&uacute;smev</trd>
</drv>

<drv mrk="rid.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="rid.0iga.KOMUNE">
    <dif>
      Kiu ka&ubreve;zas <tld/>on, subite gajigas:
      <ekz>
        lia <klr>...</klr> slango faris la &scirc;ercojn duoble
        <tld/>igaj
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>, <lok>La trimova &scirc;akproblemo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>risible</trd>,
    <trd>divertit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bud&iacute;c&iacute; sm&iacute;ch</trd>,
    <trd>&uacute;sm&ecaron;vn&yacute;</trd>
  </trdgrp>
  <trd lng="fr">amusant<klr tip="amb"> (qui fait rire)</klr></trd>
  <trdgrp lng="hu">
    <trd>nevettet&odblac;</trd>,
    <trd>mulats&aacute;gos</trd>,
    <trd>nevets&eacute;ges</trd>
  </trdgrp>
  <trd lng="nl">lachwekkend</trd>
  <trd lng="ru">&c_s;&c_m;&c_je;&c_sh;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">&uacute;smevn&yacute;</trd>
</drv>

<drv mrk="rid.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.0igi.KOMUNE">
    <dif>
      Vole a&ubreve; nevole ka&ubreve;zi ies <tld/>on:
      <ekz>
        lia malgaja viza&gcirc;o <tld/>igis lian amikon
        <fnt><bib>FK</bib>, <lok>nur unu vorton!</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amuz.0i">amuzi</ref>,
      <ref cel="gaj.0igi">gajigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>fer riure</trd>,
    <trd>causar risa</trd>
  </trdgrp>
  <trd lng="cs">rozesm&aacute;t <klr>(n&ecaron;koho)</klr></trd>
  <trd lng="es">hacer re&iacute;r</trd>
  <trd lng="fr">faire rire</trd>
  <trd lng="hu">(meg)<ind>nevettet</ind></trd>
  <trd lng="nl">doen <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_s;&c_m;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_s;&c_m;&c_je;&c_h;</ind></trd>
  </trdgrp>
  <trd lng="sk">rozosmia&tcaron; <klr>(niekoho)</klr></trd>
</drv>

<drv mrk="rid.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="rid.0inda.KOMUNE">
    <dif>
      Tiom <ref tip="super" cel="mok.0inda">mokinda</ref>, ke &gcirc;i
      <tld/>igas:
      <ekz>
        de majstra &gcirc;is <ind><tld/>inda</ind> estas nur unu
        pa&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>inda, fireligieca spirito <klr>...</klr> anta&ubreve;milite &ccirc;iam
        prezidis en esperantaj manifestacioj
        <fnt><bib>Lanti</bib>, <lok>... Vivantigi
        Esperanton</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>irrisori</trd>,
    <trd>rid&iacute;cul</trd>
  </trdgrp>
  <trd lng="cs">sm&ecaron;&scaron;n&yacute;</trd>
  <trd lng="de">l&auml;cherlich</trd>
  <trdgrp lng="es">
    <trd>irrisorio</trd>,
    <trd>risible</trd>,
    <trd>rid&iacute;culo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ridicule<klr tip="amb"> (adj.)</klr></trd>,
    <trd>risible</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nevets&eacute;ges</trd>,
    <trd>komikus</trd>
  </trdgrp>
  <trd lng="nl">belachelijk</trd>
  <trd lng="ru">&c_s;&c_m;&c_je;&c_sh;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">smie&scaron;ny</trd>
</drv>

<drv mrk="rid.0indigi">
  <kap><tld/>indigi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.0indigi.KOMUNE">
    <dif>
      Igi <tld/>inda, konduki al situacio kiu ka&ubreve;zas mokojn:
      <ekz>
        &scirc;i <tld/>indigas la svati&gcirc;antojn
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>d&ecaron;lat si &scaron;a&scaron;ky <klr>(z n&ecaron;koho)</klr></trd>,
    <trd>zesm&ecaron;&scaron;nit</trd>
  </trdgrp>
  <trd lng="fr">ridiculiser</trd>
  <trd lng="sk">zosmie&scaron;ni&tcaron;</trd>
</drv>

<drv mrk="rid.0indigxi">
  <kap><tld/>indi&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rid.0indigxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>inda, malrespektinda:
      <ekz>
        ne <tld/>indi&gcirc;u &ccirc;eeste de via hundo, mi petas
        <fnt>
          <aut>D. von Liliencron, trad. C. Brosch</aut>,
          <vrk><url
          ref="http://www.cyrilbrosch.net/wb/pages/esperanto/tradukoj/poemoj.php">
          Al mortintino</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>ridiculiser</ind></trd>
</drv>

<drv mrk="rid.al0eti">
  <kap>al<tld/>eti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.al0eti.KOMUNE">
    <dif>
      Intence, signife <tld/>eti al...:
      <ekz>
        ili al<tld/>etis ilin infane
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sourire &agrave;</trd>
</drv>

<drv mrk="rid.mok0i">
  <kap>mok<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.mok0i.KOMUNE">
    <dif>
      Moke pri<tld/>i iun a&ubreve; ion:
      <ekz>
        kaj ili mok<tld/>is lin
        <fnt><bib>NT</bib>, <lok>&Mat; 9:24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>riure's</trd>,
    <trd>burlar-se</trd>
  </trdgrp>
  <trd lng="cs">posm&iacute;vat se</trd>
  <trd lng="de">auslachen</trd>
  <trd lng="es">re&iacute;rse de</trd>
  <trd lng="fr">rire<klr tip="amb"> (de quelqu'un)</klr></trd>
  <trd lng="hu">kinevet</trd>
  <trd lng="nl">uitlachen</trd>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_m;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">posmieva&tcaron; sa</trd>
</drv>

<drv mrk="rid.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.pri0i.KOMUNE">
    <dif>
      <tld lit="R"/>i pro amuzi&gcirc;o a&ubreve; pro
      <ref tip="vid" cel="mok.0i.PSI">moko</ref> pri...;
      <ref tip="vid" cel="rid.0i.priridi">ridi</ref>:
      <ekz>
        Re&gcirc;ido Hiacinto <klr>...</klr> ne komprenis, ke la homoj 
        la&ubreve;dantaj lian nazon pri<tld/>as lin
        <fnt>
          <aut>J.-M. Leprince de Beaumont, trad. D. J. Harlow</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/blua/hiacinto.html">
          Re&gcirc;ido Hiacinto kaj la kara Re&gcirc;idineto</url></vrk>,
          <lok>[2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rire de</trd>
</drv>

<drv mrk="rid.sat0i">
  <kap>sat<tld/>i</kap>
  <snc mrk="rid.sat0i.KOMUNE">
    <dif>
      <tld lit="R"/>i &gcirc;is la stato en kiu oni ne plu deziras
      <tld/>i:
      <ekz>
        sat<tld/>is la lo&gcirc;antaro, sed poste ili beda&ubreve;ris sinjoron
        paro&hcirc;estron
        <fnt>
          <aut>J. Mahen, trad. Esperantista Rondeto</aut>,
          <vrk><url
          ref="http://www.literatura.bucek.name/mahen/koboldo.html">
          La koboldo Ondra</url></vrk>,
          <lok> 1921 </lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sat<tld/>os, &#8208; flustris la du junuloj, transpasante
        la &ccirc;ambron
        <fnt>
          <aut>J. K. Jerome, trad. G. Badash</aut>,
          <vrk>Tri viroj en boato</vrk>,
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">enfitar-se de riure</trd>
  <trd lng="es">hartarse de re&iacute;r</trd>
  <trd lng="nl">zich slap <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_m;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_h;&c_o;&c_h;&c_o;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rid.sub0i">
  <kap>sub<tld/>i</kap>
  <snc mrk="rid.sub0i.KOMUNE">
    <dif>
      Ka&scirc;e <tld/>i, subpremi sian <tld/>on:
      <ekz>
        Ilu-ittia sub<tld/>is
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a, pluaj
        insidoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>riure sorneguerament</trd>,
    <trd>riure per sota el nas</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potaj&iacute; se sm&aacute;t</trd>,
    <trd>sm&aacute;t se v duchu</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>re&iacute;r por lo bajo</trd>,
    <trd>reprimir la risa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pouffer</trd>,
    <trd>rire dans sa barbe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kuncog</trd>,
    <trd>somolyog</trd>
  </trdgrp>
  <trd lng="nl">in zijn vuistje <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_m;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_s;&c_m;&c_je;&c_h;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">potajomky sa smia&tcaron;</trd>
</drv>

<drv mrk="rid.trud0eti">
  <kap>trud<tld/>eti</kap>
  <snc mrk="rid.trud0eti.KOMUNE">
    <dif>
      Sin devigi al <tld/>eto:
      <ekz>
        <ctl>perfekte tiel</ctl> trud<tld/>etis &Ccirc;an
        <fnt>
          <aut>E. de Zilah</aut>,
          <vrk>La Princo &ccirc;e la Hunoj</vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sourire de mani&egrave;re forc&eacute;e</trd>
</drv>
