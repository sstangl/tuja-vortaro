<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: buks.xml,v 1.17 2015/07/04 08:37:31 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>buks</rad>/o
</kap>
<drv mrk="buks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="buksac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Buxus</trd>), kun flave blankaj floroj, ofte uzata
      por borderado de florbedoj, kaj kies flavkolora, malmolega ligno
      estas uzata por torna&jcirc;oj, inkrusta&jcirc;oj k.s.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_sh;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>buxus</trd>,
    <trd>zimostráz <klr>(pušpan krušpánek)</klr></trd>
  </trdgrp>
  <trd lng="de">Buchsbaum</trd>
  <trdgrp lng="en">
    <trd>box<klr tip="amb"> (tree)</klr></trd>,
    <trd>boxwood</trd>
  </trdgrp>
  <trd lng="fr">buis</trd>
  <trdgrp lng="hu">
    <trd>puszp&aacute;ng</trd>,
    <trd>bukszus</trd>
  </trdgrp>
  <trd lng="pl">bukszpan</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_sh;&c_i;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;
    &c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">boleráz</trd>
</drv>

</art>
