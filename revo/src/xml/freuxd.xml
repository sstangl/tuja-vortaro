<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: freuxd.xml,v 1.11 2010/04/16 16:31:32 revo Exp $">
<kap>
  <rad>Fre&ubreve;d</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="freuxd.0o">
  <kap><tld/>o</kap>
  <snc mrk="freuxd.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Germanlingve <trd lng="de">Sigmund <ind>Freud</ind></trd> (1856
      &ndash;
      1939), a&ubreve;stra psikiatro kaj psikologo, la fondinto de la <ref
      tip="vid" cel="psik.0analizo">psikanalizo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_r;&c_o;&c_j;&c_d;</trd>
  <trd lng="en">Freud</trd>
  <trd lng="fr">Freud</trd>
  <trd lng="hu">Freud</trd>
  <trd lng="it">Sigmund <ind>Freud</ind></trd>
  <trd lng="nl">Sigmund <ind>Freud</ind></trd>
  <trd lng="pt">Freud</trd>
  <trd lng="ru">&c_F;&c_r;&c_je;&c_j;&c_d;</trd>
</drv>

<drv mrk="freuxd.0a">
  <kap><tld lit="f"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la konceptoj de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_o;&c_j;&c_d;&c_a;&c_w;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">freudsch</trd>
  <trd lng="en">Freudian</trd>
  <trd lng="fr">freudien</trd>
  <trd lng="hu">freudi</trd>
  <trd lng="it">freudiano</trd>
  <trd lng="nl">freudiaans</trd>
  <trd lng="pt">freudiano</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_j;&c_d;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="freuxd.0ismo">
  <kap><tld lit="f"/>ismo</kap>
  <snc mrk="freuxd.0ismo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Doktrino, iniciatita de <tld/>o, pri la erotika origino de &ccirc;iaj
      homaj instinktoj kaj sentoj.
    </dif>
    <ref tip="vid" cel="libid.0o">libido</ref>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_j;&c_d;&c_y;&c_z;&c_m;</trd>
  <trd lng="en">Freudianism</trd>
  <trd lng="fr">freudisme</trd>
  <trd lng="hu">freudizmus</trd>
  <trd lng="it"><ind>teoria</ind> freudiana</trd>
  <trd lng="nl">freudianisme</trd>
  <trd lng="pt">freudismo</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_j;&c_d;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
