  <trdgrp lng="en">
    <trd>arrange</trd>,
    <trd>bring about</trd>,
    <trd>give rise to</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ocasionar</trd>,
    <trd>provocar<klr tip="ind"> (causar)</klr></trd>,
    <trd>organizar<klr tip="ind"> (un evento)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>causer<klr tip="ind"> (occasionner)</klr></trd>,
