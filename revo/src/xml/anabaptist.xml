<kap>
  <rad>anabaptist</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anabaptist.0o">
  <kap><tld/>o</kap>
  <snc mrk="anabaptist.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Subtenanto de <ref tip="vid" 
      cel="anabaptism.0o.KRI">anabaptismo</ref>;
      ano de anabaptisma komunumo:
      <ekz>
        <ind><tld/>a</ind>j eklezioj neniam decidis, kiu el la du
        kanonoj 
        (longa a&ubreve; mallonga) estas la plej bona
        <fnt>
          <aut>A. Gonz&aacute;lez</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/filozofio/137876">
          La nova Biblio de 2006...</url></vrk>,
          <lok>2009-03-22</lok>
        </fnt>.
        <trd lng="ca">anabaptista <klr>(adj.)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">anabaptista <klr>(subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>anabaptista</trd>,
    <trd>novokřtěnec</trd>
  </trdgrp>
  <trd lng="fr">anabaptiste<klr> (subst.)</klr></trd>
  <trd lng="sk">anabaptista</trd>
</drv>
</art>

