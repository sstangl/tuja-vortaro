    <trd>Variet&eacute;</trd>,
    <trd>Variet&eacute;programm</trd>
  </trdgrp>
  <trd lng="es">variedades</trd>
  <trdgrp lng="fr">
    <trd><ind>vari&eacute;t&eacute;</ind>s</trd>,
    <trd>spectacle de <ind>vari&eacute;t&eacute;</ind>s</trd>
  </trdgrp>
  <trd lng="hu">variet&eacute;</trd>
  <trd lng="nl">variet&eacute;</trd>
  <trdgrp lng="pt">
   <trd>variedades</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_mol;&c_je;&c_t;&c_je;</trd>
  <trd lng="sk">varieté</trd>
</drv>
