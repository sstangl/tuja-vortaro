<vortaro>
<art mrk="$Id: oskar.xml,v 1.5 2015/02/26 19:11:21 revo Exp $">
<kap>
  <rad>Oskar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="oskar.0o">
  <kap><tld/>o</kap>
  <snc mrk="oskar.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        onklo <tld/>o rakontis pri la vir&ccirc;evalo Narciso, kiun posedas
        lia amiko
        <fnt>
          <aut>L. Nordenstorm</aut>,
          <vrk>&Scirc;anco por Johano, &scirc;anco por Nikolao</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="oskar.0o.RIM">
    <ref tip="hom" cel="oskar1.0o">Oskaro</ref> anka&ubreve; estas nomo de 
    kinarta premio.
  </rim>
  <trd lng="fr">Oscar</trd>
  <trd lng="hu">Oszk&aacute;r</trd>
  <trd lng="it">Oscar</trd>
  <trd lng="nl">Oskar</trd>
</drv>
</art>

