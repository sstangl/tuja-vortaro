<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cimati.xml,v 1.11 2015/07/04 08:37:32 revo Exp $">
<kap>
  <rad>cimati</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cimati.0o">
  <kap><tld/>o</kap>
  <snc mrk="cimati.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="modlur.0o.ARKI">Modluro</ref>
      konsistiganta la supran parton de kornico. 
    </dif>
    <trd lng="en">cyma</trd>
    <trd lng="hu">szima<klr tip="amb">
    (p&aacute;rk&aacute;nytet&odblac;)</klr></trd>
  </snc>
  <snc mrk="cimati.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Modlura ornamo, kiun oni fiksas &ccirc;e muro,
      kaj de kiu oni ekspozicie pendigas
      pentra&jcirc;ojn.  
    </dif>
    <trdgrp lng="de">
      <trd>Abschlussleiste</trd>,
      <trd>Gesimsleiste</trd></trdgrp>
    <trd lng="en">picture rail</trd>
    <trd lng="hu">k&eacute;pl&eacute;c</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>cymatium</trd>,
    <trd>kyma</trd>,
    <trd>žebro římsy</trd>
  </trdgrp>
  <trd lng="fr">cimaise</trd>
  <trd lng="sk">rebro rímsy</trd>
</drv>
</art>
