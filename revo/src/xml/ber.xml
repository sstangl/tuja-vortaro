<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ber.xml,v 1.27 2015/10/05 04:10:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ber</rad>/o
</kap>

<drv mrk="ber.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ber.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.ferm0o">Fermfrukto</ref>
      kun suka, karneca perikarpo.
    </dif>
    <trd lng="pl">jagoda</trd>
  </snc>
  <snc mrk="ber.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Ia ajn eta karneca frukto: frago, frambo, sorpo:
      <ekz>
        ofte ili solaj kuris en la arbaro kaj kolektis ru&gcirc;ajn
        <tld/>ojn
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino kaj
        Rozulino</lok></fnt>;
      </ekz>
      <ekz>
        li estras firmaon, kiu okupi&gcirc;as pri konservado de <tld/>oj
        kaj
        fungoj
        <fnt><aut>Last</aut>,
        <vrk>Kurioza parko allogas turistojn</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 2a, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">jagoda</trd>
  </snc>
  <trd lng="be">&c_ja;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">hugenn</trd>
  <trd lng="cs">bobule</trd>
  <trd lng="de">Beere</trd>
  <trd lng="en">berry</trd>
  <trd lng="es">baya<klr tip="amb"> (fruto carnoso)</klr></trd>
  <trd lng="fr">baie <klr tip="ind">(fruit)</klr></trd>
  <trd lng="hu">bogy&oacute;</trd>
  <trd lng="nl">bes</trd>
  <trd lng="pt">baga</trd>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">bobu&lcaron;a</trd>
  <trd lng="sv">b&auml;r</trd>
</drv>

<drv mrk="ber.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="ber.0aro.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Aro da <tld/>oj sur sama tigo.
    </dif>
    <refgrp tip="vid">
      <ref cel="grapol.0o">grapolo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">blokad<klr tip="ind"> (hugenno&ugrave;)</klr></trd>
  <trd lng="cs">hrozen</trd>
  <trd lng="de">Traube</trd>
  <trd lng="en">bunch</trd>
  <trd lng="fr">grappe <klr tip="ind">(de baies)</klr></trd>
  <trd lng="hu">sz&odblac;l&odblac;f&uuml;rt</trd>
  <trd lng="nl">tros<klr tip="ind"> (druiven)</klr></trd>
  <trdgrp lng="pl">
    <trd>grono</trd>,
    <trd>ki&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_o;&c_z;&c_d;&c_mol;</trd>,
    <trd>&c_k;&c_i;&c_s;&c_t;&c_mol;
    <klr>(&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_a; &c_i;
    &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">strapec bob&uacute;&lcaron;</trd>
</drv>

<drv mrk="ber.roz0o">
  <kap>roz<tld/>o</kap>
  <snc mrk="ber.roz0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Scirc;ajnfrukto de rozo.
    </dif>
  </snc>
  <trd lng="cs">&scaron;&iacute;pek <klr>(plod)</klr></trd>
  <trd lng="de">Hagebutte</trd>
  <trd lng="sk">&scaron;&iacute;pka <klr>(plod)</klr></trd>
</drv>

<drv mrk="ber.sekvin0o">
  <kap><ofc>*</ofc>sekvin<tld/>o</kap>
  <snc mrk="ber.sekvin0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sekigita vin<tld/>o uzata kiel dol&ccirc;a gustiga&jcirc;o en
      diversaj
      pladoj,
      kukoj:
      <ekz>
        man&gcirc;a&jcirc;on el faruno, premitajn figojn,
        sekvin<tld/>ojn, vinon,
        oleon
        <fnt><bib>MT</bib>, <lok>&Kr1; 12:40</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_f;&c_i;&c_d;&c_a;</trd>
  <trd lng="br">rezin sec'h<klr> [pl.]</klr></trd>
  <trd lng="cs">rozinka</trd>
  <trd lng="de">Rosine</trd>
  <trd lng="en">raisin</trd>
  <trd lng="es">pasa</trd>
  <trdgrp lng="fr">
    <trd>raisin sec</trd>
  </trdgrp>
  <trd lng="hu">mazsola</trd>
  <trd lng="nl">rozijn</trd>
  <trd lng="pl">rodzynek</trd>
  <trd lng="pt">uva-<ind>passa</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_ju;&c_m;</trd>,
    <trd>&c_i;&c_z;&c_ju;&c_m;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hrozienko</trd>
</drv>

<drv mrk="ber.vin0o">
  <kap><ofc>*</ofc>vin<tld/>o</kap>
  <snc mrk="ber.vin0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Frukto, el kiu oni faras la vinon:
      <ekz>
        li lavas en sango de <tld/>beroj sian mantelon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        rikolta trupo gaja, konscia pri la valoro de &ccirc;iu tarda
        vin<tld/>o
        trovota
        <fnt>
        <aut>Istv&aacute;n Ertl</aut>,
        <vrk>Jarmil&scirc;an&gcirc;o</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 10a, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">rezin<klr> [pl.]</klr></trd>
  <trd lng="cs">bobule hroznu</trd>
  <trd lng="de">Weinbeere</trd>
  <trd
  lng="el">&sigma;&tau;&rho;&alfa;&phi;&ypsilon_ton;&lambda;&jota;</trd>
  <trd lng="en">grape</trd>
  <trd lng="es">uva</trd>
  <trd lng="fr">raisin</trd>
  <trdgrp lng="he">
    <trd>&ayin;&nun;&bet;</trd>,
    <trd>&ayin;&yod;&nun;&bet;</trd>
  </trdgrp>
  <trd lng="hu">sz&odblac;l&odblac;</trd>
  <trd lng="nl">druif</trd>
  <trd lng="pl">winogorono</trd>
  <trd lng="pt">uva</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrozno</trd>,
    <trd>hrozno <klr>(bobu&lcaron;a)</klr></trd>
  </trdgrp>
  <trd lng="sv">vindruva</trd>
  <trd lng="tr">&uuml;z&uuml;m</trd>
</drv>

<drv mrk="ber.vin0aro">
  <kap>vin<tld/>aro</kap>
  <snc>
    <dif>
      Aro da vin<tld/>oj, pendantaj de sama tigo:
      <ekz>
        via staturo similas la palmotrunkon,
        kaj viaj mamoj la vin<tld/>arojn
        <fnt><bib>MT</bib>, <lok>&Alt; 7:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;
  <klr>(&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_u;)</klr></trd>
  <trdgrp lng="br">
    <trd>blokad rezin</trd>,
    <trd>bodad rezin</trd>,
    <trd>barr rezin</trd>
  </trdgrp>
  <trd lng="cs">vinn&yacute; hrozen</trd>
  <trd lng="de">Weintraube</trd>
  <trd lng="el">&tau;&sigma;&alfa;&my;&pi;&jota_ton;</trd>
  <trd lng="en">bunch of grapes</trd>
  <trd lng="fr">grappe de raisin</trd>
  <trd lng="he">&alef;&shin;&kaf;&vav;&lamed;</trd>
  <trd lng="hu">sz&odblac;l&odblac;f&uuml;rt</trd>
  <trd lng="nl">druiventros</trd>
  <trd lng="pl">ki&sacute;&cacute; winogron</trd>
  <trd lng="pt">cacho</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_r;&c_o;&c_z;&c_d;&c_mol;</ind></trd>
  <trd lng="sk">strapec hrozna</trd>
</drv>
</art>
