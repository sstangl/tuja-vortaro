<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hasxisx.xml,v 1.11 2013/03/16 18:10:11 revo Exp $">
<kap>
  <rad>ha&scirc;i&scirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hasxisx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hasxisx.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="drog.0o.drogacxo">Drogo</ref>
      preparata el hinda <ref tip="vid" cel="kanab.0o.BOT">kanabo</ref>,
      ma&ccirc;ata a&ubreve; fumata sammaniere kiel tabako:
      <ekz>
        dum la du unuaj horoj mi sentis ion kvaza&ubreve; mi estus
        fuminta <tld/>on. Mi estis tre kvieta kaj serena
        <fnt>
        <aut>trad. &Eacute;. Coffinet</aut>,
        <vrk><url ref=
        "http://perso.wanadoo.fr/eric.coffinet/Iboga/lotsof-esp.html">
        La protokolo de Lotsof</url></vrk>,
        <lok>1998-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_sh;&c_y;&c_sh;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_y;&c_h;&c_u;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Haschisch</trd>,
    <trd>Marihuana</trd>
  </trdgrp>
  <trd lng="en">hashish</trd>
  <trd lng="fa">&a_H;&a_sh;&f_y;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>hachich</trd>,
    <trd>marijuana</trd>
  </trdgrp>
  <trd lng="it">hashish</trd>
  <trdgrp lng="pl">
    <trd>haszysz</trd>,
    <trd>marihuana</trd>
  </trdgrp>
  <trd lng="pt">haxixe</trd>
</drv>
</art>

