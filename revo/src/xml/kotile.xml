<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kotile.xml,v 1.19 2013/08/30 14:10:30 revo Exp $">
<kap>
  <rad>kotiledon</rad>/o
</kap>
<drv mrk="kotile.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unua folio &ccirc;e la floroplantoj, formita jam en
      la <ref tip="malprt"
cel="grajn.sem0o">semgrajno</ref>;
      &gcirc;ermfolio:
      <ekz>
        la koniferoplantoj havas plurajn <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_m;&c_ja;&c_d;&c_o;&c_l;&c_ja;</trd>
  <trd lng="ca">cotil&egrave;don</trd>
  <trd lng="de">Keimblatt</trd>
  <trd lng="en">cotyledon</trd>
  <trd lng="fa">&a_l;&f_p;&a_h;</trd>
  <trd lng="fr">cotyl&eacute;don</trd>
  <trd lng="hu">sziklev&eacute;l</trd>
  <trd lng="la">cotyledon</trd>
  <trd lng="pt">cotil&eacute;done</trd>
  <trd
lng="ru">&c_s;&c_je;&c_m;&c_ja;&c_d;&c_o;&c_l;&c_ja;</trd>
  <trd lng="sv">hj&auml;rtblad</trd>
</drv>

<drv mrk="kotile.unu0aj">
  <kap>
    unu<tld/>aj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO"
      lst="voko:botanikaj_grupoj">Grupo</ref> de <ref tip="malprt"
      cel="magnoli.0oj">magnolioj</ref>
      (<trd lng="la">Monocotelydoneae</trd>), havantaj
      nur unu <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">monocotiled&ograve;nies</trd>
  <trd lng="de">Einkeimbl&auml;ttrige</trd>
  <trd lng="en">monocotyledonous</trd>
  <trd lng="fa">&a_t;&f_k;&zwnj;&a_l;&f_p;&a_h;&zwnj;&a_A;&f_y;&zwnj;&a_h;&a_A;</trd>
  <trd lng="fr">monocotyl&eacute;dones</trd>
  <trd lng="hu">egyszik&udblac;ek</trd>
  <trdgrp lng="la">
    <trd>Monocotelydoneae</trd>,
    <trd>Liliopsida</trd>
  </trdgrp>
  <trd
lng="ru">&c_o;&c_d;&c_n;&c_o;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_je;</trd>
  <trd lng="sv">enhj&auml;rtbladiga</trd>
</drv>

<drv mrk="kotile.du0aj">
  <kap>
    du<tld/>aj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Kontraste al unu<tld/>aj, du grupoj de <ref tip="malprt"
      cel="magnoli.0oj">magnolioj</ref>, unusulkpolenaj
      (<trd lng="la">Magnoliopsida</trd>) kaj trisulkpolenaj
      (<trd lng="la">Rosopsida</trd>) havantaj
      du <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_u;&c_h;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">dicotiled&ograve;nies</trd>
  <trd lng="de">Zweikeimbl&auml;ttrige</trd>
  <trd lng="en">dicotyledonous</trd>
  <trd lng="fa">&a_d;&a_w;&a_l;&f_p;&a_h;&zwnj;&a_A;&f_y;&zwnj;&a_h;&a_A;</trd>
  <trd lng="fr">dicotyl&eacute;dones</trd>
  <trd lng="hu">k&eacute;tszik&udblac;ek</trd>
  <trd  lng="la">Dicotelydoneae</trd>
  <trd
lng="ru">&c_d;&c_v;&c_u;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_je;</trd>
  <trd lng="sv">tv&aring;hj&auml;rtbladiga</trd>
</drv>
</art>
