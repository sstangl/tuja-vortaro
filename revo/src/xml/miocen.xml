<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miocen.xml,v 1.16 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>miocen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="miocen.0o">
  <kap><tld/>o</kap>
  <snc mrk="miocen.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref tip="lst" cel="epok.0o.periodo"
      lst="voko:geologiaj_epokoj" val="-23030000">epoko</ref> inter la
      <ref
      cel="oligoc.0o">oligoceno</ref> kaj <ref
      cel="plioce.0o">plioceno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_ja;&c_c;&c_e;&c_n;</trd>
  <trd lng="ca">mioc&eacute;</trd>
  <trd lng="cs">miocén</trd>
  <trd lng="de">Mioz&auml;n</trd>
  <trd lng="fr">Mioc&egrave;ne</trd>
  <trd lng="hu">mioc&eacute;n</trd>
  <trd lng="nl">mioceen</trd>
  <trd lng="pl">miocen</trd>
  <trd lng="pt">Mioceno</trd>
  <trd lng="ru">&c_m;&c_i;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">miocén</trd>
</drv>
</art>
