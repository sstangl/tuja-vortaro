<art mrk="$Id: mentol.xml,v 1.4 2015/07/04 08:37:39 revo Exp $">
<kap>
  <rad>mentol</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mentol.0o">
  <kap><tld/>o</kap>
  <snc mrk="mentol.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Graso el <ref tip="vid" cel="ment.0o.genro">ment</ref>oleo,
      trankviliga al la ha&ubreve;to, uzata en &scirc;minka&jcirc;oj,
      medikamentoj...:
      <ekz>
        mi &ccirc;irka&ubreve;rigardis la &ccirc;ambron, glutis <tld/>an
        tablojdon
        <fnt>
        <aut>S. Lem, trad. K. Du&scirc;enko, trad. J. Finkel</aut>,
        <vrk><url
        ref="http://www.esperanto.mv.ru/Lem/index.html">Surloka
        esploro</url></vrk>,
        <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">mentol</trd>
  <trd lng="fr">menthol</trd>
  <trd lng="sk">mentol</trd>
</drv>
</art>
