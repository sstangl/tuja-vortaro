<vortaro>
<art mrk="$Id: bombik.xml,v 1.21 2015/07/04 08:37:31 revo Exp $">
<kap>
<rad>bombiks</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bombik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="bombik1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Bombyx</trd>), el kies kokono oni fabrikas la
      <ref tip="vid" cel="silk.0o">silkon</ref>.
    </dif>
    <ref tip="sin" cel="rauxp.silk0o.ZOO">silkra&ubreve;po</ref>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_p;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>b&ograve;mbix</trd>,
    <trd>cuc de seda</trd>
  </trdgrp>
  <trd lng="de">Seidenspinner</trd>
  <trdgrp lng="en">
    <trd>bombyx</trd>,
    <trd>silkworm</trd>
  </trdgrp>
  <trd lng="fr">bombyx</trd>
  <trd lng="he">&tav;&vav;&lamed;&ayin;&tav; &mem;&shin;&yod;</trd>
  <trd lng="hu">sz&ouml;v&odblac;lepke</trd>
  <trd lng="pl">jedwabnik</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_p;&c_r;&c_ja;&c_d;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bourec</trd>,
    <trd>hedvábník</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hodvábnik</trd>,
    <trd>priadka morušová</trd>
  </trdgrp>
</drv>

</art>
