<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kokeri.xml,v 1.18 2015/07/04 08:37:37 revo Exp $">
<kap>
  <rad>kokerik</rad>/i
  <fnt>C</fnt>
</kap>
<drv mrk="kokeri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kokeri.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel 
      <ref tip="vid" cel="kok.0o">koko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kokrhat</trd>
  <trd lng="de">kr&auml;hen</trd>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_w;&a_q;&a_w;&a_l;&f_y;&zwnj;&a_q;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>chanter<klr tip="ind"> (en p. d'un coq)</klr></trd>,
    <trd>faire <ind>cocorico</ind></trd>
  </trdgrp>
  <trd lng="hu">kukor&eacute;kol</trd>
  <trd lng="nl">kraaien<klr tip="ind"> (als een haan)</klr></trd>
  <trd lng="pl">pia&cacute;<klr tip="ind"> (o kogucie)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kikiríkať</trd>
</drv>

<drv mrk="kokeri.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kokokrio:
      <ekz>
        ili amba&ubreve; a&ubreve;dis <tld/>ojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p.
        129a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_u;</trd>
  <trd lng="cs">kokrhání</trd>
  <trdgrp lng="de">
    <trd>Hahnenschrei</trd>,
    <trd>Kikeriki</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_q;&a_w;&a_q;&a_w;&a_l;&f_y;&zwnj;&a_q;&a_w;</trd>,
    <trd>&a_b;&a_A;&a_n;&f_g; &a_kh;&a_r;&a_w;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">cocorico</trd>
  <trd lng="hu">kukor&eacute;kol&aacute;s</trd>
  <trd lng="nl">gekraai</trd>
  <trd lng="pl">pianie<klr tip="ind"> (koguta)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_u;</trd>
  <trd lng="sk">kikiríkanie</trd>
</drv>
</art>
