<kap>
  <rad>punik</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="punik.0a">
  <kap><tld/>a</kap>
  <snc mrk="punik.0a.HIS">
    <dif>
      De nordafrika kulturo kun &ccirc;efurbo Kartago, fondita de <ref
      tip="vid"
      cel="fenic.0o">fenicoj</ref>, da&ubreve;rinta de la 9a jarcento a. K.
      &gcirc;is la 2a jarcento a. K.:
      <ekz>
        la <tld/>a &ccirc;efo Hanibalo preska&ubreve; sie&gcirc;is Romon
        <fnt>
        <aut>A. Pareja</aut>,
        <vrk>Re: Turkio ante portas ?????</vrk>,
        <lok>soc.culture.esperanto, 2005-01-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_u;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;</trd>
  <trd lng="ca">p&uacute;nic</trd>
  <trd lng="cs">punský</trd>
  <trd lng="de">punisch</trd>
  <trd lng="en">punic</trd>
  <trd lng="es">p&uacute;nico</trd>
  <trd lng="fr">punique</trd>
  <trd lng="nl">punisch</trd>
  <trdgrp lng="pt">
    <trd>cartagin&ecirc;s</trd>,  
    <trd>p&uacute;nico</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">púnsky</trd>
</drv>
</art>

