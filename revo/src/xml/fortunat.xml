<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fortunat.xml,v 1.8 2015/02/26 20:10:28 revo Exp $">
<kap>
  <rad>Fortunat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fortunat.0o">
  <kap><tld/>o</kap>
  <snc mrk="fortunat.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        kaj mi &gcirc;ojas pro la alveno de <klr>...</klr> Fortunato kaj
        A&hcirc;aiko
        <fnt><bib>NT</bib>, <lok>&Ko1; 16:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Fortunat</trd>
  <trd lng="de">Fortunatus<klr tip="amb"> (Vorname)</klr></trd>
  <trd lng="fr">Fortun&eacute;<klr tip="amb"> (pr&eacute;nom)</klr></trd>
  <trdgrp lng="hu">
    <trd>Fortun&aacute;t</trd>,
    <trd>Fortunatus</trd>
  </trdgrp>
  <trd lng="it">Fortunato</trd>
  <trd lng="pt">Fortunato</trd>
</drv>
</art>

