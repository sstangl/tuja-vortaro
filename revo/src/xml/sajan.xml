<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sajan.xml,v 1.11 2013/08/23 11:10:13 revo Exp $">
<kap>
<rad>Sajan</rad>/o
</kap>
<drv mrk="sajan.S0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="mont.0aro" lst="voko:montaroj">Montaro</ref>
      etendi&gcirc;anta je 500km inter
      <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref> kaj Mongolujo,
      orienten de <ref cel="altaj.A0o">Altajo</ref> &gcirc;is
      <ref cel="bajkal.B0o">Bajkalo</ref>, &ccirc;e la limo inter
      <ref tip="malprt" cel="siberi.S0o">Siberio</ref>
      kaj la Centra Azio:
      <ekz>
        la <ind>Okcidenta <tld/>o</ind>;
        <trd lng="ru">&c_Z;&c_a;&c_p;&c_a;&c_d;&c_n;&c_y;&c_j;
        &c_S;&c_a;&c_ja;&c_n;</trd>
      </ekz>
      <ekz>
        la <ind>Orienta <tld/>o</ind>.
        <trd lng="ru">&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;
        &c_S;&c_a;&c_ja;&c_n;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_ja;&c_n;&c_y;</trd>
  <trd lng="ca">monts Sayan</trd>
  <trd lng="de">Sajan</trd>
  <trdgrp lng="fr">
    <trd>Sa&iuml;an</trd>, <trd>Sayan</trd>
  </trdgrp>
  <trd lng="hu">Saj&aacute;n</trd>
  <trd lng="it"><klr>Monti</klr> Saiani</trd>
  <trd lng="nl">Sajangebergte</trd>
  <trd lng="pt">Saian</trd>
  <trd lng="ru">&c_S;&c_a;&c_ja;&c_n;&c_y;</trd>
</drv>
</art>
