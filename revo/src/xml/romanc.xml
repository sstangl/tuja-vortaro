<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: romanc.xml,v 1.19 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>romanc</rad>/o
</kap>
<drv mrk="romanc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Versa <ref tip="super" cel="kant.0ajxo">kanta&jcirc;o</ref> pri
      kortu&scirc;a
      temo:
      <ekz>
        li da&ubreve;rigis la deklamon de la <tld/>o<fnt><bib>DonKihxoto</bib>
        <lok>&ccirc;apitro 5a, p. 51a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_s;</trd>
  <trd lng="ca">roman&ccedil;</trd>
  <trd lng="cs">romance</trd>
  <trd lng="de">Romanze</trd>
  <trdgrp lng="en">
    <trd>romance<klr tip="amb"> (song)</klr></trd>,
    <trd>romantic song</trd>
  </trdgrp>
  <trd lng="es">romance</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_m;&a_A;&a_n;&a_s;</trd>,
    <trd>&a_t;&a_r;&a_A;&a_n;&a_h;&a_hamza_sure; &a_ayn;&a_A;&a_sh;&a_q;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">romance</trd>
  <trd lng="hu">rom&aacute;nc</trd>
  <trd lng="nl">romance</trd>
  <trdgrp lng="pt">
    <trd>roman&ccedil;a</trd>,
    <trd>romance</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;</trd>
  <trd lng="sk">romanca</trd>
</drv>
</art>
