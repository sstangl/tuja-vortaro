<art mrk="$Id: meliac.xml,v 1.6 2013/08/25 16:10:52 revo Exp $">
<kap>
  <rad>meliac</rad>/oj
</kap>
<drv mrk="meliac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Meliaceae</trd>), de plej ofte
      tropikaj arboj.
    </dif>
    <ref tip="sub" cel="mahago.0o.arbo">mahagono</ref>
  </snc>
  <trd lng="de">Zedrachgew&auml;chse</trd>
  <trd lng="hu">imaf&uuml;z&eacute;rfaf&eacute;l&eacute;k</trd>
  <trd lng="nl">mahoniefamilie</trd>
  <trd lng="pt">m&eacute;li&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
