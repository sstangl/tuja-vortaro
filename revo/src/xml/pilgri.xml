<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pilgri.xml,v 1.17 2015/07/04 08:37:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pilgrim</rad>/i
</kap>
<drv mrk="pilgri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      Voja&gcirc;i por pie viziti sanktan lokon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_o;&c_m;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>putovat</trd>,
    <trd>účastnit se procesí</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wallfahren</trd>,
    <trd>pilgern</trd>
  </trdgrp>
  <trd lng="en">go on a pilgrimage</trd>
  <trd lng="es">peregrinar</trd>
  <trd lng="fa">&a_z;&f_y;&a_A;&a_r;&a_t; &a_r;&a_f;&a_t;&a_n;</trd>
  <trd lng="fr">aller en p&egrave;lerinage</trd>
  <trd lng="hu">zar&aacute;ndokol</trd>
  <trdgrp lng="nl">
    <trd>op <ind>bedevaart</ind> gaan</trd>,
    <trd>pelgrimeren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>peregrinar</trd>,
    <trd>ir em <ind>romaria</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_o;&c_m;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">putovať</trd>
</drv>

<drv mrk="pilgri.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      Voja&gcirc;o por pie viziti sanktan lokon.
    </dif>
    <rim>Ne konfuzu "pilgrimo" kun "pilgrimanto, pilgrimulo".</rim>
  </snc>
  <trdgrp lng="cs">
    <trd>pouť</trd>,
    <trd>putování</trd>,
    <trd>vykonávání pouti</trd>
  </trdgrp>
  <trd lng="en">pilgrimage</trd>
  <trd lng="fa">&a_z;&f_y;&a_A;&a_r;&a_t;</trd>
  <trd lng="fr">p&egrave;lerinage</trd>
  <trdgrp lng="nl">
    <trd>bedevaart</trd>,
    <trd>pelgrimstocht</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>putovanie</trd>,
    <trd>púť</trd>
  </trdgrp>
</drv>

<drv mrk="pilgri.0anto">
  <kap><tld/>anto, <var><kap><tld/>ulo</kap></var></kap>
  <snc>
    <dif>
      Homo kiu pilgrimas.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cestovatel</trd>,
    <trd>poutník</trd>
  </trdgrp>
  <trd lng="en">pilgrim</trd>
  <trd lng="fa">&a_z;&a_A;&f_y;&a_r;</trd>
  <trd lng="fr">p&egrave;lerin</trd>
  <trdgrp lng="nl">
    <trd>bedevaarder</trd>,
    <trd>pelgrim</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cestovateľ</trd>,
    <trd>pútnik</trd>
  </trdgrp>
</drv>

</art>
