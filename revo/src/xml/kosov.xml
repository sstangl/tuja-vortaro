<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kosov.xml,v 1.5 2013/02/12 02:10:26 revo Exp $">
<kap>
  <rad>kosov</rad>/o
</kap>

<drv mrk="kosov.K0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kosov.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Pri&scirc;tino.
      Teritorio de Respubliko Kosovo.
      Kelkaj &scirc;tatoj uzas nomon A&ubreve;tonoma Provinco Kosovo kaj
      Meto&hcirc;io.
      &Gcirc;i limas kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="makedon.M0ujo.GEOG">Makedonujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>
      kaj <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>.
    </dif>
    <trd lng="bg">&c_K;&c_o;&c_s;&c_o;&c_v;&c_o;</trd>
    <trd lng="de">Kosovo</trd>
    <trd lng="en">Kosovo</trd>
    <trdgrp lng="pl">
      <trd>Kosowo</trd>
    </trdgrp>
    <trd lng="pt">Kosovo</trd>
    <trd lng="fr">Kosovo</trd>
  </snc>
</drv>

</art>

