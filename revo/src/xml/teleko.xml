<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teleko.xml,v 1.11 2015/07/04 08:37:44 revo Exp $">
<kap>
  <rad>telekomunik</rad>/i
</kap>
<drv mrk="teleko.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEL</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Sendi informon per elektromagnetaj rimedoj:
      <ekz>
        <tld/>i al la fiska administracio sian enspezodeklaron.
      </ekz>
    </dif>
    <ref tip="vid" cel="komuni1.0i.sciigi">komuniki <sncref/></ref>
  </snc>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;communiquer</trd>,
    <trd>envoyer par <ind>t&eacute;l&eacute;communication</ind></trd>
  </trdgrp>
  <trd lng="id">[<ind>telekomunikasi</ind>] bertelekomunikasi</trd>
  <trd lng="pl">telekomunikowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>telecomunicar</trd>
  </trdgrp>
</drv>

<drv mrk="teleko.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="teleko.0ado.KOMUNE">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <snc mrk="teleko.0ado.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako"
      lst="voko:sciencoj">Scienco</ref>
      pri la manieroj <tld/>i informon.
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_m;&c_u;&c_n;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>komunikace na dálku</trd>,
    <trd>telekomunikační spojení</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Telekommunikation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>t&eacute;l&eacute;communication</trd>,
    <trd>t&eacute;l&eacute;com</trd>
  </trdgrp>
  <trd lng="id">telekomunikasi</trd>
  <trd lng="pl">telekomunikacja</trd>
  <trdgrp lng="pt">
    <trd>telecomunica&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_m;&c_m;&c_u;&c_n;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">komunikácia na diaľku</trd>
</drv>
</art>

