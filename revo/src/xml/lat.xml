<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lat.xml,v 1.24 2015/07/04 08:37:38 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>lat</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lat.0o">
  <kap><tld/>o</kap>
  <snc mrk="lat.0o.KOMUNE">
    <dif>
      Longa, mallar&gcirc;a kaj plate maldika peco da ligno, uzata 
      en konstruo:
      <ekz>
        kofroj faritaj el palmaj fibroj kaj &scirc;irmitaj per <tld/>oj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 67a, p. 430a</lok></fnt>;
      </ekz>
      <ekz>
        <ref tip="vid"
        cel="kurten.lat0o.ARKI"><tld/>kurteno</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lamen.0o">lameno</ref>,
      <ref cel="tabul.0o">tabulo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_e;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_je;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">llist&oacute;</trd>
  <trd lng="cs">laťka</trd>
  <trdgrp lng="de">
    <trd>Latte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lath</trd>,
    <trd>batten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lame<klr> (de bois)</klr></trd>
    <trd>latte</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;c</trd>
  <trd lng="it">stecca</trd>
  <trd lng="nl">lat</trd>
  <trdgrp lng="pt">
    <trd>ripa</trd>,
    <trd>sarrafo</trd>,
    <trd>fasquia</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lata</trd>,
    <trd>latka</trd>
  </trdgrp>
</drv>

<drv mrk="lat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lat.0i.KOMUNE">
    <dif>
      Garni per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">enllistonar</trd>
  <trd lng="cs">olaťovat</trd>
  <trd lng="fr">latter</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;cez</trd>,
    <trd>bel&eacute;cez</trd>
  </trdgrp>
  <trd lng="it">steccare</trd>
  <trdgrp lng="nl">
    <trd>belatten</trd>,
    <trd>latten</trd>,
    <trd>betengelen</trd>,
    <trd>aflatten</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol; 
<ind>&c_p;&c_l;&c_a;&c_n;&c_k;&c_a;&c_m;&c_i;</ind></trd>
  <trd lng="sk">latovať</trd>
</drv>
</art>
