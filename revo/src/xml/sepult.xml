<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sepult.xml,v 1.11 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sepult</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sepult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sepult.0i.FUN">
    <dif>
      Prizorgi la kadavron de mortinto per entombigo, bruligo a&ubreve; alia
      mora
      procedo:
      <ekz>
        la re&gcirc;ino Artemiza <tld/>is sian edzon <klr>...</klr> en tombon
        <klr>...</klr> grandiozan
        <fnt>
        <bib>DonKihxoto</bib>,
        <lok>parto 2a, &ccirc;apitro 8a, p. 438a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr
  tip="amb">(&c_m;&c_jo;&c_r;&c_t;&c_v;&c_a;&c_g;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>pochovat <klr>(zemřelého)</klr></trd>,
    <trd>pohřbít</trd>
  </trdgrp>
  <trd lng="es">sepultar</trd>
  <trd lng="fr">faire la <ind>s&eacute;pulture</ind> de</trd>
  <trdgrp lng="hu">
    <trd>temet</trd>,
    <trd>eltemet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>seppellire<klr> (cer. funebre)</klr></trd>, 
    <trd>cremare<klr> (cer. funebre)</klr></trd>
  </trdgrp>
  <trd lng="pt">sepultar</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">pochovať</trd>
</drv>

<drv mrk="sepult.0o">
  <kap><tld/>o</kap>
  <snc mrk="sepult.0o.FUN">
    <dif>
      Parto de <ref tip="malprt" cel="funebr.0o.moro">funebro</ref>
      kiam oni prizorgas la korpon de la mortinto:
        <ekz>
	Vilela prizorgis la <tld/>on, la mortokultojn, kaj la inventaron
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
        2005a, p.
        12a</lok></fnt>.
	</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_h;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">pohřeb</trd>
  <trd lng="es">sepultura</trd>
  <trd lng="fr">s&eacute;pulture<klr> (action)</klr></trd>
  <trd lng="hu">temet&eacute;s</trd>
  <trd lng="it">sepoltura</trd>
  <trd lng="pt">sepultamento</trd>
  <trd lng="ru">&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">pohreb</trd>
</drv>

<drv mrk="sepult.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="sepult.0ejo.FUN">
    <dif>
      Loko por resta&jcirc;oj de mortintoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_c;&c_a; &c_p;&c_a;&c_h;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hřbitov</trd>,
    <trd>pohřebiště</trd>
  </trdgrp>
  <trd lng="es">cementerio</trd>
  <trd lng="fr">s&eacute;pulture<klr> (lieu)</klr></trd>
  <trd lng="hu">temetkez&eacute;si hely</trd>
  <trdgrp lng="it">
    <trd>sepolcreto</trd>, 
    <trd>sepolcro</trd>
  </trdgrp>
  <trd lng="pt">sepultura</trd>
  <trd lng="ru">&c_m;&c_o;&c_g;&c_i;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>cintorín</trd>,
    <trd>pohrebisko</trd>
  </trdgrp>
</drv>
</art>
