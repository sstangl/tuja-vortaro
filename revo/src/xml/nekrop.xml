     <trd>Nekropole</trd>,
     <trd>Totenstadt</trd>
   </trdgrp>
  <trd lng="el">&kappa;&alfa;&tau;&alfa;&kappa;&omikron_ton;&my;&beta;&epsilon;&sigma_fina;</trd>
  <trdgrp lng="en">
     <trd>necropolis</trd>,
     <trd>catacombs</trd>
   </trdgrp>
  <trd lng="es">catacumbas</trd>
  <trd lng="fr">n&eacute;cropole</trd>
  <trd lng="hu">nekropolisz</trd>
  <trd lng="nl">dodenstad</trd>
  <trdgrp lng="pt">
     <trd>necr&oacute;pole</trd>,
     <trd>catacumba</trd>
   </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_k;&c_r;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">podzemný cintorín</trd>
</drv>
</art>
