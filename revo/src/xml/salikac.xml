<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: salikac.xml,v 1.12 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>salikac</rad>/oj
</kap>
<drv mrk="salikac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      lignaj plantoj (<trd lng="la">Salicaceae</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_malmol;&c_r;&c_b;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">salic&agrave;cies</trd>
  <trd lng="cs">vrbovité <klr>(čeleď)</klr></trd>
  <trd lng="de">Weidengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>salicaceae</trd>,
    <trd>willow family</trd>
  </trdgrp>
  <trd lng="fr">salicac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>f&udblac;zf&eacute;l&eacute;k</trd>,
    <trd>f&udblac;zfaf&eacute;l&eacute;k</trd>,
  </trdgrp>
  <trd lng="it">salicacee</trd>
  <trd lng="nl">wilgenfamilie</trd>
  <trdgrp lng="pt">
   <trd>salic&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_v;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">vŕbovité</trd>
</drv>
</art>
