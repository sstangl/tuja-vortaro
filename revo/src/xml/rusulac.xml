    <rad>rusulac</rad>/oj
</kap>
<drv mrk="rusulac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="rusulac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" 
      lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="fung.klab0oj">klabfungoj</ref>
      (<trd lng="la">Russulaceae</trd>)
     </dif>
  </snc>
  <trd lng="de">T&auml;ublingsverwandte</trd>
</drv>
</art>

