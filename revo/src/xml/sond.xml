<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sond.xml,v 1.16 2015/07/04 08:37:43 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>sond</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sond.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sond.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Esplori per aparato a&ubreve; nerekta metodo ne facile penetreblan
      fizikan
      medion, ekzemple akvon sub &scirc;ipo, terinternon, vivulan korpon.
    </dif>
    <trd lng="hu">szond&aacute;z</trd>
    <trd lng="nl">peilen</trd>
  </snc>
  <snc mrk="sond.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prove esplori, nerekte kolekti informojn pri malfacile alirebla
      homa
      medio:
      <ekz>
        mi <tld/>as &ccirc;e la kortego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se ni nur kura&gcirc;as <tld/>i la profundon de nia
        vivangoro
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk>Epokfara metamorfozo</vrk>,
          <bib>Monato</bib>,
          <lok>1997:6, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>kipuhatol</trd>,
      <trd>kif&uuml;rk&eacute;sz</trd>
    </trdgrp>
    <trd lng="nl">polsen</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pátrat</trd>,
    <trd>sondovat</trd>,
    <trd>zkoumat</trd>
  </trdgrp>
  <trd lng="de">peilen</trd>
  <trdgrp lng="en">
    <trd>sound</trd>,
    <trd>fathom</trd>,
    <trd>plumb</trd>
  </trdgrp>
  <trd lng="fr">sonder</trd>
  <trd lng="ru">&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pátrať</trd>,
    <trd>skúmať</trd>,
    <trd>sondovať</trd>
  </trdgrp>
</drv>

<drv mrk="sond.0o">
  <kap><tld/>o</kap>
  <snc mrk="sond.0o.KOMUNE">
    <dif>
      <tld lit="S"/>a esplorado:
      <ekz>
        por bori la putojn oni [...] uzis [...] post
        1930 sovetiajn kaj rumanajn <tld/>ojn per rondirado
        <fnt>
          <aut>F. Shehu</aut>,
          <vrk>La albana nafto kaj &gcirc;iaj perspektivoj</vrk>,
          <bib>Monato</bib>,
          <lok>1997:5, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sonda</trd>,
    <trd>vrt</trd>
  </trdgrp>
  <trd lng="fr">sondage</trd>
  <trdgrp lng="hu">
    <trd>szond&aacute;z&aacute;s</trd>,
    <trd>kutat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_d;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sonda</trd>,
    <trd>vrt</trd>
  </trdgrp>
</drv>

<drv mrk="sond.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sond.0ajxo.KOMUNE">
    <dif>
      Rezulto de <tld/>o:
      <ekz>
        dum en la lasta balotado la diversaj <tld/>a&jcirc;oj
        &gcirc;uste prognozis la rezulton, &ccirc;i-foje ili maltrafis
        <fnt>
          <aut>K. Juh&aacute;sz</aut>,
          <vrk>Apena&ubreve; venko</vrk>,
          <bib>Monato</bib>,
          <lok>2002:6, p. 12a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k; <ind>&c_z;&c_a;&c_n;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  <trd lng="fr">r&eacute;sultat de <ind>sondage</ind></trd>
  <trd lng="hu">kutat&aacute;si eredm&eacute;ny</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t; <ind>&c_z;&c_o;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
</drv>

<drv mrk="sond.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sond.0ilo.KOMUNE">
    <dif>
      Ilo, ma&scirc;ino, rimedo por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_n;&c_d;</trd>
  <trd lng="cs">měřítko hloubky s olověným závažím</trd>
  <trd lng="fr">sonde</trd>
  <trd lng="hu">szonda</trd>
  <trd lng="sk">meranie hĺbky oloveným závažím</trd>
</drv>

<drv mrk="sond.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="sond.ne0ebla.KOMUNE">
    <dif>
      Tre profunda, ne plene esplorebla:
      <ekz>
        li sidi&gcirc;is sur la se&gcirc;on, tenante ne<tld/>eblan
        silenton
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nezjistitelný</trd>
  <trd lng="fr">insondable</trd>
  <trd lng="sk">nezistiteľný</trd>
</drv>

<drv mrk="sond.opini0o">
  <kap>opini<tld/>o</kap>
  <snc mrk="sond.opini0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Enketo pri publika opinio pere de demandado
      &ccirc;e elektita subaro de la koncerna homgrupo;
      <ref tip="sin" cel="enket.opini0o">opinienketo</ref>:
      <ekz>
        la&ubreve; fre&scirc;aj opini<tld/>oj 60 % de la EU-civitanoj rifuzas man&gcirc;i
        genteknike manipulitajn nutra&jcirc;ojn
          <fnt>
            <aut>Stefan Maul</aut>:
            <vrk>Defio a&ubreve; devio de politiko</vrk>,
            <bib>Monato</bib>,
            <lok>2000:10, p. 6a</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_y;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_k;&c_a;&c_j; &c_d;&c_u;&c_m;&c_k;&c_ib;)</klr></trd>
  <trd lng="cs">průzkum veřejného mínění</trd>
  <trdgrp lng="fr">
    <trd>enqu&ecirc;te d'opinion</trd>,
    <trd>sondage<klr> (enqu&ecirc;te d'opinion)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&ouml;zv&eacute;lem&eacute;ny-kutat&aacute;s</trd>
  <trd lng="ru">&c_o;&c_p;&c_r;&c_o;&c_s; &c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_g;&c_o; &c_m;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">prieskum verejnej mienky</trd>
</drv>
</art>
