<vortaro>
<art mrk="$Id: faksim.xml,v 1.17 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>faksimil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="faksim.0o">
  <kap><tld/>o</kap>
  <snc mrk="faksim.0o.KOMUNE">
    <dif>
      Ekzakta reprodukta&jcirc;o de skribo, desegno:
      <ekz>
        al la muroj estis alkro&ccirc;itaj diversaj pentra&jcirc;oj
        <klr>...</klr>, 
        <tld/>oj de konataj majstra&jcirc;oj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="imit.0ajxo">imita&jcirc;o</ref>,
      <ref cel="kopi.0o.kopiajxo">kopio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_s;&c_ib;&c_m;&c_ib;&c_l;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>faksimile</trd>,
    <trd>kopie</trd>,
    <trd>napodobenina</trd>
  </trdgrp>
  <trd lng="de">Faksimile</trd>
  <trd lng="en">facsimile</trd>
  <trdgrp lng="fr">
    <trd>fac-simil&eacute;</trd>,
    <trd>reproduction<klr> (d'un document)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fakszimile</trd>,
    <trd>hasonm&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">facsimile</trd>
  <trdgrp lng="pl">
    <trd>reprodukcja</trd>,
    <trd>podr&oacute;bka</trd>,
    <trd>falsyfikat</trd>,
    <trd>faksimile</trd>
  </trdgrp>
  <trd lng="pt">fac-s&iacute;mile</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_s;&c_i;&c_m;&c_i;&c_l;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>presná kópia <klr>(písma, kresby)</klr></trd>,
    <trd>presná kópia <klr>(písma| kresby)</klr></trd>
  </trdgrp>
</drv>
</art>
