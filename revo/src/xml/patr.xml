    <trd>any&oacute;s&eacute;k</trd>
  </trdgrp>
  <trd lng="id">mertua</trd>
  <trd lng="it">suoceri<klr> (coppia di)</klr></trd>
  <trd lng="nl">schoonouders</trd>
  <trdgrp lng="no">
