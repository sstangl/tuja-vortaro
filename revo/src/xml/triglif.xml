<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: triglif.xml,v 1.5 2011/12/07 01:10:12 revo Exp $">
<kap><rad>triglif</rad>/o<fnt>PIV2</fnt></kap>
<drv mrk="triglif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Elemento de dorika friso, &scirc;tonplato, skulptita per tri kaneloj,
      en imito de la origina trabfino.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_g;&c_l;&c_ib;&c_f;</trd>
  <trd lng="ca">tr&iacute;glif</trd>
  <trd lng="de">Dreischlitz</trd>
  <trd lng="en">triglyph</trd>
  <trdgrp lng="es">
    <trd>triglifo</trd>,
    <trd>tr&iacute;glifo</trd>
  </trdgrp>
  <trd lng="fr">triglyphe</trd>
  <trd lng="it">triglifo</trd>
  <trdgrp lng="nl">
    <trd>triglief</trd>,
    <trd>driespleet</trd>
  </trdgrp>
  <trd lng="pl">tryglif</trd>
  <trd lng="pt">tr&iacute;glifo</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_g;&c_l;&c_i;&c_f;</trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&c_g;&c_l;&c_ib;&c_f;</trd>
</drv>
</art>

