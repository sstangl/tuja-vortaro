<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glicir.xml,v 1.20 2013/08/30 14:11:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gliciriz</rad>/o
</kap>
<drv mrk="glicir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Glycyrrhiza</trd>), el kies radiko oni ekstraktas
      dol&ccirc;an sukon, uzatan kontra&ubreve; tuso kaj por dol&ccirc;a&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>regal&iacute;ssia</trd>,
    <trd>regal&egrave;ssia</trd>
  </trdgrp>
  <trd lng="de">S&uuml;&szlig;holz</trd>
  <trdgrp lng="en">
    <trd>liquorice</trd>,
    <trd>licorice</trd>
  </trdgrp>
  <trd lng="es">regaliz</trd>
  <trd lng="fa">&a_sh;&f_y;&a_r;&f_y;&a_n;&zwnj;&a_b;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">r&eacute;glisse<klr tip="ind"> (plante)</klr></trd>
  <trd lng="nl">zoethout</trd>
  <trd lng="pt">alca&ccedil;uz<klr tip="amb"> (bot&acirc;nica)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_l;&c_o;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">meyank&ouml;k&uuml;</trd>
</drv>
<drv mrk="glicir.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Dol&ccirc;a&jcirc;o nigra, farita el infuza&jcirc;o de <tld/>o. La
      gusto povas varii de tre sala &gcirc;is tre dol&ccirc;a, kaj de
      sufi&ccirc;e mola &gcirc;is sufi&ccirc;e malmola.
    </dif>
  </snc>
  <trd lng="ca">pega dol&ccedil;a</trd>
  <trd lng="de">Lakritze</trd>
  <trdgrp lng="en">
    <trd>liquorice</trd>,
    <trd>licorice</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;glisse<klr tip="ind"> (confiserie)</klr></trd>
  <trd lng="nl">drop</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_r;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
