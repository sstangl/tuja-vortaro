<vortaro>
<art mrk="$Id: indi.xml,v 1.24 2015/07/04 08:37:36 revo Exp $">
<kap>
  <rad>indi</rad>/o
</kap>
<drv mrk="indi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">In</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="49">elemento</ref> kun
      atomnumero 49, atompezo 114,82; ar&gcirc;ente blanka
      posttransira <ref tip="super" cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">indiom</trd>
  <trd lng="ca">indi</trd>
  <trd lng="cs">indium</trd>
  <trd lng="de">Indium</trd>
  <trd lng="en">indium</trd>
  <trd lng="es">indio</trd>
  <trd lng="fr">indium</trd>
  <trd lng="hu">indium</trd>
  <trd lng="id">indium</trd>
  <trd lng="it">indio</trd>
  <trd lng="la">Indium</trd>
  <trd lng="nl">indium</trd>
  <trd lng="pl">ind</trd>
  <trd lng="pt">&iacute;ndio<klr tip="ind"> (qu&iacute;mica, 
bioqu&iacute;mica)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_j;</trd>
  <trd lng="sk">indium</trd>
  <trd lng="sv">indium</trd>
</drv>
</art>
