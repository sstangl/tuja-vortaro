<kap>
  <rad>krampf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krampf.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="krampf.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="kramf.0o.MED">kramfo</ref>
    <ekz>
      sento de <tld/>oj, de kuntiri&gcirc;o a&ubreve; de kontrakturoj
      <fnt>
      <aut>J.-J. Kasparian</aut>,
      <vrk><url
      ref="http://homeoint.org/articles/kaspar/jjk3qseo.htm">Demandaro
      de
      la sperta sistemo Duprat</url></vrk>,
      <lok>2000-07</lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="ca">rampa</trd>
  <trdgrp lng="cs">
    <trd>křeč</trd>,
    <trd>křeč tonická</trd>
  </trdgrp>
  <trd lng="fr">crampe</trd>
  <trd lng="pt">c&atilde;imbra</trd>
  <trd lng="sk">kŕč</trd>
</drv>
</art>
