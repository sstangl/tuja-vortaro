<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koklus.xml,v 1.15 2014/11/18 20:10:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>koklu&scirc;</rad>/o
</kap>
<drv mrk="koklus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Infekta <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>, karakterizata de konvulsia
      tuso.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_k;&c_l;&c_ju;&c_sh;</trd>      
  <trdgrp lng="br">
    <trd>drev<klr tip="ind"> (kle&ntilde;ved)</klr></trd>,
    <trd>paz-yud</trd>
  </trdgrp>
  <trd lng="de">Keuchhusten</trd>
  <trdgrp lng="en">
    <trd>whooping cough</trd>,
    <trd>pertussis</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&zwnj;&a_s;&a_r;&a_f;&a_h;</trd>
  <trd lng="fr">coqueluche<klr tip="ind"> (maladie)</klr></trd>
  <trdgrp lng="id">
    <trd><ind>batuk</ind> rejan</trd>,
    <trd>pertusis</trd>
  </trdgrp>
  <trd lng="nl">kinkhoest</trd>
  <trdgrp lng="pl">
  <trd>krztusiec</trd>,
  <trd>koklusz <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_l;&c_ju;&c_sh;</trd>
</drv>
</art>
