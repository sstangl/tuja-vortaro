<kap>
  <rad>betulac</rad>/oj
</kap>
<drv mrk="betula.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Betulaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_r;&c_o;&c_z;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_b;&c_r;&c_je;&c_z;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">betul&agrave;cies</trd>
  <trd lng="de">Birkengew&auml;chse</trd>
  <trd lng="es">betul&aacute;ceas</trd>
  <trd lng="en">birches</trd>
  <trd lng="fr">b&eacute;tulac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>ny&iacute;rf&eacute;l&eacute;k</trd>,
    <trd>ny&iacute;rfaf&eacute;l&eacute;k</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>berken</trd>,
    <trd>heesters</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_jo;&c_z;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">bj&ouml;rkv&auml;xter</trd>
</drv>
