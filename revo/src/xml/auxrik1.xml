<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxrik1.xml,v 1.15 2013/08/19 13:10:14 revo Exp $">
<kap>
  <rad>a&ubreve;rikul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="auxrik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxrik1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="primol.0o">primolo</ref>
      (<trd lng="la">Primula auricula</trd>), kiu floras
      anta&ubreve; la komenco de printempo.
    </dif>
    <trd lng="bg">&c_a;&c_l;&c_p;&c_i;&c_j;&c_s;&c_k;&c_a; &c_i;&c_g;&c_l;&c_i;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Gamsblume</trd>, <trd>Alpenaurikel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>auricula</trd>,
      <trd>mountain cowslip</trd>,
      <trd>primrose</trd>
    </trdgrp>
    <trd lng="fr">auricule</trd>
    <trdgrp lng="hu">
      <trd>cifra <ind>kankalin</ind></trd>,
      <trd>medvef&uuml;l <ind>kankalin</ind></trd>,
      <trd>f&uuml;les <ind>kankalin</ind></trd>
    </trdgrp>
    <trd lng="it"><ind>primula</ind> orecchia d'orso</trd>
    <trd lng="nl">aurikel</trd>
    <trd lng="ru">&c_m;&c_je;&c_d;&c_v;&c_je;&c_zh;&c_mol;&c_je; &c_u;&c_sh;&c_k;&c_o;</trd>
  </snc>
  <snc mrk="auxrik1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      G. de maraj limakoj (<trd lng="la">auriculus</trd>).
    </dif>
    <adm>
      Mi ne certas, pri kio temas, eble pri la "marorelo" (Haliotis).
      Se iu scias bv. informi min.
      <aut>Wolfram Diestel</aut>
    </adm>
  </snc>
</drv>
</art>
