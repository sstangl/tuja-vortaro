<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sivert.xml,v 1.5 2015/10/13 09:10:25 revo Exp $">
<kap>
  <rad>sivert</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="sivert.0o">
  <kap><tld/>o</kap>
  <mlg>Sv</mlg>
  <snc mrk="sivert.0o.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o.SCI"
      lst="voko:mezurunuoj">Mezurunuo</ref>
      de radioaktiveca sorbado fare de viva&jcirc;oj:
      <ekz>
        la registaro difinis tiujn lokojn inter la radiusoj de 20-30 
        kilometroj de la nuklea centralo, kiuj ne povos ricevi pli ol 
        20 <tld/>ojn jare
        <fnt>
          <aut>Hori J.</aut>,
          <vrk><url
          ref="http://satesperanto.org/la-4an-de-septembro.html">
          La 04an de decembro</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bekerel.0o.MEZ">bekerelo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Sievert</trd>
  <trd lng="en">sievert</trd>
  <trd lng="fr">sievert</trd>
  <trd lng="id">sievert</trd>
  <trd lng="pl">siwert</trd>
</drv>
</art>

