<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pseuxdoskorpi.xml,v 1.2 2013/09/01 10:10:22 revo Exp $">
<kap>
  <rad>pse&ubreve;doskorpi</rad>/oj
</kap>
<drv mrk="pseuxdoskorpi.0oj">
  <kap><tld/>oj, <var><kap><tld/>uloj
  <fnt><bib>PIV2</bib></fnt></kap></var></kap>
  <snc mrk="pseuxdoskorpi.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Skorpisimilaj bestoj kun pin&ccirc;iloj sed sen venena pikilo, du &gcirc;is
      ok milimetrojn longaj, vivantaj en grundo, en hejmoj,
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">ordo</ref> 
      el klaso araneoidoj (<trd lng="la">Pseudoscorpiones</trd>):
      <ekz>
        <tld/>oj <klr>...</klr> vivantaj inter defalintaj folioj,
        muskoj, arbo&scirc;elo kaj en talpaj kaj birdaj nestoj
        <fnt>
        <aut>E. Nieuwenhuys, trad. L. De Cooman</aut>,
        <vrk><url
        ref="http://www.xs4all.nl/~ednieuw/Spiders/InfoNed/La_araneo.html">Araneo</url></vrk>,
        <lok>[2004]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="malprt" cel="artik.0uloj">artikuloj</ref>
  <trd lng="de">Pseudoskorpione</trd>
  <trd lng="fr">pseudoscorpions</trd>
</drv>
</art>

